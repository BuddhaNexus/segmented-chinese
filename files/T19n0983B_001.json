{
    "T19n0983B_001:0441b15": "孔雀經真言等梵本",
    "T19n0983B_001:0441b17": "孔雀經上",
    "T19n0983B_001:0442a23": "         三十六句宛",
    "T19n0983B_001:0442a24": "十二行即界二十四降可得也。",
    "T19n0983B_001:0442b19": "         宛二",
    "T19n0983B_001:0442b20": "十二行即界四十四降可得也。",
    "T19n0983B_001:0442b28": "(去)           ",
    "T19n0983B_001:0442c12": "       文二十行即界四",
    "T19n0983B_001:0442c13": "十降也。",
    "T19n0983B_001:0442c27": "     (引)    ",
    "T19n0983B_001:0443a25": "       文三十八行界",
    "T19n0983B_001:0443a26": "即可得七十六降也加漢語八十六降也。",
    "T19n0983B_001:0443c11": "孔雀經中",
    "T19n0983B_001:0444b16": "        此真",
    "T19n0983B_001:0444b17": "言七也",
    "T19n0983B_001:0445a25": "        得百二降也。",
    "T19n0983B_001:0445a26": "孔雀經下",
    "T19n0983B_001:0446b13": "孔雀經一部真言"
}