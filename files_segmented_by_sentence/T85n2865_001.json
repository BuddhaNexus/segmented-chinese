[
    {
        "source_sentence": "佛說護身命經一卷",
        "from": "T85n2865_001:1325a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "界　比丘道真",
        "from": "T85n2865_001:1325a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說救護身命經濟人疾病苦厄",
        "from": "T85n2865_001:1325a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛在娑羅雙樹間臨般涅槃時。",
        "from": "T85n2865_001:1325a08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利弗阿難及無量無邊諸大菩薩諸大弟子一切諸天皆來集會。",
        "from": "T85n2865_001:1325a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛還正坐告阿難言。",
        "from": "T85n2865_001:1325a10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾當入般涅槃。",
        "from": "T85n2865_001:1325a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我滅度後五濁惡世中。",
        "from": "T85n2865_001:1325a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生當為惡魔惡鬼嬈擾。",
        "from": "T85n2865_001:1325a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吸人精氣者求人長短者橫來殺人者。",
        "from": "T85n2865_001:1325a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難汝好懃流布此經。",
        "from": "T85n2865_001:1325a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令惡世中一切眾生無有病苦。",
        "from": "T85n2865_001:1325a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有橫死。",
        "from": "T85n2865_001:1325a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾邪蠱毒悉皆消滅。",
        "from": "T85n2865_001:1325a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難我所囑消者唯有此經。",
        "from": "T85n2865_001:1325a16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾無男無女。",
        "from": "T85n2865_001:1325a16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有能讀誦此經一句一偈者。",
        "from": "T85n2865_001:1325a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾邪惡鬼不得忌近。",
        "from": "T85n2865_001:1325a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若在曠野中。",
        "from": "T85n2865_001:1325a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若在急難中。",
        "from": "T85n2865_001:1325a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若在大火中若在大水中。",
        "from": "T85n2865_001:1325a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常當讀誦是經。",
        "from": "T85n2865_001:1325a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能悉斷除。",
        "from": "T85n2865_001:1325a19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故。",
        "from": "T85n2865_001:1325a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經有大威神力故常讀之。",
        "from": "T85n2865_001:1325a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不能讀者。",
        "from": "T85n2865_001:1325a20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但當書寫安著懷中至心受持。",
        "from": "T85n2865_001:1325a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是經者則持過去未來現在諸佛神力。",
        "from": "T85n2865_001:1325a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲遠行常當持去。",
        "from": "T85n2865_001:1325a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸惡毒獸無能近者。",
        "from": "T85n2865_001:1325a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若到處處村落。",
        "from": "T85n2865_001:1325a23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心為人演說有能須申。",
        "from": "T85n2865_001:1325a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聽者所願悉得。",
        "from": "T85n2865_001:1325a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難佛不虛言。",
        "from": "T85n2865_001:1325a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經佛所祕要甚難可得。",
        "from": "T85n2865_001:1325a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如妙藥。",
        "from": "T85n2865_001:1325a25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能愈毒病。",
        "from": "T85n2865_001:1325a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能辟毒氣。",
        "from": "T85n2865_001:1325a26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能斷惡毒。",
        "from": "T85n2865_001:1325a26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有人將行者。",
        "from": "T85n2865_001:1325a26_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸惡毒虫眾邪蠱毒欲來侵害。",
        "from": "T85n2865_001:1325a27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞此藥氣四向散去不敢迴視。",
        "from": "T85n2865_001:1325a28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經亦復如是。",
        "from": "T85n2865_001:1325a28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有病痛者。",
        "from": "T85n2865_001:1325a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當淨洗浴一心讀誦。",
        "from": "T85n2865_001:1325a29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾患消除。",
        "from": "T85n2865_001:1325a29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告阿難。",
        "from": "T85n2865_001:1325a29_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有惡魔眾邪蠱毒不隨我語者。",
        "from": "T85n2865_001:1325b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當使此魔遭眾邪蠱毒。",
        "from": "T85n2865_001:1325b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如押油殃悉皆消滅無有遺餘。",
        "from": "T85n2865_001:1325b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛即舉七佛名字。",
        "from": "T85n2865_001:1325b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一維衛佛。",
        "from": "T85n2865_001:1325b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二式佛。",
        "from": "T85n2865_001:1325b03_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三隨葉佛。",
        "from": "T85n2865_001:1325b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四拘樓秦佛。",
        "from": "T85n2865_001:1325b04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五拘那含牟尼佛。",
        "from": "T85n2865_001:1325b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六迦葉佛。",
        "from": "T85n2865_001:1325b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第七釋迦牟尼佛。",
        "from": "T85n2865_001:1325b05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有苦厄病痛者。",
        "from": "T85n2865_001:1325b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便當讀誦此七佛名字。",
        "from": "T85n2865_001:1325b06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸惡蠱毒悉皆消滅無能侵近。",
        "from": "T85n2865_001:1325b07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說此經已。",
        "from": "T85n2865_001:1325b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復告阿難言。",
        "from": "T85n2865_001:1325b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今復憐愍眾生故。",
        "from": "T85n2865_001:1325b08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便當更說六神名字。",
        "from": "T85n2865_001:1325b08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名波奈羅。",
        "from": "T85n2865_001:1325b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二名迦奈羅。",
        "from": "T85n2865_001:1325b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三名禪吒迦。",
        "from": "T85n2865_001:1325b09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四名勤迦。",
        "from": "T85n2865_001:1325b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五名摩頭。",
        "from": "T85n2865_001:1325b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六名摩祁。",
        "from": "T85n2865_001:1325b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是六神名字。",
        "from": "T85n2865_001:1325b10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難若有眾生。",
        "from": "T85n2865_001:1325b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無男無女無貴無賤有苦厄者。",
        "from": "T85n2865_001:1325b11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆當稱說六神名字。",
        "from": "T85n2865_001:1325b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所患消除。",
        "from": "T85n2865_001:1325b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾惡役氣不得來近。",
        "from": "T85n2865_001:1325b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切滅盡無有遺餘。",
        "from": "T85n2865_001:1325b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告無量無邊諸大菩薩摩訶薩及天神王一切天人等。",
        "from": "T85n2865_001:1325b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我滅度後。",
        "from": "T85n2865_001:1325b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有受持我囑法者。",
        "from": "T85n2865_001:1325b15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等常當晝夜擁護令得安隱。",
        "from": "T85n2865_001:1325b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊師利菩薩白佛言。",
        "from": "T85n2865_001:1325b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我當於佛滅度後。",
        "from": "T85n2865_001:1325b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將二十五菩薩。",
        "from": "T85n2865_001:1325b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於惡世中有能讀誦此經處。",
        "from": "T85n2865_001:1325b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等晝夜在其左右擁護是人。",
        "from": "T85n2865_001:1325b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾邪魍魎不得來近。",
        "from": "T85n2865_001:1325b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常使是人臥覺俱安。",
        "from": "T85n2865_001:1325b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行善法。",
        "from": "T85n2865_001:1325b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛讚文殊師利等。",
        "from": "T85n2865_001:1325b20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉。",
        "from": "T85n2865_001:1325b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝能擁護我百千萬億劫中所可修集阿耨多羅三藐三菩提心。",
        "from": "T85n2865_001:1325b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時四天神王偏袒右肩右膝著地。",
        "from": "T85n2865_001:1325b23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心合掌白佛言世尊。",
        "from": "T85n2865_001:1325b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我常於如來滅後。",
        "from": "T85n2865_001:1325b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各將眷屬案行國界。",
        "from": "T85n2865_001:1325b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有能讀誦書寫受持是經者。",
        "from": "T85n2865_001:1325b25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等眷屬常來隨逐是人。",
        "from": "T85n2865_001:1325b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晝夜擁護令不見惡。",
        "from": "T85n2865_001:1325b26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人欲行曠野中。",
        "from": "T85n2865_001:1325b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我常隨逐是人導從前從。",
        "from": "T85n2865_001:1325b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勤心擁護不離是人。",
        "from": "T85n2865_001:1325b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不令惡魔忌來侵近。",
        "from": "T85n2865_001:1325b28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常得充足不令飢渴無所之少。",
        "from": "T85n2865_001:1325b29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可欲求者我等神王悉令供給。",
        "from": "T85n2865_001:1325c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如其所願無所之少。",
        "from": "T85n2865_001:1325c01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故。",
        "from": "T85n2865_001:1325c01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人能流布此經故。",
        "from": "T85n2865_001:1325c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行善法供養三寶故。",
        "from": "T85n2865_001:1325c02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不令斷絕。",
        "from": "T85n2865_001:1325c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時乾闥婆王阿修羅迦樓羅緊那羅摩睺羅伽人非人等。",
        "from": "T85n2865_001:1325c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各各𧿟跪於如來前。",
        "from": "T85n2865_001:1325c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心合掌白佛言。",
        "from": "T85n2865_001:1325c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我等天人常當飛行　於惡世中間有讀誦書寫受持是經者。",
        "from": "T85n2865_001:1325c07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等天人與其眷屬共到是人所住之處。",
        "from": "T85n2865_001:1325c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聽受此經法。",
        "from": "T85n2865_001:1325c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常當守護晝夜不離在其四面。",
        "from": "T85n2865_001:1325c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擁護是人。",
        "from": "T85n2865_001:1325c09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾魔惡鬼不得侵近。",
        "from": "T85n2865_001:1325c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得奪其精氣。",
        "from": "T85n2865_001:1325c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得橫來絕命。",
        "from": "T85n2865_001:1325c10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得橫來嬈害。",
        "from": "T85n2865_001:1325c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得求其長短。",
        "from": "T85n2865_001:1325c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得觸犯。",
        "from": "T85n2865_001:1325c11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令毒氣不行。",
        "from": "T85n2865_001:1325c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等眷屬常來在是人所住之處。",
        "from": "T85n2865_001:1325c12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我於空中。",
        "from": "T85n2865_001:1325c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人若遇大火。",
        "from": "T85n2865_001:1325c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等眷屬隨其方便擁護其身不令火燒。",
        "from": "T85n2865_001:1325c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遇大水。",
        "from": "T85n2865_001:1325c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "急駛𣿖去。",
        "from": "T85n2865_001:1325c14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等眷屬即於空中來接是人。",
        "from": "T85n2865_001:1325c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令不見溺水。",
        "from": "T85n2865_001:1325c15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即還停得度水難。",
        "from": "T85n2865_001:1325c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遇大賊。",
        "from": "T85n2865_001:1325c16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我四面救護是人。",
        "from": "T85n2865_001:1325c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令賊心刀杖不舉即發慈心。",
        "from": "T85n2865_001:1325c17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遇官法繫縛枷鎖晝夜愁苦。",
        "from": "T85n2865_001:1325c18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等眷屬於虛空中。",
        "from": "T85n2865_001:1325c18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令其官心生歡喜悉令放流皆得解脫。",
        "from": "T85n2865_001:1325c19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等眷屬一心救護。",
        "from": "T85n2865_001:1325c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不令他人而得嬈亂。",
        "from": "T85n2865_001:1325c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於無量無邊劫中。",
        "from": "T85n2865_001:1325c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常念此經。",
        "from": "T85n2865_001:1325c21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故。",
        "from": "T85n2865_001:1325c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經世尊慇懃所囑之法。",
        "from": "T85n2865_001:1325c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令久流布。",
        "from": "T85n2865_001:1325c22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛復讚諸天人等。",
        "from": "T85n2865_001:1325c22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉。",
        "from": "T85n2865_001:1325c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等眷屬曾於阿僧祇劫中。",
        "from": "T85n2865_001:1325c23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "值遇百千萬億諸佛。",
        "from": "T85n2865_001:1325c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復能擁護我百千萬億劫中弟子。",
        "from": "T85n2865_001:1325c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令流布此經。",
        "from": "T85n2865_001:1325c25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讀誦書寫方便救濟。",
        "from": "T85n2865_001:1325c25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不令見惡。",
        "from": "T85n2865_001:1325c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常行善心。",
        "from": "T85n2865_001:1325c26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時乾闥婆王等各與眷屬。",
        "from": "T85n2865_001:1325c26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂禮佛足一心奉行。",
        "from": "T85n2865_001:1325c27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告阿難。",
        "from": "T85n2865_001:1325c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾以右手摩汝頂上。",
        "from": "T85n2865_001:1325c28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝好用心。",
        "from": "T85n2865_001:1325c28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾所囑累唯有此經。",
        "from": "T85n2865_001:1325c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難懃流布此經。",
        "from": "T85n2865_001:1325c29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令一切眾生悉得聞知。",
        "from": "T85n2865_001:1326a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難汝最是吾心中弟子。",
        "from": "T85n2865_001:1326a01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我所出法悉付囑汝等。",
        "from": "T85n2865_001:1326a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾今憐愍一切眾生故。",
        "from": "T85n2865_001:1326a02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲令解脫故。",
        "from": "T85n2865_001:1326a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛還正坐付囑此法。",
        "from": "T85n2865_001:1326a03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難此經尊猛極有威神。",
        "from": "T85n2865_001:1326a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勸令一切族姓男女供養香花雜綵燃燈續明。",
        "from": "T85n2865_001:1326a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復能流轉讀誦。",
        "from": "T85n2865_001:1326a05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆救人疾苦厄之者。",
        "from": "T85n2865_001:1326a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現世安吉。",
        "from": "T85n2865_001:1326a06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將來往生無量壽國即生蓮華軀體金色身相具足。",
        "from": "T85n2865_001:1326a07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智慧勇健如上輩者。",
        "from": "T85n2865_001:1326a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功德如是不可稱計。",
        "from": "T85n2865_001:1326a08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告阿難。",
        "from": "T85n2865_001:1326a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用好紙好筆好墨。",
        "from": "T85n2865_001:1326a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至心書寫我所出法。",
        "from": "T85n2865_001:1326a09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上下句偈如佛所說。",
        "from": "T85n2865_001:1326a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無令妄失一畵一點。",
        "from": "T85n2865_001:1326a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難我憐愍眾生故。",
        "from": "T85n2865_001:1326a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯囑汝法。",
        "from": "T85n2865_001:1326a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆令一切有形之類悉得聞知。",
        "from": "T85n2865_001:1326a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心開意解常行善法。",
        "from": "T85n2865_001:1326a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時阿難在世尊前一心合掌。",
        "from": "T85n2865_001:1326a13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身毛悉竪戰戰兢兢。",
        "from": "T85n2865_001:1326a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心諦聽佛語不敢忘失一句一偈。",
        "from": "T85n2865_001:1326a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流淚而言。",
        "from": "T85n2865_001:1326a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊所囑至心受持。",
        "from": "T85n2865_001:1326a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣令流布此經。",
        "from": "T85n2865_001:1326a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難復言。",
        "from": "T85n2865_001:1326a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受天尊教。",
        "from": "T85n2865_001:1326a16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂禮佛足一心奉行。",
        "from": "T85n2865_001:1326a16_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修福受樂報　　所欲皆自然",
        "from": "T85n2865_001:1326a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "超然生死流　　上言至涅槃",
        "from": "T85n2865_001:1326a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人好為福　　天神自然護",
        "from": "T85n2865_001:1326a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所願皆自然　　眾魔不得壞",
        "from": "T85n2865_001:1326a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薄福多諸惱　　福能消眾患",
        "from": "T85n2865_001:1326a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福德既牢強　　速成堅固定",
        "from": "T85n2865_001:1326a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生天受快樂　　人中亦自在",
        "from": "T85n2865_001:1326a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斯由功德故　　所往皆自然",
        "from": "T85n2865_001:1326a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因此福方便　　永離生死苦",
        "from": "T85n2865_001:1326a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得□至涅槃　　不滅復不生",
        "from": "T85n2865_001:1326a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救護身命經一卷",
        "from": "T85n2865_001:1326a27_100",
        "context_with_next_sentence": false
    }
]