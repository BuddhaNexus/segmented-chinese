[
    {
        "source_sentence": "府君存惠傳",
        "from": "T85n2860_001:1320a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "府君諱存惠。字察遠。號懷氣義。夙抱溫案文",
        "from": "T85n2860_001:1320a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "武之道。相兼寬猛之誠。共濟善乘鞍馬弓開",
        "from": "T85n2860_001:1320a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而猿猴先啼頗曉陣圖施設而縱檎自在。",
        "from": "T85n2860_001:1320a13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故得入於儕輩。",
        "from": "T85n2860_001:1320a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "折旋以越於常倫。",
        "from": "T85n2860_001:1320a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡居□寮起就。",
        "from": "T85n2860_001:1320a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獨彰於群彥。",
        "from": "T85n2860_001:1320a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僉諧眾□舉。",
        "from": "T85n2860_001:1320a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薦人多尋沐□君主慎求。",
        "from": "T85n2860_001:1320a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂乃起昇班袟不注。",
        "from": "T85n2860_001:1320a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "司局清畏人知。",
        "from": "T85n2860_001:1320a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讓寶越於前賢。",
        "from": "T85n2860_001:1320a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知足過於後輩。",
        "from": "T85n2860_001:1320a17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將謂轅門之內分。",
        "from": "T85n2860_001:1320a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□君主之□憂州府之中。",
        "from": "T85n2860_001:1320a18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設機謀之佐國。",
        "from": "T85n2860_001:1320a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈期地火暗背。",
        "from": "T85n2860_001:1320a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "靈性歸常空。",
        "from": "T85n2860_001:1320a19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "留白王之肌膚。",
        "from": "T85n2860_001:1320a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不聞黃金之美語。",
        "from": "T85n2860_001:1320a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妻居孀室血淚交流。",
        "from": "T85n2860_001:1320a20_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此世難遇於魚顋。",
        "from": "T85n2860_001:1320a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別後須憑於鳥字。",
        "from": "T85n2860_001:1320a21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂請丹筆。",
        "from": "T85n2860_001:1320a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輒會容儀。",
        "from": "T85n2860_001:1320a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又邀儒生以讚芳美。",
        "from": "T85n2860_001:1320a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃讚曰。",
        "from": "T85n2860_001:1320a22_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "府君天然貌。",
        "from": "T85n2860_001:1320a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神理有多般。",
        "from": "T85n2860_001:1320a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入眾人皆敬。",
        "from": "T85n2860_001:1320a23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出君他比難。",
        "from": "T85n2860_001:1320a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文請孔氏學。",
        "from": "T85n2860_001:1320a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "武拜楚王壇。",
        "from": "T85n2860_001:1320a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "榮祿君恩重。",
        "from": "T85n2860_001:1320a24_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功勞自有千。",
        "from": "T85n2860_001:1320a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彈絃五音足。",
        "from": "T85n2860_001:1320a25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詩唱四聲全。",
        "from": "T85n2860_001:1320a25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "綜核於州府。",
        "from": "T85n2860_001:1320a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神謀著在邊。",
        "from": "T85n2860_001:1320a26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈期逝水早。",
        "from": "T85n2860_001:1320a26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "暗地鬼來先。",
        "from": "T85n2860_001:1320a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堂上空有步。",
        "from": "T85n2860_001:1320a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庭前見沒緣。",
        "from": "T85n2860_001:1320a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "官寮皆動哭。",
        "from": "T85n2860_001:1320a27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妻室又悲前。",
        "from": "T85n2860_001:1320a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "邀盡生前貌。",
        "from": "T85n2860_001:1320a28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貴徒後人看。",
        "from": "T85n2860_001:1320a28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他年蒿里下。",
        "from": "T85n2860_001:1320a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水鎮向黃泉。",
        "from": "T85n2860_001:1320a29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳥字須憑遠。",
        "from": "T85n2860_001:1320a29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蟲文輒要傳。",
        "from": "T85n2860_001:1320b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "府君諱存惠。",
        "from": "T85n2860_001:1320b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "字察遠。",
        "from": "T85n2860_001:1320b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其先著姓。",
        "from": "T85n2860_001:1320b02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殷王武丁之派。",
        "from": "T85n2860_001:1320b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隴西伯陽之苗。",
        "from": "T85n2860_001:1320b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譜諜頗明略而言矣。",
        "from": "T85n2860_001:1320b03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而乃指樹命氏射□留芳。",
        "from": "T85n2860_001:1320b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白馬東來。",
        "from": "T85n2860_001:1320b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "青牛西去。",
        "from": "T85n2860_001:1320b04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因化胡國以留子孫。",
        "from": "T85n2860_001:1320b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至□今為燉煌人也。",
        "from": "T85n2860_001:1320b05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曾祖墨釐軍事守。",
        "from": "T85n2860_001:1320b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瓜州刺史。",
        "from": "T85n2860_001:1320b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金紫光祿大夫。",
        "from": "T85n2860_001:1320b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "檢校尚書。",
        "from": "T85n2860_001:1320b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左僕射。",
        "from": "T85n2860_001:1320b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼御史大夫上。",
        "from": "T85n2860_001:1320b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "柱國諱皇祖。",
        "from": "T85n2860_001:1320b07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "管內都計使。",
        "from": "T85n2860_001:1320b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "銀青光祿大夫。",
        "from": "T85n2860_001:1320b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "檢校。",
        "from": "T85n2860_001:1320b08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兵部。",
        "from": "T85n2860_001:1320b08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常侍。",
        "from": "T85n2860_001:1320b08_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼御史大夫上。",
        "from": "T85n2860_001:1320b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "柱國諱紹丘。",
        "from": "T85n2860_001:1320b09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皇考歸義軍。",
        "from": "T85n2860_001:1320b09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "節度都頭攝。",
        "from": "T85n2860_001:1320b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "石城鎮遏使。",
        "from": "T85n2860_001:1320b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "銀青光祿大夫。",
        "from": "T85n2860_001:1320b10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "檢校。",
        "from": "T85n2860_001:1320b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左散騎常侍。",
        "from": "T85n2860_001:1320b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上騎。",
        "from": "T85n2860_001:1320b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "都尉。",
        "from": "T85n2860_001:1320b11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諱安　亡男。",
        "from": "T85n2860_001:1320b11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內親從都頭知右左霜。",
        "from": "T85n2860_001:1320b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬步軍都校練使。",
        "from": "T85n2860_001:1320b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "檢校。",
        "from": "T85n2860_001:1320b12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兵部尚書。",
        "from": "T85n2860_001:1320b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼御史大夫上。",
        "from": "T85n2860_001:1320b13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "柱國陰住延皇妣小娘子。",
        "from": "T85n2860_001:1320b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "武威郡陰氏　亡叔。",
        "from": "T85n2860_001:1320b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "節度押催錄。",
        "from": "T85n2860_001:1320b14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "青光祿大夫。",
        "from": "T85n2860_001:1320b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "檢校。",
        "from": "T85n2860_001:1320b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國祭酒。",
        "from": "T85n2860_001:1320b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼御史中承。",
        "from": "T85n2860_001:1320b15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諱安定　亡男。",
        "from": "T85n2860_001:1320b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內親從者頭。",
        "from": "T85n2860_001:1320b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "銀青光祿大夫。",
        "from": "T85n2860_001:1320b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "檢校。",
        "from": "T85n2860_001:1320b16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右散騎常侍。",
        "from": "T85n2860_001:1320b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼御史大夫。",
        "from": "T85n2860_001:1320b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陰住千　兄釋門僧正。",
        "from": "T85n2860_001:1320b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨壇供奉大德。",
        "from": "T85n2860_001:1320b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼義學法師。",
        "from": "T85n2860_001:1320b18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賜紫沙門法定　府君武功早備。",
        "from": "T85n2860_001:1320b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文業夙彰。",
        "from": "T85n2860_001:1320b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纔伸理國之方。",
        "from": "T85n2860_001:1320b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不遂匡　君之道。",
        "from": "T85n2860_001:1320b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈期齊謌暗唱魯句。",
        "from": "T85n2860_001:1320b20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先吟魄引驚波魂隨逝水。",
        "from": "T85n2860_001:1320b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於太平興國五年庚辰歲正月乙亥朔二十六日庚子。",
        "from": "T85n2860_001:1320b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "枕疾終於修文坊之私弟矣。",
        "from": "T85n2860_001:1320b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取其年二月甲辰朔三日丙午。",
        "from": "T85n2860_001:1320b23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "權殯奠高里陽。",
        "from": "T85n2860_001:1320b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開河北原之禮也。",
        "from": "T85n2860_001:1320b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別卜年華葬于先塋矣　妹聟都頭鄧慶順□□□□　外弟都頭曹祐崇。",
        "from": "T85n2860_001:1320b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並宗連骨血義親枝。",
        "from": "T85n2860_001:1320b26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或早逝於泉臺。",
        "from": "T85n2860_001:1320b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或現存於內外。",
        "from": "T85n2860_001:1320b27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倚盧痛切牽仍弟妹之膓枕魄魂消。",
        "from": "T85n2860_001:1320b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "交流女男之淚。",
        "from": "T85n2860_001:1320b28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世事遂年華私改。",
        "from": "T85n2860_001:1320b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聲名隨日月消。",
        "from": "T85n2860_001:1320b29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亡貴憑鳥跡之文。",
        "from": "T85n2860_001:1320b29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以記龍頭之碣。",
        "from": "T85n2860_001:1320c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "銘曰。",
        "from": "T85n2860_001:1320c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天列星辰號必膺。",
        "from": "T85n2860_001:1320c01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢才風雲契會兮。",
        "from": "T85n2860_001:1320c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "君臣偶諧不入。",
        "from": "T85n2860_001:1320c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徛建兮氷清玉潔。",
        "from": "T85n2860_001:1320c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一方斷割兮列直難迴。",
        "from": "T85n2860_001:1320c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將謂村巷兮水為綜核。",
        "from": "T85n2860_001:1320c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何期風送兮魄赴夜臺。",
        "from": "T85n2860_001:1320c04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "松椒寂寂兮空覩狐兔。",
        "from": "T85n2860_001:1320c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "窀窀穸傳遠兮唯石是牌。",
        "from": "T85n2860_001:1320c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千秋記遐兮莫非鳥字。",
        "from": "T85n2860_001:1320c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬載留芳兮筆墨者哉。",
        "from": "T85n2860_001:1320c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "于時太平興國五庚辰歲二月三日題起。",
        "from": "T85n2860_001:1320c07_13",
        "context_with_next_sentence": true
    }
]