[
    {
        "source_sentence": "大聖歡喜雙身大自在天毘那夜",
        "from": "T21n1270_001:0303b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦王歸依念誦供養法",
        "from": "T21n1270_001:0303b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏沙門善無畏譯",
        "from": "T21n1270_001:0303b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大聖自在天。",
        "from": "T21n1270_001:0303b28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是摩醯首羅大自在天王。",
        "from": "T21n1270_001:0303b28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烏摩女為婦。",
        "from": "T21n1270_001:0303b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所生有三千子。",
        "from": "T21n1270_001:0303b29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其左千五百。",
        "from": "T21n1270_001:0303b29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘那夜迦王為第一。",
        "from": "T21n1270_001:0303c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行諸惡事。",
        "from": "T21n1270_001:0303c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "領十萬七千諸毘那夜迦類。",
        "from": "T21n1270_001:0303c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右千五百扇那夜迦持善天為第一。",
        "from": "T21n1270_001:0303c02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修一切善利。",
        "from": "T21n1270_001:0303c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "領十七萬八千諸福伎善持眾。",
        "from": "T21n1270_001:0303c03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此扇那夜迦王。",
        "from": "T21n1270_001:0303c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則觀音之化身也。",
        "from": "T21n1270_001:0303c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為調和彼毘那夜迦王惡行同生一類成兄弟夫婦。",
        "from": "T21n1270_001:0303c05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "示現相抱同體之形。",
        "from": "T21n1270_001:0303c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其本因緣具在大明呪賊經。",
        "from": "T21n1270_001:0303c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今為諸行者。",
        "from": "T21n1270_001:0303c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略說壇法念誦供養等次第。",
        "from": "T21n1270_001:0303c07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不知此法之者。",
        "from": "T21n1270_001:0303c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於餘尊法難得成就。",
        "from": "T21n1270_001:0303c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復多障礙。",
        "from": "T21n1270_001:0303c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故先可修此道。",
        "from": "T21n1270_001:0303c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善士善女等。",
        "from": "T21n1270_001:0303c09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲供養此天求福利者。",
        "from": "T21n1270_001:0303c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取香木樹造其形像。",
        "from": "T21n1270_001:0303c10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫婦令相抱立之。",
        "from": "T21n1270_001:0303c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身長五寸象頭人身。",
        "from": "T21n1270_001:0303c11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身著天衣及腰裳夫鼻𦜏下。",
        "from": "T21n1270_001:0303c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婦鼻𦜏上。",
        "from": "T21n1270_001:0303c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四葉為座。",
        "from": "T21n1270_001:0303c12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造像已了。",
        "from": "T21n1270_001:0303c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得換價。",
        "from": "T21n1270_001:0303c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在室房中勿置佛堂。",
        "from": "T21n1270_001:0303c13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大身呪曰。",
        "from": "T21n1270_001:0303c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無毘那臾迦舍一訶室胝謨伽沙二怛姪他三唵陀曳訶陀曳訶四毘那臾訶五毘那臾訶六陀羅臾訶七破哩陀羅臾訶八商伽羯室胝九商伽羯室多十扇胝伽羅十一娑婆訶十二此大身呪用息災愛敬之法。",
        "from": "T21n1270_001:0303c19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心中呪曰。",
        "from": "T21n1270_001:0303c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵枳里虐是呪用增益福聚之法。",
        "from": "T21n1270_001:0304a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心中心呪曰。",
        "from": "T21n1270_001:0304a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵虐虐𤙖泮吒是呪用降怨調伏之法。",
        "from": "T21n1270_001:0304a05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身呪印　二手小指無名指內相叉。",
        "from": "T21n1270_001:0304a06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指二頭指相重交竪。",
        "from": "T21n1270_001:0304a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指竝直竪。",
        "from": "T21n1270_001:0304a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名根本印。",
        "from": "T21n1270_001:0304a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此印印心額喉頂。",
        "from": "T21n1270_001:0304a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成護身法。",
        "from": "T21n1270_001:0304a08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心中呪印　右小指無名指把。",
        "from": "T21n1270_001:0304a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中指頭指相重橫竪。",
        "from": "T21n1270_001:0304a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大母指同直竪　即前身印之片印耳。",
        "from": "T21n1270_001:0304a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此印護淨供具竝沐浴油。",
        "from": "T21n1270_001:0304a11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心中心呪印　二手作金剛縛。",
        "from": "T21n1270_001:0304a12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相雙當額。",
        "from": "T21n1270_001:0304a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦呪擲之惡人怨賊等。",
        "from": "T21n1270_001:0304a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必凌伏也。",
        "from": "T21n1270_001:0304a13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一壇法　作二肘圓壇。",
        "from": "T21n1270_001:0304a14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以瞿摩夷和黃土埿塗作之。",
        "from": "T21n1270_001:0304a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而備二供。",
        "from": "T21n1270_001:0304a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂遏伽水塗香時花燒香安悉薰陸蘿葡團丸。",
        "from": "T21n1270_001:0304a16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清淨蘇密。",
        "from": "T21n1270_001:0304a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時節菓子。",
        "from": "T21n1270_001:0304a16_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乳糜餚饍。",
        "from": "T21n1270_001:0304a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供具各儲二供。",
        "from": "T21n1270_001:0304a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置壇左右。",
        "from": "T21n1270_001:0304a17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別銅鋺貯清胡麻油一升。",
        "from": "T21n1270_001:0304a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦大身呪護淨一百八遍。",
        "from": "T21n1270_001:0304a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後煖油。",
        "from": "T21n1270_001:0304a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並沐浴尊像。",
        "from": "T21n1270_001:0304a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪水呪曰。",
        "from": "T21n1270_001:0304a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿那婆折哩婆隷那訶誦七遍令沐浴第二念誦供養法　佛母心印　二手內縛當頞。",
        "from": "T21n1270_001:0304a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指竝各著頭指。",
        "from": "T21n1270_001:0304a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1270_001:0304a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵時那爾伽娑婆訶三遍次蓮華母心印　如前佛母。",
        "from": "T21n1270_001:0304a25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但出右頭指來去。",
        "from": "T21n1270_001:0304a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1270_001:0304a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿嚕里伽娑婆訶三遍次金剛母心印　印與前同。",
        "from": "T21n1270_001:0304a28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左頭指來去。",
        "from": "T21n1270_001:0304a28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1270_001:0304a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵跋折羅地利迦娑婆訶三遍次被甲印　左手作拳按腰。",
        "from": "T21n1270_001:0304b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手同拳印五處。",
        "from": "T21n1270_001:0304b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "額左右肩心喉也。",
        "from": "T21n1270_001:0304b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1270_001:0304b03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵僕入縛羅吽次護淨香水印　用跋折羅印呪。",
        "from": "T21n1270_001:0304b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵枳里枳里跋折羅吽泮吒七遍次以片根本印護淨壇中供具誦大身呪三遍。",
        "from": "T21n1270_001:0304b07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次對尊像稽首懺悔。",
        "from": "T21n1270_001:0304b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨喜勸請。",
        "from": "T21n1270_001:0304b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次發念觀。",
        "from": "T21n1270_001:0304b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此聖尊自在天。",
        "from": "T21n1270_001:0304b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即摩訶毘廬遮那如來。",
        "from": "T21n1270_001:0304b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為饒益無福眾生。",
        "from": "T21n1270_001:0304b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "權化示現此像。",
        "from": "T21n1270_001:0304b10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發此念觀歸依供養之者。",
        "from": "T21n1270_001:0304b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必得無量福聚。",
        "from": "T21n1270_001:0304b11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結地界　以根本印觸地。",
        "from": "T21n1270_001:0304b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦心中呪三遍迎之。",
        "from": "T21n1270_001:0304b12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結界四方界以前印呪任意。",
        "from": "T21n1270_001:0304b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周匝四堺即成。",
        "from": "T21n1270_001:0304b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以淨土變印呪護淨土。",
        "from": "T21n1270_001:0304b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七處七處在末可見之。",
        "from": "T21n1270_001:0304b15_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次送車輅印　二手內又。",
        "from": "T21n1270_001:0304b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即作開掌。",
        "from": "T21n1270_001:0304b16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指直申頭相柱。",
        "from": "T21n1270_001:0304b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大指撥二中指頭。",
        "from": "T21n1270_001:0304b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1270_001:0304b17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵覩嚕覩嚕吽三遍次召請印　如前根本契。",
        "from": "T21n1270_001:0304b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但以大指來去。",
        "from": "T21n1270_001:0304b19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1270_001:0304b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵波伽羅主那泥波多耶七遍次作四葉坐　二手合掌。",
        "from": "T21n1270_001:0304b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指竝。",
        "from": "T21n1270_001:0304b22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二無名指二小指相附。",
        "from": "T21n1270_001:0304b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左右中指無名指。",
        "from": "T21n1270_001:0304b23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各相著竪開。",
        "from": "T21n1270_001:0304b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成四葉坐。",
        "from": "T21n1270_001:0304b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪誦心中呪。",
        "from": "T21n1270_001:0304b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即想聖者來坐此座。",
        "from": "T21n1270_001:0304b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結空界　印呪如前以契轉上。",
        "from": "T21n1270_001:0304b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三匝即成。",
        "from": "T21n1270_001:0304b26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結火炎印　以左手密掩右手背。",
        "from": "T21n1270_001:0304b27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相重直竪二大指。",
        "from": "T21n1270_001:0304b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相去二寸許。",
        "from": "T21n1270_001:0304b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成呪曰。",
        "from": "T21n1270_001:0304b28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿三莽枳尼𤙖三遍次寂心澄念作供養法　金剛合掌誦呪。",
        "from": "T21n1270_001:0304c01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵惡娑婆訶十八遍次以尊像入著煖油鋺中。",
        "from": "T21n1270_001:0304c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自遏伽水始。",
        "from": "T21n1270_001:0304c03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一依次第作手印。",
        "from": "T21n1270_001:0304c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取擎頂禮供養之。",
        "from": "T21n1270_001:0304c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其手印二手作拳相雙。",
        "from": "T21n1270_001:0304c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其上置供器。",
        "from": "T21n1270_001:0304c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以慎誠心作意奉供　遏迦呪曰。",
        "from": "T21n1270_001:0304c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵𤙖拏里孃耶夜娑婆訶塗花燒香種種供具等印。",
        "from": "T21n1270_001:0304c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆一同也。",
        "from": "T21n1270_001:0304c08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1270_001:0304c08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵吽納里孃耶夜娑婆訶各誦三遍但獻酒印呪獻遏伽水。",
        "from": "T21n1270_001:0304c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次作摩尼供印　兩手金剛合掌。",
        "from": "T21n1270_001:0304c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指如寶形。",
        "from": "T21n1270_001:0304c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二大指即成。",
        "from": "T21n1270_001:0304c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1270_001:0304c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南謨阿莫伽部若摩尼鉢頭摩婆悉利怛他伽多毘盧吉帝三曼多波羅沙羅𤙖三遍即成遍滿法界廣大供具。",
        "from": "T21n1270_001:0304c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次唱四智讚　金剛合掌。",
        "from": "T21n1270_001:0304c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當額並左右。",
        "from": "T21n1270_001:0304c16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周轉印。",
        "from": "T21n1270_001:0304c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次唱聖者讚嘆呪三遍。",
        "from": "T21n1270_001:0304c18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印同前。",
        "from": "T21n1270_001:0304c18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵伽多波胝扇提娑婆悉胝摩訶伽多波胝娑婆訶次作根本印。",
        "from": "T21n1270_001:0304c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦大身呪七遍。",
        "from": "T21n1270_001:0304c20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印身五處。",
        "from": "T21n1270_001:0304c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮大毘廬遮那如來七遍。",
        "from": "T21n1270_001:0304c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次禮伽多鉢胝帝婆羅若七遍。",
        "from": "T21n1270_001:0304c22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次念誦。",
        "from": "T21n1270_001:0304c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以匙酌油。",
        "from": "T21n1270_001:0304c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灌其像身。",
        "from": "T21n1270_001:0304c23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日之中七百遍灌之。",
        "from": "T21n1270_001:0304c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平旦四百遍。",
        "from": "T21n1270_001:0304c24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日中三百遍。",
        "from": "T21n1270_001:0304c24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正灌油之時。",
        "from": "T21n1270_001:0304c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至心慎肅勿生他念。",
        "from": "T21n1270_001:0304c25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每一百八遍終。",
        "from": "T21n1270_001:0304c25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讚嘆供養。",
        "from": "T21n1270_001:0304c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數數發願陳所念之事。",
        "from": "T21n1270_001:0304c26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若三日乃至七日。",
        "from": "T21n1270_001:0304c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切善願隨意成就一切。",
        "from": "T21n1270_001:0304c27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡事悉皆消滅。",
        "from": "T21n1270_001:0304c27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是自在天處他化自在天。",
        "from": "T21n1270_001:0304c28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降遊下界。",
        "from": "T21n1270_001:0304c28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風輪之纏淪其身故。",
        "from": "T21n1270_001:0304c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用酒藥蘿蔔及油等之物。",
        "from": "T21n1270_001:0304c29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常持齋故暮時不受供養矣。",
        "from": "T21n1270_001:0305a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其所獻之食呪師自食。",
        "from": "T21n1270_001:0305a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色力福壽共得增益。",
        "from": "T21n1270_001:0305a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三結鼻頓合法　若三日乃至七日。",
        "from": "T21n1270_001:0305a03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作法供養聖尊。",
        "from": "T21n1270_001:0305a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於念願事無感應者。",
        "from": "T21n1270_001:0305a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更燒香發願啟白所念之由。",
        "from": "T21n1270_001:0305a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取白線誦大身呪一百八遍。",
        "from": "T21n1270_001:0305a05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪其縒線結一百八節。",
        "from": "T21n1270_001:0305a06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "截於二分結二天鼻端。",
        "from": "T21n1270_001:0305a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其印用商佉印。",
        "from": "T21n1270_001:0305a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手金剛合掌。",
        "from": "T21n1270_001:0305a07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左右頭指與大指各共相捻即成。",
        "from": "T21n1270_001:0305a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結鼻之後。",
        "from": "T21n1270_001:0305a08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三日夜間。",
        "from": "T21n1270_001:0305a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得行他處及與他人共語。",
        "from": "T21n1270_001:0305a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四調和法　其象頭人身瘻。",
        "from": "T21n1270_001:0305a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有六辟牙折。",
        "from": "T21n1270_001:0305a10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "絲鼻上。",
        "from": "T21n1270_001:0305a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手把刀。",
        "from": "T21n1270_001:0305a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次手把菓盤。",
        "from": "T21n1270_001:0305a11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下手把輪。",
        "from": "T21n1270_001:0305a11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右上手把棒。",
        "from": "T21n1270_001:0305a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次手把索。",
        "from": "T21n1270_001:0305a12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下手把牙。",
        "from": "T21n1270_001:0305a12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有忿怒之時。",
        "from": "T21n1270_001:0305a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此像之前作小圓壇。",
        "from": "T21n1270_001:0305a13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設餚饍飲供酒藥蘿蔔歡喜團等迎請。",
        "from": "T21n1270_001:0305a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手合掌屈二頭指。",
        "from": "T21n1270_001:0305a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捻大母指頭。",
        "from": "T21n1270_001:0305a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1270_001:0305a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "𤙖𤙖遮都盧破羅末陀儞曳娑婆訶七遍每遍頭指來去。",
        "from": "T21n1270_001:0305a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以至誠心禮拜懺悔。",
        "from": "T21n1270_001:0305a17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以一一供具奉獻讚嘆。",
        "from": "T21n1270_001:0305a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦大自在天呪滿一千八遍。",
        "from": "T21n1270_001:0305a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其印相。",
        "from": "T21n1270_001:0305a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二小指二無名指叉掌內。",
        "from": "T21n1270_001:0305a19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二中指頭相著。",
        "from": "T21n1270_001:0305a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指各附中指背第一節。",
        "from": "T21n1270_001:0305a20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1270_001:0305a20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵毘多羅薩儞破賴摩羅他尼只多尼只多尼毘多尼毘多尼娑婆訶供養已了陳所念事。",
        "from": "T21n1270_001:0305a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取出壇中供具初夜之分。",
        "from": "T21n1270_001:0305a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外作水壇。",
        "from": "T21n1270_001:0305a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以其供等和清水散水壇。",
        "from": "T21n1270_001:0305a24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及擲灑諸方。",
        "from": "T21n1270_001:0305a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1270_001:0305a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿藹捨伽舍鉢車第五成辦一切事業法　若求曷闍等愛念者。",
        "from": "T21n1270_001:0305a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒却布羅香。",
        "from": "T21n1270_001:0305a28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三日夜。",
        "from": "T21n1270_001:0305a28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設壇供等誦婆伽羅拏呪。",
        "from": "T21n1270_001:0305a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日日三時時別千遍。",
        "from": "T21n1270_001:0305a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印法。",
        "from": "T21n1270_001:0305a29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左右小指無名指。",
        "from": "T21n1270_001:0305b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相叉入掌內。",
        "from": "T21n1270_001:0305b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指直竪頭相合開莫著。",
        "from": "T21n1270_001:0305b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭二大指押無名指。",
        "from": "T21n1270_001:0305b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印七處。",
        "from": "T21n1270_001:0305b02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂二膊咽下眉間髮際頂頭後。",
        "from": "T21n1270_001:0305b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1270_001:0305b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵毘那臾伽扇那夜提婆伽羅拏耶娑婆訶若欲降伏惡人者。",
        "from": "T21n1270_001:0305b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自脚足下書其名字。",
        "from": "T21n1270_001:0305b05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日夜不飲食。",
        "from": "T21n1270_001:0305b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作禁縛印。",
        "from": "T21n1270_001:0305b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦阿毘遮呪。",
        "from": "T21n1270_001:0305b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時別一千遍。",
        "from": "T21n1270_001:0305b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印相二手合掌伏之。",
        "from": "T21n1270_001:0305b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指直申頭相合。",
        "from": "T21n1270_001:0305b07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二名指屈。",
        "from": "T21n1270_001:0305b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指二小指申頭相合。",
        "from": "T21n1270_001:0305b08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大指按二中指上。",
        "from": "T21n1270_001:0305b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二頭指各屈大指上。",
        "from": "T21n1270_001:0305b09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指上其大指頭指頭各動。",
        "from": "T21n1270_001:0305b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用唵伽那波胝阿毘姤盧舍娑婆訶。",
        "from": "T21n1270_001:0305b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲停口舌者。",
        "from": "T21n1270_001:0305b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取井花水。",
        "from": "T21n1270_001:0305b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦大身呪一百遍。",
        "from": "T21n1270_001:0305b12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪水散擲彼方。",
        "from": "T21n1270_001:0305b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口舌必止。",
        "from": "T21n1270_001:0305b13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡諸念求事。",
        "from": "T21n1270_001:0305b13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆以身呪心中等三呪隨意修念焉。",
        "from": "T21n1270_001:0305b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間事業無不成辦是為世法要道也。",
        "from": "T21n1270_001:0305b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人倫祕術無功頓法豈過於此哉。",
        "from": "T21n1270_001:0305b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今搜求祕教奧藏傳於末後。",
        "from": "T21n1270_001:0305b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願也有心之哲。",
        "from": "T21n1270_001:0305b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宜試慎勿妄傳矣。",
        "from": "T21n1270_001:0305b17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　　　　　豐山寓居沙門快道誌",
        "from": "T21n1270_001:0305b21_0",
        "context_with_next_sentence": true
    }
]