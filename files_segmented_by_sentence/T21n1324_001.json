[
    {
        "source_sentence": "能淨一切眼疾病陀羅尼經",
        "from": "T21n1324_001:0490a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T21n1324_001:0490a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空諡大鑒",
        "from": "T21n1324_001:0490a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正號大廣智大興善寺三藏沙門不空奉　詔譯如是我聞。",
        "from": "T21n1324_001:0490a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時薄伽梵。",
        "from": "T21n1324_001:0490a12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住迦毘羅衛國。",
        "from": "T21n1324_001:0490a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋迦種族聚落。",
        "from": "T21n1324_001:0490a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時有一釋種。",
        "from": "T21n1324_001:0490a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住車尼摩迦聚落。",
        "from": "T21n1324_001:0490a13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於佛淨信於法淨信於僧淨信。",
        "from": "T21n1324_001:0490a14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸依於佛歸依於法歸依於僧。",
        "from": "T21n1324_001:0490a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不疑於佛不疑於法不疑於僧。",
        "from": "T21n1324_001:0490a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡心於佛盡心於法盡心於僧。",
        "from": "T21n1324_001:0490a16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "決定於等覺勝趣。",
        "from": "T21n1324_001:0490a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人眼所見色相而不得見。",
        "from": "T21n1324_001:0490a17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時乞曬摩迦釋種。",
        "from": "T21n1324_001:0490a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憶念如來作如是言。",
        "from": "T21n1324_001:0490a18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稽首佛世尊智炬陀羅尼能作光明者。",
        "from": "T21n1324_001:0490a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命善逝大悲者。",
        "from": "T21n1324_001:0490a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護念攝受我令我眼清淨。",
        "from": "T21n1324_001:0490a20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊超越世間耳眼。",
        "from": "T21n1324_001:0490a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以天耳聞以天眼見。",
        "from": "T21n1324_001:0490a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告阿難陀言。",
        "from": "T21n1324_001:0490a22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝往於釋種所。",
        "from": "T21n1324_001:0490a22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此陀羅尼明加護令淨其眼。",
        "from": "T21n1324_001:0490a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼拔濟令彼攝受。",
        "from": "T21n1324_001:0490a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼長養令彼結界。",
        "from": "T21n1324_001:0490a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼眼無垢翳得離疾病。",
        "from": "T21n1324_001:0490a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣令流布四部眾苾芻苾芻尼優婆塞優婆夷及餘有情。",
        "from": "T21n1324_001:0490a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1324_001:0490a26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也二合他呬里弭里黎枳呬里係帝護庾護庾護也麼寧護魯護魯怒魯怒魯娑嚩二合引訶阿難陀此陀羅尼明王。",
        "from": "T21n1324_001:0490b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼垢風垢黃病痰病三集病。",
        "from": "T21n1324_001:0490b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我及某甲眼勿令痛勿令流淚。",
        "from": "T21n1324_001:0490b02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以羅漢實語禁戒實語。",
        "from": "T21n1324_001:0490b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以苦行實語以諸仙實語。",
        "from": "T21n1324_001:0490b03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以緣生實語。",
        "from": "T21n1324_001:0490b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦實語集實語。",
        "from": "T21n1324_001:0490b04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅實語道實語。",
        "from": "T21n1324_001:0490b04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辟支佛實語。",
        "from": "T21n1324_001:0490b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我某甲願令眼清淨。",
        "from": "T21n1324_001:0490b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七佛等覺已說。",
        "from": "T21n1324_001:0490b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我釋迦牟尼應供正遍知今說。",
        "from": "T21n1324_001:0490b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四大天王亦說天帝釋亦說。",
        "from": "T21n1324_001:0490b07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娑訶世界主梵王亦說。",
        "from": "T21n1324_001:0490b07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難陀我不見天世魔世沙門婆羅門趣。",
        "from": "T21n1324_001:0490b08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持此淨眼陀羅尼者。",
        "from": "T21n1324_001:0490b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "患眼翳瞙浮暈。",
        "from": "T21n1324_001:0490b09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂令眼天作龍作。",
        "from": "T21n1324_001:0490b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥叉作羅剎作羅剎女作。",
        "from": "T21n1324_001:0490b10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必舍支女作。",
        "from": "T21n1324_001:0490b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳩槃荼作鳩槃荼女作。",
        "from": "T21n1324_001:0490b11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起屍鬼作人厭禱作梵志厭禱作。",
        "from": "T21n1324_001:0490b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無敢違越無不應効。",
        "from": "T21n1324_001:0490b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具壽阿難陀汝今受此陀羅尼。",
        "from": "T21n1324_001:0490b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將往釋種聚落。",
        "from": "T21n1324_001:0490b13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "授與乞曬麼迦。",
        "from": "T21n1324_001:0490b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳我語令晝三時夜三時誦持此陀羅尼。",
        "from": "T21n1324_001:0490b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其阿難陀至彼授與乞曬麼迦。",
        "from": "T21n1324_001:0490b15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乞曬麼迦纔聞此陀羅尼已。",
        "from": "T21n1324_001:0490b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其眼脈已淨。",
        "from": "T21n1324_001:0490b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼目得見離一切諸垢。",
        "from": "T21n1324_001:0490b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊說是經已。",
        "from": "T21n1324_001:0490b17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天人阿修羅乾闥婆等。",
        "from": "T21n1324_001:0490b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛所說歡喜奉行。",
        "from": "T21n1324_001:0490b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能淨一切眼疾病陀羅尼經",
        "from": "T21n1324_001:0490b19_100",
        "context_with_next_sentence": false
    }
]