[
    {
        "source_sentence": "佛說蓮華眼陀羅尼經",
        "from": "T21n1411_001:0926b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試鴻臚少卿",
        "from": "T21n1411_001:0926b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳法大師臣施護奉　詔譯",
        "from": "T21n1411_001:0926b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨引囉怛曩二合怛囉二合夜引野曩麼阿引哩也二合鉢納摩二合儞引怛囉二合引野怛他引誐哆引野曩謨薩哩嚩二合儞嚩囉拏尾瑟迦二合鼻尼󰂛地薩怛嚩二合野摩賀引薩怛嚩二合野怛儞也二合他引唵引哆囉哆囉底哩底哩覩嚕覩嚕迦羅迦羅吉隷吉隷俱嚕俱嚕婆囉婆囉鼻哩鼻哩部嚕部嚕曩野曩野俱嚕俱嚕謨乞叉二合引播野娑嚩二合引賀引入嚩二合羅曩嚩呬儞二合儞鼻引娑嚩二合引賀引薩哩嚩二合怛他引誐哆引底瑟恥二合帝引娑嚩二合引賀麼麼薩哩嚩二合薩怛嚩二合難左娑嚩二合賀引此蓮華眼陀羅尼。",
        "from": "T21n1411_001:0926b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人於其晨朝恭敬供養。",
        "from": "T21n1411_001:0926b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及念此陀羅尼一百八遍。",
        "from": "T21n1411_001:0926b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日二日乃至三七日志心持誦。",
        "from": "T21n1411_001:0926b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切罪障悉皆除滅。",
        "from": "T21n1411_001:0926b23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不患其眼病耳病鼻舌身病。",
        "from": "T21n1411_001:0926b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心不邪亂。",
        "from": "T21n1411_001:0926b24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復得五種眼清淨五種鼻清淨。",
        "from": "T21n1411_001:0926b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五種舌清淨五種意清淨。",
        "from": "T21n1411_001:0926b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身相端直心離垢染。",
        "from": "T21n1411_001:0926b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切生中恒發菩提心。",
        "from": "T21n1411_001:0926b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與諸聖眾常得見佛。",
        "from": "T21n1411_001:0926b27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而聽妙法直至菩提道場圓滿正覺。",
        "from": "T21n1411_001:0926b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說蓮華眼陀羅尼經",
        "from": "T21n1411_001:0926b29_100",
        "context_with_next_sentence": false
    }
]