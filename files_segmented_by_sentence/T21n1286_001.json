[
    {
        "source_sentence": "堅牢地天儀軌",
        "from": "T21n1286_001:0354b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中天竺國大那蘭陀寺善無畏奉　詔譯",
        "from": "T21n1286_001:0354b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名輸娑迦羅三藏",
        "from": "T21n1286_001:0354b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時堅牢地天及大功德天。",
        "from": "T21n1286_001:0354b23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱共從座而起白佛言。",
        "from": "T21n1286_001:0354b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "T21n1286_001:0354b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有比丘比丘尼優婆塞優婆夷童男童女等常攝五根繫念至心我地天及功德天。",
        "from": "T21n1286_001:0354b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮拜恭敬供養及念誦我真言者。",
        "from": "T21n1286_001:0354b26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我恒常出地味資潤彼人。",
        "from": "T21n1286_001:0354b27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令其身中增益壽命。",
        "from": "T21n1286_001:0354b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是地精氣充溢。",
        "from": "T21n1286_001:0354b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者身中得色力。",
        "from": "T21n1286_001:0354b28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得念得喜得精進。",
        "from": "T21n1286_001:0354b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得大智慧得辯財。",
        "from": "T21n1286_001:0354b29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得三明六通。",
        "from": "T21n1286_001:0354c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人天愛敬得無比無盡大福德。",
        "from": "T21n1286_001:0354c01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如輪王聖主福德。",
        "from": "T21n1286_001:0354c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得在在處處行者所住之處。",
        "from": "T21n1286_001:0354c02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如忉利天園觀。",
        "from": "T21n1286_001:0354c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此生彼生及千生萬生無極。",
        "from": "T21n1286_001:0354c03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得巨多廣大福。",
        "from": "T21n1286_001:0354c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得日日千萬眷屬掬養。",
        "from": "T21n1286_001:0354c04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦無量無數有情艱難之類。",
        "from": "T21n1286_001:0354c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "活備資生庫倉充溢七珍無限。",
        "from": "T21n1286_001:0354c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "象寶馬寶及輪寶隨心女寶。",
        "from": "T21n1286_001:0354c06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即不呼自來限死生。",
        "from": "T21n1286_001:0354c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是法行比丘。",
        "from": "T21n1286_001:0354c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或以種種金銀寶珠種種資生種種穀米具足供給。",
        "from": "T21n1286_001:0354c08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是繫念從初夜至中後夜。",
        "from": "T21n1286_001:0354c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐臥行立念法心無彼外緣。",
        "from": "T21n1286_001:0354c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者為營寺舍或造一屋或一樹林或復衣食坐臥床鋪。",
        "from": "T21n1286_001:0354c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於病患湯藥種種資生常供給之。",
        "from": "T21n1286_001:0354c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼得如是無乏少已。",
        "from": "T21n1286_001:0354c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身得安穩無有憂愁。",
        "from": "T21n1286_001:0354c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心常歡喜得好福田成就法器。",
        "from": "T21n1286_001:0354c13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是施已。",
        "from": "T21n1286_001:0354c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當來之世得大果報。",
        "from": "T21n1286_001:0354c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深心樂法不生惡道不造惡法。",
        "from": "T21n1286_001:0354c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我如是護彼人即得修慈悲心。",
        "from": "T21n1286_001:0354c15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是種種勝妙善根增長不減。",
        "from": "T21n1286_001:0354c16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言善哉善哉。",
        "from": "T21n1286_001:0354c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是汝等精進之心。",
        "from": "T21n1286_001:0354c17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於大乘中速能增長福之力。",
        "from": "T21n1286_001:0354c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時地神堅牢及功德天。",
        "from": "T21n1286_001:0354c19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相與白佛言。",
        "from": "T21n1286_001:0354c19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲利益安樂故說心真言。",
        "from": "T21n1286_001:0354c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛告地神功德言。",
        "from": "T21n1286_001:0354c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等善哉為利益一切眾生欲說神呪。",
        "from": "T21n1286_001:0354c21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨汝恣意說。",
        "from": "T21n1286_001:0354c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1286_001:0354c22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼多母馱南一唵二鉢羅二合婆三囉怛儞四設哩三曼多五阿馱阿馱六阿豆阿豆七比里底毘比里底毘曳八莎訶若有人誦持此呪滿足十萬遍。",
        "from": "T21n1286_001:0354c26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為最初法。",
        "from": "T21n1286_001:0354c26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦呪供養地天者。",
        "from": "T21n1286_001:0354c27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "富貴無極七珍無限。",
        "from": "T21n1286_001:0354c27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速成一切悉地。",
        "from": "T21n1286_001:0354c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又次說根本身印。",
        "from": "T21n1286_001:0354c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌以二頭指及二小指叉在掌中。",
        "from": "T21n1286_001:0355a01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中二無名直立頭相拄。",
        "from": "T21n1286_001:0355a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大指並立。",
        "from": "T21n1286_001:0355a02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "押二頭指側大指來去。",
        "from": "T21n1286_001:0355a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成辦諸事業。",
        "from": "T21n1286_001:0355a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次說諸成就護摩之法。",
        "from": "T21n1286_001:0355a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以稻穀華護摩。",
        "from": "T21n1286_001:0355a04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得五穀豐登成就。",
        "from": "T21n1286_001:0355a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法以闍底華護摩。",
        "from": "T21n1286_001:0355a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蠶養千斤成就。",
        "from": "T21n1286_001:0355a06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法以粳米護摩。",
        "from": "T21n1286_001:0355a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得無盡福德及殖生生福田。",
        "from": "T21n1286_001:0355a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法一神居住之處。",
        "from": "T21n1286_001:0355a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾人來集其所護摩。",
        "from": "T21n1286_001:0355a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得巨多富。",
        "from": "T21n1286_001:0355a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法以大麥小麥護摩。",
        "from": "T21n1286_001:0355a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "富貴。",
        "from": "T21n1286_001:0355a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法以兢婁草護摩。",
        "from": "T21n1286_001:0355a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得無數六畜財。",
        "from": "T21n1286_001:0355a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法大藥叉住古墓前。",
        "from": "T21n1286_001:0355a13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以闍底華護摩。",
        "from": "T21n1286_001:0355a13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得大富。",
        "from": "T21n1286_001:0355a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法以含華護摩。",
        "from": "T21n1286_001:0355a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得人愛。",
        "from": "T21n1286_001:0355a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法福神所住欲呼得者。",
        "from": "T21n1286_001:0355a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以青馬尾各絕截一寸二十一支。",
        "from": "T21n1286_001:0355a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每時護摩念。",
        "from": "T21n1286_001:0355a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福神必來付我身。",
        "from": "T21n1286_001:0355a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法青馬屎。",
        "from": "T21n1286_001:0355a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以前地天明加持二十一遍。",
        "from": "T21n1286_001:0355a19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即壇上及倍人乞大竈額上塗。",
        "from": "T21n1286_001:0355a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得如意富。",
        "from": "T21n1286_001:0355a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法以青馬額髮毛。",
        "from": "T21n1286_001:0355a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福神祭二十一支。",
        "from": "T21n1286_001:0355a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福神必悅授福。",
        "from": "T21n1286_001:0355a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法有福人家竈額上土取。",
        "from": "T21n1286_001:0355a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臘月八日持來我所安置。",
        "from": "T21n1286_001:0355a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必得富。",
        "from": "T21n1286_001:0355a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取土時用前天呪二十一遍。",
        "from": "T21n1286_001:0355a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須達多優婆塞。",
        "from": "T21n1286_001:0355a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壇畢理優婆塞。",
        "from": "T21n1286_001:0355a25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福寶光明優婆夷。",
        "from": "T21n1286_001:0355a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行此法。",
        "from": "T21n1286_001:0355a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法土氣強盛。",
        "from": "T21n1286_001:0355a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為四眾致刑罰。",
        "from": "T21n1286_001:0355a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以前明加持得除差。",
        "from": "T21n1286_001:0355a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法長者家碓下土取護摩。",
        "from": "T21n1286_001:0355a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必得富。",
        "from": "T21n1286_001:0355a29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法長者墓口裏土取護摩。",
        "from": "T21n1286_001:0355b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又得富。",
        "from": "T21n1286_001:0355b01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可畫堅牢地神像法。",
        "from": "T21n1286_001:0355b02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "男天肉色。",
        "from": "T21n1286_001:0355b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持鉢盛華。",
        "from": "T21n1286_001:0355b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手掌向外。",
        "from": "T21n1286_001:0355b03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "女天白肉色。",
        "from": "T21n1286_001:0355b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手抱當心。",
        "from": "T21n1286_001:0355b04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手抱當股。",
        "from": "T21n1286_001:0355b04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地天儀軌一卷",
        "from": "T21n1286_001:0355b05_100",
        "context_with_next_sentence": false
    }
]