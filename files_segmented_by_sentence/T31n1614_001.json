[
    {
        "source_sentence": "大乘百法明門論本事分中略錄",
        "from": "T31n1614_001:0855b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名數",
        "from": "T31n1614_001:0855b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天親菩薩造",
        "from": "T31n1614_001:0855b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大唐三藏法師玄奘譯如世尊言「一切法無我」。",
        "from": "T31n1614_001:0855b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等一切法？",
        "from": "T31n1614_001:0855b15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為無我？",
        "from": "T31n1614_001:0855b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切法者略有五種：",
        "from": "T31n1614_001:0855b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者心法、二者心所有法、三者色法、四者心不相應行法、五者無為法。",
        "from": "T31n1614_001:0855b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切最勝故，與此相應故，二所現影故，三分位差別故，四所顯示故。",
        "from": "T31n1614_001:0855b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是次第。",
        "from": "T31n1614_001:0855b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一心法，略有八種：",
        "from": "T31n1614_001:0855b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一眼識、二耳識、三鼻識、四舌識、五身識、六意識、七末那識、八阿賴耶識。",
        "from": "T31n1614_001:0855b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二心所有法，略有五十一種，分為六位：",
        "from": "T31n1614_001:0855b23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一遍行有五、二別境有五、三善有十一、四煩惱有六、五隨煩惱有二十、六不定有四。",
        "from": "T31n1614_001:0855b25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一遍行五者，一作意、二觸、三受、四想、五思。",
        "from": "T31n1614_001:0855b26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二別境五者，一欲、二勝解、三念、四定、五慧。",
        "from": "T31n1614_001:0855b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三善十一者，一信、二精進、三慚、四愧、五無貪、六無嗔、七無癡、八輕安、九不放逸、十行捨、十一不害。",
        "from": "T31n1614_001:0855b29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四煩惱六者，一貪、二嗔、三慢、四無明、五疑、六不正見。",
        "from": "T31n1614_001:0855c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五隨煩惱二十者，一忿、二恨、三惱、四覆、五誑、六諂、七憍、八害、九嫉、十慳、十一無慚、十二無愧、十三不信、十四懈怠、十五放逸、十六惛沈、十七掉舉、十八失念、十九不正知、二十散亂。",
        "from": "T31n1614_001:0855c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六不定四者，一睡眠、二惡作、三尋、四伺。",
        "from": "T31n1614_001:0855c06_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三色法，略有十一種：",
        "from": "T31n1614_001:0855c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一眼、二耳、三鼻、四舌、五身、六色、七聲、八香、九味、十觸、十一法處所攝色。",
        "from": "T31n1614_001:0855c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四心不相應行法，略有二十四種：",
        "from": "T31n1614_001:0855c10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一得、二命根、三眾同分、四異生性、五無想定、六滅盡定、七無想報、八名身、九句身、十文身、十一生、十二老、十三住、十四無常、十五流轉、十六定異、十七相應、十八勢速、十九次第、二十方、二十一時、二十二數、二十三和合性、二十四不和合性。",
        "from": "T31n1614_001:0855c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五無為法者，略有六種：",
        "from": "T31n1614_001:0855c17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一虛空無為、二擇滅無為、三非擇滅無為、四不動滅無為、五想受滅無為、六真如無為。",
        "from": "T31n1614_001:0855c19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言無我者，略有二種：",
        "from": "T31n1614_001:0855c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一補特伽羅無我、二法無我。",
        "from": "T31n1614_001:0855c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大乘百法明門論",
        "from": "T31n1614_001:0855c22_100",
        "context_with_next_sentence": false
    }
]