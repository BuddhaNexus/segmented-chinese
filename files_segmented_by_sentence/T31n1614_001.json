{
    "T31n1614_001:0855b10_0": "大乘百法明門論本事分中略錄",
    "T31n1614_001:0855b11_0": "名數",
    "T31n1614_001:0855b13_0": "天親菩薩造",
    "T31n1614_001:0855b14_0": "大唐三藏法師玄奘譯",
    "T31n1614_001:0855b15_0": "如世尊言。",
    "T31n1614_001:0855b15_1": "一切法無我。",
    "T31n1614_001:0855b15_2": "何等一切法。",
    "T31n1614_001:0855b15_3": "云何為無我。",
    "T31n1614_001:0855b16_4": "一切法者。",
    "T31n1614_001:0855b16_5": "略有五種。",
    "T31n1614_001:0855b16_6": "一者心法。",
    "T31n1614_001:0855b16_7": "二者心所有法。",
    "T31n1614_001:0855b17_8": "三者色法。",
    "T31n1614_001:0855b17_9": "四者心不相應行法。",
    "T31n1614_001:0855b17_10": "五者無為法。",
    "T31n1614_001:0855b18_11": "一切最勝故。",
    "T31n1614_001:0855b18_12": "與此相應故。",
    "T31n1614_001:0855b18_13": "二所現影故。",
    "T31n1614_001:0855b19_14": "三分位差別故。",
    "T31n1614_001:0855b19_15": "四所顯示故。",
    "T31n1614_001:0855b19_16": "如是次第。",
    "T31n1614_001:0855b20_17": "第一心法略有八種。",
    "T31n1614_001:0855b20_18": "一眼識。",
    "T31n1614_001:0855b20_19": "二耳識。",
    "T31n1614_001:0855b20_20": "三鼻識。",
    "T31n1614_001:0855b21_21": "四舌識。",
    "T31n1614_001:0855b21_22": "五身識。",
    "T31n1614_001:0855b21_23": "六意識。",
    "T31n1614_001:0855b21_24": "七末那識。",
    "T31n1614_001:0855b21_25": "八阿賴耶識。",
    "T31n1614_001:0855b23_0": "第二心所有法。",
    "T31n1614_001:0855b23_1": "略有五十一種。",
    "T31n1614_001:0855b23_2": "分為六位。",
    "T31n1614_001:0855b23_3": "一遍行有五。",
    "T31n1614_001:0855b24_4": "二別境有五。",
    "T31n1614_001:0855b24_5": "三善有十一。",
    "T31n1614_001:0855b24_6": "四煩惱有六。",
    "T31n1614_001:0855b25_7": "五隨煩惱有二十。",
    "T31n1614_001:0855b25_8": "六不定有四。",
    "T31n1614_001:0855b25_9": "一遍行五者。",
    "T31n1614_001:0855b26_10": "一作意二觸三受四想五思。",
    "T31n1614_001:0855b26_11": "二別境五者。",
    "T31n1614_001:0855b27_12": "一欲二勝解三念四定五慧。",
    "T31n1614_001:0855b27_13": "三善十一者。",
    "T31n1614_001:0855b28_0": "一信二精進。",
    "T31n1614_001:0855b28_1": "三慚四愧。",
    "T31n1614_001:0855b28_2": "五無貪六無嗔七無癡。",
    "T31n1614_001:0855b29_3": "八輕安九不放逸十行捨十一不害。",
    "T31n1614_001:0855b29_4": "四煩惱六者。",
    "T31n1614_001:0855c01_5": "一貪二嗔。",
    "T31n1614_001:0855c01_6": "三慢四無明。",
    "T31n1614_001:0855c01_7": "五疑六不正見。",
    "T31n1614_001:0855c02_8": "五隨煩惱二十者。",
    "T31n1614_001:0855c02_9": "一忿二恨。",
    "T31n1614_001:0855c02_10": "三惱四覆。",
    "T31n1614_001:0855c03_0": "五誑六諂。",
    "T31n1614_001:0855c03_1": "七憍八害。",
    "T31n1614_001:0855c03_2": "九嫉十慳。",
    "T31n1614_001:0855c03_3": "十一無慚十二無愧。",
    "T31n1614_001:0855c04_4": "十三不信十四懈怠。",
    "T31n1614_001:0855c04_5": "十五放逸十六惛沈。",
    "T31n1614_001:0855c05_6": "十七掉舉十八失念。",
    "T31n1614_001:0855c05_7": "十九不正知二十散亂。",
    "T31n1614_001:0855c06_8": "六不定四者。",
    "T31n1614_001:0855c06_9": "一睡眠二惡作。",
    "T31n1614_001:0855c06_10": "三尋四伺。",
    "T31n1614_001:0855c07_0": "第三色法。",
    "T31n1614_001:0855c07_1": "略有十一種。",
    "T31n1614_001:0855c07_2": "一眼二耳三鼻四舌五身。",
    "T31n1614_001:0855c08_3": "六色七聲八香九味十觸。",
    "T31n1614_001:0855c08_4": "十一法處所攝色。",
    "T31n1614_001:0855c10_0": "第四心不相應行法。",
    "T31n1614_001:0855c10_1": "略有二十四種。",
    "T31n1614_001:0855c10_2": "一得二命根。",
    "T31n1614_001:0855c11_3": "三眾同分。",
    "T31n1614_001:0855c11_4": "四異生性。",
    "T31n1614_001:0855c11_5": "五無想定。",
    "T31n1614_001:0855c11_6": "六滅盡定。",
    "T31n1614_001:0855c12_7": "七無想報。",
    "T31n1614_001:0855c12_8": "八名身九句身十文身。",
    "T31n1614_001:0855c12_9": "十一生十二老。",
    "T31n1614_001:0855c13_10": "十三住十四無常。",
    "T31n1614_001:0855c13_11": "十五流轉。",
    "T31n1614_001:0855c13_12": "十六定異。",
    "T31n1614_001:0855c14_13": "十七相應。",
    "T31n1614_001:0855c14_14": "十八勢速。",
    "T31n1614_001:0855c14_15": "十九次第。",
    "T31n1614_001:0855c14_16": "二十方。",
    "T31n1614_001:0855c15_0": "二十一時。",
    "T31n1614_001:0855c15_1": "二十二數。",
    "T31n1614_001:0855c15_2": "二十三和合性。",
    "T31n1614_001:0855c15_3": "二十四不和合性。",
    "T31n1614_001:0855c17_0": "第五無為法者。",
    "T31n1614_001:0855c17_1": "略有六種。",
    "T31n1614_001:0855c17_2": "一虛空無為。",
    "T31n1614_001:0855c17_3": "二擇滅無為。",
    "T31n1614_001:0855c18_4": "三非擇滅無為。",
    "T31n1614_001:0855c18_5": "四不動滅無為。",
    "T31n1614_001:0855c18_6": "五想受滅無為。",
    "T31n1614_001:0855c19_7": "六真如無為。",
    "T31n1614_001:0855c20_0": "言無我者。",
    "T31n1614_001:0855c20_1": "略有二種。",
    "T31n1614_001:0855c20_2": "一補特伽羅無我。",
    "T31n1614_001:0855c20_3": "二法無我。",
    "T31n1614_001:0855c22_0": "大乘百法明門論"
}