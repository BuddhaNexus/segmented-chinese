[
    {
        "source_sentence": "童子經念誦法",
        "from": "T19n1028B_001:0742c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏沙門善無畏譯詔",
        "from": "T19n1028B_001:0742c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□□天王白佛言。世尊若有女人。為小",
        "from": "T19n1028B_001:0742c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兒欲修此童子經法者。",
        "from": "T19n1028B_001:0742c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先月八日十五日。",
        "from": "T19n1028B_001:0742c14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持八戒。",
        "from": "T19n1028B_001:0742c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清淨洗浴。",
        "from": "T19n1028B_001:0742c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "着新淨衣。",
        "from": "T19n1028B_001:0742c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮十方諸佛。",
        "from": "T19n1028B_001:0742c15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晨朝時起。",
        "from": "T19n1028B_001:0742c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以覽字呪水八。",
        "from": "T19n1028B_001:0742c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "硯內寫書雜童子經。",
        "from": "T19n1028B_001:0742c16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向東方作圓壇。",
        "from": "T19n1028B_001:0742c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盛滿香花燈明閼伽等置壇上。",
        "from": "T19n1028B_001:0742c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書寫此經而後。",
        "from": "T19n1028B_001:0742c18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於淨室修此法。",
        "from": "T19n1028B_001:0742c18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於江河邊。",
        "from": "T19n1028B_001:0742c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於山邊。",
        "from": "T19n1028B_001:0742c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於僧尼住處。",
        "from": "T19n1028B_001:0742c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行此童子經祕密法。",
        "from": "T19n1028B_001:0742c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作四肘圓壇。",
        "from": "T19n1028B_001:0742c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次作書曼荼羅。",
        "from": "T19n1028B_001:0742c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內院內書大梵天王。",
        "from": "T19n1028B_001:0742c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "栴檀乾闥婆大鬼神□□□書十五鬼神形像。",
        "from": "T19n1028B_001:0742c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如所說如是造作已。",
        "from": "T19n1028B_001:0742c22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時大梵天王重白佛言。",
        "from": "T19n1028B_001:0742c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為欲利養安樂閻浮提諸小兒故。",
        "from": "T19n1028B_001:0742c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊聽許說最極密印陀羅尼。",
        "from": "T19n1028B_001:0743a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即此陀羅尼曰。",
        "from": "T19n1028B_001:0743a01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多恎他阿伽羅伽寗那伽伽寗波漏隷伽婆隷婆隷不隷羅收禰修羅俾遮羅俾波陀尼波羅呵阿曷利沙尼那易禰那易蘇婆呵其印相。",
        "from": "T19n1028B_001:0743a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定以空捻水甲。",
        "from": "T19n1028B_001:0743a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如取花形。",
        "from": "T19n1028B_001:0743a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惠拳押腰是也。",
        "from": "T19n1028B_001:0743a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若以此陀羅尼印加持病惱者。",
        "from": "T19n1028B_001:0743a06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得除滅。",
        "from": "T19n1028B_001:0743a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此陀羅尼被縛一切諸天。",
        "from": "T19n1028B_001:0743a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切大鬼神王。",
        "from": "T19n1028B_001:0743a07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切大少神冥道類。",
        "from": "T19n1028B_001:0743a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但有慈悲者皆不折縛。",
        "from": "T19n1028B_001:0743a08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切鬼神神王令縛。",
        "from": "T19n1028B_001:0743a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆頭破作七分。",
        "from": "T19n1028B_001:0743a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如阿梨樹枝。",
        "from": "T19n1028B_001:0743a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時栴檀乾闥婆大鬼王。",
        "from": "T19n1028B_001:0743a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并十五大鬼神。",
        "from": "T19n1028B_001:0743a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神祇冥道。",
        "from": "T19n1028B_001:0743a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆來集五體投地。",
        "from": "T19n1028B_001:0743a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白大梵王言。",
        "from": "T19n1028B_001:0743a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等皆捨惡心。",
        "from": "T19n1028B_001:0743a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護此陀羅尼并童子。",
        "from": "T19n1028B_001:0743a13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願大梵王說陀羅尼。",
        "from": "T19n1028B_001:0743a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令一切鬼神令護童子。",
        "from": "T19n1028B_001:0743a14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又護童子經除惡成吉祥。",
        "from": "T19n1028B_001:0743a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "栴檀乾闥婆王即說陀羅尼曰。",
        "from": "T19n1028B_001:0743a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼多沒馱引喃引一尾戌馱薩縛二合囉嚩引係係娑嚩二合賀內縛申水輪勿頭合是也。",
        "from": "T19n1028B_001:0743a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說印并陀羅尼也。",
        "from": "T19n1028B_001:0743a19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此印陀陀羅尼。",
        "from": "T19n1028B_001:0743a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅一切大疫病。",
        "from": "T19n1028B_001:0743a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除一切四百四病。",
        "from": "T19n1028B_001:0743a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切鬼神常恒守護不離此人邊。",
        "from": "T19n1028B_001:0743a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此呪成一切所求所聖。",
        "from": "T19n1028B_001:0743a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我又說十五鬼陀羅尼。",
        "from": "T19n1028B_001:0743a22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利益一切眾生故。",
        "from": "T19n1028B_001:0743a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願大梵王說十五眷屬陀羅尼。",
        "from": "T19n1028B_001:0743a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說曰。",
        "from": "T19n1028B_001:0743a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無佛陀耶一南無達麼耶二南無僧伽耶南無酬迦鬼彌迦王鬼騫陀王鬼阿波悉魔羅鬼牟致迦鬼魔致迦鬼閻彌迦鬼迦彌尼鬼梨婆坻鬼富多那鬼曼多難提鬼舍究尼鬼揵吒波尼尼鬼目佉曼荼鬼藍婆鬼莎呵大梵天王重白佛言。",
        "from": "T19n1028B_001:0743b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有眾生於此陀羅尼能受持念誦。",
        "from": "T19n1028B_001:0743b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅萬惡生萬善。",
        "from": "T19n1028B_001:0743b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切冥道常守護。",
        "from": "T19n1028B_001:0743b02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切鬼神一切惡神慈悲加護。",
        "from": "T19n1028B_001:0743b03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時大梵天帝釋天白佛言。",
        "from": "T19n1028B_001:0743b04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我有護諸童子陀羅尼。",
        "from": "T19n1028B_001:0743b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名長壽延命陀羅尼。",
        "from": "T19n1028B_001:0743b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說陀羅尼曰。",
        "from": "T19n1028B_001:0743b05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多恎他菩陀菩陀㝹摩帝菩提菩提摩隷式叉夜婆舍利婆多彌婆羅陀頭隷頭隷婆臈多頭隷舍摩膩收隷婆羅鞞陀波臈帝收藍舍彌帝般陀般絺婆呵膩摩祇摩膩陀波蘇可膩婆羅膩蘇波呵印相。",
        "from": "T19n1028B_001:0743b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手仰掌。",
        "from": "T19n1028B_001:0743b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大指捻二無名指。",
        "from": "T19n1028B_001:0743b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□頭指來去是也。",
        "from": "T19n1028B_001:0743b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此陀羅尼結此印。",
        "from": "T19n1028B_001:0743b12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常受持童子經者。",
        "from": "T19n1028B_001:0743b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即延三萬八千日壽命。",
        "from": "T19n1028B_001:0743b13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此陀羅尼能消滅定業延壽命。",
        "from": "T19n1028B_001:0743b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅無量惡生無量善。",
        "from": "T19n1028B_001:0743b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能成就所願。",
        "from": "T19n1028B_001:0743b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說勸請十五鬼神呪曰。",
        "from": "T19n1028B_001:0743b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南摩三曼多勃馱喃一耽令令閻微枳履儞弭履訶娑難羅娑難特𭟒娑難𮉂莎呵印相。",
        "from": "T19n1028B_001:0743b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手二少指內相叉。",
        "from": "T19n1028B_001:0743b19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指相柱。",
        "from": "T19n1028B_001:0743b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘四指散開來去。",
        "from": "T19n1028B_001:0743b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□召請十五鬼神呪曰。",
        "from": "T19n1028B_001:0743b21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵弭莫伽迦利沙野曳醯枳布瑟置迦鴦俱捨弱吽印相。",
        "from": "T19n1028B_001:0743b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手向外屈五指。",
        "from": "T19n1028B_001:0743b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三度召之是也。",
        "from": "T19n1028B_001:0743b24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次施食十五鬼神呪曰用金剛合掌。",
        "from": "T19n1028B_001:0743b25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵波羅步多二弭麼隷參皤嚩吽次說飽滿食十五鬼神呪曰。",
        "from": "T19n1028B_001:0743b27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南麼三曼多勃馱喃唵訶利訶利藥乞叉尾儞夜達𭉮比旨比旨囉吒蘗囉藍蘗囉藍訶散難微訶上散難弱吽鑁斛印相。",
        "from": "T19n1028B_001:0743c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛合掌。",
        "from": "T19n1028B_001:0743c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大指捻二頭指甲上是也注云與願印也。",
        "from": "T19n1028B_001:0743c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說散粇米花曰用金剛掌。",
        "from": "T19n1028B_001:0743c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵摩訶摩儞布囉耶達羅達羅吽吽唵鉢囉嚩羅謁囉嚩底娑囉娑囉吽吽常白月一日八日十五日。",
        "from": "T19n1028B_001:0743c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "撰鬼宿。",
        "from": "T19n1028B_001:0743c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設種種飲食菓子乳粥美味六種供養施神王。",
        "from": "T19n1028B_001:0743c08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是神等令一切眾生常惱害眾生坐生氣殺萬性人民。",
        "from": "T19n1028B_001:0743c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生一切病皆此神王所為也。",
        "from": "T19n1028B_001:0743c10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故十五神王常念愛。",
        "from": "T19n1028B_001:0743c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持誦中我所說陀羅尼者得無盡壽命。",
        "from": "T19n1028B_001:0743c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切天王守護。",
        "from": "T19n1028B_001:0743c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切神王恒加護。",
        "from": "T19n1028B_001:0743c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以栴檀乾闥婆。",
        "from": "T19n1028B_001:0743c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切閻浮提大鬼神中。",
        "from": "T19n1028B_001:0743c13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最尊最上。",
        "from": "T19n1028B_001:0743c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為無有上。",
        "from": "T19n1028B_001:0743c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書此鬼王名號。",
        "from": "T19n1028B_001:0743c14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并經中陀羅尼。",
        "from": "T19n1028B_001:0743c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以五色綖。",
        "from": "T19n1028B_001:0743c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪一百八遍。",
        "from": "T19n1028B_001:0743c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若呪二十一返。",
        "from": "T19n1028B_001:0743c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又呪七遍。",
        "from": "T19n1028B_001:0743c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫頸上。",
        "from": "T19n1028B_001:0743c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法小兒額書栴檀乾闥婆大鬼名號。",
        "from": "T19n1028B_001:0743c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若有世間疫病者。",
        "from": "T19n1028B_001:0743c17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以伏鬼木。",
        "from": "T19n1028B_001:0743c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪二十一遍。",
        "from": "T19n1028B_001:0743c18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲病人。",
        "from": "T19n1028B_001:0743c18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法有眼病者。",
        "from": "T19n1028B_001:0743c18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪花水二十一遍。",
        "from": "T19n1028B_001:0743c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲之洗之。",
        "from": "T19n1028B_001:0743c19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法有胸病者。",
        "from": "T19n1028B_001:0743c19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以石留呪一百八返飲之。",
        "from": "T19n1028B_001:0743c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法有利病。",
        "from": "T19n1028B_001:0743c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若赤利病下利病者。",
        "from": "T19n1028B_001:0743c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取牛膝草。",
        "from": "T19n1028B_001:0743c21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪一百八遍。",
        "from": "T19n1028B_001:0743c21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日日飲之。",
        "from": "T19n1028B_001:0743c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法有虐病者。",
        "from": "T19n1028B_001:0743c22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪大頭飲之。",
        "from": "T19n1028B_001:0743c22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即病消滅。",
        "from": "T19n1028B_001:0743c22_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法有頭病者。",
        "from": "T19n1028B_001:0743c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以柳木板。",
        "from": "T19n1028B_001:0743c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪二十一遍。",
        "from": "T19n1028B_001:0743c23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置頭下邊。",
        "from": "T19n1028B_001:0743c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法欲降伏惡人者。",
        "from": "T19n1028B_001:0743c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書彼名。",
        "from": "T19n1028B_001:0743c24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起嗔恚惡心。",
        "from": "T19n1028B_001:0743c24_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以箭弓射彼人名一返。",
        "from": "T19n1028B_001:0743c25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而誦前十五鬼呪呼加彼名。",
        "from": "T19n1028B_001:0743c26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即三日中死。",
        "from": "T19n1028B_001:0743c26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切鳥類一切萬怪來集時。",
        "from": "T19n1028B_001:0743c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先作怪向者物呪誦之。",
        "from": "T19n1028B_001:0743c27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼作怪物自身受怪。",
        "from": "T19n1028B_001:0743c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然死滅。",
        "from": "T19n1028B_001:0743c28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖多法有本藏。",
        "from": "T19n1028B_001:0743c28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具不記撰。",
        "from": "T19n1028B_001:0743c28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已上注也。",
        "from": "T19n1028B_001:0743c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此法滅萬惡成萬善。",
        "from": "T19n1028B_001:0743c29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令一切所求成就。",
        "from": "T19n1028B_001:0744a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間出世一切福。",
        "from": "T19n1028B_001:0744a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切高位。",
        "from": "T19n1028B_001:0744a01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切愛念。",
        "from": "T19n1028B_001:0744a01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆大吉也。",
        "from": "T19n1028B_001:0744a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "童子經念誦法壇上有惡字。",
        "from": "T19n1028B_001:0744a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "字變成七寶樓閣。",
        "from": "T19n1028B_001:0744a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樓閣上張天蓋。",
        "from": "T19n1028B_001:0744a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以眾寶自莊嚴。",
        "from": "T19n1028B_001:0744a05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其宮殿中有荷葉座座上有鉢羅字。",
        "from": "T19n1028B_001:0744a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變成大梵天王。",
        "from": "T19n1028B_001:0744a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王邊荷葉座有尾字。",
        "from": "T19n1028B_001:0744a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變成栴檀乾闥婆大王。",
        "from": "T19n1028B_001:0744a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其荷葉邊有吽字。",
        "from": "T19n1028B_001:0744a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成十五鬼王將。",
        "from": "T19n1028B_001:0744a08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七萬八千鬼神前後圍匝。",
        "from": "T19n1028B_001:0744a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　平治元年八月八日。",
        "from": "T19n1028B_001:0744a10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　　　　　　　　　　興然",
        "from": "T19n1028B_001:0744a12_0",
        "context_with_next_sentence": true
    }
]