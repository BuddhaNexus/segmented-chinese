[
    {
        "source_sentence": "大寒林聖難拏陀羅尼經",
        "from": "T21n1392_001:0908b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天中印度摩伽陀國那爛陀寺",
        "from": "T21n1392_001:0908b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏傳教大師賜紫沙門臣",
        "from": "T21n1392_001:0908b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法天奉　詔譯如是我聞。",
        "from": "T21n1392_001:0908b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時薄伽梵。",
        "from": "T21n1392_001:0908b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在王舍城中。",
        "from": "T21n1392_001:0908b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時尊者羅睺羅。",
        "from": "T21n1392_001:0908b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遊於孕欺迦耶怛曩地寒林之中。",
        "from": "T21n1392_001:0908b14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於大塚間。",
        "from": "T21n1392_001:0908b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼時有諸天魅龍魅藥叉羅剎緊捺囉㜸嚕荼摩護囉誐。",
        "from": "T21n1392_001:0908b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及餘一切人非人。",
        "from": "T21n1392_001:0908b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餓鬼部多比舍佐供畔拏等之所來魅。",
        "from": "T21n1392_001:0908b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有多種異類烏鵲。",
        "from": "T21n1392_001:0908b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獯狐豺狼蟲蟻等。",
        "from": "T21n1392_001:0908b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "極多擾惱。",
        "from": "T21n1392_001:0908b18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "于時尊者羅睺羅往詣佛所。",
        "from": "T21n1392_001:0908b19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到已頭面著地。",
        "from": "T21n1392_001:0908b19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮世尊足圍遶三匝。",
        "from": "T21n1392_001:0908b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "涕淚悲泣立世尊前。",
        "from": "T21n1392_001:0908b20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告羅睺羅言。",
        "from": "T21n1392_001:0908b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今云何涕淚悲泣住立我前。",
        "from": "T21n1392_001:0908b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅睺羅言如是世尊。",
        "from": "T21n1392_001:0908b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我先住於王舍城孕欺迦耶怛曩地寒林之中。",
        "from": "T21n1392_001:0908b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於大塚間。",
        "from": "T21n1392_001:0908b23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼時有諸天魅龍魅藥叉羅剎緊捺囉㜸嚕荼摩護囉誐。",
        "from": "T21n1392_001:0908b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及餘一切人非人。",
        "from": "T21n1392_001:0908b25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餓鬼部多比舍佐供畔拏等。",
        "from": "T21n1392_001:0908b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆來魅我。",
        "from": "T21n1392_001:0908b26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有多種異類鳥鵲獯狐豺狼諸蟲蟻等。",
        "from": "T21n1392_001:0908b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "極擾惱我。",
        "from": "T21n1392_001:0908b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告尊者羅睺羅。",
        "from": "T21n1392_001:0908b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言羅睺羅汝今諦聽此有大明祕密難拏陀羅尼。",
        "from": "T21n1392_001:0908b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為擁護聽眾。",
        "from": "T21n1392_001:0908b29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若苾芻苾芻尼鄔播索俱鄔播斯迦。",
        "from": "T21n1392_001:0908c01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長夜利益得安樂故。",
        "from": "T21n1392_001:0908c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此陀羅尼曰。",
        "from": "T21n1392_001:0908c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛上儞也二合他去引一阿於肯反去誐罔武肯反誐婆蒲肯反去誐二嚩㘄轉舌誐三僧去娑引去囉哆𠻴轉舌誐上四娑去引麼嚩娜娑去引五婆蒲肯反去誐𡁠仁際反上素囉六翳迦哆囉上引阿囉尾囉七哆囉尾囉八引哆囉哆囉尾囉上引九迦囉尾囉引迦囉迦囉尾囉引十印娜印娜罽居翳反娑去引羅引十一悍娑去引悍娑罽准上切娑去引囉引十二嗶唧麼攞去引十三麼賀引枳佐去引尾呬去恥迦去引十四迦去引攞砌迦去引阿於肯反虞去引娜囉引十五惹引野引惹引耶梨迦去引十六際攞去引翳攞去引十七尾跢去引梨唧唧醯梨醯梨十八三去麼底嚩素麼底十九祖魯曩恥上祖魯祖魯曩恥上二十祖攞曩引嬭上矩曩引嬭上二十一賀引栗吒枳二十二迦去引栗去吒枳迦去引栗吒枳二十三矯魚夭反哩巘馱去引哩二十四贊拏上里麼引登去儗二十五達囉抳陀去引囉抳二十六塢瑟怛囉三合播上栗計二十七迦左迦引哩計嚩羅曩引嬭二十八迦去引羯栗計二十九攞攞麼底三十囉乞叉二合麼底三十一嚩囉引矩禮三十二麼儞也二合帝三十三塢怛跛二合禮迦囉尾㘑三十四多囉尾㘑三十五哆囉哆囉尾㘑矩嚕尾㘑矩嚕矩嚕尾㘑三十六祖嚕祖嚕尾㘑三十七麼賀引尾㘑誐囉麼底三十八拶囉麼底三十九囉乞叉二合麼底四十薩轉舌嚩引囉他二合娑上馱𩕳上四十一跛囉麼引轉舌呼囉他二合娑引去馱𩕳上四十二阿鉢囉二合底賀帝四十三印捺嚕二合囉引惹引四十四素引謨去引囉引惹引四十五嚩嚕𠯆拏矩反囉引惹引四十六矩吠無閉反嚕引囉引惹引四十七麼曩引悉尾二合上囉引惹引四十八嚩去引素罽囉引惹引四十九難去拏上引仡𩕳二合囉引惹引五十沒度去引娑賀娑囉二合引地跛底丁曳反囉引惹引五十一沒度去引婆去誐挽無泮反達轉舌麼娑嚩二合引弭囉引惹引五十二阿弩哆上嚕去引路去引迦去引努劍跛迦五十三囉乞叉二合囉乞叉二合𤚥去引阿呬崩去引五十四薩轉舌嚩薩怛嚩二合引難去引左五十五囉乞產二合引迦嚕去引都五十六跛哩怛囉二合喃上五十七跛哩㜸囉二合憾五十八跛哩播攞能去五十九扇引底孕二合娑嚩二合悉底也三合野能去六十難上拏跛哩賀引囕六十一設娑怛囉三合跛哩賀引囕六十二尾灑努灑喃上六十三尾灑曩引舍喃上六十四枲上麼引滿去重呼鄧上陀去引囉抳上六十五滿鄧上左矩囉挽二合無漢反覩六十六爾上仁際反嚩都挽無鉢反哩灑二合舍蹬六十七跛舍野二合都設囉那上引設蹬六十八怛儞也二合他上引六十九嚩攞嚩底七十拶囉麼底七十一哆攞麼底七十二洛乞叉二合麼底七十三囉乞叉二合麼底七十四護嚕麼底七十五護上嚕護上嚕七十六普嚕普嚕七十七拶囉拶囉七十八設覩嚨二合詎其據反嚕詎嚕七十九麼底麼底八十普弭贊抳上八十一迦去引里計置八十二阿枳娑攞引比上禰八十三娑引麼曩帝八十四護上禮窣兔二合禮娑他二合攞始伽㘑八十五惹引野窣兔二合引禮八十六惹攞曩引嬭上八十七祖魯曩引嬭上八十八嚩引仡挽二合無漢反馱儞八十九尾嚕去引賀抳素引魯呬上帝九十阿拏上㘑上半拏上㘑上九十一迦囉引禮九十二緊曩㘑上九十三計庾㘑上九十四計都麼底九十五普蹬誐謎九十六普哆麼底歎儞曳上二合瞢上誐禮曳九十七二合麼賀引嚩攞九十八魯引呬多母上禮九十九阿拶魯抳上一百馱囉馱囉引一百一惹野引里計一百二惹野嬌魚夭反𡀔去引賀抳一百三祖嚕祖嚕一百四𡃝盧恩反轉舌馱𡃝淮上馱一百五普嚕普嚕一百六麌嚕麌嚕一百七詎淮前嚕詎嚕一百八麼底麼底一百九滿重呼兔麼底一百十度上𡃝淮上切馱嚕馱𠼝一百十一馱上㘑上馱上㘑上一百十二尾達㘑尾麼底尾瑟劍二合婆去禰一百十三曩引舍禰尾曩引舍禰一百十四滿去重呼馱禰謨去引乞叉二合抳一百十五尾謨去引拶禰一百十六謨去引賀禰婆去引嚩禰一百十七戍引馱去禰僧去戍馱禰去一百十八尾戍引馱去禰一百十九僧去契上囉抳一百二十僧去髻囉禰一百二十一僧去瑳引娜禰一百二十二僧砌上那禰一百二十三娑引去度跢上嚕一百二十四麼引儞上麼引儞上賀囉賀囉一百二十五滿度麼底一百二十六呬哩呬哩一百二十七企哩企哩伽囉禮一百二十八護嚕護嚕一百二十九氷去誐禮一百三十曩謨引窣覩二合沒馱去引喃去婆誐嚩蹬引娑嚩二合引賀引一百三十一復次羅睺羅。",
        "from": "T21n1392_001:0909b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大明陀羅尼念誦之人。",
        "from": "T21n1392_001:0909b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能以香花而作供養。",
        "from": "T21n1392_001:0909b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及結印契志心念誦一百八遍結諸線索繫於手上及安頸上。",
        "from": "T21n1392_001:0909b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得周遍百踰繕那能為擁護。",
        "from": "T21n1392_001:0909b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人非人等悉皆遠離。",
        "from": "T21n1392_001:0909b21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦迺不被水火之所焚漂刀杖毒藥瘧病沴疾。",
        "from": "T21n1392_001:0909b22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能侵害亦不中夭尾怛拏病及明呪術。",
        "from": "T21n1392_001:0909b23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言皆得安樂。",
        "from": "T21n1392_001:0909b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若他繫縛即得解脫一切災惱。",
        "from": "T21n1392_001:0909b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言誦鬪諍亦悉除滅。",
        "from": "T21n1392_001:0909b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有鬼魅來作嬈亂不退散者。",
        "from": "T21n1392_001:0909b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但專志心誦此真言。",
        "from": "T21n1392_001:0909b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼等鬼神見持誦人。",
        "from": "T21n1392_001:0909b27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如執金剛大藥叉主純一金剛。",
        "from": "T21n1392_001:0909b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "威猛熾盛炎烈火焰。",
        "from": "T21n1392_001:0909b28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四大天王各執鐵輪。",
        "from": "T21n1392_001:0910a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鋒利刀劍逐令馳散。",
        "from": "T21n1392_001:0910a01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭破七分身體劈裂。",
        "from": "T21n1392_001:0910a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若彼鬼魅還本住處。",
        "from": "T21n1392_001:0910a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼諸同類不容入眾。",
        "from": "T21n1392_001:0910a03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不令住阿吒迦嚩底大王都城。",
        "from": "T21n1392_001:0910a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次羅睺羅此難拏大明陀羅尼志心誦持。",
        "from": "T21n1392_001:0910a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得遠離王賊水火毒氣刀杖。",
        "from": "T21n1392_001:0910a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曠野山林險難惡道。",
        "from": "T21n1392_001:0910a06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往來之者一切無畏。",
        "from": "T21n1392_001:0910a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次羅睺羅。",
        "from": "T21n1392_001:0910a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此難拏大明陀羅尼。",
        "from": "T21n1392_001:0910a08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九十一殑伽沙數諸佛。",
        "from": "T21n1392_001:0910a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已說今說當說。",
        "from": "T21n1392_001:0910a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足神通。",
        "from": "T21n1392_001:0910a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大神通者諸天龍藥叉犍闥婆阿素洛㜸魯荼摩護囉誐。",
        "from": "T21n1392_001:0910a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切群生圍遶禮拜。",
        "from": "T21n1392_001:0910a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼諸眾生離一切怖皆得安樂。",
        "from": "T21n1392_001:0910a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時尊者羅睺羅及諸大眾聞世尊說一心信受禮佛而退。",
        "from": "T21n1392_001:0910a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大寒林聖難拏陀羅尼經",
        "from": "T21n1392_001:0910a15_100",
        "context_with_next_sentence": false
    }
]