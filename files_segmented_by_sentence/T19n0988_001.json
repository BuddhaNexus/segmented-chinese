        "from": "T19n0988_001:0484b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為四部眾令得安隱離諸惱患。",
        "from": "T19n0988_001:0484b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾魔惡鬼盜賊水火。",
        "from": "T19n0988_001:0484b27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旋嵐惡風羅剎惡鬼。",
        "from": "T19n0988_001:0484b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "熱病冷病風病等分諸病。",
        "from": "T19n0988_001:0484b28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "家業衰耗所向不利。",
        "from": "T19n0988_001:0484b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡獸卒暴。",
        "from": "T19n0988_001:0484b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "急誦此呪一切解脫。",
        "from": "T19n0988_001:0484b29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今當重說陀羅尼呪。",
        "from": "T19n0988_001:0484c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娑離沙離毘林婆沙呵速去速去速還汝本來處。",
        "from": "T19n0988_001:0484c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孔雀王呪場。",
        "from": "T19n0988_001:0484c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用牛𡱁塗地。",
        "from": "T19n0988_001:0484c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用散七色華。",
        "from": "T19n0988_001:0484c04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幡四十九枚刀四枚鏡四枚箭一百枚弓一張瓨七枚盛漿。",
        "from": "T19n0988_001:0484c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黑羊毛繩十六尋。",
        "from": "T19n0988_001:0484c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薄餅二十五番。",
        "from": "T19n0988_001:0484c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然七油燈。",
        "from": "T19n0988_001:0484c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酪一器󰉓漿一器。",
        "from": "T19n0988_001:0484c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飯一器薄餅一器。",
        "from": "T19n0988_001:0484c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安石榴一器華一器。",
        "from": "T19n0988_001:0484c08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孔雀王呪經",
        "from": "T19n0988_001:0484c09_100",
        "context_with_next_sentence": false
    }
