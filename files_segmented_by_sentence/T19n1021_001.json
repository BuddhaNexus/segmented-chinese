[
    {
        "source_sentence": "華嚴經心陀羅尼",
        "from": "T19n1021_001:0709c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵枳那枳那怛他伽頭踟婆陛珊隱底婆囉底",
        "from": "T19n1021_001:0709c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烏陀慕陀迷唵薩陛梯薩嚩二合訶",
        "from": "T19n1021_001:0709c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此陀羅尼是普賢如來說。",
        "from": "T19n1021_001:0709c29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "于闐國三藏實叉難陀譯。",
        "from": "T19n1021_001:0710a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別有本經云。",
        "from": "T19n1021_001:0710a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人誦此一遍。",
        "from": "T19n1021_001:0710a02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "準誦龍藏中華嚴經上中下三本一遍。",
        "from": "T19n1021_001:0710a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況常持。",
        "from": "T19n1021_001:0710a03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能滿十萬遍者。",
        "from": "T19n1021_001:0710a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人敏悟精微。",
        "from": "T19n1021_001:0710a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洞徹一切教藏。",
        "from": "T19n1021_001:0710a04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至於福聚轉障不可具說。",
        "from": "T19n1021_001:0710a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上元縣高公寺摩訶衍和尚授",
        "from": "T19n1021_001:0710a06_100",
        "context_with_next_sentence": false
    }
]