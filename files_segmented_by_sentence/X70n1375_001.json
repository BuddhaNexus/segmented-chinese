{
    "X70n1375_001:0034b06_0": "曹源和尚住饒州妙果禪寺語錄",
    "X70n1375_001:0034b08_0": "侍者　道沖　編",
    "X70n1375_001:0034b09_0": "師在雲居。",
    "X70n1375_001:0034b09_1": "受請云。",
    "X70n1375_001:0034b09_2": "安樂樹邊藏醜拙。",
    "X70n1375_001:0034b09_3": "浮生穿鑿不相干。",
    "X70n1375_001:0034b10_4": "拄杖朝來剛[跳-兆+孛]跳。",
    "X70n1375_001:0034b10_5": "無端撞破趙州關。",
    "X70n1375_001:0034b10_6": "乃提起拄杖云。",
    "X70n1375_001:0034b11_7": "看看。",
    "X70n1375_001:0034b11_8": "喚作拄杖子。",
    "X70n1375_001:0034b11_9": "天地懸隔。",
    "X70n1375_001:0034b11_10": "不喚作拄杖子。",
    "X70n1375_001:0034b11_11": "天地懸隔。",
    "X70n1375_001:0034b12_12": "三世諸佛。",
    "X70n1375_001:0034b12_13": "歷代祖師。",
    "X70n1375_001:0034b12_14": "天下老和尚。",
    "X70n1375_001:0034b12_15": "到這裏。",
    "X70n1375_001:0034b12_16": "有屈無叫處。",
    "X70n1375_001:0034b13_17": "便恁麼去。",
    "X70n1375_001:0034b13_18": "盡法無民。",
    "X70n1375_001:0034b13_19": "既然裂破面皮。",
    "X70n1375_001:0034b13_20": "不避諸方檢責。",
    "X70n1375_001:0034b14_21": "遂橫按拄杖。",
    "X70n1375_001:0034b14_22": "左右顧視。",
    "X70n1375_001:0034b14_23": "良久云。",
    "X70n1375_001:0034b14_24": "三尺龍泉光照膽。",
    "X70n1375_001:0034b15_25": "太平寰宇斬癡頑。",
    "X70n1375_001:0034b15_26": "喝一喝。",
    "X70n1375_001:0034b15_27": "卓一卓　復舉。",
    "X70n1375_001:0034b15_28": "寶公和尚。",
    "X70n1375_001:0034b16_29": "一日令傳語思大禪師云。",
    "X70n1375_001:0034b16_30": "何不下山。",
    "X70n1375_001:0034b16_31": "教化眾生。",
    "X70n1375_001:0034b17_0": "一向目視雲漢。",
    "X70n1375_001:0034b17_1": "作什麼。",
    "X70n1375_001:0034b17_2": "思大云。",
    "X70n1375_001:0034b17_3": "三世諸佛。",
    "X70n1375_001:0034b17_4": "被我一口吞盡。",
    "X70n1375_001:0034b18_5": "何處更有眾生可度。",
    "X70n1375_001:0034b18_6": "師云。",
    "X70n1375_001:0034b18_7": "慇懃傳語寶公。",
    "X70n1375_001:0034b18_8": "著甚來由。",
    "X70n1375_001:0034b19_9": "費口分疎。",
    "X70n1375_001:0034b19_10": "思大尾巴自露。",
    "X70n1375_001:0034b19_11": "生上座。",
    "X70n1375_001:0034b19_12": "因行掉臂。",
    "X70n1375_001:0034b19_13": "等是下山。",
    "X70n1375_001:0034b20_14": "不問佛及眾生。",
    "X70n1375_001:0034b20_15": "纔出頭來。",
    "X70n1375_001:0034b20_16": "便與一刀兩段。",
    "X70n1375_001:0034b20_17": "何故。",
    "X70n1375_001:0034b21_18": "一不做二不休。",
    "X70n1375_001:0034b22_0": "上堂。",
    "X70n1375_001:0034b22_1": "佛法二字。",
    "X70n1375_001:0034b22_2": "人人知有。",
    "X70n1375_001:0034b22_3": "狼毒砒霜。",
    "X70n1375_001:0034b22_4": "那容下口。",
    "X70n1375_001:0034b22_5": "直饒透出威音前。",
    "X70n1375_001:0034b23_6": "也是癡狂外邊走。",
    "X70n1375_001:0034b23_7": "山僧已是拖泥帶水。",
    "X70n1375_001:0034b24_0": "諸人合作麼生會。",
    "X70n1375_001:0034b24_1": "喝一喝。",
    "X70n1375_001:0034c01_0": "上堂。",
    "X70n1375_001:0034c01_1": "今朝八月十五。",
    "X70n1375_001:0034c01_2": "天色半陰半雨。",
    "X70n1375_001:0034c01_3": "幾多門外遊人。",
    "X70n1375_001:0034c02_0": "不覩月圓當戶。",
    "X70n1375_001:0034c02_1": "也好笑。",
    "X70n1375_001:0034c02_2": "又堪嗟。",
    "X70n1375_001:0034c02_3": "爭似西湖寺裏。",
    "X70n1375_001:0034c02_4": "一隊古佛。",
    "X70n1375_001:0034c03_5": "參退歸堂喫茶。",
    "X70n1375_001:0034c04_0": "上堂。",
    "X70n1375_001:0034c04_1": "飛金烏走玉兔。",
    "X70n1375_001:0034c04_2": "閏月十日又過去。",
    "X70n1375_001:0034c04_3": "無位真人不可尋。",
    "X70n1375_001:0034c05_4": "一夜霜風打門戶。",
    "X70n1375_001:0034c06_0": "上堂。",
    "X70n1375_001:0034c06_1": "釋迦老子道。",
    "X70n1375_001:0034c06_2": "止止不須說。",
    "X70n1375_001:0034c06_3": "我法妙難思。",
    "X70n1375_001:0034c06_4": "喝一喝云。",
    "X70n1375_001:0034c07_5": "面皮厚多少。",
    "X70n1375_001:0034c07_6": "殊不知。",
    "X70n1375_001:0034c07_7": "乾坤大地。",
    "X70n1375_001:0034c07_8": "萬象森羅。",
    "X70n1375_001:0034c07_9": "明暗色空。",
    "X70n1375_001:0034c08_10": "情與無情。",
    "X70n1375_001:0034c08_11": "盡情說了也。",
    "X70n1375_001:0034c08_12": "無你左遮右掩處。",
    "X70n1375_001:0034c08_13": "德山棒臨濟喝。",
    "X70n1375_001:0034c09_14": "俱胝豎指。",
    "X70n1375_001:0034c09_15": "雪峰輥毬。",
    "X70n1375_001:0034c09_16": "盡是隔靴抓痒。",
    "X70n1375_001:0034c09_17": "放下著。",
    "X70n1375_001:0034c10_0": "莫妄想。",
    "X70n1375_001:0034c10_1": "是什麼。",
    "X70n1375_001:0034c10_2": "會也無。",
    "X70n1375_001:0034c10_3": "也是喚狗與食。",
    "X70n1375_001:0034c10_4": "更道蘇州菱。",
    "X70n1375_001:0034c11_0": "邵伯藕。",
    "X70n1375_001:0034c11_1": "天台華頂。",
    "X70n1375_001:0034c11_2": "南岳石橋。",
    "X70n1375_001:0034c11_3": "喚作向上提持。",
    "X70n1375_001:0034c11_4": "衲僧家門下。",
    "X70n1375_001:0034c12_5": "挈草鞋。",
    "X70n1375_001:0034c12_6": "未得在。",
    "X70n1375_001:0034c12_7": "莫有傍不甘底。",
    "X70n1375_001:0034c12_8": "出來道取一句看。",
    "X70n1375_001:0034c13_9": "直饒道得。",
    "X70n1375_001:0034c13_10": "也是郭郎鼻孔。",
    "X70n1375_001:0034c14_0": "上堂。",
    "X70n1375_001:0034c14_1": "入院恰得一箇月。",
    "X70n1375_001:0034c14_2": "人事參官走不徹。",
    "X70n1375_001:0034c14_3": "草鞋咬破脚指頭。",
    "X70n1375_001:0034c15_4": "因甚嬌梵舌端流出血。",
    "X70n1375_001:0034c15_5": "拈拄杖。",
    "X70n1375_001:0034c15_6": "卓一下云。",
    "X70n1375_001:0034c15_7": "此時若不究根源。",
    "X70n1375_001:0034c16_8": "直待當來問彌勒。",
    "X70n1375_001:0034c17_0": "上堂。",
    "X70n1375_001:0034c17_1": "十五日已前。",
    "X70n1375_001:0034c17_2": "不勞再勘。",
    "X70n1375_001:0034c17_3": "十五日已後。",
    "X70n1375_001:0034c17_4": "不用將來。",
    "X70n1375_001:0034c18_0": "正當十五。",
    "X70n1375_001:0034c18_1": "不昧時機。",
    "X70n1375_001:0034c18_2": "如何通信。",
    "X70n1375_001:0034c18_3": "但見皇風成一片。",
    "X70n1375_001:0034c18_4": "不知何處是封疆。",
    "X70n1375_001:0034c20_0": "上堂。",
    "X70n1375_001:0034c20_1": "拈拄杖。",
    "X70n1375_001:0034c20_2": "卓一下云。",
    "X70n1375_001:0034c20_3": "三世諸佛。",
    "X70n1375_001:0034c20_4": "向這裏擡脚不起。",
    "X70n1375_001:0034c21_0": "又卓一下云。",
    "X70n1375_001:0034c21_1": "六代祖師。",
    "X70n1375_001:0034c21_2": "向這裏頭出頭沒。",
    "X70n1375_001:0034c21_3": "又卓一下云。",
    "X70n1375_001:0034c22_4": "天下老和尚。",
    "X70n1375_001:0034c22_5": "向這裏把䌫放船。",
    "X70n1375_001:0034c22_6": "眾中莫有全機獨脫底麼。",
    "X70n1375_001:0034c23_7": "出來當頭道看。",
    "X70n1375_001:0034c23_8": "如無。",
    "X70n1375_001:0034c23_9": "西湖不免雪上加霜去也。",
    "X70n1375_001:0034c24_10": "卓拄杖一下。",
    "X70n1375_001:0034c24_11": "下座。",
    "X70n1375_001:0035a01_0": "冬至上堂。",
    "X70n1375_001:0035a01_1": "豎起拂子云。",
    "X70n1375_001:0035a01_2": "還見麼。",
    "X70n1375_001:0035a01_3": "君子道長。",
    "X70n1375_001:0035a01_4": "又擊一下云。",
    "X70n1375_001:0035a02_5": "還聞麼。",
    "X70n1375_001:0035a02_6": "小人道消。",
    "X70n1375_001:0035a02_7": "氷河齊發燄。",
    "X70n1375_001:0035a02_8": "石笋暗抽條。",
    "X70n1375_001:0035a02_9": "喝一喝云。",
    "X70n1375_001:0035a03_10": "塵劫來事。",
    "X70n1375_001:0035a03_11": "只在今朝。",
    "X70n1375_001:0035a04_0": "上堂。",
    "X70n1375_001:0035a04_1": "舉玄紹二上座。",
    "X70n1375_001:0035a04_2": "見烏臼和尚。",
    "X70n1375_001:0035a04_3": "臼云。",
    "X70n1375_001:0035a04_4": "甚處來。",
    "X70n1375_001:0035a04_5": "云江西來。",
    "X70n1375_001:0035a05_6": "臼便打。",
    "X70n1375_001:0035a05_7": "僧云。",
    "X70n1375_001:0035a05_8": "久嚮和尚有此機要。",
    "X70n1375_001:0035a05_9": "臼云。",
    "X70n1375_001:0035a05_10": "汝既不會。",
    "X70n1375_001:0035a06_11": "第二箇近前來。",
    "X70n1375_001:0035a06_12": "其僧忙然。",
    "X70n1375_001:0035a06_13": "臼亦打云。",
    "X70n1375_001:0035a06_14": "同坑無異土。",
    "X70n1375_001:0035a07_0": "參堂去。",
    "X70n1375_001:0035a07_1": "師云。",
    "X70n1375_001:0035a07_2": "顯大機。",
    "X70n1375_001:0035a07_3": "明大用。",
    "X70n1375_001:0035a07_4": "奮寰中意氣。",
    "X70n1375_001:0035a07_5": "運閫外籌略。",
    "X70n1375_001:0035a08_6": "則不無烏臼。",
    "X70n1375_001:0035a08_7": "撿點將來。",
    "X70n1375_001:0035a08_8": "未是本分草料。",
    "X70n1375_001:0035a08_9": "若要據令而行。",
    "X70n1375_001:0035a09_10": "盡大地一時荒却。",
    "X70n1375_001:0035a09_11": "何故。",
    "X70n1375_001:0035a09_12": "寸釘入木。",
    "X70n1375_001:0035a10_0": "出鄉歸上堂。",
    "X70n1375_001:0035a10_1": "一九與二九。",
    "X70n1375_001:0035a10_2": "相逢不出手。",
    "X70n1375_001:0035a10_3": "巍巍不動尊。",
    "X70n1375_001:0035a11_0": "脚不離地走。",
    "X70n1375_001:0035a11_1": "有般漆桶。",
    "X70n1375_001:0035a11_2": "聞與麼道。",
    "X70n1375_001:0035a11_3": "便向東涌西沒。",
    "X70n1375_001:0035a11_4": "七縱八橫處。",
    "X70n1375_001:0035a12_5": "點頭嚥唾。",
    "X70n1375_001:0035a12_6": "這般野狐見解。",
    "X70n1375_001:0035a12_7": "是諸方普請會底。",
    "X70n1375_001:0035a13_8": "且超然拔萃一句。",
    "X70n1375_001:0035a13_9": "作麼生道。",
    "X70n1375_001:0035a13_10": "良久云。",
    "X70n1375_001:0035a13_11": "寒山逢拾得。",
    "X70n1375_001:0035a14_0": "撫掌笑呵呵。",
    "X70n1375_001:0035a15_0": "師在妙果。",
    "X70n1375_001:0035a15_1": "受信州龜峰請。",
    "X70n1375_001:0035a15_2": "辭眾上堂。",
    "X70n1375_001:0035a15_3": "披毛帶角入[廓-享+墨]來。",
    "X70n1375_001:0035a16_4": "跳出驢胎墮馬胎。",
    "X70n1375_001:0035a16_5": "佛手明明遮不得。",
    "X70n1375_001:0035a16_6": "從教平地起風雷。",
    "X70n1375_001:0035a18_0": "入院上堂。",
    "X70n1375_001:0035a18_1": "古帆高挂入曹源。",
    "X70n1375_001:0035a18_2": "雷動風行海嶽昏。",
    "X70n1375_001:0035a18_3": "三十二峰親坐斷。",
    "X70n1375_001:0035a19_4": "以拄杖卓一下云。",
    "X70n1375_001:0035a19_5": "一毛頭上定乾坤。",
    "X70n1375_001:0035a19_6": "提起拄杖云。",
    "X70n1375_001:0035a20_7": "看看。",
    "X70n1375_001:0035a20_8": "十方普現。",
    "X70n1375_001:0035a20_9": "剎海齊彰。",
    "X70n1375_001:0035a20_10": "走使文殊普賢。",
    "X70n1375_001:0035a21_0": "驅馳釋迦彌勒。",
    "X70n1375_001:0035a21_1": "把定則銀山銕壁。",
    "X70n1375_001:0035a21_2": "線路不通。",
    "X70n1375_001:0035a21_3": "放行則匝地普天。",
    "X70n1375_001:0035a22_4": "三頭六臂。",
    "X70n1375_001:0035a22_5": "不放行。",
    "X70n1375_001:0035a22_6": "不把住。",
    "X70n1375_001:0035a22_7": "如王寶劍。",
    "X70n1375_001:0035a22_8": "凜凜神威。",
    "X70n1375_001:0035a23_9": "擬犯鋒鋩。",
    "X70n1375_001:0035a23_10": "橫屍萬里。",
    "X70n1375_001:0035a23_11": "這裏見得徹去。",
    "X70n1375_001:0035a23_12": "堪報不報之恩。",
    "X70n1375_001:0035a24_13": "共助無為之化。",
    "X70n1375_001:0035a24_14": "其或未然。",
    "X70n1375_001:0035a24_15": "龜峰不免為蛇畫足去也。",
    "X70n1375_001:0035b01_16": "以拄杖。",
    "X70n1375_001:0035b01_17": "卓一下云。",
    "X70n1375_001:0035b01_18": "鵰弓已挂狼煙息。",
    "X70n1375_001:0035b01_19": "萬國謳歌賀太平。",
    "X70n1375_001:0035b03_0": "上堂。",
    "X70n1375_001:0035b03_1": "謝知事頭首。",
    "X70n1375_001:0035b03_2": "山僧乍住龜峰。",
    "X70n1375_001:0035b03_3": "布箇長蛇大陣。",
    "X70n1375_001:0035b03_4": "過去諸如來為先鋒。",
    "X70n1375_001:0035b04_5": "未來修學人為殿後。",
    "X70n1375_001:0035b04_6": "見在諸菩薩為中軍。",
    "X70n1375_001:0035b05_7": "頭擊則尾應。",
    "X70n1375_001:0035b05_8": "尾擊則頭應。",
    "X70n1375_001:0035b05_9": "中間擊則兩頭應。",
    "X70n1375_001:0035b06_0": "直得。",
    "X70n1375_001:0035b06_1": "天應拱手。",
    "X70n1375_001:0035b06_2": "異類潛蹤。",
    "X70n1375_001:0035b06_3": "且道。",
    "X70n1375_001:0035b06_4": "坐籌帷幄。",
    "X70n1375_001:0035b06_5": "不動干戈一句。",
    "X70n1375_001:0035b07_6": "作麼生道。",
    "X70n1375_001:0035b07_7": "良久云。",
    "X70n1375_001:0035b07_8": "大家齊唱太平歌。",
    "X70n1375_001:0035b08_0": "上堂。",
    "X70n1375_001:0035b08_1": "始見臘月初。",
    "X70n1375_001:0035b08_2": "立春又四日。",
    "X70n1375_001:0035b08_3": "白日走波波。",
    "X70n1375_001:0035b08_4": "肚裏黑似漆。",
    "X70n1375_001:0035b09_5": "攂皷上堂來。",
    "X70n1375_001:0035b09_6": "一字說不出。",
    "X70n1375_001:0035b09_7": "忽有箇漢出來道。",
    "X70n1375_001:0035b09_8": "長老長老。",
    "X70n1375_001:0035b10_9": "爭奈只今口吧吧地。",
    "X70n1375_001:0035b10_10": "聻。",
    "X70n1375_001:0035b10_11": "只對他道。",
    "X70n1375_001:0035b10_12": "山僧失利。",
    "X70n1375_001:0035b11_0": "上堂。",
    "X70n1375_001:0035b11_1": "春風東扇西扇。",
    "X70n1375_001:0035b11_2": "春雨似晴不晴。",
    "X70n1375_001:0035b11_3": "淺白深紅。",
    "X70n1375_001:0035b11_4": "爛鋪錦繡。",
    "X70n1375_001:0035b12_5": "鶯聲燕語。",
    "X70n1375_001:0035b12_6": "互奏笙篁。",
    "X70n1375_001:0035b12_7": "一一揭示圓通妙門。",
    "X70n1375_001:0035b12_8": "頭頭流通正法眼藏。",
    "X70n1375_001:0035b13_9": "擬心湊泊。",
    "X70n1375_001:0035b13_10": "依前萬水千山。",
    "X70n1375_001:0035b13_11": "直下知歸。",
    "X70n1375_001:0035b14_0": "許你七穿八穴。",
    "X70n1375_001:0035b14_1": "拍禪床下座。",
    "X70n1375_001:0035b15_0": "上堂。",
    "X70n1375_001:0035b15_1": "三月既望。",
    "X70n1375_001:0035b15_2": "惠風和暢。",
    "X70n1375_001:0035b15_3": "龜峰攂皷陞堂。",
    "X70n1375_001:0035b15_4": "雲集五湖龍象。",
    "X70n1375_001:0035b16_5": "人人鼻孔遼天。",
    "X70n1375_001:0035b16_6": "好箇西來榜樣。",
    "X70n1375_001:0035b16_7": "眾中莫有不受這般惡水潑底麼。",
    "X70n1375_001:0035b17_8": "出來掀倒禪床。",
    "X70n1375_001:0035b17_9": "喝散大眾。",
    "X70n1375_001:0035b17_10": "便見叢林價增十倍。",
    "X70n1375_001:0035b18_11": "如無。",
    "X70n1375_001:0035b18_12": "龜峰潑第二杓惡水去也。",
    "X70n1375_001:0035b18_13": "以拄杖卓一卓。",
    "X70n1375_001:0035b19_14": "下座。",
    "X70n1375_001:0035b20_0": "上堂。",
    "X70n1375_001:0035b20_1": "時節循環四月一。",
    "X70n1375_001:0035b20_2": "大道何曾有得失。",
    "X70n1375_001:0035b20_3": "夜來針眼魚兒。",
    "X70n1375_001:0035b21_4": "吞却新昌石佛。",
    "X70n1375_001:0035b21_5": "以手撫膝一下云。",
    "X70n1375_001:0035b21_6": "阿呵呵。",
    "X70n1375_001:0035b21_7": "識不識。",
    "X70n1375_001:0035b22_8": "明眼衲僧跳不出。",
    "X70n1375_001:0035b23_0": "上堂。",
    "X70n1375_001:0035b23_1": "阿剌剌。",
    "X70n1375_001:0035b23_2": "快雨快晴。",
    "X70n1375_001:0035b23_3": "天漸熱。",
    "X70n1375_001:0035b23_4": "看取而今甚時節。",
    "X70n1375_001:0035b23_5": "了即萬法本來空。",
    "X70n1375_001:0035b24_6": "未了吾今為君決。",
    "X70n1375_001:0035b24_7": "驀拈拄杖。",
    "X70n1375_001:0035b24_8": "卓一卓云。",
    "X70n1375_001:0035c01_9": "聽著則聾。",
    "X70n1375_001:0035c01_10": "復豎起云。",
    "X70n1375_001:0035c01_11": "覰著則瞎。",
    "X70n1375_001:0035c01_12": "靠倒達磨老臊胡。",
    "X70n1375_001:0035c02_0": "打殺臨濟白拈賊。",
    "X70n1375_001:0035c02_1": "復召大眾云。",
    "X70n1375_001:0035c02_2": "且道拄杖子。",
    "X70n1375_001:0035c02_3": "有甚長處。",
    "X70n1375_001:0035c03_4": "乃擲下云。",
    "X70n1375_001:0035c03_5": "賊賊。",
    "X70n1375_001:0035c04_0": "結制上堂。",
    "X70n1375_001:0035c04_1": "衲僧門下。",
    "X70n1375_001:0035c04_2": "三月一結。",
    "X70n1375_001:0035c04_3": "把斷要津。",
    "X70n1375_001:0035c04_4": "聖凡路絕。",
    "X70n1375_001:0035c05_0": "一夜簷頭雨滴聲。",
    "X70n1375_001:0035c05_1": "剛把真機俱漏泄。",
    "X70n1375_001:0035c05_2": "喝一喝。",
    "X70n1375_001:0035c05_3": "切忌守著繫驢橛。",
    "X70n1375_001:0035c07_0": "上堂。",
    "X70n1375_001:0035c07_1": "舉真如喆和尚道。",
    "X70n1375_001:0035c07_2": "慧光一訣。",
    "X70n1375_001:0035c07_3": "千聖齊列。",
    "X70n1375_001:0035c07_4": "臘月和風。",
    "X70n1375_001:0035c08_5": "炎天氷雪。",
    "X70n1375_001:0035c08_6": "是何之訣。",
    "X70n1375_001:0035c08_7": "鋒鋩結舌。",
    "X70n1375_001:0035c08_8": "龜峰也有箇道處。",
    "X70n1375_001:0035c09_0": "龜峰一訣。",
    "X70n1375_001:0035c09_1": "千聖罔測。",
    "X70n1375_001:0035c09_2": "硬似泥團。",
    "X70n1375_001:0035c09_3": "爛如生銕。",
    "X70n1375_001:0035c09_4": "是何之訣。",
    "X70n1375_001:0035c10_0": "有口無舌。",
    "X70n1375_001:0035c11_0": "上堂。",
    "X70n1375_001:0035c11_1": "步步釋迦出世。",
    "X70n1375_001:0035c11_2": "時時彌勒下生。",
    "X70n1375_001:0035c11_3": "諸人還見露柱燈籠麼。",
    "X70n1375_001:0035c12_4": "若見得。",
    "X70n1375_001:0035c12_5": "便識得釋迦彌勒。",
    "X70n1375_001:0035c12_6": "直饒識得。",
    "X70n1375_001:0035c12_7": "且道作何面孔。",
    "X70n1375_001:0035c13_8": "設或躊躇。",
    "X70n1375_001:0035c13_9": "這老漢騎天柱峰。",
    "X70n1375_001:0035c13_10": "過武石江去也。",
    "X70n1375_001:0035c14_0": "祈雨上堂。",
    "X70n1375_001:0035c14_1": "兼謝監収。",
    "X70n1375_001:0035c14_2": "舉睦州和尚。",
    "X70n1375_001:0035c14_3": "看華嚴經。",
    "X70n1375_001:0035c14_4": "僧問。",
    "X70n1375_001:0035c14_5": "和尚看什麼經。",
    "X70n1375_001:0035c15_6": "州云。",
    "X70n1375_001:0035c15_7": "大光明雲。",
    "X70n1375_001:0035c15_8": "青色光明雲。",
    "X70n1375_001:0035c15_9": "紫色光明雲。",
    "X70n1375_001:0035c16_10": "那邊是什麼雲。",
    "X70n1375_001:0035c16_11": "僧云。",
    "X70n1375_001:0035c16_12": "南邊是黑雲。",
    "X70n1375_001:0035c16_13": "州云。",
    "X70n1375_001:0035c16_14": "今日應有雨。",
    "X70n1375_001:0035c17_15": "師云。",
    "X70n1375_001:0035c17_16": "南邊是黑雲。",
    "X70n1375_001:0035c17_17": "今日應有雨。",
    "X70n1375_001:0035c17_18": "宜麻宜豆。",
    "X70n1375_001:0035c17_19": "宜禾宜黍。",
    "X70n1375_001:0035c18_20": "雖然歲稔年豐。",
    "X70n1375_001:0035c18_21": "粒粒幾多辛苦。",
    "X70n1375_001:0035c18_22": "為報參玄人。",
    "X70n1375_001:0035c18_23": "要須知落處。",
    "X70n1375_001:0035c19_24": "若也知得。",
    "X70n1375_001:0035c19_25": "受用無窮。",
    "X70n1375_001:0035c19_26": "若也未知。",
    "X70n1375_001:0035c19_27": "有煩監収諸公勤舊。",
    "X70n1375_001:0035c21_0": "散楞嚴會上堂。",
    "X70n1375_001:0035c21_1": "將聞持佛佛。",
    "X70n1375_001:0035c21_2": "何不自聞聞。",
    "X70n1375_001:0035c21_3": "曼殊室利。",
    "X70n1375_001:0035c22_0": "五臟心肝。",
    "X70n1375_001:0035c22_1": "盡情吐露。",
    "X70n1375_001:0035c22_2": "這裏薦得。",
    "X70n1375_001:0035c22_3": "九十日內。",
    "X70n1375_001:0035c22_4": "異口一音。",
    "X70n1375_001:0035c23_0": "諷演摩訶悉怛哆般怛囉。",
    "X70n1375_001:0035c23_1": "無上神呪。",
    "X70n1375_001:0035c23_2": "聲聲無間。",
    "X70n1375_001:0035c23_3": "無見頂相無為心佛。",
    "X70n1375_001:0035c24_4": "時時見前。",
    "X70n1375_001:0035c24_5": "若也躊躇。",
    "X70n1375_001:0035c24_6": "龜峰不免重下注脚。",
    "X70n1375_001:0036a01_7": "卓拄杖一下。",
    "X70n1375_001:0036a01_8": "遂高聲喚云。",
    "X70n1375_001:0036a01_9": "侍者收取拄杖。",
    "X70n1375_001:0036a02_0": "中秋上堂。",
    "X70n1375_001:0036a02_1": "舉長沙岑。",
    "X70n1375_001:0036a02_2": "與仰山。",
    "X70n1375_001:0036a02_3": "翫月次。",
    "X70n1375_001:0036a02_4": "仰山指月云。",
    "X70n1375_001:0036a02_5": "人人盡有這箇。",
    "X70n1375_001:0036a03_6": "只是用不得。",
    "X70n1375_001:0036a03_7": "長沙云。",
    "X70n1375_001:0036a03_8": "恰是。",
    "X70n1375_001:0036a03_9": "倩汝用去。",
    "X70n1375_001:0036a03_10": "仰山云。",
    "X70n1375_001:0036a04_11": "作麼生用。",
    "X70n1375_001:0036a04_12": "長沙近前一踏踏倒仰山。",
    "X70n1375_001:0036a04_13": "山起來云。",
    "X70n1375_001:0036a05_0": "直下似箇大蟲。",
    "X70n1375_001:0036a05_1": "山僧有箇頌子。",
    "X70n1375_001:0036a05_2": "舉似大眾。",
    "X70n1375_001:0036a05_3": "浮雲散盡月當空。",
    "X70n1375_001:0036a06_4": "兔子懷胎產大蟲。",
    "X70n1375_001:0036a06_5": "跳出風前弄牙爪。",
    "X70n1375_001:0036a06_6": "至今撼動廣寒宮。",
    "X70n1375_001:0036a08_0": "上堂。",
    "X70n1375_001:0036a08_1": "八月過去又九月。",
    "X70n1375_001:0036a08_2": "時節相催不暫停。",
    "X70n1375_001:0036a08_3": "拈拄杖云。",
    "X70n1375_001:0036a09_0": "雲門大師來也。",
    "X70n1375_001:0036a09_1": "劄。",
    "X70n1375_001:0036a09_2": "久雨不晴。",
    "X70n1375_001:0036a09_3": "唯有衲僧鼻孔。",
    "X70n1375_001:0036a09_4": "依前搭在上唇。",
    "X70n1375_001:0036a10_5": "好大哥。",
    "X70n1375_001:0036a10_6": "卓拄杖一下云。",
    "X70n1375_001:0036a10_7": "撲落非佗物。",
    "X70n1375_001:0036a10_8": "劃一劃云。",
    "X70n1375_001:0036a11_9": "縱橫不是塵。",
    "X70n1375_001:0036a11_10": "尀耐臨濟賊漢。",
    "X70n1375_001:0036a11_11": "喚作無位真人。",
    "X70n1375_001:0036a11_12": "喝一喝。",
    "X70n1375_001:0036a12_13": "下座。",
    "X70n1375_001:0036a13_0": "散眾會上堂。",
    "X70n1375_001:0036a13_1": "舉古德道。",
    "X70n1375_001:0036a13_2": "一大藏教是箇切脚。",
    "X70n1375_001:0036a13_3": "未審切箇什麼字。",
    "X70n1375_001:0036a14_4": "五祖和尚道。",
    "X70n1375_001:0036a14_5": "鉢囉娘。",
    "X70n1375_001:0036a14_6": "圓悟和尚道。",
    "X70n1375_001:0036a14_7": "迅雷不及掩耳。",
    "X70n1375_001:0036a15_8": "二大老與麼切字。",
    "X70n1375_001:0036a15_9": "是即是。",
    "X70n1375_001:0036a15_10": "大煞鄉談。",
    "X70n1375_001:0036a15_11": "忽有問龜峰。",
    "X70n1375_001:0036a16_12": "一大藏教。",
    "X70n1375_001:0036a16_13": "是箇切脚。",
    "X70n1375_001:0036a16_14": "未審切箇什麼字。",
    "X70n1375_001:0036a16_15": "只對他道。",
    "X70n1375_001:0036a17_16": "此去弋陽不遠。",
    "X70n1375_001:0036a18_0": "上堂。",
    "X70n1375_001:0036a18_1": "夜來夢覺。",
    "X70n1375_001:0036a18_2": "忽然記得。",
    "X70n1375_001:0036a18_3": "未出胞胎已前。",
    "X70n1375_001:0036a18_4": "一轉語。",
    "X70n1375_001:0036a18_5": "不敢自謾。",
    "X70n1375_001:0036a19_6": "攂皷陞堂。",
    "X70n1375_001:0036a19_7": "分明舉似大眾。",
    "X70n1375_001:0036a19_8": "不得作奇特商量。",
    "X70n1375_001:0036a20_0": "不得作平實會。",
    "X70n1375_001:0036a20_1": "却這裏揀辨得出。",
    "X70n1375_001:0036a20_2": "許汝出陰界。",
    "X70n1375_001:0036a20_3": "其或未然。",
    "X70n1375_001:0036a21_4": "且待阿逸多出世。",
    "X70n1375_001:0036a21_5": "拍禪床下座。",
    "X70n1375_001:0036a22_0": "上堂。",
    "X70n1375_001:0036a22_1": "舉智門祚和尚道。",
    "X70n1375_001:0036a22_2": "進一步則迷其理。",
    "X70n1375_001:0036a22_3": "退一步則失其事。",
    "X70n1375_001:0036a23_4": "大小智門和尚。",
    "X70n1375_001:0036a23_5": "被理事縛殺。",
    "X70n1375_001:0036a23_6": "龜峰則不然。",
    "X70n1375_001:0036a23_7": "若進一步。",
    "X70n1375_001:0036a24_8": "則三頭六臂。",
    "X70n1375_001:0036a24_9": "退一步。",
    "X70n1375_001:0036a24_10": "則錦上鋪花。",
    "X70n1375_001:0036a24_11": "且道與智門。",
    "X70n1375_001:0036b01_12": "是同是別。",
    "X70n1375_001:0036b01_13": "若道是同。",
    "X70n1375_001:0036b01_14": "法無定相。",
    "X70n1375_001:0036b01_15": "若道是別。",
    "X70n1375_001:0036b01_16": "佛法豈有兩般。",
    "X70n1375_001:0036b02_17": "若道不同不別。",
    "X70n1375_001:0036b02_18": "參學眼。",
    "X70n1375_001:0036b02_19": "在什麼處。",
    "X70n1375_001:0036b02_20": "畢竟如何。",
    "X70n1375_001:0036b03_0": "良久云。",
    "X70n1375_001:0036b03_1": "格。",
    "X70n1375_001:0036b04_0": "上堂。",
    "X70n1375_001:0036b04_1": "兩曜劈箭急。",
    "X70n1375_001:0036b04_2": "一年彈指間。",
    "X70n1375_001:0036b04_3": "始見大暑小暑。",
    "X70n1375_001:0036b04_4": "又是小寒大寒。",
    "X70n1375_001:0036b05_5": "通身寒暑無回互。",
    "X70n1375_001:0036b05_6": "笑倒當年老洞山。",
    "X70n1375_001:0036b06_0": "上堂。",
    "X70n1375_001:0036b06_1": "雨雪落紛紛。",
    "X70n1375_001:0036b06_2": "簷頭水滴滴。",
    "X70n1375_001:0036b06_3": "良哉觀世音。",
    "X70n1375_001:0036b06_4": "草裏跳不出。",
    "X70n1375_001:0036b07_5": "跳不出。",
    "X70n1375_001:0036b07_6": "也大屈。",
    "X70n1375_001:0036b07_7": "水裏烏龜鑽銕壁。",
    "X70n1375_001:0036b07_8": "咄。",
    "X70n1375_001:0036b08_0": "出丫頭岩。",
    "X70n1375_001:0036b08_1": "謁施主歸。",
    "X70n1375_001:0036b08_2": "上堂。",
    "X70n1375_001:0036b08_3": "前日暫出見。",
    "X70n1375_001:0036b08_4": "武石江水。",
    "X70n1375_001:0036b08_5": "穿過丫頭岩鼻孔。",
    "X70n1375_001:0036b09_6": "回到弋陽。",
    "X70n1375_001:0036b09_7": "打從閙市叢中過。",
    "X70n1375_001:0036b09_8": "又見。",
    "X70n1375_001:0036b09_9": "管絃嘹喨。",
    "X70n1375_001:0036b10_10": "車馬駢闐。",
    "X70n1375_001:0036b10_11": "或笑或悲。",
    "X70n1375_001:0036b10_12": "或歌或舞。",
    "X70n1375_001:0036b10_13": "麤言細語。",
    "X70n1375_001:0036b10_14": "百種千般。",
    "X70n1375_001:0036b11_15": "從上諸佛。",
    "X70n1375_001:0036b11_16": "說不到處。",
    "X70n1375_001:0036b11_17": "歷代祖師。",
    "X70n1375_001:0036b11_18": "傳不及處。",
    "X70n1375_001:0036b11_19": "總被當頭道著。",
    "X70n1375_001:0036b12_20": "夜來夢裏記得。",
    "X70n1375_001:0036b12_21": "分分曉曉。",
    "X70n1375_001:0036b12_22": "準擬今辰舉一遍。",
    "X70n1375_001:0036b13_23": "供養大眾。",
    "X70n1375_001:0036b13_24": "五更起來。",
    "X70n1375_001:0036b13_25": "忽被一陣狂風。",
    "X70n1375_001:0036b13_26": "將樓頭鈴鐸一撼。",
    "X70n1375_001:0036b14_27": "山僧直得。",
    "X70n1375_001:0036b14_28": "口似磉磐。",
    "X70n1375_001:0036b14_29": "喝一喝。",
    "X70n1375_001:0036b15_0": "上堂。",
    "X70n1375_001:0036b15_1": "祈雨請觀音。",
    "X70n1375_001:0036b15_2": "兼謝臺山長老。",
    "X70n1375_001:0036b15_3": "拈拄杖云。",
    "X70n1375_001:0036b15_4": "法無定相。",
    "X70n1375_001:0036b16_5": "遇緣即宗。",
    "X70n1375_001:0036b16_6": "乃提起云。",
    "X70n1375_001:0036b16_7": "看看。",
    "X70n1375_001:0036b16_8": "有時變作觀音彌勒。",
    "X70n1375_001:0036b16_9": "魚行酒肆。",
    "X70n1375_001:0036b17_10": "十字縱橫。",
    "X70n1375_001:0036b17_11": "有時變作文殊普賢。",
    "X70n1375_001:0036b17_12": "各據一方。",
    "X70n1375_001:0036b17_13": "助佛揚化。",
    "X70n1375_001:0036b18_14": "而今在龜峰手裏。",
    "X70n1375_001:0036b18_15": "變作天大將軍。",
    "X70n1375_001:0036b18_16": "遊行四天下。",
    "X70n1375_001:0036b19_17": "檢察人間善惡。",
    "X70n1375_001:0036b19_18": "見我山中禪和子。",
    "X70n1375_001:0036b19_19": "祈求懇切。",
    "X70n1375_001:0036b19_20": "踴躍歡喜。",
    "X70n1375_001:0036b20_21": "不覺失脚。",
    "X70n1375_001:0036b20_22": "踏斷天台石橋。",
    "X70n1375_001:0036b20_23": "驚起東海龍王。",
    "X70n1375_001:0036b20_24": "向空中。",
    "X70n1375_001:0036b21_25": "轟一聲霹靂。",
    "X70n1375_001:0036b21_26": "直得乾坤陟暗。",
    "X70n1375_001:0036b21_27": "電走雲奔。",
    "X70n1375_001:0036b21_28": "觀音菩薩。",
    "X70n1375_001:0036b22_0": "忍俊不禁。",
    "X70n1375_001:0036b22_1": "出來。",
    "X70n1375_001:0036b22_2": "念箇真言曰。",
    "X70n1375_001:0036b22_3": "樹前樹後遶須彌。",
    "X70n1375_001:0036b22_4": "地神速報天神知。",
    "X70n1375_001:0036b23_5": "雷公霹靂天門開。",
    "X70n1375_001:0036b23_6": "大雨疾行無使遲。",
    "X70n1375_001:0036b23_7": "乃呵呵大笑云。",
    "X70n1375_001:0036b24_8": "將謂觀音菩薩。",
    "X70n1375_001:0036b24_9": "元來却是圓應大師。",
    "X70n1375_001:0036b24_10": "喝一喝。",
    "X70n1375_001:0036c02_0": "謝監收上堂。",
    "X70n1375_001:0036c02_1": "舉古德道。",
    "X70n1375_001:0036c02_2": "汝千百人中。",
    "X70n1375_001:0036c02_3": "若有一人大肯去。",
    "X70n1375_001:0036c03_4": "山僧作驢駞物。",
    "X70n1375_001:0036c03_5": "供養你。",
    "X70n1375_001:0036c03_6": "有甚麼罪過。",
    "X70n1375_001:0036c03_7": "後來雪峰東山空和尚道。",
    "X70n1375_001:0036c04_8": "古人。",
    "X70n1375_001:0036c04_9": "不妨好心。",
    "X70n1375_001:0036c04_10": "要且揀僧布施。",
    "X70n1375_001:0036c04_11": "擇佛燒香。",
    "X70n1375_001:0036c05_12": "我觀此會。",
    "X70n1375_001:0036c05_13": "千百人。",
    "X70n1375_001:0036c05_14": "無一人不是大肯去底。",
    "X70n1375_001:0036c05_15": "所以自秀峰授了府命。",
    "X70n1375_001:0036c06_16": "即時披這毛衣。",
    "X70n1375_001:0036c06_17": "駞所供養。",
    "X70n1375_001:0036c06_18": "徑入山來。",
    "X70n1375_001:0036c07_0": "入山則不無。",
    "X70n1375_001:0036c07_1": "且道。",
    "X70n1375_001:0036c07_2": "駞底是什麼物。",
    "X70n1375_001:0036c07_3": "又作麼生供養。",
    "X70n1375_001:0036c07_4": "還有見得底麼。",
    "X70n1375_001:0036c08_5": "良久云。",
    "X70n1375_001:0036c08_6": "日日香花夜夜燈。",
    "X70n1375_001:0036c08_7": "師云。",
    "X70n1375_001:0036c08_8": "二尊宿。",
    "X70n1375_001:0036c09_0": "身心不妨殊勝。",
    "X70n1375_001:0036c09_1": "撿點將來。",
    "X70n1375_001:0036c09_2": "大似說食不濟飢。",
    "X70n1375_001:0036c09_3": "龜峰論實不論虗。",
    "X70n1375_001:0036c10_4": "這裏眾雖不多。",
    "X70n1375_001:0036c10_5": "若非大肯去底。",
    "X70n1375_001:0036c10_6": "終不到此寂寞之濵。",
    "X70n1375_001:0036c11_7": "山僧老不以筋力為能。",
    "X70n1375_001:0036c11_8": "不暇駞得物。",
    "X70n1375_001:0036c11_9": "供養你。",
    "X70n1375_001:0036c12_10": "只是趂晴割了稻。",
    "X70n1375_001:0036c12_11": "乘時搬入倉。",
    "X70n1375_001:0036c12_12": "好箇休糧藥。",
    "X70n1375_001:0036c12_13": "渾勝別思量。",
    "X70n1375_001:0036c14_0": "上堂。",
    "X70n1375_001:0036c14_1": "清凉法眼和尚道。",
    "X70n1375_001:0036c14_2": "三通皷罷。",
    "X70n1375_001:0036c14_3": "簇簇上來。",
    "X70n1375_001:0036c14_4": "佛法人事。",
    "X70n1375_001:0036c15_5": "一時周畢。",
    "X70n1375_001:0036c15_6": "後來天衣懷和尚道。",
    "X70n1375_001:0036c15_7": "三通皷罷。",
    "X70n1375_001:0036c15_8": "簇簇上來。",
    "X70n1375_001:0036c16_9": "彼此相鈍置。",
    "X70n1375_001:0036c16_10": "古人與麼說話。",
    "X70n1375_001:0036c16_11": "也是徐六擔板。",
    "X70n1375_001:0036c17_0": "上堂。",
    "X70n1375_001:0036c17_1": "月生一。",
    "X70n1375_001:0036c17_2": "拶倒銀山并銕壁。",
    "X70n1375_001:0036c17_3": "月生二。",
    "X70n1375_001:0036c17_4": "土宿騎牛穿閙市。",
    "X70n1375_001:0036c18_5": "月生三。",
    "X70n1375_001:0036c18_6": "屋頭幽鳥語喃喃。",
    "X70n1375_001:0036c18_7": "不是葛藤露布。",
    "X70n1375_001:0036c18_8": "亦非入理深談。",
    "X70n1375_001:0036c19_9": "正與麼時。",
    "X70n1375_001:0036c19_10": "賓主交參一句。",
    "X70n1375_001:0036c19_11": "作麼生道。",
    "X70n1375_001:0036c19_12": "萬仞懸崕垂隻手。",
    "X70n1375_001:0036c20_13": "百花業裏現優曇。",
    "X70n1375_001:0036c21_0": "上堂。",
    "X70n1375_001:0036c21_1": "佛昔於波羅奈。",
    "X70n1375_001:0036c21_2": "轉四諦法輪。",
    "X70n1375_001:0036c21_3": "開眼作夢。",
    "X70n1375_001:0036c21_4": "今乃復轉無上最大法輪。",
    "X70n1375_001:0036c22_5": "炙瘡瘢上更著艾焦。",
    "X70n1375_001:0036c22_6": "眾中莫有超毗盧。",
    "X70n1375_001:0036c23_7": "越釋迦底麼。",
    "X70n1375_001:0036c23_8": "出來向全機獨脫處。",
    "X70n1375_001:0036c23_9": "道取一轉語。",
    "X70n1375_001:0036c24_0": "龜峰拄杖子。",
    "X70n1375_001:0036c24_1": "兩手分付。",
    "X70n1375_001:0036c24_2": "如無。",
    "X70n1375_001:0036c24_3": "龜峰自道去也。",
    "X70n1375_001:0036c24_4": "以拄杖卓一下。",
    "X70n1375_001:0037a01_5": "下座。",
    "X70n1375_001:0037a02_0": "上堂。",
    "X70n1375_001:0037a02_1": "從朝至暮。",
    "X70n1375_001:0037a02_2": "鐘魚皷板。",
    "X70n1375_001:0037a02_3": "為汝諸人。",
    "X70n1375_001:0037a02_4": "發上上機了也。",
    "X70n1375_001:0037a03_0": "若信得及。",
    "X70n1375_001:0037a03_1": "塵沙諸佛。",
    "X70n1375_001:0037a03_2": "在諸人脚跟下[跳-兆+孛]跳。",
    "X70n1375_001:0037a03_3": "若信不及。",
    "X70n1375_001:0037a04_0": "龜峰拾得口喫飯。",
    "X70n1375_001:0037a04_1": "拍禪床下座。",
    "X70n1375_001:0037a05_0": "上堂。",
    "X70n1375_001:0037a05_1": "汾州和尚道。",
    "X70n1375_001:0037a05_2": "識得拄杖子。",
    "X70n1375_001:0037a05_3": "行脚事畢。",
    "X70n1375_001:0037a05_4": "驀拈拄杖。",
    "X70n1375_001:0037a06_0": "豎起云。",
    "X70n1375_001:0037a06_1": "這箇是拄杖子。",
    "X70n1375_001:0037a06_2": "作麼生是行脚事。",
    "X70n1375_001:0037a06_3": "卓一下云。",
    "X70n1375_001:0037a07_0": "不會則生銕鑄成。",
    "X70n1375_001:0037a07_1": "會則匙挑不上。",
    "X70n1375_001:0037a07_2": "直饒向汾州未開口已前。",
    "X70n1375_001:0037a08_3": "別有轉身一路。",
    "X70n1375_001:0037a08_4": "也是雪峰道底。",
    "X70n1375_001:0037a08_5": "拍禪床。",
    "X70n1375_001:0037a08_6": "下座。",
    "X70n1375_001:0037a09_0": "散華嚴會上堂。",
    "X70n1375_001:0037a09_1": "僧問。",
    "X70n1375_001:0037a09_2": "一塵含法界。",
    "X70n1375_001:0037a09_3": "一念越毗盧。",
    "X70n1375_001:0037a09_4": "一句宣無量義門。",
    "X70n1375_001:0037a10_5": "一相統無邊妙相。",
    "X70n1375_001:0037a10_6": "如何是四法界。",
    "X70n1375_001:0037a10_7": "師云。",
    "X70n1375_001:0037a11_0": "無孔銕鎚重下楔。",
    "X70n1375_001:0037a11_1": "僧云。",
    "X70n1375_001:0037a11_2": "如何是理法界。",
    "X70n1375_001:0037a11_3": "師云。",
    "X70n1375_001:0037a11_4": "一塵入正受。",
    "X70n1375_001:0037a12_5": "僧云。",
    "X70n1375_001:0037a12_6": "如何是事法界。",
    "X70n1375_001:0037a12_7": "師云。",
    "X70n1375_001:0037a12_8": "諸塵三昧起。",
    "X70n1375_001:0037a12_9": "僧云。",
    "X70n1375_001:0037a12_10": "如何是理事無礙法界。",
    "X70n1375_001:0037a13_11": "師云。",
    "X70n1375_001:0037a13_12": "淨瓶裏吐唾。",
    "X70n1375_001:0037a13_13": "鉢盂裏屙屎。",
    "X70n1375_001:0037a14_0": "僧云。",
    "X70n1375_001:0037a14_1": "如何是事事無礙法界。",
    "X70n1375_001:0037a14_2": "師云。",
    "X70n1375_001:0037a14_3": "荒田不揀拈來草。",
    "X70n1375_001:0037a15_0": "生殺縱橫得自由。",
    "X70n1375_001:0037a15_1": "僧云。",
    "X70n1375_001:0037a15_2": "法界已蒙師指示。",
    "X70n1375_001:0037a15_3": "向上宗乘事若何。",
    "X70n1375_001:0037a16_4": "師云。",
    "X70n1375_001:0037a16_5": "言多去道轉遠　復云。",
    "X70n1375_001:0037a16_6": "古德道。",
    "X70n1375_001:0037a16_7": "一大藏教。",
    "X70n1375_001:0037a17_8": "是箇切脚。",
    "X70n1375_001:0037a17_9": "又道一大藏教。",
    "X70n1375_001:0037a17_10": "是箇之字。",
    "X70n1375_001:0037a17_11": "龜峰則不然。",
    "X70n1375_001:0037a18_0": "乃豎起掌云。",
    "X70n1375_001:0037a18_1": "一大藏教。",
    "X70n1375_001:0037a18_2": "盡在龜峰指甲縫裏。",
    "X70n1375_001:0037a18_3": "展開則彌綸三界。",
    "X70n1375_001:0037a19_4": "囊括十虗。",
    "X70n1375_001:0037a19_5": "玉轉珠回。",
    "X70n1375_001:0037a19_6": "輝天鑑地。",
    "X70n1375_001:0037a19_7": "揑聚則綿綿密密。",
    "X70n1375_001:0037a20_8": "不漏絲毫。",
    "X70n1375_001:0037a20_9": "三乘罔測其由。",
    "X70n1375_001:0037a20_10": "千聖罕窮其際。",
    "X70n1375_001:0037a20_11": "不放開。",
    "X70n1375_001:0037a21_12": "不揑聚。",
    "X70n1375_001:0037a21_13": "一多相入。",
    "X70n1375_001:0037a21_14": "理事圓融。",
    "X70n1375_001:0037a21_15": "一門通貫一切門。",
    "X70n1375_001:0037a22_0": "一法徧含一切法。",
    "X70n1375_001:0037a22_1": "普使人人。",
    "X70n1375_001:0037a22_2": "三百六十骨節。",
    "X70n1375_001:0037a22_3": "一一現無量妙身。",
    "X70n1375_001:0037a23_4": "八萬肆千毛端。",
    "X70n1375_001:0037a23_5": "頭頭彰寶王剎海。",
    "X70n1375_001:0037a23_6": "到這裏。",
    "X70n1375_001:0037a24_0": "入息不居陰界。",
    "X70n1375_001:0037a24_1": "出息不涉萬緣。",
    "X70n1375_001:0037a24_2": "常轉如是經。",
    "X70n1375_001:0037a24_3": "百千萬億卷。",
    "X70n1375_001:0037b01_4": "且道。",
    "X70n1375_001:0037b01_5": "不落功勛一句。",
    "X70n1375_001:0037b01_6": "作麼生道。",
    "X70n1375_001:0037b01_7": "良久云。",
    "X70n1375_001:0037b01_8": "百匝千重俱粉碎。",
    "X70n1375_001:0037b02_9": "倚天長劍逼人寒。",
    "X70n1375_001:0037b03_0": "普請拽木上堂。",
    "X70n1375_001:0037b03_1": "三條椽下。",
    "X70n1375_001:0037b03_2": "衲帔蒙頭。",
    "X70n1375_001:0037b03_3": "百不知百不會。",
    "X70n1375_001:0037b04_0": "萬年一念。",
    "X70n1375_001:0037b04_1": "一念萬年。",
    "X70n1375_001:0037b04_2": "未是放身捨命處。",
    "X70n1375_001:0037b04_3": "深山裏钁頭邊。",
    "X70n1375_001:0037b05_4": "開畬種粟。",
    "X70n1375_001:0037b05_5": "搬土拽木。",
    "X70n1375_001:0037b05_6": "脚頭脚尾。",
    "X70n1375_001:0037b05_7": "橫三豎四。",
    "X70n1375_001:0037b05_8": "未是放身捨命處。",
    "X70n1375_001:0037b06_9": "擊碎兩重關。",
    "X70n1375_001:0037b06_10": "別有生機一路。",
    "X70n1375_001:0037b06_11": "龜峰有棒。",
    "X70n1375_001:0037b06_12": "未到你喫。",
    "X70n1375_001:0037b07_13": "且超然獨脫一句。",
    "X70n1375_001:0037b07_14": "作麼生道。",
    "X70n1375_001:0037b07_15": "喝一喝云。",
    "X70n1375_001:0037b07_16": "漆桶參堂去。",
    "X70n1375_001:0037b09_0": "上堂。",
    "X70n1375_001:0037b09_1": "平旦清晨三月朝。",
    "X70n1375_001:0037b09_2": "南山蒼翠插雲霄。",
    "X70n1375_001:0037b09_3": "不須更覓西來意。",
    "X70n1375_001:0037b10_4": "牕外數聲婆餅焦。",
    "X70n1375_001:0037b10_5": "拍膝云。",
    "X70n1375_001:0037b10_6": "好大哥。",
    "X70n1375_001:0037b11_0": "江迪功四月八日。",
    "X70n1375_001:0037b11_1": "請陞座說偈。",
    "X70n1375_001:0037b11_2": "我佛生辰四月八。",
    "X70n1375_001:0037b11_3": "地涌金蓮隨步發。",
    "X70n1375_001:0037b12_4": "天上天下獨稱尊。",
    "X70n1375_001:0037b12_5": "微妙真機俱漏泄。",
    "X70n1375_001:0037b13_0": "雪山成道度眾生。",
    "X70n1375_001:0037b13_1": "四十九年無剩說。",
    "X70n1375_001:0037b13_2": "滔滔苦海架舟航。",
    "X70n1375_001:0037b14_3": "杳杳昏衢懸日月。",
    "X70n1375_001:0037b14_4": "文仲宣義濟陽公。",
    "X70n1375_001:0037b14_5": "曾向靈山蒙起莂。",
    "X70n1375_001:0037b15_6": "示現人間長者身。",
    "X70n1375_001:0037b15_7": "日用機輪活鱍鱍。",
    "X70n1375_001:0037b15_8": "一壽二富三康寧。",
    "X70n1375_001:0037b16_9": "總是前生修種得。",
    "X70n1375_001:0037b16_10": "今生福上更增修。",
    "X70n1375_001:0037b16_11": "果報來生愈超越。",
    "X70n1375_001:0037b17_12": "六門晝夜放毫光。",
    "X70n1375_001:0037b17_13": "萬罪紅爐飛片雪。",
    "X70n1375_001:0037b17_14": "安然端坐白牛車。",
    "X70n1375_001:0037b18_15": "金鎖玄關盡透脫。",
    "X70n1375_001:0037b18_16": "殊勝功德利無邊。",
    "X70n1375_001:0037b18_17": "宜爾子孫皆顯達。",
    "X70n1375_001:0037b19_18": "自知作佛更無疑。",
    "X70n1375_001:0037b19_19": "何必當來問彌勒。",
    "X70n1375_001:0037b20_0": "龜峰末後為全提。",
    "X70n1375_001:0037b20_1": "銕卵生兒樹上飛。",
    "X70n1375_001:0037b21_0": "聖節上堂。",
    "X70n1375_001:0037b21_1": "威德熾盛與端嚴。",
    "X70n1375_001:0037b21_2": "名稱普聞及尊貴。",
    "X70n1375_001:0037b21_3": "如是六種義成就。",
    "X70n1375_001:0037b22_4": "饒益世間諸有情。",
    "X70n1375_001:0037b22_5": "莫有知恩報恩者麼。",
    "X70n1375_001:0037b23_0": "下座同詣大殿。",
    "X70n1375_001:0037b23_1": "啟建　聖節道場。",
    "X70n1375_001:0037b24_0": "重九上堂。",
    "X70n1375_001:0037b24_1": "天地與我同根。",
    "X70n1375_001:0037b24_2": "萬物與我一體。",
    "X70n1375_001:0037b24_3": "且如天高地厚。",
    "X70n1375_001:0037c01_4": "水闊山遙。",
    "X70n1375_001:0037c01_5": "鵠白烏玄。",
    "X70n1375_001:0037c01_6": "松直棘曲。",
    "X70n1375_001:0037c01_7": "纖洪長短。",
    "X70n1375_001:0037c01_8": "一一不同。",
    "X70n1375_001:0037c02_9": "如何得同一體去。",
    "X70n1375_001:0037c02_10": "設使不受人謾。",
    "X70n1375_001:0037c02_11": "直下當頭。",
    "X70n1375_001:0037c02_12": "坐斷。",
    "X70n1375_001:0037c03_13": "我更問你。",
    "X70n1375_001:0037c03_14": "古佛與露柱相交。",
    "X70n1375_001:0037c03_15": "是第幾機。",
    "X70n1375_001:0037c03_16": "乃喝一喝。",
    "X70n1375_001:0037c04_0": "記得。",
    "X70n1375_001:0037c04_1": "僧問古德。",
    "X70n1375_001:0037c04_2": "如何是祖師西來意。",
    "X70n1375_001:0037c04_3": "古德云。",
    "X70n1375_001:0037c04_4": "東籬黃菊。",
    "X70n1375_001:0037c05_5": "僧云。",
    "X70n1375_001:0037c05_6": "意旨如何。",
    "X70n1375_001:0037c05_7": "古德云。",
    "X70n1375_001:0037c05_8": "九日重陽。",
    "X70n1375_001:0037c05_9": "若是恁麼會。",
    "X70n1375_001:0037c05_10": "未夢見祖師西來意在。",
    "X70n1375_001:0037c06_11": "忽問龜峰。",
    "X70n1375_001:0037c06_12": "如何是祖師西來意。",
    "X70n1375_001:0037c07_0": "東籬黃菊。",
    "X70n1375_001:0037c07_1": "意旨如何。",
    "X70n1375_001:0037c07_2": "九日重陽。",
    "X70n1375_001:0037c07_3": "直是恁麼會始得。",
    "X70n1375_001:0037c07_4": "直饒會得。",
    "X70n1375_001:0037c08_5": "正好打折驢腰。",
    "X70n1375_001:0037c09_0": "上堂。",
    "X70n1375_001:0037c09_1": "知之一字。",
    "X70n1375_001:0037c09_2": "眾妙之門。",
    "X70n1375_001:0037c09_3": "喝一喝云。",
    "X70n1375_001:0037c09_4": "是何言歟。",
    "X70n1375_001:0037c09_5": "利刃有蜜。",
    "X70n1375_001:0037c10_6": "舐之有割舌之傷。",
    "X70n1375_001:0037c10_7": "蠱毒之鄉。",
    "X70n1375_001:0037c10_8": "水也不得沾他一滴。",
    "X70n1375_001:0037c11_9": "昨日與麼。",
    "X70n1375_001:0037c11_10": "事不獲已。",
    "X70n1375_001:0037c11_11": "今日與麼。",
    "X70n1375_001:0037c11_12": "罪過彌天。",
    "X70n1375_001:0037c11_13": "忽有箇漢出來道。",
    "X70n1375_001:0037c12_14": "也是作賊人心虗。",
    "X70n1375_001:0037c12_15": "便好與他劈胸一拳。",
    "X70n1375_001:0037c12_16": "何故。",
    "X70n1375_001:0037c13_17": "免教伊向語脉裏著到。",
    "X70n1375_001:0037c14_0": "謝新舊知事上堂。",
    "X70n1375_001:0037c14_1": "衲僧用處。",
    "X70n1375_001:0037c14_2": "鶻眼難窺。",
    "X70n1375_001:0037c14_3": "掀翻諸祖葛藤。",
    "X70n1375_001:0037c15_4": "扶豎叢林保社。",
    "X70n1375_001:0037c15_5": "或進或退。",
    "X70n1375_001:0037c15_6": "或卷或舒。",
    "X70n1375_001:0037c15_7": "劍刃上翻身。",
    "X70n1375_001:0037c16_0": "氷凌上走馬。",
    "X70n1375_001:0037c16_1": "臨機縱奪互顯。",
    "X70n1375_001:0037c16_2": "主賓。",
    "X70n1375_001:0037c16_3": "豎去橫來。",
    "X70n1375_001:0037c16_4": "通身手眼則故是。",
    "X70n1375_001:0037c17_5": "只如興化打克賓罰底饡飯。",
    "X70n1375_001:0037c17_6": "青平笊籬木杓。",
    "X70n1375_001:0037c18_7": "還撈摝得上也無。",
    "X70n1375_001:0037c18_8": "這裏倜儻分明。",
    "X70n1375_001:0037c18_9": "許汝具衲僧眼。",
    "X70n1375_001:0037c19_0": "其或未然。",
    "X70n1375_001:0037c19_1": "曹源水急。",
    "X70n1375_001:0037c19_2": "迦葉峰高。",
    "X70n1375_001:0037c20_0": "臘八上堂。",
    "X70n1375_001:0037c20_1": "明星覰著眼睛枯。",
    "X70n1375_001:0037c20_2": "絕後何曾得再蘇。",
    "X70n1375_001:0037c20_3": "堪笑堪悲緣底事。",
    "X70n1375_001:0037c21_4": "一年一度被茶糊。",
    "X70n1375_001:0037c22_0": "上堂。",
    "X70n1375_001:0037c22_1": "過去諸如來。",
    "X70n1375_001:0037c22_2": "斯門已成就。",
    "X70n1375_001:0037c22_3": "三生六十劫。",
    "X70n1375_001:0037c22_4": "見在諸菩薩。",
    "X70n1375_001:0037c23_5": "今各入圓明。",
    "X70n1375_001:0037c23_6": "墮坑落塹。",
    "X70n1375_001:0037c23_7": "未來修學人。",
    "X70n1375_001:0037c23_8": "當依如是法。",
    "X70n1375_001:0037c24_9": "矢上加尖。",
    "X70n1375_001:0038a01_0": "為施主陞座說偈。",
    "X70n1375_001:0038a01_1": "弋陽溪上諸檀越。",
    "X70n1375_001:0038a01_2": "勇猛精進難比況。",
    "X70n1375_001:0038a02_3": "年年歲歲入山來。",
    "X70n1375_001:0038a02_4": "總是如來親眷屬。",
    "X70n1375_001:0038a02_5": "如來法身等虗空。",
    "X70n1375_001:0038a03_6": "隨彼願心而顯發。",
    "X70n1375_001:0038a03_7": "譬如月輪出霄漢。",
    "X70n1375_001:0038a03_8": "照耀一切眾生前。",
    "X70n1375_001:0038a04_9": "眾生性水無有邊。",
    "X70n1375_001:0038a04_10": "我此一月能普現。",
    "X70n1375_001:0038a04_11": "水靜而清現全體。",
    "X70n1375_001:0038a05_12": "月非取水而遽來。",
    "X70n1375_001:0038a05_13": "水動而濁無定光。",
    "X70n1375_001:0038a05_14": "月非捨水而遽去。",
    "X70n1375_001:0038a06_15": "水有清濁有動靜。",
    "X70n1375_001:0038a06_16": "月無取捨無去來。",
    "X70n1375_001:0038a06_17": "於無取捨去來中。",
    "X70n1375_001:0038a07_18": "不見清濁動靜相。",
    "X70n1375_001:0038a07_19": "既無清濁動靜相。",
    "X70n1375_001:0038a08_0": "一一含攝無有餘。",
    "X70n1375_001:0038a08_1": "我心佛心眾生心。",
    "X70n1375_001:0038a08_2": "心佛眾生本平等。",
    "X70n1375_001:0038a09_3": "佛子當作如是觀。",
    "X70n1375_001:0038a09_4": "即獲世間諸利益。",
    "X70n1375_001:0038a10_0": "歲夜小參。",
    "X70n1375_001:0038a10_1": "記得。",
    "X70n1375_001:0038a10_2": "死心和尚云。",
    "X70n1375_001:0038a10_3": "叢林小參。",
    "X70n1375_001:0038a10_4": "謂之家訓。",
    "X70n1375_001:0038a10_5": "莫是談玄說妙。",
    "X70n1375_001:0038a11_6": "舉古明今。",
    "X70n1375_001:0038a11_7": "五日上堂。",
    "X70n1375_001:0038a11_8": "三朝入室麼。",
    "X70n1375_001:0038a11_9": "錯。",
    "X70n1375_001:0038a11_10": "莫是行須。",
    "X70n1375_001:0038a12_11": "緩步。",
    "X70n1375_001:0038a12_12": "語要低聲。",
    "X70n1375_001:0038a12_13": "嚴淨律儀。",
    "X70n1375_001:0038a12_14": "精持戒行麼。",
    "X70n1375_001:0038a12_15": "錯。",
    "X70n1375_001:0038a12_16": "莫是行棒行喝。",
    "X70n1375_001:0038a13_17": "吞栗棘蓬。",
    "X70n1375_001:0038a13_18": "透金剛圈。",
    "X70n1375_001:0038a13_19": "輥雪峰毬。",
    "X70n1375_001:0038a13_20": "擔睦州板麼。",
    "X70n1375_001:0038a14_21": "錯。",
    "X70n1375_001:0038a14_22": "衲僧家。",
    "X70n1375_001:0038a14_23": "出一叢林。",
    "X70n1375_001:0038a14_24": "入一保社。",
    "X70n1375_001:0038a14_25": "箇箇頂門具眼。",
    "X70n1375_001:0038a15_0": "人人肘後有符。",
    "X70n1375_001:0038a15_1": "見自己。",
    "X70n1375_001:0038a15_2": "如生冤家。",
    "X70n1375_001:0038a15_3": "聞禪道。",
    "X70n1375_001:0038a15_4": "如風過樹。",
    "X70n1375_001:0038a16_0": "有時孤危壁立。",
    "X70n1375_001:0038a16_1": "線路不通。",
    "X70n1375_001:0038a16_2": "有時合水和泥。",
    "X70n1375_001:0038a16_3": "縱橫十字。",
    "X70n1375_001:0038a17_0": "逢佛殺佛。",
    "X70n1375_001:0038a17_1": "逢祖殺祖。",
    "X70n1375_001:0038a17_2": "造地獄業。",
    "X70n1375_001:0038a17_3": "結無間因。",
    "X70n1375_001:0038a17_4": "馬頷驢顋。",
    "X70n1375_001:0038a18_0": "神頭鬼面。",
    "X70n1375_001:0038a18_1": "終日坐而未甞坐。",
    "X70n1375_001:0038a18_2": "終日行而未甞行。",
    "X70n1375_001:0038a18_3": "終日著衣。",
    "X70n1375_001:0038a19_4": "未甞挂一縷絲。",
    "X70n1375_001:0038a19_5": "終日喫飯未甞咬一粒米。",
    "X70n1375_001:0038a19_6": "卷舒出沒。",
    "X70n1375_001:0038a20_7": "蕩蕩無拘。",
    "X70n1375_001:0038a20_8": "如水上葫蘆。",
    "X70n1375_001:0038a20_9": "觸著便動。",
    "X70n1375_001:0038a20_10": "拶著便轉轆轆地。",
    "X70n1375_001:0038a21_11": "如何近傍。",
    "X70n1375_001:0038a21_12": "如何湊泊。",
    "X70n1375_001:0038a21_13": "直饒三世諸佛。",
    "X70n1375_001:0038a21_14": "歷代宗師。",
    "X70n1375_001:0038a22_0": "古往今來。",
    "X70n1375_001:0038a22_1": "大善知識。",
    "X70n1375_001:0038a22_2": "各各現三頭六臂出來。",
    "X70n1375_001:0038a22_3": "只得拱手歸降。",
    "X70n1375_001:0038a23_4": "撿點將來。",
    "X70n1375_001:0038a23_5": "猶是守館驛。",
    "X70n1375_001:0038a23_6": "撮馬糞底見解。",
    "X70n1375_001:0038a23_7": "向衲僧門下。",
    "X70n1375_001:0038a24_8": "更買草鞋行脚在。",
    "X70n1375_001:0038a24_9": "那堪略無所證。",
    "X70n1375_001:0038a24_10": "一味虗頭。",
    "X70n1375_001:0038b01_0": "誣謗先賢。",
    "X70n1375_001:0038b01_1": "欺謾自己。",
    "X70n1375_001:0038b01_2": "等閑拶著。",
    "X70n1375_001:0038b01_3": "兩脚捎空。",
    "X70n1375_001:0038b01_4": "乃以手拍膝云。",
    "X70n1375_001:0038b02_5": "噁。",
    "X70n1375_001:0038b02_6": "大丈夫兒。",
    "X70n1375_001:0038b02_7": "阿誰無分。",
    "X70n1375_001:0038b02_8": "隨身契券。",
    "X70n1375_001:0038b02_9": "本自分明。",
    "X70n1375_001:0038b02_10": "若解參詳。",
    "X70n1375_001:0038b03_11": "管取青霄獨步。",
    "X70n1375_001:0038b03_12": "且轉身一句。",
    "X70n1375_001:0038b03_13": "必竟如何。",
    "X70n1375_001:0038b03_14": "舉頭天外看。",
    "X70n1375_001:0038b04_15": "誰是我般人　復舉。",
    "X70n1375_001:0038b04_16": "僧問古德。",
    "X70n1375_001:0038b04_17": "生死以何為舟航。",
    "X70n1375_001:0038b05_18": "古德云。",
    "X70n1375_001:0038b05_19": "年盡不燒錢。",
    "X70n1375_001:0038b05_20": "有箇頌。",
    "X70n1375_001:0038b05_21": "舉似大眾。",
    "X70n1375_001:0038b05_22": "年盡不燒錢。",
    "X70n1375_001:0038b06_23": "鼠穴被蛇穿。",
    "X70n1375_001:0038b06_24": "直饒玄會得。",
    "X70n1375_001:0038b06_25": "對面隔西天。",
    "X70n1375_001:0038b07_0": "讚頌",
    "X70n1375_001:0038b08_0": "讚密庵禪師",
    "X70n1375_001:0038b09_0": "狼毒肝腸。",
    "X70n1375_001:0038b09_1": "生銕面具。",
    "X70n1375_001:0038b09_2": "滿口鄉談。",
    "X70n1375_001:0038b09_3": "唱喏破句。",
    "X70n1375_001:0038b09_4": "滅臨濟正宗。",
    "X70n1375_001:0038b10_5": "起東山門戶。",
    "X70n1375_001:0038b10_6": "恁麼出世為人。",
    "X70n1375_001:0038b10_7": "未免秤鎚蘸醋。",
    "X70n1375_001:0038b11_0": "題廬山楞伽院",
    "X70n1375_001:0038b12_0": "蘇內翰木三五筆。",
    "X70n1375_001:0038b12_1": "李夫人竹一二竿。",
    "X70n1375_001:0038b12_2": "會得楞伽印心法。",
    "X70n1375_001:0038b13_3": "不應來作畫圖看。",
    "X70n1375_001:0038b14_0": "題廬山圓通",
    "X70n1375_001:0038b15_0": "水聲山色裏。",
    "X70n1375_001:0038b15_1": "猿嘯月明天。",
    "X70n1375_001:0038b15_2": "喚作圓通境。",
    "X70n1375_001:0038b15_3": "更參三十年。",
    "X70n1375_001:0038b16_0": "題烈山",
    "X70n1375_001:0038b17_0": "舟船來往此登臨。",
    "X70n1375_001:0038b17_1": "酹酒燒錢慮轉深。",
    "X70n1375_001:0038b17_2": "大抵人心自巇嶮。",
    "X70n1375_001:0038b18_3": "心空山嶽自平沈。",
    "X70n1375_001:0038b19_0": "題長干塔",
    "X70n1375_001:0038b20_0": "舍利無端應念來。",
    "X70n1375_001:0038b20_1": "浮圖平地擁崔嵬。",
    "X70n1375_001:0038b20_2": "當年不假金鎚力。",
    "X70n1375_001:0038b21_3": "爭得光明耀九垓。",
    "X70n1375_001:0038b22_0": "送雪峰化主",
    "X70n1375_001:0038b23_0": "三毬輥動錯流傳。",
    "X70n1375_001:0038b23_1": "大用何曾得現前。",
    "X70n1375_001:0038b23_2": "千五百僧無頓放。",
    "X70n1375_001:0038b24_3": "累人走得脚皮穿。",
    "X70n1375_001:0038c01_0": "送聞兄持鉢",
    "X70n1375_001:0038c02_0": "聞聲悟道鳥投籠。",
    "X70n1375_001:0038c02_1": "祇麼無聞道未充。",
    "X70n1375_001:0038c02_2": "利劍拂開慳悋穴。",
    "X70n1375_001:0038c03_3": "全身輥入是非叢。",
    "X70n1375_001:0038c03_4": "脚頭脚尾無虗弁。",
    "X70n1375_001:0038c03_5": "山北山南有路通。",
    "X70n1375_001:0038c04_6": "一笑歸來能事畢。",
    "X70n1375_001:0038c04_7": "真金百鍊見全功。",
    "X70n1375_001:0038c05_0": "送觀禪客",
    "X70n1375_001:0038c06_0": "西淛江邊話別時。",
    "X70n1375_001:0038c06_1": "斷頭船子疾如飛。",
    "X70n1375_001:0038c06_2": "白拈手裏分贓去。",
    "X70n1375_001:0038c07_3": "臨濟休誇小[病-丙+斯]兒。",
    "X70n1375_001:0038c08_0": "送道者幹瀾溪橋",
    "X70n1375_001:0038c09_0": "萬古瀾溪上。",
    "X70n1375_001:0038c09_1": "無風浪拍空。",
    "X70n1375_001:0038c09_2": "路窮橋斷處。",
    "X70n1375_001:0038c09_3": "看子展神通。",
    "X70n1375_001:0038c10_0": "贈不欺信公監總餘杭建接待",
    "X70n1375_001:0038c11_0": "衲僧自有通人愛。",
    "X70n1375_001:0038c11_1": "笑揑空拳開接待。",
    "X70n1375_001:0038c11_2": "十字縱橫古路頭。",
    "X70n1375_001:0038c12_3": "做成活計潑天大。",
    "X70n1375_001:0038c12_4": "阿呵呵。",
    "X70n1375_001:0038c12_5": "也奇怪。",
    "X70n1375_001:0038c12_6": "是聖是凡俱捉敗。",
    "X70n1375_001:0038c13_7": "接取一箇半箇來。",
    "X70n1375_001:0038c13_8": "要與叢林作殃害。",
    "X70n1375_001:0038c15_0": "曹源和尚語錄(終)"
}