[
    {
        "source_sentence": "大慈大悲救苦觀世音自在王",
        "from": "T20n1113B_001:0498c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩廣大圓滿無礙自在青頸",
        "from": "T20n1113B_001:0498c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大悲心陀羅尼",
        "from": "T20n1113B_001:0498c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大廣智不空譯南無歸命頂禮南方海上蒲陀落淨土。",
        "from": "T20n1113B_001:0498c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正法教主釋迦牟尼如來。",
        "from": "T20n1113B_001:0498c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀音本師無量壽如來。",
        "from": "T20n1113B_001:0498c13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀音本正法明如來。",
        "from": "T20n1113B_001:0498c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無千手千眼觀世音菩薩廣大圓滿無礙大悲心大陀羅尼。",
        "from": "T20n1113B_001:0498c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救苦陀羅尼。",
        "from": "T20n1113B_001:0498c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "延壽陀羅尼。",
        "from": "T20n1113B_001:0498c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加滅惡趣陀羅尼。",
        "from": "T20n1113B_001:0498c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破業障陀羅尼滿願陀羅尼。",
        "from": "T20n1113B_001:0498c17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨心自在陀羅尼。",
        "from": "T20n1113B_001:0498c17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速超上地陀羅尼。",
        "from": "T20n1113B_001:0498c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一聞神呪超第八地陀羅尼。",
        "from": "T20n1113B_001:0498c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四百四病一時消滅陀羅尼。",
        "from": "T20n1113B_001:0498c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄖩南􄗭無喝􄗽羅􅑈怛那􄩞哆羅􄗲夜􄗱㖿一此觀世音菩薩本身。",
        "from": "T20n1113B_001:0498c22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大須慈悲思勿高聲此神性忽。",
        "from": "T20n1113B_001:0498c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄖩南􄗭無􆿆阿􅝪唎㖿二是如意輪菩薩本身。",
        "from": "T20n1113B_001:0498c26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到此誦在心。",
        "from": "T20n1113B_001:0498c26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄙖婆􄙒盧􄑂羯􄕝帝􈂫爍鉢􄗾囉􄗱㖿三此是轉鉢觀音。",
        "from": "T20n1113B_001:0498c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔誦此理轉菩薩。",
        "from": "T20n1113B_001:0498c29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄗕菩􄕽提􄙺薩􄽚哆婆􄗱㖿四此不空羂索菩薩。",
        "from": "T20n1113B_001:0499a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降押天兵眾。",
        "from": "T20n1113B_001:0499a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄗥摩􄚩訶􄙺薩􄽚埵婆􄗱㖿五是菩薩種子之因。",
        "from": "T20n1113B_001:0499a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦呪本身。",
        "from": "T20n1113B_001:0499a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄗥摩􄚩訶􄑁迦􄙂嚧􄕍昵􄑁迦􄗱㖿六是馬鳴菩薩本身。",
        "from": "T20n1113B_001:0499a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手把拔折羅印菩薩自身。",
        "from": "T20n1113B_001:0499a09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵七此是神唵語。",
        "from": "T20n1113B_001:0499a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉合掌聽誦呪曰。",
        "from": "T20n1113B_001:0499a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄙺薩􅞣皤􄗽囉􄗙罰􄗷曳八此是四天大王本身。",
        "from": "T20n1113B_001:0499a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降伏魔王。",
        "from": "T20n1113B_001:0499a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄙦數􄕻怛􄖩那􄕯怛􄣢寫九此是四天大王八部鬼神名字。",
        "from": "T20n1113B_001:0499a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄖩南􄗭無􈃎悉吉㗚􄕗埵􆿈伊􄗭蒙􆿅阿􇩈唎耶十此是龍樹菩薩本身。",
        "from": "T20n1113B_001:0499a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須用心誦勿令疎步菩薩性勿也。",
        "from": "T20n1113B_001:0499a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄗍婆􄙂嚧􄑂吉􄕝帝􈂫室佛􄙈楞􈍡馱􄙖婆十一此是本師毘盧遮那佛本身。",
        "from": "T20n1113B_001:0499a25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣大圓滿不可思議功德。",
        "from": "T20n1113B_001:0499a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄖩南􄗭無􄖩那􄗽囉􄑂謹􄕽墀十二此是清淨圓滿報身盧舍那佛本身。",
        "from": "T20n1113B_001:0499a29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須用心勿令放逸。",
        "from": "T20n1113B_001:0499b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄚮醯􄙀利􄗥摩􄙖皤􈍡哆􄙮沙􄗫咩十三此是半頭神王。",
        "from": "T20n1113B_001:0499b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菊大魔以為眷屬。",
        "from": "T20n1113B_001:0499b04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄙺薩􅞣婆􆿅阿􄕤陀􈇚頭􄙦輸􈁎朋十四此是甘露菩薩亦是觀世音菩薩部落。",
        "from": "T20n1113B_001:0499b07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為眷屬。",
        "from": "T20n1113B_001:0499b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􆿅阿􄒼遊􄗻孕十五此是飛騰夜叉大王。",
        "from": "T20n1113B_001:0499b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "巡歷四方察其是非。",
        "from": "T20n1113B_001:0499b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄙺薩􅞣婆􄗝菩􄕗哆􄖩那􄗥摩􄙖縛􄑘伽十六此是婆帝王其形黑大。",
        "from": "T20n1113B_001:0499b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大以豹皮為衣。",
        "from": "T20n1113B_001:0499b14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神手把衣叉。",
        "from": "T20n1113B_001:0499b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄗥摩􄙖罰􈇚特􈇚豆十七此是剎利菩薩本身。",
        "from": "T20n1113B_001:0499b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鐵輪手把夜叉素有三眼是也。",
        "from": "T20n1113B_001:0499b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄕗怛􇩊姪􄕤他十八􇩂唵此是劍語。",
        "from": "T20n1113B_001:0499b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􆿅阿􄙖波􄙒盧􄑀醯􄙒盧􄑀迦􄕝帝十九此是梵天王本身說佛為部。",
        "from": "T20n1113B_001:0499b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄑀迦􄗽羅􄕝帝二十此是立門五神。",
        "from": "T20n1113B_001:0499b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長大黑色。",
        "from": "T20n1113B_001:0499b28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􆿏夷􈋄醯唎二十一此是三十三天王。",
        "from": "T20n1113B_001:0499c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是摩醯首羅天王神領天兵。",
        "from": "T20n1113B_001:0499c03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄗥摩􄚩訶􄗕􄕽菩􄙺薩􄽙埵二十二此是寶心更無雜亂心節名薩埵。",
        "from": "T20n1113B_001:0499c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄙺薩􅞣婆􄙺􅞣薩婆二十三此是香積菩薩。",
        "from": "T20n1113B_001:0499c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "押五方兵薩婆為侍從不可思議。",
        "from": "T20n1113B_001:0499c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄗥摩􄙊羅􄗥􄙊摩羅􄗥摩􄗥摩􈋄醯唎􄕯馱􄗻孕二十四同上􄑄俱􄙂嚧􄑄􄙂俱嚧􄑀羯􅝧蒙二十五此是空身菩薩。",
        "from": "T20n1113B_001:0499c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "押夫將軍領二萬億天兵忽。",
        "from": "T20n1113B_001:0499c15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈃁度􄙂嚧􈃁􄙂度嚧􄙖罰􄒶闍􄗱耶􄕝帝二十六此是嚴峻菩薩。",
        "from": "T20n1113B_001:0499c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孔雀王兵。",
        "from": "T20n1113B_001:0499c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄗥摩􄚨訶􄙖罰􄒶闍􄗱耶􄕝帝二十七同上􄕻陀􄗽羅􄕻􄗽陀羅二十八此是觀世音菩薩。",
        "from": "T20n1113B_001:0499c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大手把金輪。",
        "from": "T20n1113B_001:0499c23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄕽地􄙀利􄕍尼二十九此是師子王。",
        "from": "T20n1113B_001:0499c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兵驗不調。",
        "from": "T20n1113B_001:0499c26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄗾囉􄗱耶三十是霹靂降伏魔眷屬。",
        "from": "T20n1113B_001:0499c29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄑼遮􄙊囉􄑼遮􄙊囉三十一此是摧碎菩薩。",
        "from": "T20n1113B_001:0500a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本身手把金輪。",
        "from": "T20n1113B_001:0500a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄗥摩􄗥摩弟子某甲受持􄙖罰􄗥摩􄗽羅三十二此是大降魔金剛本身。",
        "from": "T20n1113B_001:0500a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手把金輪。",
        "from": "T20n1113B_001:0500a08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈂼穆􈅯帝􄙐曬三十三此是佛合掌。",
        "from": "T20n1113B_001:0500a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聽誦千手千眼觀世音菩薩善神妙章句。",
        "from": "T20n1113B_001:0500a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􆿏伊􄚮醯􆿏移􄚮醯三十四此是喚摩醯首羅天。",
        "from": "T20n1113B_001:0500a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄑾室􆩮那􄑾􆩮室那三十五同上􆿅阿􅟅羅參􇩄佛羅􄑼舍􄙌利三十六此是觀世音菩薩。",
        "from": "T20n1113B_001:0500a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手把軍弓箭。",
        "from": "T20n1113B_001:0500a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄙖罰􄙮沙􄙖罰􄙸參􄪼佛羅􄙢舍􄗱耶三十七此是阿彌陀佛本身。",
        "from": "T20n1113B_001:0500a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀音菩薩師主。",
        "from": "T20n1113B_001:0500a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈂪呼􄙂嚧􈂪􄙂呼嚧􄗥麼􄗽囉三十八此是呼八部鬼神。",
        "from": "T20n1113B_001:0500a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈂪呼􄙂嚧􈂪􄙂呼嚧􄮥醯唎三十九同上􄙺沙􄗽囉􄙺􄗽沙囉四十此是五濁惡世云。",
        "from": "T20n1113B_001:0500b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄙼悉􄙀唎􄙼􄙀悉唎四十一􈂼蘇􄙂嚧􈂼􄙂蘇嚧四十二此是諸佛樹樂木聲也。",
        "from": "T20n1113B_001:0500b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄗕菩􄕽提􄗱㖿􄗕􄕽􄗱菩提㖿四十三此是觀世音豫眾生罪惡。",
        "from": "T20n1113B_001:0500b09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄗕菩􈍡提􄗱耶􄗕􈍡􄗱菩提耶四十四此是阿難本身也。",
        "from": "T20n1113B_001:0500b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄗬彌􄩠帝唎􄗱耶四十五此是本車菩薩手把金刀。",
        "from": "T20n1113B_001:0500b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄖩那􄗽囉􄑂謹􆩰墀四十六此是龍樹菩薩。",
        "from": "T20n1113B_001:0500b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手把金刀之長。",
        "from": "T20n1113B_001:0500b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄕻他􅞽唎瑟􄕍尼􄖩那􄖵波􄗱夜􄗥摩􄖩那四十七此是金光幢菩薩。",
        "from": "T20n1113B_001:0500b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手把跋折羅杵。",
        "from": "T20n1113B_001:0500b21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􅁥娑婆􄚩訶四十八同上􄙼悉􇧺陀􄗱夜四十九此是達一切法門。",
        "from": "T20n1113B_001:0500b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􅁥娑婆􄚩訶五十法語􄗥摩􄚩訶􄙼悉􇧺陀􄗱夜􅁥娑婆􄚩訶此是施光幢菩薩手把幡。",
        "from": "T20n1113B_001:0500b27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄙼悉􇧺陀􄗹喻􄑞藝五十二此是天大菩薩悉手來把刀或菩薩手把水椀。",
        "from": "T20n1113B_001:0500c01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈂫室皤􄑀伽􄗽羅􄗱耶􅁥娑婆􄚩訶五十三此是名安悉香。",
        "from": "T20n1113B_001:0500c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄖩那􄗽囉􄑂謹􆩰墀五十五同上􅁥娑婆􄚩訶五十六􄗥摩􄗽囉􄖩那􄗽囉五十七此是散水菩薩手把水椀。",
        "from": "T20n1113B_001:0500c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􅁥娑婆􄚩訶五十八同上􄙼悉􄗽囉􄚦僧􆿆阿􈁃穆􄑍佉􄗱耶五十九此是山海惠菩薩手把金鉤。",
        "from": "T20n1113B_001:0500c12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􅁥娑婆􄚩訶六十同上􄖵婆􄗥摩􄚩訶􄙼悉􇧺陀􄗱夜六十一同上􅁥娑婆􄚩訶六十二同上􄑼者􄥈吉囉阿􄙼悉􇧺陀􄗱夜六十三此是藥王菩薩本身行魔。",
        "from": "T20n1113B_001:0500c17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􅁥娑婆􄚩訶六十四同上􄖵婆􇪯摩􄑀羯􆶩悉哆􄗱夜六十五是藥上菩薩本身行魔痛。",
        "from": "T20n1113B_001:0500c22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􅁥娑婆􄚩訶六十六同上􄖩那􄗽羅􄑂謹􆩰墀􄙖皤􄑘迦􄗽羅􄗱夜六十七同上􅁥娑婆􄚩訶六十八同上􄗥摩􄙖婆􄙀唎􄙢勝􆥣羯􄗱夜六十九同上􅁥娑婆􄚩訶七十􄖩南􄗭無喝􄗽囉􅑈怛那􄩞多囉􄗲夜􄗱耶七十一􄖩南􄗭無􆿆阿􅝪唎耶七十二􄙖婆􄙆嚧􄑂吉􄕝帝七十三􈂫爍皤􄗽囉􄗱耶七十四􄗕菩􄕽提􅁥娑婆􄚩呵青頸觀音陀羅尼一卷青頸大悲心印虛心合掌屈二頭指。",
        "from": "T20n1113B_001:0501a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各拘二大指第二節是法螺也二中指竪合是蓮花也二無名指竪圓端是輪也二小指直竪合是杖也於一印具四印。",
        "from": "T20n1113B_001:0501a11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂法螺蓮華輪杖也。",
        "from": "T20n1113B_001:0501a12_2",
        "context_with_next_sentence": true
    }
]