[
    {
        "source_sentence": "千手觀音造次第法儀軌",
        "from": "T20n1068_001:0138a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中天竺國三藏善無畏奉　詔譯",
        "from": "T20n1068_001:0138a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上首正體身大黃色。結跏趺座大寶蓮華臺",
        "from": "T20n1068_001:0138a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上。",
        "from": "T20n1068_001:0138a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其華三十二葉。",
        "from": "T20n1068_001:0138a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其一一間有諸小葉。",
        "from": "T20n1068_001:0138a17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以無量百千大摩尼寶為莊嚴也。",
        "from": "T20n1068_001:0138a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其尊之正面天冠上有三重。",
        "from": "T20n1068_001:0138a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸頭面之數有五百。",
        "from": "T20n1068_001:0138a19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當面之左右造兩面。",
        "from": "T20n1068_001:0138a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右名蓮華面。",
        "from": "T20n1068_001:0138a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左名金剛面也。",
        "from": "T20n1068_001:0138a20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右者青碧貌。",
        "from": "T20n1068_001:0138a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左紺白色也。",
        "from": "T20n1068_001:0138a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正面者表佛部。",
        "from": "T20n1068_001:0138a21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是大士有大身故。",
        "from": "T20n1068_001:0138a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三部海會備具也。",
        "from": "T20n1068_001:0138a22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八大菩薩以為眷屬。",
        "from": "T20n1068_001:0138a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大士前有童目天女持可愛華。",
        "from": "T20n1068_001:0138a23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃童子并持經僧座。",
        "from": "T20n1068_001:0138a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其形七歲童子貌。",
        "from": "T20n1068_001:0138a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三重有二十八部眾。",
        "from": "T20n1068_001:0138a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有各各本形。",
        "from": "T20n1068_001:0138a25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1068_001:0138a25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一密迹金剛士。",
        "from": "T20n1068_001:0138a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "赤紅色具三眼。",
        "from": "T20n1068_001:0138a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右持金剛杵。",
        "from": "T20n1068_001:0138a26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手拳安腰。",
        "from": "T20n1068_001:0138a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二烏芻君荼央俱尸。",
        "from": "T20n1068_001:0138a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持一股金剛杵。",
        "from": "T20n1068_001:0138a28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手拳安腰。",
        "from": "T20n1068_001:0138a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八部力士賞迦羅綠色。",
        "from": "T20n1068_001:0138a29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持慧劍。",
        "from": "T20n1068_001:0138b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手三股印作也。",
        "from": "T20n1068_001:0138b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三魔醯那羅達。",
        "from": "T20n1068_001:0138b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黑赤紅色具三眼瞋怒相也。",
        "from": "T20n1068_001:0138b02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以三股揭為天冠。",
        "from": "T20n1068_001:0138b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及金剛寶以為瓔珞。",
        "from": "T20n1068_001:0138b03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持杵。",
        "from": "T20n1068_001:0138b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手把寶盤。",
        "from": "T20n1068_001:0138b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內赤外黑色也。",
        "from": "T20n1068_001:0138b04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四金毘羅陀迦毘羅。",
        "from": "T20n1068_001:0138b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白紅色。",
        "from": "T20n1068_001:0138b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手把寶弓。",
        "from": "T20n1068_001:0138b05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手把寶箭。",
        "from": "T20n1068_001:0138b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五婆馺婆樓那。",
        "from": "T20n1068_001:0138b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白紅色。",
        "from": "T20n1068_001:0138b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手索。",
        "from": "T20n1068_001:0138b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手安腰。",
        "from": "T20n1068_001:0138b07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六滿善車鉢真陀羅。",
        "from": "T20n1068_001:0138b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手金剛輪。",
        "from": "T20n1068_001:0138b08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手拳印。",
        "from": "T20n1068_001:0138b08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "紅色。",
        "from": "T20n1068_001:0138b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七薩遮摩和羅。",
        "from": "T20n1068_001:0138b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手把寶幢。",
        "from": "T20n1068_001:0138b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上有鳳鳥。",
        "from": "T20n1068_001:0138b10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手施願印。",
        "from": "T20n1068_001:0138b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八鳩蘭單吒半祇羅。",
        "from": "T20n1068_001:0138b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手金剛鐸。",
        "from": "T20n1068_001:0138b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手金剛拳。",
        "from": "T20n1068_001:0138b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白紅色。",
        "from": "T20n1068_001:0138b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九畢婆伽羅王。",
        "from": "T20n1068_001:0138b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手把刀。",
        "from": "T20n1068_001:0138b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手安腰。",
        "from": "T20n1068_001:0138b14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十應德毘多薩和羅。",
        "from": "T20n1068_001:0138b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持弓。",
        "from": "T20n1068_001:0138b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手三叉杵箭。",
        "from": "T20n1068_001:0138b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色黃黑也。",
        "from": "T20n1068_001:0138b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十一梵摩三鉢羅。",
        "from": "T20n1068_001:0138b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色紅白。",
        "from": "T20n1068_001:0138b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持寶瓶。",
        "from": "T20n1068_001:0138b17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手三股杵。",
        "from": "T20n1068_001:0138b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二五部淨居炎摩羅。",
        "from": "T20n1068_001:0138b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色紫白。",
        "from": "T20n1068_001:0138b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持炎摩幢。",
        "from": "T20n1068_001:0138b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手女竿。",
        "from": "T20n1068_001:0138b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十三釋王三十三。",
        "from": "T20n1068_001:0138b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色白紅。",
        "from": "T20n1068_001:0138b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手安腰。",
        "from": "T20n1068_001:0138b21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持金剛杵。",
        "from": "T20n1068_001:0138b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十四大辨功德娑怛那。",
        "from": "T20n1068_001:0138b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝釋天王主之女子大德天女也。",
        "from": "T20n1068_001:0138b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多聞天之大妃也。",
        "from": "T20n1068_001:0138b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手把如意珠。",
        "from": "T20n1068_001:0138b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "紫紺色也。",
        "from": "T20n1068_001:0138b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手金剛劍。",
        "from": "T20n1068_001:0138b25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十五提頭賴吒王。",
        "from": "T20n1068_001:0138b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "赤紅色又青白色。",
        "from": "T20n1068_001:0138b26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手執如意寶王色黃青八角。",
        "from": "T20n1068_001:0138b27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手刀。",
        "from": "T20n1068_001:0138b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十六神母女等大力眾。",
        "from": "T20n1068_001:0138b28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色如。",
        "from": "T20n1068_001:0138b28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十七毘樓勒叉王。",
        "from": "T20n1068_001:0138b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色赤。",
        "from": "T20n1068_001:0138b29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手執杵。",
        "from": "T20n1068_001:0138b29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手把劍。",
        "from": "T20n1068_001:0138b29_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十八毘樓博叉王。",
        "from": "T20n1068_001:0138c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色白。",
        "from": "T20n1068_001:0138c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手執杵。",
        "from": "T20n1068_001:0138c01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手把金索青色。",
        "from": "T20n1068_001:0138c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十九毘沙門天王。",
        "from": "T20n1068_001:0138c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色紺青。",
        "from": "T20n1068_001:0138c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持寶塔。",
        "from": "T20n1068_001:0138c03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手杵。",
        "from": "T20n1068_001:0138c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十金色孔雀王。",
        "from": "T20n1068_001:0138c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身色黃金。",
        "from": "T20n1068_001:0138c05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手執寶幢。",
        "from": "T20n1068_001:0138c05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上有孔雀鳥細妙色也。",
        "from": "T20n1068_001:0138c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說無量妙言。",
        "from": "T20n1068_001:0138c06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十一二十八部大仙眾者。",
        "from": "T20n1068_001:0138c07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十八天神也。",
        "from": "T20n1068_001:0138c07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伊舍那神以為上首。",
        "from": "T20n1068_001:0138c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身色黑赤白也。",
        "from": "T20n1068_001:0138c08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手執杵。",
        "from": "T20n1068_001:0138c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手取朱盤器。",
        "from": "T20n1068_001:0138c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛寶以為瓔珞。",
        "from": "T20n1068_001:0138c09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十二摩尼跋陀羅。",
        "from": "T20n1068_001:0138c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色白紅。",
        "from": "T20n1068_001:0138c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手執寶幢。",
        "from": "T20n1068_001:0138c10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上有如意玉。",
        "from": "T20n1068_001:0138c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手施願印也。",
        "from": "T20n1068_001:0138c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十三散脂大將弗羅婆。",
        "from": "T20n1068_001:0138c12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身色赤紅。",
        "from": "T20n1068_001:0138c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手執金剛。",
        "from": "T20n1068_001:0138c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手安腰。",
        "from": "T20n1068_001:0138c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十四難陀跋難陀。",
        "from": "T20n1068_001:0138c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身色上赤色。",
        "from": "T20n1068_001:0138c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手執赤索。",
        "from": "T20n1068_001:0138c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手劍頭各有五龍下黑青色。",
        "from": "T20n1068_001:0138c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手青索。",
        "from": "T20n1068_001:0138c15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手刀。",
        "from": "T20n1068_001:0138c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娑伽羅龍伊鉢羅。",
        "from": "T20n1068_001:0138c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上色赤白。",
        "from": "T20n1068_001:0138c16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手執赤龍。",
        "from": "T20n1068_001:0138c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手刀。",
        "from": "T20n1068_001:0138c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下色青白。",
        "from": "T20n1068_001:0138c17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手白龍也。",
        "from": "T20n1068_001:0138c17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十五修羅。",
        "from": "T20n1068_001:0138c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂大身修羅也。",
        "from": "T20n1068_001:0138c18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身赤紅色。",
        "from": "T20n1068_001:0138c18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持日輪。",
        "from": "T20n1068_001:0138c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手月輪。",
        "from": "T20n1068_001:0138c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乾闥婆。",
        "from": "T20n1068_001:0138c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手執歌琴。",
        "from": "T20n1068_001:0138c19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手舞印。",
        "from": "T20n1068_001:0138c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身色白紅也。",
        "from": "T20n1068_001:0138c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦樓羅王。",
        "from": "T20n1068_001:0138c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金色兩羽具。",
        "from": "T20n1068_001:0138c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手貝。",
        "from": "T20n1068_001:0138c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手執寶螺笛。",
        "from": "T20n1068_001:0138c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緊那羅摩睺羅伽。",
        "from": "T20n1068_001:0138c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此兩王形白色如羅剎女。",
        "from": "T20n1068_001:0138c22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有二眼乃至三四五眼。",
        "from": "T20n1068_001:0138c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持諸藥器等。",
        "from": "T20n1068_001:0138c23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足二四六八臂。",
        "from": "T20n1068_001:0138c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天冠天衣諸寶珠以為身嚴。",
        "from": "T20n1068_001:0138c24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十六水火雷電神。",
        "from": "T20n1068_001:0138c25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此四神皆備夫妻。",
        "from": "T20n1068_001:0138c25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雷者天雷神。",
        "from": "T20n1068_001:0138c26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "電者地電也。",
        "from": "T20n1068_001:0138c26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此餘者水火以為身嚴。",
        "from": "T20n1068_001:0138c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十七鳩槃荼王。",
        "from": "T20n1068_001:0138c28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長鼻瞋怒形也。",
        "from": "T20n1068_001:0138c28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黑色。",
        "from": "T20n1068_001:0138c28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手戰大器。",
        "from": "T20n1068_001:0138c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手執索。",
        "from": "T20n1068_001:0138c29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十八毘舍闍。",
        "from": "T20n1068_001:0139a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大目瞋怒形。",
        "from": "T20n1068_001:0139a01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黑赤色。",
        "from": "T20n1068_001:0139a01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手火玉也。",
        "from": "T20n1068_001:0139a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一密迹金剛十密語。",
        "from": "T20n1068_001:0139a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿艮阿艮沙耶多他蘖他耶娑婆訶二烏瑟君荼密語。",
        "from": "T20n1068_001:0139a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵毘瑟他耶良他羅阿羅乞叉多耶娑婆訶三魔醯那羅達密語。",
        "from": "T20n1068_001:0139a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿三沒遲良多那娑婆訶四金毘羅陀密語。",
        "from": "T20n1068_001:0139a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩悉儞阿利夜三曼他娑婆訶五婆馺婆樓那密語。",
        "from": "T20n1068_001:0139a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵烏瑟三昧耶毘秡羅尾秡羅娑婆訶六滿善車鉢密語。",
        "from": "T20n1068_001:0139a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵富樓娑多娑多加那加那阿羅瑟尼耶娑婆訶七薩遮摩和羅密語。",
        "from": "T20n1068_001:0139a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵貝檀遲揭帝阿羅叉娑婆訶八鳩蘭單吒密語。",
        "from": "T20n1068_001:0139a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵蘖那耶豹他野囉叉娑婆訶九畢婆伽羅密語。",
        "from": "T20n1068_001:0139a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵末羅伽叉耶阿知羅叉耶南謨室利耶曳娑婆訶十應德毘多密語。",
        "from": "T20n1068_001:0139a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵摩訶帝弊曳娑婆訶十一梵摩三鉢羅密語。",
        "from": "T20n1068_001:0139a25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵伐羅伐羅藥率帝藥率帝娑婆訶十二五部淨居密語。",
        "from": "T20n1068_001:0139a27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵伐羅伐羅吽𠰢吒𠰢吒娑婆訶十三釋王三十三密語。",
        "from": "T20n1068_001:0139a29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵末尼迦釋迦跋折羅壁瑠璃耶娑婆訶十四大辨功德密語。",
        "from": "T20n1068_001:0139b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵摩訶具陵吉利他曳娑婆訶十五提頭賴吒王密語。",
        "from": "T20n1068_001:0139b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵剎利耶曳薩婆訶薩婆訶薩多毘犀耶瑟吒娑婆訶十六神母等密語。",
        "from": "T20n1068_001:0139b06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵醯利醯利多醯徒醯娑婆訶十七毘樓勒叉密語。",
        "from": "T20n1068_001:0139b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵醯伊吽吽娑婆訶十八毘樓博叉密語。",
        "from": "T20n1068_001:0139b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵摩兜祇浦屈沙底娑婆訶十九毘沙門天密語。",
        "from": "T20n1068_001:0139b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵南無帝南跋折羅祇叉婆羅弭他南南壁室良半拏耶婆良耶娑婆訶二十金色孔雀密語。",
        "from": "T20n1068_001:0139b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵具光𠰢吒沙羅迦羅放光𠰢吒沙羅迦羅周遍吽羅迦唵𠰢吒娑婆訶二十一二十八部大仙密語。",
        "from": "T20n1068_001:0139b18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵南無佛法僧因他羅也縛悉儞曳娑婆訶二十二摩尼跋陀羅密語。",
        "from": "T20n1068_001:0139b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵陀羅迦羅夾曼陀娑婆訶二十三散脂大將密語。",
        "from": "T20n1068_001:0139b22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿利耶三昧曳娑婆訶二十四難陀跋難陀密語。",
        "from": "T20n1068_001:0139b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵苦摩尼豹商娑婆訶二十五修羅乾闥婆密語。",
        "from": "T20n1068_001:0139b26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵展若展具末利迦娑婆訶二十六水火雷電密語。",
        "from": "T20n1068_001:0139b28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵具羅婆曳油娑婆訶二十七鳩槃荼王密語。",
        "from": "T20n1068_001:0139b30_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵闍曳阿闍曳娑婆訶二十八毘沙闍密語。",
        "from": "T20n1068_001:0139c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵帝曳帝曳隣陀隣陀娑婆訶如是善神異口同音。",
        "from": "T20n1068_001:0139c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各各誓言顯說釋名密語神呪。",
        "from": "T20n1068_001:0139c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "領諸眷屬。",
        "from": "T20n1068_001:0139c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左右前後圍繞守護。",
        "from": "T20n1068_001:0139c05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持娑婆訶。",
        "from": "T20n1068_001:0139c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人持呪者。",
        "from": "T20n1068_001:0139c06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可誦善神呪。",
        "from": "T20n1068_001:0139c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大威德勢云云。",
        "from": "T20n1068_001:0139c07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "延寶三年。",
        "from": "T20n1068_001:0139c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨嚴和尚染筆之。",
        "from": "T20n1068_001:0139c08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天明元丑歲慈忍師寫宇治慧心院藏本。",
        "from": "T20n1068_001:0139c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今得彼本。",
        "from": "T20n1068_001:0139c09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當山慈心院傳燈等一挍。",
        "from": "T20n1068_001:0139c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和改元八月　　　長谷總持院快道誌",
        "from": "T20n1068_001:0139c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對校訶正畢。",
        "from": "T20n1068_001:0139c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "享和癸亥四月十四日　　慈順文政三年庚辰仲秋二十六日右以下總千葉妙見寺秀陽闍梨所傳寫本校之了。",
        "from": "T20n1068_001:0139c14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍肝",
        "from": "T20n1068_001:0139c14_100",
        "context_with_next_sentence": false
    }
]