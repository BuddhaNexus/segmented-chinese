[
    {
        "source_sentence": "大方廣菩薩藏文殊師利根本儀",
        "from": "T20n1191_004:0848c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "軌經卷第四",
        "from": "T20n1191_004:0848c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試鴻臚少卿",
        "from": "T20n1191_004:0848c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明教大師臣天息災奉　詔譯2 菩薩變化儀軌品1菩薩變化儀軌品第二之一爾時妙吉祥童子。",
        "from": "T20n1191_004:0848c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀察會中一切大眾。",
        "from": "T20n1191_004:0848c15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是妙吉祥童子。",
        "from": "T20n1191_004:0848c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入觀照三摩地。",
        "from": "T20n1191_004:0848c16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入此定已。",
        "from": "T20n1191_004:0848c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從其臍輪出大光明。",
        "from": "T20n1191_004:0848c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有無數百千那由他俱胝光明以為眷屬。",
        "from": "T20n1191_004:0848c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普遍照耀一切眾生界及淨光天。",
        "from": "T20n1191_004:0848c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時金剛手菩薩摩訶薩。",
        "from": "T20n1191_004:0848c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告妙吉祥童子言。",
        "from": "T20n1191_004:0848c20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今宣說曼拏羅法品儀軌。",
        "from": "T20n1191_004:0848c21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸菩薩及一切眾生得入三昧。",
        "from": "T20n1191_004:0848c22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復令一切眾生平等悟解真言句。",
        "from": "T20n1191_004:0848c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有世間出世間一切真言行皆得成就。",
        "from": "T20n1191_004:0848c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛手祕密主如是說已。",
        "from": "T20n1191_004:0848c24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時妙吉祥童子。",
        "from": "T20n1191_004:0848c25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲說最上祕密曼拏羅儀軌。",
        "from": "T20n1191_004:0848c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明照一切以神變力。",
        "from": "T20n1191_004:0848c26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其右手指端之上。",
        "from": "T20n1191_004:0848c26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆現起一切大眾。",
        "from": "T20n1191_004:0848c27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復出無數百千那由他俱胝光明。",
        "from": "T20n1191_004:0848c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照彼淨光天上一切眾會。",
        "from": "T20n1191_004:0848c28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時妙吉祥童子。",
        "from": "T20n1191_004:0848c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略說焰曼德迦忿怒明王心真言具大無畏。",
        "from": "T20n1191_004:0849a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切所作請召發遣。",
        "from": "T20n1191_004:0849a01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "息除災害增長吉祥。",
        "from": "T20n1191_004:0849a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降伏諸魔隱沒身形。",
        "from": "T20n1191_004:0849a02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行坐虛空。",
        "from": "T20n1191_004:0849a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或行地上足不履地。",
        "from": "T20n1191_004:0849a03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令他愛敬等皆得成就。",
        "from": "T20n1191_004:0849a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破一切暗如廣大明燈。",
        "from": "T20n1191_004:0849a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大無畏三字真言。",
        "from": "T20n1191_004:0849a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儀行法力最為第一。",
        "from": "T20n1191_004:0849a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求所作一切真言義悉得成就。",
        "from": "T20n1191_004:0849a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說大忿怒明王心真言。",
        "from": "T20n1191_004:0849a06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引惡吽引此大忿怒明王心真言。",
        "from": "T20n1191_004:0849a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大菩薩妙吉祥善說曼拏羅真言行。",
        "from": "T20n1191_004:0849a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能作一切事破一切障礙。",
        "from": "T20n1191_004:0849a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時妙吉祥童子。",
        "from": "T20n1191_004:0849a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉其右手摩忿怒明王頭。",
        "from": "T20n1191_004:0849a10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱如是言曩摩一切佛。",
        "from": "T20n1191_004:0849a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是說已。",
        "from": "T20n1191_004:0849a11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有十方世界諸佛世尊無量菩薩。",
        "from": "T20n1191_004:0849a12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具大福德三昧來住會中。",
        "from": "T20n1191_004:0849a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時忿怒明王復現大忿怒相。",
        "from": "T20n1191_004:0849a13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即往一切世界剎土之中。",
        "from": "T20n1191_004:0849a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有惡心大力眾生。",
        "from": "T20n1191_004:0849a14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尋遣來集淨光天上大眾之中。",
        "from": "T20n1191_004:0849a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依位安住。",
        "from": "T20n1191_004:0849a15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此眾生頂上各出熾盛光焰。",
        "from": "T20n1191_004:0849a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時妙吉祥童子。",
        "from": "T20n1191_004:0849a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀彼大眾心淨。",
        "from": "T20n1191_004:0849a17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞻仰而復告言。",
        "from": "T20n1191_004:0849a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此諸大眾汝當諦聽。",
        "from": "T20n1191_004:0849a18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我此三昧若不依行如有違犯。",
        "from": "T20n1191_004:0849a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令此忿怒明王現大威力而自調伏。",
        "from": "T20n1191_004:0849a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何為不得違犯。",
        "from": "T20n1191_004:0849a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣此祕密三昧真言行義諸佛世尊菩薩大德平等法門。",
        "from": "T20n1191_004:0849a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今諦聽善思念之。",
        "from": "T20n1191_004:0849a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當為汝說。",
        "from": "T20n1191_004:0849a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱引喃引唵引囉囉三麼二合囉阿鉢囉二合底賀多舍引娑曩俱摩囉嚕波馱哩抳吽吽𠷑吒𠷑吒薩嚩二合引賀聖者此是我根本真言聖妙吉祥印。",
        "from": "T20n1191_004:0849a26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為五髻大印。",
        "from": "T20n1191_004:0849a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若用此印持誦根本真言。",
        "from": "T20n1191_004:0849a27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切所求皆得成就。",
        "from": "T20n1191_004:0849a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說心真言。",
        "from": "T20n1191_004:0849a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作一切善事令心寂靜。",
        "from": "T20n1191_004:0849a29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩枳也二合捺曩莫入說此印法名為三髻。",
        "from": "T20n1191_004:0849b02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增長一切福德。",
        "from": "T20n1191_004:0849b02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說外心真言。",
        "from": "T20n1191_004:0849b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚩枳也二合吽說此印法亦名三髻。",
        "from": "T20n1191_004:0849b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能降伏一切眾生。",
        "from": "T20n1191_004:0849b05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說內心微妙真言。",
        "from": "T20n1191_004:0849b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱喃曼說此印法名孔雀座。",
        "from": "T20n1191_004:0849b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬愛一切眾生。",
        "from": "T20n1191_004:0849b08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有一切佛心大無畏八字真言。",
        "from": "T20n1191_004:0849b09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名最上增益大吉祥。",
        "from": "T20n1191_004:0849b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷三界生死。",
        "from": "T20n1191_004:0849b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "消除一切惡趣。",
        "from": "T20n1191_004:0849b10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能滅一切災害。",
        "from": "T20n1191_004:0849b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作一切事皆得安樂。",
        "from": "T20n1191_004:0849b11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寂靜如現在見佛。",
        "from": "T20n1191_004:0849b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此妙吉祥菩薩。",
        "from": "T20n1191_004:0849b12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宣布最上祕密真言相。",
        "from": "T20n1191_004:0849b12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為一切眾生。",
        "from": "T20n1191_004:0849b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有憶念一切所願皆得圓滿。",
        "from": "T20n1191_004:0849b13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有持誦之者。",
        "from": "T20n1191_004:0849b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有五逆重罪皆得清淨。",
        "from": "T20n1191_004:0849b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說真言曰。",
        "from": "T20n1191_004:0849b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿尾囉吽佉左洛聖者有此八字大無畏最上祕密心真言。",
        "from": "T20n1191_004:0849b17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與佛住世而無有異。",
        "from": "T20n1191_004:0849b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能作一切吉祥之事。",
        "from": "T20n1191_004:0849b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大功德我今為汝略而宣說。",
        "from": "T20n1191_004:0849b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲廣陳。",
        "from": "T20n1191_004:0849b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經無數百千那由他俱胝劫。",
        "from": "T20n1191_004:0849b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "校量功德說不能盡。",
        "from": "T20n1191_004:0849b20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若說此印法。",
        "from": "T20n1191_004:0849b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為大精進能圓滿一切願。",
        "from": "T20n1191_004:0849b21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說召請眾聖真言。",
        "from": "T20n1191_004:0849b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引呬引呬引俱摩囉尾濕嚩二合嚕閉尼薩哩嚩二合嚩引羅婆史哆鉢囉二合冒馱儞引阿野引呬婆誐挽曩引野呬俱摩囉吉哩二合拏怛婆二合羅馱哩尼曼拏羅末地野二合底瑟姹二合底瑟姹二合三摩野摩拏娑摩二合囉阿鉢囉二合底賀哆舍娑曩吽摩引尾羅嚩嚕嚕頗吒娑嚩二合賀此真言是妙吉祥菩薩。",
        "from": "T20n1191_004:0849c01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "召請一切諸佛一切菩薩。",
        "from": "T20n1191_004:0849c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切辟支佛聲聞。",
        "from": "T20n1191_004:0849c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天龍夜叉乾闥婆阿修羅誐嚕拏緊那羅摩睺羅伽。",
        "from": "T20n1191_004:0849c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘舍左羅剎娑一切部多等。",
        "from": "T20n1191_004:0849c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫欲召請。",
        "from": "T20n1191_004:0849c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以香水加持七遍。",
        "from": "T20n1191_004:0849c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灑淨一切廣闊四維上下。",
        "from": "T20n1191_004:0849c05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸佛菩薩妙吉祥。",
        "from": "T20n1191_004:0849c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并及眷屬一切世間出世間真言一切部多眾。",
        "from": "T20n1191_004:0849c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生皆赴道場。",
        "from": "T20n1191_004:0849c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱引喃引阿鉢囉二合底賀哆舍引娑曩喃引唵度度囉度囉度波嚩引悉儞度波引哩唧二合史吽底瑟姹二合三摩野摩努娑摩二合囉娑嚩二合引賀此是獻香真言。",
        "from": "T20n1191_004:0849c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若以白栴檀龍腦供俱摩香和合等。",
        "from": "T20n1191_004:0849c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒此香時誦此真言。",
        "from": "T20n1191_004:0849c13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來及諸菩薩。",
        "from": "T20n1191_004:0849c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切聖眾皆受供養。",
        "from": "T20n1191_004:0849c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前召請印名最上蓮華鬘。",
        "from": "T20n1191_004:0849c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能與一切眾生作大吉祥之事。",
        "from": "T20n1191_004:0849c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於諸佛如來一切菩薩及聖眾等。",
        "from": "T20n1191_004:0849c17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獻閼伽水。",
        "from": "T20n1191_004:0849c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所用龍腦白檀供俱摩等香。",
        "from": "T20n1191_004:0849c18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復用惹帝華適意華摩梨迦華。",
        "from": "T20n1191_004:0849c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "末哩師迦華龍華。",
        "from": "T20n1191_004:0849c19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "末俱羅華。",
        "from": "T20n1191_004:0849c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶尼哆誐囉華。",
        "from": "T20n1191_004:0849c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等香華用淹水內。",
        "from": "T20n1191_004:0849c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此名獻閼伽水。",
        "from": "T20n1191_004:0849c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獻水真言。",
        "from": "T20n1191_004:0849c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱引喃引摩鉢囉二合底賀哆舍引娑曩引喃引怛儞也二合他引呬引呬引摩賀引迦引嚕尼迦尾濕嚩二合嚕波馱哩尼引阿哩凝伽二合鉢囉二合底蹉鉢囉二合底蹉波野三摩野摩努娑摩二合囉底瑟姹二合底瑟姹二合曼拏羅末地也二合鉢囉二合吠舍野薩哩嚩二合部哆引努波迦仡哩二合恨拏二合吽阿蘇囉尾左引哩尼娑嚩二合賀引此印名圓滿印。",
        "from": "T20n1191_004:0850a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能為一切眾生作圓滿事。",
        "from": "T20n1191_004:0850a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說焚香真言。",
        "from": "T20n1191_004:0850a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若彼獻香如前真言。",
        "from": "T20n1191_004:0850a10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱引喃引曩謨三滿哆巘馱引嚩娑引娑室哩二合夜引野怛他引誐哆引野怛儞也二合巘第巘第引巘第引巘馱引巘馱摩拏囉弭引鉢囉二合底蹉鉢囉二合底砌引難巘淡三滿哆引努左引哩尼引娑嚩二合賀引此印名波羅嚩。",
        "from": "T20n1191_004:0850a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓滿一切願。",
        "from": "T20n1191_004:0850a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說獻華真言。",
        "from": "T20n1191_004:0850a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱引喃引摩鉢囉二合底賀哆舍引娑曩喃引曩謨三俱蘇弭哆囉引惹寫怛他引誐哆寫怛儞也二合他引俱蘇弭引俱蘇摩引儞曳二合俱蘇摩布囉嚩引悉儞俱蘇摩引嚩底娑嚩二合賀引夫欲獻食。",
        "from": "T20n1191_004:0850a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先頂禮一切諸佛賢聖。",
        "from": "T20n1191_004:0850a23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生不思議未曾有想。",
        "from": "T20n1191_004:0850a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此真言正覺正說。",
        "from": "T20n1191_004:0850a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後所有供獻。",
        "from": "T20n1191_004:0850a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆依此儀念此真言。",
        "from": "T20n1191_004:0850a25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱引喃引摩鉢囉二合底賀哆舍娑曩引喃引怛儞也二合他引呬引呬引婆誐嚩摩賀薩哩嚩二合沒馱引嚩路引吉哆摩引尾楞嚩伊難嚩隷誐哩二合恨拏二合引播野誐哩二合恨拏二合誐哩二合吽薩哩嚩二合尾濕嚩二合囉囉吒吒薩頗二合吒娑嚩二合引賀引今此真言印名為大力。",
        "from": "T20n1191_004:0850b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "消除一切諸惡。",
        "from": "T20n1191_004:0850b03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燃燈真言曩謨三滿哆沒馱引喃引摩鉢囉二合底賀哆舍引娑曩引喃引薩哩嚩二合怛夢引馱迦引囉尾特鑁二合悉喃引曩謨三滿哆祖底巘馱引嚩婆引娑室哩二合夜引野怛他引誐哆引野怛儞也二合他引呬引呬引婆誐鑁祖底囉舍弭二合設哆娑賀娑囉二合鉢囉二合底曼尼哆舍哩囉尾俱哩嚩二合摩賀引冒地薩怛嚩二合三滿哆入嚩二合囉引儞喻二合底哆沒哩底二合具哩那二合具哩那二合阿嚩路引迦野阿嚩路迦野曼拏羅薩哩嚩二合薩怛嚩二合難左此是燃燈真言印名曰廣開。",
        "from": "T20n1191_004:0850b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀照一切眾生。",
        "from": "T20n1191_004:0850b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱引喃引摩鉢囉二合底賀哆舍引娑曩引喃引怛儞也二合他引入嚩二合入嚩二合羅入嚩二合羅入嚩二合羅野入嚩二合羅野吽尾冒馱迦賀哩訖哩二合瑟拏二合賓誐羅此是燃火真言印。",
        "from": "T20n1191_004:0850b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名合掌光照一切眾生過去諸佛菩薩所說。",
        "from": "T20n1191_004:0850b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時妙吉祥童子。",
        "from": "T20n1191_004:0850b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告祕密主金剛手菩薩言。",
        "from": "T20n1191_004:0850b23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此微妙祕密真言。",
        "from": "T20n1191_004:0850b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等明王族部。",
        "from": "T20n1191_004:0850b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外現忿怒內含慈忍。",
        "from": "T20n1191_004:0850b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有智者求一切真言皆得成就。",
        "from": "T20n1191_004:0850b25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若彼金剛蓮華族等障礙之時。",
        "from": "T20n1191_004:0850b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說此明令彼降伏。",
        "from": "T20n1191_004:0850b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨薩哩嚩二合沒馱引喃引摩鉢囉二合底賀哆舍引娑曩引喃引怛儞也二合他引唵迦囉迦囉俱嚕俱嚕摩摩引迦引哩焰二合伴惹伴惹薩哩嚩二合尾近難二合引那賀那賀薩哩嚩二合嚩日羅二合尾那野劍引布哩嚩二合吒迦𡁠尾旦哆迦囉摩賀引尾訖哩二合哆嚕波馱哩尼鉢左鉢左薩哩嚩二合努瑟吒二合摩賀引誐拏鉢底𡁠尾旦哆迦囉滿馱滿馱薩哩嚩二合誐囉二合賀沙目佉沙部惹沙左囉拏嚕捺囉二合摩引曩野尾瑟拏二合摩引曩野沒囉二合憾摩二合儞也二合禰嚩引曩曩野摩引尾楞嚩尾楞嚩羅護羅護曼拏羅末他也二合鉢囉二合吠舍野三摩野摩努娑摩二合囉吽吽吽吽吽吽頗吒頗吒說此真言已告祕密主言。",
        "from": "T20n1191_004:0851a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是大精進最上祕密。",
        "from": "T20n1191_004:0851a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名六面大忿怒明王。",
        "from": "T20n1191_004:0851a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能破障礙。",
        "from": "T20n1191_004:0851a03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若持誦此明得自在。",
        "from": "T20n1191_004:0851a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十地菩薩猶可降伏。",
        "from": "T20n1191_004:0851a04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況諸惡障礙。",
        "from": "T20n1191_004:0851a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有持誦供養大作擁護。",
        "from": "T20n1191_004:0851a05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此印法名為大叉。",
        "from": "T20n1191_004:0851a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破滅一切障礙。",
        "from": "T20n1191_004:0851a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說忿怒明王心真言。",
        "from": "T20n1191_004:0851a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引仡哩二合瑟致哩二合哆引曩曩吽薩哩嚩二合設怛嚕二合曩舍野薩旦二合婆野頗吒頗吒若有一切冤家逼害惱亂之時。",
        "from": "T20n1191_004:0851a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依法念此真言。",
        "from": "T20n1191_004:0851a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼冤家受隔四日瘧病得大苦惱。",
        "from": "T20n1191_004:0851a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人恒常誦持。",
        "from": "T20n1191_004:0851a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無慈悲心所求之事不得成就。",
        "from": "T20n1191_004:0851a13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人不信三寶。",
        "from": "T20n1191_004:0851a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此呪法令彼生信定得成就。",
        "from": "T20n1191_004:0851a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印名大叉。",
        "from": "T20n1191_004:0851a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與呪同用其驗剋成。",
        "from": "T20n1191_004:0851a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說外心真言。",
        "from": "T20n1191_004:0851a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵仡哩二合迦羅嚕波吽欠印名大叉若此明同用。",
        "from": "T20n1191_004:0851a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能調伏一切惡者。",
        "from": "T20n1191_004:0851a18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說內心真言名為一字。",
        "from": "T20n1191_004:0851a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切佛說。",
        "from": "T20n1191_004:0851a19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印名大叉與明同用。",
        "from": "T20n1191_004:0851a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能消除一切惡事。",
        "from": "T20n1191_004:0851a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降伏一切部多。",
        "from": "T20n1191_004:0851a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於曼拏羅中呪法成就之時。",
        "from": "T20n1191_004:0851a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定見忿怒明王靈驗。",
        "from": "T20n1191_004:0851a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說發遣賢聖等真言。",
        "from": "T20n1191_004:0851a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨薩哩嚩沒馱引喃引摩鉢囉二合底賀哆舍娑曩引喃引怛儞也二合他引惹焰惹野蘇惹野摩賀迦引嚕尼迦尾室左二合嚕閉尼引誐蹉誐蹉娑嚩二合婆嚩喃引薩哩嚩二合沒淡引室左二合尾娑哩惹二合野薩波哩嚩引囉引娑嚩二合婆嚩喃左怛囉二合鉢囉二合吠引舍野滿怛囉二合娑摩二合囉薩哩嚩二合室左二合弭悉殿覩滿怛囉二合播那引摩努囉淡左弭波哩布囉野此發遣真言印。",
        "from": "T20n1191_004:0851b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名寶座亦名獻座。",
        "from": "T20n1191_004:0851b04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲發遣賢聖等。",
        "from": "T20n1191_004:0851b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "專心志意念一七遍依法加持。",
        "from": "T20n1191_004:0851b05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有一切世間出世間曼拏羅真言行。",
        "from": "T20n1191_004:0851b06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆得三昧成就。",
        "from": "T20n1191_004:0851b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸聖眾等歡喜而退。",
        "from": "T20n1191_004:0851b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時妙吉祥童子。",
        "from": "T20n1191_004:0851b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復觀淨光天內一切大眾。",
        "from": "T20n1191_004:0851b08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說自己明王真言。",
        "from": "T20n1191_004:0851b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨薩哩嚩二合沒馱引喃引摩鉢囉二合底賀哆誐底喃引唵儞哩致妙吉祥說此真言已。",
        "from": "T20n1191_004:0851b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化為明王名枳世儞。",
        "from": "T20n1191_004:0851b12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能作一切之事。",
        "from": "T20n1191_004:0851b13_5",
