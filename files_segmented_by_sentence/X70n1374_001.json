{
    "X70n1374_001:0028b06_0": "龍源清禪師語錄",
    "X70n1374_001:0028b08_0": "目次",
    "X70n1374_001:0028b09_0": "壽國□□禪寺語錄開壽普光禪寺語",
    "X70n1374_001:0028b10_0": "湖州道場禪寺語錄小參",
    "X70n1374_001:0028b11_0": "拈古頌古偈頌贊",
    "X70n1374_001:0028b12_0": "跋小佛事塔銘跋",
    "X70n1374_001:0028b14_0": "不甘底出來道。",
    "X70n1374_001:0028b14_1": "塵毛剎海中。",
    "X70n1374_001:0028b14_2": "有十種奇特事。",
    "X70n1374_001:0028b14_3": "固是總知。",
    "X70n1374_001:0028b15_4": "於中有一件不入眾數底。",
    "X70n1374_001:0028b15_5": "還知也無。",
    "X70n1374_001:0028b15_6": "只消向他道。",
    "X70n1374_001:0028b16_0": "上舡相比簍。",
    "X70n1374_001:0028b17_0": "四月旦上堂。",
    "X70n1374_001:0028b17_1": "昨日送春歸。",
    "X70n1374_001:0028b17_2": "今朝迎首夏。",
    "X70n1374_001:0028b17_3": "殘花一樹兩樹。",
    "X70n1374_001:0028b18_4": "杜宇三聲四聲。",
    "X70n1374_001:0028b18_5": "歷歷全超聲色。",
    "X70n1374_001:0028b18_6": "幾多瞌睡未醒。",
    "X70n1374_001:0028b18_7": "啞。",
    "X70n1374_001:0028b19_0": "盡將肝膽向人傾。",
    "X70n1374_001:0028b20_0": "中秋謝道舊上堂。",
    "X70n1374_001:0028b20_1": "雲開舍北。",
    "X70n1374_001:0028b20_2": "月浸東洲。",
    "X70n1374_001:0028b20_3": "指底話底翫底。",
    "X70n1374_001:0028b21_4": "多是辜負良秋。",
    "X70n1374_001:0028b21_5": "山僧今夜。",
    "X70n1374_001:0028b21_6": "却與把手同遊。",
    "X70n1374_001:0028b21_7": "卓拄杖云。",
    "X70n1374_001:0028b22_8": "抹過雲門六不收。",
    "X70n1374_001:0028b23_0": "臘八上堂。",
    "X70n1374_001:0028b23_1": "六年枯坐忍饑寒。",
    "X70n1374_001:0028b23_2": "堪笑翻身又出山。",
    "X70n1374_001:0028b23_3": "自是眼中無定見。",
    "X70n1374_001:0028b24_4": "夜來却被一星瞞。",
    "X70n1374_001:0028c01_0": "開壽普光禪寺語",
    "X70n1374_001:0028c02_0": "侍者　士洵　編",
    "X70n1374_001:0028c03_0": "府疏。",
    "X70n1374_001:0028c03_1": "丞相本無私。",
    "X70n1374_001:0028c03_2": "爭嫌福建子。",
    "X70n1374_001:0028c03_3": "提起疏云。",
    "X70n1374_001:0028c03_4": "祇此宛如壁上書名。",
    "X70n1374_001:0028c04_5": "於中本文又且如何舉似。",
    "X70n1374_001:0028c04_6": "度疏云。",
    "X70n1374_001:0028c04_7": "白底是紙。",
    "X70n1374_001:0028c05_8": "黑底是字。",
    "X70n1374_001:0028c06_0": "陞座垂語。",
    "X70n1374_001:0028c06_1": "萬頃煙波。",
    "X70n1374_001:0028c06_2": "一天風月。",
    "X70n1374_001:0028c06_3": "千尺絲綸直下垂。",
    "X70n1374_001:0028c06_4": "隨手六鰲輕一掣。",
    "X70n1374_001:0028c07_5": "莫有不貪香餌底麼(問答不錄)。",
    "X70n1374_001:0028c08_0": "提綱。",
    "X70n1374_001:0028c08_1": "還他過量人。",
    "X70n1374_001:0028c08_2": "提持過量事。",
    "X70n1374_001:0028c08_3": "據大座而宴安自若。",
    "X70n1374_001:0028c09_0": "翠橫車廐。",
    "X70n1374_001:0028c09_1": "聳壑昂霄。",
    "X70n1374_001:0028c09_2": "發大機於正按旁提。",
    "X70n1374_001:0028c09_3": "綠漲慈溪。",
    "X70n1374_001:0028c10_0": "滔天沃日。",
    "X70n1374_001:0028c10_1": "權衡在手。",
    "X70n1374_001:0028c10_2": "收放自由。",
    "X70n1374_001:0028c10_3": "殊不知。",
    "X70n1374_001:0028c10_4": "月裏麒麟看北斗。",
    "X70n1374_001:0028c11_5": "楚王城畔水東流。",
    "X70n1374_001:0028c11_6": "阿呵呵。",
    "X70n1374_001:0028c11_7": "是什麼。",
    "X70n1374_001:0028c11_8": "迦葉不傳。",
    "X70n1374_001:0028c11_9": "達磨不識。",
    "X70n1374_001:0028c12_10": "卓拄杖一下。",
    "X70n1374_001:0028c12_11": "春融一氣轉洪鈞。",
    "X70n1374_001:0028c12_12": "雲淨八荒開壽域。",
    "X70n1374_001:0028c13_0": "次日。",
    "X70n1374_001:0028c13_1": "為丞相國公上堂。",
    "X70n1374_001:0028c13_2": "大智洞明。",
    "X70n1374_001:0028c13_3": "十方通暢。",
    "X70n1374_001:0028c13_4": "乘願力以弘深。",
    "X70n1374_001:0028c14_5": "策功勛於聖世。",
    "X70n1374_001:0028c14_6": "出將入相。",
    "X70n1374_001:0028c14_7": "斡乾坤而載造。",
    "X70n1374_001:0028c14_8": "調元補衮。",
    "X70n1374_001:0028c15_9": "揭日月於中天。",
    "X70n1374_001:0028c15_10": "祇今控鶴神遊。",
    "X70n1374_001:0028c15_11": "必竟居何國土。",
    "X70n1374_001:0028c16_12": "九蓮開合處。",
    "X70n1374_001:0028c16_13": "百寶自莊嚴。",
    "X70n1374_001:0028c17_0": "上堂。",
    "X70n1374_001:0028c17_1": "舉僧問雲門。",
    "X70n1374_001:0028c17_2": "如何是沙門行。",
    "X70n1374_001:0028c17_3": "門云。",
    "X70n1374_001:0028c17_4": "會不得。",
    "X70n1374_001:0028c17_5": "進云。",
    "X70n1374_001:0028c18_0": "為什麼會不得。",
    "X70n1374_001:0028c18_1": "門云。",
    "X70n1374_001:0028c18_2": "只守會不得。",
    "X70n1374_001:0028c18_3": "師云。",
    "X70n1374_001:0028c18_4": "雲門再舉會不得。",
    "X70n1374_001:0028c19_5": "可謂泥裏有刺。",
    "X70n1374_001:0028c19_6": "忽有問開壽。",
    "X70n1374_001:0028c19_7": "如何是沙門行。",
    "X70n1374_001:0028c19_8": "只向他道。",
    "X70n1374_001:0028c20_9": "認取會不得。",
    "X70n1374_001:0028c21_0": "冬節上堂。",
    "X70n1374_001:0028c21_1": "不可以智知。",
    "X70n1374_001:0028c21_2": "不可以識識。",
    "X70n1374_001:0028c21_3": "昨夜一陽生。",
    "X70n1374_001:0028c21_4": "今朝二十一。",
    "X70n1374_001:0028c22_5": "點點梅花弄春色。",
    "X70n1374_001:0028c23_0": "燈夕移山門上堂。",
    "X70n1374_001:0028c23_1": "一燈明。",
    "X70n1374_001:0028c23_2": "燈燈無盡。",
    "X70n1374_001:0028c23_3": "一門開。",
    "X70n1374_001:0028c23_4": "門門有路。",
    "X70n1374_001:0028c24_5": "卓拄杖一下。",
    "X70n1374_001:0028c24_6": "看看。",
    "X70n1374_001:0028c24_7": "燈明古佛。",
    "X70n1374_001:0028c24_8": "現在汝諸人脚跟下。",
    "X70n1374_001:0029a01_0": "動地放光。",
    "X70n1374_001:0029a01_1": "摺折德山棒。",
    "X70n1374_001:0029a01_2": "啞却臨濟喝。",
    "X70n1374_001:0029a01_3": "要汝諸人不動脚頭。",
    "X70n1374_001:0029a02_4": "悉由此門。",
    "X70n1374_001:0029a02_5": "徑登佛地。",
    "X70n1374_001:0029a02_6": "其或遲回。",
    "X70n1374_001:0029a02_7": "古佛過去久矣。",
    "X70n1374_001:0029a03_0": "臘八入杭。",
    "X70n1374_001:0029a03_1": "更黃衣上堂。",
    "X70n1374_001:0029a03_2": "我佛面黃。",
    "X70n1374_001:0029a03_3": "我身衣黃。",
    "X70n1374_001:0029a03_4": "星移斗轉。",
    "X70n1374_001:0029a04_5": "雪上加霜。",
    "X70n1374_001:0029a04_6": "啞。",
    "X70n1374_001:0029a04_7": "住山忙似出山忙。",
    "X70n1374_001:0029a05_0": "中夏看青苗經。",
    "X70n1374_001:0029a05_1": "兼謝及翁無損商隱三藏主上堂。",
    "X70n1374_001:0029a05_2": "一夏已過半。",
    "X70n1374_001:0029a06_3": "箇事要成辨。",
    "X70n1374_001:0029a06_4": "祇今演出大藏小藏。",
    "X70n1374_001:0029a06_5": "猶是居空說空。",
    "X70n1374_001:0029a07_6": "且道。",
    "X70n1374_001:0029a07_7": "如何是必竟空。",
    "X70n1374_001:0029a07_8": "卓拄杖一下。",
    "X70n1374_001:0029a07_9": "任從滄海變。",
    "X70n1374_001:0029a08_10": "終不為君通。",
    "X70n1374_001:0029a09_0": "六月望上堂。",
    "X70n1374_001:0029a09_1": "拈拄杖。",
    "X70n1374_001:0029a09_2": "召大眾云。",
    "X70n1374_001:0029a09_3": "大地眾生。",
    "X70n1374_001:0029a09_4": "擾擾昏昏。",
    "X70n1374_001:0029a10_0": "膠膠轕轕。",
    "X70n1374_001:0029a10_1": "山僧不忍見。",
    "X70n1374_001:0029a10_2": "向六月炎炎中。",
    "X70n1374_001:0029a10_3": "卓拄杖云。",
    "X70n1374_001:0029a10_4": "轟一聲雷。",
    "X70n1374_001:0029a11_5": "直得雨似盆傾。",
    "X70n1374_001:0029a11_6": "以致三草二木。",
    "X70n1374_001:0029a11_7": "同沾滋潤。",
    "X70n1374_001:0029a11_8": "諸人還覺神清氣爽麼。",
    "X70n1374_001:0029a12_9": "靠拄杖云。",
    "X70n1374_001:0029a12_10": "蟻子不食鐵。",
    "X70n1374_001:0029a13_0": "上堂。",
    "X70n1374_001:0029a13_1": "三春云暮。",
    "X70n1374_001:0029a13_2": "綠暗紅稀。",
    "X70n1374_001:0029a13_3": "動為境轉。",
    "X70n1374_001:0029a13_4": "靜為法迷。",
    "X70n1374_001:0029a13_5": "不以色蓋。",
    "X70n1374_001:0029a14_6": "不以聲騎。",
    "X70n1374_001:0029a14_7": "風前閒聽杜鵑啼。",
    "X70n1374_001:0029a15_0": "上堂。",
    "X70n1374_001:0029a15_1": "彼彼不相知。",
    "X70n1374_001:0029a15_2": "法法不相似。",
    "X70n1374_001:0029a15_3": "秋水波澄。",
    "X70n1374_001:0029a15_4": "秋山骨露。",
    "X70n1374_001:0029a16_0": "瑞巖喚主人翁。",
    "X70n1374_001:0029a16_1": "自不惺惺。",
    "X70n1374_001:0029a16_2": "麻谷携鋤入園。",
    "X70n1374_001:0029a16_3": "偷身作務。",
    "X70n1374_001:0029a17_0": "從上諸老。",
    "X70n1374_001:0029a17_1": "弄鬼精魂。",
    "X70n1374_001:0029a17_2": "引得後代兒孫。",
    "X70n1374_001:0029a17_3": "迷却轉身一路。",
    "X70n1374_001:0029a18_0": "啞。",
    "X70n1374_001:0029a18_1": "君向西秦。",
    "X70n1374_001:0029a18_2": "我之東魯。",
    "X70n1374_001:0029a19_0": "春日病起上堂。",
    "X70n1374_001:0029a19_1": "舉古德道。",
    "X70n1374_001:0029a19_2": "有我即我病生。",
    "X70n1374_001:0029a19_3": "無我即我病滅。",
    "X70n1374_001:0029a20_4": "我病兩俱非。",
    "X70n1374_001:0029a20_5": "萬里一條鐵。",
    "X70n1374_001:0029a20_6": "更有末後句。",
    "X70n1374_001:0029a20_7": "待款款地。",
    "X70n1374_001:0029a21_8": "與諸人說。",
    "X70n1374_001:0029a21_9": "頌云。",
    "X70n1374_001:0029a21_10": "莓苔徧地榆錢滿。",
    "X70n1374_001:0029a21_11": "院落無人柳絮飛。",
    "X70n1374_001:0029a22_12": "信手翻書香篆冷。",
    "X70n1374_001:0029a22_13": "夕光山翠上窓扉。",
    "X70n1374_001:0029a23_0": "上堂。",
    "X70n1374_001:0029a23_1": "德山臨濟。",
    "X70n1374_001:0029a23_2": "盡令而行。",
    "X70n1374_001:0029a23_3": "岩頭雪峯。",
    "X70n1374_001:0029a23_4": "盡法而說。",
    "X70n1374_001:0029a23_5": "子細檢點將來。",
    "X70n1374_001:0029a24_6": "也只道得一橛。",
    "X70n1374_001:0029a24_7": "何故。",
    "X70n1374_001:0029a24_8": "六月不熱。",
    "X70n1374_001:0029a24_9": "五穀不結。",
    "X70n1374_001:0029b01_0": "湖州道場禪寺語",
    "X70n1374_001:0029b02_0": "侍者　德高　懷珠　編",
    "X70n1374_001:0029b03_0": "三門。",
    "X70n1374_001:0029b03_1": "苕水生漪。",
    "X70n1374_001:0029b03_2": "雲峯擁秀。",
    "X70n1374_001:0029b03_3": "直下承當。",
    "X70n1374_001:0029b03_4": "推門入臼佛殿(未塑佛)過去已過去。",
    "X70n1374_001:0029b04_5": "未來猶未來。",
    "X70n1374_001:0029b04_6": "現在在何所。",
    "X70n1374_001:0029b04_7": "敲香云。",
    "X70n1374_001:0029b05_8": "多是當面蹉過。",
    "X70n1374_001:0029b06_0": "據室。",
    "X70n1374_001:0029b06_1": "橫按拄杖云。",
    "X70n1374_001:0029b06_2": "此是為中下之機。",
    "X70n1374_001:0029b06_3": "若是上上人來時如何。",
    "X70n1374_001:0029b07_4": "靠拄杖云。",
    "X70n1374_001:0029b07_5": "猛虎不飡伏肉。",
    "X70n1374_001:0029b08_0": "總統疏。",
    "X70n1374_001:0029b08_1": "提起云。",
    "X70n1374_001:0029b08_2": "言言見諦。",
    "X70n1374_001:0029b08_3": "句句朝宗。",
    "X70n1374_001:0029b08_4": "度疏云。",
    "X70n1374_001:0029b08_5": "風從虎兮雲從龍。",
    "X70n1374_001:0029b10_0": "陞座拈香云。",
    "X70n1374_001:0029b10_1": "毓秀中吳地。",
    "X70n1374_001:0029b10_2": "流芳古道場。",
    "X70n1374_001:0029b10_3": "橫該南岳七十二峯。",
    "X70n1374_001:0029b11_4": "突出華頂萬八千丈。",
    "X70n1374_001:0029b11_5": "當機覿面。",
    "X70n1374_001:0029b11_6": "拈作瓣香。",
    "X70n1374_001:0029b11_7": "祝君地久與天長。",
    "X70n1374_001:0029b13_0": "提綱。",
    "X70n1374_001:0029b13_1": "大通智勝佛。",
    "X70n1374_001:0029b13_2": "迥絕名模。",
    "X70n1374_001:0029b13_3": "十劫坐道場。",
    "X70n1374_001:0029b13_4": "事久成弊。",
    "X70n1374_001:0029b14_0": "佛法不現前。",
    "X70n1374_001:0029b14_1": "却較些子。",
    "X70n1374_001:0029b14_2": "不得成佛道。",
    "X70n1374_001:0029b14_3": "覿體全真。",
    "X70n1374_001:0029b14_4": "山僧已將古佛嘉猷。",
    "X70n1374_001:0029b15_5": "一一揭示諸人了也。",
    "X70n1374_001:0029b15_6": "雖然雲月是同。",
    "X70n1374_001:0029b16_0": "溪山各異。",
    "X70n1374_001:0029b16_1": "只如兩山一門。",
    "X70n1374_001:0029b16_2": "控汝入路。",
    "X70n1374_001:0029b16_3": "汝諸人。",
    "X70n1374_001:0029b16_4": "既臨屋底清池。",
    "X70n1374_001:0029b17_5": "還曾見得火向水中焚麼。",
    "X70n1374_001:0029b17_6": "若也見得。",
    "X70n1374_001:0029b17_7": "便與祖師。",
    "X70n1374_001:0029b18_8": "把手共遊。",
    "X70n1374_001:0029b18_9": "橫行虎穴。",
    "X70n1374_001:0029b18_10": "旁提正按。",
    "X70n1374_001:0029b18_11": "收縱自由。",
    "X70n1374_001:0029b18_12": "且道。",
    "X70n1374_001:0029b18_13": "正與麼時。",
    "X70n1374_001:0029b19_14": "畢竟承誰恩力。",
    "X70n1374_001:0029b19_15": "天高羣象正。",
    "X70n1374_001:0029b19_16": "海濶百川朝。",
    "X70n1374_001:0029b20_0": "復舉。",
    "X70n1374_001:0029b20_1": "佛陀波利尊者。",
    "X70n1374_001:0029b20_2": "到五臺。",
    "X70n1374_001:0029b20_3": "有一老人。",
    "X70n1374_001:0029b20_4": "問云。",
    "X70n1374_001:0029b20_5": "何處來。",
    "X70n1374_001:0029b21_0": "尊者云。",
    "X70n1374_001:0029b21_1": "西天來。",
    "X70n1374_001:0029b21_2": "老人云。",
    "X70n1374_001:0029b21_3": "還曾將得佛頂尊勝經來麼。",
    "X70n1374_001:0029b22_0": "尊者云。",
    "X70n1374_001:0029b22_1": "不曾將得來。",
    "X70n1374_001:0029b22_2": "拈云。",
    "X70n1374_001:0029b22_3": "大眾。",
    "X70n1374_001:0029b22_4": "老人恁麼問。",
    "X70n1374_001:0029b22_5": "決定未到西天。",
    "X70n1374_001:0029b23_6": "尊者恁麼答。",
    "X70n1374_001:0029b23_7": "豈不是這卷經。",
    "X70n1374_001:0029b23_8": "具眼者辨取。",
    "X70n1374_001:0029b24_0": "上堂。",
    "X70n1374_001:0029b24_1": "馬祖陞堂。",
    "X70n1374_001:0029b24_2": "鱉咬釣魚竿。",
    "X70n1374_001:0029b24_3": "百丈卷席。",
    "X70n1374_001:0029b24_4": "虵銜老鼠尾。",
    "X70n1374_001:0029c01_0": "故是父子共出隻手。",
    "X70n1374_001:0029c01_1": "力扶大教。",
    "X70n1374_001:0029c01_2": "爭柰眾眼難瞞。",
    "X70n1374_001:0029c01_3": "作麼生是難瞞底事。",
    "X70n1374_001:0029c02_4": "霜月到門棊局冷。",
    "X70n1374_001:0029c02_5": "水雲籠石衲衣寒。",
    "X70n1374_001:0029c03_0": "結制上堂。",
    "X70n1374_001:0029c03_1": "西天結制。",
    "X70n1374_001:0029c03_2": "以蠟人為驗。",
    "X70n1374_001:0029c03_3": "虎岩結制。",
    "X70n1374_001:0029c03_4": "以何為驗。",
    "X70n1374_001:0029c04_5": "卓拄杖云。",
    "X70n1374_001:0029c04_6": "據虎頭收虎尾。",
    "X70n1374_001:0029c05_0": "上堂。",
    "X70n1374_001:0029c05_1": "不可以智知。",
    "X70n1374_001:0029c05_2": "不可以識識。",
    "X70n1374_001:0029c05_3": "昨夜轉金風。",
    "X70n1374_001:0029c05_4": "今朝七月一。",
    "X70n1374_001:0029c06_5": "去却一。",
    "X70n1374_001:0029c06_6": "拈得七。",
    "X70n1374_001:0029c06_7": "禾苗萬頃翻秋色。",
    "X70n1374_001:0029c07_0": "解制上堂。",
    "X70n1374_001:0029c07_1": "卓拄杖云。",
    "X70n1374_001:0029c07_2": "開正覺場。",
    "X70n1374_001:0029c07_3": "又卓拄杖云。",
    "X70n1374_001:0029c07_4": "放水牯牛。",
    "X70n1374_001:0029c08_5": "無繩無索。",
    "X70n1374_001:0029c08_6": "去住自由。",
    "X70n1374_001:0029c08_7": "橫笛一聲天地秋。",
    "X70n1374_001:0029c09_0": "上堂。",
    "X70n1374_001:0029c09_1": "山僧窮到無錐。",
    "X70n1374_001:0029c09_2": "一夜忽然富貴。",
    "X70n1374_001:0029c09_3": "松釵變作金釵。",
    "X70n1374_001:0029c10_0": "氷片化成玉片。",
    "X70n1374_001:0029c10_1": "即今寶物現前。",
    "X70n1374_001:0029c10_2": "盜賊其誰竊得。",
    "X70n1374_001:0029c10_3": "直饒釋迦彌勒。",
    "X70n1374_001:0029c11_4": "德山臨濟。",
    "X70n1374_001:0029c11_5": "猶是分贓草賊。",
    "X70n1374_001:0029c11_6": "且道。",
    "X70n1374_001:0029c11_7": "那箇是正賊。",
    "X70n1374_001:0029c12_8": "喝一喝。",
    "X70n1374_001:0029c13_0": "上堂。",
    "X70n1374_001:0029c13_1": "謝少明首座。",
    "X70n1374_001:0029c13_2": "靈隱侍者。",
    "X70n1374_001:0029c13_3": "三十年前。",
    "X70n1374_001:0029c13_4": "與之共遊。",
    "X70n1374_001:0029c13_5": "雲月是同。",
    "X70n1374_001:0029c14_6": "三十年後。",
    "X70n1374_001:0029c14_7": "不堪共語。",
    "X70n1374_001:0029c14_8": "溪山各異。",
    "X70n1374_001:0029c14_9": "如來禪祖師禪。",
    "X70n1374_001:0029c15_10": "拈向一壁。",
    "X70n1374_001:0029c15_11": "德山棒臨濟喝。",
    "X70n1374_001:0029c15_12": "總用不著。",
    "X70n1374_001:0029c15_13": "何故。",
    "X70n1374_001:0029c15_14": "國師打侍者。",
    "X70n1374_001:0029c16_15": "南泉斬猫兒。",
    "X70n1374_001:0029c17_0": "閏四月十五上堂。",
    "X70n1374_001:0029c17_1": "已見四月十五。",
    "X70n1374_001:0029c17_2": "又見四月十五。",
    "X70n1374_001:0029c17_3": "重重畫地為。",
    "X70n1374_001:0029c18_4": "牢猶將生鐵錮鏴。",
    "X70n1374_001:0029c18_5": "若是具透關眼底。",
    "X70n1374_001:0029c18_6": "不妨掉臂便行。",
    "X70n1374_001:0029c19_7": "其或未然。",
    "X70n1374_001:0029c19_8": "踏著方知不相似。",
    "X70n1374_001:0029c20_0": "上堂。",
    "X70n1374_001:0029c20_1": "謝首座秉拂。",
    "X70n1374_001:0029c20_2": "都寺幹齋。",
    "X70n1374_001:0029c20_3": "坐挹薰風南來。",
    "X70n1374_001:0029c20_4": "快哉一時瞥地。",
    "X70n1374_001:0029c21_5": "親承得力句。",
    "X70n1374_001:0029c21_6": "飽飫醍醐味。",
    "X70n1374_001:0029c21_7": "歷歷分明不覆藏。",
    "X70n1374_001:0029c22_0": "吉撩舌頭三千里。",
    "X70n1374_001:0029c23_0": "入新僧堂。",
    "X70n1374_001:0029c23_1": "挂扁額上堂。",
    "X70n1374_001:0029c23_2": "瓦礫堆頭。",
    "X70n1374_001:0029c23_3": "示現寶坊。",
    "X70n1374_001:0029c23_4": "特書新扁。",
    "X70n1374_001:0029c24_5": "開選佛場。",
    "X70n1374_001:0029c24_6": "瞻之在前。",
    "X70n1374_001:0029c24_7": "銀鈎鐵畫。",
    "X70n1374_001:0029c24_8": "仰之彌高。",
    "X70n1374_001:0029c24_9": "鳳翥龍驤。",
    "X70n1374_001:0030a01_10": "切切為人處。",
    "X70n1374_001:0030a01_11": "明明不覆藏。",
    "X70n1374_001:0030a01_12": "到來如具眼。",
    "X70n1374_001:0030a01_13": "直下便承當。",
    "X70n1374_001:0030a02_14": "舉足步步踏著。",
    "X70n1374_001:0030a02_15": "片磚鋪片地。",
    "X70n1374_001:0030a02_16": "舉目一一驗過。",
    "X70n1374_001:0030a02_17": "一柱拄一梁。",
    "X70n1374_001:0030a03_18": "對機有準。",
    "X70n1374_001:0030a03_19": "妙應無方。",
    "X70n1374_001:0030a03_20": "由此中選。",
    "X70n1374_001:0030a03_21": "續慧命於萬世。",
    "X70n1374_001:0030a04_22": "以此祝。",
    "X70n1374_001:0030a05_0": "聖。",
    "X70n1374_001:0030a05_1": "開壽域於八荒。",
    "X70n1374_001:0030a05_2": "堪笑力盡途窮底。",
    "X70n1374_001:0030a05_3": "低頭便入。",
    "X70n1374_001:0030a05_4": "節外生枝底。",
    "X70n1374_001:0030a06_5": "牽牛巡堂。",
    "X70n1374_001:0030a06_6": "只如涓此十八日吉辰。",
    "X70n1374_001:0030a06_7": "式摽此額。",
    "X70n1374_001:0030a07_0": "而入此堂。",
    "X70n1374_001:0030a07_1": "汝諸人。",
    "X70n1374_001:0030a07_2": "又作麼生體任。",
    "X70n1374_001:0030a07_3": "卓拄杖一下。",
    "X70n1374_001:0030a08_0": "舉黃龍南禪師。",
    "X70n1374_001:0030a08_1": "同隆慶閑禪師。",
    "X70n1374_001:0030a08_2": "入僧堂。",
    "X70n1374_001:0030a08_3": "南云。",
    "X70n1374_001:0030a08_4": "好僧堂。",
    "X70n1374_001:0030a09_0": "慶云。",
    "X70n1374_001:0030a09_1": "好僧堂。",
    "X70n1374_001:0030a09_2": "龍云。",
    "X70n1374_001:0030a09_3": "好在什麼處。",
    "X70n1374_001:0030a09_4": "慶云。",
    "X70n1374_001:0030a09_5": "一梁對一柱。",
    "X70n1374_001:0030a09_6": "龍以手指云。",
    "X70n1374_001:0030a10_7": "這一柱。",
    "X70n1374_001:0030a10_8": "得恁麼圓。",
    "X70n1374_001:0030a10_9": "那一枋。",
    "X70n1374_001:0030a10_10": "得恁麼匾。",
    "X70n1374_001:0030a10_11": "慶云。",
    "X70n1374_001:0030a11_0": "人天知識。",
    "X70n1374_001:0030a11_1": "不無和尚。",
    "X70n1374_001:0030a11_2": "拈云。",
    "X70n1374_001:0030a11_3": "黃龍隆慶二大老。",
    "X70n1374_001:0030a11_4": "恁麼問答。",
    "X70n1374_001:0030a12_5": "錦包特石。",
    "X70n1374_001:0030a12_6": "綿褁秤鎚。",
    "X70n1374_001:0030a12_7": "今日忽有人問好僧堂。",
    "X70n1374_001:0030a12_8": "却向他道。",
    "X70n1374_001:0030a13_9": "去年臘月庚申日立木。",
    "X70n1374_001:0030a13_10": "今年九月己未日歸堂。",
    "X70n1374_001:0030a14_0": "中秋上堂。",
    "X70n1374_001:0030a14_1": "舉古德云。",
    "X70n1374_001:0030a14_2": "盡大地明皎皎地。",
    "X70n1374_001:0030a14_3": "有一絲頭。",
    "X70n1374_001:0030a14_4": "便有一絲頭。",
    "X70n1374_001:0030a15_5": "有擒有縱。",
    "X70n1374_001:0030a15_6": "盡大地明皎皎地。",
    "X70n1374_001:0030a15_7": "有一絲頭。",
    "X70n1374_001:0030a15_8": "無一絲頭。",
    "X70n1374_001:0030a16_9": "全放全收。",
    "X70n1374_001:0030a16_10": "汝諸人。",
    "X70n1374_001:0030a16_11": "還知二大老落處麼。",
    "X70n1374_001:0030a16_12": "良久云。",
    "X70n1374_001:0030a17_13": "幸然平似鏡。",
    "X70n1374_001:0030a17_14": "何用曲如鈎。",
    "X70n1374_001:0030a18_0": "徑山雲峯和尚。",
    "X70n1374_001:0030a18_1": "遺書至上堂。",
    "X70n1374_001:0030a18_2": "傳佛心宗。",
    "X70n1374_001:0030a18_3": "弘佛智道。",
    "X70n1374_001:0030a18_4": "亦云居頂。",
    "X70n1374_001:0030a19_5": "亦云得住。",
    "X70n1374_001:0030a19_6": "平時橫說竪說。",
    "X70n1374_001:0030a19_7": "只是無人會得末後句。",
    "X70n1374_001:0030a20_8": "以致道場換手槌胸。",
    "X70n1374_001:0030a20_9": "何故。",
    "X70n1374_001:0030a20_10": "苦屈之詞。",
    "X70n1374_001:0030a20_11": "不妨難吐。",
    "X70n1374_001:0030a21_0": "因事再歸上堂。",
    "X70n1374_001:0030a21_1": "春融合浦再還珠。",
    "X70n1374_001:0030a21_2": "炯炯寒光爍太虗。",
    "X70n1374_001:0030a22_0": "隻手擎來攧不破。",
    "X70n1374_001:0030a22_1": "月明依舊上珊瑚。",
    "X70n1374_001:0030a23_0": "聖節上堂。",
    "X70n1374_001:0030a23_1": "至寬至大。",
    "X70n1374_001:0030a23_2": "至貴至尊。",
    "X70n1374_001:0030a23_3": "如月行空。",
    "X70n1374_001:0030a23_4": "大地全歸一照。",
    "X70n1374_001:0030a24_5": "如水在海。",
    "X70n1374_001:0030a24_6": "千波總合一源。",
    "X70n1374_001:0030a24_7": "臣僧今日。",
    "X70n1374_001:0030a24_8": "讚嘆也讚嘆不及。",
    "X70n1374_001:0030b01_9": "驀拈拄杖云。",
    "X70n1374_001:0030b01_10": "且聽拄杖子。",
    "X70n1374_001:0030b01_11": "出來共相祝讚看。",
    "X70n1374_001:0030b02_0": "卓一下。",
    "X70n1374_001:0030b02_1": "祥開天壽節。",
    "X70n1374_001:0030b03_2": "踰億萬斯年。",
    "X70n1374_001:0030b04_0": "中夏上堂。",
    "X70n1374_001:0030b04_1": "舉雲門示眾云。",
    "X70n1374_001:0030b04_2": "今日已半夏了也。",
    "X70n1374_001:0030b04_3": "敲磕一句。",
    "X70n1374_001:0030b05_4": "道將來。",
    "X70n1374_001:0030b05_5": "眾無對。",
    "X70n1374_001:0030b05_6": "門乃云。",
    "X70n1374_001:0030b05_7": "蜜怛利智。",
    "X70n1374_001:0030b05_8": "拈云。",
    "X70n1374_001:0030b05_9": "大小雲門。",
    "X70n1374_001:0030b06_0": "將梵語示人。",
    "X70n1374_001:0030b06_1": "教人作麼生會。",
    "X70n1374_001:0030b06_2": "輒成一頌。",
    "X70n1374_001:0030b06_3": "租佃朝朝到庫頭。",
    "X70n1374_001:0030b07_4": "問他時候道難消。",
    "X70n1374_001:0030b07_5": "桔槹閑在綠楊外。",
    "X70n1374_001:0030b07_6": "赫日炎炎好暠苗。",
    "X70n1374_001:0030b09_0": "賜金襴袈裟。",
    "X70n1374_001:0030b09_1": "兼佛海性空師號。",
    "X70n1374_001:0030b09_2": "謝恩上堂。",
    "X70n1374_001:0030b10_3": "為定正宗。",
    "X70n1374_001:0030b10_4": "特賜金襴法服。",
    "X70n1374_001:0030b10_5": "仍降師號。",
    "X70n1374_001:0030b10_6": "顯揚佛海性空。",
    "X70n1374_001:0030b11_7": "全身奉重。",
    "X70n1374_001:0030b11_8": "風虎雲龍。",
    "X70n1374_001:0030b11_9": "上感皇恩。",
    "X70n1374_001:0030b12_10": "何以報。",
    "X70n1374_001:0030b12_11": "萬年松在祝融峯。",
    "X70n1374_001:0030b13_0": "上堂。",
    "X70n1374_001:0030b13_1": "秋甲子雨。",
    "X70n1374_001:0030b13_2": "禾頭生耳。",
    "X70n1374_001:0030b13_3": "水拍平疇。",
    "X70n1374_001:0030b13_4": "雲包大地。",
    "X70n1374_001:0030b13_5": "莫教爛却我東山下左邊底。",
    "X70n1374_001:0030b15_0": "開爐。",
    "X70n1374_001:0030b15_1": "奉諸祖歸新祖堂。",
    "X70n1374_001:0030b15_2": "兼謝監收上堂。",
    "X70n1374_001:0030b15_3": "南方火爐頭。",
    "X70n1374_001:0030b16_0": "有一則無賓主話。",
    "X70n1374_001:0030b16_1": "諸方舉者甚多。",
    "X70n1374_001:0030b16_2": "明者不少。",
    "X70n1374_001:0030b16_3": "道場亦有則語。",
    "X70n1374_001:0030b17_4": "舉似諸人。",
    "X70n1374_001:0030b17_5": "只要大家委悉。",
    "X70n1374_001:0030b17_6": "且道。",
    "X70n1374_001:0030b17_7": "是什麼語。",
    "X70n1374_001:0030b17_8": "良久云。",
    "X70n1374_001:0030b18_9": "諸莊有收。",
    "X70n1374_001:0030b18_10": "諸祖有堂。",
    "X70n1374_001:0030b19_0": "天壽節上堂。",
    "X70n1374_001:0030b19_1": "天地之大德曰生。",
    "X70n1374_001:0030b19_2": "聖人之大寶曰位。",
    "X70n1374_001:0030b19_3": "至大至剛。",
    "X70n1374_001:0030b20_4": "至尊至貴。",
    "X70n1374_001:0030b20_5": "嵩嶽聲聲呼萬歲。",
    "X70n1374_001:0030b21_0": "上堂。",
    "X70n1374_001:0030b21_1": "終日忙忙。",
    "X70n1374_001:0030b21_2": "那事無妨。",
    "X70n1374_001:0030b21_3": "顯而不露。",
    "X70n1374_001:0030b21_4": "隱而不藏。",
    "X70n1374_001:0030b21_5": "如何是不藏底事。",
    "X70n1374_001:0030b22_6": "玉梅結子浮青樹。",
    "X70n1374_001:0030b22_7": "石筍抽條上綠牕。",
    "X70n1374_001:0030b23_0": "立春日上堂。",
    "X70n1374_001:0030b23_1": "春山春水流。",
    "X70n1374_001:0030b23_2": "春日打春牛。",
    "X70n1374_001:0030b23_3": "若作春意會。",
    "X70n1374_001:0030b24_0": "春不在林丘。",
    "X70n1374_001:0030b24_1": "不作春意會。",
    "X70n1374_001:0030b24_2": "春色滿皇州。",
    "X70n1374_001:0030b24_3": "擊拂子云。",
    "X70n1374_001:0030b24_4": "看看好事又從頭。",
    "X70n1374_001:0030c02_0": "聖節上堂。",
    "X70n1374_001:0030c02_1": "風摩雨洗石猶潤。",
    "X70n1374_001:0030c02_2": "水碧山明天更清。",
    "X70n1374_001:0030c02_3": "百億須彌同起舞。",
    "X70n1374_001:0030c03_4": "大千沙界盡歡聲。",
    "X70n1374_001:0030c03_5": "諸人還見此奇特麼。",
    "X70n1374_001:0030c04_0": "良久云。",
    "X70n1374_001:0030c04_1": "惟民得主。",
    "X70n1374_001:0030c05_0": "杭州納徵錢回上堂。",
    "X70n1374_001:0030c05_1": "心若無事。",
    "X70n1374_001:0030c05_2": "萬法不生。",
    "X70n1374_001:0030c05_3": "山僧昨日自杭州回。",
    "X70n1374_001:0030c06_4": "得箇拄杖子。",
    "X70n1374_001:0030c06_5": "了却時事。",
    "X70n1374_001:0030c06_6": "諸人且道。",
    "X70n1374_001:0030c06_7": "那裏是無事處。",
    "X70n1374_001:0030c07_8": "良久云。",
    "X70n1374_001:0030c07_9": "泉聲到池盡。",
    "X70n1374_001:0030c07_10": "山色上樓多。",
    "X70n1374_001:0030c08_0": "上堂。",
    "X70n1374_001:0030c08_1": "蠶上筐。",
    "X70n1374_001:0030c08_2": "麥上場。",
    "X70n1374_001:0030c08_3": "不寒不煖。",
    "X70n1374_001:0030c08_4": "夜短晝長。",
    "X70n1374_001:0030c08_5": "理事不二。",
    "X70n1374_001:0030c09_0": "照用全彰。",
    "X70n1374_001:0030c09_1": "且道。",
    "X70n1374_001:0030c09_2": "山前田厙翁。",
    "X70n1374_001:0030c09_3": "是第幾郎。",
    "X70n1374_001:0030c09_4": "參。",
    "X70n1374_001:0030c10_0": "上堂。",
    "X70n1374_001:0030c10_1": "舉思大和尚云。",
    "X70n1374_001:0030c10_2": "長夏坐禪。",
    "X70n1374_001:0030c10_3": "巍然不動。",
    "X70n1374_001:0030c10_4": "侍僧云。",
    "X70n1374_001:0030c10_5": "今日自恣。",
    "X70n1374_001:0030c11_6": "思自嘆云。",
    "X70n1374_001:0030c11_7": "佛在世時。",
    "X70n1374_001:0030c11_8": "證道者無數。",
    "X70n1374_001:0030c11_9": "今日去聖逾遠。",
    "X70n1374_001:0030c12_10": "一無所獲。",
    "X70n1374_001:0030c12_11": "遂放身偃仰間。",
    "X70n1374_001:0030c12_12": "忽眼根清淨。",
    "X70n1374_001:0030c12_13": "悉見三千大千世界。",
    "X70n1374_001:0030c13_14": "如觀掌中。",
    "X70n1374_001:0030c13_15": "頌云。",
    "X70n1374_001:0030c13_16": "萬里秋天西北風。",
    "X70n1374_001:0030c13_17": "淨如古井鑑秋容。",
    "X70n1374_001:0030c14_18": "箇中誰具透箐眼。",
    "X70n1374_001:0030c14_19": "識得將軍汗馬功。",
    "X70n1374_001:0030c15_0": "結制上堂。",
    "X70n1374_001:0030c15_1": "釋迦老子。",
    "X70n1374_001:0030c15_2": "二千年前。",
    "X70n1374_001:0030c15_3": "約法三章。",
    "X70n1374_001:0030c15_4": "以大圓覺。",
    "X70n1374_001:0030c16_0": "為我伽藍。",
    "X70n1374_001:0030c16_1": "身心安居。",
    "X70n1374_001:0030c16_2": "平等性智。",
    "X70n1374_001:0030c16_3": "尅期修證。",
    "X70n1374_001:0030c16_4": "取驗蠟人。",
    "X70n1374_001:0030c17_0": "總是強生節目。",
    "X70n1374_001:0030c17_1": "我這裡。",
    "X70n1374_001:0030c17_2": "只守閑閑地。",
    "X70n1374_001:0030c17_3": "一事不為。",
    "X70n1374_001:0030c17_4": "何故。",
    "X70n1374_001:0030c18_0": "善學柳下惠。",
    "X70n1374_001:0030c18_1": "終不師其跡。",
    "X70n1374_001:0030c19_0": "上堂。",
    "X70n1374_001:0030c19_1": "謝叔平首座。",
    "X70n1374_001:0030c19_2": "珠書記。",
    "X70n1374_001:0030c19_3": "水石中有珠玉。",
    "X70n1374_001:0030c19_4": "而淵藪光輝。",
    "X70n1374_001:0030c20_5": "叢林中有龍象。",
    "X70n1374_001:0030c20_6": "而佛法興隆。",
    "X70n1374_001:0030c20_7": "領徒行脚。",
    "X70n1374_001:0030c20_8": "不妨坐地商量。",
    "X70n1374_001:0030c21_9": "分座提綱。",
    "X70n1374_001:0030c21_10": "即為人天眼目。",
    "X70n1374_001:0030c21_11": "勘破雲門接洞山。",
    "X70n1374_001:0030c21_12": "超他猛虎當路坐。",
    "X70n1374_001:0030c22_13": "老我巖叢。",
    "X70n1374_001:0030c22_14": "別有囑付。",
    "X70n1374_001:0030c22_15": "擲下拄杖云。",
    "X70n1374_001:0030c22_16": "貴在橫身荷負。",
    "X70n1374_001:0030c24_0": "浴佛上堂。",
    "X70n1374_001:0030c24_1": "今朝四月八。",
    "X70n1374_001:0030c24_2": "世尊降誕日。",
    "X70n1374_001:0030c24_3": "諸方盡皆三沐三薰。",
    "X70n1374_001:0031a01_4": "雲峰體任雖同。",
    "X70n1374_001:0031a01_5": "用處猶別。",
    "X70n1374_001:0031a01_6": "遂將拂子。",
    "X70n1374_001:0031a01_7": "作臽勢。",
    "X70n1374_001:0031a01_8": "以第一杓虎跑泉水。",
    "X70n1374_001:0031a02_9": "洗他指天指地。",
    "X70n1374_001:0031a02_10": "妄自稱尊。",
    "X70n1374_001:0031a02_11": "又將拂子。",
    "X70n1374_001:0031a03_12": "作臽勢。",
    "X70n1374_001:0031a03_13": "以第二杓一勺泉水。",
    "X70n1374_001:0031a03_14": "洗他西十九年。",
    "X70n1374_001:0031a03_15": "橫說竪說。",
    "X70n1374_001:0031a04_16": "又將拂子。",
    "X70n1374_001:0031a04_17": "作臽勢。",
    "X70n1374_001:0031a04_18": "只這第三杓。",
    "X70n1374_001:0031a04_19": "八功德水。",
    "X70n1374_001:0031a04_20": "臽與諸人。",
    "X70n1374_001:0031a05_21": "同向大雄寶殿。",
    "X70n1374_001:0031a05_22": "洗他現今赤身露體。",
    "X70n1374_001:0031a05_23": "岐立金盆。",
    "X70n1374_001:0031a06_0": "擊拂子云。",
    "X70n1374_001:0031a06_1": "將此身心奉塵剎。",
    "X70n1374_001:0031a06_2": "是則名為報佛恩。",
    "X70n1374_001:0031a07_0": "上堂。",
    "X70n1374_001:0031a07_1": "逼龜成兆。",
    "X70n1374_001:0031a07_2": "達磨來梁。",
    "X70n1374_001:0031a07_3": "引人落草。",
    "X70n1374_001:0031a07_4": "摩騰入漢。",
    "X70n1374_001:0031a07_5": "山僧未入母胎已前。",
    "X70n1374_001:0031a08_6": "已與諸人。",
    "X70n1374_001:0031a08_7": "在長連床上。",
    "X70n1374_001:0031a08_8": "同粥同飯。",
    "X70n1374_001:0031a08_9": "同起同倒。",
    "X70n1374_001:0031a09_10": "今朝特地打板坐禪。",
    "X70n1374_001:0031a09_11": "也是提水放火。",
    "X70n1374_001:0031a10_0": "端午上堂。",
    "X70n1374_001:0031a10_1": "一箇採藥。",
    "X70n1374_001:0031a10_2": "一箇用藥。",
    "X70n1374_001:0031a10_3": "且道那箇是服藥底。",
    "X70n1374_001:0031a11_0": "良久云。",
    "X70n1374_001:0031a11_1": "睡美不知山雨過。",
    "X70n1374_001:0031a11_2": "覺來殿閣自生凉。",
    "X70n1374_001:0031a12_0": "風雨上堂。",
    "X70n1374_001:0031a12_1": "不可以無心求。",
    "X70n1374_001:0031a12_2": "不可以有心得。",
    "X70n1374_001:0031a12_3": "避得風灾。",
    "X70n1374_001:0031a13_0": "又遭水厄。",
    "X70n1374_001:0031a13_1": "向道。",
    "X70n1374_001:0031a13_2": "夜行莫踏白。",
    "X70n1374_001:0031a13_3": "卓拄杖。",
    "X70n1374_001:0031a13_4": "下座。",
    "X70n1374_001:0031a14_0": "謝大梅指堂首座上堂。",
    "X70n1374_001:0031a14_1": "即心即佛。",
    "X70n1374_001:0031a14_2": "育王有舍利。",
    "X70n1374_001:0031a14_3": "非心非佛。",
    "X70n1374_001:0031a15_4": "岳林有彌勒。",
    "X70n1374_001:0031a15_5": "不是心。",
    "X70n1374_001:0031a15_6": "不是佛。",
    "X70n1374_001:0031a15_7": "不是物。",
    "X70n1374_001:0031a15_8": "補陁有觀音。",
    "X70n1374_001:0031a16_9": "竪起拂子云。",
    "X70n1374_001:0031a16_10": "諸人還見三佛。",
    "X70n1374_001:0031a16_11": "在拂子頭上。",
    "X70n1374_001:0031a16_12": "轉大法輪麼。",
    "X70n1374_001:0031a17_13": "掛拂子云。",
    "X70n1374_001:0031a17_14": "若是人天眼目。",
    "X70n1374_001:0031a17_15": "早已勘破。",
    "X70n1374_001:0031a18_0": "小參",
    "X70n1374_001:0031a19_0": "結制。",
    "X70n1374_001:0031a19_1": "盡大地是箇自己。",
    "X70n1374_001:0031a19_2": "甚處安居。",
    "X70n1374_001:0031a19_3": "總十方為一伽藍。",
    "X70n1374_001:0031a20_0": "如何禁足。",
    "X70n1374_001:0031a20_1": "壁立萬仞。",
    "X70n1374_001:0031a20_2": "擡頭撞破虗空。",
    "X70n1374_001:0031a20_3": "直往無前。",
    "X70n1374_001:0031a20_4": "信步縱橫十字。",
    "X70n1374_001:0031a21_5": "廓爾了無修證。",
    "X70n1374_001:0031a21_6": "更誰取驗蠟人。",
    "X70n1374_001:0031a21_7": "直得金蓋峯點頭。",
    "X70n1374_001:0031a22_8": "唯唯。",
    "X70n1374_001:0031a22_9": "虎跑泉發笑。",
    "X70n1374_001:0031a22_10": "呵呵。",
    "X70n1374_001:0031a22_11": "祇如大愚莖虀。",
    "X70n1374_001:0031a22_12": "天平兩錯。",
    "X70n1374_001:0031a23_13": "又且如何話會。",
    "X70n1374_001:0031a23_14": "擊拂子云。",
    "X70n1374_001:0031a23_15": "款款地與儞商量。",
    "X70n1374_001:0031a24_0": "冬節。",
    "X70n1374_001:0031a24_1": "拈拄杖云。",
    "X70n1374_001:0031a24_2": "過去諸如來已用。",
    "X70n1374_001:0031a24_3": "現在諸菩薩今用。",
    "X70n1374_001:0031b01_0": "未來修學人當用。",
    "X70n1374_001:0031b01_1": "我這裏。",
    "X70n1374_001:0031b01_2": "總不與麼。",
    "X70n1374_001:0031b01_3": "今冬田熟。",
    "X70n1374_001:0031b01_4": "舊債銷鎔。",
    "X70n1374_001:0031b02_5": "倉廩充滿。",
    "X70n1374_001:0031b02_6": "飯籮不空。",
    "X70n1374_001:0031b02_7": "不妨滿盤高飣。",
    "X70n1374_001:0031b02_8": "應時大展家風。",
    "X70n1374_001:0031b03_9": "便見雲峯閣伏虎岩。",
    "X70n1374_001:0031b03_10": "交相慶賀。",
    "X70n1374_001:0031b03_11": "叉手當胸。",
    "X70n1374_001:0031b03_12": "却共耳語云。",
    "X70n1374_001:0031b04_13": "更得一年兩熟。",
    "X70n1374_001:0031b04_14": "僧堂法堂。",
    "X70n1374_001:0031b04_15": "便可興工。",
    "X70n1374_001:0031b04_16": "是則是。",
    "X70n1374_001:0031b05_0": "只如今夜用處。",
    "X70n1374_001:0031b05_1": "與從上佛祖。",
    "X70n1374_001:0031b05_2": "是別是同。",
    "X70n1374_001:0031b05_3": "卓拄杖云。",
    "X70n1374_001:0031b05_4": "義出年豐。",
    "X70n1374_001:0031b07_0": "結夏。",
    "X70n1374_001:0031b07_1": "酷日炎炎。",
    "X70n1374_001:0031b07_2": "凉風習習。",
    "X70n1374_001:0031b07_3": "黃面老子。",
    "X70n1374_001:0031b07_4": "曾於此時。",
    "X70n1374_001:0031b07_5": "以拂子。",
    "X70n1374_001:0031b08_6": "打一圓相云。",
    "X70n1374_001:0031b08_7": "將此網子。",
    "X70n1374_001:0031b08_8": "撒向四天下。",
    "X70n1374_001:0031b08_9": "撈摝羣生。",
    "X70n1374_001:0031b08_10": "喚作禁足護生。",
    "X70n1374_001:0031b09_11": "尅期取證。",
    "X70n1374_001:0031b09_12": "至今二千餘年。",
    "X70n1374_001:0031b09_13": "收拾不上。",
    "X70n1374_001:0031b09_14": "道場即今。",
    "X70n1374_001:0031b10_15": "力為提其綱。",
    "X70n1374_001:0031b10_16": "挈其維。",
    "X70n1374_001:0031b10_17": "不問聖凡。",
    "X70n1374_001:0031b10_18": "一網打就。",
    "X70n1374_001:0031b10_19": "若是已具三十六鱗底。",
    "X70n1374_001:0031b11_20": "擊拂子云。",
    "X70n1374_001:0031b11_21": "向此霹靂聲中。",
    "X70n1374_001:0031b11_22": "騰身一躍。",
    "X70n1374_001:0031b12_23": "透過禹門。",
    "X70n1374_001:0031b12_24": "其或未然。",
    "X70n1374_001:0031b12_25": "切忌點額。",
    "X70n1374_001:0031b13_0": "拈古",
    "X70n1374_001:0031b14_0": "舉天衣懷和尚示眾云。",
    "X70n1374_001:0031b14_1": "譬如雁過長空。",
    "X70n1374_001:0031b14_2": "影沈寒水。",
    "X70n1374_001:0031b14_3": "雁無遺蹤之意。",
    "X70n1374_001:0031b15_4": "水無留影之心。",
    "X70n1374_001:0031b16_0": "師云。",
    "X70n1374_001:0031b16_1": "大眾。",
    "X70n1374_001:0031b16_2": "要見天衣和尚。",
    "X70n1374_001:0031b16_3": "四楞塌地處麼。",
    "X70n1374_001:0031b16_4": "野色更無山隔斷。",
    "X70n1374_001:0031b17_5": "天光直與水相通。",
    "X70n1374_001:0031b18_0": "舉金牛和尚。",
    "X70n1374_001:0031b18_1": "凡自做飯。",
    "X70n1374_001:0031b18_2": "供養眾僧。",
    "X70n1374_001:0031b18_3": "每至齋時。",
    "X70n1374_001:0031b18_4": "舁飯桶。",
    "X70n1374_001:0031b19_0": "到僧堂前。",
    "X70n1374_001:0031b19_1": "遂作舞云。",
    "X70n1374_001:0031b19_2": "菩薩子。",
    "X70n1374_001:0031b19_3": "喫飯來。",
    "X70n1374_001:0031b19_4": "乃拍手大笑。",
    "X70n1374_001:0031b20_0": "師云。",
    "X70n1374_001:0031b20_1": "等是弄粥飯氣。",
    "X70n1374_001:0031b20_2": "金牛却較些子。",
    "X70n1374_001:0031b21_0": "舉白水仁和尚示眾云。",
    "X70n1374_001:0031b21_1": "尋常不欲向聲前句後。",
    "X70n1374_001:0031b21_2": "鼓弄人家男女。",
    "X70n1374_001:0031b22_3": "何故。",
    "X70n1374_001:0031b22_4": "且聲不是聲。",
    "X70n1374_001:0031b22_5": "色不是色。",
    "X70n1374_001:0031b22_6": "時有僧問。",
    "X70n1374_001:0031b22_7": "如何是聲不是聲。",
    "X70n1374_001:0031b23_8": "仁云。",
    "X70n1374_001:0031b23_9": "喚作色得麼。",
    "X70n1374_001:0031b23_10": "僧云。",
    "X70n1374_001:0031b23_11": "如何是色不是色。",
    "X70n1374_001:0031b24_12": "仁云。",
    "X70n1374_001:0031b24_13": "喚作聲得麼。",
    "X70n1374_001:0031b24_14": "僧禮拜。",
    "X70n1374_001:0031b24_15": "仁云。",
    "X70n1374_001:0031b24_16": "且道。",
    "X70n1374_001:0031b24_17": "為你說。",
    "X70n1374_001:0031b24_18": "答汝話。",
    "X70n1374_001:0031c01_19": "若人辨得。",
    "X70n1374_001:0031c01_20": "許汝有箇入處。",
    "X70n1374_001:0031c02_0": "師云。",
    "X70n1374_001:0031c02_1": "私商掗賣。",
    "X70n1374_001:0031c02_2": "白水急要人承當。",
    "X70n1374_001:0031c02_3": "擊拂云。",
    "X70n1374_001:0031c02_4": "者箇不是聲。",
    "X70n1374_001:0031c03_5": "為汝說。",
    "X70n1374_001:0031c03_6": "竪拂云。",
    "X70n1374_001:0031c03_7": "者箇不是色。",
    "X70n1374_001:0031c03_8": "答汝話。",
    "X70n1374_001:0031c03_9": "直饒辨得。",
    "X70n1374_001:0031c04_10": "未許汝有箇入處。",
    "X70n1374_001:0031c05_0": "舉三聖問雪峯。",
    "X70n1374_001:0031c05_1": "透網金鱗。",
    "X70n1374_001:0031c05_2": "以何為食。",
    "X70n1374_001:0031c05_3": "公案。",
    "X70n1374_001:0031c06_0": "師云。",
    "X70n1374_001:0031c06_1": "這瞎驢。",
    "X70n1374_001:0031c06_2": "放出透網金鱗。",
    "X70n1374_001:0031c06_3": "擬吞却千五百人善知識。",
    "X70n1374_001:0031c07_4": "殊不知。",
    "X70n1374_001:0031c07_5": "端坐碧油幢下。",
    "X70n1374_001:0031c07_6": "有擒縱之機。",
    "X70n1374_001:0031c07_7": "固不戰而屈人兵。",
    "X70n1374_001:0031c08_8": "二大老。",
    "X70n1374_001:0031c08_9": "雖是好手手中呈好手。",
    "X70n1374_001:0031c08_10": "要且總未有出身一路在。",
    "X70n1374_001:0031c10_0": "舉國師三喚侍者。",
    "X70n1374_001:0031c10_1": "頻呼小玉元無事。",
    "X70n1374_001:0031c10_2": "侍者三應。",
    "X70n1374_001:0031c10_3": "只要檀郎認得聲。",
    "X70n1374_001:0031c11_4": "將謂吾孤負汝。",
    "X70n1374_001:0031c11_5": "生平肝膽向人傾。",
    "X70n1374_001:0031c11_6": "誰知汝孤負吾。",
    "X70n1374_001:0031c12_7": "相識還同不相識。",
    "X70n1374_001:0031c13_0": "師云。",
    "X70n1374_001:0031c13_1": "道場這裡。",
    "X70n1374_001:0031c13_2": "朝朝暮暮。",
    "X70n1374_001:0031c13_3": "啐啄同時。",
    "X70n1374_001:0031c13_4": "應笑國師。",
    "X70n1374_001:0031c13_5": "用盡自己心。",
    "X70n1374_001:0031c14_6": "笑破他人口。",
    "X70n1374_001:0031c15_0": "頌古",
    "X70n1374_001:0031c16_0": "德山小參不答話。",
    "X70n1374_001:0031c17_0": "汲水佳人立曉風。",
    "X70n1374_001:0031c17_1": "青絲放盡轆轤空。",
    "X70n1374_001:0031c17_2": "銀缾觸破殘粧影。",
    "X70n1374_001:0031c18_3": "零落桃花映水紅。",
    "X70n1374_001:0031c19_0": "本山全和尚。",
    "X70n1374_001:0031c19_1": "示眾云。",
    "X70n1374_001:0031c19_2": "一塵起。",
    "X70n1374_001:0031c19_3": "大地收。",
    "X70n1374_001:0031c19_4": "釋迦老子。",
    "X70n1374_001:0031c20_5": "為甚麼。",
    "X70n1374_001:0031c20_6": "三七日中。",
    "X70n1374_001:0031c20_7": "思惟如是事。",
    "X70n1374_001:0031c20_8": "一花開。",
    "X70n1374_001:0031c20_9": "大地春。",
    "X70n1374_001:0031c21_10": "達麼大師。",
    "X70n1374_001:0031c21_11": "為甚麼。",
    "X70n1374_001:0031c21_12": "九年面壁。",
    "X70n1374_001:0031c21_13": "若人道得收足句。",
    "X70n1374_001:0031c22_14": "可以坐致太平。",
    "X70n1374_001:0031c23_0": "風遞山花入袖香。",
    "X70n1374_001:0031c23_1": "日長無事倚胡床。",
    "X70n1374_001:0031c23_2": "亂紅如雨春歸去。",
    "X70n1374_001:0031c24_3": "杜宇聲聲啼夕陽。",
    "X70n1374_001:0032a01_0": "古德因僧問。",
    "X70n1374_001:0032a01_1": "如何是諸佛師。",
    "X70n1374_001:0032a01_2": "德云。",
    "X70n1374_001:0032a01_3": "近日魯祖出世。",
    "X70n1374_001:0032a02_4": "為人東道西話。",
    "X70n1374_001:0032a03_0": "上苑花開到牡丹。",
    "X70n1374_001:0032a03_1": "紛紛紅紫釀春寒。",
    "X70n1374_001:0032a03_2": "遊人眼底無姚魏。",
    "X70n1374_001:0032a04_3": "空向枝頭著意看。",
    "X70n1374_001:0032a05_0": "本仁和尚示眾云。",
    "X70n1374_001:0032a05_1": "眼裏著沙不得。",
    "X70n1374_001:0032a05_2": "耳裏著水不得。",
    "X70n1374_001:0032a06_3": "時有僧問云。",
    "X70n1374_001:0032a06_4": "如何是眼裏著沙不得。",
    "X70n1374_001:0032a06_5": "答云。",
    "X70n1374_001:0032a07_6": "應真無比。",
    "X70n1374_001:0032a07_7": "如何是耳裏著水不得。",
    "X70n1374_001:0032a07_8": "答云。",
    "X70n1374_001:0032a07_9": "白淨無垢。",
    "X70n1374_001:0032a09_0": "綠樹陰濃夏日長。",
    "X70n1374_001:0032a09_1": "樓臺倒影落橫塘。",
    "X70n1374_001:0032a09_2": "水晶簾動微風起。",
    "X70n1374_001:0032a10_3": "滿架薔薇一院香。",
    "X70n1374_001:0032a11_0": "厖公指雪。",
    "X70n1374_001:0032a12_0": "當空指出白糢糊。",
    "X70n1374_001:0032a12_1": "多少禪流眼力枯。",
    "X70n1374_001:0032a12_2": "不得攔腮揮一掌。",
    "X70n1374_001:0032a13_3": "幾乎凍殺在中途。",
    "X70n1374_001:0032a14_0": "偈頌",
    "X70n1374_001:0032a15_0": "送斷橋小師。",
    "X70n1374_001:0032a15_1": "歸越住謝府菴折屐拖霜角帽斜。",
    "X70n1374_001:0032a16_2": "越王城畔立生涯。",
    "X70n1374_001:0032a16_3": "徐家兒種謝家地。",
    "X70n1374_001:0032a17_4": "莧菜根頭又發芽。",
    "X70n1374_001:0032a18_0": "寄白雲菴克翁",
    "X70n1374_001:0032a19_0": "如是去來如是住。",
    "X70n1374_001:0032a19_1": "釋天明淨翳雲開。",
    "X70n1374_001:0032a19_2": "閑邊一片坐禪石。",
    "X70n1374_001:0032a20_3": "時折松枝拂蘚苔。",
    "X70n1374_001:0032a21_0": "刺血書蓮經",
    "X70n1374_001:0032a22_0": "一指尖頭下一針。",
    "X70n1374_001:0032a22_1": "紅蓮初綻碧波深。",
    "X70n1374_001:0032a22_2": "香風吹散花狼藉。",
    "X70n1374_001:0032a23_3": "片片無非是赤心。",
    "X70n1374_001:0032a24_0": "先禪客(南劒人)",
    "X70n1374_001:0032b01_0": "問似雲興難止遏。",
    "X70n1374_001:0032b01_1": "答如劒水逆翻瀾。",
    "X70n1374_001:0032b01_2": "能從虎穴翻身去。",
    "X70n1374_001:0032b02_3": "許你親曾見一斑。",
    "X70n1374_001:0032b03_0": "宜叟號",
    "X70n1374_001:0032b04_0": "以義為繩親得路。",
    "X70n1374_001:0032b04_1": "鐵心不逐歲華更。",
    "X70n1374_001:0032b04_2": "老來義路俱忘却。",
    "X70n1374_001:0032b05_3": "是處山邊水畔行。",
    "X70n1374_001:0032b06_0": "毒海號",
    "X70n1374_001:0032b07_0": "溢目炎炎殺氣生。",
    "X70n1374_001:0032b07_1": "翻空黑浪幾層層。",
    "X70n1374_001:0032b07_2": "誰知最甚過如鴆。",
    "X70n1374_001:0032b08_3": "活得鯤鯨起化鵬。",
    "X70n1374_001:0032b09_0": "冷泉金裝石佛。",
    "X70n1374_001:0032b09_1": "分韻得沈字鷲嶺猿吟古木陰。",
    "X70n1374_001:0032b10_2": "力弘像教在于今。",
    "X70n1374_001:0032b10_3": "石頭大小黃金褁。",
    "X70n1374_001:0032b11_4": "末上一機山岳沈。",
    "X70n1374_001:0032b12_0": "玉田號",
    "X70n1374_001:0032b13_0": "慇懃抱璞入師門。",
    "X70n1374_001:0032b13_1": "彫琢猶存畦畛痕。",
    "X70n1374_001:0032b13_2": "大地耕翻無寸土。",
    "X70n1374_001:0032b14_3": "茫茫萬頃寶光寒。",
    "X70n1374_001:0032b15_0": "贊",
    "X70n1374_001:0032b16_0": "先師寂窻和尚",
    "X70n1374_001:0032b17_0": "我父喚翁。",
    "X70n1374_001:0032b17_1": "作屈突蓋。",
    "X70n1374_001:0032b17_2": "子今喚父。",
    "X70n1374_001:0032b17_3": "作屈突通。",
    "X70n1374_001:0032b17_4": "莫怪遞相訐露。",
    "X70n1374_001:0032b18_5": "於理實是難容。",
    "X70n1374_001:0032b18_6": "三斗艾三斗葱。",
    "X70n1374_001:0032b18_7": "吞吐須還老大蟲。",
    "X70n1374_001:0032b20_0": "中峯三世王蠻。",
    "X70n1374_001:0032b20_1": "最毒是這慈尊。",
    "X70n1374_001:0032b20_2": "出溈山牯牛隊。",
    "X70n1374_001:0032b20_3": "耕東山祖父田園。",
    "X70n1374_001:0032b21_4": "齊雲紫芝。",
    "X70n1374_001:0032b21_5": "靈芝瑞世。",
    "X70n1374_001:0032b21_6": "道山蘗山。",
    "X70n1374_001:0032b21_7": "食蘗連根。",
    "X70n1374_001:0032b22_8": "劄。",
    "X70n1374_001:0032b22_9": "洪塘硬寨。",
    "X70n1374_001:0032b22_10": "重壕累塹。",
    "X70n1374_001:0032b22_11": "布江心網子。",
    "X70n1374_001:0032b22_12": "魚躍龍奔。",
    "X70n1374_001:0032b22_13": "坐據玉几。",
    "X70n1374_001:0032b23_14": "眼乾坤。",
    "X70n1374_001:0032b23_15": "來者劈脊便摟。",
    "X70n1374_001:0032b23_16": "衲子湊泊無門。",
    "X70n1374_001:0032b23_17": "也勝提起破沙盆。",
    "X70n1374_001:0032c01_0": "跋",
    "X70n1374_001:0032c02_0": "慧上人。",
    "X70n1374_001:0032c02_1": "古愚頌軸古之愚也直。",
    "X70n1374_001:0032c03_2": "今之愚也詐。",
    "X70n1374_001:0032c03_3": "子以古愚號。",
    "X70n1374_001:0032c03_4": "厥有旨歟。",
    "X70n1374_001:0032c03_5": "雖然更須具決定志。",
    "X70n1374_001:0032c04_6": "具決擇眼。",
    "X70n1374_001:0032c04_7": "然後決定大事。",
    "X70n1374_001:0032c04_8": "則去古不多讓矣。",
    "X70n1374_001:0032c05_9": "否則墮在今時途轍。",
    "X70n1374_001:0032c05_10": "莫言雲峯不道。",
    "X70n1374_001:0032c06_0": "能侍者古樵頌軸",
    "X70n1374_001:0032c07_0": "應無所住。",
    "X70n1374_001:0032c07_1": "白日迷路。",
    "X70n1374_001:0032c07_2": "失脚黃梅。",
    "X70n1374_001:0032c07_3": "道傳千古。",
    "X70n1374_001:0032c07_4": "莫是大唐國裏無禪師。",
    "X70n1374_001:0032c08_5": "被箇擔柴漢。",
    "X70n1374_001:0032c08_6": "攪亂四天下。",
    "X70n1374_001:0032c08_7": "能侍者。",
    "X70n1374_001:0032c08_8": "能於柴衝未脫落時薦得。",
    "X70n1374_001:0032c09_9": "可謂能事畢矣。",
    "X70n1374_001:0032c09_10": "若是向此卷中。",
    "X70n1374_001:0032c10_0": "要見盧能得力處。",
    "X70n1374_001:0032c10_1": "則曹溪一脉平沈。",
    "X70n1374_001:0032c11_0": "小佛事",
    "X70n1374_001:0032c12_0": "小師明侍者火",
    "X70n1374_001:0032c13_0": "竪起火云。",
    "X70n1374_001:0032c13_1": "具啐啄機。",
    "X70n1374_001:0032c13_2": "超生死句。",
    "X70n1374_001:0032c13_3": "以明水氷。",
    "X70n1374_001:0032c13_4": "入大火聚。",
    "X70n1374_001:0032c14_0": "明侍者。",
    "X70n1374_001:0032c14_1": "諾。",
    "X70n1374_001:0032c14_2": "擲下火云。",
    "X70n1374_001:0032c14_3": "是我何曾孤負汝。",
    "X70n1374_001:0032c15_0": "氷首座火",
    "X70n1374_001:0032c16_0": "坐脫立亡。",
    "X70n1374_001:0032c16_1": "不無首座。",
    "X70n1374_001:0032c16_2": "敲氷取火。",
    "X70n1374_001:0032c16_3": "須是老僧。",
    "X70n1374_001:0032c16_4": "擲下火云。",
    "X70n1374_001:0032c17_0": "任是橫身三界外。",
    "X70n1374_001:0032c17_1": "也防烈焰上眉稜。",
    "X70n1374_001:0032c18_0": "松首座喫[蹎*頁]死",
    "X70n1374_001:0032c19_0": "五祖種松。",
    "X70n1374_001:0032c19_1": "春在钁頭。",
    "X70n1374_001:0032c19_2": "兩堂下喝。",
    "X70n1374_001:0032c19_3": "雷轟旱地。",
    "X70n1374_001:0032c19_4": "這裡翻得身吐得氣。",
    "X70n1374_001:0032c20_5": "火聚刀山恣游戲。",
    "X70n1374_001:0032c21_0": "能直歲結制日火",
    "X70n1374_001:0032c22_0": "搬土拽石。",
    "X70n1374_001:0032c22_1": "能事已畢。",
    "X70n1374_001:0032c22_2": "恰逢結制便抽單。",
    "X70n1374_001:0032c22_3": "詎肯安居死窟穴。",
    "X70n1374_001:0032c23_4": "雖然更須知有轉身時節。",
    "X70n1374_001:0032c23_5": "擲下火云。",
    "X70n1374_001:0032c23_6": "也是把火助熱。",
    "X70n1374_001:0033a01_0": "瑨都寺火(長溪人霅溪死)",
    "X70n1374_001:0033a02_0": "疊翠擁秦川。",
    "X70n1374_001:0033a02_1": "生緣何足戀。",
    "X70n1374_001:0033a02_2": "煙水漲霅溪。",
    "X70n1374_001:0033a02_3": "住處猶未穩。",
    "X70n1374_001:0033a03_0": "倒跨三脚驢。",
    "X70n1374_001:0033a03_1": "兩頭俱坐斷。",
    "X70n1374_001:0033a03_2": "雖然瑨本是次玉。",
    "X70n1374_001:0033a03_3": "且入火中煅。",
    "X70n1374_001:0033a05_0": "塔山皓廟官火",
    "X70n1374_001:0033a06_0": "塔峰巍巍。",
    "X70n1374_001:0033a06_1": "皓月皎皎。",
    "X70n1374_001:0033a06_2": "紙錢堆裏埋頭。",
    "X70n1374_001:0033a06_3": "明鏡臺前失照。",
    "X70n1374_001:0033a07_0": "(某人)我今為汝攧一照看。",
    "X70n1374_001:0033a07_1": "擲下火云。",
    "X70n1374_001:0033a07_2": "薦得吉凶未判前。",
    "X70n1374_001:0033a08_3": "火雲散盡千峯曉。",
    "X70n1374_001:0033a09_0": "東堂石坡和尚秉炬(辭世頌。",
    "X70n1374_001:0033a09_1": "有來而無踪之句)得處雖同用不同。",
    "X70n1374_001:0033a10_2": "玄關向上振宗風。",
    "X70n1374_001:0033a10_3": "峻機疊疊傾三峽。",
    "X70n1374_001:0033a11_4": "生死去來游戲中。",
    "X70n1374_001:0033a11_5": "前住當山。",
    "X70n1374_001:0033a11_6": "石坡大和尚。",
    "X70n1374_001:0033a11_7": "名重兩朝。",
    "X70n1374_001:0033a12_8": "道傳千古。",
    "X70n1374_001:0033a12_9": "一十一處。",
    "X70n1374_001:0033a12_10": "用文武火。",
    "X70n1374_001:0033a12_11": "破無準的。",
    "X70n1374_001:0033a12_12": "發焰死柴頭。",
    "X70n1374_001:0033a13_13": "而輝映龍淵。",
    "X70n1374_001:0033a13_14": "八十七年。",
    "X70n1374_001:0033a13_15": "享耆頤壽。",
    "X70n1374_001:0033a13_16": "量等曾郎。",
    "X70n1374_001:0033a13_17": "推倒涅槃城。",
    "X70n1374_001:0033a14_18": "而踏翻虎穴。",
    "X70n1374_001:0033a14_19": "可謂來而無蹤。",
    "X70n1374_001:0033a14_20": "如月行空。",
    "X70n1374_001:0033a14_21": "去而無物。",
    "X70n1374_001:0033a15_22": "如水赴壑。",
    "X70n1374_001:0033a15_23": "瞥爾便行。",
    "X70n1374_001:0033a15_24": "萬壑風生。",
    "X70n1374_001:0033a15_25": "虗空突兀。",
    "X70n1374_001:0033a15_26": "面目堂堂。",
    "X70n1374_001:0033a16_27": "雖然此猶是老和尚。",
    "X70n1374_001:0033a16_28": "臨行時。",
    "X70n1374_001:0033a16_29": "全提底句。",
    "X70n1374_001:0033a16_30": "已為注破。",
    "X70n1374_001:0033a17_31": "只如末後句。",
    "X70n1374_001:0033a17_32": "汝諸人。",
    "X70n1374_001:0033a17_33": "祇今還聞舉似麼。",
    "X70n1374_001:0033a17_34": "以火把打圓相云。",
    "X70n1374_001:0033a18_35": "石從空裏立。",
    "X70n1374_001:0033a18_36": "火向水中焚。",
    "X70n1374_001:0033a20_0": "語錄(終)"
}