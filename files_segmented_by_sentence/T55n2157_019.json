[
    {
        "source_sentence": "貞元新定釋教目錄卷第十九",
        "from": "T55n2157_019:0903c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西京西明寺三藏沙門圓照撰",
        "from": "T55n2157_019:0903c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總集群經錄上之十九",
        "from": "T55n2157_019:0903c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敘列古今諸家目錄之二敘列古今諸家目錄之二大唐開元釋教錄二十卷　庚午歲西崇福寺沙門智昇撰右此中所撰總分上下兩錄具件如左。",
        "from": "T55n2157_019:0903c25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總集群經錄上從漢至唐所出經教區別人代具顯此中目錄始終續於後列。",
        "from": "T55n2157_019:0903c26_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後漢傳譯緇素一十二人。",
        "from": "T55n2157_019:0903c27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經并新舊集失譯諸經。",
        "from": "T55n2157_019:0903c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總二百九十二部合三百九十五卷九十七部一百三十一卷見在一百五十五部二百六十卷闕本。",
        "from": "T55n2157_019:0903c29_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曹魏傳譯沙門五人。",
        "from": "T55n2157_019:0904a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經戒羯磨。",
        "from": "T55n2157_019:0904a01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一十二部合一十八卷四部五卷見在八部一十三卷闕本。",
        "from": "T55n2157_019:0904a02_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第一卷。",
        "from": "T55n2157_019:0904a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吳代傳譯緇素五人。",
        "from": "T55n2157_019:0904a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經等并及失譯。",
        "from": "T55n2157_019:0904a04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一百八十九部合四百一十七卷六十一部九十二卷見在一百二十八部三百二十五卷𨵗本。",
        "from": "T55n2157_019:0904a06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西晉傳譯緇素一十二人。",
        "from": "T55n2157_019:0904a07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經戒等并新舊集失譯諸經。",
        "from": "T55n2157_019:0904a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總三百三十三部合五百九十一卷一百五十六部二百三十一卷見在一百七十七部三百六十九卷本闕。",
        "from": "T55n2157_019:0904a09_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第二卷。",
        "from": "T55n2157_019:0904a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東晉傳譯緇素一十六人。",
        "from": "T55n2157_019:0904a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論并新舊集失譯諸經。",
        "from": "T55n2157_019:0904a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一百六十八部合四百六十八卷八十五部三百三十六卷見在八十三部三百三十二卷本闕。",
        "from": "T55n2157_019:0904a13_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "符秦傳譯沙門六人。",
        "from": "T55n2157_019:0904a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論等。",
        "from": "T55n2157_019:0904a14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一十五部合一百九十七卷七部六十五卷見在八部一百三十二卷本闕。",
        "from": "T55n2157_019:0904a15_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第三卷。",
        "from": "T55n2157_019:0904a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姚秦傳譯沙門五人。",
        "from": "T55n2157_019:0904a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論等。",
        "from": "T55n2157_019:0904a17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總九十四部合六百二十四卷六十六部五百二十八卷見在二十八部九十六卷𨵗本。",
        "from": "T55n2157_019:0904a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乞伏秦傳譯沙門一人所出經并三秦代新舊失譯經律論等。",
        "from": "T55n2157_019:0904a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總五十六部合一百一十卷三十二部七十九卷見在二十四部三十一卷闕本。",
        "from": "T55n2157_019:0904a22_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前涼傳譯外國優婆塞一人。",
        "from": "T55n2157_019:0904a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經。",
        "from": "T55n2157_019:0904a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總四部合六卷一部一卷見在三部五卷本𨵗。",
        "from": "T55n2157_019:0904a24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北涼傳譯緇素九人。",
        "from": "T55n2157_019:0904a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論等并新舊集失譯諸經。",
        "from": "T55n2157_019:0904a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總八十二部合三百一十一卷二十五部二百九卷見在五十七部一百二卷本闕。",
        "from": "T55n2157_019:0904a27_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第四卷。",
        "from": "T55n2157_019:0904a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宋代傳譯緇素二十二人。",
        "from": "T55n2157_019:0904a29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論等并新集失譯諸經。",
        "from": "T55n2157_019:0904b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總四百六十五部合七百一十七卷九十三部二百四十三卷見在三百七十二部四百七十四卷本𨵗。",
        "from": "T55n2157_019:0904b02_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第五卷。",
        "from": "T55n2157_019:0904b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蕭齊傳譯沙門七人。",
        "from": "T55n2157_019:0904b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律。",
        "from": "T55n2157_019:0904b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一十二部合三十三卷七部二十三卷見在五部五卷本𨵗。",
        "from": "T55n2157_019:0904b05_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梁代傳譯緇素八人。",
        "from": "T55n2157_019:0904b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經論及諸傳記。",
        "from": "T55n2157_019:0904b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并新集失譯經律集等。",
        "from": "T55n2157_019:0904b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總四十六部。",
        "from": "T55n2157_019:0904b07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合二百一十七卷四十部一百九十一卷見在六部十卷本𨵗。",
        "from": "T55n2157_019:0904b08_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元魏傳譯緇素一十二人。",
        "from": "T55n2157_019:0904b09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經論。",
        "from": "T55n2157_019:0904b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總八十三部合二百七十四卷七十三部二百五十五卷見在一十部一十九卷本𨵗。",
        "from": "T55n2157_019:0904b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高齊傳譯緇素二人。",
        "from": "T55n2157_019:0904b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經論。",
        "from": "T55n2157_019:0904b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總八部合五十二卷並在無闕。",
        "from": "T55n2157_019:0904b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第六卷。",
        "from": "T55n2157_019:0904b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周朝傳譯沙門四人。",
        "from": "T55n2157_019:0904b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經論等。",
        "from": "T55n2157_019:0904b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一十四部合二十九卷六部一十一卷見在八部一十八卷本闕。",
        "from": "T55n2157_019:0904b16_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陳代傳譯緇素三人。",
        "from": "T55n2157_019:0904b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論及集傳等。",
        "from": "T55n2157_019:0904b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總四十部合一百三十三卷二十六部八十九卷見在一十四部四十四卷本闕。",
        "from": "T55n2157_019:0904b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隋朝傳譯緇素九人。",
        "from": "T55n2157_019:0904b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經論傳錄等。",
        "from": "T55n2157_019:0904b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總九十九部合三百一卷六十二部二百八十七卷見在二部一十四卷本闕。",
        "from": "T55n2157_019:0904b21_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第七卷。",
        "from": "T55n2157_019:0904b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皇朝傳譯緇素已有三十七人。",
        "from": "T55n2157_019:0904b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論及傳錄等。",
        "from": "T55n2157_019:0904b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總三百一部合二千一百七十卷二百八十一部二千一百四十三卷見在二十部二十七卷訪本未獲。",
        "from": "T55n2157_019:0904b25_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "都計一十九代傳譯道俗等。",
        "from": "T55n2157_019:0904b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一百七十六人。",
        "from": "T55n2157_019:0904b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出大小乘經律論及賢聖集傳。",
        "from": "T55n2157_019:0904b27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二千二百七十八部合七千四十六卷一千一百三十部五千四十八卷見在一千一百四十八部一千九百九十八卷本闕。",
        "from": "T55n2157_019:0904b29_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第八第九卷。",
        "from": "T55n2157_019:0904c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合從古錄至開元釋教新錄及續補舊闕。",
        "from": "T55n2157_019:0904c02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總有四十一家具如前列。",
        "from": "T55n2157_019:0904c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第十卷。",
        "from": "T55n2157_019:0904c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別分乘藏錄下上錄所辦總顯出經而大小未分三藏混雜此錄之內具件科條闕本等經續附於後。",
        "from": "T55n2157_019:0904c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從別錄中曲分為七。",
        "from": "T55n2157_019:0904c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有譯有本錄第一一千一百二十四部五千四十八卷。",
        "from": "T55n2157_019:0904c08_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復就此錄更開為三。",
        "from": "T55n2157_019:0904c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩三藏錄第一六百八十六部二千七百四十五卷菩薩契經藏五百七十三部二千一百七十三卷大乘經單重合譯總四百三十二部一千八百八十卷般若經新舊譯二十一部七百三十六卷寶積經新舊譯八十二部一百六十九卷大集經新舊譯三十四部一百四十二卷花嚴經新舊譯二十六部一百八十七卷涅槃經新舊譯六部五十八卷五大部外諸重譯經二百七十三部五百八十八卷大乘經單譯總一百三十一部二百九十三卷菩薩調伏藏二十六部五十四卷菩薩對法藏九十七部五百一十八卷大乘釋經論二十一部一百五十五卷大乘集義論七十六部三百六十三卷右為第十一十二卷。",
        "from": "T55n2157_019:0904c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聲聞三藏錄第二三百三十部一千七百六十二卷聲聞契經藏二百四十部六百一十八卷小乘經單重合譯總一百五十三部三百九十四卷根本四阿含經五部二百三卷長阿含中別譯經二十二卷中阿含中別譯經六十卷增壹阿含中別譯經五十一卷雜阿含中別譯經五十卷別譯雜阿含經二十卷四含外諸重譯經一百四十八部一百九十一卷小乘經單譯總八十七部二百二十四卷聲聞調伏藏五十四部四百四十六卷正調伏藏四十一部三百五十八卷調伏藏眷屬一十三部八十八卷聲聞對法藏三十六部六百九十八卷有部根本身足論八部一百三十一卷有部及餘支派論二十八部五百六十七卷聖賢傳記錄第三一百八部五百四十一卷梵本翻譯集傳六十八部一百七十三卷此方撰述集傳四十部三百六十八卷右為第十三卷。",
        "from": "T55n2157_019:0905a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有譯無本錄第二一千一百四十八部一千九百八十卷大乘經闕本四百八部八百一卷大乘經重譯闕本總二百部四百八十四卷般若部中闕本一十部二十七卷寶積部中闕本三十部五十四卷大集部中闕本一十三部九十七卷花嚴部中闕本一十二部二十七卷涅槃部中闕本四部二十六卷諸重譯經闕本一百三十一部二百五十三卷大乘經單譯闕本總二百八部三百一十七卷大乘律闕本二十二部二十五卷大乘論闕本二十部四十八卷大乘釋經論闕本四部一十一卷大乘集義論闕本一十六部三十七卷右為第十四卷。",
        "from": "T55n2157_019:0905b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小乘經闕本六百五部八百一十五卷小乘重譯經闕本總一百二十五部二百六十五卷根本四阿含闕本二部一百九卷長阿含部分闕本一十四部二十八卷中阿含部分闕本一十四部一十四卷增一阿含部分闕本七部八卷雜阿含部分闕本四十五部四十五卷諸重譯經闕本四十三部六十二卷小乘經單譯闕本總四百八十部五百五十卷小乘律闕本三十七部四十二卷賢聖集傳闕本四十七部一百八十四卷右為第十五卷。",
        "from": "T55n2157_019:0905b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "支派別行錄第三六百八十二部八百一十二卷大乘別生經二百九部三百二十八卷般若部中別生一十部一百一十九卷寶積部中別生一十三部一十五卷大集部中別生五十一部五十八卷花嚴部中別生一十部一十一卷諸大乘經別生二百二十五部一百二十五卷大乘律別生七部七卷大乘論別生七部一十一卷小乘別生經二百八十三部二百八十三卷長阿含部分別生一十四部一十四卷中阿含部分別生二十四部二十四卷增一阿含部分別生二十六部二十六卷雜阿含部分別生一百三十六部一百三十六卷諸小乘經別生八十三部八十三卷小乘律別生小乘論無別生四十二部四十九卷賢聖集傳別生一百三十四部一百三十四卷右為第十六卷。",
        "from": "T55n2157_019:0905c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刪略繁重錄第四一百四十七部四百八卷新撿出別生經六十七部一百八十五卷新撿出名異文同經二十部五十二卷新撿出重上錄經八部三十卷補闕拾遺錄第五三百六部一千一百一十一卷大乘經舊譯五十九部六十九卷大乘律舊譯二部二卷大乘論舊譯三部四卷小乘經舊譯五十九部六十八卷小乘律舊譯小乘舊論無遺五部五卷賢聖集傳舊譯三十六部五十七卷右已上一百六十部二百五卷。",
        "from": "T55n2157_019:0906a01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並是舊譯今見有本。",
        "from": "T55n2157_019:0906a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大周入藏兼拾遺編入。",
        "from": "T55n2157_019:0906a02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "新譯大乘經六十一部三百三十六卷新譯大乘律一部一卷新譯大乘論一十二部一十九卷新譯小乘經八部八卷新譯小乘律小乘論無新譯一十一部一百五十九卷新譯賢聖集傳三部五卷右已上九十六部五百二十八卷並是大周刊定錄後。",
        "from": "T55n2157_019:0906a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "新譯所以前錄未載。",
        "from": "T55n2157_019:0906a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今補闕編。",
        "from": "T55n2157_019:0906a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又小乘律戒羯磨六部一十卷然並撰述有據時代盛行補闕編入。",
        "from": "T55n2157_019:0906a12_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此方所撰集傳四十部三百六十八卷然皆補助正教故然補闕編入目錄。",
        "from": "T55n2157_019:0906a13_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第十七卷。",
        "from": "T55n2157_019:0906a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疑惑再詳錄第六一十四部一十九卷偽妄亂真錄第七三百九十四部或九十四一千五十七卷或五十五卷未詳開元釋教錄新編偽經三十九部或三十七五十六卷或五十四未詳苻秦釋道安錄中偽經二十五部二十八卷梁釋僧祐錄中偽經一十四部一十六卷簫齊釋道備偽撰經見長房錄五部五卷簫齊僧法尼誦出經僧祐等錄並編入偽二十一部三十五卷元魏孫敬夢授經見內典錄一部一卷梁沙門妙光偽造經見僧祐錄一部一卷隋開皇眾經錄中偽經八十六部一百四十一卷隋仁壽眾經錄中偽經一十一部四十一卷大唐內典錄中偽經二十二部八十七卷大周刊定錄中偽經八十部或二十未詳一百一卷隋沙門信行三階集偽錄周錄雖載收之不盡三十五部　四十四卷諸雜抄經增減聖說五十四部　五百一卷有為第十八卷。",
        "from": "T55n2157_019:0906b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大乘入藏錄上大乘經律論總六百三十八部二千七百四十五卷　二百五十八帙此直述注名及摽紙數餘如廣錄大乘經五百六十三部　二千一百七十三卷　二百三帙大乘律二十六部　五十四卷　五帙大乘論九十七部　五百一十八卷　五十帙右為第十九卷。",
        "from": "T55n2157_019:0906b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小乘入藏錄下小乘經律論總三百三十一部　一千七百六十二卷　一百六十五帙集傳錄附此直述經名及摽紙數餘如廣錄小乘經二百四十部　六百一十八卷　四十八帙小乘律五十四部　四百四十六卷　四十五帙小乘論三十六部　六百九十八卷　七十二帙賢聖集一百八部　五百四十一卷　五十七帙都計大小乘經律論及賢聖傳見入藏者。",
        "from": "T55n2157_019:0906b17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一千七十六部　合五千四十八卷　四百八十帙此入藏中大乘經部數與前廣錄部數不同者以前大寶積經諸部合成故在本數為四十九部上錄此中合為一部故欠四十八不同。",
        "from": "T55n2157_019:0906b20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第二十卷。",
        "from": "T55n2157_019:0906b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庚辰歲西明寺翻經臨壇賜紫沙門圓照等奉勅撰。",
        "from": "T55n2157_019:0906b22_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貞元新定釋教目錄三十卷右此中所撰准舊。",
        "from": "T55n2157_019:0906b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦總分上下兩錄。",
        "from": "T55n2157_019:0906b24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具件如左。",
        "from": "T55n2157_019:0906b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總集群經錄上。",
        "from": "T55n2157_019:0906b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中分二。",
        "from": "T55n2157_019:0906b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一特奉制旨。",
        "from": "T55n2157_019:0906b27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二總集群經。",
        "from": "T55n2157_019:0906c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後漢傳譯緇素一十二人。",
        "from": "T55n2157_019:0906c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律并新舊集失譯諸經。",
        "from": "T55n2157_019:0906c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總二百九十二部合三百九十五卷九十七部一百三十一卷見在一百九十五部二百六十卷本闕。",
        "from": "T55n2157_019:0906c04_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曹魏傳譯沙門五人。",
        "from": "T55n2157_019:0906c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經戒羯磨。",
        "from": "T55n2157_019:0906c05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一十二部合一十八卷四部五卷見在八部一十三卷本闕。",
        "from": "T55n2157_019:0906c06_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吳代傳譯緇素五人。",
        "from": "T55n2157_019:0906c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經等并及失譯。",
        "from": "T55n2157_019:0906c07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一百八十九部合四百一十七卷六十一部九十二卷見在一百二十八部三百二十五卷本闕。",
        "from": "T55n2157_019:0906c09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西晉傳譯緇素一十二人。",
        "from": "T55n2157_019:0906c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經戒等并新舊集失譯諸經。",
        "from": "T55n2157_019:0906c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總三百三十三部合五百九十一卷一百五十六部三百三十一卷見在一百七十七部二百六十九卷本闕。",
        "from": "T55n2157_019:0906c12_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第一第二第三第四卷。",
        "from": "T55n2157_019:0906c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東晉傳譯緇素一十六人。",
        "from": "T55n2157_019:0906c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論并新舊集失譯經一百六十八部。",
        "from": "T55n2157_019:0906c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合四百六十八卷八十五部三百三十六卷見在八十三部三百三十二卷本闕。",
        "from": "T55n2157_019:0906c16_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苻秦傳譯沙門六人。",
        "from": "T55n2157_019:0906c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論等。",
        "from": "T55n2157_019:0906c17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一十五部。",
        "from": "T55n2157_019:0906c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合一百九十七卷七部六十五卷見在八部一百三十二卷闕本。",
        "from": "T55n2157_019:0906c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第五卷。",
        "from": "T55n2157_019:0906c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姚秦傳譯沙門五人。",
        "from": "T55n2157_019:0906c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論等。",
        "from": "T55n2157_019:0906c21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總九十四部。",
        "from": "T55n2157_019:0906c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合六百二十四卷六十六部五百二十八卷見在二十八部九十六卷本闕。",
        "from": "T55n2157_019:0906c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乞伏秦傳譯沙門一人。",
        "from": "T55n2157_019:0906c24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經并三秦代新舊失譯經律論等。",
        "from": "T55n2157_019:0906c25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總五十六部。",
        "from": "T55n2157_019:0906c25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合一百一十卷三十二部七十九卷見在二十四部三十一卷本闕。",
        "from": "T55n2157_019:0906c26_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前涼傳譯外國優婆塞一人。",
        "from": "T55n2157_019:0906c27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經總四部合六卷一部一卷見在三部五卷本闕。",
        "from": "T55n2157_019:0906c28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北涼傳譯緇素九人。",
        "from": "T55n2157_019:0906c29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論等并新舊集失譯諸經。",
        "from": "T55n2157_019:0907a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總八十二部。",
        "from": "T55n2157_019:0907a01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合三百一十一卷一十五部二百九卷見在五十七部一百二卷本闕。",
        "from": "T55n2157_019:0907a02_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第六卷。",
        "from": "T55n2157_019:0907a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宋代傳譯緇素二十二人。",
        "from": "T55n2157_019:0907a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論等并集失譯諸經。",
        "from": "T55n2157_019:0907a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總四百六十五部。",
        "from": "T55n2157_019:0907a05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合七百一十七卷九十三部二百四十三卷見在三百七十三部四百七十四卷本闕。",
        "from": "T55n2157_019:0907a06_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "簫齊傳譯沙門七人。",
        "from": "T55n2157_019:0907a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律。",
        "from": "T55n2157_019:0907a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一十二部。",
        "from": "T55n2157_019:0907a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合三十三卷七部二十八卷見在五卷本闕。",
        "from": "T55n2157_019:0907a08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第七第八卷。",
        "from": "T55n2157_019:0907a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梁代傳譯緇素九人。",
        "from": "T55n2157_019:0907a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經論。",
        "from": "T55n2157_019:0907a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸傳記并新集失譯經律集等。",
        "from": "T55n2157_019:0907a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總四十七部。",
        "from": "T55n2157_019:0907a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合二百一十七卷四十一部二百七卷見在六部十卷本闕。",
        "from": "T55n2157_019:0907a12_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元魏傳譯緇素一十二人。",
        "from": "T55n2157_019:0907a13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經論總八十三部。",
        "from": "T55n2157_019:0907a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合二百七十四卷七十三部二百五十五卷見在一十部一十九卷本闕。",
        "from": "T55n2157_019:0907a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高齊傳譯緇素二人。",
        "from": "T55n2157_019:0907a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經論。",
        "from": "T55n2157_019:0907a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總八部合五十二卷並在無闕。",
        "from": "T55n2157_019:0907a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第九卷。",
        "from": "T55n2157_019:0907a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周朝傳譯沙門四人。",
        "from": "T55n2157_019:0907a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經論等。",
        "from": "T55n2157_019:0907a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一十四部。",
        "from": "T55n2157_019:0907a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合二十九卷六部一十一卷見在八部一十八卷本闕。",
        "from": "T55n2157_019:0907a20_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陳代傳譯緇素三人。",
        "from": "T55n2157_019:0907a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論及集傳等。",
        "from": "T55n2157_019:0907a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總四十部。",
        "from": "T55n2157_019:0907a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合一百三十三卷二十六部八十九卷見在一十四部四十四卷本闕。",
        "from": "T55n2157_019:0907a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隋朝傳譯緇素十人。",
        "from": "T55n2157_019:0907a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經論傳集錄等。",
        "from": "T55n2157_019:0907a24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總九十九部。",
        "from": "T55n2157_019:0907a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合三百四十五卷九十七部三百三十一卷見在二部一十四卷本闕。",
        "from": "T55n2157_019:0907a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第十卷。",
        "from": "T55n2157_019:0907a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皇朝傳譯緇素已有四十六人。",
        "from": "T55n2157_019:0907a28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論及傳錄等。",
        "from": "T55n2157_019:0907a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總四百三十四部。",
        "from": "T55n2157_019:0907a29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合二千四百六十三卷四百十部二千三百八十六卷見在二十七部七十七卷訪本未獲足。",
        "from": "T55n2157_019:0907b01_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "都計一十九代傳譯道俗。",
        "from": "T55n2157_019:0907b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一百八十七人。",
        "from": "T55n2157_019:0907b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出大小乘經律論及賢聖集傳。",
        "from": "T55n2157_019:0907b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總二千四百四十七部。",
        "from": "T55n2157_019:0907b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合七千三百七十九卷一千二百五十七部五千三百七十八卷見在一千一百五十五部二千四十八卷本𨵗內五十遺編律文七部五十卷闕三十卷故此重摽五十卷。",
        "from": "T55n2157_019:0907b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第十一十二十三十四十五十六十七卷。",
        "from": "T55n2157_019:0907b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合從古錄至貞元新定釋教目錄及。",
        "from": "T55n2157_019:0907b08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "續補舊闕。",
        "from": "T55n2157_019:0907b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總有四十二家。",
        "from": "T55n2157_019:0907b09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具如前列。",
        "from": "T55n2157_019:0907b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第十八十九卷。",
        "from": "T55n2157_019:0907b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別分乘藏錄下上餘所辯總顯出經而大小未分三藏混雜此錄之內具件科條𨵗本等經續附於後。",
        "from": "T55n2157_019:0907b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中分二。",
        "from": "T55n2157_019:0907b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初別分乘藏。",
        "from": "T55n2157_019:0907b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二賢傳記。",
        "from": "T55n2157_019:0907b13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就別錄曲分第七。",
        "from": "T55n2157_019:0907b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有譯有本錄第一一千二百五十六部五千三百七十八卷。",
        "from": "T55n2157_019:0907b15_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復就此錄更開為二。",
        "from": "T55n2157_019:0907b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一三藏差殊。",
        "from": "T55n2157_019:0907b16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二聖賢傳記。",
        "from": "T55n2157_019:0907b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏復二。",
        "from": "T55n2157_019:0907b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一菩薩三藏。",
        "from": "T55n2157_019:0907b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二聲聞三藏。",
        "from": "T55n2157_019:0907b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩三藏錄第一八百七部二千九百七十八卷菩薩契經藏六百八十一部二千四百三卷大乘經單重合譯五百五十四部二千一百二卷般若經新舊譯三十一部七百五十七卷寶積經新舊譯八十三部一百七十二卷大集經新舊譯三十七部一百五十二卷華嚴經新舊譯四十部二百四十七卷涅槃經舊譯六部五十八卷五大部外諸重譯經三百一十八部六百四十五卷大乘經單譯總一百七十九部三百九十七卷菩薩調伏藏二十七部五十五卷菩薩對法藏九十九部五百二十卷大乘釋經論二十一部一百五十五卷大乘集義論七十八部三百六十五卷右為第二十卷二十一卷二十二卷。",
        "from": "T55n2157_019:0907c06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聲聞三藏錄第二三百三十八部一千八百一十二卷聲聞契經藏二百四十部六百一十八卷小乘經單重合譯總一百五十三部三百九十四卷根本四阿含經五部二百三卷長阿含中別譯經二十二卷中阿含中別譯經六十卷增壹阿含中別譯經五十一卷雜阿含中別譯經五十卷別譯雜阿含經二十卷四阿含外諸重譯經一百四十八部一百九十一卷小乘經單譯總八十七部二百二十四卷聲聞調伏藏六十一部四百九十三卷正調伏藏四十八部四百五卷內欠三卷未獲本調伏藏眷屬一十三部八十八卷聲聞對法藏三十六部六百九十八卷有部根本身足論八部一百三十一卷有部及餘支派論二十八部五百六十七卷一聖賢傳記錄第三一百一十二部五百九十一卷就此錄中復分為二。",
        "from": "T55n2157_019:0908a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一梵本翻譯集傳六十八部一百七十三卷二此方撰述集傳四十四部四百一十八卷右為第二十三卷。",
        "from": "T55n2157_019:0908a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有譯無本錄第二一千一百四十部一千九百八十卷大乘經闕本四百八部八百一卷大乘重譯闕本總二百部四百八十四卷般若部中闕本一十部二十七卷寶積部中闕本十三部五十四卷大集部中闕本一十三部九十七卷華嚴部中闕本一十二部二十七卷涅槃部中闕本四部二十六卷諸重譯經闕本一百三十一部二百五十三卷大乘經單譯闕本總二百八部三百一十七卷大乘律闕本二十二部二十五卷大乘論闕本二十部四十八卷大乘釋經論闕本四部一十一卷大乘集義論闕本一十六部三十七卷右為第二十四卷。",
        "from": "T55n2157_019:0908a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小乘經闕本六百五部八百一十五卷小乘經重譯闕本總二百二十五部二百六十五卷根本四阿含闕本二部一百九卷長阿含部分闕本一十四部二十八卷中阿含部分闕本一十四部一十四卷增一阿含部分闕本七部八卷雜阿含部分闕本四十五部四十五卷諸重譯經闕本四十三部六十二卷小乘經單譯闕本總四百八十部五百五十卷小乘律闕本四十四部九十二卷小乘論闕本九部六十五卷賢聖集傳闕本四十七部一百八十四卷右為第二十五卷。",
        "from": "T55n2157_019:0908b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "支派別行錄第三六百八十二部八百一十二卷大乘別生經二百九部三百二十八卷般若部中別生一十部一百一十九卷寶積部中別生一十二部一十五卷大集部中別生五十一部五十八卷華嚴部中別生一十部一十一卷諸大乘經別生一百二十五部一百二十五卷大乘律別生七部七卷大乘論別生七部一十一卷小乘別生經二百八十三部二百八十三卷長阿含部分別生一十四部一十四卷中阿含部分別生二十四部二十四卷增一阿含部分別生二十六部二十六卷雜阿含部分別生一百三十部一百三十六卷諸小乘經別生八十三部八十三卷小乘律別生小乘論無別生四十二部四十九卷賢聖集傳別生一百三十四部一百三十四卷右為第二十六卷。",
        "from": "T55n2157_019:0908c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刪略繁重錄第四一百四十七部四百八卷新撿出別生經六十七部一百八十五卷新撿出名異文同經三十部五十二卷新撿出重上錄經八部三十卷新撿出合入大部經五十二部一百四十一卷補闕拾遺錄第五三百六部一千一百一十一卷大乘經舊譯五十九部六十九卷大乘律舊譯二部二卷大乘論舊譯三部四卷小乘經舊譯五十九部六十八卷小乘律舊譯小乘舊論無遺五部五卷賢聖集論舊譯三十六部五十七卷右已上一百六十四部二百五卷並是舊譯今見有本。",
        "from": "T55n2157_019:0908c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大周入藏中無。",
        "from": "T55n2157_019:0908c17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今拾遺編入。",
        "from": "T55n2157_019:0908c17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "新譯大乘經六十一部三百三十六卷新譯大乘律一部一卷新譯大乘論一十二部一十九卷新譯小乘經八部八卷新譯小乘律小乘論無新譯一十一部一百五十九卷新譯賢聖集傳三部五卷右已上九十六部五百二十八卷並是大周刊定錄後新譯所以前錄未載。",
        "from": "T55n2157_019:0908c25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今補闕編入。",
        "from": "T55n2157_019:0908c25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又小乘律戒羯磨六部一十卷然並撰述有據時代盛行補闕編入。",
        "from": "T55n2157_019:0909a01_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此方所撰集傳四十部三百六十八卷然皆補助正教並補𨵗編入見錄。",
        "from": "T55n2157_019:0909a02_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此方撰集經一部一十六卷或十八卷。",
        "from": "T55n2157_019:0909a03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右撿舊錄。",
        "from": "T55n2157_019:0909a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在偽妄亂真錄中。",
        "from": "T55n2157_019:0909a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今准勅編入貞元新定釋教目錄此一朝傳出梁朝今准入錄年代唐朝年月日編在梁末。",
        "from": "T55n2157_019:0909a05_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此方法琳別傳三卷。",
        "from": "T55n2157_019:0909a06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右開元錄。",
        "from": "T55n2157_019:0909a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中禁斷。",
        "from": "T55n2157_019:0909a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今再詳准勅編入貞元釋教錄。",
        "from": "T55n2157_019:0909a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此方續開元釋教錄一部三卷。",
        "from": "T55n2157_019:0909a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准勅編入貞元釋教錄。",
        "from": "T55n2157_019:0909a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此方撰述目錄一部三十卷准例編入貞元新定釋教目錄。",
        "from": "T55n2157_019:0909a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第二十七卷。",
        "from": "T55n2157_019:0909a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疑惑再詳錄第六一十四部一十九卷偽妄亂真錄第七三百九十四部一千四十一卷開元釋教錄新編偽經三十九部五十六卷符秦釋道安錄中偽經三十五部二十八卷梁釋僧祐錄中偽經一十四部一十六卷蕭齊釋道備偽撰經見長房錄五卷元魏孫敬德夢授經見內典錄一部一卷梁沙門妙光偽造經見僧祐錄一部一卷隋開皇眾經中偽經八十六部一百四十一卷隋仁壽眾經錄中偽經一十一部四十一卷大唐內典錄中偽經二十二部八十七卷大周刊定錄中偽經八十部一百一卷隋沙門信行三階集錄周錄雖載收之不盡三十五部　四十四卷諸雜抄經增減聖說五十四部　五百一卷右為第二十八卷。",
        "from": "T55n2157_019:0909b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大乘入藏錄上大乘經律論總七百六十三部　二千九百五十八卷　二百六十七帙此直列經名及標紙數餘如廣錄大乘經六百八十六部　二千七百四十五卷　二百二十帙大乘律二十七部　五十五卷　五帙大乘論九十九部　五百二十卷　五十帙右為第二十九卷。",
        "from": "T55n2157_019:0909b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小乘藏入藏錄下小乘經律論總四百四十七部　二千三百八十九卷　一百七十帙傳錄附此未此直述經名及標紙數餘如廣錄小乘經二百四十部　六百一十八卷　四十八帙小乘律六十一部　四百九十三卷　五十帙小乘論三十六部　六百九十八卷　七十帙賢聖集一百一十二部　五百九十一卷　六十四帙都大小乘經律論及賢聖傳見入藏者。",
        "from": "T55n2157_019:0909b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一千二百五十六部。",
        "from": "T55n2157_019:0909b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合五千三百七十八卷。",
        "from": "T55n2157_019:0909b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五百九帙。",
        "from": "T55n2157_019:0909b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此入藏中大乘經部數與前廣錄數不同者。",
        "from": "T55n2157_019:0909b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前以大寶積經諸部合成。",
        "from": "T55n2157_019:0909b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故存本數為四十九部上錄。",
        "from": "T55n2157_019:0909b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中合為一部。",
        "from": "T55n2157_019:0909b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故欠四十八。",
        "from": "T55n2157_019:0909b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不同。",
        "from": "T55n2157_019:0909b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第三十卷。",
        "from": "T55n2157_019:0909b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貞元新定釋教目錄卷第十九貞元十五年十月二十三日奉勅修撰。",
        "from": "T55n2157_019:0909b23_15",
        "context_with_next_sentence": true
    },
