[
    {
        "source_sentence": "佛說三厨經",
        "from": "T85n2894_001:1413b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西國婆羅門達多羅及",
        "from": "T85n2894_001:1413b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闍那崛多等奉詔譯",
        "from": "T85n2894_001:1413b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一慈悲自覺自然厨。",
        "from": "T85n2894_001:1413b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二辟支四果聲聞無思厨。",
        "from": "T85n2894_001:1413b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三非有無非神自然厨。",
        "from": "T85n2894_001:1413b08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此三厨法若能誦之。",
        "from": "T85n2894_001:1413b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令人不飢。",
        "from": "T85n2894_001:1413b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "湛然長生。",
        "from": "T85n2894_001:1413b09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先當仰信三寶。",
        "from": "T85n2894_001:1413b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恭敬三寶。",
        "from": "T85n2894_001:1413b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒須禮拜三寶。",
        "from": "T85n2894_001:1413b10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晨夜殷勤常念不絕。",
        "from": "T85n2894_001:1413b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孝養師僧父母親族。",
        "from": "T85n2894_001:1413b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能學六波羅蜜。",
        "from": "T85n2894_001:1413b11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不惜身命。",
        "from": "T85n2894_001:1413b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後得傳此三厨法。",
        "from": "T85n2894_001:1413b12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃是三世諸佛菩薩自然厨。",
        "from": "T85n2894_001:1413b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常住湛然不動累劫慈悲厨。",
        "from": "T85n2894_001:1413b13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此三厨法佛垂涅槃時。",
        "from": "T85n2894_001:1413b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對十方一切諸佛菩薩前八部大眾中。",
        "from": "T85n2894_001:1413b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此過現未來三世諸佛常住湛然不動累劫慈悲天厨法。",
        "from": "T85n2894_001:1413b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而說偈言。",
        "from": "T85n2894_001:1413b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一氣和太和　　得一道皆太　　和乃元一和",
        "from": "T85n2894_001:1413b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玄理同玄際此是長生木偈。",
        "from": "T85n2894_001:1413b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦九十遍。",
        "from": "T85n2894_001:1413b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不以意思意　　亦不求無思　　意而不復思",
        "from": "T85n2894_001:1413b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是事如是持此是不熱火偈。",
        "from": "T85n2894_001:1413b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦三十遍。",
        "from": "T85n2894_001:1413b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸食氣結氣　　非諸久定結　　氣歸諸本氣",
        "from": "T85n2894_001:1413b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨聚當隨泄此是中央不飢土偈。",
        "from": "T85n2894_001:1413b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦一百二十遍。",
        "from": "T85n2894_001:1413b28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修理志理志　　積修不扶埋　　志而不修志",
        "from": "T85n2894_001:1413c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已業無已智此是不寒金偈。",
        "from": "T85n2894_001:1413c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦七十遍。",
        "from": "T85n2894_001:1413c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫將心緣心　　還莫住絕緣　　心在莫存心",
        "from": "T85n2894_001:1413c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真則守真淵此是不渴水偈。",
        "from": "T85n2894_001:1413c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦五十遍。",
        "from": "T85n2894_001:1413c07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此慈悲厨。",
        "from": "T85n2894_001:1413c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛垂涅槃時。",
        "from": "T85n2894_001:1413c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對十方一切諸佛八部大眾中。",
        "from": "T85n2894_001:1413c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此過現未來三世諸佛菩薩自然三厨常住。",
        "from": "T85n2894_001:1413c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不飢不渴。",
        "from": "T85n2894_001:1413c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不寒不熱。",
        "from": "T85n2894_001:1413c10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不老不少。",
        "from": "T85n2894_001:1413c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切聖眾湛然不動自然三厨。",
        "from": "T85n2894_001:1413c11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能受持一天厨名字者。",
        "from": "T85n2894_001:1413c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或日日誦之。",
        "from": "T85n2894_001:1413c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不念飲食。",
        "from": "T85n2894_001:1413c12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或逕百日不食。",
        "from": "T85n2894_001:1413c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或逕十年百年或千年萬年。",
        "from": "T85n2894_001:1413c13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逕微塵劫念之。",
        "from": "T85n2894_001:1413c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常住不動不思飲食。",
        "from": "T85n2894_001:1413c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如阿彌陀佛無量壽國等無有異也。",
        "from": "T85n2894_001:1413c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告大眾。",
        "from": "T85n2894_001:1413c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行此法者唯須存心信重三寶。",
        "from": "T85n2894_001:1413c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能入深山學道者。",
        "from": "T85n2894_001:1413c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始可行流通此法。",
        "from": "T85n2894_001:1413c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若生疑不信誹謗之者。",
        "from": "T85n2894_001:1413c17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得輒傳。",
        "from": "T85n2894_001:1413c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恐生毀謗者墮於惡道。",
        "from": "T85n2894_001:1413c18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故不得妄傳於世。",
        "from": "T85n2894_001:1413c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦東方九十遍乃至北方五十遍。",
        "from": "T85n2894_001:1413c19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總成一遍。",
        "from": "T85n2894_001:1413c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若未飽者如上更誦。",
        "from": "T85n2894_001:1413c20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不過兩遍。",
        "from": "T85n2894_001:1413c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即飽不飢不渴。",
        "from": "T85n2894_001:1413c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦偈時皆從寅為初。",
        "from": "T85n2894_001:1413c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始至人定分作四時誦之。",
        "from": "T85n2894_001:1413c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍數多少如前所說。",
        "from": "T85n2894_001:1413c22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又初欲誦時。",
        "from": "T85n2894_001:1413c22_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宜令口動舌轉斷默誦。",
        "from": "T85n2894_001:1413c23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有津液下可咽之。",
        "from": "T85n2894_001:1413c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初一日至三日時。",
        "from": "T85n2894_001:1413c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或小便色赤。",
        "from": "T85n2894_001:1413c24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿以為怪。",
        "from": "T85n2894_001:1413c24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初誦至第三日。",
        "from": "T85n2894_001:1413c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身力極弱。",
        "from": "T85n2894_001:1413c25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過此三日即得七日。",
        "from": "T85n2894_001:1413c25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "溫七日已後身力漸強。",
        "from": "T85n2894_001:1413c26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法成不宜誦之。",
        "from": "T85n2894_001:1413c26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百日已來每夢見得世間上味悉飽之。",
        "from": "T85n2894_001:1413c27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦聞天香。",
        "from": "T85n2894_001:1413c27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿三百日功力圓備。",
        "from": "T85n2894_001:1413c28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲食即食不食不飢。",
        "from": "T85n2894_001:1413c28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說三厨法一卷其天厨名字有本具說。",
        "from": "T85n2894_001:1414a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此本五行偈。",
        "from": "T85n2894_001:1414a01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是天厨名字。",
        "from": "T85n2894_001:1414a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此法南方崛多師口決受得之。",
        "from": "T85n2894_001:1414a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一言得之土。",
        "from": "T85n2894_001:1414a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三言得之火。",
        "from": "T85n2894_001:1414a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五言得之水。",
        "from": "T85n2894_001:1414a03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七言得之金。",
        "from": "T85n2894_001:1414a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九言得之木。",
        "from": "T85n2894_001:1414a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東方木九十遍。",
        "from": "T85n2894_001:1414a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南方火誦三十遍。",
        "from": "T85n2894_001:1414a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中央土誦一百二十遍。",
        "from": "T85n2894_001:1414a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方金誦七十遍。",
        "from": "T85n2894_001:1414a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方水誦五十遍。",
        "from": "T85n2894_001:1414a06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從九十至北方五十遍。",
        "from": "T85n2894_001:1414a06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總成一遍。",
        "from": "T85n2894_001:1414a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若未飽者如上更誦。",
        "from": "T85n2894_001:1414a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不過兩遍。",
        "from": "T85n2894_001:1414a07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必飽不或凡誦從壬日起首。",
        "from": "T85n2894_001:1414a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從寅為初。",
        "from": "T85n2894_001:1414a08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至人定分此可為四時。",
        "from": "T85n2894_001:1414a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦如上法。",
        "from": "T85n2894_001:1414a09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又初欲得誦。",
        "from": "T85n2894_001:1414a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宜令齧動舌轉斷默。",
        "from": "T85n2894_001:1414a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦有津液。",
        "from": "T85n2894_001:1414a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宜咽下。",
        "from": "T85n2894_001:1414a10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如後經二三四日間一度小便。",
        "from": "T85n2894_001:1414a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿以為怪。",
        "from": "T85n2894_001:1414a11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今只是一切眾生皆稟陰陽氣以成身。",
        "from": "T85n2894_001:1414a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今既納氣天厨。",
        "from": "T85n2894_001:1414a12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以通身能為益事故爾。",
        "from": "T85n2894_001:1414a13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初誦至第三日力最漸弱。",
        "from": "T85n2894_001:1414a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過此日即得七日。",
        "from": "T85n2894_001:1414a14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已來去定成不疑。",
        "from": "T85n2894_001:1414a14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百日已來每夢見得世間上味飲食。",
        "from": "T85n2894_001:1414a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百日以後即夢見得天上飲食。",
        "from": "T85n2894_001:1414a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鼻所恒聞天食香氣。",
        "from": "T85n2894_001:1414a16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三百日竟功德圓滿。",
        "from": "T85n2894_001:1414a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得道果也。",
        "from": "T85n2894_001:1414a17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願精進斷三毒。",
        "from": "T85n2894_001:1414a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈悲眾生。",
        "from": "T85n2894_001:1414a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸敬三寶。",
        "from": "T85n2894_001:1414a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒念阿彌陀佛。",
        "from": "T85n2894_001:1414a18_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至一年必得見佛。",
        "from": "T85n2894_001:1414a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "決定無疑。",
        "from": "T85n2894_001:1414a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏法師口云。",
        "from": "T85n2894_001:1414a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲求無上菩提。",
        "from": "T85n2894_001:1414a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "專心念定者。",
        "from": "T85n2894_001:1414a20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但患食多過障修禪定。",
        "from": "T85n2894_001:1414a21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得道果。",
        "from": "T85n2894_001:1414a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先依占察經云。",
        "from": "T85n2894_001:1414a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懺悔罪已須於山林靜谷閑房靜室。",
        "from": "T85n2894_001:1414a22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先結跏趺。",
        "from": "T85n2894_001:1414a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手置右手上。",
        "from": "T85n2894_001:1414a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合兩目。",
        "from": "T85n2894_001:1414a23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令齒上下相著。",
        "from": "T85n2894_001:1414a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端坐調和。",
        "from": "T85n2894_001:1414a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已初依卯酉子午四時。",
        "from": "T85n2894_001:1414a24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨方面數攝心默誦如在齊下。",
        "from": "T85n2894_001:1414a25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得攀緣外境。",
        "from": "T85n2894_001:1414a25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從九十遍至五十遍。",
        "from": "T85n2894_001:1414a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如其未飽亦不過至兩遍即飽。",
        "from": "T85n2894_001:1414a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正誦中間時時以舌斷穴外津液咽之。",
        "from": "T85n2894_001:1414a27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其不飢渴一偈一偈於外酉子午中間常誦之。",
        "from": "T85n2894_001:1414a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食此三厨滿七日即知法成不。",
        "from": "T85n2894_001:1414a29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為不至誠者。",
        "from": "T85n2894_001:1414b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為有福者。",
        "from": "T85n2894_001:1414b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為無罪者。",
        "from": "T85n2894_001:1414b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為得禪定者。",
        "from": "T85n2894_001:1414b01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為至道果者。",
        "from": "T85n2894_001:1414b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦此偈三百六十遍即不飢。",
        "from": "T85n2894_001:1414b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊。",
        "from": "T85n2894_001:1414b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T85n2894_001:1414b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那致㝹那致沙訶　烏那致摩訶烏那致莎訶前件法受持須著淨潔衣服。",
        "from": "T85n2894_001:1414b05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在靜室內。",
        "from": "T85n2894_001:1414b05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然種種名香供養一切賢聖。",
        "from": "T85n2894_001:1414b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後誦前件法二十一遍。",
        "from": "T85n2894_001:1414b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "度一壽至須殷重。",
        "from": "T85n2894_001:1414b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七日不得與人言說。",
        "from": "T85n2894_001:1414b07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大須慎之。",
        "from": "T85n2894_001:1414b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得見女人畜生等。",
        "from": "T85n2894_001:1414b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如乾渴得飲牛乳并棗及漿。",
        "from": "T85n2894_001:1414b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如欲得知厨神名者。",
        "from": "T85n2894_001:1414b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今說之。",
        "from": "T85n2894_001:1414b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無十偈伽末四婆　南無末伽摩志那　南無支鉢羅涕死平旦向於辰。",
        "from": "T85n2894_001:1414b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地鳴天鼓。",
        "from": "T85n2894_001:1414b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二七下食玉漿。",
        "from": "T85n2894_001:1414b13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸善男子善女人欲行此三厨法者。",
        "from": "T85n2894_001:1414b14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先於齋戒至心。",
        "from": "T85n2894_001:1414b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然無價名香至心。",
        "from": "T85n2894_001:1414b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密念前件厨神名字。",
        "from": "T85n2894_001:1414b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作禮懺悔乞祐助。",
        "from": "T85n2894_001:1414b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "厨禮作四十九禮。",
        "from": "T85n2894_001:1414b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是已了了別識其形名。",
        "from": "T85n2894_001:1414b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後誦偈。",
        "from": "T85n2894_001:1414b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若直誦偈一百日。",
        "from": "T85n2894_001:1414b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一百日不飢。",
        "from": "T85n2894_001:1414b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若求出世因緣速證四果者。",
        "from": "T85n2894_001:1414b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要當受持厨神名字。",
        "from": "T85n2894_001:1414b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世人不知以口得言神名。",
        "from": "T85n2894_001:1414b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心便不敬。",
        "from": "T85n2894_001:1414b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若心慢上法。",
        "from": "T85n2894_001:1414b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法不成終無盡也。",
        "from": "T85n2894_001:1414b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四戊四己腸堅塞　　四壬四癸不用食",
        "from": "T85n2894_001:1414b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六甲六丁太倉盈　　五庚五辛五藏貞",
        "from": "T85n2894_001:1414b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仙人玉女事我神　　天官行厨供養身",
        "from": "T85n2894_001:1414b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使我顏色常充悅　　延年益壽數萬年",
        "from": "T85n2894_001:1414b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說請厨三食陀羅尼神呪。",
        "from": "T85n2894_001:1414b27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無曷羅怛那　怛囉夜㖿　豆留　豆留摩訶　豆留唵　急速訶尼攝　唵急速訶尼攝此陀羅尼別行法中。",
        "from": "T85n2894_001:1414c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行至空閑無人之處。",
        "from": "T85n2894_001:1414c02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於平旦時淨水洗鉢。",
        "from": "T85n2894_001:1414c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "楊柳枝拂其鉢中。",
        "from": "T85n2894_001:1414c03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取淨草一把。",
        "from": "T85n2894_001:1414c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以淨巾敷在草上。",
        "from": "T85n2894_001:1414c04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合其鉢盂面。",
        "from": "T85n2894_001:1414c04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向東結跏趺坐。",
        "from": "T85n2894_001:1414c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "閉目舌柱上齶。",
        "from": "T85n2894_001:1414c05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "默誦上件呪一百八遍。",
        "from": "T85n2894_001:1414c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開目視之。",
        "from": "T85n2894_001:1414c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其食即至恣意食之。",
        "from": "T85n2894_001:1414c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如有餘食。",
        "from": "T85n2894_001:1414c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦前呪七遍。",
        "from": "T85n2894_001:1414c07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦閉目心思念云。",
        "from": "T85n2894_001:1414c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘食至本方所即去。",
        "from": "T85n2894_001:1414c08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說三厨經一卷　助三厨　谷家法　其後法別就一婆羅門。",
        "from": "T85n2894_001:1414c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得將助三厨大要。",
        "from": "T85n2894_001:1414c10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀音受我法　　仙人賜我糧　　事隨五方色",
        "from": "T85n2894_001:1414c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "青黑赤白黃　　和合得　餌　　諸塵以自防",
        "from": "T85n2894_001:1414c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T85n2894_001:1414c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無佛噤　南無法噤　南無僧噤　南無娑利莎噤　莎訶誦五十遍如見如如不如如見　有非空　有非自有非他有　莎訶此呪恐有惡魔來惱人為作障難誦五十遍。",
        "from": "T85n2894_001:1414c18_17",
        "context_with_next_sentence": true
    }
]