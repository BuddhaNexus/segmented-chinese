[
    {
        "source_sentence": "佛說鬼子母經",
        "from": "T21n1262_001:0290c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "失譯人今附西晉錄",
        "from": "T21n1262_001:0290c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛遊大兜國。時國中有一母人，多子，性極惡，",
        "from": "T21n1262_001:0290c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常喜行盜人子殺噉之。",
        "from": "T21n1262_001:0290c07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亡子家亦不知何誰取者，行街里啼哭。",
        "from": "T21n1262_001:0290c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人已還共議，如是非一日。",
        "from": "T21n1262_001:0290c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難及眾沙門出行，輒見啼哭人，已還共議，傷哀亡子家。",
        "from": "T21n1262_001:0290c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛即知眾沙門議，佛到眾沙門所。",
        "from": "T21n1262_001:0290c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛問眾沙門：",
        "from": "T21n1262_001:0290c11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「向者何等議？",
        "from": "T21n1262_001:0290c11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」眾沙門、阿難白佛言：",
        "from": "T21n1262_001:0290c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「向者出行分衛，見街里有啼哭人眾多，即問啼哭人：",
        "from": "T21n1262_001:0290c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『汝何為啼哭？",
        "from": "T21n1262_001:0290c13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』報言：",
        "from": "T21n1262_001:0290c13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『生亡我子，不知屍死處所。",
        "from": "T21n1262_001:0290c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』如是啼哭者非一家，皆亡子。",
        "from": "T21n1262_001:0290c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」佛便為阿難、眾沙門說：",
        "from": "T21n1262_001:0290c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「是國中盜人子者，非凡人故，現鬼子母。",
        "from": "T21n1262_001:0290c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今生作人，喜行盜人子。",
        "from": "T21n1262_001:0290c16_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是母有千子，五百子在天上，五百子在世間。",
        "from": "T21n1262_001:0290c17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千子皆為鬼王，一王者從數萬鬼。",
        "from": "T21n1262_001:0290c18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是五百鬼王，在天上嬈諸天；",
        "from": "T21n1262_001:0290c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五百鬼王，在世間嬈帝王人民。",
        "from": "T21n1262_001:0290c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是五百鬼王，天亦無奈何。",
        "from": "T21n1262_001:0290c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」阿難白佛言：",
        "from": "T21n1262_001:0290c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「鬼子母來，在是國中。",
        "from": "T21n1262_001:0290c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今寧可勅令不盜人子耶？",
        "from": "T21n1262_001:0290c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」佛言：",
        "from": "T21n1262_001:0290c22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「大善！",
        "from": "T21n1262_001:0290c22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可令不復盜人子。",
        "from": "T21n1262_001:0290c22_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」阿難問佛言：",
        "from": "T21n1262_001:0290c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「當用何等方便使不復盜人子耶？",
        "from": "T21n1262_001:0290c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」佛便語阿難：",
        "from": "T21n1262_001:0290c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「到是母所居，眾沙門共伺是母出已，後悉歛取子來，著精舍中逃之。",
        "from": "T21n1262_001:0290c25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」眾沙門即往。",
        "from": "T21n1262_001:0290c25_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伺是母出行，隨後歛取子。",
        "from": "T21n1262_001:0290c26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得千數子，逃著精舍中。",
        "from": "T21n1262_001:0290c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是母便復行盜人子，來入舍中，不見其子，便捨他人子，不敢復殺。",
        "from": "T21n1262_001:0290c28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便行索其子，遍舍中不知其子處。",
        "from": "T21n1262_001:0290c29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便出行至街里，遍城中不得。",
        "from": "T21n1262_001:0291a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復出城外索不得，便入城行道啼哭。",
        "from": "T21n1262_001:0291a01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是十日，母便狂。",
        "from": "T21n1262_001:0291a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被髮入市，啼哭自擗，撲仰天大呼，為狂梁語，亦不能復飲食。",
        "from": "T21n1262_001:0291a03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛遣沙門往視之，見母問：",
        "from": "T21n1262_001:0291a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「何為市中被髮啼哭？",
        "from": "T21n1262_001:0291a04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」母即報沙門言：",
        "from": "T21n1262_001:0291a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「亡我子眾多，故哭耳。",
        "from": "T21n1262_001:0291a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」沙門言：",
        "from": "T21n1262_001:0291a05_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「汝欲得汝子不？",
        "from": "T21n1262_001:0291a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」母報言：",
        "from": "T21n1262_001:0291a06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「我欲得之。",
        "from": "T21n1262_001:0291a06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」沙門言：",
        "from": "T21n1262_001:0291a06_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「汝審欲得者，是間有佛，可往問佛。",
        "from": "T21n1262_001:0291a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛者，知當來已去之事。",
        "from": "T21n1262_001:0291a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝往則可得汝子。",
        "from": "T21n1262_001:0291a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」母聞是語，則歡喜意解，便隨沙門去。",
        "from": "T21n1262_001:0291a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到佛所，歡喜前為佛作禮。",
        "from": "T21n1262_001:0291a09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛即問母：",
        "from": "T21n1262_001:0291a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「何為市中啼哭？",
        "from": "T21n1262_001:0291a10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」母報佛言：",
        "from": "T21n1262_001:0291a10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「亡我子故。",
        "from": "T21n1262_001:0291a10_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」佛問母：",
        "from": "T21n1262_001:0291a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「汝捨汝子至何所而亡汝子？",
        "from": "T21n1262_001:0291a11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」母即默然不言。",
        "from": "T21n1262_001:0291a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛復問母：",
        "from": "T21n1262_001:0291a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「汝捨子至何所而反？",
        "from": "T21n1262_001:0291a12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」默然無語。",
        "from": "T21n1262_001:0291a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母知盜人子為惡，母即起為佛作禮，頭面著地：",
        "from": "T21n1262_001:0291a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「我愚癡故。",
        "from": "T21n1262_001:0291a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」佛復問：",
        "from": "T21n1262_001:0291a14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「汝有子，愛之不？",
        "from": "T21n1262_001:0291a14_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」母言：",
        "from": "T21n1262_001:0291a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「我有子坐起常欲著我傍。",
        "from": "T21n1262_001:0291a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」佛復問：",
        "from": "T21n1262_001:0291a15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「汝有子，知愛之。",
        "from": "T21n1262_001:0291a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何故日行盜他人子？",
        "from": "T21n1262_001:0291a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他人有子，亦如汝愛之；",
        "from": "T21n1262_001:0291a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亡子家，亦行道啼哭如汝。",
        "from": "T21n1262_001:0291a17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝反盜人子殺噉之，死後當入太山地獄中。",
        "from": "T21n1262_001:0291a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」母聞是語便恐怖。",
        "from": "T21n1262_001:0291a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛復問：",
        "from": "T21n1262_001:0291a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「汝寧欲得汝子不？",
        "from": "T21n1262_001:0291a19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」母即起，復持頭面著地：",
        "from": "T21n1262_001:0291a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「願哀我。",
        "from": "T21n1262_001:0291a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」佛便語母言：",
        "from": "T21n1262_001:0291a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「汝子若在，汝寧能自悔不？",
        "from": "T21n1262_001:0291a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能自悔，當還汝子。",
        "from": "T21n1262_001:0291a21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」母言：",
        "from": "T21n1262_001:0291a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「我能自悔。",
        "from": "T21n1262_001:0291a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」佛言：",
        "from": "T21n1262_001:0291a22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「汝能自悔，當作何等自悔？",
        "from": "T21n1262_001:0291a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」母言：",
        "from": "T21n1262_001:0291a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「我聽佛教誡，當隨佛語自悔。",
        "from": "T21n1262_001:0291a23_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛還我子，我不敢遠離佛所語。",
        "from": "T21n1262_001:0291a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」佛言：",
        "from": "T21n1262_001:0291a24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「審如汝語不？",
        "from": "T21n1262_001:0291a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」母言：",
        "from": "T21n1262_001:0291a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「我審如佛語。",
        "from": "T21n1262_001:0291a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」佛便授以五戒：",
        "from": "T21n1262_001:0291a25_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一、不殺生；",
        "from": "T21n1262_001:0291a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二、不盜；",
        "from": "T21n1262_001:0291a26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三、不婬；",
        "from": "T21n1262_001:0291a26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四、不兩舌；",
        "from": "T21n1262_001:0291a26_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五、不飲酒。",
        "from": "T21n1262_001:0291a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "報能，悉還其子。",
        "from": "T21n1262_001:0291a27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛便為說：",
        "from": "T21n1262_001:0291a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「汝有千子，皆為說千子名字。",
        "from": "T21n1262_001:0291a28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五百子在天上，皆是鬼中王，將鬼官屬嫉害嬈天民；",
        "from": "T21n1262_001:0291a29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五百子在世間，嬈諸人民。",
        "from": "T21n1262_001:0291b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝子作鬼王，將數萬鬼。",
        "from": "T21n1262_001:0291b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是五百子將鬼官屬，一不可稱數。",
        "from": "T21n1262_001:0291b02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "極嫉害惡，或自稱作樹木神者；",
        "from": "T21n1262_001:0291b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或作地神者；",
        "from": "T21n1262_001:0291b03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或作水神者；",
        "from": "T21n1262_001:0291b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或詐為人兄弟妻子，自怨枉家室內外者；",
        "from": "T21n1262_001:0291b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或作海神者；",
        "from": "T21n1262_001:0291b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或作船車神者；",
        "from": "T21n1262_001:0291b05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或作舍宅神者；",
        "from": "T21n1262_001:0291b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或自稱夜在冥中神者；",
        "from": "T21n1262_001:0291b06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或使人夢寤者；",
        "from": "T21n1262_001:0291b06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或使人恐怖，為人作怪者；",
        "from": "T21n1262_001:0291b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或自稱星死者，自稱病死者——如是耗亂人，適不在一處。",
        "from": "T21n1262_001:0291b08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "極嫉害惡，如是矯稱，令人祠祀烹殺，人不知多為烹殺飲食是鬼；",
        "from": "T21n1262_001:0291b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是鬼亦不肯食。",
        "from": "T21n1262_001:0291b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端嫉害欲使人犯殺，欲使人入地獄中。",
        "from": "T21n1262_001:0291b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要不食之。",
        "from": "T21n1262_001:0291b11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見人祠祀喜。",
        "from": "T21n1262_001:0291b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是鬼亦不能護活人命，但益罪。",
        "from": "T21n1262_001:0291b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愚癡人不知，坐鬼貧窮。",
        "from": "T21n1262_001:0291b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」鬼子母聞佛說是語，即一心自悔，即得須陀洹道，知方來去事。",
        "from": "T21n1262_001:0291b14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長跪白佛言：",
        "from": "T21n1262_001:0291b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「我愚癡不知，世世有惡乃爾。",
        "from": "T21n1262_001:0291b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今我得持戒，思惟中正之道，我心皆徹視；",
        "from": "T21n1262_001:0291b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還見我千子。",
        "from": "T21n1262_001:0291b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今我乃知，佛所至至語成。",
        "from": "T21n1262_001:0291b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願佛哀我！",
        "from": "T21n1262_001:0291b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我欲止佛精舍傍，我欲呼千子王，我欲使與佛結要，我欲報復天上天下人恩。",
        "from": "T21n1262_001:0291b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」佛言：",
        "from": "T21n1262_001:0291b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「善哉！",
        "from": "T21n1262_001:0291b19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如汝有是意大善。",
        "from": "T21n1262_001:0291b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」佛言：",
        "from": "T21n1262_001:0291b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「汝從是已去，當稱是語，便止佛精舍邊。",
        "from": "T21n1262_001:0291b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其國中人民，無子者來求子，當與之子，自在所願。",
        "from": "T21n1262_001:0291b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當勅子姓，與使隨護人。",
        "from": "T21n1262_001:0291b22_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得復妄嬈之。",
        "from": "T21n1262_001:0291b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」欲從鬼子母求願者，名浮陀摩尼鉢，姊名炙匿，天上天下鬼屬是摩尼鉢；",
        "from": "T21n1262_001:0291b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "主四海內，船車治生有財產，皆屬摩尼鉢。",
        "from": "T21n1262_001:0291b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩尼鉢與佛結要、受戒，主護人財物。",
        "from": "T21n1262_001:0291b26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "炙匿主人——若有產生，當救之。",
        "from": "T21n1262_001:0291b27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有天王名毘沙門，主四天地——護人命，出入常從毘沙門求願。",
        "from": "T21n1262_001:0291b28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大鬼王，名阿須倫，主諸龍王、諸毒氣，人從求願，令毒不干人；",
        "from": "T21n1262_001:0291c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求願當慈心，無所用謝，亦無所噉食人。",
        "from": "T21n1262_001:0291c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從求願者，在人何求與耳；",
        "from": "T21n1262_001:0291c02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要無所求索，亦不責人。",
        "from": "T21n1262_001:0291c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人至浮陀摩尼鉢前，為作禮。",
        "from": "T21n1262_001:0291c03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說鬼子母經",
        "from": "T21n1262_001:0291c04_100",
        "context_with_next_sentence": false
    }
]