[
    {
        "source_sentence": "佛說大吉祥天女十二名號經",
        "from": "T21n1252b_001:0252c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐三藏沙門大廣智不空譯",
        "from": "T21n1252b_001:0252c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。一時薄伽梵。住極樂世界。與無",
        "from": "T21n1252b_001:0252c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "量大菩薩眾。",
        "from": "T21n1252b_001:0252c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後圍繞而為說法。",
        "from": "T21n1252b_001:0252c19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時觀自在菩薩摩訶薩。",
        "from": "T21n1252b_001:0252c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大吉祥天女菩薩摩訶薩等。",
        "from": "T21n1252b_001:0252c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆從座起詣世尊所。",
        "from": "T21n1252b_001:0252c21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭面禮足各坐一面。",
        "from": "T21n1252b_001:0252c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊為欲利益薄福貧窮諸有情故。",
        "from": "T21n1252b_001:0252c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告觀自在菩薩言。",
        "from": "T21n1252b_001:0252c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子若有苾芻苾芻尼近事男近事女諸有情類。",
        "from": "T21n1252b_001:0252c24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知此大吉祥天女十二名號。",
        "from": "T21n1252b_001:0252c25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能受持讀誦修習供養為他宣說。",
        "from": "T21n1252b_001:0252c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除一切貧窮業障。",
        "from": "T21n1252b_001:0252c26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲大富貴豐饒財寶。",
        "from": "T21n1252b_001:0252c27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時會中天龍八部。",
        "from": "T21n1252b_001:0252c27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "異口同音咸作是言。",
        "from": "T21n1252b_001:0252c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如世尊說真實不虛。",
        "from": "T21n1252b_001:0252c28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等願聞十二名號。",
        "from": "T21n1252b_001:0252c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願世尊大悲演說。",
        "from": "T21n1252b_001:0252c29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言汝當善聽今為汝說所謂　吉慶　吉祥蓮華　嚴飾　具財　白色　大名稱　蓮華眼　大光曜　施食者　施飲者　寶光　大吉祥　是為十二名號。",
        "from": "T21n1252b_001:0253a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當受持。",
        "from": "T21n1252b_001:0253a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今復說大吉祥陀羅尼曰。",
        "from": "T21n1252b_001:0253a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也二合他去聲引一室哩二合抳室哩二合抳二薩嚩迦引哩野二合娑去聲引馱𩕳三悉𩕳悉𩕳四𩕳𩕳𩕳𩕳五阿上聲洛乞史茗三合曩引捨野娑嚩二合引賀引六爾時世尊說是陀羅尼已。",
        "from": "T21n1252b_001:0253a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告觀自在菩薩言。",
        "from": "T21n1252b_001:0253a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大吉祥陀羅尼及十二名號。",
        "from": "T21n1252b_001:0253a11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除貧窮一切不祥。",
        "from": "T21n1252b_001:0253a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有願求皆得圓滿。",
        "from": "T21n1252b_001:0253a12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能晝夜三時讀誦此經每時三遍。",
        "from": "T21n1252b_001:0253a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或常受持不間。",
        "from": "T21n1252b_001:0253a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作饒益心隨力虔誠供養大吉祥天女菩薩。",
        "from": "T21n1252b_001:0253a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速獲一切財寶豐饒吉祥安樂。",
        "from": "T21n1252b_001:0253a15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時觀自在菩薩摩訶薩。",
        "from": "T21n1252b_001:0253a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸大眾天龍八部。",
        "from": "T21n1252b_001:0253a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從佛聞說十二名號及陀羅尼。",
        "from": "T21n1252b_001:0253a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歎末曾有。",
        "from": "T21n1252b_001:0253a17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆大歡喜信受奉行。",
        "from": "T21n1252b_001:0253a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說大吉祥天女十二名號經􄕗􄟯􄕤􄭠􄕍􇥗􄙺􅞣􄑁􅝩􄙻􄕻􄖫􄙼􄖫􇥗􄖫􄖫􄖫􄖫􆿅􄙊􅋪􄖪􄙢􄗱􅁥􄚩",
        "from": "T21n1252b_001:0253a22_100",
        "context_with_next_sentence": false
    }
]