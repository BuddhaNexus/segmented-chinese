[
    {
        "source_sentence": "佛說七千佛神符經",
        "from": "T85n2904_001:1446a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人身延年益算。年一百二十歲。甲辰□□□",
        "from": "T85n2904_001:1446a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□益算。七千佛守護人身。令不見惡□□□",
        "from": "T85n2904_001:1446a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□救我益算。",
        "from": "T85n2904_001:1446a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七千佛為護人身斷諸□□□□甲子。",
        "from": "T85n2904_001:1446a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從佛益算人身命盡之者。",
        "from": "T85n2904_001:1446a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□□千佛下生與我益算。",
        "from": "T85n2904_001:1446a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七千佛救護人□□□之者。",
        "from": "T85n2904_001:1446a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "益其食算七百七十佛下生。",
        "from": "T85n2904_001:1446a11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與我益算。",
        "from": "T85n2904_001:1446a11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七百七十佛下生身。",
        "from": "T85n2904_001:1446a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疾病之者菩薩破魔。",
        "from": "T85n2904_001:1446a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十五佛下生。",
        "from": "T85n2904_001:1446a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與我益算。",
        "from": "T85n2904_001:1446a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七百七十佛守護人身。",
        "from": "T85n2904_001:1446a13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令得堅強勇猛精進五十三佛下生。",
        "from": "T85n2904_001:1446a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與我益算。",
        "from": "T85n2904_001:1446a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七百七十護人身命億千萬歲常得佛道。",
        "from": "T85n2904_001:1446a15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六佛下生。",
        "from": "T85n2904_001:1446a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與我益算。",
        "from": "T85n2904_001:1446a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七百七十佛護人益身命。",
        "from": "T85n2904_001:1446a16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開智慧憑通達無礙。",
        "from": "T85n2904_001:1446a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七佛下生與我益算。",
        "from": "T85n2904_001:1446a17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十五佛護導人身。",
        "from": "T85n2904_001:1446a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "威儀無缺。",
        "from": "T85n2904_001:1446a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾魔自畏。",
        "from": "T85n2904_001:1446a18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八萬眾菩薩與我益算。",
        "from": "T85n2904_001:1446a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七十三佛願佛及諸菩薩並聲聞求辟支佛。",
        "from": "T85n2904_001:1446a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸阿羅漢為弟子益算。",
        "from": "T85n2904_001:1446a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七千佛符打鍛眾魔蠱道山林社稷郎岸北神盡自消滅。",
        "from": "T85n2904_001:1446a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千佛符打鍛眾魔風王竈君水龍之神土公盡自消滅。",
        "from": "T85n2904_001:1446a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千佛符打鍛五秏六噓惡夢悲啼野道呪咀盡自消滅。",
        "from": "T85n2904_001:1446a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破軍星主秏虛。",
        "from": "T85n2904_001:1446a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "武曲星主官事。",
        "from": "T85n2904_001:1446a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廉貞星主口舌。",
        "from": "T85n2904_001:1446a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文曲星主惡夢。",
        "from": "T85n2904_001:1446a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祿存星主百鬼。",
        "from": "T85n2904_001:1446a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "巨門星注非尸。",
        "from": "T85n2904_001:1446a26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貪狼星主惡氣。",
        "from": "T85n2904_001:1446a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在北斗星注生命。",
        "from": "T85n2904_001:1446a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在北斗星注百鬼。",
        "from": "T85n2904_001:1446a27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三台星主護身命。",
        "from": "T85n2904_001:1446a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一怨星第二良星。",
        "from": "T85n2904_001:1446a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三辟星第。",
        "from": "T85n2904_001:1446a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四熒惑星。",
        "from": "T85n2904_001:1446a29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五危星能消災受難。",
        "from": "T85n2904_001:1446a29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辟除天下疫氣疾病。",
        "from": "T85n2904_001:1446a30_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姦非魍魎訪。",
        "from": "T85n2904_001:1446a30_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亂鬼皆去千萬里之外。",
        "from": "T85n2904_001:1446b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "急急如律令。",
        "from": "T85n2904_001:1446b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一心開符。",
        "from": "T85n2904_001:1446b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二益算符。",
        "from": "T85n2904_001:1446b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三救護身命符。",
        "from": "T85n2904_001:1446b02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四金木水火土不相刻符。",
        "from": "T85n2904_001:1446b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五注生人阿姬死鬼秏噓神符。",
        "from": "T85n2904_001:1446b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所生之注一切惡物見符。",
        "from": "T85n2904_001:1446b04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散滅急急如律令。",
        "from": "T85n2904_001:1446b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將軍在子。",
        "from": "T85n2904_001:1446b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六月壬子朔二十日癸丑開白。",
        "from": "T85n2904_001:1446b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千佛神符保請百年。",
        "from": "T85n2904_001:1446b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若月衰日衰。",
        "from": "T85n2904_001:1446b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若歲衰時衰。",
        "from": "T85n2904_001:1446b07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日忌當佩符之後無有衰忌。",
        "from": "T85n2904_001:1446b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千佛符厭令其人過災度難延年益算。",
        "from": "T85n2904_001:1446b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受符以後壽命延長。",
        "from": "T85n2904_001:1446b09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七千佛神符請命。",
        "from": "T85n2904_001:1446b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願受一百二十歲。",
        "from": "T85n2904_001:1446b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得算備足符。",
        "from": "T85n2904_001:1446b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生之身。",
        "from": "T85n2904_001:1446b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佩符之後。",
        "from": "T85n2904_001:1446b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛生我。",
        "from": "T85n2904_001:1446b11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸經載我。",
        "from": "T85n2904_001:1446b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光明照我。",
        "from": "T85n2904_001:1446b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陰陽長我。",
        "from": "T85n2904_001:1446b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四時養我。",
        "from": "T85n2904_001:1446b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五大將我。",
        "from": "T85n2904_001:1446b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五雲蓋我。",
        "from": "T85n2904_001:1446b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩護我。",
        "from": "T85n2904_001:1446b13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六百甲子覆我。",
        "from": "T85n2904_001:1446b13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五帝助我。",
        "from": "T85n2904_001:1446b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五音樂我。",
        "from": "T85n2904_001:1446b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五兵衛我。",
        "from": "T85n2904_001:1446b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "星辰格我。",
        "from": "T85n2904_001:1446b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五絲依我。",
        "from": "T85n2904_001:1446b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五香薰我。",
        "from": "T85n2904_001:1446b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金床臥我。",
        "from": "T85n2904_001:1446b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "錦被覆我。",
        "from": "T85n2904_001:1446b15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仙人扶我。",
        "from": "T85n2904_001:1446b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玉女侍我。",
        "from": "T85n2904_001:1446b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "青龍引我。",
        "from": "T85n2904_001:1446b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白虎狹我。",
        "from": "T85n2904_001:1446b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "守公候使人受我。",
        "from": "T85n2904_001:1446b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五姓敬我。",
        "from": "T85n2904_001:1446b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬姓榮我。",
        "from": "T85n2904_001:1446b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長者順我。",
        "from": "T85n2904_001:1446b17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鄉里媚我。",
        "from": "T85n2904_001:1446b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弓箭刀兵盜賊離我。",
        "from": "T85n2904_001:1446b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虎狼蚖蛇避我。",
        "from": "T85n2904_001:1446b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五毒蠱道避我之身。",
        "from": "T85n2904_001:1446b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佩符之後天開四通。",
        "from": "T85n2904_001:1446b19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百鬼不從。",
        "from": "T85n2904_001:1446b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "墮水不沒。",
        "from": "T85n2904_001:1446b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀兵不傷。",
        "from": "T85n2904_001:1446b20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出入吉良。",
        "from": "T85n2904_001:1446b20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不逢禍殃。",
        "from": "T85n2904_001:1446b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四方五岳將軍所護。",
        "from": "T85n2904_001:1446b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無道備具。",
        "from": "T85n2904_001:1446b21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "保宣在前。",
        "from": "T85n2904_001:1446b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢懷敬我。",
        "from": "T85n2904_001:1446b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "良藥與我。",
        "from": "T85n2904_001:1446b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神符護我。",
        "from": "T85n2904_001:1446b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百鬼避我。",
        "from": "T85n2904_001:1446b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百神愛我。",
        "from": "T85n2904_001:1446b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天門開我。",
        "from": "T85n2904_001:1446b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地戶出我。",
        "from": "T85n2904_001:1446b23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "涼泉飲我。",
        "from": "T85n2904_001:1446b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風霜雨師除道。",
        "from": "T85n2904_001:1446b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當符前者死。",
        "from": "T85n2904_001:1446b24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逆符者亡。",
        "from": "T85n2904_001:1446b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "符厭死喪新舊注雌雄破殃伏連之鬼。",
        "from": "T85n2904_001:1446b25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "符厭山林社稷之鬼。",
        "from": "T85n2904_001:1446b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "符厭遊天之鬼。",
        "from": "T85n2904_001:1446b26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "符厭赤舌之鬼。",
        "from": "T85n2904_001:1446b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "符厭比舍五土注之鬼。",
        "from": "T85n2904_001:1446b27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "符厭日遊土氣之鬼。",
        "from": "T85n2904_001:1446b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "符厭星死之鬼。",
        "from": "T85n2904_001:1446b28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "符厭客死之鬼。",
