        "context_with_next_sentence": true
    },
    {
        "source_sentence": "符厭兵死獄死之鬼。",
        "from": "T85n2904_001:1446b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弟子佩千佛符之後。",
        "from": "T85n2904_001:1446b29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四出行來。",
        "from": "T85n2904_001:1446b29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千道無窮。",
        "from": "T85n2904_001:1446b30_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬道無難。",
        "from": "T85n2904_001:1446b30_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臥不惡夢。",
        "from": "T85n2904_001:1446b30_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求常得。",
        "from": "T85n2904_001:1446b30_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所願從心。",
        "from": "T85n2904_001:1446c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千佛法正如符。",
        "from": "T85n2904_001:1446c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所勅急急如律令。",
