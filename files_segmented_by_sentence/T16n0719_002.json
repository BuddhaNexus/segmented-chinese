[
    {
        "source_sentence": "十二緣生祥瑞經卷下",
        "from": "T16n0719_002:0847c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試鴻臚",
        "from": "T16n0719_002:0847c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少卿傳法大師臣施護奉　詔譯",
        "from": "T16n0719_002:0847c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "注云：",
        "from": "T16n0719_002:0847c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「有支順轉，積日逆流，黑白月餘，如經廣說。",
        "from": "T16n0719_002:0847c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」爾時眾會，無量人天，白世尊言：",
        "from": "T16n0719_002:0847c20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「烏鳥鳴吟，時來不定，如何了知？",
        "from": "T16n0719_002:0847c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願佛演說。",
        "from": "T16n0719_002:0847c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」爾時世尊告諸大眾：",
        "from": "T16n0719_002:0847c22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「若復有人，於十二支，審諦觀察，了知憂喜。",
        "from": "T16n0719_002:0847c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無明支日，烏鳴於右，妻女見喜；",
        "from": "T16n0719_002:0847c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烏鳴在左，行人必來。",
        "from": "T16n0719_002:0847c24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於行支日，右鳴安吉、左鳴獲財。",
        "from": "T16n0719_002:0847c25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於識支日，右鳴所望必得遂意；",
        "from": "T16n0719_002:0847c26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左鳴所望皆不成就。",
        "from": "T16n0719_002:0847c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於名色支，右鳴得財、左鳴囹圄。",
        "from": "T16n0719_002:0847c28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於六入支，右鳴驚怖、左鳴諍訟。",
        "from": "T16n0719_002:0847c29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於觸支日，右鳴驚怖、左鳴家人至。",
        "from": "T16n0719_002:0848a01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於受支日，右鳴稱意；",
        "from": "T16n0719_002:0848a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左鳴獲信，散失財物。",
        "from": "T16n0719_002:0848a02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於愛支日，右鳴安樂、左獲善信。",
        "from": "T16n0719_002:0848a03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於取支日，右鳴苦辛、左獲安吉。",
        "from": "T16n0719_002:0848a04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於有支日，右鳴人問、左鳴安吉。",
        "from": "T16n0719_002:0848a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於生支日，右鳴所求和合遂意；",
        "from": "T16n0719_002:0848a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左鳴得信，財物破散。",
        "from": "T16n0719_002:0848a06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於老死支，右鳴無患；",
        "from": "T16n0719_002:0848a07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左鳴破壞、財物少許。",
        "from": "T16n0719_002:0848a07_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」爾時世尊告眾會言：",
        "from": "T16n0719_002:0848a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「若復有人，於無明支，烏鳴北方，必獲來信。",
        "from": "T16n0719_002:0848a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於行支日，烏鳴北方，有喜事獲。",
        "from": "T16n0719_002:0848a10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於識支日，鳴北安吉，諧和遂意。",
        "from": "T16n0719_002:0848a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於名色支，北方鳴時，聞悅意事。",
        "from": "T16n0719_002:0848a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於六入支，烏鳴北方，獲路遠事，財寶去遙，必非和合。",
        "from": "T16n0719_002:0848a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於觸支日，烏鳴北方，有諍訟起。",
        "from": "T16n0719_002:0848a15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「受支日鳴，北方安吉。",
        "from": "T16n0719_002:0848a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「愛支北鳴，遠離繫縛。",
        "from": "T16n0719_002:0848a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「取支北鳴，卒事獲信。",
        "from": "T16n0719_002:0848a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「有支北鳴，衣物必獲。",
        "from": "T16n0719_002:0848a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「生支北鳴，人來問事。",
        "from": "T16n0719_002:0848a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「老死北鳴，一切安吉。",
        "from": "T16n0719_002:0848a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」時諸大眾白世尊言：",
        "from": "T16n0719_002:0848a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「心齶上眴，云何了知？",
        "from": "T16n0719_002:0848a22_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」爾時世尊告大眾曰：",
        "from": "T16n0719_002:0848a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「若復有人，於十二支，憶念不忘，悉皆了達。",
        "from": "T16n0719_002:0848a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無明支，齶上眴動，大聖者至。",
        "from": "T16n0719_002:0848a25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「行支日眴，家長安和。",
        "from": "T16n0719_002:0848a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「識支齶眴，聖者必來。",
        "from": "T16n0719_002:0848a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「名色支眴，多獲財物。",
        "from": "T16n0719_002:0848a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「六入支眴，心起煩惱。",
        "from": "T16n0719_002:0848a29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「觸支齶眴，適意慶快。",
        "from": "T16n0719_002:0848b01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「受支日眴，有驚怖事。",
        "from": "T16n0719_002:0848b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「愛支日眴，增長家財。",
        "from": "T16n0719_002:0848b03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「取支日眴，中一人夭。",
        "from": "T16n0719_002:0848b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「有支日眴，聖者來至。",
        "from": "T16n0719_002:0848b05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「生支日眴，盜賊必至。",
        "from": "T16n0719_002:0848b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「老死支眴，家內和合。",
        "from": "T16n0719_002:0848b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「若復有人，觀十二支，了知憂喜。",
        "from": "T16n0719_002:0848b08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於無明支，心上眴動，諍訟煩惱。",
        "from": "T16n0719_002:0848b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「行支日眴，獲於財物。",
        "from": "T16n0719_002:0848b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「識支心眴，有驚怖事。",
        "from": "T16n0719_002:0848b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「名色心眴，父母歡喜。",
        "from": "T16n0719_002:0848b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「六入支眴，於所求事，和合現前。",
        "from": "T16n0719_002:0848b13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「觸支心眴，父母驚怖。",
        "from": "T16n0719_002:0848b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「受支心眴，南行禳之，乃可宜吉。",
        "from": "T16n0719_002:0848b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「愛支心眴，獲大憂惱。",
        "from": "T16n0719_002:0848b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「取支心眴，有惡人至。",
        "from": "T16n0719_002:0848b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「有支心眴，有憂悲事。",
        "from": "T16n0719_002:0848b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「生支心眴，必大怖畏。",
        "from": "T16n0719_002:0848b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「老死支日心上眴動，老者必卒。",
        "from": "T16n0719_002:0848b20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」爾時大眾復白佛言：",
        "from": "T16n0719_002:0848b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「此十二支，有此祥瑞，於日用事，未能了知，復云何行，唯願宣說。",
        "from": "T16n0719_002:0848b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」爾時世尊告大眾言：",
        "from": "T16n0719_002:0848b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「若復有人，於無明支及行支日，聚會筵設、修飾田宅、問事見貴、洗頭等吉；",
        "from": "T16n0719_002:0848b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "澡浴作衣，宜勿用之。",
        "from": "T16n0719_002:0848b25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於行支日，求事聽法、習學弧矢、收伏冤賊、澡浴親貴、皆宜善用；",
        "from": "T16n0719_002:0848b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洗頭有患，作衣退敗，慎勿用之。",
        "from": "T16n0719_002:0848b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於識支日，王受灌頂、伏冤收軍、修營聚落、州府舍宅、成就皆吉，洗髮咽項、造衣宜用；",
        "from": "T16n0719_002:0848c01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "澡浴驚怖，切宜慎之。",
        "from": "T16n0719_002:0848c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「若名色支，諸所作事、及常住事、剃除髮爪、財物收藏、皆善宜用；",
        "from": "T16n0719_002:0848c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洗頭色變，澡浴驚怖，若作衣時，獲喜速破。",
        "from": "T16n0719_002:0848c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「六入支日，王受灌頂、修治聚落、圓滿成就、他人勿拜、遷動新宅、財物遠離、收護獲罪；",
        "from": "T16n0719_002:0848c07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若澡浴時，父母驚怖，若洗頭時，名稱無怖。",
        "from": "T16n0719_002:0848c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於觸支日，作善不成、惡事害人、罪犯速成。",
        "from": "T16n0719_002:0848c09_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於取支日，一切所為、父母驚怖、澡浴作衣、愛人非久，必有血光，作衣未浴，宜勿著之。",
        "from": "T16n0719_002:0848c11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於受支日，所求事喜、交易布施、修治精舍、吉慶宜用；",
        "from": "T16n0719_002:0848c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洗頭驚怖，獲不遂意，若作衣時，所希非難，宜對友人著之前吉。",
        "from": "T16n0719_002:0848c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於愛支日，資具珍寶、宜收護吉；",
        "from": "T16n0719_002:0848c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輕微急速行用大憂，洗頭髮敗，作衣滿足。",
        "from": "T16n0719_002:0848c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於取支日，小兒剃髮、所為輕事、皆宜用之；",
        "from": "T16n0719_002:0848c17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師度弟子、剃除髭髮、服藥和合、送客嫁娶、吉慶富貴、澡浴獲財、作衣逢喜。",
        "from": "T16n0719_002:0848c19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於有支日，結識交友、及一切事、諍訟遠離，洗頭勿用，作衣澡浴，皆宜吉慶。",
        "from": "T16n0719_002:0848c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於生支日，修飾舍宅及鞍馬庫藏、見貴臣主、皆吉宜用；",
        "from": "T16n0719_002:0848c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "澡浴遠行，亦宜用之，及洗頭喜，作衣非久，宜當日著。",
        "from": "T16n0719_002:0848c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「老死支日，所起惡事、富者財散、作於利益及論義事、遠離諸非、洗頭速獲、飲食多美、澡浴心惱、作衣長久。",
        "from": "T16n0719_002:0848c27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」於是眾會白世尊言：",
        "from": "T16n0719_002:0848c28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「足眴地動，烏吟犬吠，油火鼠傷，善惡未了，願為演說。",
        "from": "T16n0719_002:0848c29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」爾時世尊告大眾言：",
        "from": "T16n0719_002:0849a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「若復有人，於十二支，憶念審察，當知善惡。",
        "from": "T16n0719_002:0849a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何？",
        "from": "T16n0719_002:0849a02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於無明支日，足眴有喜。",
        "from": "T16n0719_002:0849a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於行支日，足眴主事不吉勿用。",
        "from": "T16n0719_002:0849a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「識支名色，此日諍論。",
        "from": "T16n0719_002:0849a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「六入支日，足眴人卒。",
        "from": "T16n0719_002:0849a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「觸、受、愛支，此三足眴，諍論事起。",
        "from": "T16n0719_002:0849a07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於取支日，足眴人卒。",
        "from": "T16n0719_002:0849a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於有支日，足眴獲衣。",
        "from": "T16n0719_002:0849a09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「生支足眴，賊難病怖。",
        "from": "T16n0719_002:0849a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「老死支日，若足眴時，獲悲惱事。",
        "from": "T16n0719_002:0849a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「復次，觀察十二有支，於無明支日，足眴動時，有劫盜至；",
        "from": "T16n0719_002:0849a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "家犬吠時，人來問遠；",
        "from": "T16n0719_002:0849a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣燒無事；",
        "from": "T16n0719_002:0849a13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若鼠嚙衣，危難來至；",
        "from": "T16n0719_002:0849a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有烏鳴，家人來問；",
        "from": "T16n0719_002:0849a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣油污時，人卒獲信；",
        "from": "T16n0719_002:0849a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若地動時，王恩普降。",
        "from": "T16n0719_002:0849a15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於行支日，足眴動時宜出行吉；",
        "from": "T16n0719_002:0849a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若犬吠時，有少王事；",
        "from": "T16n0719_002:0849a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有烏鳴，善信必至；",
        "from": "T16n0719_002:0849a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及見血光、衣燒獲喜；",
        "from": "T16n0719_002:0849a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鼠嚙衣時，獲大富貴；",
        "from": "T16n0719_002:0849a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣油污時，財物多獲；",
        "from": "T16n0719_002:0849a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若地動時，飢饉劫殺，外國來侵。",
        "from": "T16n0719_002:0849a19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於識支日，足眴動時，有非人至；",
        "from": "T16n0719_002:0849a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有犬吠時，有賊盜至，及獲財物；",
        "from": "T16n0719_002:0849a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有烏鳴吟，諍訟事起；",
        "from": "T16n0719_002:0849a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若衣燒時，而有所得；",
        "from": "T16n0719_002:0849a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若鼠咬衣，財失却得；",
        "from": "T16n0719_002:0849a22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "油污衣時，父母驚怖；",
        "from": "T16n0719_002:0849a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有地動時，二王吞併。",
        "from": "T16n0719_002:0849a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「名色支日，足眴獲財，不求自至；",
        "from": "T16n0719_002:0849a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "犬吠人卒；",
        "from": "T16n0719_002:0849a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若烏鳴吟，有愛人至；",
        "from": "T16n0719_002:0849a24_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣鼠傷時，財失却來；",
        "from": "T16n0719_002:0849a25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣燒有喜；",
        "from": "T16n0719_002:0849a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣若油污，大人思念；",
        "from": "T16n0719_002:0849a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有地動時，有冤賊起，除剪國靜。",
        "from": "T16n0719_002:0849a26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「六入支日，若足眴時，友人遠至，家長安吉；",
        "from": "T16n0719_002:0849a27_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "犬吠諍訟；",
        "from": "T16n0719_002:0849a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烏鳴人來，作和合事；",
        "from": "T16n0719_002:0849a28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鼠傷衣服，有人水沒而卒；",
        "from": "T16n0719_002:0849a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若衣燒損，諍訟獲財；",
        "from": "T16n0719_002:0849a29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣若油污，有驚怖事；",
        "from": "T16n0719_002:0849b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有地動時，老者卒矣。",
        "from": "T16n0719_002:0849b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於觸支日，若足眴時，喜事得聞；",
        "from": "T16n0719_002:0849b02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "家犬吠時，盜賊來至；",
        "from": "T16n0719_002:0849b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烏鳴驚怖；",
        "from": "T16n0719_002:0849b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣鼠損時，獲於財物；",
        "from": "T16n0719_002:0849b03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣火傷時，表於人卒；",
        "from": "T16n0719_002:0849b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣油污時，獲眷屬喜；",
        "from": "T16n0719_002:0849b04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地動諍論。",
        "from": "T16n0719_002:0849b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於受支日，足眴動時，出行宜吉；",
        "from": "T16n0719_002:0849b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "家犬吠時，有聖者至；",
        "from": "T16n0719_002:0849b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣鼠傷時，必得見主；",
        "from": "T16n0719_002:0849b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣火傷時，有少事起；",
        "from": "T16n0719_002:0849b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "油污衣時，得不可意；",
        "from": "T16n0719_002:0849b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有地動時，遠信必至。",
        "from": "T16n0719_002:0849b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於愛支日，足眴獲財；",
        "from": "T16n0719_002:0849b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "犬非時吠，必有瞋訟；",
        "from": "T16n0719_002:0849b10_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若烏鳴吟，聞於子信；",
        "from": "T16n0719_002:0849b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鼠傷衣時，必有人卒；",
        "from": "T16n0719_002:0849b11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火傷衣時，得於財物；",
        "from": "T16n0719_002:0849b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣油污時，有吉慶事；",
        "from": "T16n0719_002:0849b12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有地動時，冤賊必起，及有使命，從東而來。",
        "from": "T16n0719_002:0849b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於取支日，足眴獲罪，人卒有信；",
        "from": "T16n0719_002:0849b14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "犬非時吠，有諍訟事；",
        "from": "T16n0719_002:0849b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烏鳴吟時，有眷屬卒；",
        "from": "T16n0719_002:0849b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣鼠傷時，有飢饉至；",
        "from": "T16n0719_002:0849b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣火損時，必有所獲；",
        "from": "T16n0719_002:0849b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣油污時，散失財物；",
        "from": "T16n0719_002:0849b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有地動時，即外處來侵。",
        "from": "T16n0719_002:0849b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於有支日，足眴動時，賊來聞信；",
        "from": "T16n0719_002:0849b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "犬吠非時，土地不寧；",
        "from": "T16n0719_002:0849b18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若烏鳴吟，必獲於財；",
        "from": "T16n0719_002:0849b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣鼠傷時，家賊必至；",
        "from": "T16n0719_002:0849b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣火損時，獲美飲食；",
        "from": "T16n0719_002:0849b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣油有喜，地動豐熟。",
        "from": "T16n0719_002:0849b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於生支日，足眴動時，遠乘速至；",
        "from": "T16n0719_002:0849b21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "犬非時吠，有喜悅事；",
        "from": "T16n0719_002:0849b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烏鳴吟時，無事安和；",
        "from": "T16n0719_002:0849b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鼠傷衣服，廣得財物；",
        "from": "T16n0719_002:0849b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火損衣時，什物增長；",
        "from": "T16n0719_002:0849b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣油污時，有喜必來；",
        "from": "T16n0719_002:0849b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若地動時，有軍兵至。",
        "from": "T16n0719_002:0849b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「老死支日，足眴動時，諍論事虛；",
        "from": "T16n0719_002:0849b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "犬吠非時，友人遠至；",
        "from": "T16n0719_002:0849b25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烏鳴吟時，有諍訟事；",
        "from": "T16n0719_002:0849b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣鼠損時，財物散失；",
        "from": "T16n0719_002:0849b26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火傷衣時，有囹圄難；",
        "from": "T16n0719_002:0849b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣油點染，諍論事至；",
        "from": "T16n0719_002:0849b27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有地動時，難處東方。",
        "from": "T16n0719_002:0849b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」爾時世尊復告大眾：",
        "from": "T16n0719_002:0849b29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「若復有人，於此緣生十二有支，思念不忘，必能了遠憂喜等事。",
        "from": "T16n0719_002:0849c01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無明支日，宜護財物。",
        "from": "T16n0719_002:0849c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於行支日，收捨財物。",
        "from": "T16n0719_002:0849c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於識支日，宜學藝能。",
        "from": "T16n0719_002:0849c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「名色支日，為眷屬吉。",
        "from": "T16n0719_002:0849c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「六入支日，結識事吉。",
        "from": "T16n0719_002:0849c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「若觸支日，宜破冤事。",
        "from": "T16n0719_002:0849c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於受支日，宜嫁娶事。",
        "from": "T16n0719_002:0849c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於愛支日，奉王命事行禁非法。",
        "from": "T16n0719_002:0849c09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於取支日，宜知上事。",
        "from": "T16n0719_002:0849c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「老死支日，起非法業，正行勿用。",
        "from": "T16n0719_002:0849c11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」時諸大眾白世尊言：",
        "from": "T16n0719_002:0849c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「此十二支，於卜問時，云何行之？",
        "from": "T16n0719_002:0849c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」爾時世尊告眾會言：",
        "from": "T16n0719_002:0849c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「若復有人，於無明支日，欲求卜問，獲於財物、鞍馬皆吉、眷屬聚會、不永勿用。",
        "from": "T16n0719_002:0849c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於行支日，宜卜問事，出行食飲、思子念友、說法皆吉。",
        "from": "T16n0719_002:0849c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於識支日，若卜問時，事勿動喜、結識知友、不起煩惱、事說成就、災難息除。",
        "from": "T16n0719_002:0849c20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「名色支日，若卜問時，有怖煩惱、事散遍普、骨肉離別。",
        "from": "T16n0719_002:0849c21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「六入支日，行卜問時，獲財禮事、及妻室悅、女男分貴。",
        "from": "T16n0719_002:0849c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於觸支日，卜問有諍，賊怖憂惱、非理患難、眷屬不和。",
        "from": "T16n0719_002:0849c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於受支日，求卜問事，多獲財寶、飲食衣物、妻室悅意、莊嚴具足。",
        "from": "T16n0719_002:0849c27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於愛支日，求卜問事，心思搆畫、虛妄所求、皆非成就。",
        "from": "T16n0719_002:0849c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於取支日，求卜問事，見者歡喜、稱讚護持、座兼所乘、財獲多喜。",
        "from": "T16n0719_002:0850a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於有支日，求卜問事，獲怖王法、破壞憂惱、骨肉離散。",
        "from": "T16n0719_002:0850a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於生支日，求卜問事，獲大財物、一切諧和、圓滿成就、朋友歡喜、珍重稱讚。",
        "from": "T16n0719_002:0850a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「於老死支，求卜問事，虛妄善惡、破壞星散、無有精華、事虛大憂、貪瞋數起。",
        "from": "T16n0719_002:0850a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」爾時世尊，說是法已，告諸大眾：",
        "from": "T16n0719_002:0850a09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「若復有人，審諦觀察十二緣生，了達善惡憂喜得失，應畫轉輪圖寫分明；",
        "from": "T16n0719_002:0850a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂從無明，乃至老死，月日分位，次第羅列，鼠牛虎兔龍蛇馬羊猴雞犬豕，十二相狀，本形輪轉，次第為人解說。",
        "from": "T16n0719_002:0850a13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」時諸大眾，聞佛所說，踊躍歡喜，信受奉行。",
        "from": "T16n0719_002:0850a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二緣生祥瑞經卷下",
        "from": "T16n0719_002:0850a15_100",
        "context_with_next_sentence": false
    }
]