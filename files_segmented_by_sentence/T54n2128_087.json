[
    {
        "source_sentence": "一切經音義卷第八十七",
        "from": "T54n2128_087:0863c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翻經沙門慧琳撰",
        "from": "T54n2128_087:0863c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "音破邪論兩卷",
        "from": "T54n2128_087:0863c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "崇正錄十五卷甄正論三卷十門辯惑論兩卷右四論二十二卷同此卷音。",
        "from": "T54n2128_087:0863c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破邪論序破邪論序慧琳音窅冥上𦝫晈反說文云窅深目也從穴中目今文筆之土每有製作皆用此字但取深義殊不知於理甚乖也失之遠矣或從幼作窈毛詩傳云幽靜也說文云深遠也於義猶未為得合從󲺻作󲿶此正󲿶冥之字時多不用蓋謂不見本字也說文云󲿶冥也從穴󲺼聲󲺼音同上亦作杳。",
        "from": "T54n2128_087:0863c13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發擿汀歷反考聲云擿揩也撥也古今正字注才適聲也。",
        "from": "T54n2128_087:0863c14_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "𦘺遁屯頓反王逸注楚辭云遁隱也廣雅云避世也說文云僊也一云巡也從辵盾聲論作盾遯俗用字。",
        "from": "T54n2128_087:0863c15_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破邪論1 卷上破邪論卷上褫運上池尒反蒼頡云褫徹也說文奪也從衣虒聲虒音斷。",
        "from": "T54n2128_087:0863c17_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洊雷上前薦反周易云洊雷震王弼云洊重也說文作瀳從水薦聲亦作荐。",
        "from": "T54n2128_087:0863c18_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憤懣上汾吻反下門本反王逸注楚辭云懣亦憤也蒼頡篇云悶也說文云煩也從心滿聲古文作󱳴義亦同。",
        "from": "T54n2128_087:0863c19_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惄焉上寧歷反正體字也毛詩傳云惄愁也思也說文云憂也從心叔聲。",
        "from": "T54n2128_087:0863c20_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "磣黷上初錦反下音獨陸機漢祖功臣頌茫茫宇宙上磣下黷波振四海塵飛五岳九服俳佪三靈改卜古今正字從石參聲或從土作墋也。",
        "from": "T54n2128_087:0863c22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刁斗上鳥聊反下兜偶反孟康注漢書云以銅為之受一斗晝炊飲食夜擊以警眾持行隨軍在熒陽庫中今改為金鉦是也古今正字云二字並象形也象斗有柄論作升誤。",
        "from": "T54n2128_087:0863c24_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "哇歌上亞佳反蒼頡篇云哇謳也聲類󰦇也說文讇聲也從口圭聲亦作㰪。",
        "from": "T54n2128_087:0863c25_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白紈換巒反許叔重云紈素也說文從糸丸聲。",
        "from": "T54n2128_087:0863c26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蔡愔揖淫反人名也。",
        "from": "T54n2128_087:0864a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蠲嗜欲上决玄反考聲云蠲潔也除也。",
        "from": "T54n2128_087:0864a02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "崆峒上音空下音同山名也已釋辯正論第二卷海中山。",
        "from": "T54n2128_087:0864a03_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婕妤上尖葉反下與諸反聲類云接󲘪也女人官名也說文云二字並從女疌予皆聲也。",
        "from": "T54n2128_087:0864a04_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闞澤上坎濫反去聲字也。",
        "from": "T54n2128_087:0864a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飾繪上舒力反下回外反孔注尚書云繪五采也鄭注論語云畫也說文從糸貴聲亦作繪。",
        "from": "T54n2128_087:0864a06_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "涿鹿上音卓應劭注漢書云𣵠鹿縣名屬上谷即易州界。",
        "from": "T54n2128_087:0864a07_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九癭嬰郢反項瘤也。",
        "from": "T54n2128_087:0864a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昏墊佔念反辯正論第二卷中已釋佔音丁焰反。",
        "from": "T54n2128_087:0864a09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妲已上丹捺反下飢擬反紂之嬖妃也論從女作妃音芳微反非也。",
        "from": "T54n2128_087:0864a10_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玁狁上扻斂反下音允毛詩傳云北狄古名也鄭箋云兇奴也文字典說訓同上並從犬嚴允皆聲論作獫音力贍反非也。",
        "from": "T54n2128_087:0864a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汧渭上遣堅反下音謂二水名也漢書注云汧水出汧陽縣北入渭汭說文二字並從水幵胃皆聲幵音堅。",
        "from": "T54n2128_087:0864a13_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "楊玠皆薤反人名也。",
        "from": "T54n2128_087:0864a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "𢏗篡上霓計反正體從弓作𢏗下蒭患反論語云𢏗善射說文云帝譽射官也夏少康滅之從弓幵聲論從羽作羿是鳥飛也今俗用之久請詳焉實非本字也。",
        "from": "T54n2128_087:0864a16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "浞篡上仕角反左傳云伯明氏之讒子弟有寒浞即殺𢏗者也。",
        "from": "T54n2128_087:0864a17_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "綈衣上弟奚反說文云綈厚繒也從糸弟聲也。",
        "from": "T54n2128_087:0864a18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "2 卷下破邪論卷下溝洫下兄域反義已具釋辯正論第七卷中。",
        "from": "T54n2128_087:0864a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淴滑上音忽下音骨考聲云淴滑大水混流貌也滑亂也水圜轉旋流湍波大也。",
        "from": "T54n2128_087:0864a21_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "磅礴上薄郎反辯正論第一卷中有下蒲莫反考聲云廣大也。",
        "from": "T54n2128_087:0864a22_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依耐國中音奈梵語也。",
        "from": "T54n2128_087:0864a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嵐毘園上覽含反梵語也。",
        "from": "T54n2128_087:0864a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爇除上壖拙反杜注左傳云𤑔燒也說文作𤑔從火從執聲。",
        "from": "T54n2128_087:0864b01_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "驊驑上畫瓜反下音留郭璞驊色如華而赤色毛詩傳云馬赤身黑鬣曰騮亦作𩤃。",
        "from": "T54n2128_087:0864b02_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八駿遵峻反。",
        "from": "T54n2128_087:0864b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豺心上仕皆反俗作犲。",
        "from": "T54n2128_087:0864b04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不悛音銓孔注尚書云悛改也說文從心夋聲。",
        "from": "T54n2128_087:0864b05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尺鴳鵶諫反義已具釋辯正論第六卷中。",
        "from": "T54n2128_087:0864b06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "井蛙烏佳反正作鼃聲類云蝦蟇也。",
        "from": "T54n2128_087:0864b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朝菌上張遙反下群隕反郭注爾雅云菌地蕈也似蓋莊子云朝菌不知晦朔司馬彪云夭芝也。",
        "from": "T54n2128_087:0864b08_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蟪蛄上音惠下音蛄莊子云蟪蛄不知春秋說文二字並從虫惠古皆聲也。",
        "from": "T54n2128_087:0864b09_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婉娩上紆阮反下音晚毛詩傳云婉從也鄭玄云婉謂言語從也娩之言媚也謂容貌也古今正字並從女宛免皆聲。",
        "from": "T54n2128_087:0864b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盤古上伴瞞反古亦作估是古帝名号論文從木從半作柈非也。",
        "from": "T54n2128_087:0864b12_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蟬聯下音連顧野王云蟬聯相續之言也。",
        "from": "T54n2128_087:0864b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衮飾上公本反鄭眾注周禮衮龍衣也鄭玄云玄衣而畫以龍爾雅衮𭸪也說文云天子享先王卷龍衣繡於下裳幅一龍蜿上卿服說文從衣公聲也。",
        "from": "T54n2128_087:0864b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "跬步上窺癸反禮記云君子跬步不敢忘也方言半步為跬說文從足圭聲。",
        "from": "T54n2128_087:0864b16_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憫殤上旻隕反杜注左傳云憫憂貌也離騷云傷太息之憫憐說文從心閔聲下賞章反。",
        "from": "T54n2128_087:0864b17_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "㧖󳉏上鸎革反字書云㧖把也持也說文作搹從手鬲聲鬲音革論作扼俗字也下烏灌反正體字也說文云󳆓手掌後節也從手𥄗聲𥄗音冤板反論作腕俗字也。",
        "from": "T54n2128_087:0864b19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "抵掌上之尒反孝聲云抵側擊也側手投於掌也。",
        "from": "T54n2128_087:0864b20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盱衡上况于反郭璞云盱謂之舉眼也蒼頡篇張目也漢書盱衡厲色也古今正字從目于聲也。",
        "from": "T54n2128_087:0864b21_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煬帝上羊亮反隋帝謚法云逆天虐民曰煬好內怠政曰煬。",
        "from": "T54n2128_087:0864b22_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "杓之上正遙反北斗柄端第一星也。",
        "from": "T54n2128_087:0864b23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "補鼇𢾕高反考聲云大龜也論作󳅁俗字不可用也。",
        "from": "T54n2128_087:0864b24_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刊出上偘干反杜注左傳云刊除也說文云從刀干聲。",
        "from": "T54n2128_087:0864c01_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剗海上察簡反廣雅云剗削也古今正字從刀戔聲。",
        "from": "T54n2128_087:0864c02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縵瞼上蠻攀反考聲云縵緩也或作嫚論從目從慢非也下劒奄反。",
        "from": "T54n2128_087:0864c03_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咆勃上鮑茅反廣雅云咆鳴也說文噑也從口包聲下盆沒反顧野王云勃󲙐盛也蒼頡篇云猝󲙐也古今正字從力孛聲也。",
        "from": "T54n2128_087:0864c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耽湎上膽甘反下綿褊反。",
        "from": "T54n2128_087:0864c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蝍蛆上子力反下子餘反方言云馬蚿也古今正字二字並從虫即且聲。",
        "from": "T54n2128_087:0864c07_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "崇正錄1 第一卷崇正錄第一卷韋㡚狗侯反釋名云㡚單衣之無胡者也字書上衣也案韋㡚者戎虜之皮服也古今正字從巾冓聲亦從衣作褠冓音同上也。",
        "from": "T54n2128_087:0864c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毳幙上昌芮反下音幕案毳幙蕃戎之氈廬毛帳之屬。",
        "from": "T54n2128_087:0864c11_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "𤡝牢上并奚反王肅注家語云𤡝牢獄也集訓云𤡝牢也門外行馬也考聲從木非從土作㯇獄名也今俗用從比作狴誤也說文牢也所以枸非從陛省聲也論中從犬作狴非也。",
        "from": "T54n2128_087:0864c13_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酺醵上步胡反史記云始皇二十五年天下大酺五日漢律無故三人已上群飲者罰金四兩今賜令會飲五日也下渠虐反鄭注禮記云合錢飲酒也說文酺王德布大飲酒已從酉甫聲醵會飲食也從酉豦聲或作酢。",
        "from": "T54n2128_087:0864c15_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "󱈜毫上旋緣反郭璞云󱈜玉名也穆天子傳云春山之寶有󱈜珠顧野王云即𧉻所含珠也說文美人也從玉睿聲籀文作壑古文作󳂲下號高反。",
        "from": "T54n2128_087:0864c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天授雔宥反論本作󳆎偽字此則天朝偽字也不堪行用也今不取。",
        "from": "T54n2128_087:0864c18_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馭一境上魚據反與此御同。",
        "from": "T54n2128_087:0864c19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "推揖揞習反張戩云揖拱手而舉之以相敬也󳂻也錄本作挹音同義異是斟酌也。",
        "from": "T54n2128_087:0864c20_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不皦梟了反明也。",
        "from": "T54n2128_087:0864c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "2 第二卷第二卷智瀲廉贍反。",
        "from": "T54n2128_087:0864c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煩冗昔隴反聲類云冗散雜無食之人皆曰冗。",
        "from": "T54n2128_087:0864c24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庸謏消了反鄭注禮記云謏之言小也古今正字云從言叟聲也。",
        "from": "T54n2128_087:0865a01_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "撟偽上嬌小反鄭注周禮云撟稱詐以有為也顧野王云假稱為之撟賈逵云非先王之法曰撟楚辭云撟茲媚以弘處說文擅也從手喬聲錄本從矢作矯音則同乖於義也。",
        "from": "T54n2128_087:0865a03_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詭妄上歸委反作也。",
        "from": "T54n2128_087:0865a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重󳐧堅顯反考聲云皮虛起如䓣也古今正字作󲻘枅䄯今錄本作躎謬。",
        "from": "T54n2128_087:0865a05_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "推鞠鳩六反張戩考聲云鞠窮罪人也正作󳁖從人從竹從言或作諊󳂧。",
        "from": "T54n2128_087:0865a06_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愚獷觥猛反說文云獷不可附也從犬廣聲也。",
        "from": "T54n2128_087:0865a07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "齊󳆈麥彭反。",
        "from": "T54n2128_087:0865a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昚莫上臣刃反古文字也今作慎。",
        "from": "T54n2128_087:0865a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "𨡴使上酉周反毛詩云𨡴車鑾鏕傳云輶輕車蓋單車之使。",
        "from": "T54n2128_087:0865a10_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "于闐國中田鍊反。",
        "from": "T54n2128_087:0865a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辭訣音决。",
        "from": "T54n2128_087:0865a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼𩲐眉秘反鄭注周禮云󲑴所以從其為人與物也蓋祭天地之明曰百物之神曰󲑴山海經云󲑴之為物人身黑首說文正作鬽老物精也從鬼生毛從彡今亦作魅或作󱩡也。",
        "from": "T54n2128_087:0865a14_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魚豢還慢反。",
        "from": "T54n2128_087:0865a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "噂𠴲上尊損反下談合反毛詩傳文云噂𠴲噂𠴲相對言也說文聚語也並從口尊沓皆聲也亦作僔今錄從足作蹲踏非也。",
        "from": "T54n2128_087:0865a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "3 第三卷第三卷闕本不音4 第四卷第四卷煒󲶫上韋鬼反下炎輒反古文正體字毛詩傳云煒赤貌也說文盛明貌也從火韋聲廣雅云燁光明貌古今正字從火󲘶聲󲘶音同上正體字錄作熚謬也。",
        "from": "T54n2128_087:0865a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刳命上苦孤反顧野王云刳空其腹也說文云判也從刀夸聲。",
        "from": "T54n2128_087:0865a22_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蟒蛇上󲞫晃反。",
        "from": "T54n2128_087:0865a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "驢𩥇上旅㞐反下鱣碾反埤蒼云𩥇馬臥土中也張戩云馬展轉也古今正字從馬展聲。",
        "from": "T54n2128_087:0865a24_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二襜昌詹反毛詩傳云衣蔽前謂之襜方言襜謂之掖郭璞云衣掖下也說文衣蔽前也從衣詹聲傳從示作祲氛祲字音子󲸛反無義。",
        "from": "T54n2128_087:0865b02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩褾卑蓼反廣雅云褾袂也埤蒼云衣袖也古今正字從衣票聲也。",
        "from": "T54n2128_087:0865b03_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "5 第五卷第五卷此卷中並錄辯正論第二卷第五卷文已於本論中音訓訖更不重釋也6 第六卷第六卷此卷中錄辯正論第六卷首盡第六卷一半已於本論音訓訖7 第七卷第七卷此卷錄辯正論第六卷半後盡第六卷終已於本論音訓訖8 第八卷第八卷此卷錄辯正論第八欠尾頂四紙餘已於本論音訓訖9 第九卷第九卷闕本不音10 第十卷第十卷此卷錄甄正論已於本論音訓訖11 第十一卷第十一卷此卷錄甄正論足已於本論音訓訖12 第十二卷第十二卷投󱨈歸鮪反孔注尚書云匭匣也今見有之四方名別。",
        "from": "T54n2128_087:0865b12_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拉天藍合反何休注公羊云拉折也說文云拉搉也從手立聲亦作摺。",
        "from": "T54n2128_087:0865b13_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廊廡上音郎下無府反釋名云大屋曰廡說文堂下周室也從广無聲。",
        "from": "T54n2128_087:0865b14_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "𢈫扆上慎真反下衣蟣反。",
        "from": "T54n2128_087:0865b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悃款上坤穩反下寬管反王逸注楚辭云志伅一也廣雅志也說文從心困聲。",
        "from": "T54n2128_087:0865b16_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "品彙韋貴反廣雅云彙類也以其彙類相牽引也文字典說從󳊘胃省聲󳊘音大奚反也。",
        "from": "T54n2128_087:0865b17_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "丹慊謙簟反考聲云慊情切也足也。",
        "from": "T54n2128_087:0865b18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眭希寂上髓遺反人姓也。",
        "from": "T54n2128_087:0865b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "13-15 第十三卷-第十五卷第十三卷　第十四卷　第十五卷已上並闕本不音甄正論1 卷上甄正論卷上慧琳音蹎躓上典年反聲類云蹎蝢仆也廣雅云仆也孔注尚書反倒也說文跋也從足真聲亦作顛𧽍今論本作顛音同也失於義下知利反顧野王云躓猶頓也廣雅蹋也說文跉也從足質聲跉音鉗業也。",
        "from": "T54n2128_087:0865b23_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐袪去魚反考聲云袪開也却也除也裁也。",
        "from": "T54n2128_087:0865b24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隱机上殷靳反下音几。",
        "from": "T54n2128_087:0865c01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "簡𠕋上間眼反。",
        "from": "T54n2128_087:0865c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "郭璞注爾雅簡札也。",
        "from": "T54n2128_087:0865c02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顧野王云所用以寫書記事也。",
        "from": "T54n2128_087:0865c02_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說文牒也。",
        "from": "T54n2128_087:0865c02_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從竹間聲論從草作蕳誤也。",
        "from": "T54n2128_087:0865c02_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下鎗責反。",
        "from": "T54n2128_087:0865c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周禮云九命諸侯則𠕋命之。",
        "from": "T54n2128_087:0865c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鄭玄云謂簡𠕋書王命也。",
        "from": "T54n2128_087:0865c03_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蔡邕獨斷𠕋者簡也。",
        "from": "T54n2128_087:0865c03_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不滿百文不書於𠕋。",
        "from": "T54n2128_087:0865c03_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其制長二尺短者半之。",
        "from": "T54n2128_087:0865c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其次一長一短兩編下上篆書起年月凡命諸侯三公薨及以罪免悉以𠕋書。",
        "from": "T54n2128_087:0865c04_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說文符命也。",
        "from": "T54n2128_087:0865c04_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸侯進受於王象其札一長一短中有二編也。",
        "from": "T54n2128_087:0865c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "古文從竹作𥬰也。",
        "from": "T54n2128_087:0865c05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "紕謬上匹夷反考聲云紕繒帛踈薄也下靡幼反妄也欺也誤也。",
        "from": "T54n2128_087:0865c06_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "籧廬上巨魚反張戩云籧篨口柔怕同人顏色不能俯也下呂猪反考聲云庵類也寄也。",
        "from": "T54n2128_087:0865c07_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言誼冝寄反鄭玄注禮記云𧨏者制事之冝也王弼注周易𧨏猶理也毛詩傳云善也冝也說文人所冝也從言冝聲。",
        "from": "T54n2128_087:0865c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道該改哀反賈逵云該備也說文從言亥聲。",
        "from": "T54n2128_087:0865c10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斯鑑監懺反考聲云明也照也亦作鑒。",
        "from": "T54n2128_087:0865c11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敢遡蘇故反鄭注禮記云遡猶向也古今正字義同從辵朔聲亦從人作傃。",
        "from": "T54n2128_087:0865c12_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "簡牘同鹿反。",
        "from": "T54n2128_087:0865c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞿然上具遇反顧野王云驚懼之貌也毛詩傳云無守之貌也說文從隹䀠聲正從二目作䀠云左右視也今傳作懼誤也。",
        "from": "T54n2128_087:0865c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多懵墨崩反考聲云慙也悶也說文作懜云不明也從心從夢聲也。",
        "from": "T54n2128_087:0865c16_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寨木捷偃反諸字書及經史並無此字詢問道家相傳音蹇未詳何義。",
        "from": "T54n2128_087:0865c17_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "楊𣙜江岳反許叔重注淮南子云楊榷粗略也說文從木寉聲音同上。",
        "from": "T54n2128_087:0865c18_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縑緗上頰嫌反下想羊反。",
        "from": "T54n2128_087:0865c19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝嚳空汱反堯父。",
        "from": "T54n2128_087:0865c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皇甫謐民必反晉高士也。",
        "from": "T54n2128_087:0865c21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "󲔢澆上垂淪反孔注尚書云󲔢粹也廣雅厚也說文云不澆也從酉󲈅聲󲈅音同上論作純音同義則非也下晈堯反考聲云澆薄也。",
        "from": "T54n2128_087:0865c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "贙兕上玄犬反郭注爾雅云出西海大秦國有養者似犬多力說文分別從虤對爭貝也虤音五閑反論作𧷠誤也下辭姉反山海經云兕在舜葬東明水南狀如牛蒼黑色郭璞云一角青色重子斤說文如野牛而青象形字禽离頭同也論本作先字。",
        "from": "T54n2128_087:0866a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼魍勅知反考聲云則魑魅也。",
        "from": "T54n2128_087:0866a03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "派其上葩瓦反說文云派別水也從水𠂢𠂢亦聲𠂢音普賣反論作沠俗謬字。",
        "from": "T54n2128_087:0866a04_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有巢仕交反即古帝号亦云巢居。",
        "from": "T54n2128_087:0866a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燧人上音遂亦古帝号始出火化生為熟也。",
        "from": "T54n2128_087:0866a06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "韜聲上討刀反考聲云韜藏也說文從韋舀聲。",
        "from": "T54n2128_087:0866a07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "再黷同鹿反賈逵云黷媾也鄭注禮記云謂數而不禮也說文握持垢也從黑賣或作嬻。",
        "from": "T54n2128_087:0866a08_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摭實上正石反方言云摭取也說文作拓云捨也從手石聲也。",
        "from": "T54n2128_087:0866a09_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剖析上普厚反下星亦反說文析判木也分也從木斤聲論作㭊非也。",
        "from": "T54n2128_087:0866a10_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剏作上初向反賈逵云剏始也說文造法剏業也從并刃聲論作創俗用字刃音楚良反也。",
        "from": "T54n2128_087:0866a11_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "章醮焦肖反義已釋辯正論中亦作󲺎󲺍也。",
        "from": "T54n2128_087:0866a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "考覈𢖍戹反說文云考事實也西笮敷遮其辭得實覈也。",
        "from": "T54n2128_087:0866a13_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "之槧慙敢反說文云槧牘撲從木斬聲。",
        "from": "T54n2128_087:0866a14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "之觚古胡反馬融注論語云觚禮器也容二升案論文義即筆札之類所謂操觚染翰者也亦作㧓。",
        "from": "T54n2128_087:0866a15_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "籤題上妾閻反張戩云小簡也古者題簡以白事謂之籤今官名典尚主白事也說文驗人也從竹韱聲韱音尖。",
        "from": "T54n2128_087:0866a16_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "𥷴理上官六反考聲云窮也論作鞫俗字。",
        "from": "T54n2128_087:0866a17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鑠金上商若反論作爍失義也。",
        "from": "T54n2128_087:0866a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蝸角上寡花反莊子云有國於蝸之左角觸氏有國於蝸之右角者蠻氏時相與爭地而戰伏尸數万逐北旬有五日而返也顧野王云即螔蝓也說文羸也從虫咼聲。",
        "from": "T54n2128_087:0866a20_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "濛翳上木蓬反下繄討反道家天名也。",
        "from": "T54n2128_087:0866a21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諒亦上良尚反毛詩傳云諒信也眾信曰諒文字典說從言京聲也。",
        "from": "T54n2128_087:0866a22_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辯囿尤救反。",
        "from": "T54n2128_087:0866a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詞葩普巴反說文云葩花也從草皅聲。",
        "from": "T54n2128_087:0866a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "之煦吁句反廣雅云煦藝也溫也說文䒱也一云赤色之貌也一曰溫潤也從火昫聲。",
        "from": "T54n2128_087:0866b01_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "秋飆必遙反。",
        "from": "T54n2128_087:0866b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大噱強虐反說文云噱大笑也從口豦聲。",
        "from": "T54n2128_087:0866b03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "狙公上七余反獼猴之類案莊子云狙公謂眾狙曰與尒朝三而暮四何如眾狙皆怒朝四而暮三何如眾狙皆悅說文從犬且聲論作狙字也。",
        "from": "T54n2128_087:0866b05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "󳏠󳎲上古懷反廣雅云𦮃背也邪也說文戾也𦫳而北也比古文別也𦫳音摜下川兖反顧野王云󳎲差𢆰不齊也孟康云交也說文云對臥也從夕㐄相背也夕音雖㐄音跨楊雄作蹖也。",
        "from": "T54n2128_087:0866b07_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "2 卷中甄正論卷中󱈜璣上夕緣反下紀希反虞書云在󱈜璣玉衡以齊七政。",
        "from": "T54n2128_087:0866b09_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "駐景上誅具反蒼頡篇云駐止也說文立馬也從馬主聲也。",
        "from": "T54n2128_087:0866b10_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斂靄哀蓋反。",
        "from": "T54n2128_087:0866b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "確實上腔角反韓康伯注周易云確堅也古今正字從石寉聲論作確誤也。",
        "from": "T54n2128_087:0866b12_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斡運上剜活反王逸注楚詞云斡轉也說文從斗斡聲。",
        "from": "T54n2128_087:0866b13_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喑者上於衿反方言云齊宋之間謂啼極無聲曰喑俗謂喑瘂也說文從口音聲。",
        "from": "T54n2128_087:0866b14_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發洩仙拽反廣雅云洩漏也毛詩傳云發也說文從水曳聲亦作泄。",
        "from": "T54n2128_087:0866b15_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "醨俗里之反廣雅云醨薄酒說文酢也從酉离聲。",
        "from": "T54n2128_087:0866b16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "狙詐上七余反廣雅云狙獼猴也漢書云騁狙詐之兵顧野王云狙向也謂伺候也漢書從犬且聲。",
        "from": "T54n2128_087:0866b17_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月竁川汭反杜子春注周禮云竁謂葬穿壙也今南陽名穿地為竁古今正字從穴毳聲也。",
        "from": "T54n2128_087:0866b18_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蔕芙蕖上低計反說文云蔕瓜當也從草帶聲中音扶下巨魚反則蓮荷是。",
        "from": "T54n2128_087:0866b19_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未攄褚於反廣雅云攄張也舒也古今正字從手慮聲。",
        "from": "T54n2128_087:0866b20_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闔閭上含獵反下呂居反也。",
        "from": "T54n2128_087:0866b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賷五符上濟齊反持也工作齎。",
        "from": "T54n2128_087:0866b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雷霆上盧堆反正作靁下定丁反爾雅云疾雷為霆郭璞云雷之急激者也蒼頡篇霹靂也說文云雷餘聲也鈴鈴所以提出万物也從雨廷聲。",
        "from": "T54n2128_087:0866b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "之玷音點。",
        "from": "T54n2128_087:0866c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "驪珠上里知反則所謂驪龍之珠。",
        "from": "T54n2128_087:0866c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "參糅挐救反鄭注儀禮云糅雜也正作󲜛說文云從米丑聲。",
        "from": "T54n2128_087:0866c03_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摳衣上口溝反禮記云兩手摳衣去齊尺顧野王云謂以手挈衣前也廣疋舉也古今正字從手區聲論從木作樞非也。",
        "from": "T54n2128_087:0866c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "撤軔上緾列反博雅云撤發也下尼震反王逸注楚詞云軔轉也輪也輪轉於地說文礙車也從車刃聲。",
        "from": "T54n2128_087:0866c06_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "踠足上冤阮反張戩云足未騁也折也。",
        "from": "T54n2128_087:0866c07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沈艎晃光反埤蒼云艅艎也吳船名也吳公子先與楚戰止其乘舟即此也艅音餘。",
        "from": "T54n2128_087:0866c08_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬彙為貴反義已具釋崇正錄中。",
        "from": "T54n2128_087:0866c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傲物上吾到反孔注尚書云慢也論從心作慠義同。",
        "from": "T54n2128_087:0866c10_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏羲許羈反亦号庖羲論作󳉵亦同。",
        "from": "T54n2128_087:0866c11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "邅迴上展連反王逸注楚辭云邅轉也古今正字從辵亶聲。",
        "from": "T54n2128_087:0866c12_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稽叔夜上系雞反晉人竹林七賢之一者。",
        "from": "T54n2128_087:0866c13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "㡿神上音尺正體字論作󲆛俗字也。",
        "from": "T54n2128_087:0866c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攘災上壤章反考聲云止也除也下宰才反正體字論作灾俗字也。",
        "from": "T54n2128_087:0866c15_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "紋綵上吻分反考聲云吳越謂小綾為紋下猜宰反考聲云繒帛有色者也。",
        "from": "T54n2128_087:0866c16_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "3 卷下甄正論卷下恩吻文粉反說文云吻口邊也從口勿聲。",
        "from": "T54n2128_087:0866c18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爝火上將藥反字書云𥯤炬火也說文從火爵聲。",
        "from": "T54n2128_087:0866c19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曦景上喜羈反。",
        "from": "T54n2128_087:0866c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "層曜上賊登反郭璞注山海經云層重也文字典說從尸曾聲。",
        "from": "T54n2128_087:0866c21_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吸氣上歆邑反說文吸內息也從口及聲論作喰俗字。",
        "from": "T54n2128_087:0866c22_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "𧪖昚古慎字。",
        "from": "T54n2128_087:0866c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "炳然上兵皿反鹿疋云炳明也說文從火丙聲或作昺。",
        "from": "T54n2128_087:0866c24_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五祚臧洛反鄭箋詩云祚櫟也漢武帝宮名。",
        "from": "T54n2128_087:0867a01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雲褐寒遏反方言楚人謂袍為褐也言道家多於衣上畫作雲霞之氣也。",
        "from": "T54n2128_087:0867a02_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "霓裳上鷁奚反王逸注楚辭云霓雲之有色似龍者郭注爾雅云霓雌虹也亦於裳上畫雜色閒錯或青或赤日暈似虹霓也說文屈虹陰氣從雨兒聲。",
        "from": "T54n2128_087:0867a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各騖無付反顧野王云騖疾馳也說文從馬務聲。",
        "from": "T54n2128_087:0867a05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逗緣上頭陋反顧野王云逗亦住也漢書音議云逗曲行避敵也說文止也從辵豆聲也。",
        "from": "T54n2128_087:0867a06_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "涓棘上決玄反下矝力反二人名也則師涓棘成子也。",
        "from": "T54n2128_087:0867a07_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "締賞上提戾反說文云締結也從糸帝聲。",
        "from": "T54n2128_087:0867a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壇纂上但丹反鄭玄注禮記封土為壇祭處也壇之言怛怛明皇也下鑽󲆃反󲆃音酸纂反案壇纂之式者每醮祭之時於壇四面立木節刻畫其土或毛者也若今楚人每有所禱則縛茅為把短截貫於竿上插立於地設香酒祈之祭訖方去也非纂集之義也。",
        "from": "T54n2128_087:0867a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "膏肓上音高下音荒杜注左傳云󳅍隔也顧野王云胷之󳅍心之上也說文從心上隔也從月從亾聲。",
        "from": "T54n2128_087:0867a12_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沈痼上朕林反下孤互反說文作㽽病也從疒古聲。",
        "from": "T54n2128_087:0867a13_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一槩該愛反鄭注禮記云槩量也可以平斗斛者說文從木既聲。",
        "from": "T54n2128_087:0867a14_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謇訥上居輦反周易云謇者難也方言吃也古今正字從言寒省聲下奴兀反包咸注論語云訥遲鈍也說文言難也從言內聲論作蹇誤。",
        "from": "T54n2128_087:0867a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摶搖上段鸞反下音遙莊子云北溟有魚其名曰鯤化而為鳥其名曰鵬將適南溟水激三千里摶扶搖而上者九萬若垂天之雲是其異也。",
        "from": "T54n2128_087:0867a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咳唾上開蓋反下吐臥反。",
        "from": "T54n2128_087:0867a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "削槀上相藥反下高老反則書草木本末刑削者也今見有槀草語也。",
        "from": "T54n2128_087:0867a20_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蒐狩上所流反下收宥反郭注爾雅云春獵曰蒐冬獵曰狩二時之獵名也。",
        "from": "T54n2128_087:0867a21_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "𢧵馘上前節反說文云𢧵斷也從戈雀聲下觥獲反毛詩傳云馘獲也不𦨕者殺而獻其耳曰馘鄭注禮記云生獲斷耳者也說文軍戰斷耳也從首或聲正從耳作聝。",
        "from": "T54n2128_087:0867a23_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宰犧喜羈反。",
        "from": "T54n2128_087:0867a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "符璽斯氏反鄭注周禮云璽印也亦作璽論作璽非也。",
        "from": "T54n2128_087:0867b01_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餼芉上希既反杜注左傳云牲生曰餼埤蒼云餼饋也古今正字從食氣聲。",
        "from": "T54n2128_087:0867b02_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "杜郵有求反鄭注禮記云郵表畷田畯所督約百姓於井閒之處也顧野王云境上行書舍也郭璞云道路經過所也亦作卸也。",
        "from": "T54n2128_087:0867b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分鑣表驕反說文云馬銜也從金麃聲。",
        "from": "T54n2128_087:0867b05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上𠑗音仙莊子云猒世而上僊乘彼白雲至于帝宮廣雅云化也說文長生僊去從人從遷省聲。",
        "from": "T54n2128_087:0867b06_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來𮚐豊諷反杜注左傳云𮚐助喪之物也車馬月𮚐古今正字從貝󰂛聲。",
        "from": "T54n2128_087:0867b07_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勦絕上焦小反考聲云勦絕也割也論從力作勦誤也。",
        "from": "T54n2128_087:0867b08_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朝菌逵殞反考聲云菌地蕈也莊子云朝菌不知晦朔。",
        "from": "T54n2128_087:0867b09_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大椿下黜倫反莊子云大椿之水八千歲為春八千歲為秋。",
        "from": "T54n2128_087:0867b10_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妍醜上齧堅反廣雅云妍好也說文忮也慧也從女幵聲幵音堅也論作妍俗也。",
        "from": "T54n2128_087:0867b11_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "習蓼聊鳥反菜名也。",
        "from": "T54n2128_087:0867b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隤光上隊雷反考聲云隤物下墜也說文墜下也從𨸏貴聲論作穨非也。",
        "from": "T54n2128_087:0867b13_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書紳音申論語云子張書諸紳紳大帶也。",
        "from": "T54n2128_087:0867b14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十門辯惑論1 卷上十門辯惑論卷上復禮上馮福反。",
        "from": "T54n2128_087:0867b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "󳋀紳上津信反。",
        "from": "T54n2128_087:0867b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅縠紅屋反。",
        "from": "T54n2128_087:0867b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迺眷上音乃鄭注儀禮云迺猶而也顧野王云往也說文從西乚聲乚者古文乃字也論從辵作迺俗用字非也下居媛反孔注尚書云眷視也毛詩傳顧野說文義同從目卷省聲論作睠俗字。",
        "from": "T54n2128_087:0867b20_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "矤兮上申忍反郭注爾雅云矧况也說文况也詞也從矢引省聲論作矧通俗字也。",
        "from": "T54n2128_087:0867b21_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煨燼上猥回反下詞盡反廣雅云煨熅也杜注左傳云燼火餘木也正作󱽃字也。",
        "from": "T54n2128_087:0867b22_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讙也暉𡊮反鄭注禮記云讙嚻聲類云譁也從言雚聲雚音觀。",
        "from": "T54n2128_087:0867b23_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "䤈雞上馨雞反󲻾中虫也一名蠛蠓也。",
        "from": "T54n2128_087:0867b24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大噱強虐反義已具釋甄正論上卷中。",
        "from": "T54n2128_087:0867c01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "絕膐下盧貯反賈逵云膂脊也說文從肉旅聲論作膂非也。",
        "from": "T54n2128_087:0867c02_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徼妙上古弔反說文從彳敫聲。",
        "from": "T54n2128_087:0867c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四瀛郢精反考聲云瀛海也謂四海也。",
        "from": "T54n2128_087:0867c04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輪奐歡貫反毛詩傳云彩明貌也說文云大也從廾敻省聲。",
        "from": "T54n2128_087:0867c05_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "美餗音速周易云鼎折足覆公餗顧野王云鼎實也文字典說從食束聲。",
        "from": "T54n2128_087:0867c06_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "穢圂上紆廢反亦作薉下魂困反蒼頡篇云圂豕所居也說文廁也從口豕在其中。",
        "from": "T54n2128_087:0867c07_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "止𧂟逵位反張戩云𧂟草器也可以運土也論語云為山九仞功虧一𧂟說文義同從草貴聲。",
        "from": "T54n2128_087:0867c08_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖䔏蒭瘦反杜注左傳云䔏副也考聲云廁也古今正字從草造聲論從竹作簉誤也。",
        "from": "T54n2128_087:0867c09_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伯𦆖了蕭反亦作󲍛。",
        "from": "T54n2128_087:0867c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有渰淹儼反毛詩傳云渰陰雲貌也古今正字從水弇聲弇亦作󱜋也。",
        "from": "T54n2128_087:0867c11_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樞機上昌珠反下紀希反。",
        "from": "T54n2128_087:0867c12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行褊鞭緬反顧野王云褊陿也說文從衣扁聲也。",
        "from": "T54n2128_087:0867c13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "㧖󳉏上鸎革反正作搹論作扼俗字也下𠛠𢍅反正體字亦作捥也作腕非也。",
        "from": "T54n2128_087:0867c14_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僥倖上皎堯反考聲云僥非分而求也說文從人堯聲下莖耿反考聲云󲿙徼也為尊貴所寵愛也古今正字從人從㚔聲字書作󲷶也。",
        "from": "T54n2128_087:0867c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "2 卷下十門辯惑論卷下瘡痟小遙反鄭注周禮云痟酸削也首疾頭痛也埤蒼渴病也說文義同從疒肖聲。",
        "from": "T54n2128_087:0867c18_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "茨棘上自咨反考聲云茨蒺䔧也聚也下矜憶反正體字也辯惑論作棘字非也。",
        "from": "T54n2128_087:0867c19_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鹵莽上盧覩反下摹補反。",
        "from": "T54n2128_087:0867c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "𦔐而殞軍反毛詩傳云除草也說文從耒員聲亦作𦓷論作耘通俗用。",
        "from": "T54n2128_087:0867c21_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不稂朗航反毛詩傳云稂童稂也郭璞云秀類說文禾粟生而不成謂之童稂正作蓈從廾郎聲。",
        "from": "T54n2128_087:0867c22_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如坻雉尼反說文云坻小渚也從土氐聲也。",
        "from": "T54n2128_087:0867c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薜荔上鼙計反下犁第反考聲香草也。",
        "from": "T54n2128_087:0867c24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "󱻍芙蓉上朅焉反方言南楚謂取曰󱻍蒼頡篇拔取也亦作㩙論作搴亦通俗字。",
        "from": "T54n2128_087:0868a01_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禾菽收六反俗字考聲云豆也正作此尗。",
        "from": "T54n2128_087:0868a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "麰󳃣上墨侯反下賴該反劉熈注孟子云麰麥有兩縫者也今齊北河特多春種夏熟廣雅麰大麥󳃣小麥也說文麰從麥牟聲古今正字云󳃣從麥來聲或作󳈩字書作䅘與論同。",
        "from": "T54n2128_087:0868a04_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今惰徒臥反說文正作憜云不敬也從心隋聲或省𨸏作惰與論同古文作媠。",
        "from": "T54n2128_087:0868a05_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口噤下琴禁反王逸注楚辭云噤閉口也說文云作唫從口金聲。",
        "from": "T54n2128_087:0868a06_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比轅遠元反考工記云車人為車凡為轅三其輪引三分其長二在前一在後說文輈也從車𡊮聲。",
        "from": "T54n2128_087:0868a07_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馳騁丑領反杜注左傳云騁𡗱也廣疋奔也古今正字義同從馬𦥚聲𦥚音匹丁反。",
        "from": "T54n2128_087:0868a08_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弑逆施志反白虎通云弑者何猶殺也言臣子殺其君父候伺可稍稍試之也說文從殺省式聲也。",
        "from": "T54n2128_087:0868a09_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "濫觴上藍淡反濡上也漬也下賞章反說文云觴觶也實曰觴虛曰觶從角󲅻聲󱶜文作󲽘觶音志論本作𨢩誤也。",
        "from": "T54n2128_087:0868a10_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不撓鐃巧反杜注左傳云撓亂也說文擾也從手堯聲。",
        "from": "T54n2128_087:0868a11_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾籟來代反案此籟非簫也是巖谷幽深竹木森𨗉輕風搖吹眾響泠泠所謂万籟也說文從竹賴聲。",
        "from": "T54n2128_087:0868a12_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸闡蚩善反韓康伯注周易云揚也蒼頡篇云開也。",
        "from": "T54n2128_087:0868a13_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "泯然上民引反。",
        "from": "T54n2128_087:0868a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "班倕上板蠻反下音垂二人名也。",
        "from": "T54n2128_087:0868a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舟檝尖󰪑反考聲云駕船具棹類也論從舟作艥非也。",
        "from": "T54n2128_087:0868a16_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫鷁電歷反水鳥也。",
        "from": "T54n2128_087:0868a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "芳橈繞昭反即掉也。",
        "from": "T54n2128_087:0868a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萆輟知劣反考聲云輟止也。",
        "from": "T54n2128_087:0868a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即剖普厚反說文云剖判木從刀咅聲論從手作掊非也。",
        "from": "T54n2128_087:0868a20_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恍兮上皇廣反張戩云戃恍虛曠貌。",
        "from": "T54n2128_087:0868a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迃哉禺俱反孔注尚書云迃僻也鄭玄云廣大也說文從辵亐聲。",
        "from": "T54n2128_087:0868a22_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憺尒上談濫反考聲云憺靜也安也論作澹水動貌非也。",
        "from": "T54n2128_087:0868a23_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沚清上音止蒼頡篇云沚小渚也案鑒於澄止則見其形流動則亂其影止則定也合作止從水者失其義也。",
        "from": "T54n2128_087:0868a24_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倐焉上舒六反。",
        "from": "T54n2128_087:0868b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而謐泯必反爾雅云謐靜也古今正字從言𥁑聲也𥁑音同上。",
        "from": "T54n2128_087:0868b02_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喟然上口愧反何晏注論語云喟歎聲說文太息也從口胃聲。",
        "from": "T54n2128_087:0868b03_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "井哇烏媧反即蝦蟇也。",
        "from": "T54n2128_087:0868b04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "棲甃上音西下鄒救反說文云井甃也從瓦秋聲。",
        "from": "T54n2128_087:0868b05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "澤鷃音晏賈逵注國語云鷃鳸也杜預云鶬鴳也以立春鳴立秋而去也正作鴳。",
        "from": "T54n2128_087:0868b06_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無󳎘盈跡反文字典說云󳍺厭也從攴𥇡聲。",
        "from": "T54n2128_087:0868b07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嘈囋上音曹下才曷反廣疋云聲也考聲云聲諠多貌也亦作󳄇囐並同。",
        "from": "T54n2128_087:0868b08_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煢𡠉上揆營反孔注尚書云煢單也鄭注周禮無兄弟曰煢毛詩傳無所依也說文從卂熒省聲或作惸與論同亦作嬛下理之反考聲云𡠉寡婦也論作釐誤也。",
        "from": "T54n2128_087:0868b10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昏󳐃丁念反孔注尚書云󳐃溺困於水灾也方言下也古今正字從土󲶭聲也。",
        "from": "T54n2128_087:0868b11_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馭曰上音御。",
        "from": "T54n2128_087:0868b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闃其上窺役反埤蒼云闃靜也古今正字從門狊聲。",
        "from": "T54n2128_087:0868b13_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "󲸚翦上坎甘反孔注尚書云󲸚勝也大傳云克也說文殺也今聲古文作勘論作戡俗通用字下煎鮮反截也。",
        "from": "T54n2128_087:0868b14_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "扈從上胡古反鄭注考工記云扈緩也𦵮綜云勇健貌廣疋使也下才用反。",
        "from": "T54n2128_087:0868b15_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髣髴上芳𠕀反下敷勿反。",
        "from": "T54n2128_087:0868b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩構上囊朗反賈逵云曩曏也爾疋夂也說文義同從日襄聲下鈎候反。",
        "from": "T54n2128_087:0868b17_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藹君子上埃蓋反考聲云藹容止貌也臣盡忠也。",
        "from": "T54n2128_087:0868b18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "邃鍵上雖翠反說文云𨗉深也從穴遂聲下乾偃反周禮司門掌受管鍵以啟閉國門鄭眾云鍵壯也方言謂籥為鍵說文從金建聲。",
        "from": "T54n2128_087:0868b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "探賾上菼南反下士革反。",
        "from": "T54n2128_087:0868b21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "訒兮上人振反論語云仁者其言也訒為之難言之得無訒乎說文頓也從言刃聲。",
        "from": "T54n2128_087:0868b22_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切經音義卷第八十七",
        "from": "T54n2128_087:0868b23_100",
        "context_with_next_sentence": false
    }
]