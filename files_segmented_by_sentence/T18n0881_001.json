[
    {
        "source_sentence": "賢劫十六尊",
        "from": "T18n0881_001:0339a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彼輪壇外，　　賢劫千佛中，",
        "from": "T18n0881_001:0339a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十六為上首，　　彌勒持軍持，",
        "from": "T18n0881_001:0339a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空蓮華眼，　　除憂梵夾相，",
        "from": "T18n0881_001:0339a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除惡三股杵，　　香象執蓮華，",
        "from": "T18n0881_001:0339a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大精進鏘戟，　　虛空藏寶光，",
        "from": "T18n0881_001:0339a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智幢幢幖幟，　　無量光蓮華，",
        "from": "T18n0881_001:0339a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢護持寶瓶，　　綱明綱傘蓋，",
        "from": "T18n0881_001:0339a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月光半月幢，　　無量意梵夾，",
        "from": "T18n0881_001:0339b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辨積持華雲，　　金剛藏獨股，",
        "from": "T18n0881_001:0339b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普賢五智印，　　種子初阿字，",
        "from": "T18n0881_001:0339b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吽字左按地。",
        "from": "T18n0881_001:0339b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼彼真言曰：",
        "from": "T18n0881_001:0339b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛界外院，　　地居空行天，",
        "from": "T18n0881_001:0339c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略說有五類，　　忿怒稱吽字，",
        "from": "T18n0881_001:0340a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安布二十天，　　芰荷以為座，",
        "from": "T18n0881_001:0340a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東北那羅延，　　執輪羅剎形，",
        "from": "T18n0881_001:0340a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱摩羅鈴契童子形，　　摧碎夜迦天，",
        "from": "T18n0881_001:0340a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傘蓋寶東門，　　梵天持紅蓮如天，",
        "from": "T18n0881_001:0340a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天帝獨股杵童子形，　　火天在東南赤色，",
        "from": "T18n0881_001:0340a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三角為標幟，　　日天童子形，",
        "from": "T18n0881_001:0340a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛食持鬘，　　焰魔檀荼印赤色，",
        "from": "T18n0881_001:0340a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "熒惑火光聚恐怖相赤色，　　西南羅剎主，",
        "from": "T18n0881_001:0340a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執鎚并眷屬，　　歲星天持棒，",
        "from": "T18n0881_001:0340a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛衣夜迦，　　西門持弓箭，",
        "from": "T18n0881_001:0340a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水天執羂索，　　次置於月天童子形，",
        "from": "T18n0881_001:0340a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風幢西北隅羅剎形灰色，　　金剛面執鉤黑，",
        "from": "T18n0881_001:0340a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "調伏金剛劍，　　門東多門天金色，",
        "from": "T18n0881_001:0340a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掌塔持寶棒，　　伊舍那戟印左持劫波羅。",
        "from": "T18n0881_001:0340a15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼彼真言曰：",
        "from": "T18n0881_001:0340a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是大天眾，　　威德有多門，",
        "from": "T18n0881_001:0340b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妃后並眷屬，　　皆在曼荼羅，",
        "from": "T18n0881_001:0340c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當具開示，　　供養夜迦天，",
        "from": "T18n0881_001:0340c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應居北門東。",
        "from": "T18n0881_001:0340c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼真言曰：",
        "from": "T18n0881_001:0340c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「􇩂唵􄙖縛􄦽日羅二合􄙘尾􄖩曩􄗱野􄑀迦􅁥娑嚩二合􄚩賀」十六尊終享和二壬戌年五月以京兆智積院之本，挍豐山小池坊什本，梓印行於世。",
        "from": "T18n0881_001:0340c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長谷妙音院寓居上毛沙門快道誌",
        "from": "T18n0881_001:0340c10_100",
        "context_with_next_sentence": false
    }
]