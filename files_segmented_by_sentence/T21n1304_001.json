[
    {
        "source_sentence": "宿曜儀軌一卷",
        "from": "T21n1304_001:0422b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一行撰",
        "from": "T21n1304_001:0422b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空藏印　合掌縛水指。風指如寶形。真言",
        "from": "T21n1304_001:0422b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰。",
        "from": "T21n1304_001:0422b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿伽沙一迦羅波伊野二唵麼哩三伽摩哩四母梨五娑嚩賀若人欲求福智。",
        "from": "T21n1304_001:0422b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當歸依此菩薩。",
        "from": "T21n1304_001:0422b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日月星。",
        "from": "T21n1304_001:0422b15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆虛空藏所變也。",
        "from": "T21n1304_001:0422b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊印　虛空合掌。",
        "from": "T21n1304_001:0422b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大指各押二水甲。",
        "from": "T21n1304_001:0422b17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二風屈捻空端。",
        "from": "T21n1304_001:0422b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1304_001:0422b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引阿引入惡聲味引羅吽佉左洛　若求息災當欲殄滅七種災難。",
        "from": "T21n1304_001:0422b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂日月薄喰。",
        "from": "T21n1304_001:0422b20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五星違失常度。",
        "from": "T21n1304_001:0422b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兵賊競起。",
        "from": "T21n1304_001:0422b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水旱不時風雨失度。",
        "from": "T21n1304_001:0422b21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡臣背逆損害國民。",
        "from": "T21n1304_001:0422b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "武狼惡獸食噉眾生。",
        "from": "T21n1304_001:0422b22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五穀不登。",
        "from": "T21n1304_001:0422b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是災禍急厄官府王難死厄怖等。",
        "from": "T21n1304_001:0422b23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆當消散。",
        "from": "T21n1304_001:0422b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普賢印　虛心合掌真言曰。",
        "from": "T21n1304_001:0422b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "支波啄一決定毘尼波啄二斷結鳥蘇波啄三盡生死此呪平旦誦七遍夜七遍。",
        "from": "T21n1304_001:0422b27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "厭魅野道蟲毒皆悉消滅。",
        "from": "T21n1304_001:0422b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能得身心慧三解脫。",
        "from": "T21n1304_001:0422b28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後生不受生死之身。",
        "from": "T21n1304_001:0422b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲得法身。",
        "from": "T21n1304_001:0422b29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內外國土怨賊。",
        "from": "T21n1304_001:0422b29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切惡人一切鬼神一切盜賊虎狼師子毒蟲惡獸聞此呪聲。",
        "from": "T21n1304_001:0422c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆口噤閉不相惱亂。",
        "from": "T21n1304_001:0422c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天變致厄惡夢災殃。",
        "from": "T21n1304_001:0422c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百鳥怪異自然消滅。",
        "from": "T21n1304_001:0422c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此呪功能不可說盡。",
        "from": "T21n1304_001:0422c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "延命印　左手捻火指空指。",
        "from": "T21n1304_001:0422c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如持蓮花。",
        "from": "T21n1304_001:0422c05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手直申如垂。",
        "from": "T21n1304_001:0422c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1304_001:0422c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引摩折羅喻碎某甲娑嚩賀若誦此呪延命除災。",
        "from": "T21n1304_001:0422c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝釋天印　右手作拳安腰。",
        "from": "T21n1304_001:0422c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手直豎相著。",
        "from": "T21n1304_001:0422c09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地水二指屈中節。",
        "from": "T21n1304_001:0422c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以風著火背。",
        "from": "T21n1304_001:0422c10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈空中節。",
        "from": "T21n1304_001:0422c10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1304_001:0422c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵因捺羅野娑嚩賀若誦此呪授與官位。",
        "from": "T21n1304_001:0422c12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿足所求。",
        "from": "T21n1304_001:0422c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘沙門天印　虛心合掌。",
        "from": "T21n1304_001:0422c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "交二空並竪。",
        "from": "T21n1304_001:0422c14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二風付二火側。",
        "from": "T21n1304_001:0422c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如鉤端相去一寸不著。",
        "from": "T21n1304_001:0422c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1304_001:0422c15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵吠室羅縛拏野娑嚩賀若誦此呪得大富饒。",
        "from": "T21n1304_001:0422c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晝夜守護。",
        "from": "T21n1304_001:0422c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日天真言曰。",
        "from": "T21n1304_001:0422c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨羅怛曩二合怛羅夜引野曩莫索里野薩嚩曩乞灑二合怛羅二合羅若野唵引阿謨伽烏名位設底娑嚩二合賀月天真言曰。",
        "from": "T21n1304_001:0422c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵戰上怛羅二合曩乞灑二合引怛羅二合羅惹野名位設底娑嚩二合賀火星真言曰。",
        "from": "T21n1304_001:0422c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿誐羅嚕儗野名位莎訶水星真言曰。",
        "from": "T21n1304_001:0422c27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵母馱曩乞殺二合怛羅二合弭曩名位契努摩莎賀木星真言曰。",
        "from": "T21n1304_001:0423a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵婆羅二合訶薩鉢二合底曩摩比跢嚩曩師名位摩攞嚩羅引馱二合寧娑嚩賀金星真言曰。",
        "from": "T21n1304_001:0423a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵戌羯羅誐馱嚩二合羅惹野名位室里二合迦里娑嚩賀土星真言曰。",
        "from": "T21n1304_001:0423a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引捨泥殺作二合羅曩乞殺二合怛羅二合跛羅二合訶摩曩嚕波野名位普瑟底二合迦里莎訶羅睺星真言曰。",
        "from": "T21n1304_001:0423a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵羅戶曩阿素羅二合邏惹野塞摩捨都曩野名位扇底迦里娑嚩賀計都星真言曰。",
        "from": "T21n1304_001:0423a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉二合計都曩乞殺二合怛羅二合邏惹野名位娑嚩賀九執曜天印　堅固合掌。",
        "from": "T21n1304_001:0423a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二空相並直竪。",
        "from": "T21n1304_001:0423a17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二風舒。",
        "from": "T21n1304_001:0423a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名本開立來去。",
        "from": "T21n1304_001:0423a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1304_001:0423a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一蘖羅二合係濕嚩二合二里耶三鉢羅鉢多四珠底五摩野六娑嚩賀二十八宿印　堅固合掌。",
        "from": "T21n1304_001:0423a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並二空直立當心。",
        "from": "T21n1304_001:0423a21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二空召之。",
        "from": "T21n1304_001:0423a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1304_001:0423a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵諾乞灑二合怛羅二合爼寧吉反蘇那儞曳莎訶先奉供虛空藏文殊普賢延命帝釋毘沙門等菩薩天等。",
        "from": "T21n1304_001:0423a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後勸請九曜二十八宿天北斗之中本命屬星及以泰山府君司命司祿供之。",
        "from": "T21n1304_001:0423a27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈乞除災延命消除厄害之事。",
        "from": "T21n1304_001:0423a28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耳。",
        "from": "T21n1304_001:0423a28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能吉祥真言曰。",
        "from": "T21n1304_001:0423a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨羅怛曩二合怛羅二合夜野一曩莫素摩薩羅嚩二合諾訖灑二合怛羅二合邏引惹上野者者都地波阿去引路迦羅野怛儞也意他努摩底跛努摩底薩賓上儞佉上細娑嚩賀誦此九曜息災大白衣觀音陀羅尼。",
        "from": "T21n1304_001:0423b06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若日月在人本命宮中。",
        "from": "T21n1304_001:0423b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及五星在本命宮鬪戰失度。",
        "from": "T21n1304_001:0423b07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可立大白衣觀音或文殊八字熾盛光佛頂等道場。",
        "from": "T21n1304_001:0423b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各依本法念誦。",
        "from": "T21n1304_001:0423b09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切災難自然消散。",
        "from": "T21n1304_001:0423b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切曜不吉祥誦此真言成能吉祥。",
        "from": "T21n1304_001:0423b10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北斗七星真言曰。",
        "from": "T21n1304_001:0423b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵颯跢曩而齒曩野伴惹密野二合染普他摩娑嚩二合弭曩名位羅訖山婆嚩都莎訶若羅睺計都暗行人本命星宮。",
        "from": "T21n1304_001:0423b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須誦此北斗真言。",
        "from": "T21n1304_001:0423b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來說。",
        "from": "T21n1304_001:0423b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破一切宿曜障吉祥。",
        "from": "T21n1304_001:0423b15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1304_001:0423b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩羅嚩諾剎怛羅二合三摩曳室里曳扇底迦俱嚕娑嚩賀金剛宿成就經。",
        "from": "T21n1304_001:0423b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉祥成就品曰。",
        "from": "T21n1304_001:0423b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今宣說世間成就妙祕法。",
        "from": "T21n1304_001:0423b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是諸曜運行虛空。",
        "from": "T21n1304_001:0423b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一若二三四五等。",
        "from": "T21n1304_001:0423b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨入眾生命宿對衝宿遷移宿大殺業宿安宿薄相宿奴婢宿。",
        "from": "T21n1304_001:0423b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作諸厄害。",
        "from": "T21n1304_001:0423b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四大惡曜。",
        "from": "T21n1304_001:0423b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂火曜土曜羅睺計都最重眾生。",
        "from": "T21n1304_001:0423b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時修諸福業廣施仁慈。",
        "from": "T21n1304_001:0423b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或依文殊八字真言。",
        "from": "T21n1304_001:0423b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或依熾盛光佛頂。",
        "from": "T21n1304_001:0423b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或依被葉衣觀音。",
        "from": "T21n1304_001:0423b25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或依一字王佛頂。",
        "from": "T21n1304_001:0423b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立大息災護摩壇場。",
        "from": "T21n1304_001:0423b26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各依本法念誦供養。",
        "from": "T21n1304_001:0423b27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切災難自然消滅。",
        "from": "T21n1304_001:0423b27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作成就法於心宿直日柳宿直日昴宿直日牛宿直日。",
        "from": "T21n1304_001:0423b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不簡日月吉凶。",
        "from": "T21n1304_001:0423b29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但於此宿直日於一日之中。",
        "from": "T21n1304_001:0423c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不食念誦滿一千八十遍。",
        "from": "T21n1304_001:0423c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有心願應時便渴大悉地。",
        "from": "T21n1304_001:0423c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若厄難時。",
        "from": "T21n1304_001:0423c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於本生日最相應。",
        "from": "T21n1304_001:0423c02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作木曜成就法者。",
        "from": "T21n1304_001:0423c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月十六日最為相應。",
        "from": "T21n1304_001:0423c03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作火曜成就法者。",
        "from": "T21n1304_001:0423c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即彼直日最為相應。",
        "from": "T21n1304_001:0423c04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作土曜成就法者。",
        "from": "T21n1304_001:0423c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每月七日最為相應。",
        "from": "T21n1304_001:0423c05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作金曜成就法者。",
        "from": "T21n1304_001:0423c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨所見方平旦最為相應。",
        "from": "T21n1304_001:0423c06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作水曜成就法者依水曜直日而作成就。",
        "from": "T21n1304_001:0423c07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作羅睺計都法者。",
        "from": "T21n1304_001:0423c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依本生日以為相應。",
        "from": "T21n1304_001:0423c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復供養本生宿者。",
        "from": "T21n1304_001:0423c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於本生年月。",
        "from": "T21n1304_001:0423c09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若本生日而可取成就。",
        "from": "T21n1304_001:0423c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若惡宿生每月供養。",
        "from": "T21n1304_001:0423c10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若三日七日供養直轉成吉祥直。",
        "from": "T21n1304_001:0423c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祕勿令知俗也。",
        "from": "T21n1304_001:0423c11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宿曜儀軌一卷",
        "from": "T21n1304_001:0423c12_100",
        "context_with_next_sentence": false
    }
]