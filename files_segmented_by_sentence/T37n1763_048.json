[
    {
        "source_sentence": "4大般涅槃經集解卷第四十八",
        "from": "T37n1763_048:0525a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　出通法緣起　廣辯離漏",
        "from": "T37n1763_048:0525a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　廣辯念漏　四毒蛇譬",
        "from": "T37n1763_048:0525a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　五栴陀羅譬　一詐親譬　空聚落譬　六大賊譬　河譬　草栰譬德王品之第四善男子！",
        "from": "T37n1763_048:0525a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人能書寫是經至是經難遇過優曇華。",
        "from": "T37n1763_048:0525a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案智秀曰：",
        "from": "T37n1763_048:0525a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向明如來非天非人亦天亦人，今出其事，以證不虛。",
        "from": "T37n1763_048:0525a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子！",
        "from": "T37n1763_048:0525a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我念過去無量無邊至唯能受持一偈文句。",
        "from": "T37n1763_048:0525a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案智秀曰：",
        "from": "T37n1763_048:0525a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣出緣起以證不虛，兼明所聞之偈以驗經力，又顯求法之至者也。",
        "from": "T37n1763_048:0525a25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來證涅槃永斷於生死至如來甚深祕密之德。",
        "from": "T37n1763_048:0525a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案智秀曰：",
        "from": "T37n1763_048:0525a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣歎經力，以顯離惡之勝緣也。",
        "from": "T37n1763_048:0525a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以是義故能受持者至況於如來是故非漏。",
        "from": "T37n1763_048:0525a29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案寶亮曰：",
        "from": "T37n1763_048:0525b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結離漏也。",
        "from": "T37n1763_048:0525b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生死即虛妄，依如來藏有，故不假遠避也。",
        "from": "T37n1763_048:0525b02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何親近漏？",
        "from": "T37n1763_048:0525b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切凡夫至是故如來不名有漏。",
        "from": "T37n1763_048:0525b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案寶亮曰：",
        "from": "T37n1763_048:0525b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若親近衣食，生人貪染，斷人慧命；",
        "from": "T37n1763_048:0525b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能遠離，則善法增長。",
        "from": "T37n1763_048:0525b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次善男子！",
        "from": "T37n1763_048:0525b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切凡夫至如來世尊非是有漏。",
        "from": "T37n1763_048:0525b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案僧亮曰：",
        "from": "T37n1763_048:0525b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受漏者，因三受後起。",
        "from": "T37n1763_048:0525b07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若就治道，應名定漏也。",
        "from": "T37n1763_048:0525b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦受後生煩惱為瞋覺，樂受後起貪覺也。",
        "from": "T37n1763_048:0525b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次善男子！",
        "from": "T37n1763_048:0525b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡夫若遇至捨離眾惡趣向善道。",
        "from": "T37n1763_048:0525b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案僧亮曰：",
        "from": "T37n1763_048:0525b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第七念漏也。",
        "from": "T37n1763_048:0525b11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起三受時未是煩惱，覺苦樂已更生邪念，念苦樂方生煩惱，故念為漏因。",
        "from": "T37n1763_048:0525b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖人覺苦樂已不生邪念，故不起生漏也。",
        "from": "T37n1763_048:0525b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為心身造種種苦者，為邪身心相，故無苦不更，明見身之過也。",
        "from": "T37n1763_048:0525b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰：",
        "from": "T37n1763_048:0525b15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下第七明念漏。",
        "from": "T37n1763_048:0525b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由邪念因緣故起漏，名之念漏。",
        "from": "T37n1763_048:0525b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若就治道，稱為行漏。",
        "from": "T37n1763_048:0525b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以言身心苦惱起種種惡者，夫苦惱之名本是果報。",
        "from": "T37n1763_048:0525b18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而凡夫愚癡，不達宿業所招，便呼嗟非分，謂苦不可忍。",
        "from": "T37n1763_048:0525b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既內狹顛倒，不以理自處，於輕苦之中發緣更重。",
        "from": "T37n1763_048:0525b20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若當識因果性體、前世因作，如此安心，豈容生漏？",
        "from": "T37n1763_048:0525b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正由物於此二途無解，又於生死之中妄生樂想、保著為實，邪念橫生，故起種種惡。",
        "from": "T37n1763_048:0525b23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若是聖人，以道居心，終日處苦不覺為苦、處樂不以為樂，是故不生漏也。",
        "from": "T37n1763_048:0525b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今菩薩覺過，故修假名法空，斷於苦本。",
        "from": "T37n1763_048:0525b26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是以此下就八事作五階譬辨，若安心所從於聖言，必諸漏永息、涅槃可期；",
        "from": "T37n1763_048:0525b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如其不爾，生死常續。",
        "from": "T37n1763_048:0525b28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八事者，從「觀四大」去訖「登上大山」。",
        "from": "T37n1763_048:0525b29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五階者，第一得假名空；",
        "from": "T37n1763_048:0525b29_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二得實法空；",
        "from": "T37n1763_048:0525c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三明兩空，成後八真無漏，斷見諦煩惱盡，得須陀洹果；",
        "from": "T37n1763_048:0525c02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四從得初果後，更進斷三界思惟結盡，至羅漢果；",
        "from": "T37n1763_048:0525c03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五從斷三界外惑盡，乃至得大涅槃也。",
        "from": "T37n1763_048:0525c04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子！",
        "from": "T37n1763_048:0525c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如有王以四毒蛇至心生惶怖捨篋逃走。",
        "from": "T37n1763_048:0525c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案僧亮曰：",
        "from": "T37n1763_048:0525c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一譬，以假名無明煩惱之主譬王。",
        "from": "T37n1763_048:0525c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經稱四大，譬四蛇也。",
        "from": "T37n1763_048:0525c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盛有漏身，以譬一篋。",
        "from": "T37n1763_048:0525c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使其將護，譬瞻養也。",
        "from": "T37n1763_048:0525c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "消息失所身壞命終，譬戮之都市也。",
        "from": "T37n1763_048:0525c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見老死之苦，譬恐怖。",
        "from": "T37n1763_048:0525c09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知無實我，譬逃走。",
        "from": "T37n1763_048:0525c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰：",
        "from": "T37n1763_048:0525c10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王譬佛也。",
        "from": "T37n1763_048:0525c10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四毒蛇者，喻四大也。",
        "from": "T37n1763_048:0525c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盛一篋者，如說四大以成一身也。",
        "from": "T37n1763_048:0525c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令人養食者，令觀此身作苦空無常。",
        "from": "T37n1763_048:0525c12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若就有中解者，欲依此四大身上得行道進德，故須將養也。",
        "from": "T37n1763_048:0525c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩洗其身者，即教作空觀，令識四大過，便善法續起，喻如摩洗也。",
        "from": "T37n1763_048:0525c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若使不觀四大是虛、愛著己身，必斷慧命，義如戮之都市也。",
        "from": "T37n1763_048:0525c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人聞王切令者，菩薩即承佛教，觀四大過也。",
        "from": "T37n1763_048:0525c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨篋逃走者，即諦觀此身，唯見四大常虛假，無有一實，故得假名空。",
        "from": "T37n1763_048:0525c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲論四大五陰及與六入，盡成眾生同得作假名空觀，但隨其初觀，捉法而入空，同之內凡夫。",
        "from": "T37n1763_048:0525c21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而文不可並說，故階級明之也。",
        "from": "T37n1763_048:0525c22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王時復遣五栴陀羅至見後五人遂疾捨去。",
        "from": "T37n1763_048:0525c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案僧亮曰：",
        "from": "T37n1763_048:0525c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二譬。",
        "from": "T37n1763_048:0525c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得五陰空、實法空，見陰是實，能劫善則，譬栴陀羅。",
        "from": "T37n1763_048:0525c25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智以斷惑，譬刀。",
        "from": "T37n1763_048:0525c25_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而害事交至，譬拔也。",
        "from": "T37n1763_048:0525c26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出空見有，譬迴顧。",
        "from": "T37n1763_048:0525c26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已見假空，解實則易，譬疾去也。",
        "from": "T37n1763_048:0525c27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰：",
        "from": "T37n1763_048:0525c27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬觀五陰作實法空觀也。",
        "from": "T37n1763_048:0525c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時五人以惡之方便至汝可來還其人不信。",
        "from": "T37n1763_048:0525c29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案僧亮曰：",
        "from": "T37n1763_048:0526a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三譬。",
        "from": "T37n1763_048:0526a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破二愛已，得假實二，諸結漸薄，唯實法愛難去，譬惡方便。",
        "from": "T37n1763_048:0526a02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入觀見空，譬藏刀。",
        "from": "T37n1763_048:0526a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出觀起愛，譬詐親。",
        "from": "T37n1763_048:0526a03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲令起著，譬還來。",
        "from": "T37n1763_048:0526a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀空斷愛，譬不信也。",
        "from": "T37n1763_048:0526a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰：",
        "from": "T37n1763_048:0526a04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖得假實二空解，然出有心中還於五陰上起愛，故更捉五陰而觀。",
        "from": "T37n1763_048:0526a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未能即得，喻如藏所持刀也。",
        "from": "T37n1763_048:0526a06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起愛從情，如詐親也。",
        "from": "T37n1763_048:0526a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "投一聚落欲自隱匿至求物不得即便坐地。",
        "from": "T37n1763_048:0526a08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案僧亮曰：",
        "from": "T37n1763_048:0526a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四譬。",
        "from": "T37n1763_048:0526a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既成初果，緣假起慢，不勤修道。",
        "from": "T37n1763_048:0526a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知內入假實，譬空聚。",
        "from": "T37n1763_048:0526a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "染著假名，不以患藏隱不見真我，譬無人。",
        "from": "T37n1763_048:0526a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不見實陰，譬無物。",
        "from": "T37n1763_048:0526a11_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不復進修，譬坐地也。",
        "from": "T37n1763_048:0526a12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰：",
        "from": "T37n1763_048:0526a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既得六入空，入無相行，斷見諦惑，證須陀洹果，是一止息處也。",
        "from": "T37n1763_048:0526a13_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞空中聲：",
        "from": "T37n1763_048:0526a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咄哉男子至汝當云何而得免之。",
        "from": "T37n1763_048:0526a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案僧亮曰：",
        "from": "T37n1763_048:0526a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五譬。",
        "from": "T37n1763_048:0526a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷二界思惟結，垂其先解為因，進求上果，譬聞空聲。",
        "from": "T37n1763_048:0526a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說其先解，譬無居民。",
        "from": "T37n1763_048:0526a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "染著假名，譬今夜。",
        "from": "T37n1763_048:0526a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因有內惑外塵得入，譬六賊。",
        "from": "T37n1763_048:0526a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能生眾結，譬命不全。",
        "from": "T37n1763_048:0526a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷慢成果，譬捨去也。",
        "from": "T37n1763_048:0526a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰：",
        "from": "T37n1763_048:0526a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得須陀洹果後，出觀心中，起思惟門惑受生。",
        "from": "T37n1763_048:0526a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既乘本見諦無漏解見，思惟門中受生，惑為過欲。",
        "from": "T37n1763_048:0526a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更觀六塵空，進斷所餘結故，義言如聞空中聲也。",
        "from": "T37n1763_048:0526a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時其人恐怖漸遂增復捨而去。",
        "from": "T37n1763_048:0526a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案僧亮曰：",
        "from": "T37n1763_048:0526a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復得六塵空解，無漏轉深，斷思惟門惑，三界結盡，證羅漢果也。",
        "from": "T37n1763_048:0526a25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "路值一河河水漂急至心意泰然恐怖消除。",
        "from": "T37n1763_048:0526a26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案僧亮曰：",
        "from": "T37n1763_048:0526a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六譬也。",
        "from": "T37n1763_048:0526a27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷無色界思惟及習。",
        "from": "T37n1763_048:0526a27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼煩惱難斷，譬河漂急也。",
        "from": "T37n1763_048:0526a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定多慧少，譬無船栰。",
        "from": "T37n1763_048:0526a28_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必假以道，譬取草木也。",
        "from": "T37n1763_048:0526a29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上地苦輕，有深定樂，而行者樂故，譬更思惟。",
        "from": "T37n1763_048:0526b01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知是苦已，譬推栰入河。",
        "from": "T37n1763_048:0526b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定道難用，必須眾善相助，譬身手皆動也。",
        "from": "T37n1763_048:0526b02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰：",
        "from": "T37n1763_048:0526b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷無明住地惑，得於佛果，乃通三界內外皆是河義，而今取無明住地惑難斷者為河也。",
        "from": "T37n1763_048:0526b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即取草木為栰者，於三界外既未有有治道，欲修行萬善以趣極果也。",
        "from": "T37n1763_048:0526b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從我設住此下，說蛇賊所害者，若指斷三界結盡而終未免於蛇賊。",
        "from": "T37n1763_048:0526b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既與已相隣，云何相免？",
        "from": "T37n1763_048:0526b08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寧當任無明住地，或牽於生死，終不為彼蛇賊所害也。",
        "from": "T37n1763_048:0526b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即推草栰者，思惟既定，便修萬行，運實智方便智，即達涅槃岸，登上高山也。",
        "from": "T37n1763_048:0526b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從觀身如篋下，還次第合向八事五階也。",
        "from": "T37n1763_048:0526b12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩摩訶薩得聞受持至背之馳趣修八聖道。",
        "from": "T37n1763_048:0526b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案僧亮曰：",
        "from": "T37n1763_048:0526b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀四大，合第一譬。",
        "from": "T37n1763_048:0526b14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四種毒者，見譬眠也，解譬身也，氣從口出譬聲香也，味從口入譬囓也。",
        "from": "T37n1763_048:0526b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五旃陀羅者即是五陰至涉路而去無所顧留。",
        "from": "T37n1763_048:0526b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案僧亮曰：",
        "from": "T37n1763_048:0526b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀五陰，合第二譬也。",
        "from": "T37n1763_048:0526b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詐親善者名為貪愛至及一詐親涉路不迴。",
        "from": "T37n1763_048:0526b19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_048:0526b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰觀愛貪，合第三譬也。",
        "from": "T37n1763_048:0526b20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見不見聞不聞，至後還不見者，入觀則了，出觀則生惑。",
        "from": "T37n1763_048:0526b21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二乘亦爾，本為向初果為譬，因說愛之難除耳。",
        "from": "T37n1763_048:0526b22_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空聚落者即內六入至及六大賊怖著正路。",
        "from": "T37n1763_048:0526b23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案僧亮曰：",
        "from": "T37n1763_048:0526b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀六入，合第四譬也。",
        "from": "T37n1763_048:0526b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡夫不生空想者，於聖理懸遠也。",
        "from": "T37n1763_048:0526b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六大賊者即外六塵至捨空聚落涉路而去。",
        "from": "T37n1763_048:0526b26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案僧亮曰：",
        "from": "T37n1763_048:0526b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "患從外來，劫內功德，譬之賊也。",
        "from": "T37n1763_048:0526b27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合第五譬也。",
        "from": "T37n1763_048:0526b28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "路值一河者即是煩惱至及一闡提悉皆不定。",
        "from": "T37n1763_048:0526c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案僧亮曰：",
        "from": "T37n1763_048:0526c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依譬次第，應觀斷無色思惟煩惱及習為河也。",
        "from": "T37n1763_048:0526c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合第六譬也。",
        "from": "T37n1763_048:0526c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大般涅槃經集解卷第四十八",
        "from": "T37n1763_048:0526c03_100",
        "context_with_next_sentence": false
    }
]