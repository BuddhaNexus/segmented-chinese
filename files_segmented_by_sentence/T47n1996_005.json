{
    "T47n1996_005:0698a10_0": "明覺禪師祖英集卷第五",
    "T47n1996_005:0698a12_0": "參學小師文政編",
    "T47n1996_005:0698a13_0": "師之形言也。",
    "T47n1996_005:0698a13_1": "且異乎陽春白雪碧雲清風者也。",
    "T47n1996_005:0698a14_2": "夫大圭不琢貴乎天真。",
    "T47n1996_005:0698a14_3": "至言不文尚於理實。",
    "T47n1996_005:0698a15_4": "乃世之衡鑑。",
    "T47n1996_005:0698a15_5": "豈智識而擬議哉。",
    "T47n1996_005:0698a15_6": "師自戾止翠峯雪竇。",
    "T47n1996_005:0698a16_7": "或先德言句淵密。",
    "T47n1996_005:0698a16_8": "師因而頌之。",
    "T47n1996_005:0698a16_9": "或感興懷別貽贈之作。",
    "T47n1996_005:0698a17_10": "固亦多矣。",
    "T47n1996_005:0698a17_11": "其有好道者。",
    "T47n1996_005:0698a18_12": "並錄而囊之。",
    "T47n1996_005:0698a18_13": "一日總緝成二百二十首。",
    "T47n1996_005:0698a18_14": "乃寫呈師。",
    "T47n1996_005:0698a19_15": "師曰。",
    "T47n1996_005:0698a19_16": "余偶興而作。",
    "T47n1996_005:0698a19_17": "寧存于本。",
    "T47n1996_005:0698a19_18": "不許行焉。",
    "T47n1996_005:0698a20_19": "禪者應曰。",
    "T47n1996_005:0698a20_20": "乃祖闈千載之芳烈也。",
    "T47n1996_005:0698a20_21": "勿輕舍諸。",
    "T47n1996_005:0698a21_22": "師察其慤志勉。",
    "T47n1996_005:0698a21_23": "弗獲已抑而從之。",
    "T47n1996_005:0698a21_24": "文政幸侍座机。",
    "T47n1996_005:0698a22_25": "輒述序引。",
    "T47n1996_005:0698a22_26": "用識歲時。",
    "T47n1996_005:0698a22_27": "炎宋天聖十年孟陬月。",
    "T47n1996_005:0698a23_28": "文政謹序。",
    "T47n1996_005:0698a24_0": "偈頌",
    "T47n1996_005:0698a25_0": "送寶相長老(并序)",
    "T47n1996_005:0698a26_0": "大師歡禪德。",
    "T47n1996_005:0698a26_1": "將赴丹丘辟命光闡宗乘。",
    "T47n1996_005:0698a26_2": "蓋時應必行。",
    "T47n1996_005:0698a27_3": "固不可抑留者也。",
    "T47n1996_005:0698a27_4": "且撫會之作肇曠絕之道。",
    "T47n1996_005:0698a28_5": "雖一凝一流一彼一此又何間然。",
    "T47n1996_005:0698a28_6": "率織蕪辭。",
    "T47n1996_005:0698a29_7": "以代贐別。",
    "T47n1996_005:0698b01_0": "奧域靈區存物外。",
    "T47n1996_005:0698b01_1": "獨標台嶺為絕概。",
    "T47n1996_005:0698b01_2": "掩勝潛奇列作屏。",
    "T47n1996_005:0698b02_3": "堆青寫碧深如黛。",
    "T47n1996_005:0698b02_4": "彤霞暖影生巖壁。",
    "T47n1996_005:0698b03_5": "香桂茂陰籠蘇石。",
    "T47n1996_005:0698b03_6": "赤松子也浪虛閑。",
    "T47n1996_005:0698b03_7": "白道猷兮大輕擲。",
    "T47n1996_005:0698b04_8": "曹溪有叟歸其中。",
    "T47n1996_005:0698b04_9": "風從虎兮雲從龍。",
    "T47n1996_005:0698b05_10": "乘興正值二三月。",
    "T47n1996_005:0698b05_11": "坐斷還依千萬峯。",
    "T47n1996_005:0698b05_12": "華飛飛日遲遲。",
    "T47n1996_005:0698b06_13": "清飈[颱-台+弗][颱-台+弗]吹無時。",
    "T47n1996_005:0698b06_14": "玲瓏八面自回合。",
    "T47n1996_005:0698b07_15": "峭峻一方誰敢窺。",
    "T47n1996_005:0698b07_16": "窺來須得乾坤眼。",
    "T47n1996_005:0698b07_17": "照古騰今謂非間。",
    "T47n1996_005:0698b08_18": "若能此去副全提。",
    "T47n1996_005:0698b08_19": "開發人天有何限。",
    "T47n1996_005:0698b10_0": "送法海長老",
    "T47n1996_005:0698b11_0": "常愛裴相國式芳塵。",
    "T47n1996_005:0698b11_1": "斷際高風慕要倫。",
    "T47n1996_005:0698b11_2": "擬欲事師為弟子。",
    "T47n1996_005:0698b12_3": "不知將法付何人。",
    "T47n1996_005:0698b12_4": "常愛李相國垂列星。",
    "T47n1996_005:0698b13_5": "藥嶠深源宅性靈。",
    "T47n1996_005:0698b13_6": "我來問道無餘說。",
    "T47n1996_005:0698b14_0": "雲在青天水在瓶。",
    "T47n1996_005:0698b14_1": "緬想當時二台輔。",
    "T47n1996_005:0698b14_2": "出鎮藩維訪諸祖。",
    "T47n1996_005:0698b15_3": "寥寥浮幻輕百年。",
    "T47n1996_005:0698b15_4": "落落宏規照千古。",
    "T47n1996_005:0698b16_5": "今聞仙都賢太守。",
    "T47n1996_005:0698b16_6": "入政褰帷聲浩浩。",
    "T47n1996_005:0698b16_7": "英佐一一分化條。",
    "T47n1996_005:0698b17_8": "文經武緯亦難討。",
    "T47n1996_005:0698b17_9": "遠遠牋函飛乳峯。",
    "T47n1996_005:0698b18_10": "選開士兮恢吾宗。",
    "T47n1996_005:0698b18_11": "覿夜光非震滄海。",
    "T47n1996_005:0698b18_12": "聆正音豈玩焦桐。",
    "T47n1996_005:0698b19_13": "徒誇麟龍自西自東。",
    "T47n1996_005:0698b19_14": "應排罔象得象。",
    "T47n1996_005:0698b20_15": "必須覺雄讓雄。",
    "T47n1996_005:0698b20_16": "今既塞請還也奇別。",
    "T47n1996_005:0698b21_0": "茫茫普熱紛紛下雪。",
    "T47n1996_005:0698b21_1": "倒流四河載發枯枿。",
    "T47n1996_005:0698b21_2": "卷舒立方外乾坤。",
    "T47n1996_005:0698b22_3": "縱橫掛域中日月。",
    "T47n1996_005:0698b22_4": "黃頭碧眼。",
    "T47n1996_005:0698b23_0": "知未知去去。",
    "T47n1996_005:0698b23_1": "憑誰繼清絕。",
    "T47n1996_005:0698b24_0": "送文政禪者",
    "T47n1996_005:0698b25_0": "古有焦桐音。",
    "T47n1996_005:0698b25_1": "聽寡不在彈。",
    "T47n1996_005:0698b25_2": "古有陽春曲。",
    "T47n1996_005:0698b25_3": "和寡不在言。",
    "T47n1996_005:0698b26_4": "言兮牙齒寒。",
    "T47n1996_005:0698b26_5": "未極離微根。",
    "T47n1996_005:0698b26_6": "彈兮歲月闌。",
    "T47n1996_005:0698b27_7": "未盡昇沈源。",
    "T47n1996_005:0698b27_8": "少林幾坐華木落。",
    "T47n1996_005:0698b27_9": "庾嶺獨行天地寬。",
    "T47n1996_005:0698b28_10": "因笑仲尼溫伯雪。",
    "T47n1996_005:0698b28_11": "傾蓋同途不同轍。",
    "T47n1996_005:0698b29_0": "麟兮鳳兮安可論。",
    "T47n1996_005:0698b29_1": "許兮巢兮復何說。",
    "T47n1996_005:0698b29_2": "秋光澄澄蟾印水。",
    "T47n1996_005:0698c01_3": "秋風蕭蕭葉初墜。",
    "T47n1996_005:0698c01_4": "送君高蹈誰不知。",
    "T47n1996_005:0698c02_5": "如曰不知則為貴。",
    "T47n1996_005:0698c03_0": "送昭敏首座",
    "T47n1996_005:0698c04_0": "君不見。",
    "T47n1996_005:0698c04_1": "鷲峯勝集百萬。",
    "T47n1996_005:0698c04_2": "茫茫等閑過。",
    "T47n1996_005:0698c04_3": "壞衲之外皆清墮。",
    "T47n1996_005:0698c05_4": "君又不見。",
    "T47n1996_005:0698c05_5": "熊嶺孤運歲月。",
    "T47n1996_005:0698c05_6": "索索艱難生。",
    "T47n1996_005:0698c06_7": "深雪之中有一箇。",
    "T47n1996_005:0698c06_8": "宛轉流落千餘年。",
    "T47n1996_005:0698c06_9": "危分嶮布空平闐。",
    "T47n1996_005:0698c07_10": "辯龍蛇兮眼何正。",
    "T47n1996_005:0698c07_11": "擒虎兕兮機不全。",
    "T47n1996_005:0698c08_12": "石窓四顧滄溟窄。",
    "T47n1996_005:0698c08_13": "寥寥不許白雲白。",
    "T47n1996_005:0698c09_0": "掣斷金鎖天麒麟。",
    "T47n1996_005:0698c09_1": "高舉鐵鞭擊三百。",
    "T47n1996_005:0698c09_2": "猶輕舍爭知也。",
    "T47n1996_005:0698c10_3": "別有七星光鬪射。",
    "T47n1996_005:0698c10_4": "風前把欲贈行人。",
    "T47n1996_005:0698c11_0": "將報不平繞天下。",
    "T47n1996_005:0698c12_0": "送知白禪者",
    "T47n1996_005:0698c13_0": "松不直棘不曲。",
    "T47n1996_005:0698c13_1": "誰笑卞和三獻玉。",
    "T47n1996_005:0698c13_2": "經天緯地太無端。",
    "T47n1996_005:0698c14_3": "邁古超今亦輕觸。",
    "T47n1996_005:0698c14_4": "靡覉束何必云。",
    "T47n1996_005:0698c14_5": "素範還還真規復復。",
    "T47n1996_005:0698c15_6": "栴檀葉落香風清。",
    "T47n1996_005:0698c15_7": "千里萬里長相逐。",
    "T47n1996_005:0698c17_0": "送勝因長老",
    "T47n1996_005:0698c18_0": "黃梅散席三百載。",
    "T47n1996_005:0698c18_1": "續焰聯芳事空在。",
    "T47n1996_005:0698c18_2": "宗兮派兮生異端。",
    "T47n1996_005:0698c19_3": "華兮葉兮太煩碎。",
    "T47n1996_005:0698c19_4": "韶陽間出多慷慨。",
    "T47n1996_005:0698c20_5": "權要雄雄曾絕待。",
    "T47n1996_005:0698c20_6": "曲木據位知幾何。",
    "T47n1996_005:0698c20_7": "利刀剪却令人愛。",
    "T47n1996_005:0698c21_8": "近還有箇披老衲。",
    "T47n1996_005:0698c21_9": "楚甸橫身風颯颯。",
    "T47n1996_005:0698c22_10": "鐵作一尋非等閑。",
    "T47n1996_005:0698c22_11": "壁立千仞須摧踏。",
    "T47n1996_005:0698c22_12": "報君知江南江北徒纍纍。",
    "T47n1996_005:0698c23_13": "鼇轉海運兮纖麟片甲。",
    "T47n1996_005:0698c24_14": "雷奔電驅兮寸毫尺氂。",
    "T47n1996_005:0698c24_15": "斯言勿謂存規矩。",
    "T47n1996_005:0698c25_0": "平不留兮險非取。",
    "T47n1996_005:0698c25_1": "周行獨立如便休。",
    "T47n1996_005:0698c25_2": "誰振宏綱照千古。",
    "T47n1996_005:0698c27_0": "送重郜禪者",
    "T47n1996_005:0698c28_0": "春雨如膏春雲如鶴。",
    "T47n1996_005:0698c28_1": "忽此忽彼乍休乍作。",
    "T47n1996_005:0698c28_2": "枯荄離離維風太遲。",
    "T47n1996_005:0698c29_3": "幽石片片遼空亦危。",
    "T47n1996_005:0698c29_4": "一華開五葉兮不相似。",
    "T47n1996_005:0699a01_5": "獨孤明夸還自知。",
    "T47n1996_005:0699a01_6": "還自知。",
    "T47n1996_005:0699a02_0": "歷魏遊梁徒爾為。",
    "T47n1996_005:0699a03_0": "送僧歸靈隱(因矚白雲無覉)",
    "T47n1996_005:0699a04_0": "白雲無覉冷淡清奇。",
    "T47n1996_005:0699a04_1": "雪格未可鶴態還卑。",
    "T47n1996_005:0699a04_2": "垂天沃日兮似結不結。",
    "T47n1996_005:0699a05_3": "為雨從龍夸後期必期。",
    "T47n1996_005:0699a06_0": "噫悠悠忽爾春風吹。",
    "T47n1996_005:0699a06_1": "南北東西唯我知。",
    "T47n1996_005:0699a06_2": "誰知。",
    "T47n1996_005:0699a07_0": "菡萏峯前布影時。",
    "T47n1996_005:0699a08_0": "送僧之石梁",
    "T47n1996_005:0699a09_0": "萬卉流芳不知春力。",
    "T47n1996_005:0699a09_1": "巖畔澗底蹙紅皺碧。",
    "T47n1996_005:0699a09_2": "乘興復誰同。",
    "T47n1996_005:0699a10_3": "孤蹤遠讎敵。",
    "T47n1996_005:0699a10_4": "君不見。",
    "T47n1996_005:0699a10_5": "五百聖者導雄機。",
    "T47n1996_005:0699a11_6": "靈峯晦育深無極。",
    "T47n1996_005:0699a11_7": "寒山老寒山老隨沈跡。",
    "T47n1996_005:0699a12_8": "迢迢此去須尋覓。",
    "T47n1996_005:0699a12_9": "華落華開獨望時。",
    "T47n1996_005:0699a12_10": "記取白雲抱幽石。",
    "T47n1996_005:0699a14_0": "送師旻禪者",
    "T47n1996_005:0699a15_0": "深巖寂寂披蘭芷。",
    "T47n1996_005:0699a15_1": "碧霧紅霞映流水。",
    "T47n1996_005:0699a15_2": "空生別我期未期。",
    "T47n1996_005:0699a16_3": "絕域殊方擬輕擬。",
    "T47n1996_005:0699a16_4": "堪笑歸嶺南奔馳。",
    "T47n1996_005:0699a17_5": "何鄙彼危急亂拋下。",
    "T47n1996_005:0699a17_6": "盡云提不起。",
    "T47n1996_005:0699a17_7": "伊予本自不將來。",
    "T47n1996_005:0699a18_8": "相送奚憑掛唇齒。",
    "T47n1996_005:0699a18_9": "旻禪客旻禪客師子子。",
    "T47n1996_005:0699a19_10": "應須落落存終始。",
    "T47n1996_005:0699a19_11": "君不見。",
    "T47n1996_005:0699a19_12": "古人有言兮。",
    "T47n1996_005:0699a20_13": "撲碎驪龍明月珠。",
    "T47n1996_005:0699a20_14": "大丈夫到如此。",
    "T47n1996_005:0699a20_15": "行行不用頻彈指。",
    "T47n1996_005:0699a22_0": "寄白雲長老",
    "T47n1996_005:0699a23_0": "八紘雲靜明寥沈。",
    "T47n1996_005:0699a23_1": "夜永松堂對寒月。",
    "T47n1996_005:0699a23_2": "凋殘片葉墜虛庭。",
    "T47n1996_005:0699a24_3": "冷寂何人立深雪。",
    "T47n1996_005:0699a24_4": "因憶錢唐郜禪者。",
    "T47n1996_005:0699a25_5": "十載巖栖曾未下。",
    "T47n1996_005:0699a25_6": "分飛誰謂絕相同。",
    "T47n1996_005:0699a25_7": "遠念冥冥欲奚寫。",
    "T47n1996_005:0699a26_8": "忽聞赴請之仙都。",
    "T47n1996_005:0699a26_9": "聲光藹藹登清途。",
    "T47n1996_005:0699a27_10": "孰云天驥驟方外。",
    "T47n1996_005:0699a27_11": "自笑大鵬離海隅。",
    "T47n1996_005:0699a28_0": "乾坤窄。",
    "T47n1996_005:0699a28_1": "乾坤窄。",
    "T47n1996_005:0699a28_2": "湛盧潛射斗牛白。",
    "T47n1996_005:0699a28_3": "茫茫無限未歸人。",
    "T47n1996_005:0699a29_4": "到必為時除點額。",
    "T47n1996_005:0699b01_0": "送智遷首座",
    "T47n1996_005:0699b02_0": "雲蘿杳杳藏巖曲。",
    "T47n1996_005:0699b02_1": "碧疊清飛冷相促。",
    "T47n1996_005:0699b02_2": "瘦藤輕衲休便休。",
    "T47n1996_005:0699b03_3": "短焰殘芳續何續。",
    "T47n1996_005:0699b03_4": "禪家本自冥覉絆。",
    "T47n1996_005:0699b04_5": "洲渚園林曾不憚。",
    "T47n1996_005:0699b04_6": "十影神駒立海涯。",
    "T47n1996_005:0699b04_7": "五色祥麟步天岸。",
    "T47n1996_005:0699b05_8": "君看取君看取。",
    "T47n1996_005:0699b05_9": "匝地茫茫有誰舉。",
    "T47n1996_005:0699b06_10": "瓴甋頻磨如未回。",
    "T47n1996_005:0699b06_11": "為吾深憶盧公語。",
    "T47n1996_005:0699b07_0": "送善暹首座",
    "T47n1996_005:0699b08_0": "名之基實之蒂。",
    "T47n1996_005:0699b08_1": "深兮固兮宛相繼。",
    "T47n1996_005:0699b08_2": "古之名也在希聲。",
    "T47n1996_005:0699b09_3": "今之實也同浮瞖。",
    "T47n1996_005:0699b09_4": "子州善卷之流也。",
    "T47n1996_005:0699b10_0": "堯驅舜馳讓無暇。",
    "T47n1996_005:0699b10_1": "歸去來兮歸不歸。",
    "T47n1996_005:0699b10_2": "到頭未出冥冥者。",
    "T47n1996_005:0699b11_3": "吾徒孰謂標奇絕。",
    "T47n1996_005:0699b11_4": "動靜憑君試甄別。",
    "T47n1996_005:0699b12_5": "葉零零兮秋暮半凋。",
    "T47n1996_005:0699b12_6": "華片片兮春暖齊發。",
    "T47n1996_005:0699b13_0": "暹禪老。",
    "T47n1996_005:0699b13_1": "暹禪老。",
    "T47n1996_005:0699b13_2": "意曾高曠排沽待忽致譏褒。",
    "T47n1996_005:0699b14_0": "天人列請兮屢輕笑。",
    "T47n1996_005:0699b14_1": "祖佛位卑兮還擬逃。",
    "T47n1996_005:0699b14_2": "我恐逃之逃不得。",
    "T47n1996_005:0699b15_3": "大方無外皆充塞。",
    "T47n1996_005:0699b15_4": "茫茫擾擾知何極。",
    "T47n1996_005:0699b16_5": "八面香風惹衣裓。",
    "T47n1996_005:0699b17_0": "送僧",
    "T47n1996_005:0699b18_0": "吳山碧。",
    "T47n1996_005:0699b18_1": "楚江碧。",
    "T47n1996_005:0699b18_2": "吳楚悠悠興何極。",
    "T47n1996_005:0699b18_3": "一尋寒木自為隣。",
    "T47n1996_005:0699b19_4": "三事秋雲更誰識。",
    "T47n1996_005:0699b19_5": "乾坤不是無知己。",
    "T47n1996_005:0699b20_0": "玉石休云辯真偽。",
    "T47n1996_005:0699b20_1": "待時沽譽漫淪生。",
    "T47n1996_005:0699b20_2": "晦跡韜光亦何意。",
    "T47n1996_005:0699b21_3": "春風急。",
    "T47n1996_005:0699b21_4": "春風急。",
    "T47n1996_005:0699b21_5": "八駿奔馳追不及。",
    "T47n1996_005:0699b22_0": "南北東西把定時。",
    "T47n1996_005:0699b22_1": "為君直上孤峯立(觀氣分枝非獨古有誠之言也)。",
    "T47n1996_005:0699b24_0": "頌藥山師子話送僧",
    "T47n1996_005:0699b25_0": "斥愬金毛師子子。",
    "T47n1996_005:0699b25_1": "栴檀林下青莎裏。",
    "T47n1996_005:0699b25_2": "置也置也威自全。",
    "T47n1996_005:0699b26_3": "一出六出眉剔起。",
    "T47n1996_005:0699b26_4": "非擬擬知幾幾。",
    "T47n1996_005:0699b27_0": "星流不問三千里。",
    "T47n1996_005:0699b27_1": "天外風清哮吼時。",
    "T47n1996_005:0699b27_2": "為君吸盡西江水(咄)。",
    "T47n1996_005:0699b29_0": "送秀大師",
    "T47n1996_005:0699c01_0": "巖竇宵寒擁山帔。",
    "T47n1996_005:0699c01_1": "月高古木霜禽睡。",
    "T47n1996_005:0699c01_2": "西庵禪者來扣門。",
    "T47n1996_005:0699c02_3": "別我凌晨下層翠。",
    "T47n1996_005:0699c02_4": "欲留不可留。",
    "T47n1996_005:0699c02_5": "寫意不及意。",
    "T47n1996_005:0699c03_6": "屈眴迢迢安足云。",
    "T47n1996_005:0699c03_7": "華偈聯聯太容易。",
    "T47n1996_005:0699c04_8": "君不見。",
    "T47n1996_005:0699c04_9": "劉陽叟絕希冀。",
    "T47n1996_005:0699c04_10": "送人只道無他事。",
    "T47n1996_005:0699c05_0": "行行會有知音知。",
    "T47n1996_005:0699c05_1": "何必清風動天地。",
    "T47n1996_005:0699c06_0": "送廣華嚴歸鷲峯",
    "T47n1996_005:0699c07_0": "海山孤僻非蓬島。",
    "T47n1996_005:0699c07_1": "霧冷雲深松桂老。",
    "T47n1996_005:0699c07_2": "有客凝冬何太高。",
    "T47n1996_005:0699c08_3": "巨野宵征苦相討。",
    "T47n1996_005:0699c08_4": "巖房杳杳凌寒空。",
    "T47n1996_005:0699c09_5": "氷霜落落分譚叢。",
    "T47n1996_005:0699c09_6": "誰云百城沈古月。",
    "T47n1996_005:0699c09_7": "自笑八面生清風。",
    "T47n1996_005:0699c10_8": "俄然別我還歸去。",
    "T47n1996_005:0699c10_9": "惠理之徒望回馭。",
    "T47n1996_005:0699c11_10": "重重無盡樓閣門。",
    "T47n1996_005:0699c11_11": "到必為時略輕據。",
    "T47n1996_005:0699c12_0": "送遠塵禪者",
    "T47n1996_005:0699c13_0": "衲卷殘雲風高絕隣。",
    "T47n1996_005:0699c13_1": "倚天照雪堪抗要津。",
    "T47n1996_005:0699c13_2": "八紘極目兮春山若黛。",
    "T47n1996_005:0699c14_3": "九野縱步兮汀草如茵。",
    "T47n1996_005:0699c15_0": "三十四老未輕識。",
    "T47n1996_005:0699c15_1": "凜然方外奚相親。",
    "T47n1996_005:0699c16_0": "送德隆山主",
    "T47n1996_005:0699c17_0": "霜葉凋殘巖風凜寒。",
    "T47n1996_005:0699c17_1": "彼之禪老忽下崇巒。",
    "T47n1996_005:0699c17_2": "衲有雲兮曾卷未卷。",
    "T47n1996_005:0699c18_3": "琴無絃分解彈不彈。",
    "T47n1996_005:0699c18_4": "迢迢既行宜聽斯語。",
    "T47n1996_005:0699c19_5": "明闇路岐生死洲渚。",
    "T47n1996_005:0699c19_6": "而今而後知不知。",
    "T47n1996_005:0699c20_7": "頹綱委地憑誰舉。",
    "T47n1996_005:0699c21_0": "送澄禪者",
    "T47n1996_005:0699c22_0": "春色依依襲爾原草。",
    "T47n1996_005:0699c22_1": "春風浩浩拂我窓牖。",
    "T47n1996_005:0699c22_2": "念此分飛贈無瓊玖。",
    "T47n1996_005:0699c23_3": "片片亂飄巖上梅。",
    "T47n1996_005:0699c23_4": "條條縱舞溪邊柳。",
    "T47n1996_005:0699c24_5": "澄禪澄禪聽斯言。",
    "T47n1996_005:0699c24_6": "古也今也行路難。",
    "T47n1996_005:0699c25_7": "知之者石火星流未急。",
    "T47n1996_005:0699c25_8": "不知者龍驥步驟曾寬。",
    "T47n1996_005:0699c26_9": "看看。",
    "T47n1996_005:0699c26_10": "孰云平地起波瀾。",
    "T47n1996_005:0699c27_0": "送惠儔禪者",
    "T47n1996_005:0699c28_0": "少林風規何大瀟灑。",
    "T47n1996_005:0699c28_1": "籠古罩今睥真睨假。",
    "T47n1996_005:0699c28_2": "誰云發機射虎。",
    "T47n1996_005:0699c29_3": "自笑品類觀馬。",
    "T47n1996_005:0699c29_4": "劍客茫茫不要呈。",
    "T47n1996_005:0700a01_5": "甑人往往須擒下。",
    "T47n1996_005:0700a01_6": "儔禪儔禪崢嶸象駕。",
    "T47n1996_005:0700a02_0": "送惠文禪者",
    "T47n1996_005:0700a03_0": "正法眼絕塵沙。",
    "T47n1996_005:0700a03_1": "二三四七水月空華。",
    "T47n1996_005:0700a03_2": "千燈續焰曾間五葉分披未葩。",
    "T47n1996_005:0700a04_3": "君不見。",
    "T47n1996_005:0700a04_4": "卷蓆百丈掩耳丹霞。",
    "T47n1996_005:0700a05_5": "龍行虎步爭孤立。",
    "T47n1996_005:0700a05_6": "盡同雲雨去無涯。",
    "T47n1996_005:0700a06_0": "文禪文禪騰煥吾家。",
    "T47n1996_005:0700a07_0": "送道成禪者",
    "T47n1996_005:0700a08_0": "曹溪流非止水。",
    "T47n1996_005:0700a08_1": "一點忽來千波自起。",
    "T47n1996_005:0700a08_2": "直須釣鼇釣鯨。",
    "T47n1996_005:0700a09_3": "莫問得皮得髓。",
    "T47n1996_005:0700a09_4": "君不見。",
    "T47n1996_005:0700a09_5": "石頭有言兮。",
    "T47n1996_005:0700a10_0": "聖不慕他靈不在已。",
    "T47n1996_005:0700a10_1": "成禪成禪誰家之子。",
    "T47n1996_005:0700a11_0": "送清演禪者",
    "T47n1996_005:0700a12_0": "我年老大心力衰微。",
    "T47n1996_005:0700a12_1": "贈別無語冥同振飛。",
    "T47n1996_005:0700a12_2": "因思古之送人有言。",
    "T47n1996_005:0700a13_3": "吾不知其殊途同歸。",
    "T47n1996_005:0700a13_4": "獨愛新豐曲騰清輝。",
    "T47n1996_005:0700a14_5": "寸草不生千萬里。",
    "T47n1996_005:0700a14_6": "出門春色共依依。",
    "T47n1996_005:0700a16_0": "送繼寶禪者",
    "T47n1996_005:0700a17_0": "寶非寶日杲杲。",
    "T47n1996_005:0700a17_1": "上上機無處討。",
    "T47n1996_005:0700a17_2": "赤水求來何太狂。",
    "T47n1996_005:0700a18_3": "荊山覓得苦相惱。",
    "T47n1996_005:0700a18_4": "不惱不狂排夜光。",
    "T47n1996_005:0700a18_5": "險惡道中為津梁。",
    "T47n1996_005:0700a20_0": "送小師元楚",
    "T47n1996_005:0700a21_0": "道之冥機一何相守。",
    "T47n1996_005:0700a21_1": "汝競光陰我親蒲柳。",
    "T47n1996_005:0700a21_2": "毋厚辨之奪席。",
    "T47n1996_005:0700a22_3": "毋薄愚之誦箒。",
    "T47n1996_005:0700a22_4": "深思彼伐木丁丁之聲。",
    "T47n1996_005:0700a23_5": "照古照今兮宜善求友。",
    "T47n1996_005:0700a24_0": "送清果禪者",
    "T47n1996_005:0700a25_0": "春雨濛濛春風[颱-台+弗][颱-台+弗]。",
    "T47n1996_005:0700a25_1": "動兮靜兮匪待時出。",
    "T47n1996_005:0700a25_2": "雲霞閑澹作性。",
    "T47n1996_005:0700a26_3": "金鐵冷落為骨。",
    "T47n1996_005:0700a26_4": "知我者謂我高蹈世表。",
    "T47n1996_005:0700a27_5": "不知我者謂我下視箇窟。",
    "T47n1996_005:0700a27_6": "道恣隨方情。",
    "T47n1996_005:0700a28_7": "融覊鎖紫栗。",
    "T47n1996_005:0700a28_8": "一尋青山萬朵行行思。",
    "T47n1996_005:0700a28_9": "古人之言無可不可。",
    "T47n1996_005:0700a29_10": "南北東西但唯我。",
    "T47n1996_005:0700b01_0": "酬行奯長老",
    "T47n1996_005:0700b02_0": "黃金為骨松為恣。",
    "T47n1996_005:0700b02_1": "道高曾鄙天人師。",
    "T47n1996_005:0700b02_2": "有言遺我千古奇。",
    "T47n1996_005:0700b03_3": "無人知石虎吞却木羊兒。",
    "T47n1996_005:0700b04_0": "至人不器",
    "T47n1996_005:0700b05_0": "誰當機舉不賺亦還稀。",
    "T47n1996_005:0700b05_1": "摧殘峭峻銷爍玄微。",
    "T47n1996_005:0700b06_0": "重關曾巨闢。",
    "T47n1996_005:0700b06_1": "作者未同歸。",
    "T47n1996_005:0700b06_2": "玉兔乍圓乍闕。",
    "T47n1996_005:0700b06_3": "金烏似飛不飛。",
    "T47n1996_005:0700b07_4": "盧老不知何處去。",
    "T47n1996_005:0700b07_5": "白雲流水共依依。",
    "T47n1996_005:0700b09_0": "因事示眾",
    "T47n1996_005:0700b10_0": "石本落落玉自碌碌。",
    "T47n1996_005:0700b10_1": "古之今之一何[言*斯]速。",
    "T47n1996_005:0700b10_2": "師子不咬麒麟。",
    "T47n1996_005:0700b11_3": "猛虎不食伏肉。",
    "T47n1996_005:0700b11_4": "君不見。",
    "T47n1996_005:0700b11_5": "洞庭孤島煙浪深。",
    "T47n1996_005:0700b12_6": "木馬追風有人識。",
    "T47n1996_005:0700b13_0": "日暮遊東澗(五首)",
    "T47n1996_005:0700b14_0": "極目生晚照。",
    "T47n1996_005:0700b14_1": "溪雲偶成朵。",
    "T47n1996_005:0700b14_2": "大朴曾未分。",
    "T47n1996_005:0700b14_3": "青山自唯我。",
    "T47n1996_005:0700b16_0": "極目生晚照。",
    "T47n1996_005:0700b16_1": "遠樹籠微陰。",
    "T47n1996_005:0700b16_2": "誰知清淺流。",
    "T47n1996_005:0700b16_3": "別有滄海深。",
    "T47n1996_005:0700b18_0": "極目生晚照。",
    "T47n1996_005:0700b18_1": "幽情眷蘭芷。",
    "T47n1996_005:0700b18_2": "白蘋葉裏風。",
    "T47n1996_005:0700b18_3": "不在秋江起。",
    "T47n1996_005:0700b20_0": "極目生晚照。",
    "T47n1996_005:0700b20_1": "步影何遲遲。",
    "T47n1996_005:0700b20_2": "歸禽古木中。",
    "T47n1996_005:0700b20_3": "相對頻相窺。",
    "T47n1996_005:0700b22_0": "極目生晚照。",
    "T47n1996_005:0700b22_1": "蓬萊匪仙境。",
    "T47n1996_005:0700b22_2": "釣得十二鼇。",
    "T47n1996_005:0700b22_3": "重來謝孤影。",
    "T47n1996_005:0700b24_0": "思歸引(三首)",
    "T47n1996_005:0700b25_0": "一住翠峯頂。",
    "T47n1996_005:0700b25_1": "兩見溪草綠。",
    "T47n1996_005:0700b25_2": "不知朝市間。",
    "T47n1996_005:0700b25_3": "幾番生榮辱。",
    "T47n1996_005:0700b26_4": "蕭條巖上雲。",
    "T47n1996_005:0700b26_5": "冷淡水邊竹。",
    "T47n1996_005:0700b26_6": "報誰歸去來。",
    "T47n1996_005:0700b27_7": "向此空踟躅。",
    "T47n1996_005:0700b28_0": "常憶在廬山。",
    "T47n1996_005:0700b28_1": "隨時寄瓶錫。",
    "T47n1996_005:0700b28_2": "五百與一千。",
    "T47n1996_005:0700b28_3": "聚頭同遣日。",
    "T47n1996_005:0700b29_4": "猿攀影未回。",
    "T47n1996_005:0700b29_5": "鶴望情還失。",
    "T47n1996_005:0700b29_6": "教他王老師。",
    "T47n1996_005:0700c01_7": "癡鈍無處覓。",
    "T47n1996_005:0700c02_0": "時雨灑如膏。",
    "T47n1996_005:0700c02_1": "萬卉皆滋益。",
    "T47n1996_005:0700c02_2": "枯根甘自休。",
    "T47n1996_005:0700c02_3": "也似春無力。",
    "T47n1996_005:0700c03_4": "耕夫曉尚眠。",
    "T47n1996_005:0700c03_5": "蠶婦夜多息。",
    "T47n1996_005:0700c03_6": "從茲家業荒。",
    "T47n1996_005:0700c04_7": "共落風塵跡。",
    "T47n1996_005:0700c05_0": "送蘊歡禪者西上",
    "T47n1996_005:0700c06_0": "金闕路曾遙。",
    "T47n1996_005:0700c06_1": "行行值開泰。",
    "T47n1996_005:0700c06_2": "石房雲未閑。",
    "T47n1996_005:0700c06_3": "杳杳若相待。",
    "T47n1996_005:0700c07_4": "高蹤逾履水。",
    "T47n1996_005:0700c07_5": "何人不傾蓋。",
    "T47n1996_005:0700c07_6": "早晚承帝恩。",
    "T47n1996_005:0700c08_7": "再卜林泉會。",
    "T47n1996_005:0700c09_0": "送僧",
    "T47n1996_005:0700c10_0": "春雲情既高。",
    "T47n1996_005:0700c10_1": "片段飛虛碧。",
    "T47n1996_005:0700c10_2": "去留機未消。",
    "T47n1996_005:0700c10_3": "今古望還積。",
    "T47n1996_005:0700c11_4": "澄澄天影回。",
    "T47n1996_005:0700c11_5": "杳杳地形直。",
    "T47n1996_005:0700c11_6": "別夜共相思。",
    "T47n1996_005:0700c12_7": "誰栖此泉石。",
    "T47n1996_005:0700c13_0": "法爾不爾",
    "T47n1996_005:0700c14_0": "夏雲多奇峯。",
    "T47n1996_005:0700c14_1": "乾城冷相映。",
    "T47n1996_005:0700c14_2": "借問諸禪僧。",
    "T47n1996_005:0700c14_3": "那箇堪憑定。",
    "T47n1996_005:0700c15_4": "乾城高鎖月。",
    "T47n1996_005:0700c15_5": "夏雲欲為雨。",
    "T47n1996_005:0700c15_6": "若謂非全功。",
    "T47n1996_005:0700c16_7": "子細看規矩。",
    "T47n1996_005:0700c17_0": "送諸方化主",
    "T47n1996_005:0700c18_0": "空巖暖律回。",
    "T47n1996_005:0700c18_1": "極目望還普。",
    "T47n1996_005:0700c18_2": "數點方外雲。",
    "T47n1996_005:0700c18_3": "幾處人間雨。",
    "T47n1996_005:0700c19_4": "寥寥滄海月。",
    "T47n1996_005:0700c19_5": "依依少林祖。",
    "T47n1996_005:0700c19_6": "去必示勞生。",
    "T47n1996_005:0700c20_7": "清風立千古。",
    "T47n1996_005:0700c21_0": "劉禹端公問雲居雨從何來。",
    "T47n1996_005:0700c21_1": "東平問官人風作何色。",
    "T47n1996_005:0700c22_2": "雨從何來風作何色。",
    "T47n1996_005:0700c22_3": "龍門萬仞曾留宿客。",
    "T47n1996_005:0700c23_4": "進退相將誰遭點額。",
    "T47n1996_005:0700c24_0": "風作何色雨從何來。",
    "T47n1996_005:0700c24_1": "不用彈指樓閣門開。",
    "T47n1996_005:0700c24_2": "波波稜稜南方未回。",
    "T47n1996_005:0700c26_0": "送僧",
    "T47n1996_005:0700c27_0": "松風清未休。",
    "T47n1996_005:0700c27_1": "水月淡相對。",
    "T47n1996_005:0700c27_2": "去來非等閑。",
    "T47n1996_005:0700c27_3": "必許孤雲會。",
    "T47n1996_005:0700c29_0": "頌雲門九九八十一(二首)",
    "T47n1996_005:0701a01_0": "三三九九八十一。",
    "T47n1996_005:0701a01_1": "一一觀風隨召出。",
    "T47n1996_005:0701a01_2": "千古有誰同共知。",
    "T47n1996_005:0701a02_3": "一毛師子眾毛畢。",
    "T47n1996_005:0701a03_0": "九九八十一。",
    "T47n1996_005:0701a03_1": "大勳不竪賞。",
    "T47n1996_005:0701a03_2": "若謂無誵訛。",
    "T47n1996_005:0701a03_3": "金剛曾合掌。",
    "T47n1996_005:0701a05_0": "烏龍和尚",
    "T47n1996_005:0701a06_0": "空巖清夜坐。",
    "T47n1996_005:0701a06_1": "蘚徑積深雪。",
    "T47n1996_005:0701a06_2": "瞑目思古人。",
    "T47n1996_005:0701a06_3": "徹曙落殘月。",
    "T47n1996_005:0701a07_4": "童敲石磬寒。",
    "T47n1996_005:0701a07_5": "猿掛枯枝折。",
    "T47n1996_005:0701a07_6": "杳杳無限情。",
    "T47n1996_005:0701a08_7": "分明向誰說。",
    "T47n1996_005:0701a09_0": "秋日送僧",
    "T47n1996_005:0701a10_0": "邊雁影邪寒蟬聲速。",
    "T47n1996_005:0701a10_1": "乘時毳流遠別巖谷。",
    "T47n1996_005:0701a10_2": "林驚一葉兮微風觸袖。",
    "T47n1996_005:0701a11_3": "水肅百川兮片月在目。",
    "T47n1996_005:0701a12_0": "因憶象骨老師曾送人。",
    "T47n1996_005:0701a12_1": "行行不謂抽金鏃。",
    "T47n1996_005:0701a13_0": "早參示眾",
    "T47n1996_005:0701a14_0": "曉天雲靜濃霜白。",
    "T47n1996_005:0701a14_1": "千峯萬峯鎖寒色。",
    "T47n1996_005:0701a14_2": "驪龍失珠知不知。",
    "T47n1996_005:0701a15_3": "無限平人遭點額。",
    "T47n1996_005:0701a16_0": "春風辭寄武威石祕校",
    "T47n1996_005:0701a17_0": "春風何蕭蕭。",
    "T47n1996_005:0701a17_1": "和雨復兼雪。",
    "T47n1996_005:0701a17_2": "折華功未深。",
    "T47n1996_005:0701a17_3": "偃草勢曾烈。",
    "T47n1996_005:0701a18_4": "毘城癡愛老。",
    "T47n1996_005:0701a18_5": "怯寒對清拙。",
    "T47n1996_005:0701a18_6": "襄巖影響士。",
    "T47n1996_005:0701a19_7": "難御同孤劣。",
    "T47n1996_005:0701a19_8": "鼇峯人不來。",
    "T47n1996_005:0701a19_9": "柴門亦休閉。",
    "T47n1996_005:0701a19_10": "松頭栗鼠下。",
    "T47n1996_005:0701a20_11": "時把藤床嚙。",
    "T47n1996_005:0701a20_12": "庭際霜禽歸。",
    "T47n1996_005:0701a20_13": "屢啄苔錢闕。",
    "T47n1996_005:0701a21_14": "一旦春風息。",
    "T47n1996_005:0701a21_15": "暖日生林樾。",
    "T47n1996_005:0701a21_16": "幽徑磐石上。",
    "T47n1996_005:0701a22_0": "拄笻行且歇。",
    "T47n1996_005:0701a22_1": "無絃兮莫彈。",
    "T47n1996_005:0701a22_2": "有語兮存舌。",
    "T47n1996_005:0701a22_3": "冷落流水聲。",
    "T47n1996_005:0701a23_4": "古之若為說。",
    "T47n1996_005:0701a23_5": "凋殘早梅樹。",
    "T47n1996_005:0701a23_6": "今之若為別。",
    "T47n1996_005:0701a24_7": "俯仰身力輕。",
    "T47n1996_005:0701a24_8": "翻憶春風切。",
    "T47n1996_005:0701a24_9": "為吾吹却塵欲革分岐轍。",
    "T47n1996_005:0701a25_10": "為吾吹却雲。",
    "T47n1996_005:0701a25_11": "欲問遼空月。",
    "T47n1996_005:0701a25_12": "不知天地間。",
    "T47n1996_005:0701a26_13": "堪為誰交結。",
    "T47n1996_005:0701a27_0": "送百丈專使",
    "T47n1996_005:0701a28_0": "大雄孤頂曾遐舉。",
    "T47n1996_005:0701a28_1": "遍索諸方誰敢拒。",
    "T47n1996_005:0701a28_2": "乳竇峯前捋虎鬚。",
    "T47n1996_005:0701a29_3": "再得完全又歸去。",
    "T47n1996_005:0701b01_0": "送清素禪者之金華",
    "T47n1996_005:0701b02_0": "古策風高瓶浪闊。",
    "T47n1996_005:0701b02_1": "春雲片段分清絕。",
    "T47n1996_005:0701b02_2": "金盆後夜孤頂寒。",
    "T47n1996_005:0701b03_3": "去去誰同落殘月。",
    "T47n1996_005:0701b04_0": "擬寒山送僧",
    "T47n1996_005:0701b05_0": "擇木有靈禽。",
    "T47n1996_005:0701b05_1": "寒空寄羽翼。",
    "T47n1996_005:0701b05_2": "不止蓬萊山。",
    "T47n1996_005:0701b05_3": "冥冥去何極。",
    "T47n1996_005:0701b07_0": "送如香大師",
    "T47n1996_005:0701b08_0": "栴檀葉落雨初歇。",
    "T47n1996_005:0701b08_1": "天外風清亦何別。",
    "T47n1996_005:0701b08_2": "後夜蓮城溪月寒。",
    "T47n1996_005:0701b09_3": "孤光誰共倚寥沈。",
    "T47n1996_005:0701b10_0": "寄于祕丞(二首)",
    "T47n1996_005:0701b11_0": "石徑通巖竇。",
    "T47n1996_005:0701b11_1": "引步藏欹側。",
    "T47n1996_005:0701b11_2": "蓬萊人不來。",
    "T47n1996_005:0701b11_3": "掃盡蒼苔色。",
    "T47n1996_005:0701b13_0": "飛瀑千萬層。",
    "T47n1996_005:0701b13_1": "五月狀氷雪。",
    "T47n1996_005:0701b13_2": "將期雲霧開。",
    "T47n1996_005:0701b13_3": "永夜對孤月。",
    "T47n1996_005:0701b15_0": "再成古詩",
    "T47n1996_005:0701b16_0": "霜華一鑷中。",
    "T47n1996_005:0701b16_1": "玉童摘未摘。",
    "T47n1996_005:0701b16_2": "斯言如不聞。",
    "T47n1996_005:0701b16_3": "千古動愁色。",
    "T47n1996_005:0701b17_4": "因憶商山吟。",
    "T47n1996_005:0701b17_5": "在烏不在白。",
    "T47n1996_005:0701b18_0": "答當生不生",
    "T47n1996_005:0701b19_0": "咄咄休強名。",
    "T47n1996_005:0701b19_1": "芻狗亦為累。",
    "T47n1996_005:0701b19_2": "寂寥金粟身。",
    "T47n1996_005:0701b19_3": "曾未求諸己。",
    "T47n1996_005:0701b21_0": "戲靠安巖呈雙溪大師",
    "T47n1996_005:0701b22_0": "陝府鐵牛却知有。",
    "T47n1996_005:0701b22_1": "春秋幾幾成過咎。",
    "T47n1996_005:0701b22_2": "一身還作二如來。",
    "T47n1996_005:0701b23_3": "黑白不分辯香臭。",
    "T47n1996_005:0701b24_0": "疏黑白無從",
    "T47n1996_005:0701b25_0": "天地不仁萬化蠢蠢。",
    "T47n1996_005:0701b25_1": "若謂非緣竹何從笋。",
    "T47n1996_005:0701b25_2": "髮兮髮兮黑白是準。",
    "T47n1996_005:0701b27_0": "暮冬感懷寄瑞巖禪師",
    "T47n1996_005:0701b28_0": "雪水繞松檻。",
    "T47n1996_005:0701b28_1": "遲遲結清淺。",
    "T47n1996_005:0701b28_2": "病眼時懶開。",
    "T47n1996_005:0701b28_3": "幽情況難遣。",
    "T47n1996_005:0701b29_4": "故人久相別。",
    "T47n1996_005:0701b29_5": "飛文屢慚靦。",
    "T47n1996_005:0701b29_6": "仰謝十二峯。",
    "T47n1996_005:0701c01_7": "分照月如剪。",
    "T47n1996_005:0701c02_0": "送知久禪者",
    "T47n1996_005:0701c03_0": "霜竹凝寒携九節。",
    "T47n1996_005:0701c03_1": "銅瓶浪鎖千溪月。",
    "T47n1996_005:0701c03_2": "天上人間不自知。",
    "T47n1996_005:0701c04_3": "行行誰共分清絕。",
    "T47n1996_005:0701c05_0": "送慶顏禪者",
    "T47n1996_005:0701c06_0": "巖桂風清香露滴。",
    "T47n1996_005:0701c06_1": "定起高秋映虛碧。",
    "T47n1996_005:0701c06_2": "斷雲不是歸帝鄉。",
    "T47n1996_005:0701c07_3": "飛落人間有誰識。",
    "T47n1996_005:0701c08_0": "春日懷古(四首)",
    "T47n1996_005:0701c09_0": "門外春將半。",
    "T47n1996_005:0701c09_1": "巖氷暖有聲。",
    "T47n1996_005:0701c09_2": "玄沙曾未到。",
    "T47n1996_005:0701c09_3": "虛得偃溪名。",
    "T47n1996_005:0701c11_0": "門外春將半。",
    "T47n1996_005:0701c11_1": "青青野色分。",
    "T47n1996_005:0701c11_2": "桃華開欲盡。",
    "T47n1996_005:0701c11_3": "無處覓靈雲。",
    "T47n1996_005:0701c13_0": "門外春將半。",
    "T47n1996_005:0701c13_1": "群芳鬪盛時。",
    "T47n1996_005:0701c13_2": "隣家有庭柏。",
    "T47n1996_005:0701c13_3": "諸祖共相知。",
    "T47n1996_005:0701c15_0": "門外春將半。",
    "T47n1996_005:0701c15_1": "幽禽語共新。",
    "T47n1996_005:0701c15_2": "寶陀巖上客。",
    "T47n1996_005:0701c15_3": "應笑未歸人。",
    "T47n1996_005:0701c17_0": "送僧之金陵",
    "T47n1996_005:0701c18_0": "勝遊生末跡。",
    "T47n1996_005:0701c18_1": "杳自狎時群。",
    "T47n1996_005:0701c18_2": "卷衲消寒木。",
    "T47n1996_005:0701c18_3": "揚帆寄斷雲。",
    "T47n1996_005:0701c19_4": "曙瓶華外汲。",
    "T47n1996_005:0701c19_5": "午磬浪邊聞。",
    "T47n1996_005:0701c19_6": "別後石城月。",
    "T47n1996_005:0701c20_7": "依依遠共分。",
    "T47n1996_005:0701c21_0": "送僧",
    "T47n1996_005:0701c22_0": "知方流古意。",
    "T47n1996_005:0701c22_1": "雲樹別諸隣。",
    "T47n1996_005:0701c22_2": "月不澄微水。",
    "T47n1996_005:0701c22_3": "山應立是塵。",
    "T47n1996_005:0701c23_4": "靜空孤鶚遠。",
    "T47n1996_005:0701c23_5": "高柳一蟬新。",
    "T47n1996_005:0701c23_6": "欲究勞生問。",
    "T47n1996_005:0701c24_7": "歸思莫厭頻。",
    "T47n1996_005:0701c25_0": "千里不來",
    "T47n1996_005:0701c26_0": "不見古君子。",
    "T47n1996_005:0701c26_1": "因循又隔秋。",
    "T47n1996_005:0701c26_2": "浮生多自擲。",
    "T47n1996_005:0701c26_3": "好事更誰留。",
    "T47n1996_005:0701c27_4": "碧巘高沈月。",
    "T47n1996_005:0701c27_5": "寒雲靜鎖樓。",
    "T47n1996_005:0701c27_6": "宗雷何處是。",
    "T47n1996_005:0701c28_7": "白鳥下汀洲。",
    "T47n1996_005:0701c29_0": "僧歸霅上",
    "T47n1996_005:0702a01_0": "海國浮輕檝。",
    "T47n1996_005:0702a01_1": "悠悠興未闌。",
    "T47n1996_005:0702a01_2": "草隨春岸綠。",
    "T47n1996_005:0702a01_3": "風倚夜濤寒。",
    "T47n1996_005:0702a02_4": "沙鷺宜相狎。",
    "T47n1996_005:0702a02_5": "霜蟾望更寬。",
    "T47n1996_005:0702a02_6": "河聲西聽日。",
    "T47n1996_005:0702a03_7": "誰得共雲端。",
    "T47n1996_005:0702a04_0": "春晴野步",
    "T47n1996_005:0702a05_0": "乘興携多士。",
    "T47n1996_005:0702a05_1": "遲遲傍水濆。",
    "T47n1996_005:0702a05_2": "春山不在目。",
    "T47n1996_005:0702a05_3": "啼鳥共誰聞。",
    "T47n1996_005:0702a06_4": "片石寒籠蘚。",
    "T47n1996_005:0702a06_5": "殘華冷襯雲。",
    "T47n1996_005:0702a06_6": "只應融老輩。",
    "T47n1996_005:0702a07_7": "庵際境猶分。",
    "T47n1996_005:0702a08_0": "賦瑞雪送穆大師",
    "T47n1996_005:0702a09_0": "五六皆名出。",
    "T47n1996_005:0702a09_1": "飄華獨見稀。",
    "T47n1996_005:0702a09_2": "若教同一色。",
    "T47n1996_005:0702a09_3": "還似負群機。",
    "T47n1996_005:0702a10_4": "玉馬獨空說。",
    "T47n1996_005:0702a10_5": "銅駝轉更非。",
    "T47n1996_005:0702a10_6": "爭如千萬里相對共依依。",
    "T47n1996_005:0702a12_0": "送鐵佛專使",
    "T47n1996_005:0702a13_0": "荷策來尋我。",
    "T47n1996_005:0702a13_1": "泛舟思舊山。",
    "T47n1996_005:0702a13_2": "不知何處月。",
    "T47n1996_005:0702a13_3": "相照在深灣。",
    "T47n1996_005:0702a14_4": "風助秋濤急。",
    "T47n1996_005:0702a14_5": "雲兼野樹閑。",
    "T47n1996_005:0702a14_6": "到時如請益。",
    "T47n1996_005:0702a15_7": "先憶趙州關。",
    "T47n1996_005:0702a16_0": "同于祕丞賦瀑泉",
    "T47n1996_005:0702a17_0": "大禹不知鑿。",
    "T47n1996_005:0702a17_1": "來源亦自成。",
    "T47n1996_005:0702a17_2": "色應隣眾白。",
    "T47n1996_005:0702a17_3": "聲合讓孤清。",
    "T47n1996_005:0702a18_4": "遠勢曾吞海。",
    "T47n1996_005:0702a18_5": "飛流未噴鯨。",
    "T47n1996_005:0702a18_6": "靈槎如可泛。",
    "T47n1996_005:0702a19_7": "天派問歸程。",
    "T47n1996_005:0702a20_0": "送簡能禪者歸仙都",
    "T47n1996_005:0702a21_0": "荷策下丹嶂。",
    "T47n1996_005:0702a21_1": "紛紛雪正飛。",
    "T47n1996_005:0702a21_2": "浮生誰未到。",
    "T47n1996_005:0702a21_3": "舊國自重歸。",
    "T47n1996_005:0702a22_4": "雲背猿聲斷。",
    "T47n1996_005:0702a22_5": "天遙鶚影微。",
    "T47n1996_005:0702a22_6": "蓮城古風月。",
    "T47n1996_005:0702a23_7": "又得振清機。",
    "T47n1996_005:0702a24_0": "天竺送僧",
    "T47n1996_005:0702a25_0": "雪霽蓮峯頂。",
    "T47n1996_005:0702a25_1": "孤禪起石床。",
    "T47n1996_005:0702a25_2": "向時機自絕。",
    "T47n1996_005:0702a25_3": "異域路空長。",
    "T47n1996_005:0702a26_4": "啼狖衝寒影。",
    "T47n1996_005:0702a26_5": "歸鴻見斷行。",
    "T47n1996_005:0702a26_6": "後期無定跡。",
    "T47n1996_005:0702a27_7": "煙水共茫茫。",
    "T47n1996_005:0702a28_0": "寄石祕校",
    "T47n1996_005:0702a29_0": "重林冥坐久。",
    "T47n1996_005:0702a29_1": "引望復遲遲。",
    "T47n1996_005:0702a29_2": "煩暑未消日。",
    "T47n1996_005:0702a29_3": "涼風來幾時。",
    "T47n1996_005:0702b01_4": "天雲飛積火。",
    "T47n1996_005:0702b01_5": "巖溜散垂絲。",
    "T47n1996_005:0702b01_6": "欲擬相尋去。",
    "T47n1996_005:0702b02_7": "浮生已共知。",
    "T47n1996_005:0702b03_0": "因事示眾",
    "T47n1996_005:0702b04_0": "客從遠方來。",
    "T47n1996_005:0702b04_1": "遺我徑寸璧。",
    "T47n1996_005:0702b04_2": "中有四箇字。",
    "T47n1996_005:0702b04_3": "字字無人識。",
    "T47n1996_005:0702b05_4": "清涵鯨海寬。",
    "T47n1996_005:0702b05_5": "冷射蟾輪窄。",
    "T47n1996_005:0702b05_6": "今朝呈似看。",
    "T47n1996_005:0702b06_7": "請道末後句。",
    "T47n1996_005:0702b07_0": "靜而善應(二首)",
    "T47n1996_005:0702b08_0": "覿面相見不在多端。",
    "T47n1996_005:0702b08_1": "龍蛇易辯衲子難瞞。",
    "T47n1996_005:0702b08_2": "金槌影動寶劍光寒。",
    "T47n1996_005:0702b09_3": "直下來也急著眼看。",
    "T47n1996_005:0702b10_0": "對揚殊特本同參。",
    "T47n1996_005:0702b10_1": "誰自遼空強指南。",
    "T47n1996_005:0702b10_2": "今古不存師弟子。",
    "T47n1996_005:0702b11_3": "一輪秋月印寒潭。",
    "T47n1996_005:0702b12_0": "自誨",
    "T47n1996_005:0702b13_0": "麟龍不為瑞。",
    "T47n1996_005:0702b13_1": "草木生光輝。",
    "T47n1996_005:0702b13_2": "三尺一丈六。",
    "T47n1996_005:0702b13_3": "且同携手歸。",
    "T47n1996_005:0702b14_4": "慚爾懲世師。",
    "T47n1996_005:0702b14_5": "巍巍何巍巍。",
    "T47n1996_005:0702b15_0": "宗門三印(三首)",
    "T47n1996_005:0702b16_0": "印空印水印泥。",
    "T47n1996_005:0702b16_1": "炳然字義還迷。",
    "T47n1996_005:0702b16_2": "黃頭大士不識。",
    "T47n1996_005:0702b17_3": "敢問誰得親提。",
    "T47n1996_005:0702b18_0": "印泥印空印水。",
    "T47n1996_005:0702b18_1": "匝地寒濤競起。",
    "T47n1996_005:0702b18_2": "其中無限鱗龍。",
    "T47n1996_005:0702b19_3": "幾處爭求出觜。",
    "T47n1996_005:0702b20_0": "印水印泥印空。",
    "T47n1996_005:0702b20_1": "衲子不辯西東。",
    "T47n1996_005:0702b20_2": "撥開向上一竅。",
    "T47n1996_005:0702b21_3": "千聖齊立下風。",
    "T47n1996_005:0702b22_0": "革轍二門(四首)",
    "T47n1996_005:0702b23_0": "劫火曾洞然。",
    "T47n1996_005:0702b23_1": "木人淚先落。",
    "T47n1996_005:0702b23_2": "可憐傅大士。",
    "T47n1996_005:0702b23_3": "處處失樓閣。",
    "T47n1996_005:0702b25_0": "德雲閑古錐。",
    "T47n1996_005:0702b25_1": "幾下妙峯頂。",
    "T47n1996_005:0702b25_2": "喚他癡聖人。",
    "T47n1996_005:0702b25_3": "擔雪共填井。",
    "T47n1996_005:0702b27_0": "祖佛未生前。",
    "T47n1996_005:0702b27_1": "已振塗毒鼓。",
    "T47n1996_005:0702b27_2": "如今誰樂聞。",
    "T47n1996_005:0702b27_3": "請試分回互。",
    "T47n1996_005:0702b29_0": "宛轉復宛轉。",
    "T47n1996_005:0702b29_1": "真金休百鍊。",
    "T47n1996_005:0702b29_2": "喪却毘耶離。",
    "T47n1996_005:0702b29_3": "無人解看箭。",
    "T47n1996_005:0702c02_0": "擬弋者慕",
    "T47n1996_005:0702c03_0": "翠羽立高枝。",
    "T47n1996_005:0702c03_1": "危巢對落暉。",
    "T47n1996_005:0702c03_2": "碧潭千萬丈。",
    "T47n1996_005:0702c03_3": "直下取魚歸。",
    "T47n1996_005:0702c05_0": "透法身句(二首)",
    "T47n1996_005:0702c06_0": "潦倒雲門泛鐵船。",
    "T47n1996_005:0702c06_1": "江南江北競頭看。",
    "T47n1996_005:0702c06_2": "可憐無限垂鉤者。",
    "T47n1996_005:0702c07_3": "隨例茫茫失釣竿。",
    "T47n1996_005:0702c08_0": "一葉飄空便見秋。",
    "T47n1996_005:0702c08_1": "法身須透鬧啾啾。",
    "T47n1996_005:0702c08_2": "明年更有新條在。",
    "T47n1996_005:0702c09_3": "惱亂春風卒未休。",
    "T47n1996_005:0702c10_0": "靈隱小參",
    "T47n1996_005:0702c11_0": "六合茫茫竟不知。",
    "T47n1996_005:0702c11_1": "靈山經夏是便宜。",
    "T47n1996_005:0702c11_2": "虛堂夜靜無餘事。",
    "T47n1996_005:0702c12_3": "留得禪僧立片時。",
    "T47n1996_005:0702c13_0": "因雪示眾",
    "T47n1996_005:0702c14_0": "清光欹月不相饒。",
    "T47n1996_005:0702c14_1": "堆積虛庭卒未消。",
    "T47n1996_005:0702c14_2": "為瑞為祥也難得。",
    "T47n1996_005:0702c15_3": "不知誰解立齊腰。",
    "T47n1996_005:0702c16_0": "祕魔巖",
    "T47n1996_005:0702c17_0": "把斷重津過者難。",
    "T47n1996_005:0702c17_1": "擎杈須信髑髏乾。",
    "T47n1996_005:0702c17_2": "霍山到後知端的。",
    "T47n1996_005:0702c18_3": "同死同生未足觀。",
    "T47n1996_005:0702c19_0": "保福四謾人",
    "T47n1996_005:0702c20_0": "竿木隨身老作家。",
    "T47n1996_005:0702c20_1": "逢場作戲更難加。",
    "T47n1996_005:0702c20_2": "謾人謾我無人會。",
    "T47n1996_005:0702c21_3": "水長船高眼裏沙。",
    "T47n1996_005:0702c22_0": "靈雲和尚",
    "T47n1996_005:0702c23_0": "本無迷悟數如麻。",
    "T47n1996_005:0702c23_1": "獨許靈雲是作家。",
    "T47n1996_005:0702c23_2": "借問遍參諸祖客。",
    "T47n1996_005:0702c24_3": "不知何處身桃華。",
    "T47n1996_005:0702c25_0": "僧問緣生義",
    "T47n1996_005:0702c26_0": "義列緣生笑未聞。",
    "T47n1996_005:0702c26_1": "就呈布鼓向雷門。",
    "T47n1996_005:0702c26_2": "金剛鐵券諸方問。",
    "T47n1996_005:0702c27_3": "報道三千海嶽昏。",
    "T47n1996_005:0702c28_0": "名實無當",
    "T47n1996_005:0702c29_0": "玉轉珠回祖佛言。",
    "T47n1996_005:0702c29_1": "精通猶是汗心田。",
    "T47n1996_005:0702c29_2": "老盧只解長舂米。",
    "T47n1996_005:0703a01_3": "何得黃梅萬古傳。",
    "T47n1996_005:0703a02_0": "迷悟相返",
    "T47n1996_005:0703a03_0": "霏霏梅雨灑危層。",
    "T47n1996_005:0703a03_1": "五月山房冷似氷。",
    "T47n1996_005:0703a03_2": "莫謂乾坤乖大信。",
    "T47n1996_005:0703a04_3": "未明心地是炎蒸。",
    "T47n1996_005:0703a05_0": "道貴如愚",
    "T47n1996_005:0703a06_0": "雨過雲凝曉半開。",
    "T47n1996_005:0703a06_1": "數峯如畫碧崔嵬。",
    "T47n1996_005:0703a06_2": "空生不解巖中坐。",
    "T47n1996_005:0703a07_3": "惹得天華動地來。",
    "T47n1996_005:0703a08_0": "大功不宰",
    "T47n1996_005:0703a09_0": "牛頭峯頂鎖重雲。",
    "T47n1996_005:0703a09_1": "獨坐寥寥寄此身。",
    "T47n1996_005:0703a09_2": "百鳥不來春又過。",
    "T47n1996_005:0703a10_3": "不知誰是到庵人。",
    "T47n1996_005:0703a11_0": "晦跡自貽",
    "T47n1996_005:0703a12_0": "圖畫當年愛洞庭。",
    "T47n1996_005:0703a12_1": "波心七十二峯青。",
    "T47n1996_005:0703a12_2": "如今高臥思前事。",
    "T47n1996_005:0703a13_3": "添得盧公倚石屏。",
    "T47n1996_005:0703a14_0": "五老師子",
    "T47n1996_005:0703a15_0": "踞地磐空勢未休。",
    "T47n1996_005:0703a15_1": "爪牙何必競時流。",
    "T47n1996_005:0703a15_2": "天教生在千峯上。",
    "T47n1996_005:0703a16_3": "不得雲擎也出頭。",
    "T47n1996_005:0703a17_0": "與時寡合",
    "T47n1996_005:0703a18_0": "居士門高謁未期。",
    "T47n1996_005:0703a18_1": "閑隈巖石且相宜。",
    "T47n1996_005:0703a18_2": "太湖三萬六千頃。",
    "T47n1996_005:0703a19_3": "月在清波說向誰。",
    "T47n1996_005:0703a20_0": "宜謙山主赴鄞城命",
    "T47n1996_005:0703a21_0": "休向千峯過好時。",
    "T47n1996_005:0703a21_1": "白雲高臥趣還卑。",
    "T47n1996_005:0703a21_2": "塗中無限未歸客。",
    "T47n1996_005:0703a22_3": "不待相依更待誰。",
    "T47n1996_005:0703a23_0": "庭前柏樹子(二首)",
    "T47n1996_005:0703a24_0": "七百甲子老禪和。",
    "T47n1996_005:0703a24_1": "安貼家邦苦是他。",
    "T47n1996_005:0703a24_2": "人問西來指庭柏。",
    "T47n1996_005:0703a25_3": "却令天下動干戈。",
    "T47n1996_005:0703a26_0": "千聖靈機不易親。",
    "T47n1996_005:0703a26_1": "龍生龍子莫因循。",
    "T47n1996_005:0703a26_2": "趙州奪得連城璧。",
    "T47n1996_005:0703a27_3": "秦王相如總喪身。",
    "T47n1996_005:0703a28_0": "贈琴僧",
    "T47n1996_005:0703a29_0": "太古清音發指端。",
    "T47n1996_005:0703a29_1": "月當松頂夜堂寒。",
    "T47n1996_005:0703a29_2": "悲風流水多嗚咽。",
    "T47n1996_005:0703b01_3": "不聽希聲不用彈。",
    "T47n1996_005:0703b02_0": "送僧",
    "T47n1996_005:0703b03_0": "帆掛澄江雨霽時。",
    "T47n1996_005:0703b03_1": "綠鋪春岸草離離。",
    "T47n1996_005:0703b03_2": "定乾坤句輕相送。",
    "T47n1996_005:0703b04_3": "逢著知音舉向伊。",
    "T47n1996_005:0703b05_0": "送僧之婺城(二首)",
    "T47n1996_005:0703b06_0": "孤雲徒自類行蹤。",
    "T47n1996_005:0703b06_1": "高指金華思不窮。",
    "T47n1996_005:0703b06_2": "日慕輕帆映秋色。",
    "T47n1996_005:0703b07_3": "沙禽啼斷一江風。",
    "T47n1996_005:0703b08_0": "婺溪煙景稱生涯。",
    "T47n1996_005:0703b08_1": "輕泛蘭舟意未賒。",
    "T47n1996_005:0703b08_2": "八詠清風好相繼。",
    "T47n1996_005:0703b09_3": "碧雲流水是詩家。",
    "T47n1996_005:0703b10_0": "送文用庵主歸舊隱",
    "T47n1996_005:0703b11_0": "太白峯前舊隱基。",
    "T47n1996_005:0703b11_1": "杉松寒翠滴無時。",
    "T47n1996_005:0703b11_2": "經年拋却又歸去。",
    "T47n1996_005:0703b12_3": "再聽巖猿只自知。",
    "T47n1996_005:0703b13_0": "送顯冲禪者之霅上覲兄著作",
    "T47n1996_005:0703b14_0": "選佛選官應在我。",
    "T47n1996_005:0703b14_1": "難兄難弟不唯他。",
    "T47n1996_005:0703b14_2": "汀華岸草芳菲日。",
    "T47n1996_005:0703b15_3": "遠遠清風爭奈何。",
    "T47n1996_005:0703b16_0": "送寶月禪者之天台",
    "T47n1996_005:0703b17_0": "春風吹斷海山雲。",
    "T47n1996_005:0703b17_1": "別夜寥寥絕四隣。",
    "T47n1996_005:0703b17_2": "月在石橋更無月。",
    "T47n1996_005:0703b18_3": "不知誰是月邊人。",
    "T47n1996_005:0703b19_0": "玄沙和尚",
    "T47n1996_005:0703b20_0": "本是釣魚船上客。",
    "T47n1996_005:0703b20_1": "偶除鬚髮著袈裟。",
    "T47n1996_005:0703b20_2": "祖佛位中留不得。",
    "T47n1996_005:0703b21_3": "夜來依舊宿蘆華。",
    "T47n1996_005:0703b22_0": "偶作",
    "T47n1996_005:0703b23_0": "拾翠尋芳烈夜燈。",
    "T47n1996_005:0703b23_1": "蘆芽穿膝笑無能。",
    "T47n1996_005:0703b23_2": "飛泉冷淡與誰聽。",
    "T47n1996_005:0703b24_3": "空落斷崖千萬層。",
    "T47n1996_005:0703b25_0": "送僧",
    "T47n1996_005:0703b26_0": "路岐長草帶青青。",
    "T47n1996_005:0703b26_1": "雲片相兼野思生。",
    "T47n1996_005:0703b26_2": "多謝春風莫吹散。",
    "T47n1996_005:0703b27_3": "等閑為蓋贈君行。",
    "T47n1996_005:0703b28_0": "送純禪者",
    "T47n1996_005:0703b29_0": "莎蘿雨滴蒼苔痕。",
    "T47n1996_005:0703b29_1": "前峯後峯啼斷猿。",
    "T47n1996_005:0703b29_2": "携笻別我下層翠。",
    "T47n1996_005:0703c01_3": "何處靜敲仁者門。",
    "T47n1996_005:0703c02_0": "和頑書記見寄",
    "T47n1996_005:0703c03_0": "古松吟繞石磷磷。",
    "T47n1996_005:0703c03_1": "湯惠休辭豈易聞。",
    "T47n1996_005:0703c03_2": "紅葉寫成藏不得。",
    "T47n1996_005:0703c04_3": "暮風吹斷碧溪雲。",
    "T47n1996_005:0703c05_0": "送允誠侍者",
    "T47n1996_005:0703c06_0": "飛泉列岫壓窮野。",
    "T47n1996_005:0703c06_1": "冷碧寒青光鬪射。",
    "T47n1996_005:0703c06_2": "片雲片石何太高。",
    "T47n1996_005:0703c07_3": "為誰留在長松下。",
    "T47n1996_005:0703c08_0": "送僧",
    "T47n1996_005:0703c09_0": "古藤枝寒索索。",
    "T47n1996_005:0703c09_1": "方倚靠又拈却。",
    "T47n1996_005:0703c09_2": "海闊天遙非等閑。",
    "T47n1996_005:0703c10_3": "風前曾共孤雲約。",
    "T47n1996_005:0703c11_0": "送清禪者",
    "T47n1996_005:0703c12_0": "瘦藤春雲深。",
    "T47n1996_005:0703c12_1": "天涯去無侶。",
    "T47n1996_005:0703c12_2": "時笑野泉聲。",
    "T47n1996_005:0703c12_3": "似共流鶯語。",
    "T47n1996_005:0703c13_4": "落落風規今古情。",
    "T47n1996_005:0703c13_5": "相逢會有知音舉。",
    "T47n1996_005:0703c14_0": "辭于祕丞",
    "T47n1996_005:0703c15_0": "永夜潛思槁木身。",
    "T47n1996_005:0703c15_1": "蓬仙門館漸經旬。",
    "T47n1996_005:0703c15_2": "雖干清政為高客。",
    "T47n1996_005:0703c16_3": "爭奈白雲無主人。",
    "T47n1996_005:0703c16_4": "巖瀉瀑泉機未息。",
    "T47n1996_005:0703c17_5": "雨零寒葉夢猶頻。",
    "T47n1996_005:0703c17_6": "此時賢宰容歸去。",
    "T47n1996_005:0703c17_7": "古像焚檀祝有因。",
    "T47n1996_005:0703c19_0": "送僧",
    "T47n1996_005:0703c20_0": "涼颷新葉墜巖陰。",
    "T47n1996_005:0703c20_1": "禪起高秋別翠今。",
    "T47n1996_005:0703c20_2": "孤月冷光清有興。",
    "T47n1996_005:0703c21_3": "斷雲閑影合無心。",
    "T47n1996_005:0703c21_4": "瓶分吳浪情何極。",
    "T47n1996_005:0703c22_5": "鉢化膺門道更深。",
    "T47n1996_005:0703c22_6": "好是却迴舊房日。",
    "T47n1996_005:0703c22_7": "倚欄同看橘鋪金。",
    "T47n1996_005:0703c24_0": "往復無間(十二首)",
    "T47n1996_005:0703c25_0": "平旦寅。",
    "T47n1996_005:0703c25_1": "眹兆之前已喪真。",
    "T47n1996_005:0703c25_2": "老胡鶴樹漸開口。",
    "T47n1996_005:0703c26_0": "猶舉雙趺誑後人。",
    "T47n1996_005:0703c27_0": "日出卯。",
    "T47n1996_005:0703c27_1": "萬國香華競頭走。",
    "T47n1996_005:0703c27_2": "邯鄲學步笑傍觀。",
    "T47n1996_005:0703c28_0": "豈知凶禍逐其後。",
    "T47n1996_005:0703c29_0": "食時辰。",
    "T47n1996_005:0703c29_1": "大嚮那堪列主賓。",
    "T47n1996_005:0703c29_2": "維摩香飯本非讚。",
    "T47n1996_005:0704a01_0": "怪他鶖鷺獨生瞋。",
    "T47n1996_005:0704a02_0": "禺中已。",
    "T47n1996_005:0704a02_1": "荊棘園林遍大地。",
    "T47n1996_005:0704a02_2": "南北東西卒未休。",
    "T47n1996_005:0704a03_0": "金剛焰復從何起。",
    "T47n1996_005:0704a04_0": "日南午。",
    "T47n1996_005:0704a04_1": "寥廓騰輝示天鼓。",
    "T47n1996_005:0704a04_2": "欝頭藍已定全身。",
    "T47n1996_005:0704a05_0": "何假周行誇七步。",
    "T47n1996_005:0704a06_0": "日昳未。",
    "T47n1996_005:0704a06_1": "碧眼胡來欺漢地。",
    "T47n1996_005:0704a06_2": "九年計較不能成。",
    "T47n1996_005:0704a07_0": "剛有癡人求斷臂。",
    "T47n1996_005:0704a08_0": "晡時申。",
    "T47n1996_005:0704a08_1": "急急趒生路上人。",
    "T47n1996_005:0704a08_2": "草鞋踏盡家鄉遠。",
    "T47n1996_005:0704a09_0": "頂罩燒鍾一萬斤。",
    "T47n1996_005:0704a10_0": "日入酉。",
    "T47n1996_005:0704a10_1": "室內覆盆且依舊。",
    "T47n1996_005:0704a10_2": "塵塵彼彼丈夫兒。",
    "T47n1996_005:0704a11_0": "井中之物同哮吼。",
    "T47n1996_005:0704a12_0": "黃昏戌。",
    "T47n1996_005:0704a12_1": "寰中不礙平人出。",
    "T47n1996_005:0704a12_2": "瓦礫光生珠玉閑。",
    "T47n1996_005:0704a13_0": "將軍豈用驅邊卒。",
    "T47n1996_005:0704a14_0": "人定亥。",
    "T47n1996_005:0704a14_1": "六合茫茫誰不在。",
    "T47n1996_005:0704a14_2": "長空有月自尋常。",
    "T47n1996_005:0704a15_0": "霧起雲騰也奇怪。",
    "T47n1996_005:0704a16_0": "半夜子。",
    "T47n1996_005:0704a16_1": "樵唱漁歌聲未已。",
    "T47n1996_005:0704a16_2": "雨華徒說問空生。",
    "T47n1996_005:0704a17_0": "高枕千門睡方美。",
    "T47n1996_005:0704a18_0": "鷄鳴丑。",
    "T47n1996_005:0704a18_1": "貴賤尊卑名相守。",
    "T47n1996_005:0704a18_2": "忙者忙兮閑者閑。",
    "T47n1996_005:0704a19_0": "古今休論自長久。",
    "T47n1996_005:0704a20_0": "送僧",
    "T47n1996_005:0704a21_0": "巖泉高鎖黃金宅。",
    "T47n1996_005:0704a21_1": "衲卷秋雲古標格。",
    "T47n1996_005:0704a21_2": "離歌誰贈欲行人。",
    "T47n1996_005:0704a22_3": "遍界同為一宿客。",
    "T47n1996_005:0704a22_4": "春色依依日杲杲。",
    "T47n1996_005:0704a23_5": "南北東西好看好。",
    "T47n1996_005:0704a23_6": "鬧市拶笑嬌尸迦。",
    "T47n1996_005:0704a23_7": "草頭青點俱胝老。",
    "T47n1996_005:0704a24_8": "阿呵呵。",
    "T47n1996_005:0704a24_9": "人間天上不知他。",
    "T47n1996_005:0704a24_10": "糲竭節有頂門眼。",
    "T47n1996_005:0704a25_11": "歸去清風拂薜蘿。",
    "T47n1996_005:0704a26_0": "寄李都尉",
    "T47n1996_005:0704a27_0": "水月拈來作者殊。",
    "T47n1996_005:0704a27_1": "東西南北謾區區。",
    "T47n1996_005:0704a27_2": "也知金粟李居士。",
    "T47n1996_005:0704a28_3": "端坐重城笑老盧(咄)。",
    "T47n1996_005:0704a29_0": "寄池陽曾學士",
    "T47n1996_005:0704b01_0": "山萬重兮水萬枝。",
    "T47n1996_005:0704b01_1": "堆青流碧冷便宜。",
    "T47n1996_005:0704b01_2": "算來免得生遙恨。",
    "T47n1996_005:0704b02_3": "不在詩情在祖師。",
    "T47n1996_005:0704b03_0": "寄四明使君沈祠部(二首)",
    "T47n1996_005:0704b04_0": "露冕民謠物物成。",
    "T47n1996_005:0704b04_1": "江山千里古風清。",
    "T47n1996_005:0704b04_2": "曹溪客是無機者。",
    "T47n1996_005:0704b05_3": "日在深雲聽頌聲。",
    "T47n1996_005:0704b06_0": "蒼蒼德也亦如斯。",
    "T47n1996_005:0704b06_1": "政化全歸副倚毘。",
    "T47n1996_005:0704b06_2": "十萬人家寫春色。",
    "T47n1996_005:0704b07_3": "不知誰解立生祠。",
    "T47n1996_005:0704b08_0": "寄內侍太保(二首)",
    "T47n1996_005:0704b09_0": "千尺巖泉噴冷聲。",
    "T47n1996_005:0704b09_1": "草堂雲淡竹風清。",
    "T47n1996_005:0704b09_2": "蒲團時倚無他事。",
    "T47n1996_005:0704b10_3": "永日寥寥謝太平。",
    "T47n1996_005:0704b11_0": "蘿龕蘚室狎猿猱。",
    "T47n1996_005:0704b11_1": "忽捧綸言掛紫袍。",
    "T47n1996_005:0704b11_2": "恩大不知何以報。",
    "T47n1996_005:0704b12_3": "五雲天上望空勞。",
    "T47n1996_005:0704b13_0": "寄曹都護",
    "T47n1996_005:0704b14_0": "故國休言萬里程。",
    "T47n1996_005:0704b14_1": "為官為釋且分明。",
    "T47n1996_005:0704b14_2": "道存不必曾傾蓋。",
    "T47n1996_005:0704b15_3": "俱有清風匝地生。",
    "T47n1996_005:0704b16_0": "送僧",
    "T47n1996_005:0704b17_0": "虎角深藏不待時。",
    "T47n1996_005:0704b17_1": "全機曾許雪林知。",
    "T47n1996_005:0704b17_2": "如今百越拈來也。",
    "T47n1996_005:0704b18_3": "草偃風行是信旗。",
    "T47n1996_005:0704b19_0": "寄靈隱惠明禪師(二首)",
    "T47n1996_005:0704b20_0": "千峯影裏葉初凋。",
    "T47n1996_005:0704b20_1": "極望還將慰寂寥。",
    "T47n1996_005:0704b20_2": "也謂毫端不相隔。",
    "T47n1996_005:0704b21_3": "秋雲秋水奈遙遙。",
    "T47n1996_005:0704b22_0": "海嶠生片雲。",
    "T47n1996_005:0704b22_1": "有時忽如蓋。",
    "T47n1996_005:0704b22_2": "不掛飛來峯。",
    "T47n1996_005:0704b22_3": "悠悠擬何待。",
    "T47n1996_005:0704b24_0": "送益書記之霅水",
    "T47n1996_005:0704b25_0": "白蘋汀是舊家鄉。",
    "T47n1996_005:0704b25_1": "歸興蘭舟泛渺茫。",
    "T47n1996_005:0704b25_2": "日暮沙禽啼欲斷。",
    "T47n1996_005:0704b26_3": "不知誰在碧雲房。",
    "T47n1996_005:0704b27_0": "明覺禪師祖英集卷第五"
}