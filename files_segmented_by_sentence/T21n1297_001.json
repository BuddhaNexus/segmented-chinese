[
    {
        "source_sentence": "供養十二大威德天報恩品一卷",
        "from": "T21n1297_001:0383c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空譯",
        "from": "T21n1297_001:0383c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時普賢菩薩。觀察人天哀愍一切。無間自",
        "from": "T21n1297_001:0383c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說言。",
        "from": "T21n1297_001:0383c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生四大遠變。",
        "from": "T21n1297_001:0383c07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有種種病或鬼魔來作種種病。",
        "from": "T21n1297_001:0383c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迷倒世間減損壽命。",
        "from": "T21n1297_001:0383c08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何對治如是內外種種損害。",
        "from": "T21n1297_001:0383c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂諸眾生不知恩故。",
        "from": "T21n1297_001:0383c09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有如是違以何為恩。",
        "from": "T21n1297_001:0383c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂地水火風。",
        "from": "T21n1297_001:0383c10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日月諸天皆有內外養育之恩。",
        "from": "T21n1297_001:0383c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何得酬如是諸恩。",
        "from": "T21n1297_001:0383c11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂四大種有其精。",
        "from": "T21n1297_001:0383c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天及日月等也。",
        "from": "T21n1297_001:0383c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養是天有種種利。",
        "from": "T21n1297_001:0383c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "器界生界皆悉增力也。",
        "from": "T21n1297_001:0383c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其矣為誰。",
        "from": "T21n1297_001:0383c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其數幾何。",
        "from": "T21n1297_001:0383c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂彼天數有十二也。",
        "from": "T21n1297_001:0383c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地天水天火天風天伊舍那天帝釋天焰魔天梵天毘沙門天羅剎天日天月天也。",
        "from": "T21n1297_001:0383c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地天喜時有二利益。",
        "from": "T21n1297_001:0383c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者人身堅固色力增長。",
        "from": "T21n1297_001:0383c17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者器界地種味力增長。",
        "from": "T21n1297_001:0383c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此天瞋時亦有二損。",
        "from": "T21n1297_001:0383c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者人身亂壞色力減少。",
        "from": "T21n1297_001:0383c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者器界地味力皆違本。",
        "from": "T21n1297_001:0383c19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水天喜時有二利益。",
        "from": "T21n1297_001:0383c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者人身不渴。",
        "from": "T21n1297_001:0383c20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者雨澤順時。",
        "from": "T21n1297_001:0383c20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此天瞋時亦有二損。",
        "from": "T21n1297_001:0383c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者人身乾渴。",
        "from": "T21n1297_001:0383c21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者器界旱魃萬物乾盡。",
        "from": "T21n1297_001:0383c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或雨大雨世界滿水流損草不及與眾生。",
        "from": "T21n1297_001:0383c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火天喜時有二利益。",
        "from": "T21n1297_001:0383c23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者人身熱氣隨時增減。",
        "from": "T21n1297_001:0383c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者時節不逆此天瞋時亦有二損。",
        "from": "T21n1297_001:0383c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者人身熱氣非時增減。",
        "from": "T21n1297_001:0383c25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者自然散火焚燒諸物。",
        "from": "T21n1297_001:0383c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風天喜時有二利益。",
        "from": "T21n1297_001:0383c26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者人身輕安舉動隨心意。",
        "from": "T21n1297_001:0383c27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者器界安隱無有傾動。",
        "from": "T21n1297_001:0383c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而隨世間有冷風和不損情非情等。",
        "from": "T21n1297_001:0383c28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此天瞋時亦有二損。",
        "from": "T21n1297_001:0383c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者人身及音而不隨意。",
        "from": "T21n1297_001:0383c29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者大風吹滿散破世間。",
        "from": "T21n1297_001:0384a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或不吹風草木不順時也。",
        "from": "T21n1297_001:0384a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有如是四大精。",
        "from": "T21n1297_001:0384a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天瞋怒難者王及人民。",
        "from": "T21n1297_001:0384a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入於如來五輪塔中。",
        "from": "T21n1297_001:0384a03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持諸戒歸依三寶。",
        "from": "T21n1297_001:0384a03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝心而住應作是觀。",
        "from": "T21n1297_001:0384a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地依水生水性空故地亦無常。",
        "from": "T21n1297_001:0384a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水依風生風性空故水亦無常。",
        "from": "T21n1297_001:0384a05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火依水風生水風空故火亦無常。",
        "from": "T21n1297_001:0384a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風依虛空虛空無體故風亦無常。",
        "from": "T21n1297_001:0384a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風空故火空。",
        "from": "T21n1297_001:0384a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火空故水空。",
        "from": "T21n1297_001:0384a07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水空故地空。",
        "from": "T21n1297_001:0384a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地空故地上所生。",
        "from": "T21n1297_001:0384a08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有情非情皆悉無常。",
        "from": "T21n1297_001:0384a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是觀時。",
        "from": "T21n1297_001:0384a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天瞋滅無所依住。",
        "from": "T21n1297_001:0384a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時行者還作是觀。",
        "from": "T21n1297_001:0384a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空雖無尚有虛空。",
        "from": "T21n1297_001:0384a10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故得名虛空。",
        "from": "T21n1297_001:0384a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名必有體。",
        "from": "T21n1297_001:0384a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以虛空有故風亦有。",
        "from": "T21n1297_001:0384a11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風有故火水地等皆悉有體。",
        "from": "T21n1297_001:0384a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是觀時諸天歡喜。",
        "from": "T21n1297_001:0384a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "威光倍增安穩而住。",
        "from": "T21n1297_001:0384a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伊舍那天喜時諸天亦喜。",
        "from": "T21n1297_001:0384a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魔眾不亂也。",
        "from": "T21n1297_001:0384a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舊名摩醯首羅也。",
        "from": "T21n1297_001:0384a14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言若供養摩醯首羅唐云大自在已為供養一切諸天。",
        "from": "T21n1297_001:0384a15_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此天瞋時魔眾皆現國土荒亂。",
        "from": "T21n1297_001:0384a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天帝釋者地居之主。",
        "from": "T21n1297_001:0384a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "注記眾生所作善惡。",
        "from": "T21n1297_001:0384a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此天喜時國土安穩人民不亂。",
        "from": "T21n1297_001:0384a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此天瞋時刀兵相戰。",
        "from": "T21n1297_001:0384a18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地居諸王皆悉安。",
        "from": "T21n1297_001:0384a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "焰魔天喜時人無橫死疫氣不發。",
        "from": "T21n1297_001:0384a19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此天瞋時人非時死疫氣充滿。",
        "from": "T21n1297_001:0384a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵天者上天之主眾生之父。",
        "from": "T21n1297_001:0384a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此天喜時器世間安穩無有亂動。",
        "from": "T21n1297_001:0384a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故。",
        "from": "T21n1297_001:0384a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劫初之時。",
        "from": "T21n1297_001:0384a22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此天成立器世間也。",
        "from": "T21n1297_001:0384a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生不亂以正治世。",
        "from": "T21n1297_001:0384a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故。",
        "from": "T21n1297_001:0384a23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父王喜故。",
        "from": "T21n1297_001:0384a23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此天瞋時。",
        "from": "T21n1297_001:0384a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間不安有種種病。",
        "from": "T21n1297_001:0384a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至于草木皆悉惱落。",
        "from": "T21n1297_001:0384a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生迷惑名如醉人。",
        "from": "T21n1297_001:0384a25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘沙門喜時。",
        "from": "T21n1297_001:0384a25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥叉眾喜不害人民。",
        "from": "T21n1297_001:0384a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不行毒腫。",
        "from": "T21n1297_001:0384a26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞋時皆亂。",
        "from": "T21n1297_001:0384a26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅剎天喜時。",
        "from": "T21n1297_001:0384a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸噉完鬼隨亦喜。",
        "from": "T21n1297_001:0384a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不唾毒氣不作惡行。",
        "from": "T21n1297_001:0384a27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此天瞋時皆悉亂現。",
        "from": "T21n1297_001:0384a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日天喜時光不損物。",
        "from": "T21n1297_001:0384a28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人瞋不鈍有情非情皆悉快樂。",
        "from": "T21n1297_001:0384a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此天瞋時失度無光。",
        "from": "T21n1297_001:0384b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖有眼目不能見物。",
        "from": "T21n1297_001:0384b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寒苦怱逼。",
        "from": "T21n1297_001:0384b01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月天喜時冷光增物人無熱病。",
        "from": "T21n1297_001:0384b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞋時皆捨矣。",
        "from": "T21n1297_001:0384b02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日月互照有大利益時節和融。",
        "from": "T21n1297_001:0384b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生作事一一隨喜。",
        "from": "T21n1297_001:0384b03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劫初之時人如色天而漸貪味減少身光。",
        "from": "T21n1297_001:0384b04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彌捨善心而作惡業。",
        "from": "T21n1297_001:0384b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世皆不順而有寒熱。",
        "from": "T21n1297_001:0384b05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故此日月天現世化夢也。",
        "from": "T21n1297_001:0384b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是諸天何時歡喜何時瞋怒。",
        "from": "T21n1297_001:0384b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂諸國王及諸人民。",
        "from": "T21n1297_001:0384b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以非治世作不善業。",
        "from": "T21n1297_001:0384b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殺盜常行而捨正法。",
        "from": "T21n1297_001:0384b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時諸天皆生愁憂。",
        "from": "T21n1297_001:0384b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愁憂即過便生瞋怒。",
        "from": "T21n1297_001:0384b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若天災欲起先有瑞怪。",
        "from": "T21n1297_001:0384b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂無雲而雨矣。",
        "from": "T21n1297_001:0384b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸星隨亂無雲而雨者。",
        "from": "T21n1297_001:0384b10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸天悲泣之淚也若止惡業。",
        "from": "T21n1297_001:0384b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以正治世諸天歡喜。",
        "from": "T21n1297_001:0384b11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆悉來護妙香遍薰。",
        "from": "T21n1297_001:0384b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善增惡減。",
        "from": "T21n1297_001:0384b12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人了知如是大聖威德天等。",
        "from": "T21n1297_001:0384b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以財之施嚴彼生身。",
        "from": "T21n1297_001:0384b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後以法施顯彼法身。",
        "from": "T21n1297_001:0384b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼行慈悲不殺生命。",
        "from": "T21n1297_001:0384b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以是供養為報彼恩也。",
        "from": "T21n1297_001:0384b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世有諸天鬼神其數甚多。",
        "from": "T21n1297_001:0384b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何唯供養此十二天。",
        "from": "T21n1297_001:0384b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安立國土萬姓安樂。",
        "from": "T21n1297_001:0384b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂十二天總攝一切天龍鬼神星宿冥官。",
        "from": "T21n1297_001:0384b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故供養了知此十二天。",
        "from": "T21n1297_001:0384b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得一切天龍等護也。",
        "from": "T21n1297_001:0384b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以然者地天與地上諸神樹下野沙諸鬼神。",
        "from": "T21n1297_001:0384b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱來入壇場同時受供。",
        "from": "T21n1297_001:0384b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水天與諸川流江河大海諸神及諸龍眾。",
        "from": "T21n1297_001:0384b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱來入壇場同時受供。",
        "from": "T21n1297_001:0384b21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火天與諸火神及諸持明神仙眾。",
        "from": "T21n1297_001:0384b22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱來入壇場同時受供。",
        "from": "T21n1297_001:0384b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風天與諸風神無形流行神等。",
        "from": "T21n1297_001:0384b23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱來入壇場同時受供。",
        "from": "T21n1297_001:0384b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伊舍那天與諸魔眾。",
        "from": "T21n1297_001:0384b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱來入此壇同時受供。",
        "from": "T21n1297_001:0384b25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝釋天與諸蘇迷盧等一切諸山所攝天鬼等。",
        "from": "T21n1297_001:0384b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱來入壇場同時受供。",
        "from": "T21n1297_001:0384b26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "焰魔天與諸五道冥官太山府君司命行疫神諸餓鬼等。",
        "from": "T21n1297_001:0384b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱來入壇場同時受供。",
        "from": "T21n1297_001:0384b28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵天與諸色界靜慮一切諸天。",
        "from": "T21n1297_001:0384b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱來入壇場同時受供。",
        "from": "T21n1297_001:0384b29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘沙門天與諸藥叉吞食鬼神等。",
        "from": "T21n1297_001:0384c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱來入壇場同時受供羅剎天子羅剎食血鬼眾。",
        "from": "T21n1297_001:0384c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱來入壇場同時受供。",
        "from": "T21n1297_001:0384c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日天與諸星眾七曜諸執遊空一切光神。",
        "from": "T21n1297_001:0384c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱來入壇場同時受供。",
        "from": "T21n1297_001:0384c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月天與諸住空二十八宿十二宮神一切宿眾。",
        "from": "T21n1297_001:0384c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱來入壇場同時受供。",
        "from": "T21n1297_001:0384c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應如是觀矣。",
        "from": "T21n1297_001:0384c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是天主親對佛前。",
        "from": "T21n1297_001:0384c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誓當荷負一切有情護持國界守護正法。",
        "from": "T21n1297_001:0384c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若召請時不越本誓必定影嚮。",
        "from": "T21n1297_001:0384c08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未發遣之間。",
        "from": "T21n1297_001:0384c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而住壇場不違行者。",
        "from": "T21n1297_001:0384c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故召請供養然後應迸謝。",
        "from": "T21n1297_001:0384c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸事疎而乞歡喜。",
        "from": "T21n1297_001:0384c10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "召請之時用各別印明。",
        "from": "T21n1297_001:0384c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或用總請印明亦得也。",
        "from": "T21n1297_001:0384c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何少施之物。",
        "from": "T21n1297_001:0384c11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若干天眾及諸鬼神各得受用。",
        "from": "T21n1297_001:0384c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂由三種義之少供物一切受用。",
        "from": "T21n1297_001:0384c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一由三密加持。",
        "from": "T21n1297_001:0384c13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂運心法界實相妙供。",
        "from": "T21n1297_001:0384c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二由大慈悲。",
        "from": "T21n1297_001:0384c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三由彼諸天主福力多。",
        "from": "T21n1297_001:0384c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如龍得少水以自福力遍灑諸國。",
        "from": "T21n1297_001:0384c15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪於諸天所以滿四天下人福。",
        "from": "T21n1297_001:0384c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不及四王天一人福財。",
        "from": "T21n1297_001:0384c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨持主志有增減。",
        "from": "T21n1297_001:0384c17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而諸天福多故。",
        "from": "T21n1297_001:0384c17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得乏少供遍與一切。",
        "from": "T21n1297_001:0384c18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如少火隨薪有增減。",
        "from": "T21n1297_001:0384c18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少供亦爾。",
        "from": "T21n1297_001:0384c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天福多故付彼成廣。",
        "from": "T21n1297_001:0384c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又縱雖色味下劣。",
        "from": "T21n1297_001:0384c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由先三義皆成清淨微妙醍醐。",
        "from": "T21n1297_001:0384c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如耆婆悲一切即萬物成藥。",
        "from": "T21n1297_001:0384c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尼健呪毒毒返成藥。",
        "from": "T21n1297_001:0384c21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間尚爾。",
        "from": "T21n1297_001:0384c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "況行者隨順佛教。",
        "from": "T21n1297_001:0384c22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起大慈悲作三密加持哉。",
        "from": "T21n1297_001:0384c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又由天福而彼下劣供物。",
        "from": "T21n1297_001:0384c23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成清淨微妙供如石磨男得石成王。",
        "from": "T21n1297_001:0384c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作世女取沙為金。",
        "from": "T21n1297_001:0384c24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色夫人眾不耽世財唯喜順正。",
        "from": "T21n1297_001:0384c25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以不輕少施而悅慈施也。",
        "from": "T21n1297_001:0384c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若天不悅即請外金剛部主四臂不動忿怒明王供養歸依。",
        "from": "T21n1297_001:0384c27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時諸天隨順教勅歡喜護持也。",
        "from": "T21n1297_001:0384c28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若供養時來位方何。",
        "from": "T21n1297_001:0384c28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂東方帝釋東南火天。",
        "from": "T21n1297_001:0384c29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南方焰魔西南羅剎。",
        "from": "T21n1297_001:0384c29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方水天西北風天。",
        "from": "T21n1297_001:0385a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方毘沙門東北伊舍那。",
        "from": "T21n1297_001:0385a01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中央本尊四隅梵天地天日天月天。",
        "from": "T21n1297_001:0385a02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是而住。",
        "from": "T21n1297_001:0385a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何供養何天為首。",
        "from": "T21n1297_001:0385a03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂或依方位。",
        "from": "T21n1297_001:0385a03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始自東方迴順供之。",
        "from": "T21n1297_001:0385a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有別願依息災等法而供養之。",
        "from": "T21n1297_001:0385a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若求息災帝釋為首。",
        "from": "T21n1297_001:0385a05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若求增益梵天為主。",
        "from": "T21n1297_001:0385a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若求調伏大自在為首。",
        "from": "T21n1297_001:0385a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若求敬愛毘沙門為首。",
        "from": "T21n1297_001:0385a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘迴順供。",
        "from": "T21n1297_001:0385a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或欲止刀兵帝釋為主。",
        "from": "T21n1297_001:0385a07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲除藥叉羅剎難者用毘沙門羅剎天。",
        "from": "T21n1297_001:0385a08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲除疫用焰魔天。",
        "from": "T21n1297_001:0385a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲除旱魃洪水難者俱用水天。",
        "from": "T21n1297_001:0385a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲除怨災用風天。",
        "from": "T21n1297_001:0385a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并於此天祈風難等。",
        "from": "T21n1297_001:0385a10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲調人用梵天王。",
        "from": "T21n1297_001:0385a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水難用火天。",
        "from": "T21n1297_001:0385a11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火難用水天。",
        "from": "T21n1297_001:0385a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降魔用伊舍那。",
        "from": "T21n1297_001:0385a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或用火天。",
        "from": "T21n1297_001:0385a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鎮惡處用地天。",
        "from": "T21n1297_001:0385a12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并祈五穀也。",
        "from": "T21n1297_001:0385a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若求官位帝釋天。",
        "from": "T21n1297_001:0385a13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若求智日天若求定用月天。",
        "from": "T21n1297_001:0385a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲除熱寒病隨用日月天日天除寒月天除熱若有四大病。",
        "from": "T21n1297_001:0385a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨用四大精天。",
        "from": "T21n1297_001:0385a15_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是為首。",
        "from": "T21n1297_001:0385a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘次第迴順供之。",
        "from": "T21n1297_001:0385a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為富用毘沙門。",
        "from": "T21n1297_001:0385a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為貴更用梵天。",
        "from": "T21n1297_001:0385a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其供養具各儲一器。",
        "from": "T21n1297_001:0385a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香水塗香時華五穀飯若粥蠟燭等。",
        "from": "T21n1297_001:0385a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "插盛一器而供養之。",
        "from": "T21n1297_001:0385a18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒香普薰清淨如法。",
        "from": "T21n1297_001:0385a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養儀式各別印明。",
        "from": "T21n1297_001:0385a19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及彼畫像種種法義如餘部說。",
        "from": "T21n1297_001:0385a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普賢菩薩為度眾生。",
        "from": "T21n1297_001:0385a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自問自答如是因緣。",
        "from": "T21n1297_001:0385a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是報恩如是祈願。",
        "from": "T21n1297_001:0385a21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是觀法如是三密。",
        "from": "T21n1297_001:0385a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是方便如是大力。",
        "from": "T21n1297_001:0385a22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意演說奉上世尊。",
        "from": "T21n1297_001:0385a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊說言義理如是。",
        "from": "T21n1297_001:0385a23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉如汝所說真實不虛。",
        "from": "T21n1297_001:0385a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是十二天乃是往古諸佛。",
        "from": "T21n1297_001:0385a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為度眾生而來現也。",
        "from": "T21n1297_001:0385a25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故行者不可依形勢天。",
        "from": "T21n1297_001:0385a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯觀彼天法身莊嚴。",
        "from": "T21n1297_001:0385a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人隨順我及汝說。",
        "from": "T21n1297_001:0385a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養是天。",
        "from": "T21n1297_001:0385a27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此天像前講讀般若甚深經法。",
        "from": "T21n1297_001:0385a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令此諸天皆大歡喜者。",
        "from": "T21n1297_001:0385a28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天歡喜故。",
        "from": "T21n1297_001:0385a28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生皆得歡喜。",
        "from": "T21n1297_001:0385a29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得者一切眾生喜。",
        "from": "T21n1297_001:0385a29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故諸佛亦喜。",
        "from": "T21n1297_001:0385b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛喜故。",
        "from": "T21n1297_001:0385b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世出世間悉地圓滿。",
        "from": "T21n1297_001:0385b01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二天儀軌一卷",
        "from": "T21n1297_001:0385b02_100",
        "context_with_next_sentence": false
    }
]