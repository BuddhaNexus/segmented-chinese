[
    {
        "source_sentence": "摩醯首羅天法要",
        "from": "T21n1279_001:0339c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時摩醯首羅天。於天上與諸天女遊戲化",
        "from": "T21n1279_001:0339c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樂作諸伎樂。忽然於髮中化出一天女。容顏",
        "from": "T21n1279_001:0339c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端正伎藝第一。",
        "from": "T21n1279_001:0340a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天之眾無能過者。",
        "from": "T21n1279_001:0340a01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而說呪曰。",
        "from": "T21n1279_001:0340a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨欝爾灑摩毘藥二合尸佉地毘也二合鉢囉二合鉢底丁以反野式伽羅者僧去林僧去林但儞也二合他濕嚩二合諾底兼跋囉麼諾哩儞吽泮吒半音持誦此呪者。",
        "from": "T21n1279_001:0340a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不問淨與不淨。",
        "from": "T21n1279_001:0340a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有妻無妻。",
        "from": "T21n1279_001:0340a07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食五辛酒肉與不食。",
        "from": "T21n1279_001:0340a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常誦持者。",
        "from": "T21n1279_001:0340a08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切善願悉得成就。",
        "from": "T21n1279_001:0340a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "況三業清淨人早成悉地。",
        "from": "T21n1279_001:0340a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更有何疑。",
        "from": "T21n1279_001:0340a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人日沒之時。",
        "from": "T21n1279_001:0340a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此呪一百八反。",
        "from": "T21n1279_001:0340a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即有効驗。",
        "from": "T21n1279_001:0340a10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次畫像法　先畫摩醯首羅天像。",
        "from": "T21n1279_001:0340a11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "容顏奇特端正殊妙。",
        "from": "T21n1279_001:0340a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從髮際化出一天女。",
        "from": "T21n1279_001:0340a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端正第一垂妙瓔珞。",
        "from": "T21n1279_001:0340a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩手絡環釧。",
        "from": "T21n1279_001:0340a13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手捧天花。",
        "from": "T21n1279_001:0340a13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與天王相視喜媚之容。",
        "from": "T21n1279_001:0340a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手向下捻裙之勢。",
        "from": "T21n1279_001:0340a14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於左膝上坐摩登伽天。",
        "from": "T21n1279_001:0340a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說手印相。",
        "from": "T21n1279_001:0340a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二羽外相叉。",
        "from": "T21n1279_001:0340a16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左押右成縛。",
        "from": "T21n1279_001:0340a16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "檀惠進力竪禪智申相合禪庾為請召。",
        "from": "T21n1279_001:0340a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1279_001:0340a17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵尾跢羅娑上柅跛羅摩哩馱抳瞋馱抳瞋馱抳頻馱尼頻馱抳娑嚩二合賀若有人日日受持此印真言。",
        "from": "T21n1279_001:0340a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養摩醯首羅天者。",
        "from": "T21n1279_001:0340a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種得驗。",
        "from": "T21n1279_001:0340a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦每作是印呪。",
        "from": "T21n1279_001:0340a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養一切諸佛。",
        "from": "T21n1279_001:0340a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛生歡喜。",
        "from": "T21n1279_001:0340a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼療一切諸病有驗。",
        "from": "T21n1279_001:0340a22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦滿一千反。",
        "from": "T21n1279_001:0340a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令婦人生歡喜。",
        "from": "T21n1279_001:0340a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦二千反。",
        "from": "T21n1279_001:0340a23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令一切人生歡喜。",
        "from": "T21n1279_001:0340a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦三千反。",
        "from": "T21n1279_001:0340a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得非人愛樂。",
        "from": "T21n1279_001:0340a24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦四千反。",
        "from": "T21n1279_001:0340a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切毘那多羅鬼皆歡喜。",
        "from": "T21n1279_001:0340a25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦五千反。",
        "from": "T21n1279_001:0340a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切龍女生歡喜心。",
        "from": "T21n1279_001:0340a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至誦一萬反。",
        "from": "T21n1279_001:0340a26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切天龍藥叉五通神仙皆悉歡喜。",
        "from": "T21n1279_001:0340a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲見本身者。",
        "from": "T21n1279_001:0340a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取燒死人。",
        "from": "T21n1279_001:0340a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殘柴誦呪一百八反招之。",
        "from": "T21n1279_001:0340a28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得見本身。",
        "from": "T21n1279_001:0340a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若能降伏怨家者。",
        "from": "T21n1279_001:0340a29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取赤土畫其形。",
        "from": "T21n1279_001:0340b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書其姓名。",
        "from": "T21n1279_001:0340b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左脚踏之。",
        "from": "T21n1279_001:0340b01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦呪一百八反。",
        "from": "T21n1279_001:0340b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不論遠近彼人自然來向所降伏。",
        "from": "T21n1279_001:0340b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲令夫婦相愛。",
        "from": "T21n1279_001:0340b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫其形書姓名。",
        "from": "T21n1279_001:0340b03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前法踏之。",
        "from": "T21n1279_001:0340b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即相愛敬。",
        "from": "T21n1279_001:0340b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲欲得國王大臣一切諸人敬愛者。",
        "from": "T21n1279_001:0340b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前法作之。",
        "from": "T21n1279_001:0340b05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若療鬼病者。",
        "from": "T21n1279_001:0340b05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取伽陀木。",
        "from": "T21n1279_001:0340b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "截作二十一段。",
        "from": "T21n1279_001:0340b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪二十一反。",
        "from": "T21n1279_001:0340b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一呪一燒病差。",
        "from": "T21n1279_001:0340b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若神病。",
        "from": "T21n1279_001:0340b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取阿魏安悉香雄黃青木香苦練葉。",
        "from": "T21n1279_001:0340b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各等分擣篩和水為丸。",
        "from": "T21n1279_001:0340b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦呪燒之病即差。",
        "from": "T21n1279_001:0340b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若狐媚病。",
        "from": "T21n1279_001:0340b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取雄黃安悉香芎藭獨頭萩𡱝角羝羊尿白馬懸蹄驢夜等。",
        "from": "T21n1279_001:0340b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各分此八味為朱燒之。",
        "from": "T21n1279_001:0340b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薰入鼻孔及塗身上。",
        "from": "T21n1279_001:0340b11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲成結界者。",
        "from": "T21n1279_001:0340b11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以水呪七反。",
        "from": "T21n1279_001:0340b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散灑四方。",
        "from": "T21n1279_001:0340b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意遠近如法成結界。",
        "from": "T21n1279_001:0340b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩醯首羅天王法要此法中摩醯首羅者東北伊舍那天也。",
        "from": "T21n1279_001:0340b15_15",
        "context_with_next_sentence": true
    }
]