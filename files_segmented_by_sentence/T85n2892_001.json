[
    {
        "source_sentence": "佛說現報當受經",
        "from": "T85n2892_001:1409c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "政一人獨行。而無伴侶。即問之言。姊今何",
        "from": "T85n2892_001:1409c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故顏容媚麗。焦悴如此。為何所苦。時女答言。",
        "from": "T85n2892_001:1409c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我之薄福。",
        "from": "T85n2892_001:1409c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嫁得夫主。",
        "from": "T85n2892_001:1409c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "望相保守。",
        "from": "T85n2892_001:1409c20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不期一旦疾病死亡。",
        "from": "T85n2892_001:1409c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "家親捉我生埋塚中。",
        "from": "T85n2892_001:1409c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逕得七日。",
        "from": "T85n2892_001:1409c21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸群賊劫破此塚。",
        "from": "T85n2892_001:1409c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲求財物。",
        "from": "T85n2892_001:1409c22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將我還家。",
        "from": "T85n2892_001:1409c22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "納用為妻。",
        "from": "T85n2892_001:1409c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賊師後時劫破一村。",
        "from": "T85n2892_001:1409c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為他所殺。",
        "from": "T85n2892_001:1409c23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "收斂屍骸。",
        "from": "T85n2892_001:1409c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而殯葬之。",
        "from": "T85n2892_001:1409c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "親屬將我。",
        "from": "T85n2892_001:1409c24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復生埋。",
        "from": "T85n2892_001:1409c24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逕得三日。",
        "from": "T85n2892_001:1409c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復為野狐。",
        "from": "T85n2892_001:1409c25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲噉死屍。",
        "from": "T85n2892_001:1409c25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來坌此塚。",
        "from": "T85n2892_001:1409c25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我因得出。",
        "from": "T85n2892_001:1409c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父母眷屬不相知聞。",
        "from": "T85n2892_001:1409c26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時長者子即語女言。",
        "from": "T85n2892_001:1409c26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我妻始死。",
        "from": "T85n2892_001:1409c27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝既無夫。",
        "from": "T85n2892_001:1409c27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今二人共為夫婦。",
        "from": "T85n2892_001:1409c27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能爾以否。",
        "from": "T85n2892_001:1409c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時此女人無所趣向。",
        "from": "T85n2892_001:1409c28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即然許之。",
        "from": "T85n2892_001:1409c28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相隨至家。",
        "from": "T85n2892_001:1409c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共相愛念。",
        "from": "T85n2892_001:1409c29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即逕一年便生一男。",
        "from": "T85n2892_001:1409c29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又於後時復生一男。",
        "from": "T85n2892_001:1410a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又於後時更復懷姙。",
        "from": "T85n2892_001:1410a01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨月垂產時。",
        "from": "T85n2892_001:1410a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長者子出外遊行。",
        "from": "T85n2892_001:1410a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與諸朋伴共相飲會。",
        "from": "T85n2892_001:1410a02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至冥而還。",
        "from": "T85n2892_001:1410a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其妻於後堅關門戶。",
        "from": "T85n2892_001:1410a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "存地生子。",
        "from": "T85n2892_001:1410a03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "血露狼藉。",
        "from": "T85n2892_001:1410a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫還喚妻。",
        "from": "T85n2892_001:1410a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妻為生子存踞在地。",
        "from": "T85n2892_001:1410a04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "血露狼藉。",
        "from": "T85n2892_001:1410a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得開門。",
        "from": "T85n2892_001:1410a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫喚不應。",
        "from": "T85n2892_001:1410a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拔刀破門。",
        "from": "T85n2892_001:1410a05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即問其妻。",
        "from": "T85n2892_001:1410a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我朝出外。",
        "from": "T85n2892_001:1410a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比其迴還喚汝。",
        "from": "T85n2892_001:1410a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "關門而不應。",
        "from": "T85n2892_001:1410a06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我有何情異。",
        "from": "T85n2892_001:1410a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妻答夫言。",
        "from": "T85n2892_001:1410a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為生子故血露狼藉。",
        "from": "T85n2892_001:1410a07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫語妻言。",
        "from": "T85n2892_001:1410a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所生子者今何所在。",
        "from": "T85n2892_001:1410a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其妻即將所生之子授與其夫。",
        "from": "T85n2892_001:1410a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫得此子即用生蘇煎煮。",
        "from": "T85n2892_001:1410a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "限妻令食。",
        "from": "T85n2892_001:1410a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妻不忍食。",
        "from": "T85n2892_001:1410a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即拔利刀而欲斷命。",
        "from": "T85n2892_001:1410a10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妻為貪命遂便食子。",
        "from": "T85n2892_001:1410a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時都盡。",
        "from": "T85n2892_001:1410a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至於明旦其夫酒醒。",
        "from": "T85n2892_001:1410a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即問妻言。",
        "from": "T85n2892_001:1410a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我昨冥還。",
        "from": "T85n2892_001:1410a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "醉酒悶迷應無過失。",
        "from": "T85n2892_001:1410a12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妻答夫言。",
        "from": "T85n2892_001:1410a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "君昨冥還。",
        "from": "T85n2892_001:1410a13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喚我開門。",
        "from": "T85n2892_001:1410a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為生子不時開門。",
        "from": "T85n2892_001:1410a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "君斫門入即便問我。",
        "from": "T85n2892_001:1410a14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有何情異。",
        "from": "T85n2892_001:1410a14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我答君言。",
        "from": "T85n2892_001:1410a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為生子故血露狼藉。",
        "from": "T85n2892_001:1410a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "君即從我索所生子。",
        "from": "T85n2892_001:1410a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我授與君。",
        "from": "T85n2892_001:1410a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "君得此子即用生蘇煎煮。",
        "from": "T85n2892_001:1410a16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "限我令食。",
        "from": "T85n2892_001:1410a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我不忍食。",
        "from": "T85n2892_001:1410a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "君即拔刀而欲殺我。",
        "from": "T85n2892_001:1410a17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為貪命食子遂盡。",
        "from": "T85n2892_001:1410a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其夫聞說。",
        "from": "T85n2892_001:1410a18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即自低頭。",
        "from": "T85n2892_001:1410a18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "極大慚愧。",
        "from": "T85n2892_001:1410a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辭謝其婦。",
        "from": "T85n2892_001:1410a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "醉酒過失。",
        "from": "T85n2892_001:1410a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願不為恨。",
        "from": "T85n2892_001:1410a19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從今以後誓不飲酒。",
        "from": "T85n2892_001:1410a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又於後時其婦語夫。",
        "from": "T85n2892_001:1410a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我與父母分別以來。",
        "from": "T85n2892_001:1410a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逕年度月不知消息。",
        "from": "T85n2892_001:1410a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等二人及以兩子。",
        "from": "T85n2892_001:1410a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往父母家參承問訊。",
        "from": "T85n2892_001:1410a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時長者子即共其妻并及二子隨路而去。",
        "from": "T85n2892_001:1410a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "值天抗熱。",
        "from": "T85n2892_001:1410a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其夫即便止一樹下。",
        "from": "T85n2892_001:1410a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "避曰取陰遂便失睡。",
        "from": "T85n2892_001:1410a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忽為黑蛇螫夫即死。",
        "from": "T85n2892_001:1410a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其妻啼哭。",
        "from": "T85n2892_001:1410a25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "抱持二子即便前路。",
        "from": "T85n2892_001:1410a25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "值一水坑。",
        "from": "T85n2892_001:1410a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長𣿖急難可得度。",
        "from": "T85n2892_001:1410a26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光送大子至達岸上。",
        "from": "T85n2892_001:1410a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始復欲迴迎取小子。",
        "from": "T85n2892_001:1410a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其子見母即投趣水。",
        "from": "T85n2892_001:1410a27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而為波浪之所沒溺。",
        "from": "T85n2892_001:1410a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復上岸上欲抱大子。",
        "from": "T85n2892_001:1410a28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即為虎狼之所食噉。",
        "from": "T85n2892_001:1410a29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時女人呼天喚地。",
        "from": "T85n2892_001:1410a29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有何惡業今逢此苦。",
        "from": "T85n2892_001:1410b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即便前進見父母家。",
        "from": "T85n2892_001:1410b01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "村中知識借問之言。",
        "from": "T85n2892_001:1410b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見我父母兄弟眷屬悉平安不。",
        "from": "T85n2892_001:1410b02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知識答曰。",
        "from": "T85n2892_001:1410b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝家失火。",
        "from": "T85n2892_001:1410b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大小悉皆為火燒殺並無人在。",
        "from": "T85n2892_001:1410b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時女聞之。",
        "from": "T85n2892_001:1410b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉復悶絕無所趣向。",
        "from": "T85n2892_001:1410b04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊為諸大眾說微妙法。",
        "from": "T85n2892_001:1410b05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以天眼觀此女人。",
        "from": "T85n2892_001:1410b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即告阿難。",
        "from": "T85n2892_001:1410b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝持寶衣迎此女人。",
        "from": "T85n2892_001:1410b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難受教將以寶衣迎此女人。",
        "from": "T85n2892_001:1410b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來至眾中。",
        "from": "T85n2892_001:1410b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮拜世尊。",
        "from": "T85n2892_001:1410b07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慚愧懺悔。",
        "from": "T85n2892_001:1410b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛為說法。",
        "from": "T85n2892_001:1410b08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷諸結漏得羅漢果。",
        "from": "T85n2892_001:1410b08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛又告一切大眾。",
        "from": "T85n2892_001:1410b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等欲知此女前世之善惡事者。",
        "from": "T85n2892_001:1410b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諦聽善思念之。",
        "from": "T85n2892_001:1410b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慎勿忘失。",
        "from": "T85n2892_001:1410b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃於過世無數劫時有長者子。",
        "from": "T85n2892_001:1410b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "財富無量。",
        "from": "T85n2892_001:1410b11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取得一婦。",
        "from": "T85n2892_001:1410b11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逕年度月并無兒息。",
        "from": "T85n2892_001:1410b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時長者子即自思惟。",
        "from": "T85n2892_001:1410b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今財富并無兒子。",
        "from": "T85n2892_001:1410b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一旦無常財無委付。",
        "from": "T85n2892_001:1410b13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於後時取得一妾。",
        "from": "T85n2892_001:1410b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逕始一年便生一男。",
        "from": "T85n2892_001:1410b14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面貌端政。",
        "from": "T85n2892_001:1410b14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長者憐念。",
        "from": "T85n2892_001:1410b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時大婦即自思惟。",
        "from": "T85n2892_001:1410b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為大婦並無兒子。",
        "from": "T85n2892_001:1410b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始取妾來未得多年。",
        "from": "T85n2892_001:1410b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以生一男兒。",
        "from": "T85n2892_001:1410b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若長大財當屬已。",
        "from": "T85n2892_001:1410b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我必為夫之所棄薄。",
        "from": "T85n2892_001:1410b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今宜應設諸方計殺此小兒。",
        "from": "T85n2892_001:1410b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時妾母出外不在。",
        "from": "T85n2892_001:1410b18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而是大妻內計以定即用鐵針二枚刺兒顋上。",
        "from": "T85n2892_001:1410b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小兒啼哭痛不可言。",
        "from": "T85n2892_001:1410b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妾母抱兒莫知痛處。",
        "from": "T85n2892_001:1410b20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呼天喚地。",
        "from": "T85n2892_001:1410b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有惡心瞋心疾心妬心殺我子者。",
        "from": "T85n2892_001:1410b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令其生生世世還受苦報。",
        "from": "T85n2892_001:1410b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時大婦心自忓清。",
        "from": "T85n2892_001:1410b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於夫前及於妾前而立呪誓。",
        "from": "T85n2892_001:1410b23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我若惡心嗔心嫉心妬心殺汝子者。",
        "from": "T85n2892_001:1410b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令我生生世世還受女身。",
        "from": "T85n2892_001:1410b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嫁得夫主生埋塚中。",
        "from": "T85n2892_001:1410b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令我生生世世若生兒者。",
        "from": "T85n2892_001:1410b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還以生蘇煎煮食之。",
        "from": "T85n2892_001:1410b26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令我生生世世夫為蛇螫。",
        "from": "T85n2892_001:1410b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "子為水㵱兒為虎食。",
        "from": "T85n2892_001:1410b27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闔家大小悉皆為火之所燒殺。",
        "from": "T85n2892_001:1410b28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪誓已訖兒即死亡。",
        "from": "T85n2892_001:1410b28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時大妻心自忓清而立呪誓言。",
        "from": "T85n2892_001:1410b29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無罪福如影隨形不相放捨。",
        "from": "T85n2892_001:1410c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而此大妻於五百世中。",
        "from": "T85n2892_001:1410c01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨身受身常受苦報。",
        "from": "T85n2892_001:1410c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛又告一切大眾。",
        "from": "T85n2892_001:1410c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今為汝等說。",
        "from": "T85n2892_001:1410c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此女人前世之時修何功德。",
        "from": "T85n2892_001:1410c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而復值我。",
        "from": "T85n2892_001:1410c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以衣覆之。",
        "from": "T85n2892_001:1410c04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為其說法斷諸結漏得羅漢道時。",
        "from": "T85n2892_001:1410c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此女人於過去世無量劫時。",
        "from": "T85n2892_001:1410c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "家甚貧窮。",
        "from": "T85n2892_001:1410c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有一辟支佛。",
        "from": "T85n2892_001:1410c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從其教化。",
        "from": "T85n2892_001:1410c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此女無錢。",
        "from": "T85n2892_001:1410c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即解衣帶施辟支佛。",
        "from": "T85n2892_001:1410c07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由是因緣所生端政恒生富家。",
        "from": "T85n2892_001:1410c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖復受如前苦於五百世還得值我。",
        "from": "T85n2892_001:1410c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以衣覆之。",
        "from": "T85n2892_001:1410c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為其說法斷諸結漏得羅漢道。",
        "from": "T85n2892_001:1410c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖復得道一日六時常苦熱鐵。",
        "from": "T85n2892_001:1410c10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從頂上入痛徹於心。",
        "from": "T85n2892_001:1410c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛又告一切大眾。",
        "from": "T85n2892_001:1410c11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從今已往慎勿惡心瞋心妬心。",
        "from": "T85n2892_001:1410c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更相殺害更相呪誓。",
        "from": "T85n2892_001:1410c12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言無罪報如影隨形終不放捨。",
        "from": "T85n2892_001:1410c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作善作惡得惡終無我作他人受報身。",
        "from": "T85n2892_001:1410c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖滅壞善惡不朽。",
        "from": "T85n2892_001:1410c14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如上所說汝諦受持。",
        "from": "T85n2892_001:1410c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時大眾及諸天龍八部鬼神有惡心者。",
        "from": "T85n2892_001:1410c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆生慈念。",
        "from": "T85n2892_001:1410c16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛所說頂戴受持。",
        "from": "T85n2892_001:1410c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說現報當受經一卷",
        "from": "T85n2892_001:1410c18_100",
        "context_with_next_sentence": false
    }
]