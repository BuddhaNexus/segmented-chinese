[
    {
        "source_sentence": "歷代三寶記卷第十五",
        "from": "T49n2034_015:0120a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上開皇三寶錄表上開皇三寶錄表",
        "from": "T49n2034_015:0120a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臣房言。臣聞有功於國史錄其勳。有政於民",
        "from": "T49n2034_015:0120a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "碑傳其德。",
        "from": "T49n2034_015:0120a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "況如來大聖化洽無窮而不垂美。",
        "from": "T49n2034_015:0120a21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百王流芳千載者也。",
        "from": "T49n2034_015:0120a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臣竊尋覽。",
        "from": "T49n2034_015:0120a22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自漢魏已來代有翻譯。",
        "from": "T49n2034_015:0120a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而錄目星散經多失源。",
        "from": "T49n2034_015:0120a23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世罕綴修時致間絕。",
        "from": "T49n2034_015:0120a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣此佛以正法付囑國王。",
        "from": "T49n2034_015:0120a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是知教興寄在帝主。",
        "from": "T49n2034_015:0120a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏惟陛下應運秉圖。",
        "from": "T49n2034_015:0120a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受如來記。",
        "from": "T49n2034_015:0120a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "紹輪王業。",
        "from": "T49n2034_015:0120a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "統閻浮提。",
        "from": "T49n2034_015:0120a26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愍世間昏開慧日照。",
        "from": "T49n2034_015:0120a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣緝經像大啟伽藍。",
        "from": "T49n2034_015:0120a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闡解脫之門。",
        "from": "T49n2034_015:0120a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "導天人之路。",
        "from": "T49n2034_015:0120a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建善舟檝濟拔蒼生。",
        "from": "T49n2034_015:0120a28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斯實曠古一代盛歟。",
        "from": "T49n2034_015:0120a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈臣庸微輕敢妄述。",
        "from": "T49n2034_015:0120a29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但昔毀廢臣在染衣。",
        "from": "T49n2034_015:0120a29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今日興隆還參法侶。",
        "from": "T49n2034_015:0120b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時事所接頗預見聞。",
        "from": "T49n2034_015:0120b01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因綱歷世佛法緣起。",
        "from": "T49n2034_015:0120b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始自姬周莊王甲午佛誕西域後漢明皇永平丁卯經度東歲。",
        "from": "T49n2034_015:0120b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迄今開皇太歲丁巳。",
        "from": "T49n2034_015:0120b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歷一千二百七十四載。",
        "from": "T49n2034_015:0120b04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其間靈瑞帝主名僧代別。",
        "from": "T49n2034_015:0120b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯彰名開皇三寶錄。",
        "from": "T49n2034_015:0120b05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡十五卷。",
        "from": "T49n2034_015:0120b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庶法無隱冀經有弘。",
        "from": "T49n2034_015:0120b06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不任下情惶悚戰懼。",
        "from": "T49n2034_015:0120b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輕冒奉表上錄以聞。",
        "from": "T49n2034_015:0120b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願天慈垂神降省謹言。",
        "from": "T49n2034_015:0120b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開皇三寶錄總目序開皇三寶錄總目序開皇十七年十二月二十三日。",
        "from": "T49n2034_015:0120b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大興善寺翻經學士臣成都費長房上竊惟三寶所資四生蒙潤而世有興毀。",
        "from": "T49n2034_015:0120b12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "致人自昇沈。",
        "from": "T49n2034_015:0120b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "興則福業恒感天堂輪王人主。",
        "from": "T49n2034_015:0120b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毀則罪報常受地獄餓鬼畜生。",
        "from": "T49n2034_015:0120b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論益物深無過於法。",
        "from": "T49n2034_015:0120b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何者法是佛母佛從法生。",
        "from": "T49n2034_015:0120b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三世如來皆供養法。",
        "from": "T49n2034_015:0120b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故勝天王般若經云。",
        "from": "T49n2034_015:0120b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若供養法即供養佛。",
        "from": "T49n2034_015:0120b16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是知法教津流乃傳萬代。",
        "from": "T49n2034_015:0120b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛僧開導止利一時。",
        "from": "T49n2034_015:0120b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故賢劫之興千佛同其化。",
        "from": "T49n2034_015:0120b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脩短之壽四聖異其年。",
        "from": "T49n2034_015:0120b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖復住世延促有殊。",
        "from": "T49n2034_015:0120b19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取其宣揚弘法無別。",
        "from": "T49n2034_015:0120b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫不煎熬愛海濟含識以趣涅槃。",
        "from": "T49n2034_015:0120b20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鏨鑿慢山度蒼生以會般若。",
        "from": "T49n2034_015:0120b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然般若玄寂。",
        "from": "T49n2034_015:0120b21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非因聲難以通。",
        "from": "T49n2034_015:0120b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聲必託形。",
        "from": "T49n2034_015:0120b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不藉相無由顯。",
        "from": "T49n2034_015:0120b22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以境稱忍剎。",
        "from": "T49n2034_015:0120b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總百億之須彌。",
        "from": "T49n2034_015:0120b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世號娑婆。",
        "from": "T49n2034_015:0120b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "統三千之國土。",
        "from": "T49n2034_015:0120b23_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "區分三界五濁之穢土沙。",
        "from": "T49n2034_015:0120b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形別六道二乘之鄙羊鹿。",
        "from": "T49n2034_015:0120b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大聖慈愍俯降迦毘。",
        "from": "T49n2034_015:0120b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "丈六金容應王宮之裏。",
        "from": "T49n2034_015:0120b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十二相炳太子之身。",
        "from": "T49n2034_015:0120b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十九出家三十成道。",
        "from": "T49n2034_015:0120b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四十九載處在世間。",
        "from": "T49n2034_015:0120b27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "假以言音方便演暢。",
        "from": "T49n2034_015:0120b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無染之法金口自宣。",
        "from": "T49n2034_015:0120b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一音敷揚萬類各解。",
        "from": "T49n2034_015:0120b28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "機緣匪一教有塵沙。",
        "from": "T49n2034_015:0120b29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難總持渧無遺失。",
        "from": "T49n2034_015:0120b29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬別器水瀉之異瓶。",
        "from": "T49n2034_015:0120c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雙樹入般涅槃。",
        "from": "T49n2034_015:0120c01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉王城結集。",
        "from": "T49n2034_015:0120c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一千羅漢迭察迭書。",
        "from": "T49n2034_015:0120c02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著之葉皮布乎天竺。",
        "from": "T49n2034_015:0120c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五百中國各共奉持。",
        "from": "T49n2034_015:0120c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十六大王皆同擁護。",
        "from": "T49n2034_015:0120c03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後漢之始方屆脂那。",
        "from": "T49n2034_015:0120c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝世交參十有六代。",
        "from": "T49n2034_015:0120c04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翻彼域語作此方言。",
        "from": "T49n2034_015:0120c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相承迄今五百餘祀。",
        "from": "T49n2034_015:0120c05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "古舊二錄條目殘亡。",
        "from": "T49n2034_015:0120c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "士行道安創維其缺。",
        "from": "T49n2034_015:0120c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾來間有祖述不同。",
        "from": "T49n2034_015:0120c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各紀一方互存所見。",
        "from": "T49n2034_015:0120c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三隅致隔故多失疑。",
        "from": "T49n2034_015:0120c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又齊周陳並皆翻譯。",
        "from": "T49n2034_015:0120c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弗刊錄目靡所遵承。",
        "from": "T49n2034_015:0120c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼值毀焚絕無依據。",
        "from": "T49n2034_015:0120c09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賴我皇帝維地柱天。",
        "from": "T49n2034_015:0120c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "澄靜二儀廓清六合。",
        "from": "T49n2034_015:0120c10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庭來萬國化攝九州。",
        "from": "T49n2034_015:0120c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "異出遺文莫不皆萃。",
        "from": "T49n2034_015:0120c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臣幸有遇屬此休時。",
        "from": "T49n2034_015:0120c11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忝預譯經稟受佛語。",
        "from": "T49n2034_015:0120c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執筆暇隙寢食敢忘。",
        "from": "T49n2034_015:0120c12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十餘年來。",
        "from": "T49n2034_015:0120c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詢訪舊老。",
        "from": "T49n2034_015:0120c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "搜討方獲。",
        "from": "T49n2034_015:0120c13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖粗緝綴猶慮未周。",
        "from": "T49n2034_015:0120c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣究博尋敬俟來俊。",
        "from": "T49n2034_015:0120c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今之所撰集。",
        "from": "T49n2034_015:0120c14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略准三書以為指南。",
        "from": "T49n2034_015:0120c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯茲三寶。",
        "from": "T49n2034_015:0120c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛生年瑞依周夜明。",
        "from": "T49n2034_015:0120c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經度時祥承漢宵夢。",
        "from": "T49n2034_015:0120c16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧之元始城塹棟梁。",
        "from": "T49n2034_015:0120c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘贊光輝崇於慧晈。",
        "from": "T49n2034_015:0120c17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其外傍採隱居歷年國志典墳僧祐集記諸史傳等僅數十家。",
        "from": "T49n2034_015:0120c18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擿彼翠零成斯紀翮。",
        "from": "T49n2034_015:0120c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "扇之千載風於百王。",
        "from": "T49n2034_015:0120c19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共秉智炬之光。",
        "from": "T49n2034_015:0120c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照時昏暗。",
        "from": "T49n2034_015:0120c20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同傳法流之潤。",
        "from": "T49n2034_015:0120c20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洽世燋枯。",
        "from": "T49n2034_015:0120c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闡我皇猷導開厥始。",
        "from": "T49n2034_015:0120c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔結集之首。",
        "from": "T49n2034_015:0120c21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並指在某國城。",
        "from": "T49n2034_015:0120c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今宣譯之功。",
        "from": "T49n2034_015:0120c22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "理須各宗時代。",
        "from": "T49n2034_015:0120c22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故此錄體率舉號。",
        "from": "T49n2034_015:0120c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱為漢魏吳及大隋錄也。",
        "from": "T49n2034_015:0120c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "失譯疑偽。",
        "from": "T49n2034_015:0120c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依舊注之人。",
        "from": "T49n2034_015:0120c24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以年為先。",
        "from": "T49n2034_015:0120c24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經隨大而次。",
        "from": "T49n2034_015:0120c24_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有重列者。",
        "from": "T49n2034_015:0120c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶約世分總其華戎。",
        "from": "T49n2034_015:0120c25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黑白道俗合有一百九十七人。",
        "from": "T49n2034_015:0120c26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "都所出經律戒論傳。",
        "from": "T49n2034_015:0120c26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二千一百四十六部。",
        "from": "T49n2034_015:0120c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六千二百三十五卷。",
        "from": "T49n2034_015:0120c27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "位而分之為十五軸。",
        "from": "T49n2034_015:0120c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一卷總目。",
        "from": "T49n2034_015:0120c28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩卷入藏。",
        "from": "T49n2034_015:0120c28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三卷帝年。",
        "from": "T49n2034_015:0120c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九卷代錄。",
        "from": "T49n2034_015:0120c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "代錄編鑒經翻譯之少多。",
        "from": "T49n2034_015:0120c29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝年張知佛在世之遐邇。",
        "from": "T49n2034_015:0121a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入藏別識教小大之淺深。",
        "from": "T49n2034_015:0121a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔姬潛之鼎出現。",
        "from": "T49n2034_015:0121a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彰漢室之將隆。",
        "from": "T49n2034_015:0121a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "近周毀之法重興。",
        "from": "T49n2034_015:0121a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯大隋之永泰。",
        "from": "T49n2034_015:0121a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛日再照。",
        "from": "T49n2034_015:0121a03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起自大興之初。",
        "from": "T49n2034_015:0121a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經論冥歸。",
        "from": "T49n2034_015:0121a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發乎開皇之始。",
        "from": "T49n2034_015:0121a04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "事扶理契。",
        "from": "T49n2034_015:0121a04_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合此會昌。",
        "from": "T49n2034_015:0121a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "述紀所由。",
        "from": "T49n2034_015:0121a05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因斯而作。",
        "from": "T49n2034_015:0121a05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以外題稱曰開皇三寶錄云。",
        "from": "T49n2034_015:0121a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其卷內甄為歷代紀。",
        "from": "T49n2034_015:0121a06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開皇三寶紀卷第一　　帝年上周秦",
        "from": "T49n2034_015:0121a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合二十六主四百八十一年周　莊王　十五年今止取六年入紀九年佛生僖王　五年惠王　二十五年襄王　三十三年傾王　六年匡王　六年四年佛入涅槃定王　二十一年簡王　十四年靈王　二十七年景王　二十五年敬王　四十三年二十六年。",
        "from": "T49n2034_015:0121a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿育王起八萬四千寶塔元王　八年真定王　二十八年孝王　十五年威列王　二十四年元安王　二十六年夷列王　七年顯聖王　四十八年順靜王　六年𧹞王　五十九年秦　昭襄王　五年孝文王　一年莊襄王　三年始皇帝　三十七年二世皇帝　三年始皇帝子　四十六日開皇三寶錄卷第二帝年前漢次新後漢合二十六主四百一十四年前漢　高帝　十二年都長安惠帝　七年呂后攝　八年文帝　二十三年景帝　十六年武帝　五十四年昭帝　十三年宣帝　二十五年元帝　十六年成帝　二十六年哀帝　六年平帝　五年新　王莽　十七年治長安更始帝　二年亦長安後漢　光武帝　二十三年都雒陽明帝　十八年十年譯經章帝　十三年和帝　十七年殤帝　一年安帝　十九年順帝　十九年沖帝　一年質帝　一年桓帝　二十一年靈帝　二十二年獻帝　三十年開皇三寶錄卷第三帝年下魏晉宋齊梁周大隋合四十五主三百八十一年魏　文帝　七年都雒陽明帝　十三年齊王　十四年高貴鄉公　六年元帝　五年西晉　武帝　二十六年都雒陽惠帝　十六年懷帝　六年愍帝　四年都長安東晉元帝　六年都建康明帝　三年成帝　十七年康帝　二年穆帝　十七年哀帝　四年海西公　五年簡文帝　二年孝武帝　二十四年安帝　二十二年恭帝　一年宋　武帝　三年都建康前廢帝　一年文帝　三十年孝武帝　九年中廢帝　一年明帝　八年後廢帝　五年順帝　二年齊　高帝　五年都建康武帝　十年廢帝　半年新安王　半年明帝　四年東昏侯　二年南康王　一年梁　武帝　四十八年都建康簡文帝　二年孝元帝　四年都江陵西魏　齊王　二年都長安周　略陽王　二年都長安明帝　三年武帝　十八年宣帝　二年靜帝　十年大隋　開皇來　十七年都大興開皇三寶錄卷第四譯經後漢總一十二人合三百五十九部五百七十五卷後漢沙門迦葉摩騰　一部一卷經沙門竺法蘭　五部十六卷經沙門安世高　一百七十六部一百九十七卷經律沙門支婁迦讖　二十一部六十三卷經優婆塞都尉安玄　二部三卷經沙門竺佛朔　二部三卷經沙門支曜　十一部十二卷經沙門康巨　一部一卷經清信士嚴佛調　七部十卷經沙門康孟詳　六部九卷經沙門釋曇果　一部二卷經沙門竺大力　一部二卷經諸失譯經　一百二十五部二百五十九卷經呪開皇三寶錄卷第五譯經魏吳總道俗一十人合三百七十一部。",
        "from": "T49n2034_015:0122b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五百六卷魏沙門曇柯迦羅　一部一卷戒沙門康僧鎧　二部四卷經沙門曇帝　一部一卷羯磨沙門白延　六部八卷經沙門支彊梁接　一部六卷經沙門安法賢　二部五卷經吳沙門維祇難　二部六卷經沙門竺律炎　三部三卷經優婆塞支謙　一百二十九部一百五十二卷經沙門康僧會　一十四部二十九卷經及注諸失譯經　一百一十部二百九十一卷經開皇三寶錄卷第六譯經西晉總道俗一十三人合四百五十部。",
