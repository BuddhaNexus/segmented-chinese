[
    {
        "source_sentence": "寶悉地成佛陀羅尼經",
        "from": "T19n0962_001:0335a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "特進試鴻臚卿大興善寺沙門",
        "from": "T19n0962_001:0335a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大廣智不空奉　詔譯",
        "from": "T19n0962_001:0335a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T19n0962_001:0335a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時薄伽梵。",
        "from": "T19n0962_001:0335a07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在摩伽陀國清淨薗白蓮池側七寶如意樹王下與金剛手等諸大菩薩及以無量大聲聞眾無數恒沙諸天龍王藥叉羅剎異類大眾前後圍遶。",
        "from": "T19n0962_001:0335a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各禮佛足退坐一面。",
        "from": "T19n0962_001:0335a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時如來住大總持王如意寶珠轉輪祕密三摩地。",
        "from": "T19n0962_001:0335a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從頂上肉髻中放大光明。",
        "from": "T19n0962_001:0335a13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名曰雨寶滿願清淨除有情業障光明。",
        "from": "T19n0962_001:0335a14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放斯光明照觸十方。",
        "from": "T19n0962_001:0335a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆驚覺十方諸佛國土及諸八部大宮殿。",
        "from": "T19n0962_001:0335a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使六變振動普雨香花無量珍寶。",
        "from": "T19n0962_001:0335a16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣作佛事。",
        "from": "T19n0962_001:0335a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光明中出現設利如意寶珠。",
        "from": "T19n0962_001:0335a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而作廣大利益事業。",
        "from": "T19n0962_001:0335a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時會中金剛祕密手菩薩摩訶薩。",
        "from": "T19n0962_001:0335a18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與無量百千金剛族眷屬俱。",
        "from": "T19n0962_001:0335a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從座而起前禮佛足合掌恭敬而白佛言。",
        "from": "T19n0962_001:0335a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊以大慈悲。",
        "from": "T19n0962_001:0335a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愍念有情及與我等解釋所疑。",
        "from": "T19n0962_001:0335a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今日如來以何因緣放大光明。",
        "from": "T19n0962_001:0335a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及以現瑞示非常變。",
        "from": "T19n0962_001:0335a22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲知因緣。",
        "from": "T19n0962_001:0335a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊演說法要。",
        "from": "T19n0962_001:0335a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛告金剛手菩薩言。",
        "from": "T19n0962_001:0335a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉。",
        "from": "T19n0962_001:0335a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝金剛手為欲利益諸有情故。",
        "from": "T19n0962_001:0335a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恣問如來甚深法要。",
        "from": "T19n0962_001:0335a25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今演說汝等諦聽。",
        "from": "T19n0962_001:0335a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時金剛手重白佛言。",
        "from": "T19n0962_001:0335a26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願如來演說祕要。",
        "from": "T19n0962_001:0335a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等樂聞如是三請默然而住。",
        "from": "T19n0962_001:0335a27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告金剛手菩薩言。",
        "from": "T19n0962_001:0335a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我以佛眼觀察一切諸有情類。",
        "from": "T19n0962_001:0335a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖在諸趣煩惱穢中。",
        "from": "T19n0962_001:0335a29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心性寶珠無有污染。",
        "from": "T19n0962_001:0335b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "德利具足如我無異。",
        "from": "T19n0962_001:0335b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然諸有情不知苦因不證樂果。",
        "from": "T19n0962_001:0335b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常沒苦海無有出期。",
        "from": "T19n0962_001:0335b02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故如來出金剛淨界。",
        "from": "T19n0962_001:0335b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而入穢惡胎藏世界。",
        "from": "T19n0962_001:0335b03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "演說法要利樂有情。",
        "from": "T19n0962_001:0335b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅罪生福降伏魔王。",
        "from": "T19n0962_001:0335b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就佛身還住涅槃。",
        "from": "T19n0962_001:0335b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畢竟平等無所不至。",
        "from": "T19n0962_001:0335b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故如來。",
        "from": "T19n0962_001:0335b05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今日住如意寶珠王三摩地。",
        "from": "T19n0962_001:0335b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是清淨平等理趣深義法要究竟寶印。",
        "from": "T19n0962_001:0335b07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時如來說是法時。",
        "from": "T19n0962_001:0335b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切大眾證得無生甚深法忍。",
        "from": "T19n0962_001:0335b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并證如意寶珠光明三昧。",
        "from": "T19n0962_001:0335b09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同等無有次位差別。",
        "from": "T19n0962_001:0335b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如乳水。",
        "from": "T19n0962_001:0335b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時金剛手菩薩。",
        "from": "T19n0962_001:0335b10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以伽陀讚曰。",
        "from": "T19n0962_001:0335b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉大導師　　放光利益諸有情",
        "from": "T19n0962_001:0335b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉大調御　　雨寶利益諸有情",
        "from": "T19n0962_001:0335b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉大丈夫　　演說祕密利群生",
        "from": "T19n0962_001:0335b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉世尊大師子　　噉食一切諸惡魔",
        "from": "T19n0962_001:0335b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉世尊大龍王　　起大雲雨潤世間",
        "from": "T19n0962_001:0335b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願大日遍照尊　　開示般若入阿字",
        "from": "T19n0962_001:0335b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時金剛手菩薩說是偈已。",
        "from": "T19n0962_001:0335b18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手所持五峯金剛擲於空中還住手中。",
        "from": "T19n0962_001:0335b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸住本座而觀佛意。",
        "from": "T19n0962_001:0335b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目不暫捨。",
        "from": "T19n0962_001:0335b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛告金剛手及虛空藏等大菩薩言。",
        "from": "T19n0962_001:0335b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切有情與諸如來同俱一法平等法身。",
        "from": "T19n0962_001:0335b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更無別體。",
        "from": "T19n0962_001:0335b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦無別心。",
        "from": "T19n0962_001:0335b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "事理平等猶如虛空常住一相無有差別。",
        "from": "T19n0962_001:0335b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然諸眾生深著邪見。",
        "from": "T19n0962_001:0335b23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因緣諸法不捨妄念起分別相。",
        "from": "T19n0962_001:0335b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不知如來萬德光明堅固設利。",
        "from": "T19n0962_001:0335b25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常住有情一念心中。",
        "from": "T19n0962_001:0335b25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清淨白蓮猶如滿月。",
        "from": "T19n0962_001:0335b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故常生六趣貧窮下賤。",
        "from": "T19n0962_001:0335b26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不親善友不近智人。",
        "from": "T19n0962_001:0335b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造作惡業漂流生死。",
        "from": "T19n0962_001:0335b27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得悉地無有出期。",
        "from": "T19n0962_001:0335b28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故我今為度彼有情故。",
        "from": "T19n0962_001:0335b28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說大寶陀羅尼名曰法身馱都如意寶珠甘露藥王金剛精進常住真如寶王大印。",
        "from": "T19n0962_001:0335c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則末法世中於南閻浮提。",
        "from": "T19n0962_001:0335c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人。",
        "from": "T19n0962_001:0335c02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得佛設利乃至一粒分散一分信受受持。",
        "from": "T19n0962_001:0335c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知是人是佛設利真是佛子。",
        "from": "T19n0962_001:0335c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是法身釋迦牟尼如來常住之體。",
        "from": "T19n0962_001:0335c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人即名大毘盧遮那。",
        "from": "T19n0962_001:0335c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名救世大阿闍梨耶。",
        "from": "T19n0962_001:0335c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持佛設利誦此真言。",
        "from": "T19n0962_001:0335c06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得如是名號。",
        "from": "T19n0962_001:0335c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即名大智慧。",
        "from": "T19n0962_001:0335c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善巧薩埵。",
        "from": "T19n0962_001:0335c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人天一切大眾。",
        "from": "T19n0962_001:0335c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若持此呪思惟佛性禮敬設利者。",
        "from": "T19n0962_001:0335c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "消滅一切四重五逆謗法等。",
        "from": "T19n0962_001:0335c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切重罪一時消滅一切吉祥悉多成辦。",
        "from": "T19n0962_001:0335c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子及以女人。",
        "from": "T19n0962_001:0335c10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信心勇猛不懈怠。",
        "from": "T19n0962_001:0335c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心思念佛性觀察理趣消業成善。",
        "from": "T19n0962_001:0335c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常憶諷誦持念。",
        "from": "T19n0962_001:0335c12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪咀短壽怨害一時斷滅。",
        "from": "T19n0962_001:0335c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如劫火焚燒乾草無有遺餘。",
        "from": "T19n0962_001:0335c13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生慳貪業故生貧乏家。",
        "from": "T19n0962_001:0335c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多病痟衰造罪背善。",
        "from": "T19n0962_001:0335c14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應隨地獄及諸難處。",
        "from": "T19n0962_001:0335c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人懺悔先世惡業。",
        "from": "T19n0962_001:0335c15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸依三寶敬信馱都。",
        "from": "T19n0962_001:0335c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命本覺寶珠設利。",
        "from": "T19n0962_001:0335c16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即作方便莊嚴道場。",
        "from": "T19n0962_001:0335c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勸請十方諸佛如來金剛藏身。",
        "from": "T19n0962_001:0335c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不散全身碎身化身一切設利。",
        "from": "T19n0962_001:0335c18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辦備香花消罪生福。",
        "from": "T19n0962_001:0335c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況依法結印誦呪觀念自性。",
        "from": "T19n0962_001:0335c19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心中常住如來大威德設利。",
        "from": "T19n0962_001:0335c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切悉地即得成就。",
        "from": "T19n0962_001:0335c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖不入大曼陀羅灌頂壇場。",
        "from": "T19n0962_001:0335c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得入福智平等三昧耶。",
        "from": "T19n0962_001:0335c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受得諸佛許可法門甚深祕密大灌頂法。",
        "from": "T19n0962_001:0335c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即時證得佛性智海大龍王印。",
        "from": "T19n0962_001:0335c23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復告金剛手言。",
        "from": "T19n0962_001:0335c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未來世中於南閻浮提。",
        "from": "T19n0962_001:0335c24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有極重惡人及以惡畜。",
        "from": "T19n0962_001:0335c25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於命終時遇得有大度量深智上慧。",
        "from": "T19n0962_001:0335c26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具大慈悲救世大智菩薩。",
        "from": "T19n0962_001:0335c26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救世大智尊者。",
        "from": "T19n0962_001:0335c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "急取設利所在之處。",
        "from": "T19n0962_001:0335c27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殿堂塔下之土。",
        "from": "T19n0962_001:0335c27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汲於淨水洗此淨土。",
        "from": "T19n0962_001:0335c28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灌其惡人等類口中及灑頂上。",
        "from": "T19n0962_001:0335c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人臨命終時。",
        "from": "T19n0962_001:0335c29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惱亂皆息善住正念。",
        "from": "T19n0962_001:0335c29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漸生慚愧棄捨生死。",
        "from": "T19n0962_001:0336a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發往生意當得往生。",
        "from": "T19n0962_001:0336a01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速證法身之位。",
        "from": "T19n0962_001:0336a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有比丘及四輩弟子。",
        "from": "T19n0962_001:0336a02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於末法中法欲滅時。",
        "from": "T19n0962_001:0336a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切有情欲得成就大悉地法。",
        "from": "T19n0962_001:0336a03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依前法要造作塔婆。",
        "from": "T19n0962_001:0336a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其量大小隨力所辦。",
        "from": "T19n0962_001:0336a04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金銀水精及土木安置佛骨。",
        "from": "T19n0962_001:0336a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若薄福者不得設利者。",
        "from": "T19n0962_001:0336a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以金銀銅鐵摩尼海寶。",
        "from": "T19n0962_001:0336a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牛王鹿王真珠馬腦。",
        "from": "T19n0962_001:0336a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "靈石奇木如芥子許。",
        "from": "T19n0962_001:0336a07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號曰馱都安置塔婆及寶篋箱。",
        "from": "T19n0962_001:0336a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其數六粒乃至七粒以為最上數。",
        "from": "T19n0962_001:0336a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至一粒以為最下數。",
        "from": "T19n0962_001:0336a09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮拜供養頓雨七寶如雨盛降。",
        "from": "T19n0962_001:0336a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四種大法三種悉地一時圓滿。",
        "from": "T19n0962_001:0336a10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子等汝等當知如來設利即是無上世間難有。",
        "from": "T19n0962_001:0336a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸菩薩等亦難值遇。",
        "from": "T19n0962_001:0336a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況世間諸凡夫類。",
        "from": "T19n0962_001:0336a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此佛設利即是無相摩尼。",
        "from": "T19n0962_001:0336a13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是真性如意寶珠。",
        "from": "T19n0962_001:0336a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是祕密大精進也。",
        "from": "T19n0962_001:0336a14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方佛土所有莊嚴之寶。",
        "from": "T19n0962_001:0336a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如意幢幡摩尼瓔珞。",
        "from": "T19n0962_001:0336a15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "珠簾玉床真珠天蓋。",
        "from": "T19n0962_001:0336a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七寶屏風眾綵臥具。",
        "from": "T19n0962_001:0336a16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是妙物等類。",
        "from": "T19n0962_001:0336a16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最勝珍寶不能為價。",
        "from": "T19n0962_001:0336a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦復無器得置。",
        "from": "T19n0962_001:0336a17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故此經中說。",
        "from": "T19n0962_001:0336a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用團圓之物。",
        "from": "T19n0962_001:0336a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可置設利。",
        "from": "T19n0962_001:0336a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如汝心形如佛心形。",
        "from": "T19n0962_001:0336a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝心佛心互相置故。",
        "from": "T19n0962_001:0336a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝體佛體相互融故。",
        "from": "T19n0962_001:0336a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既相互融性即非二佛身。",
        "from": "T19n0962_001:0336a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設利可如是持。",
        "from": "T19n0962_001:0336a20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦可安置五輪塔中。",
        "from": "T19n0962_001:0336a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦可安置多寶塔中。",
        "from": "T19n0962_001:0336a21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦可安置三股塔中。",
        "from": "T19n0962_001:0336a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦可安置五股塔中。",
        "from": "T19n0962_001:0336a22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦可安置獨股塔中。",
        "from": "T19n0962_001:0336a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同前置物互融。",
        "from": "T19n0962_001:0336a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不異證位平等若比丘比丘尼優婆塞優婆夷四部眾等。",
        "from": "T19n0962_001:0336a24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得受持佛設利乃至一粒分散一分。",
        "from": "T19n0962_001:0336a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常行恭敬常帶者。",
        "from": "T19n0962_001:0336a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此人所到之處。",
        "from": "T19n0962_001:0336a26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉成金剛界大曼荼羅。",
        "from": "T19n0962_001:0336a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所親近一切之物。",
        "from": "T19n0962_001:0336a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆成四種法身之體。",
        "from": "T19n0962_001:0336a27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若近男近女若近牛若近馬。",
        "from": "T19n0962_001:0336a28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若近狗若近猫。",
        "from": "T19n0962_001:0336a28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若近一切飛鳥禽走獸虫蛛畜生等有情類。",
        "from": "T19n0962_001:0336a29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉令當生即入菩提初門。",
        "from": "T19n0962_001:0336b01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令得菩薩初位。",
        "from": "T19n0962_001:0336b01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊即說馱都根本陀羅尼曰。",
        "from": "T19n0962_001:0336b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵􄙺薩􅞣縳􄕗怛􄕤他􄑘蘖􄕗多􆿉塢􈃇瑟抳二合􄙯沙二􄕼馱􄕛都􈁃畝􄩶陀羅二合􄕍抳三􄙺薩􅞣嚩􄕗怛􄕤他􄑘蘖􄕡單四􄙺沙􄕼馱􄕛覩􄙘尾􄗝補􄙼悉􄕘多引五􄕽地􆵍瑟知􄕝底六􄓁󰸒􈅼吽􄙠鑁􈁟斛七􈅼吽牛鳴音􈅼吽八􅁥娑婆二合􄚩訶九佛告金剛藥叉菩薩言。",
        "from": "T19n0962_001:0336b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若諸有情欲見十方諸佛大菩薩者。",
        "from": "T19n0962_001:0336b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲求最勝悉地者。",
        "from": "T19n0962_001:0336b14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對設利塔結如來大鉢印。",
        "from": "T19n0962_001:0336b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可誦真言二萬三千遍。",
        "from": "T19n0962_001:0336b15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必得見我常住全身。",
        "from": "T19n0962_001:0336b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切悉地決定成就不作小疑。",
        "from": "T19n0962_001:0336b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則其印相者二輪相重以右手安左掌上。",
        "from": "T19n0962_001:0336b17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二空端相拄。",
        "from": "T19n0962_001:0336b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名法身大鉢印。",
        "from": "T19n0962_001:0336b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名寶篋金剛印。",
        "from": "T19n0962_001:0336b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次有神呪名如意寶珠滿願心陀羅尼。",
        "from": "T19n0962_001:0336b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能成一切事業。",
        "from": "T19n0962_001:0336b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切心中所念無不成就。",
        "from": "T19n0962_001:0336b21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯除不信不至心者輕法要者。",
        "from": "T19n0962_001:0336b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T19n0962_001:0336b22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵􄑾振􆩖多􄗥摩􄕍抳􄗽阿囉􅑈怛那􄙖縳引􄑂枳􄗷曳􄖩娜􄖩曩􄗭謨􄙢扇􄕙底􄑀迦􄗽羅􈅼吽􈅼吽􅁥娑婆􄚩訶若有人誦此真言二遍三遍乃至七遍。",
        "from": "T19n0962_001:0336b27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切罪滅一切福生。",
        "from": "T19n0962_001:0336b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日日常誦二十一遍。",
        "from": "T19n0962_001:0336b28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必得諸佛真身設利。",
        "from": "T19n0962_001:0336c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大海龍王收藏。",
        "from": "T19n0962_001:0336c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七佛頂輪王設利。",
        "from": "T19n0962_001:0336c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神變寶珠。",
        "from": "T19n0962_001:0336c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無上悉地皆能成就。",
        "from": "T19n0962_001:0336c02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有神呪名祕密真性如意珠印。",
        "from": "T19n0962_001:0336c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能持誦者無始業障速得消滅。",
        "from": "T19n0962_001:0336c04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量福智不求自得。",
        "from": "T19n0962_001:0336c04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降伏魔怨即脫生死。",
        "from": "T19n0962_001:0336c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切悉地無不成就。",
        "from": "T19n0962_001:0336c05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其印相者。",
        "from": "T19n0962_001:0336c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手寶蓮合掌。",
        "from": "T19n0962_001:0336c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭寶形二大並立。",
        "from": "T19n0962_001:0336c06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此名真性摩尼寶印。",
        "from": "T19n0962_001:0336c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名大精進峯如來頂珠印。",
        "from": "T19n0962_001:0336c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說神呪曰。",
        "from": "T19n0962_001:0336c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵􄑾振􆩖多􄗥摩􄕍尼􄕼馱􄕛都􈅼吽􅁥娑婆􄚩訶每日誦持一百八遍。",
        "from": "T19n0962_001:0336c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五部悉地三部如意寶。",
        "from": "T19n0962_001:0336c11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆悉圓滿。",
        "from": "T19n0962_001:0336c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有神呪名曰能滿。",
        "from": "T19n0962_001:0336c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所願大滿虛空藏寶。",
        "from": "T19n0962_001:0336c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能持者一切意願速得圓滿。",
        "from": "T19n0962_001:0336c13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其印相者。",
        "from": "T19n0962_001:0336c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二輪外縛。",
        "from": "T19n0962_001:0336c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭寶形二大並立。",
        "from": "T19n0962_001:0336c14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T19n0962_001:0336c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵􈎭謨􇧺馱􄙢設􄙀利􄗽羅􄙖嚩􄦽日羅􄗽囉􅑈怛曩􇩂唵􄓁󰸒􈅼吽􄙠鑁􈁟斛若得佛設利者。",
        "from": "T19n0962_001:0336c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不論淨穢隨身供養。",
        "from": "T19n0962_001:0336c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不離臆胸訶栗多妙蓮華間。",
        "from": "T19n0962_001:0336c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常念佛德晝夜不息。",
        "from": "T19n0962_001:0336c21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現身證得佛清淨身住金剛頂。",
        "from": "T19n0962_001:0336c22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人名曰持佛身者持金剛者持寶法者。",
        "from": "T19n0962_001:0336c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是大菩薩摩訶薩。",
        "from": "T19n0962_001:0336c23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛復告言。",
        "from": "T19n0962_001:0336c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子等。",
        "from": "T19n0962_001:0336c24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "假使世間若有愚童有情愚夫有情。",
        "from": "T19n0962_001:0336c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愚惑有情老耄婦女愚癡僧尼。",
        "from": "T19n0962_001:0336c25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是有情得佛設利乃至一粒分散一分。",
        "from": "T19n0962_001:0336c26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及與設利所置之物。",
        "from": "T19n0962_001:0336c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶於身上不論晝夜。",
        "from": "T19n0962_001:0336c27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若淨若不淨。",
        "from": "T19n0962_001:0336c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若觸若不觸。",
        "from": "T19n0962_001:0336c28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不離其身常可帶之。",
        "from": "T19n0962_001:0336c28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所獲功德無有所計比量。",
        "from": "T19n0962_001:0337a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若頂上若頸上。",
        "from": "T19n0962_001:0337a01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若心前若背後。",
        "from": "T19n0962_001:0337a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若兩脇若腰閒帶設利者。",
        "from": "T19n0962_001:0337a02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如世間犀牛之角。",
        "from": "T19n0962_001:0337a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人帶之離水六尺。",
        "from": "T19n0962_001:0337a03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶設利者亦復如是。",
        "from": "T19n0962_001:0337a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離一切罪業惡業惡趣不能染著。",
        "from": "T19n0962_001:0337a04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善根漸生惡障漸離。",
        "from": "T19n0962_001:0337a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善人自遇惡人自去。",
        "from": "T19n0962_001:0337a05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所作事業皆同佛行。",
        "from": "T19n0962_001:0337a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同釋迦牟尼如來故。",
        "from": "T19n0962_001:0337a06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其身即是世間應化故。",
        "from": "T19n0962_001:0337a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是清淨法身大毘盧遮那佛。",
        "from": "T19n0962_001:0337a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人必入大涅槃位。",
        "from": "T19n0962_001:0337a08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於現身中證得無相法身大精進峯。",
        "from": "T19n0962_001:0337a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故當知十二大天金剛天等。",
        "from": "T19n0962_001:0337a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切靈祇常當守護。",
        "from": "T19n0962_001:0337a10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽命延長見百秋已。",
        "from": "T19n0962_001:0337a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命終決定上生都史多天宮聞法愛樂。",
        "from": "T19n0962_001:0337a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更隨志願亦得往生十方淨土。",
        "from": "T19n0962_001:0337a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見佛聞法證不退地。",
        "from": "T19n0962_001:0337a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生即身成佛欲度有情。",
        "from": "T19n0962_001:0337a13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結有情與諸佛平等一相妙智大印。",
        "from": "T19n0962_001:0337a14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左右拳以拄左風指安心上。",
        "from": "T19n0962_001:0337a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是印亦名能滅無明黑暗印。",
        "from": "T19n0962_001:0337a15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由斯印加持故。",
        "from": "T19n0962_001:0337a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有情與諸佛。",
        "from": "T19n0962_001:0337a16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平等一相。",
        "from": "T19n0962_001:0337a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是則法身設利妙智大印。",
        "from": "T19n0962_001:0337a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名如意寶珠王成辦一切大悉地印。",
        "from": "T19n0962_001:0337a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有大陀羅尼名金輪王一字馱都明。",
        "from": "T19n0962_001:0337a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T19n0962_001:0337a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈃯母嚕唵三合急呼善男子等。",
        "from": "T19n0962_001:0337a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此呪即是設利寶珠祕密神呪。",
        "from": "T19n0962_001:0337a22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復持誦者隱密此呪。",
        "from": "T19n0962_001:0337a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無智人中輕莫演說。",
        "from": "T19n0962_001:0337a23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見其法器說此祕密最上乘法不令斷絕。",
        "from": "T19n0962_001:0337a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利益世間皆得成就。",
        "from": "T19n0962_001:0337a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛不壞馱都法身。",
        "from": "T19n0962_001:0337a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切悉地皆得成辦。",
        "from": "T19n0962_001:0337a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有興佛乘大阿闍梨。",
        "from": "T19n0962_001:0337a26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有興佛跡大量菩薩。",
        "from": "T19n0962_001:0337a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有救世大智慧者。",
        "from": "T19n0962_001:0337a27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可將此佛設利晝夜禮敬發無上誓願。",
        "from": "T19n0962_001:0337a28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雨一切珍寶妙財。",
        "from": "T19n0962_001:0337a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣利法界一切有情。",
        "from": "T19n0962_001:0337a29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從吾所住。",
        "from": "T19n0962_001:0337a29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "珍寶財物從佛設利出生。",
        "from": "T19n0962_001:0337b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "涌現令充滿漸遍法界。",
        "from": "T19n0962_001:0337b01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此寶財興隆佛法興造佛跡次當利潤有緣有情漸至無緣。",
        "from": "T19n0962_001:0337b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令其繫縛慈悲之索。",
        "from": "T19n0962_001:0337b03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至法界金剛寶場。",
        "from": "T19n0962_001:0337b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊說是法時。",
        "from": "T19n0962_001:0337b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方一切如來真身化身。",
        "from": "T19n0962_001:0337b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設利菩薩摩訶薩。",
        "from": "T19n0962_001:0337b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方佛剎金剛薩埵菩薩摩訶薩。",
        "from": "T19n0962_001:0337b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛王金剛愛菩薩等。",
        "from": "T19n0962_001:0337b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切金剛族眷屬夜叉大將諸忿怒眾。",
        "from": "T19n0962_001:0337b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曼殊師利觀自在虛空藏寶積寶幢等諸大菩薩。",
        "from": "T19n0962_001:0337b09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮華眼蓮華族等諸大士。",
        "from": "T19n0962_001:0337b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵釋護世十二大天。",
        "from": "T19n0962_001:0337b10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持明輪仙魔王人王。",
        "from": "T19n0962_001:0337b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大海主娑竭羅龍王。",
        "from": "T19n0962_001:0337b11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無熱惱池龍王善如龍王女等。",
        "from": "T19n0962_001:0337b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切大眾聞持祕法。",
        "from": "T19n0962_001:0337b12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆大歡喜信受奉行。",
        "from": "T19n0962_001:0337b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶悉地成佛陀羅尼經",
        "from": "T19n0962_001:0337b14_100",
        "context_with_next_sentence": false
    }
]