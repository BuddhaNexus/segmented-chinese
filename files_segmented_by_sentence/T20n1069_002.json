[
    {
        "source_sentence": "十一面觀自在菩薩心念誦儀軌",
        "from": "T20n1069_002:0142b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經卷中",
        "from": "T20n1069_002:0142b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀三司特進試鴻臚卿肅國",
        "from": "T20n1069_002:0142b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空謚大鑒正號大廣智大興善寺三藏沙門不空奉　詔譯此儀軌通蓮花部一切尊念誦。",
        "from": "T20n1069_002:0142b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今說脩行儀軌。",
        "from": "T20n1069_002:0142b18_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通一切觀自在法。",
        "from": "T20n1069_002:0142b18_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結護迎請供養等。",
        "from": "T20n1069_002:0142b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脩行者先應澡浴潔身淨服。",
        "from": "T20n1069_002:0142b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於清淨處對尊像。",
        "from": "T20n1069_002:0142b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當結本部三麼耶印。",
        "from": "T20n1069_002:0142b20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手合蓮花掌。",
        "from": "T20n1069_002:0142b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散餘六指如開敷蓮花。",
        "from": "T20n1069_002:0142b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀自在本部三麼耶密言曰。",
        "from": "T20n1069_002:0142b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵鉢納謨二合納婆二合縛引也娑嚩二合引訶引次應加持水澡浴。",
        "from": "T20n1069_002:0142b25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於河池或於浴室。",
        "from": "T20n1069_002:0142b25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持水密言曰。",
        "from": "T20n1069_002:0142b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨囉怛曩二合怛囉二合夜也曩莫阿引哩夜二合下同嚩路枳帝濕嚩二合下同囉也󰂛地薩怛嚩二合也摩訶薩怛嚩二合也唵三摩曳掃銘曳二合扇帝難去帝薩嚩三麼夜努鉢囉二合下同尾瑟致二合下同怒囉引努倪娑嚩二合訶引然後入池。",
        "from": "T20n1069_002:0142c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或取水澡浴。",
        "from": "T20n1069_002:0142c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結蓮華部辦事濕嚩嚩訶印　二手右押左。",
        "from": "T20n1069_002:0142c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內相叉作拳。",
        "from": "T20n1069_002:0142c04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中指相合微屈初節。",
        "from": "T20n1069_002:0142c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指附中指上節勿著。",
        "from": "T20n1069_002:0142c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如金剛形密言曰。",
        "from": "T20n1069_002:0142c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨摩訶室哩二合夜曳唵鑠討曳二合三摩曳掃去銘曳二合悉地悉地娑引馱也始吠帝商羯哩始鑁銘曳阿引嚩訶也薩嚩引囉他二合娑馱儞娑嚩二合訶引以此印護身。",
        "from": "T20n1069_002:0142c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辟除毘那夜迦。",
        "from": "T20n1069_002:0142c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及香花飲食等。",
        "from": "T20n1069_002:0142c11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除穢令光顯。",
        "from": "T20n1069_002:0142c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結甘露軍荼利印。",
        "from": "T20n1069_002:0142c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言相應。",
        "from": "T20n1069_002:0142c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "澡浴時應用思惟。",
        "from": "T20n1069_002:0142c13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二小指內相叉。",
        "from": "T20n1069_002:0142c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雙屈無名指。",
        "from": "T20n1069_002:0142c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "押叉間。",
        "from": "T20n1069_002:0142c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪合中指。",
        "from": "T20n1069_002:0142c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指附中指上節背。",
        "from": "T20n1069_002:0142c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不著如金剛。",
        "from": "T20n1069_002:0142c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指竝竪博中指側。",
        "from": "T20n1069_002:0142c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1069_002:0142c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨囉怛曩二合怛囉二合夜引也曩麼室戰二合下同拏嚩日囉二合播拏曳摩訶藥乞叉二合下同細曩引跛多曳曩謨嚩日囉二合俱𡀔二合馱引也唵虎魯虎魯底瑟咤二合底瑟咤二合滿馱滿馱訶曩訶曩阿密哩二合帝吽發次結加持土印右手四指握大指為拳。",
        "from": "T20n1069_002:0143a06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印按土上。",
        "from": "T20n1069_002:0143a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其土分作三分澡浴。",
        "from": "T20n1069_002:0143a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此密言加持。",
        "from": "T20n1069_002:0143a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1069_002:0143a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵步引入入嚩二合攞吽入一切觸穢處。",
        "from": "T20n1069_002:0143a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加護自身。",
        "from": "T20n1069_002:0143a10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用觸身忿怒烏芻沙摩印。",
        "from": "T20n1069_002:0143a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手作拳。",
        "from": "T20n1069_002:0143a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翹大指印五處。",
        "from": "T20n1069_002:0143a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂額右左肩心喉各誦一遍。",
        "from": "T20n1069_002:0143a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂上散密言曰。",
        "from": "T20n1069_002:0143a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵俱𡀔二合馱曩吽弱洗手漱口已訖。",
        "from": "T20n1069_002:0143a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應作潔淨印。",
        "from": "T20n1069_002:0143a14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仰右手掌。",
        "from": "T20n1069_002:0143a14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈無名指在掌中。",
        "from": "T20n1069_002:0143a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指頭指根相著。",
        "from": "T20n1069_002:0143a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此印承水三飲。",
        "from": "T20n1069_002:0143a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩度拭脣。",
        "from": "T20n1069_002:0143a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次印二目兩鼻兩耳兩肩心臍。",
        "from": "T20n1069_002:0143a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灑兩足。",
        "from": "T20n1069_002:0143a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又取水灑身。",
        "from": "T20n1069_002:0143a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1069_002:0143a17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵跬跬隷矩嚕娑嚩二合訶引下並同則於浴室或河池側。",
        "from": "T20n1069_002:0143a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分土等作三聚。",
        "from": "T20n1069_002:0143a19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用一聚洗從脚至腰。",
        "from": "T20n1069_002:0143a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二聚從腰至項。",
        "from": "T20n1069_002:0143a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三聚從項至頭。",
        "from": "T20n1069_002:0143a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "澡浴已了。",
        "from": "T20n1069_002:0143a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則運想佛法僧及本尊觀自在菩薩。",
        "from": "T20n1069_002:0143a22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印掬水。",
        "from": "T20n1069_002:0143a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運心沐浴聖眾。",
        "from": "T20n1069_002:0143a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仰二手掌。",
        "from": "T20n1069_002:0143a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以中指已下六指背合顯指甲二頭指。",
        "from": "T20n1069_002:0143a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲相拄二大指側此印通一切觀自在菩薩澡浴密言曰。",
        "from": "T20n1069_002:0143a25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨囉怛曩二合怛囉二合夜引也曩莫阿哩也二合嚩路枳帝濕嚩二合囉也󰂛地薩怛嚩引二合也摩訶薩怛嚩二合也怛儞也二合他惹隷摩訶惹隷娑囉跛底娑嚩二合訶引次結閼伽印。",
        "from": "T20n1069_002:0143b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仰二手掌二大指各捻頭指。",
        "from": "T20n1069_002:0143b02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掬水獻閼伽密言曰。",
        "from": "T20n1069_002:0143b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵帝囉㘑沒馱娑嚩二合訶然後以印掬水自灌頂。",
        "from": "T20n1069_002:0143b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀想觀自在菩薩持甘露賢瓶。",
        "from": "T20n1069_002:0143b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身出光明眾聖圍遶。",
        "from": "T20n1069_002:0143b06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天奏妙音樂。",
        "from": "T20n1069_002:0143b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想觀自在菩薩以甘露灌注密言者身。",
        "from": "T20n1069_002:0143b07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "軍荼利印。",
        "from": "T20n1069_002:0143b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指各住中指上節。",
        "from": "T20n1069_002:0143b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "背二大指。",
        "from": "T20n1069_002:0143b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "附頭指側密言曰。",
        "from": "T20n1069_002:0143b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨摩訶室哩二合夜曳唵訶囉訶囉摩訶尾儞曳二合度那度那跛麼跛曩也咄瑟訖哩二合擔覩嚕覩嚕尾持縫二合娑野攞。",
        "from": "T20n1069_002:0143b12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乞廁泯二合儞比也二合儞比帝二合訶曩訶曩尾覲曩尾曩引也建麼他攞具麼他攞具薩嚩迦里迦魯數跛多引半引曩室哩二合曳󰡊陛󰡊明去霓󰡊上婆惹曩儞止哩弭哩抳鑠枳曳二合惹曩儞戍馱也娑嚩二合訶燥浴既了。",
        "from": "T20n1069_002:0143b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則取淨衣。",
        "from": "T20n1069_002:0143b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此如來衣密言加持。",
        "from": "T20n1069_002:0143b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所著衣服皆成如來衣密言曰。",
        "from": "T20n1069_002:0143b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵囉乞叉二合囉乞叉二合薩嚩沒馱引地瑟恥二合多引答莽二合止引嚩囉娑嚩二合訶當誦七遍。",
        "from": "T20n1069_002:0143b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著衣已。",
        "from": "T20n1069_002:0143b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從澡浴處出。",
        "from": "T20n1069_002:0143b22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往淨室時。",
        "from": "T20n1069_002:0143b22_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離貪瞋癡。",
        "from": "T20n1069_002:0143b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不顧視穢惡雜物旃陀羅等弊惡人。",
        "from": "T20n1069_002:0143b23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當觀自身胸臆間有滿月輪。",
        "from": "T20n1069_002:0143b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即此月輪是自性光明所成。",
        "from": "T20n1069_002:0143b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩提心圓滿潔白如淨月輪。",
        "from": "T20n1069_002:0143b25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又於月輪面觀紇唎二合引字。",
        "from": "T20n1069_002:0143b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如紅頗梨色。",
        "from": "T20n1069_002:0143b26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放光照曜十方世界。",
        "from": "T20n1069_002:0143b27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於光明中自身成觀自在菩薩。",
        "from": "T20n1069_002:0143b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "等無差別。",
        "from": "T20n1069_002:0143b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手金剛拳置左𦜮持蓮花。",
        "from": "T20n1069_002:0143b28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手當自心。",
        "from": "T20n1069_002:0143b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如開敷蓮花勢。",
        "from": "T20n1069_002:0143b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀一切法自性清淨。",
        "from": "T20n1069_002:0143c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不染諸煩惱塵垢。",
        "from": "T20n1069_002:0143c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶若蓮花。",
        "from": "T20n1069_002:0143c01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身背圓光。",
        "from": "T20n1069_002:0143c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冠有無量壽如來。",
        "from": "T20n1069_002:0143c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身被眾寶瓔珞。",
        "from": "T20n1069_002:0143c02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "步踐八葉蓮花。",
        "from": "T20n1069_002:0143c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至於精室門外。",
        "from": "T20n1069_002:0143c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灌灑如前。",
        "from": "T20n1069_002:0143c03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則應加持頂。",
        "from": "T20n1069_002:0143c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手作拳。",
        "from": "T20n1069_002:0143c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大指頭指相捻。",
        "from": "T20n1069_002:0143c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即印頂誦多羅菩薩心密言曰。",
        "from": "T20n1069_002:0143c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵矩嚕矩隷娑嚩二合訶引則入精舍。",
        "from": "T20n1069_002:0143c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心念一切賢聖。",
        "from": "T20n1069_002:0143c07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慇懃五體投地作禮右膝著地。",
        "from": "T20n1069_002:0143c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍觀十方一切如來諸大菩薩。",
        "from": "T20n1069_002:0143c08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切賢聖如對目前。",
        "from": "T20n1069_002:0143c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發露懺悔隨喜勸請發願迴向無上菩提。",
        "from": "T20n1069_002:0143c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則結跏趺坐。",
        "from": "T20n1069_002:0143c10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結本部三麼耶亦通諸觀自在菩薩念誦先應用多羅菩薩護身。",
        "from": "T20n1069_002:0143c11_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘俱胝菩薩亦殊勝。",
        "from": "T20n1069_002:0143c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或誦餘四明王大威德者亦通。",
        "from": "T20n1069_002:0143c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖多羅菩薩印契者。",
        "from": "T20n1069_002:0143c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉作拳。",
        "from": "T20n1069_002:0143c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪合頭指。",
        "from": "T20n1069_002:0143c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如未敷青蓮花密言曰歸命同上加持水密言。",
        "from": "T20n1069_002:0143c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵多引㘑咄多引㘑咄㘑娑嚩二合訶次說毘俱胝菩薩印。",
        "from": "T20n1069_002:0143c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如上多羅印。",
        "from": "T20n1069_002:0143c17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少彎屈青蓮華。",
        "from": "T20n1069_002:0144a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰歸命如上。",
        "from": "T20n1069_002:0144a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵娑囉娑囉惹曳娑嚩二合訶次說四明王印。",
        "from": "T20n1069_002:0144a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以二手內相叉。",
        "from": "T20n1069_002:0144a03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印項頂密言曰。",
        "from": "T20n1069_002:0144a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵婆嚩阿引塞普二合囉又平二手掌掩自口。",
        "from": "T20n1069_002:0144a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口印密言曰。",
        "from": "T20n1069_002:0144a06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡引次結蓮花印。",
        "from": "T20n1069_002:0144a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如上開敷蓮花印。",
        "from": "T20n1069_002:0144a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置於臍臍。",
        "from": "T20n1069_002:0144a07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印密言曰。",
        "from": "T20n1069_002:0144a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵跛娜謨二合跛娜謨二合摩訶跛娜謨二合鉢納麼二合馱邏孽囉二合播抳誐多引也娑嚩二合訶引次結馬頭明王印。",
        "from": "T20n1069_002:0144a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先金剛合掌。",
        "from": "T20n1069_002:0144a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪合二中指。",
        "from": "T20n1069_002:0144a12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二頭指各鉤無名指頭。",
        "from": "T20n1069_002:0144a13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭指各押中指上節。",
        "from": "T20n1069_002:0144a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小指並竪入掌中。",
        "from": "T20n1069_002:0144a14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指並竪與小指聚。",
        "from": "T20n1069_002:0144a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1069_002:0144a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿密哩二合姤納婆二合下同舞納婆二合嚩吽發以此印密言辟除已。",
        "from": "T20n1069_002:0144a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結地界及曼荼羅界。",
        "from": "T20n1069_002:0144a18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大指相鉤。",
        "from": "T20n1069_002:0144a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散開竪諸指。",
        "from": "T20n1069_002:0144a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "揚掌如鷹翅。",
        "from": "T20n1069_002:0144a19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結方隅界。",
        "from": "T20n1069_002:0144a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1069_002:0144a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫囉怛曩二合怛囉二合夜引也曩謨枳穰二合引曩娑引誐囉吠𡀔左曩尾喻二合下同賀囉惹也怛他孽多引夜囉賀二合下同帝三藐三沒馱引也曩莫薩嚩怛他引誐．帝毘喻二合下同囉賀二合毘藥二合下同三藐三沒第毘藥二合曩莫阿哩夜二合嚩路枳帝濕嚩二合囉也󰂛地薩怛嚩二合也摩訶薩怛嚩二合也摩訶迦嚕抳迦野．怛儞也二合他伊里弭里企里弭里毘里呬里娑嚩二合訶引下並同以此密言加持香水。",
        "from": "T20n1069_002:0144b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦一遍結方隅界。",
        "from": "T20n1069_002:0144b01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結曼荼羅界。",
        "from": "T20n1069_002:0144b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手內相叉。",
        "from": "T20n1069_002:0144b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開掌竪合頭指。",
        "from": "T20n1069_002:0144b02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指極下垂相合。",
        "from": "T20n1069_002:0144b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1069_002:0144b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿引路力以此印密言結曼荼羅界應用。",
        "from": "T20n1069_002:0144b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又結辟除用密言曰。",
        "from": "T20n1069_002:0144b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵爾爾爾南引誐毘哩二合婆也陛儞甯娑嚩二合訶次說淨空界密言曰。",
        "from": "T20n1069_002:0144b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵鉢納冥二合下同儞婆誐嚩底慕賀也慕賀也惹孽慕賀儞娑嚩二合引訶以此密言加持香爐。",
        "from": "T20n1069_002:0144b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向上旋轉誦七遍。",
        "from": "T20n1069_002:0144b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先辦閼伽器。",
        "from": "T20n1069_002:0144b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "商佉或金銀。",
        "from": "T20n1069_002:0144b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "熟銅及淨葉。",
        "from": "T20n1069_002:0144b13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瓦與木器等。",
        "from": "T20n1069_002:0144b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱讚如是類。",
        "from": "T20n1069_002:0144b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "閼伽器中盛滿香水及花。",
        "from": "T20n1069_002:0144b14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨求四種事。",
        "from": "T20n1069_002:0144b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并置四色花於壇前。",
        "from": "T20n1069_002:0144b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當觀曼荼羅為大乳海。",
        "from": "T20n1069_002:0144b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此密言曰。",
        "from": "T20n1069_002:0144b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵尾麼路捺地吽以二手內相叉仰掌旋轉。",
        "from": "T20n1069_002:0144b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成甘露大海。",
        "from": "T20n1069_002:0144b18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復於大海中觀蘇彌盧山。",
        "from": "T20n1069_002:0144b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其山四寶所成。",
        "from": "T20n1069_002:0144b19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量眾寶間錯莊嚴。",
        "from": "T20n1069_002:0144b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手內相叉作拳。",
        "from": "T20n1069_002:0144b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此密言曰。",
        "from": "T20n1069_002:0144b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿者攞吽結此印誦密言。",
        "from": "T20n1069_002:0144b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思惟從大海中出生寶山已。",
        "from": "T20n1069_002:0144b23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復於山上想寶樓閣。",
        "from": "T20n1069_002:0144b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其殿無量眾寶所成。",
        "from": "T20n1069_002:0144b24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處處懸列珠鬘瓔珞。",
        "from": "T20n1069_002:0144b25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鈴鐸繒幡微風搖激。",
        "from": "T20n1069_002:0144b25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出和雅音。",
        "from": "T20n1069_002:0144b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "間錯種種摩尼半滿月等而校飾之。",
        "from": "T20n1069_002:0144b26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有無量諸供養具遍滿樓中。",
        "from": "T20n1069_002:0144b27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其殿內觀大曼荼羅。",
        "from": "T20n1069_002:0144b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是觀時以十指右押左初分相交。",
        "from": "T20n1069_002:0144b28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦後普供養密言。",
        "from": "T20n1069_002:0144b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即送七寶車輅。",
        "from": "T20n1069_002:0144b29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "送往聖者所。",
        "from": "T20n1069_002:0144c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其印以二手內相叉。",
        "from": "T20n1069_002:0144c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仰掌竪二頭指。",
        "from": "T20n1069_002:0144c01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "側相拄。",
        "from": "T20n1069_002:0144c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指各附頭指側密言曰。",
        "from": "T20n1069_002:0144c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵覩嚕覩嚕吽次結軍荼利印。",
        "from": "T20n1069_002:0144c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印相如前說密言曰。",
        "from": "T20n1069_002:0144c04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿蜜哩二合帝吽發行者觀想。",
        "from": "T20n1069_002:0144c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "軍荼利金剛駕御七寶車輅。",
        "from": "T20n1069_002:0144c06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至於極樂世界想請無量壽如來昇七寶車。",
        "from": "T20n1069_002:0144c07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中央無量壽如來坐。",
        "from": "T20n1069_002:0144c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左大勢至右邊觀自在。",
        "from": "T20n1069_002:0144c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想阿彌陀佛前本尊坐。",
        "from": "T20n1069_002:0144c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則結奉請印。",
        "from": "T20n1069_002:0144c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉作拳。",
        "from": "T20n1069_002:0144c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左大指入掌。",
        "from": "T20n1069_002:0144c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右大指竪屈向身招來去。",
        "from": "T20n1069_002:0144c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉送向外撥。",
        "from": "T20n1069_002:0144c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用蓮華部觀自在菩薩心密言曰歸命同餘觀自在密言。",
        "from": "T20n1069_002:0144c12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也二合他鉢娜麼二合鉢納麼二合鉢納麼二合播抳娑囉娑囉噎係曳二合呬婆誐刎阿引哩也二合嚩𡀔枳帝濕嚩二合囉噎迦引那捨目佉莽引嚩賀也此加句若請諸觀自在隨稱彼名阿引𡀔力則誦自本所尊密言。",
        "from": "T20n1069_002:0144c18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獻閼伽。",
        "from": "T20n1069_002:0144c18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先想淨室寶樓閣。",
        "from": "T20n1069_002:0144c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉請聖眾入中。",
        "from": "T20n1069_002:0144c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後獻座。",
        "from": "T20n1069_002:0144c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以前蓮花印諸指微相近。",
        "from": "T20n1069_002:0144c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1069_002:0144c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵鉢納麼二合尾囉也娑嚩二合訶次誦此偈敬謝聖眾伽陀曰。",
        "from": "T20n1069_002:0144c22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娑嚩二合誐擔婆誐挽甯呬鉢囉二合娑那失那去寫多引弭訶仡哩二合訶拏布惹麼娑麼二合多引鉢羅二合娑引難者地夜矩嚕此頌同真言。",
        "from": "T20n1069_002:0145a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應誦三遍或七遍。",
        "from": "T20n1069_002:0145a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應結部尊印警覺。",
        "from": "T20n1069_002:0145a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以密言相應誦三遍。",
        "from": "T20n1069_002:0145a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則成加護本尊。",
        "from": "T20n1069_002:0145a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "部尊印密言先以說。",
        "from": "T20n1069_002:0145a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬頭觀自在是也。",
        "from": "T20n1069_002:0145a06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結部母白衣觀自在菩薩印。",
        "from": "T20n1069_002:0145a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印相如多羅菩薩。",
        "from": "T20n1069_002:0145a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓屈頭指。",
        "from": "T20n1069_002:0145a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結此印亦護本尊亦護自身。",
        "from": "T20n1069_002:0145a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦速疾成就密言曰。",
        "from": "T20n1069_002:0145a09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵濕吠二合帝若致儞半拏囉嚩引悉儞惹吒麼矩吒馱引哩抳娑嚩二合引訶則結牆界印。",
        "from": "T20n1069_002:0145a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉。",
        "from": "T20n1069_002:0145a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪合二頭指微屈。",
        "from": "T20n1069_002:0145a12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1069_002:0145a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "紇唎二合引度矩度矩鉢囉二合吉引囉拏二合嚩日囉二合俱致羅句致入嚩二合下同攞嚩囉嚩日囉二合馱囉吽發下方界先以說用之。",
        "from": "T20n1069_002:0145a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則結大界印。",
        "from": "T20n1069_002:0145a17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍結護。",
        "from": "T20n1069_002:0145a17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應奉獻香等。",
        "from": "T20n1069_002:0145a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼印結上方界。",
        "from": "T20n1069_002:0145a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此結大護二手內相叉。",
        "from": "T20n1069_002:0145a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並竪中指如針。",
        "from": "T20n1069_002:0145a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭指各附中指上節下不著。",
        "from": "T20n1069_002:0145a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指各附頭指側密言曰。",
        "from": "T20n1069_002:0145a20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨婆誐嚩帝阿鉢囉二合底訶妬瑟膩二合沙引也唵商羯󰓕摩訶三麼焰娑嚩二合訶由此大三麼耶護故。",
        "from": "T20n1069_002:0145a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隣近頂輪王尚不能侵凌。",
        "from": "T20n1069_002:0145a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "況諸魔等。",
        "from": "T20n1069_002:0145a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以二手內相叉。",
        "from": "T20n1069_002:0145a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪合二頭指。",
        "from": "T20n1069_002:0145a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指極下垂相合。",
        "from": "T20n1069_002:0145a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結上方界密言曰。",
        "from": "T20n1069_002:0145a27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵爾爾爾能引誐苾𠻱二合婆也陛儞甯娑嚩二合訶次說塗香。",
        "from": "T20n1069_002:0145b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以青木香兩分多誐羅等分比哩孕愚四分蘇合香八分。",
        "from": "T20n1069_002:0145b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "細擣篩。",
        "from": "T20n1069_002:0145b02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和水再研。",
        "from": "T20n1069_002:0145b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通一切蓮花部塗香。",
        "from": "T20n1069_002:0145b03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通四種法。",
        "from": "T20n1069_002:0145b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "花燒香飲食燈明差別隨類應知。",
        "from": "T20n1069_002:0145b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獻時各以密言加持。",
        "from": "T20n1069_002:0145b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨所求事心請如上。",
        "from": "T20n1069_002:0145b05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五種供養。",
        "from": "T20n1069_002:0145b05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手捧當置於額。",
        "from": "T20n1069_002:0145b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各以供養印而奉獻。",
        "from": "T20n1069_002:0145b06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普通供養印。",
        "from": "T20n1069_002:0145b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手合掌。",
        "from": "T20n1069_002:0145b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸指初分互相交。",
        "from": "T20n1069_002:0145b07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指各安中指上節。",
        "from": "T20n1069_002:0145b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言五遍密言曰。",
        "from": "T20n1069_002:0145b08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨薩嚩沒馱引󰂛地薩怛嚩二合南引薩嚩兔那誐二合下同帝娑頗二合羅係𤚥誐誐曩劍娑嚩二合訶則誦祕密讚王。",
        "from": "T20n1069_002:0145b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歌詠讚歎本尊讚曰。",
        "from": "T20n1069_002:0145b12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵鉢納麼二合囉誐涅寧逸反麼攬迦引麼囉誐母答𤚥二合盧迦曩他滿馱銘薩嚩󰡊馱悉地者誦讚歎已。",
        "from": "T20n1069_002:0145b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意發廣大願。",
        "from": "T20n1069_002:0145b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發露懺悔發菩提心。",
        "from": "T20n1069_002:0145b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先誦本部母然後誦部尊。",
        "from": "T20n1069_002:0145b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由誦部母及部尊加護。",
        "from": "T20n1069_002:0145b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則一切罪障皆得消滅。",
        "from": "T20n1069_002:0145b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智者誦七遍或三七遍。",
        "from": "T20n1069_002:0145b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見不祥惡夢誦一百八遍則得除滅。",
        "from": "T20n1069_002:0145b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持臂釧及茅環。",
        "from": "T20n1069_002:0145b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆用部母密言。",
        "from": "T20n1069_002:0145b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如上所說。",
        "from": "T20n1069_002:0145b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "部尊密言曰。",
        "from": "T20n1069_002:0145b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日哩二合阿引𡀔力蓮花部念珠用蓮花子或摩尼寶。",
        "from": "T20n1069_002:0145b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "童女槎線。",
        "from": "T20n1069_002:0145b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此密言穿貫密言曰。",
        "from": "T20n1069_002:0145b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿密哩二合黨誐冥室哩二合曳室唎二合摩引里儞娑嚩二合引訶次結十一面觀自在根本印。",
        "from": "T20n1069_002:0145b27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手右押左外相叉合掌以印置頂上。",
        "from": "T20n1069_002:0145b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成本尊身。",
        "from": "T20n1069_002:0145b28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦根本密言七遍然後取念珠念誦。",
        "from": "T20n1069_002:0145b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲念誦取珠蟠安手中。",
        "from": "T20n1069_002:0145c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便芙蓉合掌當心。",
        "from": "T20n1069_002:0145c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦加持珠密言。",
        "from": "T20n1069_002:0145c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又便頂戴密言曰。",
        "from": "T20n1069_002:0145c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩蘇麼底室哩二合曳娑嚩二合訶以二手聚五指捻珠。",
        "from": "T20n1069_002:0145c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名念珠印。",
        "from": "T20n1069_002:0145c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此印念誦不緩不急。",
        "from": "T20n1069_002:0145c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至不疲懈。",
        "from": "T20n1069_002:0145c05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦時心不異緣。",
        "from": "T20n1069_002:0145c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀念本尊坐茅薦或瑜伽床子。",
        "from": "T20n1069_002:0145c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以密言文字實理相應。",
        "from": "T20n1069_002:0145c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或千或百數限畢已。",
        "from": "T20n1069_002:0145c07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又芙蓉合掌頂戴念珠。",
        "from": "T20n1069_002:0145c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞻觀本尊慇懃心禮。",
        "from": "T20n1069_002:0145c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復陳供養讚歎並如前法。",
        "from": "T20n1069_002:0145c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉獻閼伽。",
        "from": "T20n1069_002:0145c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結阿三󰽙擬儞印。",
        "from": "T20n1069_002:0145c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解方隅界。",
        "from": "T20n1069_002:0145c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手內相叉。",
        "from": "T20n1069_002:0145c10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中小指並竪合。",
        "from": "T20n1069_002:0145c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二頭指各安中指甲密言曰。",
        "from": "T20n1069_002:0145c11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵紇唎二合阿三莽擬儞引吽即以此印護身。",
        "from": "T20n1069_002:0146a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又結本部三摩耶印。",
        "from": "T20n1069_002:0146a01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮佛迴向等已。",
        "from": "T20n1069_002:0146a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方出道場。",
        "from": "T20n1069_002:0146a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一淨處讀轉摩訶般若波羅蜜。",
        "from": "T20n1069_002:0146a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "積集福德聚迴向無上菩提隨意經行。",
        "from": "T20n1069_002:0146a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復結無能勝印。",
        "from": "T20n1069_002:0146a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切時處加護。",
        "from": "T20n1069_002:0146a04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉作拳。",
        "from": "T20n1069_002:0146a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪合二中指。",
        "from": "T20n1069_002:0146a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名無能勝印。",
        "from": "T20n1069_002:0146a05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1069_002:0146a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫薩嚩沒馱南唵虎魯虎魯贊拏里莽引蹬儗娑嚩二合訶修行者每朝嚼齒木洗漱已訖。",
        "from": "T20n1069_002:0146a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結淨灌灑。",
        "from": "T20n1069_002:0146a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手掬水誦此密言七遍加飲。",
        "from": "T20n1069_002:0146a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或六月先行成就法。",
        "from": "T20n1069_002:0146a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有觸穢不祥業障皆得清淨密言曰。",
        "from": "T20n1069_002:0146a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵󰡊第訥輸馱曩也娑嚩二合引訶十一面觀自在菩薩念誦儀軌經卷中",
        "from": "T20n1069_002:0146a14_100",
        "context_with_next_sentence": false
    }
]