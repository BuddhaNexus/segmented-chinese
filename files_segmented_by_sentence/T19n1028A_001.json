[
    {
        "source_sentence": "佛說護諸童子陀羅尼經",
        "from": "T19n1028A_001:0741b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元魏天竺三藏菩提流支譯",
        "from": "T19n1028A_001:0741b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時如來初成正覺。有一大梵天王。來詣佛",
        "from": "T19n1028A_001:0741b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所敬禮佛足。",
        "from": "T19n1028A_001:0741b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而作是言。",
        "from": "T19n1028A_001:0741b23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無佛陀耶　　南無達摩耶",
        "from": "T19n1028A_001:0741b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無僧伽耶　　我禮佛世尊",
        "from": "T19n1028A_001:0741b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照世大法王　　在於閻浮提",
        "from": "T19n1028A_001:0741b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最初說神呪　　甘露淨勝法",
        "from": "T19n1028A_001:0741b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及禮無著僧　　已禮牟尼足",
        "from": "T19n1028A_001:0741b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即時說偈言　　世尊諸如來",
        "from": "T19n1028A_001:0741b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聲聞及辟支　　諸仙護世王",
        "from": "T19n1028A_001:0741c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大力龍天神　　如是等諸眾",
        "from": "T19n1028A_001:0741c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆於人中生　　有夜叉羅剎",
        "from": "T19n1028A_001:0741c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常憙噉人胎　　非人王境界",
        "from": "T19n1028A_001:0741c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "強力所不制　　能令人無子",
        "from": "T19n1028A_001:0741c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傷害於胞胎　　男女交會時",
        "from": "T19n1028A_001:0741c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使其意迷亂　　懷任不成就",
        "from": "T19n1028A_001:0741c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或歌羅安浮　　無子以傷胎",
        "from": "T19n1028A_001:0741c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及生時奪命　　皆是諸惡鬼",
        "from": "T19n1028A_001:0741c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為其作嬈害　　我今說彼名",
        "from": "T19n1028A_001:0741c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願佛聽我說第一名彌酬迦　第二名彌伽王　第三名騫陀　第四名阿波悉魔羅　第五名牟致迦　第六名魔致迦　第七名閻彌迦　第八名迦彌尼　第九名黎婆坻　第十名富多那　第十一名曼多難提　第十二名舍究尼　第十三名揵吒波尼尼　第十四名目佉曼荼　第十五名藍婆此十五鬼神。",
        "from": "T19n1028A_001:0741c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常遊行世間。",
        "from": "T19n1028A_001:0741c19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為嬰孩小兒。",
        "from": "T19n1028A_001:0741c19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而作於恐怖。",
        "from": "T19n1028A_001:0741c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今當說此諸鬼神恐怖形相。",
        "from": "T19n1028A_001:0741c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此形相。",
        "from": "T19n1028A_001:0741c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸小兒皆生驚畏。",
        "from": "T19n1028A_001:0741c21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彌酬迦者其形如牛。",
        "from": "T19n1028A_001:0741c22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彌迦王者其形如師子。",
        "from": "T19n1028A_001:0741c23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "騫陀者其形如鳩魔羅天。",
        "from": "T19n1028A_001:0741c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿波悉魔羅者其形如野狐。",
        "from": "T19n1028A_001:0741c25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牟致迦者其形如獼猴。",
        "from": "T19n1028A_001:0741c26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魔致迦者其形如羅剎女。",
        "from": "T19n1028A_001:0741c27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "閻彌迦者其形如馬。",
        "from": "T19n1028A_001:0741c28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦彌尼者其形如婦女。",
        "from": "T19n1028A_001:0741c29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黎婆坻者其形如狗。",
        "from": "T19n1028A_001:0742a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "富多那者其形如猪。",
        "from": "T19n1028A_001:0742a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曼多難提者其形如猫兒。",
        "from": "T19n1028A_001:0742a03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍究尼者其形如鳥。",
        "from": "T19n1028A_001:0742a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乾吒婆尼尼者其形如雉。",
        "from": "T19n1028A_001:0742a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目佉曼荼者其形如獯狐。",
        "from": "T19n1028A_001:0742a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藍婆者其形如蛇。",
        "from": "T19n1028A_001:0742a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此十五鬼。",
        "from": "T19n1028A_001:0742a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神著諸小兒令其驚怖。",
        "from": "T19n1028A_001:0742a08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今當復說諸小兒怖畏之相。",
        "from": "T19n1028A_001:0742a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彌酬迦鬼者著小兒眼睛迴轉。",
        "from": "T19n1028A_001:0742a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彌迦王鬼者著小兒數數嘔吐。",
        "from": "T19n1028A_001:0742a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "騫陀鬼者著小兒其兩肩動。",
        "from": "T19n1028A_001:0742a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿波悉魔羅鬼者著小兒口中沫出。",
        "from": "T19n1028A_001:0742a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牟致迦鬼者著小兒把捲不展。",
        "from": "T19n1028A_001:0742a14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魔致迦鬼者著小兒自齧其舌。",
        "from": "T19n1028A_001:0742a15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "閻彌迦鬼者著小兒憙啼喜笑。",
        "from": "T19n1028A_001:0742a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦彌尼鬼者著小兒樂著女人。",
        "from": "T19n1028A_001:0742a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黎婆坻鬼者著小兒見種種雜相。",
        "from": "T19n1028A_001:0742a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "富多那鬼者著小兒眠中驚怖啼哭。",
        "from": "T19n1028A_001:0742a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曼多難提鬼者著小兒憙啼喜笑。",
        "from": "T19n1028A_001:0742a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金究尼鬼者著小兒不肯飲乳。",
        "from": "T19n1028A_001:0742a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乾吒波尼尼鬼者著小兒咽喉聲塞。",
        "from": "T19n1028A_001:0742a22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目佉曼荼鬼者著小兒時氣熱病下痢。",
        "from": "T19n1028A_001:0742a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藍婆鬼者著小兒數數噫噦。",
        "from": "T19n1028A_001:0742a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此十五鬼神。",
        "from": "T19n1028A_001:0742a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以如是等形。",
        "from": "T19n1028A_001:0742a25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怖諸小兒。",
        "from": "T19n1028A_001:0742a25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及其小兒驚怖之相。",
        "from": "T19n1028A_001:0742a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我皆已說。",
        "from": "T19n1028A_001:0742a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有大鬼神王。",
        "from": "T19n1028A_001:0742a26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名栴檀乾闥婆於諸鬼神最為上首。",
        "from": "T19n1028A_001:0742a27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當以五色綖。",
        "from": "T19n1028A_001:0742a27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此陀羅尼一遍一結。",
        "from": "T19n1028A_001:0742a28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作一百八結。",
        "from": "T19n1028A_001:0742a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并書其鬼神名字。",
        "from": "T19n1028A_001:0742a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使人齎此書綖。",
        "from": "T19n1028A_001:0742a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "語彼使言。",
        "from": "T19n1028A_001:0742a29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今疾去急速如風到於四方。",
        "from": "T19n1028A_001:0742b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨彼十五鬼神所住之處。",
        "from": "T19n1028A_001:0742b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與栴檀乾闥婆大鬼神王。",
        "from": "T19n1028A_001:0742b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令以五色線縛彼鬼神勿嬈眾生。",
        "from": "T19n1028A_001:0742b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼以種種美味飲食香華燈明。",
        "from": "T19n1028A_001:0742b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及以乳粥供養神王。",
        "from": "T19n1028A_001:0742b04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時。",
        "from": "T19n1028A_001:0742b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大梵天王。",
        "from": "T19n1028A_001:0742b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復白佛言。",
        "from": "T19n1028A_001:0742b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊若有女人不生男女。",
        "from": "T19n1028A_001:0742b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或在胎中失壞墮落。",
        "from": "T19n1028A_001:0742b06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或生已奪命。",
        "from": "T19n1028A_001:0742b06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此諸女等欲求子息保命長壽者。",
        "from": "T19n1028A_001:0742b07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常當繫念修行善法。",
        "from": "T19n1028A_001:0742b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於月八日十五日受持八戒。",
        "from": "T19n1028A_001:0742b08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清淨洗浴著新淨衣。",
        "from": "T19n1028A_001:0742b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮十方佛至於中夜。",
        "from": "T19n1028A_001:0742b09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取少芥子置已頂上。",
        "from": "T19n1028A_001:0742b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦我所說陀羅尼呪者。",
        "from": "T19n1028A_001:0742b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令此女人即得如願。",
        "from": "T19n1028A_001:0742b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所生童子安隱無患。",
        "from": "T19n1028A_001:0742b11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡其形壽終不中夭。",
        "from": "T19n1028A_001:0742b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有鬼神不順我呪者。",
        "from": "T19n1028A_001:0742b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當令其頭破作七分如阿梨樹枝。",
        "from": "T19n1028A_001:0742b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說護諸童子陀羅尼呪。",
        "from": "T19n1028A_001:0742b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多𭊾他　阿伽羅　伽甯　那伽　伽甯　婆漏隷　秖隷　伽婆隷　婆隷　不隷　羅扠禰　修羅俾　遮羅俾　娑尼　波羅呵　沙尼　那易　彌那易　穌婆呵世尊我今說此陀羅尼呪護諸童子。",
        "from": "T19n1028A_001:0742b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令得安隱獲其長壽故。",
        "from": "T19n1028A_001:0742b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊一切種智。",
        "from": "T19n1028A_001:0742b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T19n1028A_001:0742b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多𭊾他　菩陀菩陀　菩陀㝹摩帝　菩提菩提　摩隷　式叉夜　婆舍利　婆多禰　婆羅陀　頭隷　頭隷　婆臘多頭隷　舍摩膩　收隷　婆呵膩　陀波膩　穌婆呵　膩婆羅膩　穌婆呵此十五鬼神常食血肉。",
        "from": "T19n1028A_001:0742b27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此陀羅尼呪力故。",
        "from": "T19n1028A_001:0742b27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆遠離不生惡心。",
        "from": "T19n1028A_001:0742b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸童子離於恐怖安隱無患。",
        "from": "T19n1028A_001:0742b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處胎初生無諸患難。",
        "from": "T19n1028A_001:0742b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此呪者。",
        "from": "T19n1028A_001:0742b29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於城邑聚落隨其住處。",
        "from": "T19n1028A_001:0742c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能令彼嬰孩小兒。",
        "from": "T19n1028A_001:0742c01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長得安隱終保年壽。",
        "from": "T19n1028A_001:0742c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無佛陀成就此呪。",
        "from": "T19n1028A_001:0742c02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護諸童子不為諸惡鬼神之所嬈害。",
        "from": "T19n1028A_001:0742c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸難一切恐怖。",
        "from": "T19n1028A_001:0742c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆遠離穌婆呵。",
        "from": "T19n1028A_001:0742c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時此梵天聞說此經歡欣奉行。",
        "from": "T19n1028A_001:0742c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說護諸童子陀羅尼經",
        "from": "T19n1028A_001:0742c06_100",
        "context_with_next_sentence": false
    }
]