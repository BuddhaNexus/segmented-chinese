[
    {
        "source_sentence": "梵天火羅九曜",
        "from": "T21n1311_001:0459b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一行禪師修述。二十八宿在天左轉。數知",
        "from": "T21n1311_001:0459b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人犯觸之位。大唐武德元年起戊寅。至咸",
        "from": "T21n1311_001:0459b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通十五年甲午。",
        "from": "T21n1311_001:0459b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "都得二百五十七年矣。",
        "from": "T21n1311_001:0459b07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵天火羅九曜及暗虛二星圖在此。",
        "from": "T21n1311_001:0459b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但諸星都所在。",
        "from": "T21n1311_001:0459b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "看之決定。",
        "from": "T21n1311_001:0459b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一生吉凶萬不失一神妙之極云云。",
        "from": "T21n1311_001:0459b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一羅睺蝕神星明曰。",
        "from": "T21n1311_001:0459b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵囉戶曩阿素三合囉邏惹野塞麻捨覩野曩名位扇底伽哩莎婆訶羅睺星以錢供養口決云向丑寅供之。",
        "from": "T21n1311_001:0459b14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年一十十九二十八三十七四十六五十五六十四七十三八十二九十一并大凶。",
        "from": "T21n1311_001:0459b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行年至此宿者凶。",
        "from": "T21n1311_001:0459b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "星隱而不見。",
        "from": "T21n1311_001:0459b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名羅睺。",
        "from": "T21n1311_001:0459b26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名羅師。",
        "from": "T21n1311_001:0459b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名黃幡。",
        "from": "T21n1311_001:0459b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名火陽。",
        "from": "T21n1311_001:0459b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨人本命。",
        "from": "T21n1311_001:0459b27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憂官失位。",
        "from": "T21n1311_001:0459b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重病相纏。",
        "from": "T21n1311_001:0459b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "財物破散喪服愁口舌。",
        "from": "T21n1311_001:0459b28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國王以神不祭至此宿修福攘之。",
        "from": "T21n1311_001:0459b29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元神錢畫所犯神形供養大吉云云人命屬大清天。",
        "from": "T21n1311_001:0459c01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此年百事如意。",
        "from": "T21n1311_001:0459c01_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求必得。",
        "from": "T21n1311_001:0459c01_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行來出入皆大吉利。",
        "from": "T21n1311_001:0459c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中宮土宿星王在四季鷄緩　真言。",
        "from": "T21n1311_001:0459c03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命唵贊日利曳娑婆賀又真言曰。",
        "from": "T21n1311_001:0459c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵捨泥殺作二合羅曩乞殺二合怛囉二合跛羅二合訶曩嚕娑野名位普瑟底二合迦哩娑縛二合賀又真言。",
        "from": "T21n1311_001:0459c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵戍戍羯囉誐馱嚩二合囉嚩二合邏嚩二合邏若名位室哩娑婆二合賀春巽夏坤秋乾冬艮　季夏月菓子一盤祭之　口決云其方不定。",
        "from": "T21n1311_001:0459c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "春季向巽餘以次知之。",
        "from": "T21n1311_001:0459c12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年二十一二十二十九三十八四十七五十六六十五七十四八十三九十二并大凶。",
        "from": "T21n1311_001:0459c14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行年至此宿者是中宮土星。",
        "from": "T21n1311_001:0459c27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其星周九十里屬楚國之分。",
        "from": "T21n1311_001:0459c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其宿最凶。",
        "from": "T21n1311_001:0459c28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偏臨宮之然及遮人此宿貞慎疾病牢獄。",
        "from": "T21n1311_001:0459c29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "君子重厄之年號曰土星。",
        "from": "T21n1311_001:0460a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不欲犯之。",
        "from": "T21n1311_001:0460a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其形如波羅門。",
        "from": "T21n1311_001:0460a01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牛冠首手持錫杖。",
        "from": "T21n1311_001:0460a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與為禍國王以季夏月菓子一槃祭之。",
        "from": "T21n1311_001:0460a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宜送本命元神錢。",
        "from": "T21n1311_001:0460a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫所犯神形供養。",
        "from": "T21n1311_001:0460a03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黃衣攘之。",
        "from": "T21n1311_001:0460a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必消災增福壽命本命。",
        "from": "T21n1311_001:0460a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屬赤虛天百事不如意。",
        "from": "T21n1311_001:0460a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "男不宜官女不宜夫。",
        "from": "T21n1311_001:0460a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忌三月九月庚辛日。",
        "from": "T21n1311_001:0460a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三嘀北辰星水星王在冬三月明曰。",
        "from": "T21n1311_001:0460a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵俱悉陀他姪佗利多崖崖紫紫儞帝莎婆訶又明曰。",
        "from": "T21n1311_001:0460a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵母馱曩乞殺二合娑嚩二合弭曩名位契努二合摩莎縛二合賀又心呪曰。",
        "from": "T21n1311_001:0460a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵蘇底哩二合瑟吒莎婆訶以中夏之月用油祭向北方供之年三十二二十一三十三十九四十八五十七六十六七十五八十四九十三少吉。",
        "from": "T21n1311_001:0460a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行年至此宿名北辰。",
        "from": "T21n1311_001:0460a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名毚星一名滴星。",
        "from": "T21n1311_001:0460a23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周迴一百里屬燕趙之分野。",
        "from": "T21n1311_001:0460a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若臨人本命。",
        "from": "T21n1311_001:0460a24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "主陰愁口舌盜賊牽喚。",
        "from": "T21n1311_001:0460a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其神狀婦人。",
        "from": "T21n1311_001:0460a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭首戴猿冠手持紙筆。",
        "from": "T21n1311_001:0460a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國王以仲夏之月用油祭。",
        "from": "T21n1311_001:0460a26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宜醮北斗穰之。",
        "from": "T21n1311_001:0460a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數此十二宮。",
        "from": "T21n1311_001:0460a27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從生月下一宿為一日。",
        "from": "T21n1311_001:0460a27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左數至生日止即知也云云。",
        "from": "T21n1311_001:0460a28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已行無異本人命屬遊咸天百事不成。",
        "from": "T21n1311_001:0460a29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "男不宜官女不宜夫。",
        "from": "T21n1311_001:0460a29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忌四月丙戌日。",
        "from": "T21n1311_001:0460b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四西方大白星金星金神也王在秋三月明曰。",
        "from": "T21n1311_001:0460b02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命唵吠尾毘娑婆訶又真言曰。",
        "from": "T21n1311_001:0460b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵戍上羯囉誐馱嚩馱嚩二合邏若名位室哩娑婆訶常以仲秋之月用生錢祭之向。",
        "from": "T21n1311_001:0460b07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方供之年四十三二十二三十一四十四十九五十八六十七七十六八十五九十四少凶。",
        "from": "T21n1311_001:0460b09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行年至那頡■。",
        "from": "T21n1311_001:0460b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是太白星西方金精也。",
        "from": "T21n1311_001:0460b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其星一名大白。",
        "from": "T21n1311_001:0460b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名長庚。",
        "from": "T21n1311_001:0460b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名那頡。",
        "from": "T21n1311_001:0460b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其星周迴一百里屬秦國之分野。",
        "from": "T21n1311_001:0460b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若臨人年本命。",
        "from": "T21n1311_001:0460b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至有哭泣刀兵。",
        "from": "T21n1311_001:0460b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形如女人。",
        "from": "T21n1311_001:0460b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭戴首冠。",
        "from": "T21n1311_001:0460b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白練衣彈絃。",
        "from": "T21n1311_001:0460b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與人為患。",
        "from": "T21n1311_001:0460b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國王常以仲秋之月用生錢祭之。",
        "from": "T21n1311_001:0460b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行年至此星。",
        "from": "T21n1311_001:0460b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宜著白衣醮謝本命元神作福田。",
        "from": "T21n1311_001:0460b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大吉也。",
        "from": "T21n1311_001:0460b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人命屬飛揰天百事皆吉憂解患。",
        "from": "T21n1311_001:0460b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "男財宜官婦子相係。",
        "from": "T21n1311_001:0460b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "治生萬倍。",
        "from": "T21n1311_001:0460b23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五大陽密日星真言曰。",
        "from": "T21n1311_001:0460b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨羅怛曩二合怛羅夜引野曩莫素哩野薩縛曩乞羅二合怛囉二合羅惹野唵阿謨伽寫名位設底去莎婆訶以冬至之日用眾寶祭之。",
        "from": "T21n1311_001:0460b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向卯辰供之年五十四二十三三十二四十一五十五十九六十八七十七八十六九十五大吉。",
        "from": "T21n1311_001:0460c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行年至此宿者。",
        "from": "T21n1311_001:0460c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "主大陽屬日。",
        "from": "T21n1311_001:0460c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其星周迴一千五百里。",
        "from": "T21n1311_001:0460c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日一周天。",
        "from": "T21n1311_001:0460c03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若臨人本命。",
        "from": "T21n1311_001:0460c03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加官進祿有喜事。",
        "from": "T21n1311_001:0460c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常得貴人接引。",
        "from": "T21n1311_001:0460c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所作通達。",
        "from": "T21n1311_001:0460c04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國王以冬至之日用眾寶祭之大吉矣　此星真言不可思議。",
        "from": "T21n1311_001:0460c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若至心帶佩并供養一年。",
        "from": "T21n1311_001:0460c06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非橫不死云云。",
        "from": "T21n1311_001:0460c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人命屬通光■百事如意。",
        "from": "T21n1311_001:0460c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天神相保終無實難。",
        "from": "T21n1311_001:0460c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所向開通福祿自至。",
        "from": "T21n1311_001:0460c08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六南方火熒惑星火王在夏三月火星真言曰。",
        "from": "T21n1311_001:0460c17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命唵摩訶利多莎婆訶又真言曰。",
        "from": "T21n1311_001:0460c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿誐羅迦嚕儗野名位娑婆二合賀以仲夏之月火祭之。",
        "from": "T21n1311_001:0460c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向南方供之。",
        "from": "T21n1311_001:0460c21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年六十五二十四三十三四十二五十一六十六十九七十八八十七九十六並是凶年也。",
        "from": "T21n1311_001:0460c23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行年至此星。",
        "from": "T21n1311_001:0460c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名南方熒惑星。",
        "from": "T21n1311_001:0460c24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名四利星。",
        "from": "T21n1311_001:0460c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名虛漢。",
        "from": "T21n1311_001:0460c25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其星周迴七十里。",
        "from": "T21n1311_001:0460c25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屬魏國之分。",
        "from": "T21n1311_001:0460c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若臨人命。",
        "from": "T21n1311_001:0460c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必生口舌。",
        "from": "T21n1311_001:0460c26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疾病相纏。",
        "from": "T21n1311_001:0460c26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神形如外道。",
        "from": "T21n1311_001:0460c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "首戴驢冠。",
        "from": "T21n1311_001:0460c27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四手兵器刀刃。",
        "from": "T21n1311_001:0460c27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國王以仲夏之月。",
        "from": "T21n1311_001:0460c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用火祭之。",
        "from": "T21n1311_001:0460c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行年至此者。",
        "from": "T21n1311_001:0460c28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "君子臨福。",
        "from": "T21n1311_001:0460c28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "職事進益。",
        "from": "T21n1311_001:0460c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小人臨福。",
        "from": "T21n1311_001:0460c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "災害不生。",
        "from": "T21n1311_001:0460c29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬之即無殃咎。",
        "from": "T21n1311_001:0460c29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人命屬雲霄。",
        "from": "T21n1311_001:0461a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天神相保。",
        "from": "T21n1311_001:0461a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不敢蘖禍。",
        "from": "T21n1311_001:0461a01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "錢財滿盈。",
        "from": "T21n1311_001:0461a01_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福祿廣至。",
        "from": "T21n1311_001:0461a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖有少憂。",
        "from": "T21n1311_001:0461a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自解無苦。",
        "from": "T21n1311_001:0461a02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七計都蝕神星明曰。",
        "from": "T21n1311_001:0461a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵縛日羅二合計都曩引曩乞殺二合怛羅二合邏惹野名位吽畫此形深室供養禳之。",
        "from": "T21n1311_001:0461a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迴禍為福。",
        "from": "T21n1311_001:0461a11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向未申供之。",
        "from": "T21n1311_001:0461a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅睺帶珠寶。",
        "from": "T21n1311_001:0461a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並日月計都著錦繡衣。",
        "from": "T21n1311_001:0461a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年七十六二十五三十四四十三五十二六十一七十九八十八九十七並凶也。",
        "from": "T21n1311_001:0461a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行年至此計都亦是隱星。",
        "from": "T21n1311_001:0461a15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名豹尾。",
        "from": "T21n1311_001:0461a15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名大隱。",
        "from": "T21n1311_001:0461a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "首隱不見。",
        "from": "T21n1311_001:0461a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不見而行無定形。",
        "from": "T21n1311_001:0461a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若臨人名。",
        "from": "T21n1311_001:0461a16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "官最多逼塞。",
        "from": "T21n1311_001:0461a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求官不遂。",
        "from": "T21n1311_001:0461a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "務被遷移。",
        "from": "T21n1311_001:0461a17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "官符相纏。",
        "from": "T21n1311_001:0461a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多憂疾病。",
        "from": "T21n1311_001:0461a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此星凶。",
        "from": "T21n1311_001:0461a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國王不祭。",
        "from": "T21n1311_001:0461a18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行年至此須送五道司命。",
        "from": "T21n1311_001:0461a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫此神形。",
        "from": "T21n1311_001:0461a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深室供養禳之。",
        "from": "T21n1311_001:0461a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迴禍作福。",
        "from": "T21n1311_001:0461a29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人命屬溫獨天。",
        "from": "T21n1311_001:0461a29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所向不如意。",
        "from": "T21n1311_001:0461b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行■無利。",
        "from": "T21n1311_001:0461b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫婦哭泣。",
        "from": "T21n1311_001:0461b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或夫流淚。",
        "from": "T21n1311_001:0461b01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忌五月六月甲乙日。",
        "from": "T21n1311_001:0461b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八暮大陰月天明曰。",
        "from": "T21n1311_001:0461b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵戰上怛羅二合曩乞灑二合引怛羅二合邏惹野名位設底娑婆賀常以夏至之日。",
        "from": "T21n1311_001:0461b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾寶玉及水祭之向申酉供之。",
        "from": "T21n1311_001:0461b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年八十七二十六三十五四十四五十三六十二七十一八十八十九九十八。",
        "from": "T21n1311_001:0461b08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行年至此宿位者。",
        "from": "T21n1311_001:0461b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是太陰。",
        "from": "T21n1311_001:0461b18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "也屬月。",
        "from": "T21n1311_001:0461b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其月周迴一千五百里。",
        "from": "T21n1311_001:0461b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日周迴天下。",
        "from": "T21n1311_001:0461b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若臨人本命大吉。",
        "from": "T21n1311_001:0461b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加官進祿所作通達。",
        "from": "T21n1311_001:0461b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得貴人接引。",
        "from": "T21n1311_001:0461b20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長有喜慶之事。",
        "from": "T21n1311_001:0461b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國王以夏至之日。",
        "from": "T21n1311_001:0461b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用眾珍玉及水祭之。",
        "from": "T21n1311_001:0461b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行年至此方。",
        "from": "T21n1311_001:0461b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "事通和求官得遂所作大吉。",
        "from": "T21n1311_001:0461b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人命屬微供天千百事皆凶。",
        "from": "T21n1311_001:0461b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遭病恐死田蠶不收憂縣官事。",
        "from": "T21n1311_001:0461b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忌正月九月丙丁日。",
        "from": "T21n1311_001:0461b24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九東方歲星王在春三月木星明曰。",
        "from": "T21n1311_001:0461b25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三曼多沒馱喃唵印那羅野娑婆訶又真言曰。",
        "from": "T21n1311_001:0461b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵婆羅二合訶薩鉢二合底曩摩地比跢縛曩二合野名位摩攞縛馱二合寧婆縛二合賀引常以仲春月用眾寶祭之。",
        "from": "T21n1311_001:0461c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向東供之。",
        "from": "T21n1311_001:0461c01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年九十八二十七三十六四十五五十四六十三七十二八十一九十九十九並是大吉。",
        "from": "T21n1311_001:0461c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行年至嗢沒斯者是歲星。",
        "from": "T21n1311_001:0461c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東方木精。",
        "from": "T21n1311_001:0461c04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名攝提。",
        "from": "T21n1311_001:0461c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其星周迴一百里屬魯衛之分。",
        "from": "T21n1311_001:0461c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨人本命。",
        "from": "T21n1311_001:0461c05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加官進祿。",
        "from": "T21n1311_001:0461c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬事吉祥。",
        "from": "T21n1311_001:0461c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其神形如卿相。",
        "from": "T21n1311_001:0461c06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著青衣。",
        "from": "T21n1311_001:0461c06_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戴亥冠。",
        "from": "T21n1311_001:0461c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手執華菓。",
        "from": "T21n1311_001:0461c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國王常以仲春月。",
        "from": "T21n1311_001:0461c07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用眾寶祭之。",
        "from": "T21n1311_001:0461c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行年至此宿宜與貴人交通婚姻和合。",
        "from": "T21n1311_001:0461c14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此年■■大吉之事也。",
        "from": "T21n1311_001:0461c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人命屬高倉天萬事皆諧。",
        "from": "T21n1311_001:0461c15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所向如意往來有利。",
        "from": "T21n1311_001:0461c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居為平安子孫熾昌。",
        "from": "T21n1311_001:0461c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北斗七星明曰。",
        "from": "T21n1311_001:0461c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼多那羅那羅破左邏吽九執明曰。",
        "from": "T21n1311_001:0461c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命唵蘖羅二醯涅嚩二合哩也二合鉢囉鉢多二合𭮥底囉摩野三娑婆二合賀蝕神頭從正月至年終常居二宿翼宿張宿羅尾宿低宿計蝕神尾從正月至年終帶居此二宿以上無異本按聿斯經云。",
        "from": "T21n1311_001:0461c28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡人只知有七曜不晴虛星號曰羅睺計都。",
        "from": "T21n1311_001:0461c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此星在隱位而不見。",
        "from": "T21n1311_001:0461c29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逢日月即蝕。",
        "from": "T21n1311_001:0462a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號曰蝕神。",
        "from": "T21n1311_001:0462a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "計都者蝕神之尾也。",
        "from": "T21n1311_001:0462a01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號豹尾。",
        "from": "T21n1311_001:0462a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若行年到此宿。",
        "from": "T21n1311_001:0462a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "切須畫所犯神形。",
        "from": "T21n1311_001:0462a02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深室供養燒錢攘之。",
        "from": "T21n1311_001:0462a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即災害不生。",
        "from": "T21n1311_001:0462a03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遇惡星須攘之。",
        "from": "T21n1311_001:0462a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸不逆其所犯。",
        "from": "T21n1311_001:0462a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即變凶成吉。",
        "from": "T21n1311_001:0462a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不信即變吉成凶。",
        "from": "T21n1311_001:0462a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遇吉星喜慶重重福德自在。",
        "from": "T21n1311_001:0462a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遇惡星災害競生。",
        "from": "T21n1311_001:0462a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王侯犯之即謫官降職。",
        "from": "T21n1311_001:0462a06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但以亥時面向北斗。",
        "from": "T21n1311_001:0462a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至心祭拜本命星。",
        "from": "T21n1311_001:0462a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "切不得向北小便折人壽命。",
        "from": "T21n1311_001:0462a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宜思真念善獲福宜財。",
        "from": "T21n1311_001:0462a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不穰之災害競起。",
        "from": "T21n1311_001:0462a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "葛仙公禮北斗法葛仙公禮北斗法鎮上玄九北極北斗。",
        "from": "T21n1311_001:0462a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從王侯及於士庶。",
        "from": "T21n1311_001:0462a11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡皆屬北斗七星。",
        "from": "T21n1311_001:0462a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常須敬重。",
        "from": "T21n1311_001:0462a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當不逢橫禍凶惡之事。",
        "from": "T21n1311_001:0462a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍救世人之襄厄。",
        "from": "T21n1311_001:0462a13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得延年益算無諸災難。",
        "from": "T21n1311_001:0462a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并本命元神至心供養。",
        "from": "T21n1311_001:0462a14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆得󱁲遂人之命祿。",
        "from": "T21n1311_001:0462a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "災害殃咎迷塞澁。",
        "from": "T21n1311_001:0462a15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆由不敬星像。",
        "from": "T21n1311_001:0462a15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不知有犯星辰。",
        "from": "T21n1311_001:0462a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黯黯而行災難自然來至。",
        "from": "T21n1311_001:0462a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攘之即大吉也。",
        "from": "T21n1311_001:0462a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祭本命元神日。",
        "from": "T21n1311_001:0462a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一年有六日。",
        "from": "T21n1311_001:0462a17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但至心本命日。",
        "from": "T21n1311_001:0462a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用好紙剪隨年錢。",
        "from": "T21n1311_001:0462a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用茶菓三疊淨床一鋪。",
        "from": "T21n1311_001:0462a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "焚香虔心面視北斗。",
        "from": "T21n1311_001:0462a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "再拜啟告曰。",
        "from": "T21n1311_001:0462a19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隔居少人好道求靈常見尊儀。",
        "from": "T21n1311_001:0462a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本命日謹奉銀錢仙菓。",
        "from": "T21n1311_001:0462a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養於北斗辰星并本命神形。",
        "from": "T21n1311_001:0462a21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將長是生益壽無諸橫禍。",
        "from": "T21n1311_001:0462a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神魂為安。",
        "from": "T21n1311_001:0462a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元神自在。",
        "from": "T21n1311_001:0462a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "襄年凡■驅向遠方。",
        "from": "T21n1311_001:0462a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "再拜燒錢合掌供養。",
        "from": "T21n1311_001:0462a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　┌○破軍星持大置午生人",
        "from": "T21n1311_001:0462a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　└○廉貞星不灌子辰申生人",
        "from": "T21n1311_001:0462a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　└○文曲星微慧子卯酉生人",
        "from": "T21n1311_001:0462a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　　├○祿存星祿會寅戌生人",
        "from": "T21n1311_001:0462a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　┌─┴○巨門星貞文子亥丑生人",
        "from": "T21n1311_001:0462b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　○貪狼星司希子子生人",
        "from": "T21n1311_001:0462b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡祿命書云云。",
        "from": "T21n1311_001:0462b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屬破軍星人。",
        "from": "T21n1311_001:0462b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日食一升餘命八十歲　男女午年字大京子。",
        "from": "T21n1311_001:0462b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屬武曲星人。",
        "from": "T21n1311_001:0462b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日食一石餘命八十五歲　巳未年男女字大東子。",
        "from": "T21n1311_001:0462b07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屬廉貞星人。",
        "from": "T21n1311_001:0462b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日食一升餘命八十歲　辰申年男女字術不隣子。",
        "from": "T21n1311_001:0462b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屬文曲星人。",
        "from": "T21n1311_001:0462b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日食四升餘命九十歲　卯酉年男女字微慧字。",
        "from": "T21n1311_001:0462b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屬祿存星人。",
        "from": "T21n1311_001:0462b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日食五升餘命八十歲　寅戌年男女字祿存會子。",
        "from": "T21n1311_001:0462b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屬巨門星人。",
        "from": "T21n1311_001:0462b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日食八升餘命八十歲　丑亥年男女字貞文子。",
        "from": "T21n1311_001:0462b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屬貪狼星人。",
        "from": "T21n1311_001:0462b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日食二升餘命六十五歲　子年男女字司希神子。",
        "from": "T21n1311_001:0462b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來說破一切宿曜障吉祥真言唵薩縛諾剎怛羅二合三磨曳室哩曳扇底迦俱嚕娑婆二合賀男忌天羅女忌地網欲知衰禍但在行年所犯無不知也　夫人行年吉少凶多乍可知而迴避不可坐受其災云若莫日　■■。",
        "from": "T21n1311_001:0462b23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵天火羅圖梵天火羅圖一帖敬白大梵天王帝釋天王。",
        "from": "T21n1311_001:0462b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "閻羅天子五道大神。",
        "from": "T21n1311_001:0462b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "太山府君司命司祿。",
        "from": "T21n1311_001:0462b26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二宮神七曜九執。",
        "from": "T21n1311_001:0462b26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十八宿藥叉藥叉女。",
        "from": "T21n1311_001:0462b27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘舍遮毘舍支步多那天等。",
        "from": "T21n1311_001:0462b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殊別當所鎮守護法善神。",
        "from": "T21n1311_001:0462b28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若家內守宅諸大小神等。",
        "from": "T21n1311_001:0462b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并三千大千世界不可說不可說微塵剎土六道四生類。",
        "from": "T21n1311_001:0462c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "某甲願今月吉日良時。",
        "from": "T21n1311_001:0462c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設神供無遮供具。",
        "from": "T21n1311_001:0462c02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其大志者可知心云云。",
        "from": "T21n1311_001:0462c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次勸請　向東方合掌　已上畢。",
        "from": "T21n1311_001:0462c04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　已下表紙袖書也今此記之。",
        "from": "T21n1311_001:0462c05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一羅睺大惡二土少惡三水中吉四金中吉五日大吉六火少惡七計大吉八月中吉九木大吉。",
        "from": "T21n1311_001:0462c07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名一切少天呪法以右手中指直。",
        "from": "T21n1311_001:0462c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘指作拳。",
        "from": "T21n1311_001:0462c08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手亦然。",
        "from": "T21n1311_001:0462c08_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手合腕去心胸八寸。",
        "from": "T21n1311_001:0462c08_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手中指少屈。",
        "from": "T21n1311_001:0462c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坤左中悤節來去。",
        "from": "T21n1311_001:0462c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "動令忿。",
        "from": "T21n1311_001:0462c09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天通用呪　唵路迦　迦路野曳　娑婆賀文治五年八月書寫。",
        "from": "T21n1311_001:0462c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以四本挍之。",
        "from": "T21n1311_001:0462c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玄證正德三巳年。",
        "from": "T21n1311_001:0462c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以栂尾本寫之。",
        "from": "T21n1311_001:0462c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧旭寬延四年書之宥證　明和四亥年以御室真乘院法務前大僧正宥證大和尚本寫挍。",
        "from": "T21n1311_001:0462c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智積院動潮　寬政五丑年以右本書之。",
        "from": "T21n1311_001:0462c16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洛北福勝密寺秀陽享和二壬戌冬以件本挍正上木行於普天。",
        "from": "T21n1311_001:0462c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以傳領之本對挍加筆畢　　慈順",
        "from": "T21n1311_001:0462c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　　　　　　　　龍肝年四十三戒三十七",
        "from": "T21n1311_001:0462c24_0",
        "context_with_next_sentence": true
    }
]