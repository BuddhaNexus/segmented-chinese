[
    {
        "source_sentence": "寺塔記",
        "from": "T51n2093_001:1022b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐段成式",
        "from": "T51n2093_001:1022b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "武宗癸亥三年夏。予與張君希復善繼。同官",
        "from": "T51n2093_001:1022b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "秘丘鄭君符夢復連職仙署。",
        "from": "T51n2093_001:1022b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "會暇日遊大興善寺。",
        "from": "T51n2093_001:1022b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因問。",
        "from": "T51n2093_001:1022b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩京新記及遊目記多所遺。",
        "from": "T51n2093_001:1022b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略乃約一旬尋兩街寺。",
        "from": "T51n2093_001:1022b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以街東興善為首。",
        "from": "T51n2093_001:1022b28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二記所不具。",
        "from": "T51n2093_001:1022b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則別錄之。",
        "from": "T51n2093_001:1022b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遊及慈恩。",
        "from": "T51n2093_001:1022b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初知官將併寺。",
        "from": "T51n2093_001:1022b29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧眾草草。",
        "from": "T51n2093_001:1022c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃泛問一二。",
        "from": "T51n2093_001:1022c01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上人及記塔下畫跡。",
        "from": "T51n2093_001:1022c01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遊於此遂絕。",
        "from": "T51n2093_001:1022c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後三年。",
        "from": "T51n2093_001:1022c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "予職于京洛及刺安成。",
        "from": "T51n2093_001:1022c02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至大中七年歸京。",
        "from": "T51n2093_001:1022c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在外六甲子所留書籍揃壞居半。",
        "from": "T51n2093_001:1022c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於故簡中覩與二亡父遊寺。",
        "from": "T51n2093_001:1022c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瀝血淚交。",
        "from": "T51n2093_001:1022c04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當時造適樂事邈不可追。",
        "from": "T51n2093_001:1022c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復方刊整。",
        "from": "T51n2093_001:1022c05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纔足續穿蠧。",
        "from": "T51n2093_001:1022c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然十亡五六矣。",
        "from": "T51n2093_001:1022c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次成兩卷。",
        "from": "T51n2093_001:1022c06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳諸釋子。",
        "from": "T51n2093_001:1022c06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東牟人段成式字柯古。",
        "from": "T51n2093_001:1022c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "靖恭坊大興善寺。",
        "from": "T51n2093_001:1022c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寺取大興兩字。",
        "from": "T51n2093_001:1022c08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坊名一字為名。",
        "from": "T51n2093_001:1022c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "新記云。",
        "from": "T51n2093_001:1022c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "優填像。",
        "from": "T51n2093_001:1022c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總章初為火所燒。",
        "from": "T51n2093_001:1022c09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "據梁時西域優填在荊州。",
        "from": "T51n2093_001:1022c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言隋自臺城移來此寺非也。",
        "from": "T51n2093_001:1022c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今又有旃檀像。",
        "from": "T51n2093_001:1022c11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開目其工頗拙。",
        "from": "T51n2093_001:1022c11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶差謬矣。",
        "from": "T51n2093_001:1022c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空三藏塔前多老松。",
        "from": "T51n2093_001:1022c13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歲早則官伐其枝。",
        "from": "T51n2093_001:1022c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為龍骨以祈雨。",
        "from": "T51n2093_001:1022c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓋三藏役龍意。",
        "from": "T51n2093_001:1022c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其樹必有靈也行香院堂後壁上。",
        "from": "T51n2093_001:1022c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元和中。",
        "from": "T51n2093_001:1022c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫人梁洽畫雙松。",
        "from": "T51n2093_001:1022c15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稍脫俗格。",
        "from": "T51n2093_001:1022c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曼殊堂工塑極精妙。",
        "from": "T51n2093_001:1022c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外壁有泥金幀。",
        "from": "T51n2093_001:1022c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空自西域齎來者。",
        "from": "T51n2093_001:1022c17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旃檀像堂中。",
        "from": "T51n2093_001:1022c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有時非時經。",
        "from": "T51n2093_001:1022c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "界朱寫之。",
        "from": "T51n2093_001:1022c18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盛以漆龕。",
        "from": "T51n2093_001:1022c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧云。",
        "from": "T51n2093_001:1022c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隋朝舊物。",
        "from": "T51n2093_001:1022c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寺後先有曲池。",
        "from": "T51n2093_001:1022c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空臨終時。",
        "from": "T51n2093_001:1022c20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忽然涸竭。",
        "from": "T51n2093_001:1022c20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至惟寬禪師止住。",
        "from": "T51n2093_001:1022c21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因潦通泉。",
        "from": "T51n2093_001:1022c21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白蓮藻自生。",
        "from": "T51n2093_001:1022c21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今復成陸矣。",
        "from": "T51n2093_001:1022c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東廊之南素和尚院。",
        "from": "T51n2093_001:1022c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庭有青桐四株。",
        "from": "T51n2093_001:1022c23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "素之手植。",
        "from": "T51n2093_001:1022c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元和中卿相多遊此院。",
        "from": "T51n2093_001:1022c24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "桐至夏有汗。",
        "from": "T51n2093_001:1022c24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "污人衣如輠。",
        "from": "T51n2093_001:1022c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脂不可浣。",
        "from": "T51n2093_001:1022c25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左顧蛤像。",
        "from": "T51n2093_001:1022c26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舊傳云。",
        "from": "T51n2093_001:1022c26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隋帝嗜蛤。",
        "from": "T51n2093_001:1022c26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所食必兼蛤味。",
        "from": "T51n2093_001:1022c26_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數逾數千萬矣。",
        "from": "T51n2093_001:1022c27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忽有一蛤。",
        "from": "T51n2093_001:1022c27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "推擊如舊。",
        "from": "T51n2093_001:1022c27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝異之寘諸几上。",
        "from": "T51n2093_001:1022c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一夜有光。",
        "from": "T51n2093_001:1022c28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及明肉自脫。",
        "from": "T51n2093_001:1022c28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中有一佛二菩薩像。",
        "from": "T51n2093_001:1022c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝悲悔誓不食蛤。",
        "from": "T51n2093_001:1022c29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非陳宣帝　于闐玉像。",
        "from": "T51n2093_001:1023a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高一尺七寸闊寸餘。",
        "from": "T51n2093_001:1023a01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一佛四菩薩。",
        "from": "T51n2093_001:1023a01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一飛仙一叚玉成。",
        "from": "T51n2093_001:1023a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "截肪無玷。",
        "from": "T51n2093_001:1023a02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "膩彩若滴。",
        "from": "T51n2093_001:1023a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天王閣。",
        "from": "T51n2093_001:1023a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長慶中造。",
        "from": "T51n2093_001:1023a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本在春明門內。",
        "from": "T51n2093_001:1023a03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與南內連牆。",
        "from": "T51n2093_001:1023a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其形大為天下之最。",
        "from": "T51n2093_001:1023a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "太和二年勅移就此寺。",
        "from": "T51n2093_001:1023a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "折時腹中。",
        "from": "T51n2093_001:1023a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得布五百端漆數十筩。",
        "from": "T51n2093_001:1023a05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今部落鬼神形像墮壞。",
        "from": "T51n2093_001:1023a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯天王不損。",
        "from": "T51n2093_001:1023a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長樂坊安國寺紅樓。",
        "from": "T51n2093_001:1023a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "睿宗在藩時舞榭。",
        "from": "T51n2093_001:1023a07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東禪院。",
        "from": "T51n2093_001:1023a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦曰木塔院。",
        "from": "T51n2093_001:1023a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "院門北西廊五壁。",
        "from": "T51n2093_001:1023a08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吳道玄弟子釋思道畫釋梵八部。",
        "from": "T51n2093_001:1023a09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不施彩色。",
        "from": "T51n2093_001:1023a09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尚有典刑。",
        "from": "T51n2093_001:1023a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光明寺中鬼子母。",
        "from": "T51n2093_001:1023a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及文惠太子塑像。",
        "from": "T51n2093_001:1023a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉止態度如生。",
        "from": "T51n2093_001:1023a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "工名李岫。",
        "from": "T51n2093_001:1023a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "山庭院。",
        "from": "T51n2093_001:1023a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "古木崇阜。",
        "from": "T51n2093_001:1023a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幽若山谷。",
        "from": "T51n2093_001:1023a13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當時輦土營之上座璘公院。",
        "from": "T51n2093_001:1023a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有穗柏一株。",
        "from": "T51n2093_001:1023a14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衢柯偃覆。",
        "from": "T51n2093_001:1023a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下坐十餘人。",
        "from": "T51n2093_001:1023a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常樂坊趙景公寺隋開皇三年置。",
        "from": "T51n2093_001:1023a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本曰弘善寺。",
        "from": "T51n2093_001:1023a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十八年改焉。",
        "from": "T51n2093_001:1023a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南中三門裏東壁上。",
        "from": "T51n2093_001:1023a17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吳道玄白畫地獄變。",
        "from": "T51n2093_001:1023a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "筆力勁怒。",
        "from": "T51n2093_001:1023a18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變狀陰怪。",
        "from": "T51n2093_001:1023a18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覩之不覺毛戴。",
        "from": "T51n2093_001:1023a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吳畫中得意處。",
        "from": "T51n2093_001:1023a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三階院西廊下。",
        "from": "T51n2093_001:1023a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "范長壽畫西方變及十六對事寶池。",
        "from": "T51n2093_001:1023a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "池尤妙絕。",
        "from": "T51n2093_001:1023a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諦視之。",
        "from": "T51n2093_001:1023a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覺水入深。",
        "from": "T51n2093_001:1023a21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壁院門上。",
        "from": "T51n2093_001:1023a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白畫樹石。",
        "from": "T51n2093_001:1023a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頗似閻立德。",
        "from": "T51n2093_001:1023a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "予携立德行天詞粉本。",
        "from": "T51n2093_001:1023a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "驗之無異。",
        "from": "T51n2093_001:1023a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西中三門裡門南。",
        "from": "T51n2093_001:1023a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吳生畫龍。",
        "from": "T51n2093_001:1023a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及刷天王鬚。",
        "from": "T51n2093_001:1023a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "筆蹟如鐵。",
        "from": "T51n2093_001:1023a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有執爐天女。",
        "from": "T51n2093_001:1023a25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竊眸欲語。",
        "from": "T51n2093_001:1023a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華嚴院中鍮石盧舍立像高六尺。",
        "from": "T51n2093_001:1023a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "古樣精巧。",
        "from": "T51n2093_001:1023a26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塔下有舍利三斗四升。",
        "from": "T51n2093_001:1023a27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "移塔之時。",
        "from": "T51n2093_001:1023a27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧守行建道場出舍利。",
        "from": "T51n2093_001:1023a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俾士庶觀之。",
        "from": "T51n2093_001:1023a28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唄讚未畢。",
        "from": "T51n2093_001:1023a28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿地現舍利。",
        "from": "T51n2093_001:1023a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "士女不敢踐之。",
        "from": "T51n2093_001:1023a29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉出寺外。",
        "from": "T51n2093_001:1023a29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "守公乃造小泥塔及木塔。",
        "from": "T51n2093_001:1023b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "近十萬枚葬之。",
        "from": "T51n2093_001:1023b01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今尚有數萬存焉。",
        "from": "T51n2093_001:1023b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寺有小銀象六百餘軀。",
        "from": "T51n2093_001:1023b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金佛一軀長數尺。",
        "from": "T51n2093_001:1023b03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大銀象高六尺餘。",
        "from": "T51n2093_001:1023b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "古樣精巧。",
        "from": "T51n2093_001:1023b04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又有篏七寶字多心經小屏風。",
        "from": "T51n2093_001:1023b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盛以寶函。",
        "from": "T51n2093_001:1023b05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上有雜色珠及白珠。",
        "from": "T51n2093_001:1023b05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "駢甃亂目。",
        "from": "T51n2093_001:1023b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祿山亂。",
        "from": "T51n2093_001:1023b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "官人藏於此寺。",
        "from": "T51n2093_001:1023b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屏風十五牒三十行。",
        "from": "T51n2093_001:1023b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經後云。",
        "from": "T51n2093_001:1023b07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發心主司馬恒存。",
        "from": "T51n2093_001:1023b07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願成主上柱國索伏寶息。",
        "from": "T51n2093_001:1023b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上柱國真德。",
        "from": "T51n2093_001:1023b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為法界眾生造黃金牒。",
        "from": "T51n2093_001:1023b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善繼疑外國物。",
        "from": "T51n2093_001:1023b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遊目記所說刺柏。",
        "from": "T51n2093_001:1023b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "太和中伐為殿材。",
        "from": "T51n2093_001:1023b10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道政坊寶應寺韓幹。",
        "from": "T51n2093_001:1023b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藍田人。",
        "from": "T51n2093_001:1023b11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少時常為貰酒家送酒。",
        "from": "T51n2093_001:1023b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王右丞兄弟未遇。",
        "from": "T51n2093_001:1023b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每一貰酒漫遊。",
        "from": "T51n2093_001:1023b12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幹常徵債於王家。",
        "from": "T51n2093_001:1023b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戲畫地為人馬。",
        "from": "T51n2093_001:1023b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右丞精思丹青。",
        "from": "T51n2093_001:1023b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奇其意趣乃成。",
        "from": "T51n2093_001:1023b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與錢二萬。",
        "from": "T51n2093_001:1023b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令學畫十餘年。",
        "from": "T51n2093_001:1023b14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今寺中釋梵天女。",
        "from": "T51n2093_001:1023b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉齊公妓小小等寫真也。",
        "from": "T51n2093_001:1023b15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寺有韓幹畫下生幀。",
        "from": "T51n2093_001:1023b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彌勒衣紫袈裟。",
        "from": "T51n2093_001:1023b16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右邊仰面菩薩及二獅子。",
        "from": "T51n2093_001:1023b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶入神。",
        "from": "T51n2093_001:1023b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西北角院內。",
        "from": "T51n2093_001:1023b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有懷素書顏魯公序。",
        "from": "T51n2093_001:1023b18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "張渭侍郎錢起郎中讚。",
        "from": "T51n2093_001:1023b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平康坊菩薩寺　食堂東壁上。",
        "from": "T51n2093_001:1023b20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吳道玄畫智度論色偈變。",
        "from": "T51n2093_001:1023b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈是吳自題。",
        "from": "T51n2093_001:1023b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "筆跡遒勁。",
        "from": "T51n2093_001:1023b21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如磔鬼神毛𩬊。",
        "from": "T51n2093_001:1023b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次堵畫禮骨仙人。",
        "from": "T51n2093_001:1023b22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天衣飛揚。",
        "from": "T51n2093_001:1023b22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿壁風動。",
        "from": "T51n2093_001:1023b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛殿內槽後壁面。",
        "from": "T51n2093_001:1023b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吳道玄畫消災經事。",
        "from": "T51n2093_001:1023b24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樹石古嶮。",
        "from": "T51n2093_001:1023b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元和中上欲令移之。",
        "from": "T51n2093_001:1023b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慮其摧壞。",
        "from": "T51n2093_001:1023b25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃下詔。",
        "from": "T51n2093_001:1023b25_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擇畫手寫進。",
        "from": "T51n2093_001:1023b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛殿內槽東壁。",
        "from": "T51n2093_001:1023b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "維摩變。",
        "from": "T51n2093_001:1023b27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利弗角而轉膝。",
        "from": "T51n2093_001:1023b27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元和末。",
        "from": "T51n2093_001:1023b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俗講僧文淑裝之。",
        "from": "T51n2093_001:1023b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "筆蹟盡矣。",
        "from": "T51n2093_001:1023b28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寺之制度鍾樓在東。",
        "from": "T51n2093_001:1023b29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯此寺緣李右座林甫宅在東。",
        "from": "T51n2093_001:1023c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故建鍾樓於西。",
        "from": "T51n2093_001:1023c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寺內有郭令玳瑁鞭。",
        "from": "T51n2093_001:1023c01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及郭令王夫人七寶帳。",
        "from": "T51n2093_001:1023c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寺主元竟多識釋門。",
        "from": "T51n2093_001:1023c02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故事云。",
        "from": "T51n2093_001:1023c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "李右座每至生日。",
        "from": "T51n2093_001:1023c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常轉請此寺僧。",
        "from": "T51n2093_001:1023c03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就宅設齋。",
        "from": "T51n2093_001:1023c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有僧乙甞嘆佛。",
        "from": "T51n2093_001:1023c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施鞍一具。",
        "from": "T51n2093_001:1023c04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賣之材直七萬。",
        "from": "T51n2093_001:1023c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又僧廣有聲名。",
        "from": "T51n2093_001:1023c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口經數年。",
        "from": "T51n2093_001:1023c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當嘆佛。",
        "from": "T51n2093_001:1023c05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因極視右座功德。",
        "from": "T51n2093_001:1023c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冀獲厚襯。",
        "from": "T51n2093_001:1023c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "齋畢簾下出綵篚香羅帕籍一物。",
        "from": "T51n2093_001:1023c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如朽釘長數寸。",
        "from": "T51n2093_001:1023c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧歸失望慚惋。",
        "from": "T51n2093_001:1023c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數日且意。",
        "from": "T51n2093_001:1023c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大臣不容欺己。",
        "from": "T51n2093_001:1023c08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂携至西市。",
        "from": "T51n2093_001:1023c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "示於商胡。",
        "from": "T51n2093_001:1023c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "商胡見之驚曰。",
        "from": "T51n2093_001:1023c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上人安得此物。",
        "from": "T51n2093_001:1023c09_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必貨此不違價。",
        "from": "T51n2093_001:1023c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧試求百千。",
        "from": "T51n2093_001:1023c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "胡人大笑曰。",
        "from": "T51n2093_001:1023c10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未也。",
        "from": "T51n2093_001:1023c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更極意言之。",
        "from": "T51n2093_001:1023c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加至五百千。",
        "from": "T51n2093_001:1023c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "胡人曰。",
        "from": "T51n2093_001:1023c11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此直一千萬。",
        "from": "T51n2093_001:1023c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂與之。",
        "from": "T51n2093_001:1023c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧訪其名。",
        "from": "T51n2093_001:1023c12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰此寶骨也。",
        "from": "T51n2093_001:1023c12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又寺先有僧。",
        "from": "T51n2093_001:1023c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不言姓名。",
        "from": "T51n2093_001:1023c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常負束󱡐。",
        "from": "T51n2093_001:1023c13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐臥於寺兩廊下。",
        "from": "T51n2093_001:1023c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不肯住院。",
        "from": "T51n2093_001:1023c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經數年。",
        "from": "T51n2093_001:1023c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寺綱維或勸其住房。",
        "from": "T51n2093_001:1023c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰爾厭我耶。",
        "from": "T51n2093_001:1023c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其夕遂以束󱡐焚身。",
        "from": "T51n2093_001:1023c15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至明唯灰燼耳。",
        "from": "T51n2093_001:1023c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無血膋之臭。",
        "from": "T51n2093_001:1023c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾方知異人。",
        "from": "T51n2093_001:1023c16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂塑灰為像。",
        "from": "T51n2093_001:1023c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今在佛殿上。",
        "from": "T51n2093_001:1023c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世號束草師。",
        "from": "T51n2093_001:1023c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光宅坊光宅寺　普賢堂本天后梳洗堂。",
        "from": "T51n2093_001:1023c18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蒲萄垂實則幸此堂。",
        "from": "T51n2093_001:1023c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今堂中尉遲畫頗有奇處。",
        "from": "T51n2093_001:1023c19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四壁畫像及脫皮白骨。",
        "from": "T51n2093_001:1023c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "匠意極嶮。",
        "from": "T51n2093_001:1023c20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又變形三魔女。",
        "from": "T51n2093_001:1023c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身若出壁。",
        "from": "T51n2093_001:1023c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又佛圓光均彩相錯亂目成講。",
        "from": "T51n2093_001:1023c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東壁佛座前錦如斷古標。",
        "from": "T51n2093_001:1023c22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西壁逼之摽摽然。",
        "from": "T51n2093_001:1023c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宣陽坊靜域寺　本太穆皇后宅。",
        "from": "T51n2093_001:1023c24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寺僧云。",
        "from": "T51n2093_001:1023c24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三階院門外。",
        "from": "T51n2093_001:1023c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是神堯皇帝射孔雀處。",
        "from": "T51n2093_001:1023c25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上蟠蛇污烟可懼。",
        "from": "T51n2093_001:1023c26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東廊樹石嶮怪高僧亦怪。",
        "from": "T51n2093_001:1023c26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "招國坊崇濟寺　寺後有天后織成蛟龍披襖子及繡衣六事　東廓從南第二院。",
        "from": "T51n2093_001:1023c28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有宣律師製袈裟堂曼殊堂。",
        "from": "T51n2093_001:1023c29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有松數株甚奇。",
        "from": "T51n2093_001:1023c29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "崇聖坊資聖寺　淨土院門外。",
        "from": "T51n2093_001:1024a01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相傳吳生一夕秉燭醉畫。",
        "from": "T51n2093_001:1024a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就中戟手視之惡駭。",
        "from": "T51n2093_001:1024a02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "院門裏盧楞伽常學吳勢。",
        "from": "T51n2093_001:1024a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吳亦授以手訣。",
        "from": "T51n2093_001:1024a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃畫總持三門寺方半。",
        "from": "T51n2093_001:1024a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吳大賞之。",
        "from": "T51n2093_001:1024a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂人曰。",
        "from": "T51n2093_001:1024a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "楞伽不得心訣。",
        "from": "T51n2093_001:1024a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用思太苦其能久乎。",
        "from": "T51n2093_001:1024a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫畢而卒。",
        "from": "T51n2093_001:1024a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈恩寺　寺不淨覺故伽藍因而營建焉。",
        "from": "T51n2093_001:1024a06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡十餘院。",
        "from": "T51n2093_001:1024a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一千八百九十七間。",
        "from": "T51n2093_001:1024a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勅度三百僧。",
        "from": "T51n2093_001:1024a07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初三藏自西域迴。",
        "from": "T51n2093_001:1024a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詔太常卿江夏王道宗設九部樂。",
        "from": "T51n2093_001:1024a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迎經像入寺。",
        "from": "T51n2093_001:1024a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "綵車凡千餘輛。",
        "from": "T51n2093_001:1024a09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上御安福門觀之。",
        "from": "T51n2093_001:1024a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "太宗常賜三藏衲約直百餘金。",
        "from": "T51n2093_001:1024a10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其工無鍼綖之迹。",
        "from": "T51n2093_001:1024a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寺中柹樹白牡丹。",
        "from": "T51n2093_001:1024a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是法力上人手植。",
        "from": "T51n2093_001:1024a12_15",
        "context_with_next_sentence": true
    }
]