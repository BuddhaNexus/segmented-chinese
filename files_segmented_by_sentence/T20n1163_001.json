[
    {
        "source_sentence": "佛說雨寶陀羅尼經",
        "from": "T20n1163_001:0667c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐三藏沙門大廣智不空奉　詔譯",
        "from": "T20n1163_001:0667c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。一時薄伽梵住憍睒彌國建吒迦",
        "from": "T20n1163_001:0667c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "林。",
        "from": "T20n1163_001:0667c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大苾芻眾五百人俱。",
        "from": "T20n1163_001:0667c19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又與多諸大菩薩摩訶薩俱。",
        "from": "T20n1163_001:0667c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時憍睒彌國中有一長者。",
        "from": "T20n1163_001:0667c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名曰妙月。",
        "from": "T20n1163_001:0667c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸根寂靜心意寂靜。",
        "from": "T20n1163_001:0667c21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多有男女及多僮僕。",
        "from": "T20n1163_001:0667c21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨信成就。",
        "from": "T20n1163_001:0667c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往詣佛所頭面禮足。",
        "from": "T20n1163_001:0667c22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繞百千匝却住一面。",
        "from": "T20n1163_001:0667c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌恭敬而白佛言。",
        "from": "T20n1163_001:0667c23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊欲問如來應正等覺。",
        "from": "T20n1163_001:0667c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少有所疑事。",
        "from": "T20n1163_001:0667c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願大慈垂愍聽許。",
        "from": "T20n1163_001:0667c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告長者言。",
        "from": "T20n1163_001:0667c25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恣汝意問。",
        "from": "T20n1163_001:0667c25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當為汝說。",
        "from": "T20n1163_001:0667c25_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令汝心喜。",
        "from": "T20n1163_001:0667c26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時彼長者聞是語已歡喜踊躍。",
        "from": "T20n1163_001:0667c26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊云何善男子善女人諸貧匱者可得富饒。",
        "from": "T20n1163_001:0667c27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸有疾病令無疾病。",
        "from": "T20n1163_001:0667c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告妙月長者言。",
        "from": "T20n1163_001:0667c28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何緣作如是問。",
        "from": "T20n1163_001:0667c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時彼長者重白佛言。",
        "from": "T20n1163_001:0667c29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我等在家多諸眷屬。",
        "from": "T20n1163_001:0668a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "資財乏少難可支濟。",
        "from": "T20n1163_001:0668a01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又多疾病。",
        "from": "T20n1163_001:0668a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願世尊開示法要。",
        "from": "T20n1163_001:0668a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當令貧者永離貧窮。",
        "from": "T20n1163_001:0668a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倉庫財寶皆悉盈滿。",
        "from": "T20n1163_001:0668a03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "存濟家中妻子男女眷屬有來求者必生歡喜。",
        "from": "T20n1163_001:0668a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為大施主。",
        "from": "T20n1163_001:0668a04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使諸倉庫金銀珍寶如意摩尼金剛諸珍商佉室羅赤珠碼碯金寶之類豐饒無有盡竭。",
        "from": "T20n1163_001:0668a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周給親屬廣修惠施饒益有情。",
        "from": "T20n1163_001:0668a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告妙月長者言。",
        "from": "T20n1163_001:0668a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子我於過去阿僧祇劫前遇佛世尊。",
        "from": "T20n1163_001:0668a08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名持金剛海音如來應正遍知。",
        "from": "T20n1163_001:0668a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從彼如來受得此雨寶陀羅尼。",
        "from": "T20n1163_001:0668a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持讀誦思惟計念隨喜為他廣說流布。",
        "from": "T20n1163_001:0668a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此陀羅尼威德力故。",
        "from": "T20n1163_001:0668a11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子人與非人藥叉羅剎畢隷多畢舍遮鳩槃拏烏娑多羅迦布單那羯吒布單那等起惡心者。",
        "from": "T20n1163_001:0668a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能為害。",
        "from": "T20n1163_001:0668a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有諸鬼噉人脂髓膿血涕唾大小便利欲來惱者。",
        "from": "T20n1163_001:0668a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能為障礙。",
        "from": "T20n1163_001:0668a15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告妙月。",
        "from": "T20n1163_001:0668a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子心念手持書寫。",
        "from": "T20n1163_001:0668a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但聞名字受持隨喜廣為他敷演者。",
        "from": "T20n1163_001:0668a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼善男子善女人長夜安隱受諸快樂。",
        "from": "T20n1163_001:0668a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為瑜伽資糧安隱豐饒故。",
        "from": "T20n1163_001:0668a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人欲受持此雨寶陀羅尼者。",
        "from": "T20n1163_001:0668a19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應供養一切如來。",
        "from": "T20n1163_001:0668a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一夜二夜或三夜。",
        "from": "T20n1163_001:0668a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "專心誦持。",
        "from": "T20n1163_001:0668a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受敬淨信三寶諸天悉皆歡喜。",
        "from": "T20n1163_001:0668a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即雨財寶穀麥。",
        "from": "T20n1163_001:0668a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為彼讀誦法師故。",
        "from": "T20n1163_001:0668a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說陀羅尼曰。",
        "from": "T20n1163_001:0668a22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨引婆去聲誐嚩帝一嚩日囉二合馱囉二娑引誐囉捏奴逸切具灑引耶三怛他引𮒶多引野四引怛儞也二合他引唵素嚕閉五跋捺囉二合嚩底丁以切六矒誐阿左㘑七阿左跛㘑八嗢鳥骨切伽引跢儞九嗢陛娜儞十薩寫嚩底十一馱引孃上聲嚩底丁以切十二馱曩嚩底十三室唎二合麼底十四鉢囉二合婆嚩底十五阿上聲麼㘑十六尾麼黎十七嚕嚕十八素嚕閉十九尾麼黎二十阿娜多悉帝二合二十一尾娜多悉帝二合二十二尾濕嚩二合計如二十三鴦矩㘑二十四矒矩㘑二十五地地冥二十六度度冥二十七跢跢㘑二十八多囉多囉二十九嚩日㘑二合三十阿引󰽣跢儞三十一步計屋計三十二吒計吒計三十三󰽣囉灑二合尼三十四儞澁播二合引娜儞三十五婆誐挽三十六嚩日囉二合馱囉三十七娑引誐囉三十八捏具引衫三十九怛他引蘖跢麼弩娑磨二合囉四十娑麼二合囉娑麼二合囉四十一薩嚩怛他引孽跢四十二薩底也二合四十三麼弩娑麼二合囉四十四僧伽薩底也二合麼弩娑麼二合囉四十五怛吒怛吒四十六布引囉布引囉四十七布引囉也布引囉野四十八婆囉婆去聲囉婆去聲囉抳四十九素矒誐麗五十扇引跢麼底五十一瞢誐攞麼底五十二鉢囉二合婆引麼底摩訶引麼底五十三素婆捺囉二合嚩底五十四阿引蘖蹉引阿引蘖蹉五十五引三麼野麼弩娑麼二合囉娑嚩二合引賀引五十六阿馱引囉拏麼弩娑麼二合囉娑嚩二合引賀引五十七鉢囉二合婆去聲引嚩麼弩娑麼二合囉娑嚩二合賀引五十八馱哩二合底麼弩娑麼二合囉娑嚩二合引賀五十九引尾惹野麼弩娑麼二合囉娑嚩二合賀引六十薩嚩薩怛嚩二合尾惹野麼弩娑麼二合囉娑嚩二合引賀引六十一佛告妙月長者。",
        "from": "T20n1163_001:0668b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此名雨寶陀羅尼。",
        "from": "T20n1163_001:0668b25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此陀羅尼威力。",
        "from": "T20n1163_001:0668c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "病患飢儉疾疫業障悉皆消滅。",
        "from": "T20n1163_001:0668c01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人先應供養一切如來。",
        "from": "T20n1163_001:0668c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一日一夜無間斷誦持此陀羅尼。",
        "from": "T20n1163_001:0668c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其家即雨寶如大人量。",
        "from": "T20n1163_001:0668c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切災禍悉皆銷滅。",
        "from": "T20n1163_001:0668c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故善男子當受持此雨寶陀羅尼。",
        "from": "T20n1163_001:0668c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣為他人分別演說。",
        "from": "T20n1163_001:0668c05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉世尊。",
        "from": "T20n1163_001:0668c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙月長者聞佛所說歡喜踊躍。",
        "from": "T20n1163_001:0668c06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今從佛受此雨寶陀羅尼。",
        "from": "T20n1163_001:0668c07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持讀誦廣為他人分別解說。",
        "from": "T20n1163_001:0668c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時妙月長者受佛教已。",
        "from": "T20n1163_001:0668c08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右繞世尊百千匝已。",
        "from": "T20n1163_001:0668c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌恭敬頭面禮足歡喜而去。",
        "from": "T20n1163_001:0668c09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛告具壽阿難陀。",
        "from": "T20n1163_001:0668c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝往妙月長者家。",
        "from": "T20n1163_001:0668c10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "看彼長者諸庫藏中種種財穀諸珍寶物及諸資具今悉盈滿。",
        "from": "T20n1163_001:0668c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時具壽阿難陀受佛教已。",
        "from": "T20n1163_001:0668c12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往詣憍睒彌大城往妙月長者家中。",
        "from": "T20n1163_001:0668c13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入已見諸庫藏之中財寶悉皆盈滿。",
        "from": "T20n1163_001:0668c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見此事已心大歡喜踊躍而還。",
        "from": "T20n1163_001:0668c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時具壽阿難陀怪未曾有心甚歡喜而白佛言。",
        "from": "T20n1163_001:0668c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊以何因緣妙月長者家中庫藏盈滿。",
        "from": "T20n1163_001:0668c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言。",
        "from": "T20n1163_001:0668c17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子妙月長者淨信於我。",
        "from": "T20n1163_001:0668c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持此雨寶陀羅尼。",
        "from": "T20n1163_001:0668c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為一切有情宣說。",
        "from": "T20n1163_001:0668c18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故阿難受持此陀羅尼廣為人說。",
        "from": "T20n1163_001:0668c19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我以佛眼觀諸世間天人魔梵沙門婆羅門。",
        "from": "T20n1163_001:0668c20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此受持雨寶陀羅尼者不能作其障難。",
        "from": "T20n1163_001:0668c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故。",
        "from": "T20n1163_001:0668c21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來不異語故。",
        "from": "T20n1163_001:0668c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此真言句不可壞故。",
        "from": "T20n1163_001:0668c22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此陀羅尼無善根有情耳尚不聞。",
        "from": "T20n1163_001:0668c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況書寫受持讀誦。",
        "from": "T20n1163_001:0668c23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故。",
        "from": "T20n1163_001:0668c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來真語宣說。",
        "from": "T20n1163_001:0668c24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來隨喜。",
        "from": "T20n1163_001:0668c24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來稱讚。",
        "from": "T20n1163_001:0668c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來顯揚。",
        "from": "T20n1163_001:0668c25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來種植。",
        "from": "T20n1163_001:0668c25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難陀白佛言。",
        "from": "T20n1163_001:0668c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉世尊。",
        "from": "T20n1163_001:0668c26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以妙伽他而說頌曰。",
        "from": "T20n1163_001:0668c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛不思議　　佛法亦復然",
        "from": "T20n1163_001:0668c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨信不思議　　果報亦復然",
        "from": "T20n1163_001:0668c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寂慧一切智　　法王不生滅",
        "from": "T20n1163_001:0669a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已到勝彼岸　　稽首佛勇猛",
        "from": "T20n1163_001:0669a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時具壽阿難陀。",
        "from": "T20n1163_001:0669a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛說此雨寶陀羅尼經。",
        "from": "T20n1163_001:0669a03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "踊躍歡喜白佛言。",
        "from": "T20n1163_001:0669a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊今此法要當何名。",
        "from": "T20n1163_001:0669a04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經我等今者云何奉持。",
        "from": "T20n1163_001:0669a05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告阿難陀。",
        "from": "T20n1163_001:0669a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經名妙月長者所問。",
        "from": "T20n1163_001:0669a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當受持。",
        "from": "T20n1163_001:0669a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名能獲一切財寶伏藏。",
        "from": "T20n1163_001:0669a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名一切如來稱讚雨寶陀羅尼教。",
        "from": "T20n1163_001:0669a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當受持。",
        "from": "T20n1163_001:0669a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時薄伽梵說此經已。",
        "from": "T20n1163_001:0669a08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量苾芻及諸菩薩并諸天人阿蘇羅等一切大眾。",
        "from": "T20n1163_001:0669a09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛所說皆大歡喜信受奉行。",
        "from": "T20n1163_001:0669a10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心真言曰。",
        "from": "T20n1163_001:0669a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩素馱㘑娑嚩二合引賀引心中心真言曰。",
        "from": "T20n1163_001:0669a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵室唎二合嚩素娑嚩二合引賀引小心真言曰。",
        "from": "T20n1163_001:0669a15_5",
        "context_with_next_sentence": true
    },
