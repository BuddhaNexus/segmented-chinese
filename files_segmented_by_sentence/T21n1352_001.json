[
    {
        "source_sentence": "佛說陀隣尼鉢經",
        "from": "T21n1352_001:0865b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東晉西域沙門竺曇無蘭譯",
        "from": "T21n1352_001:0865b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞如是。一時佛在舍衛國祇樹給孤獨園。與",
        "from": "T21n1352_001:0865b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大比丘眾千二百五十人菩薩萬人俱。",
        "from": "T21n1352_001:0865b10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時去是佛剎百千億拘利佛剎。",
        "from": "T21n1352_001:0865b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過爾所佛土。",
        "from": "T21n1352_001:0865b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其剎世界名阿難陀拘蠶晉言華積彼佛號伊迦波提羅耶晉言最上天王如來至真等正覺。",
        "from": "T21n1352_001:0865b13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今現在遣兩菩薩。",
        "from": "T21n1352_001:0865b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名阿彌陀法晉言無量光明二名摩訶法晉言大光明爾時二菩薩來到佛所。",
        "from": "T21n1352_001:0865b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前以頭面禮佛足。",
        "from": "T21n1352_001:0865b15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長跪叉手白佛言世尊。",
        "from": "T21n1352_001:0865b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從是間過百千億拘利佛剎。",
        "from": "T21n1352_001:0865b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世界名曰華積。",
        "from": "T21n1352_001:0865b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼佛號最上天王如來至真等正覺。",
        "from": "T21n1352_001:0865b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今現在遣我來問訊世尊。",
        "from": "T21n1352_001:0865b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說法安隱受者增進皆無他不。",
        "from": "T21n1352_001:0865b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得不為天龍夜叉鬼神。",
        "from": "T21n1352_001:0865b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若薜茘若鳩洹鬼神。",
        "from": "T21n1352_001:0865b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若羅剎鬼神若虎若狼。",
        "from": "T21n1352_001:0865b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人非人所嬈害。",
        "from": "T21n1352_001:0865b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼世如來。",
        "from": "T21n1352_001:0865b21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至真等正覺。",
        "from": "T21n1352_001:0865b21_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今遣我持陀隣尼鉢來。",
        "from": "T21n1352_001:0865b22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今為一切故欲令安隱。",
        "from": "T21n1352_001:0865b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得名聲遠聞色貌端政。",
        "from": "T21n1352_001:0865b23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有氣力有筋力強如是。",
        "from": "T21n1352_001:0865b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闍離　摩訶闍離　闍蘭尼　郁奇目𬽾　三波提　摩訶三波提是時佛告阿難陀言。",
        "from": "T21n1352_001:0865b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝受是陀隣尼鉢持諷誦讀。",
        "from": "T21n1352_001:0865b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有佛世尊甚難得值。",
        "from": "T21n1352_001:0865b28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陀隣尼鉢亦難得聞。",
        "from": "T21n1352_001:0865b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人。",
        "from": "T21n1352_001:0865b29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持誦讀識七世生宿命。",
        "from": "T21n1352_001:0865c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人。",
        "from": "T21n1352_001:0865c01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持諷誦讀。",
        "from": "T21n1352_001:0865c01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切鬼神人非人蛇蚖蝮蠍。",
        "from": "T21n1352_001:0865c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆不能害。",
        "from": "T21n1352_001:0865c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒不能中。",
        "from": "T21n1352_001:0865c02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蠱道為不行。",
        "from": "T21n1352_001:0865c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為刀兵所傷害。",
        "from": "T21n1352_001:0865c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝王不能得其便。",
        "from": "T21n1352_001:0865c03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵不恚之。",
        "from": "T21n1352_001:0865c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是阿難陀。",
        "from": "T21n1352_001:0865c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是陀隣尼鉢七十七億諸佛所說。",
        "from": "T21n1352_001:0865c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有中害者。",
        "from": "T21n1352_001:0865c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸佛語為無有異。",
        "from": "T21n1352_001:0865c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿逸多菩薩字彌勒。",
        "from": "T21n1352_001:0865c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "語賢者阿難陀言。",
        "from": "T21n1352_001:0865c06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦當復說陀隣尼鉢。",
        "from": "T21n1352_001:0865c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何。",
        "from": "T21n1352_001:0865c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦欲令一切安隱。",
        "from": "T21n1352_001:0865c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有名聲德遠聞。",
        "from": "T21n1352_001:0865c08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色貌端政饒益力。",
        "from": "T21n1352_001:0865c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其筋力強如是。",
        "from": "T21n1352_001:0865c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頞䩚　跋䩚　滅支叉離　勒支　羅嵐彌　漏嵐彌　醯離　彌離　提離　爾時佛告阿難陀言。",
        "from": "T21n1352_001:0865c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝受是陀隣尼鉢持諷誦讀。",
        "from": "T21n1352_001:0865c12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有佛世尊甚難得值。",
        "from": "T21n1352_001:0865c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是陀隣尼鉢亦難得聞。",
        "from": "T21n1352_001:0865c13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人。",
        "from": "T21n1352_001:0865c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受是陀隣尼鉢奉持諷誦讀。",
        "from": "T21n1352_001:0865c14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識十四生宿命。",
        "from": "T21n1352_001:0865c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人。",
        "from": "T21n1352_001:0865c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉持陀隣尼鉢諷誦讀說。",
        "from": "T21n1352_001:0865c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終不為一切鬼神人非人所觸嬈。",
        "from": "T21n1352_001:0865c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蛇蚖蝮螫諸含毒之蟲所不能害。",
        "from": "T21n1352_001:0865c17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒不能中。",
        "from": "T21n1352_001:0865c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蠱道為不行。",
        "from": "T21n1352_001:0865c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀兵不能傷。",
        "from": "T21n1352_001:0865c18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝王不能得其便。",
        "from": "T21n1352_001:0865c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵釋四天王所共擁護。",
        "from": "T21n1352_001:0865c19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難陀是陀隣尼鉢。",
        "from": "T21n1352_001:0865c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八萬四千億佛所說。",
        "from": "T21n1352_001:0865c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告阿難陀言。",
        "from": "T21n1352_001:0865c20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦欲復說陀隣尼鉢。",
        "from": "T21n1352_001:0865c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲令一切安隱。",
        "from": "T21n1352_001:0865c21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有名聲德遠聞。",
        "from": "T21n1352_001:0865c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色貌端正饒氣力。",
        "from": "T21n1352_001:0865c22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其筋力強如是。",
        "from": "T21n1352_001:0865c22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頞䩚　跋䩚　涅䩚　鳩涅䩚　鐵離　抄羅波提　安那波提　般那波提　迦前尼摩訶前尼是時佛告賢者阿難陀言。",
        "from": "T21n1352_001:0865c26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受是陀隣尼鉢。",
        "from": "T21n1352_001:0865c26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持諷誦讀為一切廣說。",
        "from": "T21n1352_001:0865c27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人。",
        "from": "T21n1352_001:0865c27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受是陀隣尼鉢持諷誦讀。",
        "from": "T21n1352_001:0865c28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識無央數生宿命。",
        "from": "T21n1352_001:0865c28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是陀隣尼鉢。",
        "from": "T21n1352_001:0865c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難陀不可稱計億佛所說。",
        "from": "T21n1352_001:0865c29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是阿難陀是陀隣尼鉢。",
        "from": "T21n1352_001:0866a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若行道若為賊若為虎狼。",
        "from": "T21n1352_001:0866a01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若水中若犯帝王縣官事。",
        "from": "T21n1352_001:0866a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當念是陀隣尼鉢諷誦讀持。",
        "from": "T21n1352_001:0866a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是陀隣尼鉢。",
        "from": "T21n1352_001:0866a03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難陀繫著枯樹即便生葉華實。",
        "from": "T21n1352_001:0866a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況為人說病不愈。",
        "from": "T21n1352_001:0866a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當為一切病人呪。",
        "from": "T21n1352_001:0866a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說經已。",
        "from": "T21n1352_001:0866a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢者阿難陀及諸會者。",
        "from": "T21n1352_001:0866a05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆歡喜受行。",
        "from": "T21n1352_001:0866a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說陀隣尼鉢經",
        "from": "T21n1352_001:0866a07_100",
        "context_with_next_sentence": false
    }
]