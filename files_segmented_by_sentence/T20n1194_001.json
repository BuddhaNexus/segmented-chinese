[
    {
        "source_sentence": "妙吉祥平等觀門大教王經略出",
        "from": "T20n1194_001:0934a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護摩儀",
        "from": "T20n1194_001:0934a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宋大契丹國師中天竺摩竭陀國",
        "from": "T20n1194_001:0934a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命一切佛　　諸大菩薩眾",
        "from": "T20n1194_001:0934a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今說護摩　　種種之儀式",
        "from": "T20n1194_001:0934a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先誦於真言　　加持其鍬钁",
        "from": "T20n1194_001:0934a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後應廣分別　　加持真言曰",
        "from": "T20n1194_001:0934a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引𩕳佉曩嚩蘇弟二娑嚩二合賀誦此真言加持鍬钁。",
        "from": "T20n1194_001:0934a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掘護摩爐。",
        "from": "T20n1194_001:0934a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於掘爐前先取河水盛淨器中。",
        "from": "T20n1194_001:0934a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦本部佛真言加持。",
        "from": "T20n1194_001:0934a25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以水灑地及淨鍬钁灑掘爐人。",
        "from": "T20n1194_001:0934a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灑淨真言曰。",
        "from": "T20n1194_001:0934a26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一阿密哩二合姤二納婆二合嚩野三吽四發吒五娑嚩二合賀六引若依護摩法。",
        "from": "T20n1194_001:0934a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於未掘爐前一宿。",
        "from": "T20n1194_001:0934a29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以上方明王真言加持生飯。",
        "from": "T20n1194_001:0934b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養土地及天龍八部。",
        "from": "T20n1194_001:0934b01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然可掘爐。",
        "from": "T20n1194_001:0934b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其爐內土收於淨處。",
        "from": "T20n1194_001:0934b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然於河內取砂。",
        "from": "T20n1194_001:0934b02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以真言加持其砂。",
        "from": "T20n1194_001:0934b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1194_001:0934b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一嚩蘇麼底二嚩蘇室哩二合曳三唵四引誦此真言加持已。",
        "from": "T20n1194_001:0934b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其砂填於爐內。",
        "from": "T20n1194_001:0934b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次用五㯏七寶四鐵末擦於其上。",
        "from": "T20n1194_001:0934b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方可用淨墼。",
        "from": "T20n1194_001:0934b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "砌護摩爐勿用甎作。",
        "from": "T20n1194_001:0934b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先誦金剛羯磨菩薩真言。",
        "from": "T20n1194_001:0934b07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持淨墼真言曰。",
        "from": "T20n1194_001:0934b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一嚩日囉二合羯囉磨二合二劍三如是加持已。",
        "from": "T20n1194_001:0934b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用淨墼如法砌之。",
        "from": "T20n1194_001:0934b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護摩法者說其四種。",
        "from": "T20n1194_001:0934b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其四種者。",
        "from": "T20n1194_001:0934b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一訕底此言息災二補瑟置此云增益或云富貴三嚩舍此云敬愛四阿尾左囉此云降伏此四護摩爐亦四種。",
        "from": "T20n1194_001:0934b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其四種者。",
        "from": "T20n1194_001:0934b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若求訕底當作圓爐。",
        "from": "T20n1194_001:0934b13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若求補瑟置二合爐如半月或作八角。",
        "from": "T20n1194_001:0934b14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若求嚩舍爐作四角。",
        "from": "T20n1194_001:0934b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若求阿尾左囉爐作三角。",
        "from": "T20n1194_001:0934b15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此之四種隨所應作。",
        "from": "T20n1194_001:0934b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說護摩爐量。",
        "from": "T20n1194_001:0934b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略有三種。",
        "from": "T20n1194_001:0934b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一供養爐八肘。",
        "from": "T20n1194_001:0934b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是八角爐也。",
        "from": "T20n1194_001:0934b17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "量深二肘。",
        "from": "T20n1194_001:0934b17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二供養爐一肘半。",
        "from": "T20n1194_001:0934b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "量深一肘半。",
        "from": "T20n1194_001:0934b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四面共六肘。",
        "from": "T20n1194_001:0934b18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三供養爐一肘。",
        "from": "T20n1194_001:0934b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四面共四肘深亦一肘。",
        "from": "T20n1194_001:0934b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨爐大小作三重緣。",
        "from": "T20n1194_001:0934b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於爐前面第三緣上西邊。",
        "from": "T20n1194_001:0934b20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "量比正半。",
        "from": "T20n1194_001:0934b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦用淨墼砌作九重至上漸小。",
        "from": "T20n1194_001:0934b21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於正半右邊壘作層級。",
        "from": "T20n1194_001:0934b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正半左邊上下直砌。",
        "from": "T20n1194_001:0934b22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爐緣三重表為三界。",
        "from": "T20n1194_001:0934b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其九層級表為九地也。",
        "from": "T20n1194_001:0934b23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是作已當於爐內砂上墼下。",
        "from": "T20n1194_001:0934b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "採於七寶四鐵末五穀。",
        "from": "T20n1194_001:0934b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想如五種子及七寶四鐵所成。",
        "from": "T20n1194_001:0934b25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七寶者金銀真珠水精玉瑠璃琥珀四鐵者金銀銅鐵五穀者稻穤大麥菉豆油麻其五穀四鐵末。",
        "from": "T20n1194_001:0934b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦毘盧佛真言加持。",
        "from": "T20n1194_001:0934b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1194_001:0934b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一嚩日囉二合馱二覩鑁誦此真言已次念四波羅蜜菩薩真言。",
        "from": "T20n1194_001:0934b29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持七寶末。",
        "from": "T20n1194_001:0934c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1194_001:0934c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛波羅蜜菩薩。",
        "from": "T20n1194_001:0934c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一薩怛嚩二合嚩日哩二合二吽三寶波羅蜜菩薩。",
        "from": "T20n1194_001:0934c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一囉怛曩二合嚩日哩二合二怛𠸪二合三法波羅蜜菩薩。",
        "from": "T20n1194_001:0934c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一達囉磨二合嚩日哩二合二紇哩以三合三羯磨波羅蜜菩薩。",
        "from": "T20n1194_001:0934c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一羯囉磨二合嚩日哩二合二噁三誦四明加持已。",
        "from": "T20n1194_001:0934c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用採爐內。",
        "from": "T20n1194_001:0934c10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應用一切香末而和於泥。",
        "from": "T20n1194_001:0934c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持泥真言曰。",
        "from": "T20n1194_001:0934c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一嚩蘇麼底二室哩二合曳三室哩二合嚩蘇弟四婆嚩二合賀引五誦此真言已。",
        "from": "T20n1194_001:0934c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用香泥而泥於爐。",
        "from": "T20n1194_001:0934c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如上次第布置訖。",
        "from": "T20n1194_001:0934c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應於爐中諦想壇場運心安布。",
        "from": "T20n1194_001:0934c15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作訕底法當想佛身形如白色。",
        "from": "T20n1194_001:0934c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復觀己身形相亦爾。",
        "from": "T20n1194_001:0934c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應於左手力度第二文上捻數珠。",
        "from": "T20n1194_001:0934c17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒物供養。",
        "from": "T20n1194_001:0934c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用水精數珠一百八個。",
        "from": "T20n1194_001:0934c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面向東坐作補瑟置二合法。",
        "from": "T20n1194_001:0934c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想佛金色自身亦爾。",
        "from": "T20n1194_001:0934c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於願度中節內捻數。",
        "from": "T20n1194_001:0934c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "珠用菩提子一百個。",
        "from": "T20n1194_001:0934c20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面向南坐。",
        "from": "T20n1194_001:0934c20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作嚩舍法。",
        "from": "T20n1194_001:0934c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想佛赤黃色自身亦爾。",
        "from": "T20n1194_001:0934c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於方度中節內捻數珠。",
        "from": "T20n1194_001:0934c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用蓮子九十個。",
        "from": "T20n1194_001:0934c22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面向西坐。",
        "from": "T20n1194_001:0934c22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作阿尾左囉法。",
        "from": "T20n1194_001:0934c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想佛綠色自身亦爾。",
        "from": "T20n1194_001:0934c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於力度第一文上捻數珠。",
        "from": "T20n1194_001:0934c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用槵子七十個面向北坐。",
        "from": "T20n1194_001:0934c24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作護摩法隨時四種所求之事。",
        "from": "T20n1194_001:0934c25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當於爐內各別想壇。",
        "from": "T20n1194_001:0934c25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨所作部主想安中方。",
        "from": "T20n1194_001:0934c26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於四種護摩法外。",
        "from": "T20n1194_001:0934c26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如恒常供養。",
        "from": "T20n1194_001:0934c27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中方想阿閦佛。",
        "from": "T20n1194_001:0934c27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數珠或一百八或所用不定。",
        "from": "T20n1194_001:0934c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其上想壇法廣如大教說。",
        "from": "T20n1194_001:0934c28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是運想已。",
        "from": "T20n1194_001:0934c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應取吉祥草用青妙細軟草代之當用於八結安於爐八方。",
        "from": "T20n1194_001:0935a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "表想淨八位。",
        "from": "T20n1194_001:0935a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又用草三結安布於爐中表淨三寶位。",
        "from": "T20n1194_001:0935a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以吉祥草三結為一繫。",
        "from": "T20n1194_001:0935a02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三繫共九結。",
        "from": "T20n1194_001:0935a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護摩最初燒而表淨爐法。",
        "from": "T20n1194_001:0935a03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更用草二結。",
        "from": "T20n1194_001:0935a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一結安灌頂瓶內。",
        "from": "T20n1194_001:0935a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一結安右手戒度上。",
        "from": "T20n1194_001:0935a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想如護摩杵。",
        "from": "T20n1194_001:0935a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦表淨手法表為金剛手。",
        "from": "T20n1194_001:0935a05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其草結如金剛杵形。",
        "from": "T20n1194_001:0935a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以四智真言加持真言曰。",
        "from": "T20n1194_001:0935a06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一悉馱路左𩕳二薩嚩囉他二合娑馱𩕳三娑嚩二合賀引四唵引一計哩計哩麼麼計二薩嚩囉他二合娑馱𩕳三娑嚩二合賀引四唵引一濕吠二合帝二半拏囉嚩悉𩕳三惹致𩕳四薩嚩囉他二合娑馱𩕳五娑嚩二合賀引六唵引一哆哩二咄哆哩三咄哩吽四娑嚩二合賀引五如是加持已。",
        "from": "T20n1194_001:0935a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當取所燒柴。",
        "from": "T20n1194_001:0935a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如法乾好者。",
        "from": "T20n1194_001:0935a15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可等於施主。",
        "from": "T20n1194_001:0935a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一肘之手量。",
        "from": "T20n1194_001:0935a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以香水灑之。",
        "from": "T20n1194_001:0935a16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦毘盧真言。",
        "from": "T20n1194_001:0935a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持真言曰。",
        "from": "T20n1194_001:0935a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一嚩日囉二合馱覩二鑁三作法加持已。",
        "from": "T20n1194_001:0935a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安柴於爐內。",
        "from": "T20n1194_001:0935a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "根梢應順用。",
        "from": "T20n1194_001:0935a19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若是刺木柴。",
        "from": "T20n1194_001:0935a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法中不得用。",
        "from": "T20n1194_001:0935a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如上布置已。",
        "from": "T20n1194_001:0935a20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當呪護摩柴。",
        "from": "T20n1194_001:0935a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦誦毘盧明。",
        "from": "T20n1194_001:0935a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而用加持之。",
        "from": "T20n1194_001:0935a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵云半左薩曳囉瑟恥二合此云五乳柴表是五如來正種供養飯。",
        "from": "T20n1194_001:0935a22_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所用五乳柴者。",
        "from": "T20n1194_001:0935a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂尼俱律陀樹等。",
        "from": "T20n1194_001:0935a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此方緣無但取不凋者及無果子柴。",
        "from": "T20n1194_001:0935a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "枝條令纖細。",
        "from": "T20n1194_001:0935a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梢根搵於酥。",
        "from": "T20n1194_001:0935a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三杖或五枝。",
        "from": "T20n1194_001:0935a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或二十五枝。",
        "from": "T20n1194_001:0935a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而投於火中。",
        "from": "T20n1194_001:0935a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想供養於佛。",
        "from": "T20n1194_001:0935a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用五種穀。",
        "from": "T20n1194_001:0935a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作法燒供養。",
        "from": "T20n1194_001:0935a26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦金剛波羅蜜菩薩真言。",
        "from": "T20n1194_001:0935a27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先加持五穀。",
        "from": "T20n1194_001:0935a27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1194_001:0935a27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引薩怛嚩二合嚩日哩二合二吽三加持五穀已。",
        "from": "T20n1194_001:0935a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次第而供養。",
        "from": "T20n1194_001:0935a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "表是食之根酥蜜乳酪等。",
        "from": "T20n1194_001:0935b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦誦於金剛波羅蜜真言。",
        "from": "T20n1194_001:0935b01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先用加持之。",
        "from": "T20n1194_001:0935b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以五般粥護摩燒供養。",
        "from": "T20n1194_001:0935b02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五粥者謂乳酪酥蜜沙糖乳糖粥。",
        "from": "T20n1194_001:0935b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念法波羅蜜菩薩真言加持。",
        "from": "T20n1194_001:0935b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1194_001:0935b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一達囉磨二合嚩日哩二合二紇哩以三合三如是加持已。",
        "from": "T20n1194_001:0935b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "表是淨供養。",
        "from": "T20n1194_001:0935b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當以根本波羅蜜真言。",
        "from": "T20n1194_001:0935b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持五般飯與五粥同真言曰。",
        "from": "T20n1194_001:0935b07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一商迦哩二訕底迦哩三齲吒𩕳伽吒野四薩嚩囉他二合娑馱𩕳五娑嚩二合賀引六加持五飯已。",
        "from": "T20n1194_001:0935b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "表法亦同前。",
        "from": "T20n1194_001:0935b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如上依法加持訖。",
        "from": "T20n1194_001:0935b10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當於爐上想一明王。",
        "from": "T20n1194_001:0935b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身如舍利塔。",
        "from": "T20n1194_001:0935b11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又想諸佛皆於塔內。",
        "from": "T20n1194_001:0935b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所作護摩運心供養。",
        "from": "T20n1194_001:0935b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然可取火。",
        "from": "T20n1194_001:0935b12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其火想隨所作。",
        "from": "T20n1194_001:0935b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "部主佛種子字為火。",
        "from": "T20n1194_001:0935b13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或用本明加持。",
        "from": "T20n1194_001:0935b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或內想智火發大光焰。",
        "from": "T20n1194_001:0935b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或想明王遍身火焰。",
        "from": "T20n1194_001:0935b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變用燒物而作供養。",
        "from": "T20n1194_001:0935b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燃其火已。",
        "from": "T20n1194_001:0935b15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或以淨水及與塗香灑火作淨。",
        "from": "T20n1194_001:0935b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次下於水表為淨水。",
        "from": "T20n1194_001:0935b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方用酥蜜乳酪。",
        "from": "T20n1194_001:0935b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及五柴五穀粥飯。",
        "from": "T20n1194_001:0935b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先各五投火中。",
        "from": "T20n1194_001:0935b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各別供養五如來。",
        "from": "T20n1194_001:0935b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨彼如來各念本明。",
        "from": "T20n1194_001:0935b18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒物供養五如來真言曰。",
        "from": "T20n1194_001:0935b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘盧遮那如來唵引一嚩日囉二合馱覩二鑁三阿閦如來唵引一惡乞蒭二合毘夜二合二吽三寶生如來唵引一囉恒曩二合三婆嚩二怛𠸪三合三阿彌陀如來唵引一阿弭多婆二紇哩以三合三不空成就如來唵引一阿謨伽悉弟二噁三先想供養五佛已。",
        "from": "T20n1194_001:0935c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次下水供養。",
        "from": "T20n1194_001:0935c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想與佛漱口。",
        "from": "T20n1194_001:0935c01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下水真言曰。",
        "from": "T20n1194_001:0935c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一訖哩二合嚕娜地二吽三訖哩二合囉嗚娜迦四唵引五次以華供養乃擲於爐內。",
        "from": "T20n1194_001:0935c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想華安佛頂上華供養真言。",
        "from": "T20n1194_001:0935c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一薩嚩怛他誐哆二補瑟跛二合布惹三次用塗香獻而灑於火中。",
        "from": "T20n1194_001:0935c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想塗灑諸佛塗。",
        "from": "T20n1194_001:0935c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香供養真言曰。",
        "from": "T20n1194_001:0935c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一薩嚩怛他誐哆二巘馱布惹又以香供養。",
        "from": "T20n1194_001:0935c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想於佛鼻入供養諸如來。",
        "from": "T20n1194_001:0935c11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒香供養真言曰。",
        "from": "T20n1194_001:0935c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一薩嚩怛他誐哆二度播布惹三次振鈴念讚或誦伽陀一切臨時。",
        "from": "T20n1194_001:0935c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次燒五穀。",
        "from": "T20n1194_001:0935c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念隨所求者。",
        "from": "T20n1194_001:0935c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "部主真言加持燒物。",
        "from": "T20n1194_001:0935c15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如別無所求事。",
        "from": "T20n1194_001:0935c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "只念法舍利真言燒物。",
        "from": "T20n1194_001:0935c16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養真言曰。",
        "from": "T20n1194_001:0935c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一達囉磨二合馱覩二誐羅陛二合三娑嚩二合賀燒五穀已。",
        "from": "T20n1194_001:0935c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用酥蜜乳酪各三投火中。",
        "from": "T20n1194_001:0935c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應下水運想供養一如前法。",
        "from": "T20n1194_001:0935c21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以粥供養先燒乳粥。",
        "from": "T20n1194_001:0935c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次下燒粥隨意。",
        "from": "T20n1194_001:0935c22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當燒粥時。",
        "from": "T20n1194_001:0935c22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取八所淨器。",
        "from": "T20n1194_001:0935c22_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或取八葉以塗香灑之。",
        "from": "T20n1194_001:0935c23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用粥飯盛於淨器中。",
        "from": "T20n1194_001:0935c23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以十方天真言加持。",
        "from": "T20n1194_001:0935c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於爐八方供養十方天上下二方。",
        "from": "T20n1194_001:0935c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心想供養十方天。",
        "from": "T20n1194_001:0935c25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明次第當說。",
        "from": "T20n1194_001:0935c25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東方帝釋天真言曰。",
        "from": "T20n1194_001:0935c26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱南一印捺囉二合野二娑嚩二合賀東南方火天真言曰。",
        "from": "T20n1194_001:0935c29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱南一阿仡曩二合曳二娑嚩二合賀南方焰摩天真言曰。",
        "from": "T20n1194_001:0936a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱南一焰摩野二娑嚩二合賀西南方羅剎主天真言曰。",
        "from": "T20n1194_001:0936a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱南一銘伽舍曩野二娑嚩二合賀西方水天真言曰。",
        "from": "T20n1194_001:0936a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱南一嚩嚕拏野二娑嚩二合賀西北方風天真言曰。",
        "from": "T20n1194_001:0936a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱南一嚩野吠二娑嚩二合賀北方毘沙門天真言曰。",
        "from": "T20n1194_001:0936a12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱南一吠室囉末拏野二娑嚩二合賀東北方伊舍那天真言曰。",
        "from": "T20n1194_001:0936a15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱南一伊舍娜野二娑嚩二合賀上方梵天真言曰。",
        "from": "T20n1194_001:0936a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱南一沒囉二合憾麼二合野二娑嚩二合賀下方地天真言曰。",
        "from": "T20n1194_001:0936a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱南一必哩二合替尾曳二合二娑嚩二合賀二合賀此等十方天。",
        "from": "T20n1194_001:0936a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先於爐八方。",
        "from": "T20n1194_001:0936a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安置八坐位。",
        "from": "T20n1194_001:0936a23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵天地天等。",
        "from": "T20n1194_001:0936a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於爐上下想。",
        "from": "T20n1194_001:0936a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然以瓶內水。",
        "from": "T20n1194_001:0936a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瀉供養飯內。",
        "from": "T20n1194_001:0936a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次用香華塗。",
        "from": "T20n1194_001:0936a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香隨位而供養。",
        "from": "T20n1194_001:0936a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各念本真言加持供養物。",
        "from": "T20n1194_001:0936a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本明誦三遍安於本方位。",
        "from": "T20n1194_001:0936a26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然可燒粥供養。",
        "from": "T20n1194_001:0936a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒粥已。",
        "from": "T20n1194_001:0936a27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酥蜜乳酪等。",
        "from": "T20n1194_001:0936a27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各三投火中。",
        "from": "T20n1194_001:0936a27_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下水次第一如前法。",
        "from": "T20n1194_001:0936a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以飯供養先燒於乳飯。",
        "from": "T20n1194_001:0936a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次酥後酪飯。",
        "from": "T20n1194_001:0936a29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是燒飯已。",
        "from": "T20n1194_001:0936a29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酥蜜乳酪等。",
        "from": "T20n1194_001:0936a29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各三投火中。",
        "from": "T20n1194_001:0936b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下水次第並同前法。",
        "from": "T20n1194_001:0936b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以護摩柴。",
        "from": "T20n1194_001:0936b01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梢根搵於酥投於火中。",
        "from": "T20n1194_001:0936b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又用酥蜜乳酪。",
        "from": "T20n1194_001:0936b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各三投火中。",
        "from": "T20n1194_001:0936b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是次第訖然後起立。",
        "from": "T20n1194_001:0936b03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手執酥器并護摩匙。",
        "from": "T20n1194_001:0936b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "振鈴念誦。",
        "from": "T20n1194_001:0936b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用酥瀉於火中。",
        "from": "T20n1194_001:0936b04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有隨喜人。",
        "from": "T20n1194_001:0936b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各令塗手執華候念讚瀉酥訖。",
        "from": "T20n1194_001:0936b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各擲華於爐內。",
        "from": "T20n1194_001:0936b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往火壇禮拜表供養圓滿。",
        "from": "T20n1194_001:0936b06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次用淨帛揩酥覆火燒之。",
        "from": "T20n1194_001:0936b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想為佛衣如上了畢。",
        "from": "T20n1194_001:0936b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應用淨瓶繞爐下水。",
        "from": "T20n1194_001:0936b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "再想漱口作淨。",
        "from": "T20n1194_001:0936b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作護摩時。",
        "from": "T20n1194_001:0936b08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運想應志心。",
        "from": "T20n1194_001:0936b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養勿須忙。",
        "from": "T20n1194_001:0936b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所抄供養物。",
        "from": "T20n1194_001:0936b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如菴摩羅果子許此方如杏子大所說護摩壇。",
        "from": "T20n1194_001:0936b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知七佛舌。",
        "from": "T20n1194_001:0936b10_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱胝諸天口。",
        "from": "T20n1194_001:0936b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切供養中。",
        "from": "T20n1194_001:0936b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護摩最為上。",
        "from": "T20n1194_001:0936b11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有燒不盡。",
        "from": "T20n1194_001:0936b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五穀粥飯等。",
        "from": "T20n1194_001:0936b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更用別飲食。",
        "from": "T20n1194_001:0936b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸華果實。",
        "from": "T20n1194_001:0936b12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共盛一器中。",
        "from": "T20n1194_001:0936b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用塗香灑之。",
        "from": "T20n1194_001:0936b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復用燒香熏。",
        "from": "T20n1194_001:0936b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先作鉤召契。",
        "from": "T20n1194_001:0936b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三昧耶蓮華。",
        "from": "T20n1194_001:0936b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及金剛縛印。",
        "from": "T20n1194_001:0936b14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次下水半振鈴念加持飯真言。",
        "from": "T20n1194_001:0936b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想供養天龍八部及火壇外諸天。",
        "from": "T20n1194_001:0936b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運心皆供養。",
        "from": "T20n1194_001:0936b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是護摩供養已。",
        "from": "T20n1194_001:0936b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後奉送諸聖眾。",
        "from": "T20n1194_001:0936b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所集祕密殊勝福。",
        "from": "T20n1194_001:0936b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普霑一切諸含識。",
        "from": "T20n1194_001:0936b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙吉祥平等觀門大教王經略出護摩儀",
        "from": "T20n1194_001:0936b19_100",
        "context_with_next_sentence": false
    }
]