[
    {
        "source_sentence": "地藏菩薩十齋日",
        "from": "T85n2850_001:1300a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日童子下念定光如來不塗刀鎗地獄持齋",
        "from": "T85n2850_001:1300a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除罪四十劫。",
        "from": "T85n2850_001:1300a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八日太子下念藥師瑠璃光佛不塗糞屎地獄持齋除罪三十劫。",
        "from": "T85n2850_001:1300a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十四日察命下念賢劫千佛不塗鑊湯地獄持齋除罪一千劫。",
        "from": "T85n2850_001:1300a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十五日五道大將軍下念阿彌陀佛不墮寒氷地獄持齋除罪二百劫。",
        "from": "T85n2850_001:1300a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十八日閻羅王下念觀世音菩薩不墮劍樹地獄持齋除罪九十劫。",
        "from": "T85n2850_001:1300a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十三日大將軍下念盧舍那佛不墮餓鬼地獄持齋除罪一千劫。",
        "from": "T85n2850_001:1300a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十四日太山府君下念地獄菩薩不墮斬斫地獄持齋除罪一千劫。",
        "from": "T85n2850_001:1300a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十八日帝釋下念阿彌陀佛不墮鐵鋸地獄持齋除罪九十劫。",
        "from": "T85n2850_001:1300a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十九日四天王下念藥師上菩薩不墮磑摩地獄持齋除罪七千劫。",
        "from": "T85n2850_001:1300a28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十日梵天王下念釋迦牟尼佛不墮灰何地獄持齋除罪八千劫。",
        "from": "T85n2850_001:1300b01_8",
        "context_with_next_sentence": true
    }
