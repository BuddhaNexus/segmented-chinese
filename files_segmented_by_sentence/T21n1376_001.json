[
    {
        "source_sentence": "佛說聖莊嚴陀羅尼經卷上",
        "from": "T21n1376_001:0895c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試鴻臚少卿",
        "from": "T21n1376_001:0895c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳法大師臣施護奉　詔譯",
        "from": "T21n1376_001:0895c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T21n1376_001:0895c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時世尊。",
        "from": "T21n1376_001:0895c29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在迦毘羅城無憂樹園。",
        "from": "T21n1376_001:0895c29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與阿難陀等大比丘眾。",
        "from": "T21n1376_001:0896a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及慈氏菩薩摩訶薩俱會一處。",
        "from": "T21n1376_001:0896a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時復有天眾龍眾夜叉眾。",
        "from": "T21n1376_001:0896a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅剎眾阿修羅眾。",
        "from": "T21n1376_001:0896a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦樓羅眾乾闥婆眾。",
        "from": "T21n1376_001:0896a03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緊曩羅眾毘舍遮眾。",
        "from": "T21n1376_001:0896a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畢哩多眾鳩槃茶眾。",
        "from": "T21n1376_001:0896a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "布單曩眾執魅眾。",
        "from": "T21n1376_001:0896a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十八大藥叉軍主眾。",
        "from": "T21n1376_001:0896a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四大藥叉軍主眾。",
        "from": "T21n1376_001:0896a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切星曜眾。",
        "from": "T21n1376_001:0896a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至雪山苦行諸仙眾。",
        "from": "T21n1376_001:0896a06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等眾皆來集會。",
        "from": "T21n1376_001:0896a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有他方一切羅剎等眾。",
        "from": "T21n1376_001:0896a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂護迦儞隷布寫那哩枳吒迦囉拏穌摩娜摩賀賀哩摩怛哩眾。",
        "from": "T21n1376_001:0896a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等眾亦來集會。",
        "from": "T21n1376_001:0896a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有四方四大羅剎。",
        "from": "T21n1376_001:0896a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂東方賀哩羅剎。",
        "from": "T21n1376_001:0896a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南方虞拏羅剎。",
        "from": "T21n1376_001:0896a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方枳設哩羅剎。",
        "from": "T21n1376_001:0896a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方摩尼袒哆羅剎。",
        "from": "T21n1376_001:0896a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此四羅剎性極暴惡。",
        "from": "T21n1376_001:0896a13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各有六十眷屬。",
        "from": "T21n1376_001:0896a13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是等羅剎亦來在會。",
        "from": "T21n1376_001:0896a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時淨飯王宮羅睺羅童子。",
        "from": "T21n1376_001:0896a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於夜臥時為惡羅剎而來嬈亂。",
        "from": "T21n1376_001:0896a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數數驚怖不得安隱。",
        "from": "T21n1376_001:0896a16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時羅睺羅母耶輸陀羅及諸眷屬。",
        "from": "T21n1376_001:0896a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉知世尊與諸聖眾。",
        "from": "T21n1376_001:0896a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在無憂樹園去宮城不遠。",
        "from": "T21n1376_001:0896a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於夜後分令羅睺羅。",
        "from": "T21n1376_001:0896a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與眾童子及諸侍人。",
        "from": "T21n1376_001:0896a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後導從往詣佛所。",
        "from": "T21n1376_001:0896a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時羅睺羅童子至佛會已頂禮佛足。",
        "from": "T21n1376_001:0896a20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獨於佛前涕泣而住。",
        "from": "T21n1376_001:0896a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告阿難言。",
        "from": "T21n1376_001:0896a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難汝見羅睺羅童子。",
        "from": "T21n1376_001:0896a22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來詣我所涕泣已否。",
        "from": "T21n1376_001:0896a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難言唯然已見。",
        "from": "T21n1376_001:0896a23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言阿難。",
        "from": "T21n1376_001:0896a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅睺羅童子於夜臥時。",
        "from": "T21n1376_001:0896a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為惡羅剎及種種鬼神。",
        "from": "T21n1376_001:0896a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而來嬈亂不得安隱。",
        "from": "T21n1376_001:0896a25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故來我所而求擁護。",
        "from": "T21n1376_001:0896a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難非獨今日羅睺羅童子。",
        "from": "T21n1376_001:0896a26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼此嬈亂不得安隱。",
        "from": "T21n1376_001:0896a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我滅度後末世之時。",
        "from": "T21n1376_001:0896a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生福力微劣。",
        "from": "T21n1376_001:0896a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於不善業增益熾盛。",
        "from": "T21n1376_001:0896a28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由是諸惡羅剎。",
        "from": "T21n1376_001:0896a28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及鬼神等遊行世間。",
        "from": "T21n1376_001:0896a29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒與眾生作不饒益。",
        "from": "T21n1376_001:0896a29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種嬈亂使無安隱。",
        "from": "T21n1376_001:0896b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我觀是事深可憐愍。",
        "from": "T21n1376_001:0896b01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難我今為擁護羅睺羅童子。",
        "from": "T21n1376_001:0896b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及愍念未來眾生。",
        "from": "T21n1376_001:0896b02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說大莊嚴陀羅尼。",
        "from": "T21n1376_001:0896b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當諦聽。",
        "from": "T21n1376_001:0896b03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時阿難及會大眾。",
        "from": "T21n1376_001:0896b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸各默然願佛宣說。",
        "from": "T21n1376_001:0896b04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊即說大莊嚴陀羅尼曰。",
        "from": "T21n1376_001:0896b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也二合引他引伽儞引誐囉二合引伽曩鉢底具彌引誐囉二合引具摩鉢底瑟波二合哩引迦伽哩尼伽囉鉢底娑引囉鉢底馱曩鉢底酥囉鉢底阿隷半阿隷半鉢底摩鉢底阿鉢底嚩　護鉢底護羅賀囉二合引呬隷誐囉二合護嚕烏底隷普呬隷唵引唧致隷鉢哆二合鉢致引吒吒吠引賀囉二合引唵引羅吠引摩尼娑哩引護摩賀引呬隷護護三滿多娑娑哩阿三滿多娑哩波隷佉娑哩引阿引挽哩哆二合摩賀引挽哩帝二合引伽儞引彌佉隷引賀囉二合引娑嚩二合引賀引阿難此大莊嚴陀羅尼。",
        "from": "T21n1376_001:0896b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃是過去諸佛之所宣說。",
        "from": "T21n1376_001:0896b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今為擁護羅睺羅童子。",
        "from": "T21n1376_001:0896b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦復利益末世眾生。",
        "from": "T21n1376_001:0896b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今復宣說。",
        "from": "T21n1376_001:0896b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難若有眾生。",
        "from": "T21n1376_001:0896b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被諸羅剎及惡鬼神種種嬈亂者。",
        "from": "T21n1376_001:0896b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當以此莊嚴陀羅尼書於紙素。",
        "from": "T21n1376_001:0896b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作彼隨求戴於身臂。",
        "from": "T21n1376_001:0896b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當令諸惡羅剎。",
        "from": "T21n1376_001:0896b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至世間一切食膏血者。",
        "from": "T21n1376_001:0896b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡鬼神類不敢侵近悉皆奔走。",
        "from": "T21n1376_001:0896b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四方四維十由旬外遠避而去。",
        "from": "T21n1376_001:0896b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當令眾生得安隱樂。",
        "from": "T21n1376_001:0896b23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難若有眾生。",
        "from": "T21n1376_001:0896b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒常讀誦此大莊嚴陀羅尼者。",
        "from": "T21n1376_001:0896b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有一切刀杖毒藥不能為害。",
        "from": "T21n1376_001:0896b25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至邪明呪術羂索繫縛。",
        "from": "T21n1376_001:0896b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人及非人一切惡事悉皆解脫。",
        "from": "T21n1376_001:0896b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至十由旬內。",
        "from": "T21n1376_001:0896b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生皆蒙擁護。",
        "from": "T21n1376_001:0896b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切災沴咸皆殄滅。",
        "from": "T21n1376_001:0896b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難若有眾生。",
        "from": "T21n1376_001:0896b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲持念此大莊嚴陀羅尼擁護自身。",
        "from": "T21n1376_001:0896c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及為饒益一切眾生者。",
        "from": "T21n1376_001:0896c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先當擇取清淨勝地。",
        "from": "T21n1376_001:0896c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "預立幖幟。",
        "from": "T21n1376_001:0896c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然自齋戒香水澡浴。",
        "from": "T21n1376_001:0896c02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當令身心內外清淨。",
        "from": "T21n1376_001:0896c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以線結界香泥塗地作曼拏羅。",
        "from": "T21n1376_001:0896c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於曼拏羅中備諸名花。",
        "from": "T21n1376_001:0896c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗香燒香然燈種種供養。",
        "from": "T21n1376_001:0896c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面北端坐正念想佛世尊。",
        "from": "T21n1376_001:0896c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來降曼拏羅已。",
        "from": "T21n1376_001:0896c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後奉獻種種供養。",
        "from": "T21n1376_001:0896c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命讚歎。",
        "from": "T21n1376_001:0896c06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發堅固心勇猛不退。",
        "from": "T21n1376_001:0896c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲饒益擁護諸眾生故。",
        "from": "T21n1376_001:0896c07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及求現前所作成就故。",
        "from": "T21n1376_001:0896c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先當持誦結界莊嚴陀羅尼。",
        "from": "T21n1376_001:0896c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "警覺召請梵王帝釋及一切天龍夜叉羅剎諸鬼神等皆來集會。",
        "from": "T21n1376_001:0896c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當令世間一切惱害眾生。",
        "from": "T21n1376_001:0896c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸惡夜叉羅剎部多等。",
        "from": "T21n1376_001:0896c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞此陀羅尼已。",
        "from": "T21n1376_001:0896c11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "驚怖退散無諸障難得願圓滿。",
        "from": "T21n1376_001:0896c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說結界陀羅尼曰。",
        "from": "T21n1376_001:0896c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也二合引他儞隷儞隷尾摩隷引怛摩隷引護唧護唧悉哩底哩怛哩二合娑儞引囉普莎賀引阿難若誦此結界莊嚴陀羅尼已。",
        "from": "T21n1376_001:0896c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後誦彼聖大莊嚴陀羅尼。",
        "from": "T21n1376_001:0896c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切所作無不成就。",
        "from": "T21n1376_001:0896c17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時慈氏菩薩摩訶薩從座而起。",
        "from": "T21n1376_001:0896c18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住立佛前白言世尊。",
        "from": "T21n1376_001:0896c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦為欲擁護羅睺羅童子令得安隱。",
        "from": "T21n1376_001:0896c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦復為欲利益未來一切眾生。",
        "from": "T21n1376_001:0896c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有被諸嬈惱。",
        "from": "T21n1376_001:0896c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及一切惡人刀杖毒藥。",
        "from": "T21n1376_001:0896c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸惡禁呪羂索繫縛。",
        "from": "T21n1376_001:0896c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等事悉令解脫。",
        "from": "T21n1376_001:0896c22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於晝夜中安隱快樂。",
        "from": "T21n1376_001:0896c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦說莊嚴陀羅尼。",
        "from": "T21n1376_001:0896c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願世尊。",
        "from": "T21n1376_001:0896c23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "許我宣說。",
        "from": "T21n1376_001:0896c23_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言當隨汝意。",
        "from": "T21n1376_001:0896c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時慈氏菩薩即說莊嚴陀羅尼曰。",
        "from": "T21n1376_001:0896c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也二合引他引阿嚩那引底播那引儞阿尾那引努祖嚕祖嚕惹哩馱二合難尼引難尼引阿馱難尼引三滿哆難尼引儞哩瞢二合尼引儞哩弭二合引目儞引扇引底娑嚩二合悉怛也三合引野喃摩摩薩哩嚩二合薩怛嚩二合引喃引左洛乞叉二合引俱嚕娑嚩二合引賀引爾時娑婆世界主大梵天王。",
        "from": "T21n1376_001:0897a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前白佛言。",
        "from": "T21n1376_001:0897a03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦為欲擁護羅睺羅童子。",
        "from": "T21n1376_001:0897a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及擁護未來一切眾生。",
        "from": "T21n1376_001:0897a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說自莊嚴明。",
        "from": "T21n1376_001:0897a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願如來許我宣說。",
        "from": "T21n1376_001:0897a05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言善哉善哉當隨汝意。",
        "from": "T21n1376_001:0897a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時大梵天王即說莊嚴明曰。",
        "from": "T21n1376_001:0897a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三野體難盎虞哩引半虞哩引阿努哩引嫩努哩引尸契孕二合迦尸佉哩引嫩度哩引努哩努二合囉野底阿左阿左尾目左扇引底孕二合娑嚩二合悉怛野三合野喃俱嚕摩摩薩哩嚩二合薩怛嚩二合引喃引左娑嚩二合引賀引爾時世尊。",
        "from": "T21n1376_001:0897a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知彼天眾龍眾夜叉等眾及諸鬼神。",
        "from": "T21n1376_001:0897a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各各為欲擁護羅睺羅童子及未來眾生。",
        "from": "T21n1376_001:0897a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆悉欲說自明。",
        "from": "T21n1376_001:0897a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言汝等天眾龍眾及諸鬼神等。",
        "from": "T21n1376_001:0897a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲說自明為擁護諸眾生者。",
        "from": "T21n1376_001:0897a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨汝等意各各說之。",
        "from": "T21n1376_001:0897a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時善時分天。",
        "from": "T21n1376_001:0897a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於佛前說自明曰。",
        "from": "T21n1376_001:0897a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三野體難囉瑟致哩三合引波那引瑟致哩三合引嚕地囉迦囉尼二合引儼鼻哩引努哩努二合哩母唧引設引訖哩二合囉引濟曳二合引扇引底孕二合娑嚩二合悉怛也三合野喃迦嚕弭摩摩洛乞叉二合引俱嚕娑嚩二合引賀引佛說聖莊嚴陀羅尼經卷上",
        "from": "T21n1376_001:0897a24_100",
        "context_with_next_sentence": false
    }
]