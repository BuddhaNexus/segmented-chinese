[
    {
        "source_sentence": "新菩薩經一卷",
        "from": "T85n2917A_001:1462a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賈耽頒下諸州眾生。每日念阿彌陀佛一千",
        "from": "T85n2917A_001:1462a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口。斷惡行善。今年大熱無人收苅。有數種",
        "from": "T85n2917A_001:1462a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "病死。",
        "from": "T85n2917A_001:1462a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一虐病死。",
        "from": "T85n2917A_001:1462a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二天行病死。",
        "from": "T85n2917A_001:1462a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三卒死。",
        "from": "T85n2917A_001:1462a28_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四腫病死。",
        "from": "T85n2917A_001:1462a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五產坐死。",
        "from": "T85n2917A_001:1462a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六患腹死。",
        "from": "T85n2917A_001:1462a29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第七血癰死。",
        "from": "T85n2917A_001:1462b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八風黃病死。",
        "from": "T85n2917A_001:1462b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第九水裏死。",
        "from": "T85n2917A_001:1462b01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十患眼死。",
        "from": "T85n2917A_001:1462b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勸諸眾生寫一本免一身。",
        "from": "T85n2917A_001:1462b02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寫兩本免一門。",
        "from": "T85n2917A_001:1462b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寫三本免一村。",
        "from": "T85n2917A_001:1462b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不寫者滅門。",
        "from": "T85n2917A_001:1462b03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "門上傍之得過此難。",
        "from": "T85n2917A_001:1462b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但看七八月三家使一牛五男同一婦僧尼巡門。",
        "from": "T85n2917A_001:1462b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勸寫此經。",
        "from": "T85n2917A_001:1462b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其經西凉州正月二日盛中時。",
        "from": "T85n2917A_001:1462b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雷鳴雨聲。",
        "from": "T85n2917A_001:1462b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有一石下大如㪷等。",
        "from": "T85n2917A_001:1462b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "石遂兩片即見此經。",
        "from": "T85n2917A_001:1462b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "報諸眾生今戴饒患。",
        "from": "T85n2917A_001:1462b07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "新菩薩經一卷",
        "from": "T85n2917A_001:1462b08_100",
        "context_with_next_sentence": false
    }
]