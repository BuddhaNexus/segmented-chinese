[
    {
        "source_sentence": "大乘四法經釋抄",
        "from": "T85n2784_001:0557c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大乘四法經　夫立經名者。皆約四種而立",
        "from": "T85n2784_001:0557c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其名。者人。如維摩經等是　二處者。如楞",
        "from": "T85n2784_001:0557c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伽經。",
        "from": "T85n2784_001:0557c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊謂楞伽王名羅波羅王。",
        "from": "T85n2784_001:0557c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就楞伽山說法。",
        "from": "T85n2784_001:0557c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂約□立其名　三法者。",
        "from": "T85n2784_001:0557c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如大般若經說。",
        "from": "T85n2784_001:0557c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘊界等法。",
        "from": "T85n2784_001:0557c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內外俱空。",
        "from": "T85n2784_001:0557c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不執人法二我故。",
        "from": "T85n2784_001:0557c09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名約法立也　四喻者。",
        "from": "T85n2784_001:0557c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如寶雲經等。",
        "from": "T85n2784_001:0557c10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說彼經時。",
        "from": "T85n2784_001:0557c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方菩薩悉皆雲集故。",
        "from": "T85n2784_001:0557c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立喻為名　問。",
        "from": "T85n2784_001:0557c11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經於此四種約何立名　答。",
        "from": "T85n2784_001:0557c12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此經者約法立也。",
        "from": "T85n2784_001:0557c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其法云何。",
        "from": "T85n2784_001:0557c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為不捨菩提心。",
        "from": "T85n2784_001:0557c13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及不捨於空閑等。",
        "from": "T85n2784_001:0557c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是也。",
        "from": "T85n2784_001:0557c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言所緣　言修行大者。",
        "from": "T85n2784_001:0557c15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣修自行及他行者。",
        "from": "T85n2784_001:0557c15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言自行者修六度等。",
        "from": "T85n2784_001:0557c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六度之中。",
        "from": "T85n2784_001:0557c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前三是他行。",
        "from": "T85n2784_001:0557c16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後二是自利行。",
        "from": "T85n2784_001:0557c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精進者行他行者。",
        "from": "T85n2784_001:0557c17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生。",
        "from": "T85n2784_001:0557c17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應以佛身得度者。",
        "from": "T85n2784_001:0557c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即現佛身及聲聞菩薩等是也。",
        "from": "T85n2784_001:0557c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以經四法合能發菩提心能攝自利利他行。",
        "from": "T85n2784_001:0557c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又釋不捨菩提心。",
        "from": "T85n2784_001:0557c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善知諸樂住空閑等是自行。",
        "from": "T85n2784_001:0557c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心辱柔和是利他行。",
        "from": "T85n2784_001:0557c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言智大者。",
        "from": "T85n2784_001:0557c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了人法俱空。",
        "from": "T85n2784_001:0557c22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不同聲聞能了人空。",
        "from": "T85n2784_001:0557c22_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為法空。",
        "from": "T85n2784_001:0557c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是以住於涅槃。",
        "from": "T85n2784_001:0557c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無身利他。",
        "from": "T85n2784_001:0557c23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不同異生俱不能了被煩惱縛。",
        "from": "T85n2784_001:0557c24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛菩薩。",
        "from": "T85n2784_001:0557c24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了人空故不住生死。",
        "from": "T85n2784_001:0557c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了法空故不住涅槃。",
        "from": "T85n2784_001:0557c25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言精進大者。",
        "from": "T85n2784_001:0557c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三無數劫　一無數劫修四加行圓滿　二無數劫。",
        "from": "T85n2784_001:0557c27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從初地至八地。",
        "from": "T85n2784_001:0557c27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修見道成就　三無數劫。",
        "from": "T85n2784_001:0557c28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從八地至無學。",
        "from": "T85n2784_001:0557c28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如舍利弗行菩薩行時。",
        "from": "T85n2784_001:0557c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有一婆羅門乞眼。",
        "from": "T85n2784_001:0557c29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂施與之婆羅門得眼便以脚磋之。",
        "from": "T85n2784_001:0558a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利弗却問云。",
        "from": "T85n2784_001:0558a01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何故磋之。",
        "from": "T85n2784_001:0558a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答言。",
        "from": "T85n2784_001:0558a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我要其聲。",
        "from": "T85n2784_001:0558a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言方便大。",
        "from": "T85n2784_001:0558a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同上智大說。",
        "from": "T85n2784_001:0558a03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言業大者。",
        "from": "T85n2784_001:0558a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡生生死際能作諸佛。",
        "from": "T85n2784_001:0558a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘者能發四願。",
        "from": "T85n2784_001:0558a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願大。",
        "from": "T85n2784_001:0558a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有一眾生不成佛者。",
        "from": "T85n2784_001:0558a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誓不趣求無上菩提。",
        "from": "T85n2784_001:0558a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不同聲聞不發大願。",
        "from": "T85n2784_001:0558a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就大者。",
        "from": "T85n2784_001:0558a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而能成就十力。",
        "from": "T85n2784_001:0558a07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四無畏。",
        "from": "T85n2784_001:0558a07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十八不共法等。",
        "from": "T85n2784_001:0558a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大功德故。",
        "from": "T85n2784_001:0558a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一如餘處明。",
        "from": "T85n2784_001:0558a08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上來七大義者。",
        "from": "T85n2784_001:0558a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前六為因。",
        "from": "T85n2784_001:0558a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為一一不能成十力等。",
        "from": "T85n2784_001:0558a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要須修此六大。",
        "from": "T85n2784_001:0558a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後一為果。",
        "from": "T85n2784_001:0558a10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言四法船者。",
        "from": "T85n2784_001:0558a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即不捨菩提心。",
        "from": "T85n2784_001:0558a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及樂住於空閑者是。",
        "from": "T85n2784_001:0558a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言列數者論中自明。",
        "from": "T85n2784_001:0558a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言謂出生者。",
        "from": "T85n2784_001:0558a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此四法修行能出死海。",
        "from": "T85n2784_001:0558a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言號顯示者。",
        "from": "T85n2784_001:0558a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能顯甚深法性。",
        "from": "T85n2784_001:0558a15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言古今不易者。",
        "from": "T85n2784_001:0558a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合上常義。",
        "from": "T85n2784_001:0558a16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前佛後佛不曾移益。",
        "from": "T85n2784_001:0558a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦如十二因緣。",
        "from": "T85n2784_001:0558a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法逕二字亦如上合。",
        "from": "T85n2784_001:0558a17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言示處者。",
        "from": "T85n2784_001:0558a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞至菩薩魔訶薩也是。",
        "from": "T85n2784_001:0558a18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言於相者。",
        "from": "T85n2784_001:0558a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即三相。",
        "from": "T85n2784_001:0558a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是遍計所執等。",
        "from": "T85n2784_001:0558a19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言法者。",
        "from": "T85n2784_001:0558a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五蘊十八界等是。",
        "from": "T85n2784_001:0558a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言義者。",
        "from": "T85n2784_001:0558a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即能詮其五蘊十八界體住者是。",
        "from": "T85n2784_001:0558a21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言發起序者有二。",
        "from": "T85n2784_001:0558a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一因請發起。",
        "from": "T85n2784_001:0558a22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二不因識發起。",
        "from": "T85n2784_001:0558a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "T85n2784_001:0558a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何不廣釋證信之序。",
        "from": "T85n2784_001:0558a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "T85n2784_001:0558a23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證信之序通諸經共有。",
        "from": "T85n2784_001:0558a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不假重釋。",
        "from": "T85n2784_001:0558a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖然今略明矣。",
        "from": "T85n2784_001:0558a24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言佛化緣畢者。",
        "from": "T85n2784_001:0558a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但度乃緣。",
        "from": "T85n2784_001:0558a25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不度無緣故。",
        "from": "T85n2784_001:0558a25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "維摩經云。",
        "from": "T85n2784_001:0558a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛威雖普。",
        "from": "T85n2784_001:0558a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不度無根無澤無□不潤枯木。",
        "from": "T85n2784_001:0558a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言四合處者。",
        "from": "T85n2784_001:0558a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言觀身不淨者。",
        "from": "T85n2784_001:0558a28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此身之上三十六物等種種不淨。",
        "from": "T85n2784_001:0558a29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共成此身故。",
        "from": "T85n2784_001:0558a29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名不淨言云云言觀受是苦者。",
        "from": "T85n2784_001:0558b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦受樂受不苦不樂受。",
        "from": "T85n2784_001:0558b01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並皆是苦。",
        "from": "T85n2784_001:0558b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生不了。",
        "from": "T85n2784_001:0558b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我執為樂。",
        "from": "T85n2784_001:0558b02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉菩薩等。",
        "from": "T85n2784_001:0558b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "等取彌勒文殊者是。",
        "from": "T85n2784_001:0558b03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言觀心無常者。",
        "from": "T85n2784_001:0558b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此心念念還謝。",
        "from": "T85n2784_001:0558b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生不了。",
        "from": "T85n2784_001:0558b04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執為常相。",
        "from": "T85n2784_001:0558b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言觀法無我者。",
        "from": "T85n2784_001:0558b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於五蘊十八界上。",
        "from": "T85n2784_001:0558b06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本無其我人眾生等。",
        "from": "T85n2784_001:0558b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生不了。",
        "from": "T85n2784_001:0558b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "橫計有我。",
        "from": "T85n2784_001:0558b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但不起如上倒見。",
        "from": "T85n2784_001:0558b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得成聖道。",
        "from": "T85n2784_001:0558b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口者因也。",
        "from": "T85n2784_001:0558b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言梵壇者。",
        "from": "T85n2784_001:0558b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此云默檳。",
        "from": "T85n2784_001:0558b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言雜事律者。",
        "from": "T85n2784_001:0558b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即根本部中下五十卷中說。",
        "from": "T85n2784_001:0558b10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言結集時者。",
        "from": "T85n2784_001:0558b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛涅槃後。",
        "from": "T85n2784_001:0558b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於王舍城七葉巖窟。",
        "from": "T85n2784_001:0558b11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "集諸五百大阿羅漢。",
        "from": "T85n2784_001:0558b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾請阿難昇座說法。",
        "from": "T85n2784_001:0558b12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾起三疑。",
        "from": "T85n2784_001:0558b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及乎阿難說如是我聞等。",
        "from": "T85n2784_001:0558b13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛初成道。",
        "from": "T85n2784_001:0558b13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鹿野苑中初度五俱輪等。",
        "from": "T85n2784_001:0558b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說四諦法。",
        "from": "T85n2784_001:0558b14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三轉者云云。",
        "from": "T85n2784_001:0558b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言時方人者。",
        "from": "T85n2784_001:0558b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即彼經中一時是。",
        "from": "T85n2784_001:0558b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方者處也。",
        "from": "T85n2784_001:0558b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人者聽法之徒。",
        "from": "T85n2784_001:0558b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言教者。",
        "from": "T85n2784_001:0558b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即十二分教是。",
        "from": "T85n2784_001:0558b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言理者。",
        "from": "T85n2784_001:0558b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是真如之性。",
        "from": "T85n2784_001:0558b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言行者。",
        "from": "T85n2784_001:0558b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即八聖道。",
        "from": "T85n2784_001:0558b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言果者。",
        "from": "T85n2784_001:0558b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即有上果無上果等云云。",
        "from": "T85n2784_001:0558b21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言依願得果為如者。",
        "from": "T85n2784_001:0558b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "布施得大福等云云。",
        "from": "T85n2784_001:0558b22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言廢別就總者。",
        "from": "T85n2784_001:0558b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有二義。",
        "from": "T85n2784_001:0558b23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一要假耳根發識。",
        "from": "T85n2784_001:0558b23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "意隨分別。",
        "from": "T85n2784_001:0558b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言一一陳遂成繁廣者。",
        "from": "T85n2784_001:0558b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是伏難云。",
        "from": "T85n2784_001:0558b25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經題之中但安根意思識則得。",
        "from": "T85n2784_001:0558b26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何要我聞。",
        "from": "T85n2784_001:0558b26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "T85n2784_001:0558b26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義者安如是遂成繁廣　何故但不舉一。",
        "from": "T85n2784_001:0558b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "T85n2784_001:0558b27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若舉一者義用不用　又伏難云。",
        "from": "T85n2784_001:0558b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若爾何不經。",
        "from": "T85n2784_001:0558b28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言無我聞。",
        "from": "T85n2784_001:0558b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為引外道入法故。",
        "from": "T85n2784_001:0558b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂立我聞。",
        "from": "T85n2784_001:0558b29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如佛方便為長爪梵志問佛世尊假立其我。",
        "from": "T85n2784_001:0558c01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言善合時宜者。",
        "from": "T85n2784_001:0558c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應食前堪化度者即以度之。",
        "from": "T85n2784_001:0558c02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若食後堪度者即以度之。",
        "from": "T85n2784_001:0558c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言機感相應者。",
        "from": "T85n2784_001:0558c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如清水。",
        "from": "T85n2784_001:0558c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨器大小。",
        "from": "T85n2784_001:0558c04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並感月影於中。",
        "from": "T85n2784_001:0558c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能現大小之影。",
        "from": "T85n2784_001:0558c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言相應得益者。",
        "from": "T85n2784_001:0558c05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛如來。",
        "from": "T85n2784_001:0558c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨眾生三乘根性。",
        "from": "T85n2784_001:0558c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應時說法。",
        "from": "T85n2784_001:0558c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令眾生各獲益。",
        "from": "T85n2784_001:0558c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦如醫人。",
        "from": "T85n2784_001:0558c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨病冷熱令服其藥。",
        "from": "T85n2784_001:0558c07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得疾癒。",
        "from": "T85n2784_001:0558c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言外典無如是能者。",
        "from": "T85n2784_001:0558c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無如上應機設教。",
        "from": "T85n2784_001:0558c09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應病施藥。",
        "from": "T85n2784_001:0558c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無一時之言。",
        "from": "T85n2784_001:0558c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言前簡說聽者。",
        "from": "T85n2784_001:0558c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘部經中簡取四法。",
        "from": "T85n2784_001:0558c11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言後簡說聽者。",
        "from": "T85n2784_001:0558c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則是食前食後義。",
        "from": "T85n2784_001:0558c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言若爾諸法應非唯識者。",
        "from": "T85n2784_001:0558c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏難云。",
        "from": "T85n2784_001:0558c13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此四法經。",
        "from": "T85n2784_001:0558c13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若依大乘三宗之中。",
        "from": "T85n2784_001:0558c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且是唯識宗之所攝。",
        "from": "T85n2784_001:0558c14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如上立一及時心外法故。",
        "from": "T85n2784_001:0558c15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此法應非唯識。",
        "from": "T85n2784_001:0558c15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答辭同決文。",
        "from": "T85n2784_001:0558c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言世識者。",
        "from": "T85n2784_001:0558c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是世間器世間。",
        "from": "T85n2784_001:0558c17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言數識者。",
        "from": "T85n2784_001:0558c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一二三四是。",
        "from": "T85n2784_001:0558c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言最勝尊者。",
        "from": "T85n2784_001:0558c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是佛也。",
        "from": "T85n2784_001:0558c19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言自在者。",
        "from": "T85n2784_001:0558c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三界九地見修等煩惱不能繫縛。",
        "from": "T85n2784_001:0558c20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於煩惱得自在故。",
        "from": "T85n2784_001:0558c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言炎猛者。",
        "from": "T85n2784_001:0558c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以智慧火燒煩惱薪故。",
        "from": "T85n2784_001:0558c22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言端嚴者。",
        "from": "T85n2784_001:0558c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為恭敬父母師僧和上。",
        "from": "T85n2784_001:0558c23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲得三十二相八十種好等。",
        "from": "T85n2784_001:0558c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言吉祥者。",
        "from": "T85n2784_001:0558c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "親近恭養咸稱讚故具吉祥。",
        "from": "T85n2784_001:0558c25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如盲者得見啞者能語等云云。",
        "from": "T85n2784_001:0558c26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言常起方便利益安樂者。",
        "from": "T85n2784_001:0558c27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間利益出世間安樂。",
        "from": "T85n2784_001:0558c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或增上勝安樂決定生利益翻。",
        "from": "T85n2784_001:0558c28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覆用名尊貴義。",
        "from": "T85n2784_001:0558c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦云精進。",
        "from": "T85n2784_001:0558c29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言佛具十德者。",
        "from": "T85n2784_001:0559a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏難云。",
        "from": "T85n2784_001:0559a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何不經初安正遍知等。",
        "from": "T85n2784_001:0559a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但云薄伽梵名。",
        "from": "T85n2784_001:0559a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "T85n2784_001:0559a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辭如決中。",
        "from": "T85n2784_001:0559a02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言然佛世尊有三身者。",
        "from": "T85n2784_001:0559a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏難云。",
        "from": "T85n2784_001:0559a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因論生論。",
        "from": "T85n2784_001:0559a03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因論生語。",
        "from": "T85n2784_001:0559a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又問。",
        "from": "T85n2784_001:0559a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛有三者。",
        "from": "T85n2784_001:0559a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為小根人即化身說。",
        "from": "T85n2784_001:0559a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化身者即丈六全身是　應身者為地上說。",
        "from": "T85n2784_001:0559a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應以佛身得度者。",
        "from": "T85n2784_001:0559a06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即現佛身而為說法。",
        "from": "T85n2784_001:0559a06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言報身者。",
        "from": "T85n2784_001:0559a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大圓鏡智為體。",
        "from": "T85n2784_001:0559a07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以分別智隱一真如性為境。",
        "from": "T85n2784_001:0559a08_4",
