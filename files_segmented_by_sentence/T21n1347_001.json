[
    {
        "source_sentence": "息除中夭陀羅尼經",
        "from": "T21n1347_001:0860a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天北印度烏填曩國帝釋",
        "from": "T21n1347_001:0860a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宮寺三藏傳法大師賜紫沙門",
        "from": "T21n1347_001:0860a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臣施護奉　詔譯如是我聞。",
        "from": "T21n1347_001:0860a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在殑伽河邊。",
        "from": "T21n1347_001:0860a08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大眾俱及於護世四大天王多聞天王最為上首。",
        "from": "T21n1347_001:0860a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告四王言。",
        "from": "T21n1347_001:0860a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大怖畏深可厭患。",
        "from": "T21n1347_001:0860a10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等應知若男若女童男童女若人若天。",
        "from": "T21n1347_001:0860a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我觀中夭生大怖畏。",
        "from": "T21n1347_001:0860a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒時擾惱各不相救。",
        "from": "T21n1347_001:0860a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至老死甚大怖畏。",
        "from": "T21n1347_001:0860a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而無窮盡。",
        "from": "T21n1347_001:0860a13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾今為汝說救護法。",
        "from": "T21n1347_001:0860a13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即時會中四大天王。",
        "from": "T21n1347_001:0860a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而白佛言世尊。",
        "from": "T21n1347_001:0860a14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等今日於世尊前。",
        "from": "T21n1347_001:0860a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞此說已踴躍歡喜。",
        "from": "T21n1347_001:0860a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以身命財奉於如來。",
        "from": "T21n1347_001:0860a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願佛哀愍為我演說。",
        "from": "T21n1347_001:0860a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊一彈指間。",
        "from": "T21n1347_001:0860a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面東而住說如是言南無十方。",
        "from": "T21n1347_001:0860a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來無上正等正覺。",
        "from": "T21n1347_001:0860a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣大慈悲憐愍有情。",
        "from": "T21n1347_001:0860a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼皆證得阿耨多羅三藐三菩提。",
        "from": "T21n1347_001:0860a19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願慈悲同共觀察。",
        "from": "T21n1347_001:0860a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著我有情與力加護。",
        "from": "T21n1347_001:0860a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "息除中夭敷演解說。",
        "from": "T21n1347_001:0860a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是東西南北乃至上下。",
        "from": "T21n1347_001:0860a21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來唱如是言。",
        "from": "T21n1347_001:0860a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無一切如來無上正等正覺。",
        "from": "T21n1347_001:0860a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不捨慈悲而為眾生。",
        "from": "T21n1347_001:0860a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證得阿耨多羅三藐三菩提。",
        "from": "T21n1347_001:0860a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願慈悲同共觀察著我有情敷演解說。",
        "from": "T21n1347_001:0860a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時諸佛同共證明。",
        "from": "T21n1347_001:0860a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今宣說救護怖畏息除難法。",
        "from": "T21n1347_001:0860a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是乃至第二法輪演說之時。",
        "from": "T21n1347_001:0860a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此會眾中皆獲壽命色相具足。",
        "from": "T21n1347_001:0860a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉得離於生死怖畏而無中夭。",
        "from": "T21n1347_001:0860a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至老病亦復如是。",
        "from": "T21n1347_001:0860a29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時十方一切諸佛。",
        "from": "T21n1347_001:0860b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照見一一世界如來形像。",
        "from": "T21n1347_001:0860b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如胡麻量遍滿一切。",
        "from": "T21n1347_001:0860b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又彼十方一一如來。",
        "from": "T21n1347_001:0860b02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同共發聲乃說呪曰。",
        "from": "T21n1347_001:0860b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也二合他去引一左禮左攞引左禮二尾𩕳跓傑固反娑嚩二合悉底二合計三作訖㘓二合引囉引爾曩四鉢囉二合捨滿覩五薩嚩枲誐引入六阿曩矩曩注准上切七左㘑左㘑八計麼𩕳設焰二合九儞麼儞設也二合十儞呬麼詩悉十一嬌居夭反引囉尾曳二合十二呬矩囉㘑十三矩麼底十四尾灑麼引抳麼抳十五尸數醯嚩十六阿左禮尾左禮十七麼引尾覽嚩十八母護母轉舌護十九娑嚩二合引賀二十爾時十方一切如來。",
        "from": "T21n1347_001:0860b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此祕密神呪已作如是言。",
        "from": "T21n1347_001:0860b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來無上正等正覺皆同此說。",
        "from": "T21n1347_001:0860b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時北方多聞天王而白佛言世尊。",
        "from": "T21n1347_001:0860b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今聞彼十方如來威神之力加持擁護。",
        "from": "T21n1347_001:0860b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而無中夭息除怖畏。",
        "from": "T21n1347_001:0860b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面於佛前而說呪曰。",
        "from": "T21n1347_001:0860b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也二合一他去引濕吠二合去帝二濕吠二合去帝三里里里里四爾時東方持國天王。",
        "from": "T21n1347_001:0860b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於佛前乃說呪曰。",
        "from": "T21n1347_001:0860b20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拶哩拶哩娑嚩二合引賀引爾時南方增長天王。",
        "from": "T21n1347_001:0860b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在於佛前即說呪曰。",
        "from": "T21n1347_001:0860b22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚩去引梨嚩嚩嚩嚩爾時西方廣目天王。",
        "from": "T21n1347_001:0860b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "侍立佛前亦說呪曰。",
        "from": "T21n1347_001:0860b24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "麼去引鄧擬儞蘇麼引蘇母蘇母爾時世尊告四天王言。",
        "from": "T21n1347_001:0860b26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此神呪是一切佛證明觀察。",
        "from": "T21n1347_001:0860b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有族姓男女等輩。",
        "from": "T21n1347_001:0860b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晝夜受持志誠憶念。",
        "from": "T21n1347_001:0860b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必獲長命而無中夭。",
        "from": "T21n1347_001:0860b28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命終之後不墮惡趣。",
        "from": "T21n1347_001:0860c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人利益有情。",
        "from": "T21n1347_001:0860c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發誠諦心受持讀誦。",
        "from": "T21n1347_001:0860c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨命終時無諸怖畏。",
        "from": "T21n1347_001:0860c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦無變怪及諸惡相。",
        "from": "T21n1347_001:0860c02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於現在世又無白癩顛狂諸病。",
        "from": "T21n1347_001:0860c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及離水火刀兵疾疫蟲藥等毒。",
        "from": "T21n1347_001:0860c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至雷雹一切諸難皆悉遠離。",
        "from": "T21n1347_001:0860c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若是經典所在之處。",
        "from": "T21n1347_001:0860c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方諸佛及眾菩薩悉皆來集。",
        "from": "T21n1347_001:0860c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擁護是經令無障難。",
        "from": "T21n1347_001:0860c06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人於此經典。",
        "from": "T21n1347_001:0860c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書寫莊嚴護淨轉讀。",
        "from": "T21n1347_001:0860c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如造佛像種種嚴飾。",
        "from": "T21n1347_001:0860c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所獲功德而無有異。",
        "from": "T21n1347_001:0860c08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人供養諸佛精心離慢。",
        "from": "T21n1347_001:0860c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲吉祥福息除災難。",
        "from": "T21n1347_001:0860c09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有受持書寫此經恒離懈怠。",
        "from": "T21n1347_001:0860c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所獲功德亦復如是。",
        "from": "T21n1347_001:0860c10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切聖賢常恒擁護離諸災患。",
        "from": "T21n1347_001:0860c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊說是經已。",
        "from": "T21n1347_001:0860c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四大天王龍神八部阿蘇囉等。",
        "from": "T21n1347_001:0860c12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸人天一切大眾。",
        "from": "T21n1347_001:0860c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞法歡喜信受奉行。",
        "from": "T21n1347_001:0860c13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "息除中夭陀羅尼經",
        "from": "T21n1347_001:0860c14_100",
        "context_with_next_sentence": false
    }
]