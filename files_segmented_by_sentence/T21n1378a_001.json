[
    {
        "source_sentence": "佛說玄師颰陀所說神呪經",
        "from": "T21n1378a_001:0901c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東晉天竺三藏竺曇無蘭譯",
        "from": "T21n1378a_001:0901c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞如是。一時佛遊於羅閱祇國中鸚鵡樹間。",
        "from": "T21n1378a_001:0901c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時有一異比丘。",
        "from": "T21n1378a_001:0901c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於竹園去羅閱祇國。",
        "from": "T21n1378a_001:0901c07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "適在中間為蛇所齧。",
        "from": "T21n1378a_001:0901c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復為鬼神所著。",
        "from": "T21n1378a_001:0901c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復為賊所劫。",
        "from": "T21n1378a_001:0901c08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見佛爾時即往到是比丘所。",
        "from": "T21n1378a_001:0901c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時玄師颰陀隨佛俱往。",
        "from": "T21n1378a_001:0901c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玄師颰陀即白佛言。",
        "from": "T21n1378a_001:0901c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我有術甚神今欲為呪。",
        "from": "T21n1378a_001:0901c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言止止颰陀。",
        "from": "T21n1378a_001:0901c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝所呪莫令有傷害。",
        "from": "T21n1378a_001:0901c11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "颰陀白佛言。",
        "from": "T21n1378a_001:0901c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後當國國相攻伐。",
        "from": "T21n1378a_001:0901c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賊賊更相劫。",
        "from": "T21n1378a_001:0901c12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相增者復為相劫。",
        "from": "T21n1378a_001:0901c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼神更劫。",
        "from": "T21n1378a_001:0901c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒毒更相害。",
        "from": "T21n1378a_001:0901c13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有比丘在山中樹下坐。",
        "from": "T21n1378a_001:0901c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被五納衣若露地坐。",
        "from": "T21n1378a_001:0901c14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四輩弟子當令無有害我者。",
        "from": "T21n1378a_001:0901c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆令安隱無有病痛如是。",
        "from": "T21n1378a_001:0901c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧深味尼欝遮梨首利拔提　珊于兮披拔陀泥如是呪。",
        "from": "T21n1378a_001:0901c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷口　語斷諸迷斷諸病痛。",
        "from": "T21n1378a_001:0901c19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸閱叉皆作是迷　須摩菟　摩呵一曜沙難　摩訶二閱叉梵　摩曜摩訶三閱叉　因呵羅　其摩呵四閱叉呵祇槃摩呵五。",
        "from": "T21n1378a_001:0901c22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸閱叉在鬼神中尊。",
        "from": "T21n1378a_001:0901c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我字因偷諸欲病者害人者。",
        "from": "T21n1378a_001:0901c23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我皆使不知人處。",
        "from": "T21n1378a_001:0901c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "障蔽汝眼令不見一切人。",
        "from": "T21n1378a_001:0901c24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆使不得其便。",
        "from": "T21n1378a_001:0901c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當爾之時便呼言。",
        "from": "T21n1378a_001:0901c25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來。",
        "from": "T21n1378a_001:0901c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "邏持鬼神摩呵　邏持鬼神來。",
        "from": "T21n1378a_001:0901c26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兜勒　摩訶兜勒　因持兜羅　來梵　摩訶兜羅　摩訶梵摩　兜羅　來閱叉兜羅　訶多　得　兜勒　首羅俱沙　兜勒　摩訶首羅沙兜勒我字因偷諸怨家　相求便者我皆救之覆蔽兩不相見。",
        "from": "T21n1378a_001:0902a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為繩索所縛。",
        "from": "T21n1378a_001:0902a02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鎖械所縛。",
        "from": "T21n1378a_001:0902a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亂意所縛。",
        "from": "T21n1378a_001:0902a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱歸命佛一切皆解脫。",
        "from": "T21n1378a_001:0902a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命佛　時南無佛大吉。",
        "from": "T21n1378a_001:0902a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說玄師颰陀所說神呪經",
        "from": "T21n1378a_001:0902a05_100",
        "context_with_next_sentence": false
    }
]