[
    {
        "source_sentence": "勝幢臂印陀羅尼經",
        "from": "T21n1363_001:0882c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐三藏法師玄奘奉　詔譯",
        "from": "T21n1363_001:0882c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。一時薄伽梵。在雞羅山頂天仙神",
        "from": "T21n1363_001:0882c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宮。",
        "from": "T21n1363_001:0882c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大苾芻眾千二百五十人俱。",
        "from": "T21n1363_001:0882c10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及無量無數菩薩摩訶薩。",
        "from": "T21n1363_001:0882c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曼殊室利跋陀波羅。",
        "from": "T21n1363_001:0882c11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十六大士而為上首。",
        "from": "T21n1363_001:0882c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并諸天人阿素洛等。",
        "from": "T21n1363_001:0882c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大梵天王而為上首。",
        "from": "T21n1363_001:0882c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量大眾前後圍繞。",
        "from": "T21n1363_001:0882c13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊住正念智。",
        "from": "T21n1363_001:0882c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸大眾說微妙法。",
        "from": "T21n1363_001:0882c14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時大梵王率諸天眾。",
        "from": "T21n1363_001:0882c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿素洛等頂禮佛足。",
        "from": "T21n1363_001:0882c15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌恭敬住如來前。",
        "from": "T21n1363_001:0882c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱白佛言世尊在昔。",
        "from": "T21n1363_001:0882c16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲利樂諸有情故。",
        "from": "T21n1363_001:0882c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求證無上正等菩提。",
        "from": "T21n1363_001:0882c17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悲願熏心成等正覺。",
        "from": "T21n1363_001:0882c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大神力具大慈悲。",
        "from": "T21n1363_001:0882c18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何故現見諸有情類。",
        "from": "T21n1363_001:0882c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "墮在地獄餓鬼傍生。",
        "from": "T21n1363_001:0882c19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及人天中受諸劇苦。",
        "from": "T21n1363_001:0882c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不設善巧方便濟拔。",
        "from": "T21n1363_001:0882c20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願哀愍令脫苦難。",
        "from": "T21n1363_001:0882c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時觀自在亦率無量持呪天仙。",
        "from": "T21n1363_001:0882c21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂禮佛足合掌恭敬。",
        "from": "T21n1363_001:0882c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住如來前俱白佛言。",
        "from": "T21n1363_001:0882c22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願哀愍諸有情故。",
        "from": "T21n1363_001:0882c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開示善巧拔苦難法。",
        "from": "T21n1363_001:0882c23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告二大士及其眷屬。",
        "from": "T21n1363_001:0882c24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等就座。",
        "from": "T21n1363_001:0882c24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾今愍念一切有情。",
        "from": "T21n1363_001:0882c25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略說善巧拔苦難法。",
        "from": "T21n1363_001:0882c25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝應諦聽極善思惟有大陀羅尼名勝幢臂印。",
        "from": "T21n1363_001:0882c26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若常誦念能滅五逆十惡等罪。",
        "from": "T21n1363_001:0882c27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終不更受諸惡趣生。",
        "from": "T21n1363_001:0882c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及人天中所有極苦。",
        "from": "T21n1363_001:0882c28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒受持者現得財位。",
        "from": "T21n1363_001:0882c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於當來世生尊貴家。",
        "from": "T21n1363_001:0882c29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所欲現前受諸快樂。",
        "from": "T21n1363_001:0882c29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常值諸佛得宿命念。",
        "from": "T21n1363_001:0883a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至無上正等菩提。",
        "from": "T21n1363_001:0883a01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陀羅尼曰。",
        "from": "T21n1363_001:0883a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羯洛羯洛　抧利抧利　矩路矩路　薩洛薩洛　徙利徙利　速路速路　薩縛佛陀婆　盧抧諦　達洛達洛　薩縛達摩路迦目契　折洛折洛　薩縛僧伽地瑟恥諦　跋洛跋洛　頞泥迦佛　陀俱胝婆史諦　廁拏廁拏　薩縛羯摩筏剌拏禰　莫摩薩縛薩埵難遮　薩婆波耶尼　薩縛獨佉波捺耶　勃栗吒勃栗吒　達栗吒達栗吒　捺吒捺吒　羯吒羯吒　颯怖吒耶颯怖吒耶　薩婆筏剌拏波耶突揭底尼　莎訶善男子此勝幢臂印陀羅尼。",
        "from": "T21n1363_001:0883a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是殑伽沙等諸佛共說。",
        "from": "T21n1363_001:0883a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾今愍念一切有情。",
        "from": "T21n1363_001:0883a14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因汝等請亦為眾說。",
        "from": "T21n1363_001:0883a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子諸佛出世甚為難遇。",
        "from": "T21n1363_001:0883a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善得人身復難於是。",
        "from": "T21n1363_001:0883a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞此神呪更復為難。",
        "from": "T21n1363_001:0883a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人。",
        "from": "T21n1363_001:0883a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能正受持尚為難得。",
        "from": "T21n1363_001:0883a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況書寫精勤誦念。",
        "from": "T21n1363_001:0883a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及勸他人令受持者。",
        "from": "T21n1363_001:0883a18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子我念過去。",
        "from": "T21n1363_001:0883a18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曾於藥師琉璃光勝觀等諸佛所。",
        "from": "T21n1363_001:0883a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞此神呪受持讀誦正為他說。",
        "from": "T21n1363_001:0883a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此證得無上菩提。",
        "from": "T21n1363_001:0883a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故汝等應隨勤學。",
        "from": "T21n1363_001:0883a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勸諸有情受持讀誦。",
        "from": "T21n1363_001:0883a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令脫苦難獲勝利樂。",
        "from": "T21n1363_001:0883a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時薄伽梵說此經已。",
        "from": "T21n1363_001:0883a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曼殊室利菩薩跋陀波羅菩薩觀自在菩薩。",
        "from": "T21n1363_001:0883a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大梵天王及諸天人阿素洛等。",
        "from": "T21n1363_001:0883a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切大眾聞佛所說。",
        "from": "T21n1363_001:0883a25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆大歡喜信受奉行。",
        "from": "T21n1363_001:0883a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勝幢臂印陀羅尼經",
        "from": "T21n1363_001:0883a27_100",
        "context_with_next_sentence": false
    }
]