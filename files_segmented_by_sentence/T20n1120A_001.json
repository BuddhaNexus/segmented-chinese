[
    {
        "source_sentence": "金剛頂勝初瑜伽經中略出大樂",
        "from": "T20n1120A_001:0513c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛薩埵念誦儀",
        "from": "T20n1120A_001:0513c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T20n1120A_001:0513c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稽首禮大密　　從持金剛生",
        "from": "T20n1120A_001:0513c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開演微妙儀　　簡要契真實",
        "from": "T20n1120A_001:0513c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瑜伽修習者　　願興利樂心",
        "from": "T20n1120A_001:0513c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方盡無餘　　一切眾生界",
        "from": "T20n1120A_001:0513c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言性成就　　隨意稱誦之",
        "from": "T20n1120A_001:0513c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由纔發此心　　滅除一切障",
        "from": "T20n1120A_001:0513c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲安樂悅意　　超彼眾魔羅",
        "from": "T20n1120A_001:0513c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即同諸如來　　世天皆供養",
        "from": "T20n1120A_001:0513c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知法無自性　　達勝義菩提",
        "from": "T20n1120A_001:0513c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思惟己體中　　普賢心若月",
        "from": "T20n1120A_001:0513c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身相歸空寂　　湧現跋折囉",
        "from": "T20n1120A_001:0513c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五股潔白輝　　變同薩埵貌",
        "from": "T20n1120A_001:0513c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "質素蓮臺爾　　五佛用嚴冠",
        "from": "T20n1120A_001:0513c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朱衣背赤光　　安住圓明內",
        "from": "T20n1120A_001:0513c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言及自印　　其次建立之",
        "from": "T20n1120A_001:0513c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仁者應半跏　　左肢右足押",
        "from": "T20n1120A_001:0513c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智拳輪擲已　　用按於胸間",
        "from": "T20n1120A_001:0514a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "𤙖為心真言　　定拳慢居𦜮",
        "from": "T20n1120A_001:0514a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛身語意　　支分勿傾斜",
        "from": "T20n1120A_001:0514a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本尊大印成　　次結金剛掌",
        "from": "T20n1120A_001:0514a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪合交初分　　密言如是稱",
        "from": "T20n1120A_001:0514a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄙖嚩􈁺日囒引􄒶若􄙌里便為金剛縛。",
        "from": "T20n1120A_001:0514a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其明如後陳。",
        "from": "T20n1120A_001:0514a08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄙖嚩􄦽日囉二合􄙖滿􄽙馱開縛摧拍心。",
        "from": "T20n1120A_001:0514a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應誦密言曰。",
        "from": "T20n1120A_001:0514a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次前金剛縛　　禪智檀慧間",
        "from": "T20n1120A_001:0514a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱遍入真言　　降臨每加護",
        "from": "T20n1120A_001:0514a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不改次前契　　禪智進力加",
        "from": "T20n1120A_001:0514a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名三昧拳　　真言如是誦",
        "from": "T20n1120A_001:0514a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分彼拳作二　　左慢右安心",
        "from": "T20n1120A_001:0514a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身語意金剛　　形體依初觀",
        "from": "T20n1120A_001:0514a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同前縛為准　　智與進如門",
        "from": "T20n1120A_001:0514a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪逼於其中　　祕密三昧契",
        "from": "T20n1120A_001:0514a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印心額喉頂　　皆誦此真言",
        "from": "T20n1120A_001:0514a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛縛又陳　　忍願成刀狀",
        "from": "T20n1120A_001:0514a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力捻刃側　　依初第一文",
        "from": "T20n1120A_001:0514b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪押於智端　　如結加趺勢",
        "from": "T20n1120A_001:0514b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印頂心當想　　毘盧遮那佛",
        "from": "T20n1120A_001:0514b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儼然潔白輝　　誦此真言曰",
        "from": "T20n1120A_001:0514b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當印其額　　應想阿閦鼙",
        "from": "T20n1120A_001:0514b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色青處於前　　稱此祕明句",
        "from": "T20n1120A_001:0514b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次按於頂右　　其名寶生尊",
        "from": "T20n1120A_001:0514b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黃色相端嚴　　真言如是誦",
        "from": "T20n1120A_001:0514b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉置在頂後　　無量壽如來",
        "from": "T20n1120A_001:0514b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色赤殊特儀　　稱此真言曰",
        "from": "T20n1120A_001:0514b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復至於頂左　　不空成就尊",
        "from": "T20n1120A_001:0514b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "綠色五佛周　　誦此祕明句",
        "from": "T20n1120A_001:0514b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次作寶鬘印　　灌頂以嚴身",
        "from": "T20n1120A_001:0514b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用二金剛手　　額前遂縈繞",
        "from": "T20n1120A_001:0514b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復分拳腦後　　如前又繫鬘",
        "from": "T20n1120A_001:0514b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自檀慧徐開　　以羽兩傍下",
        "from": "T20n1120A_001:0514b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若垂繒帶想　　誦次復真言",
        "from": "T20n1120A_001:0514b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定慧羽皆舒　　俱拍契成就",
        "from": "T20n1120A_001:0514c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能悅一切聖　　真言如是稱",
        "from": "T20n1120A_001:0514c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛慢印明　　默心誦一遍",
        "from": "T20n1120A_001:0514c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復具明如上　　金剛薩埵冠",
        "from": "T20n1120A_001:0514c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以五佛色身　　安住相應印",
        "from": "T20n1120A_001:0514c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍照薄伽梵　　契住如來拳",
        "from": "T20n1120A_001:0514c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次陳阿閦鼙　　定羽持衣角",
        "from": "T20n1120A_001:0514c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成拳按心上　　慧觸地如儀",
        "from": "T20n1120A_001:0514c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施願寶生尊　　智掌仰當乳",
        "from": "T20n1120A_001:0514c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量光勝印　　定拳幔執蓮",
        "from": "T20n1120A_001:0514c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧拳似敷華　　又如無動佛",
        "from": "T20n1120A_001:0514c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智羽三幡相　　如拔濟有情",
        "from": "T20n1120A_001:0514c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "揚掌於乳傍　　不空成就印",
        "from": "T20n1120A_001:0514c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又作金剛縛　　開掌禪智合",
        "from": "T20n1120A_001:0514c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "檀慧直如峯　　忍願入於掌",
        "from": "T20n1120A_001:0514c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相合如箭狀　　印心額及喉",
        "from": "T20n1120A_001:0514c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於頂亦如之　　皆用心密語",
        "from": "T20n1120A_001:0514c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大樂隨心印　　用結護己身",
        "from": "T20n1120A_001:0514c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前印箭㓨心　　誦後真言句",
        "from": "T20n1120A_001:0514c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸尊當適悅　　警覺三昧耶",
        "from": "T20n1120A_001:0515a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦至弱字明　　忍願如鉤狀",
        "from": "T20n1120A_001:0515a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開鉤已心畢　　本尊降道場",
        "from": "T20n1120A_001:0515a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾聖亦同臻　　依位儼而住",
        "from": "T20n1120A_001:0515b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忍願便相拄　　如環索契成",
        "from": "T20n1120A_001:0515b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "𤙖字羂入之　　二度極相繞",
        "from": "T20n1120A_001:0515b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鎖成止諸聖　　懵字是其明",
        "from": "T20n1120A_001:0515b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上下徐動搖　　磐成稱斛句",
        "from": "T20n1120A_001:0515b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念其音清暢　　能悅眾聖心",
        "from": "T20n1120A_001:0515b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "閼伽契乃陳　　堅住金剛掌",
        "from": "T20n1120A_001:0515b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忍願遂相合　　進力若鉤形",
        "from": "T20n1120A_001:0515b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪智捻其根　　側印稱唵句",
        "from": "T20n1120A_001:0515b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便捧其香水　　稱後閼伽明",
        "from": "T20n1120A_001:0515b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "近額奉獻之　　如儀浴眾聖",
        "from": "T20n1120A_001:0515b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依前觀滿月　　皓白現壇中",
        "from": "T20n1120A_001:0515b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大聖處於間　　契住金剛慢",
        "from": "T20n1120A_001:0515b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復陳四尊位　　眼箭在其前",
        "from": "T20n1120A_001:0515b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色赤衣服然　　冠鬘以嚴飾",
        "from": "T20n1120A_001:0515b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二羽彎弓矢　　瞻矚薩埵儀",
        "from": "T20n1120A_001:0515c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "計里計羅尊　　色白居其右",
        "from": "T20n1120A_001:0515c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛拳二羽　　交抱三昧耶",
        "from": "T20n1120A_001:0515c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於後愛為名　　形服皆青色",
        "from": "T20n1120A_001:0515c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二拳竪慧臂　　肘以定羽承",
        "from": "T20n1120A_001:0515c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共執摩竭幢　　於左名為慢",
        "from": "T20n1120A_001:0515c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色黃拳在胯　　向左小低頭",
        "from": "T20n1120A_001:0515c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羯磨印真言　　加持諦安立",
        "from": "T20n1120A_001:0515c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大聖金剛慢　　應誦心密言",
        "from": "T20n1120A_001:0515c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸尊次復陳　　自眼箭為始",
        "from": "T20n1120A_001:0515c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二拳各堅固　　彎弓放箭儀",
        "from": "T20n1120A_001:0515c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密契相已成　　祕明如後誦",
        "from": "T20n1120A_001:0515c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前拳二羽　　交臂抱於胸",
        "from": "T20n1120A_001:0515c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "計里計羅尊　　當誦此後句",
        "from": "T20n1120A_001:0515c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又改次前印　　慧臂直如幢",
        "from": "T20n1120A_001:0515c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定拳承肘間　　誦明名愛契",
        "from": "T20n1120A_001:0515c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二拳各居胯　　以頭向左傾",
        "from": "T20n1120A_001:0515c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慢契相遂成　　真言如後誦",
        "from": "T20n1120A_001:0515c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次陳內供養　　初起東南隅",
        "from": "T20n1120A_001:0516a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捧華形服白　　金剛妙適悅",
        "from": "T20n1120A_001:0516a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛適悅性　　色黑執香鑪",
        "from": "T20n1120A_001:0516a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛眼獻燈　　色赤嚴飾爾",
        "from": "T20n1120A_001:0516a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "末為塗香位　　金剛大吉祥",
        "from": "T20n1120A_001:0516a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形質皆以黃　　次陳其契相",
        "from": "T20n1120A_001:0516a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱先二拳舞　　如儀遂結成",
        "from": "T20n1120A_001:0516a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二金剛拳　　相並上擲散",
        "from": "T20n1120A_001:0516a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想妙華供養　　真言如後稱",
        "from": "T20n1120A_001:0516a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普拳乃下擲　　念焚香雲海",
        "from": "T20n1120A_001:0516a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普遍於一切　　誦如後真言",
        "from": "T20n1120A_001:0516a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二拳禪智合　　如燈應運想",
        "from": "T20n1120A_001:0516a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣施為佛事　　密言如是稱",
        "from": "T20n1120A_001:0516a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並覆其二拳　　依胸兩向散",
        "from": "T20n1120A_001:0516a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若妙塗香勢　　當誦此真言",
        "from": "T20n1120A_001:0516a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外供養諸尊　　四隅又存想",
        "from": "T20n1120A_001:0516a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東南名喜戲　　二拳以當心",
        "from": "T20n1120A_001:0516a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "笑處於西南　　二羽口傍散",
        "from": "T20n1120A_001:0516a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歌居於西北　　彈執其箜篌",
        "from": "T20n1120A_001:0516b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東北舞為名　　如儀旋轉勢",
        "from": "T20n1120A_001:0516b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形服皆金色　　真言契又陳",
        "from": "T20n1120A_001:0516b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覆並於二拳　　繞心應右轉",
        "from": "T20n1120A_001:0516b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名喜戲印　　其明如後稱",
        "from": "T20n1120A_001:0516b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前印口傍　　自檀慧徐散",
        "from": "T20n1120A_001:0516b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "揚掌極舒臂　　含笑誦真言",
        "from": "T20n1120A_001:0516b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定臂如箜篌　　改進力微屈",
        "from": "T20n1120A_001:0516b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧羽彈絃勢　　為歌誦其明",
        "from": "T20n1120A_001:0516b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二拳舞於心　　頂上合便散",
        "from": "T20n1120A_001:0516b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其名舞契相　　真言句遂陳",
        "from": "T20n1120A_001:0516b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四承旨居門　　形儀復當演",
        "from": "T20n1120A_001:0516b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初持鉤青色　　嚴麗處於間",
        "from": "T20n1120A_001:0516b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南携索皆黃　　西執鎖尚赤",
        "from": "T20n1120A_001:0516b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其北名為磬　　具綠色冠鬘",
        "from": "T20n1120A_001:0516b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祕契及真言　　復次今當設",
        "from": "T20n1120A_001:0516b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二拳背應逼　　檀慧反相鉤",
        "from": "T20n1120A_001:0516b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力皆極舒　　又稍屈進度",
        "from": "T20n1120A_001:0516c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微招是鉤契　　其明如後稱",
        "from": "T20n1120A_001:0516c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前印進力交　　反以頭相拄",
        "from": "T20n1120A_001:0516c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其中如環索　　稱誦後真言",
        "from": "T20n1120A_001:0516c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "改進力相鉤　　開拳背交辟",
        "from": "T20n1120A_001:0516c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂名鎖契成　　密言如是稱",
        "from": "T20n1120A_001:0516c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如鎖背相著　　動搖磬明曰",
        "from": "T20n1120A_001:0516c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大聖所嚴飾　　華座及衣服",
        "from": "T20n1120A_001:0516c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并餘見前尊　　其色隨身相",
        "from": "T20n1120A_001:0516c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殊形具眾德　　首戴五佛冠",
        "from": "T20n1120A_001:0516c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將建漫茶羅　　諸位先存想",
        "from": "T20n1120A_001:0516c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月輪圓明現　　其中觀念之",
        "from": "T20n1120A_001:0516c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "半加面本尊　　適悅目瞻仰",
        "from": "T20n1120A_001:0516c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又宣三昧印　　本尊前所陳",
        "from": "T20n1120A_001:0516c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼契及真言　　如儀勿差謬",
        "from": "T20n1120A_001:0516c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘尊次當設　　亦眼箭為初",
        "from": "T20n1120A_001:0516c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前所護身　　大樂隨心印",
        "from": "T20n1120A_001:0516c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "極屈其進力　　初分背相著",
        "from": "T20n1120A_001:0516c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪智並押之　　契相當成就",
        "from": "T20n1120A_001:0516c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以禪押於智　　深交印乃成",
        "from": "T20n1120A_001:0516c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名計里計羅　　次陳摩竭相",
        "from": "T20n1120A_001:0516c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進鉤於願度　　力掘其智端",
        "from": "T20n1120A_001:0517a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戒方舒成針　　檀慧合而直",
        "from": "T20n1120A_001:0517a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪智自相並　　各押進力傍",
        "from": "T20n1120A_001:0517a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛契相已成　　不改次前印",
        "from": "T20n1120A_001:0517a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從外觸其股　　先右左亦然",
        "from": "T20n1120A_001:0517a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內供養又明　　側綟金剛掌",
        "from": "T20n1120A_001:0517a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上擲為華印　　下散成燒香",
        "from": "T20n1120A_001:0517a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪智逼為燈　　塗香依胸啟",
        "from": "T20n1120A_001:0517a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喜戲旋轉禮　　笑契近口傍",
        "from": "T20n1120A_001:0517a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自檀慧徐開　　又結其歌印",
        "from": "T20n1120A_001:0517a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "改力度微屈　　進虛撥於間",
        "from": "T20n1120A_001:0517a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定羽佉吒迦　　慧作三幡相",
        "from": "T20n1120A_001:0517a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當心乃旋舞　　八供養已周",
        "from": "T20n1120A_001:0517a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復結金剛縛　　改進度微屈",
        "from": "T20n1120A_001:0517a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徐招是鉤契　　從縛索當生",
        "from": "T20n1120A_001:0517a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪度智力間　　印成又為鎖",
        "from": "T20n1120A_001:0517a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "環進禪智力　　捻已便相鉤",
        "from": "T20n1120A_001:0517a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又作堅固縛　　禪智戒方下",
        "from": "T20n1120A_001:0517a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當胸遂搖動　　為磬四攝成",
        "from": "T20n1120A_001:0517a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三昧耶真言　　同前羯磨句",
        "from": "T20n1120A_001:0517a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛薩埵位　　及行者所居",
        "from": "T20n1120A_001:0517a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆有十六尊　　圍遶端嚴住",
        "from": "T20n1120A_001:0517a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讚王如後述　　應當稱誦之",
        "from": "T20n1120A_001:0517a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此金剛歌　　大樂隨愛樂",
        "from": "T20n1120A_001:0517b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "適悅皆堅固　　當得如意成",
        "from": "T20n1120A_001:0517b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又誦次所陳　　最勝真實讚",
        "from": "T20n1120A_001:0517b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能纔稱念故　　速令悉地圓",
        "from": "T20n1120A_001:0517b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次陳眼印相　　自大聖為先",
        "from": "T20n1120A_001:0517b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘各異觀瞻　　契名亦分別",
        "from": "T20n1120A_001:0517b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行人復應以　　金剛眼視尊",
        "from": "T20n1120A_001:0517b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大適悅金剛　　不空警悟印",
        "from": "T20n1120A_001:0517b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當觀薩埵際　　兼興大染心",
        "from": "T20n1120A_001:0517b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此印加持　　當速疾成就",
        "from": "T20n1120A_001:0517b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧作三幡相　　定羽佉吒迦",
        "from": "T20n1120A_001:0517b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前當自心　　舞畢擲華勢",
        "from": "T20n1120A_001:0517b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目半開笑面　　瞻仰於本尊",
        "from": "T20n1120A_001:0517b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大適意金剛　　不空箭印相",
        "from": "T20n1120A_001:0517b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法界體性智　　由此能住持",
        "from": "T20n1120A_001:0517b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大適悅目觀　　本尊聖者貌",
        "from": "T20n1120A_001:0517b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大適悅視印　　少福者亦成",
        "from": "T20n1120A_001:0517c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速願盻為容　　加持大成就",
        "from": "T20n1120A_001:0517c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復當令現證　　超勝外道流",
        "from": "T20n1120A_001:0517c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大適悅金剛　　幢幡莊嚴印",
        "from": "T20n1120A_001:0517c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "揚眸徐顧眄　　定作佉吒迦",
        "from": "T20n1120A_001:0517c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧當結三幡　　如前旋舞相",
        "from": "T20n1120A_001:0517c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦作擲華勢　　其名禮印成",
        "from": "T20n1120A_001:0517c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三界諸人天　　恭敬當供養",
        "from": "T20n1120A_001:0517c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次前五種視　　本尊𤙖字明",
        "from": "T20n1120A_001:0517c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金願慢印并　　餘皆羯磨契",
        "from": "T20n1120A_001:0517c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各誦真言句　　如前無復殊",
        "from": "T20n1120A_001:0517c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大聖所成身　　法界體性智",
        "from": "T20n1120A_001:0517c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前貪染顧　　圓寂令覩之",
        "from": "T20n1120A_001:0517c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以眼少開　　其睫當瞤動",
        "from": "T20n1120A_001:0517c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼名曰明視　　用此請本尊",
        "from": "T20n1120A_001:0517c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍入己體中　　身心愛染想",
        "from": "T20n1120A_001:0517c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉事於聖者　　印力契真如",
        "from": "T20n1120A_001:0517c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智自性皆達　　薩埵因之住",
        "from": "T20n1120A_001:0517c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今修此方便　　故我當證成",
        "from": "T20n1120A_001:0517c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依教供養周　　次用辟除印",
        "from": "T20n1120A_001:0517c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛熾盛日　　三昧耶為名",
        "from": "T20n1120A_001:0517c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二羽內相叉　　八度互著掌",
        "from": "T20n1120A_001:0517c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舒禪智面合　　如杵乃虛拳",
        "from": "T20n1120A_001:0517c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左轉能辟魔　　右旋遍結界",
        "from": "T20n1120A_001:0517c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吽聲若雷震　　隨意為提防",
        "from": "T20n1120A_001:0517c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自勝解思惟　　一切虛空界",
        "from": "T20n1120A_001:0517c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗香燈飲食　　華鬘及燒香",
        "from": "T20n1120A_001:0517c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣服蓋幢幡　　扇拂歌寶鐸",
        "from": "T20n1120A_001:0517c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙舞并宮殿　　殊勝供養儀",
        "from": "T20n1120A_001:0517c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今變化成　　眾聖皆受用",
        "from": "T20n1120A_001:0518a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前唵字句　　稱誦運想之",
        "from": "T20n1120A_001:0518a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不瞬微笑容　　堅固愛樂意",
        "from": "T20n1120A_001:0518a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "久觀聖者體　　金剛眼乃成",
        "from": "T20n1120A_001:0518a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本尊大印陳　　薩埵三摩地",
        "from": "T20n1120A_001:0518a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脣齒應俱合　　金剛語離聲",
        "from": "T20n1120A_001:0518a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不大動舌端　　能成諸教法",
        "from": "T20n1120A_001:0518a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方欲止息除　　復想供養儀",
        "from": "T20n1120A_001:0518a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并稱唵字明　　如前當奉獻",
        "from": "T20n1120A_001:0518a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一百八名讚　　次當稱誦之",
        "from": "T20n1120A_001:0518a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若持此讚王　　金剛法語誦",
        "from": "T20n1120A_001:0519b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所樂當成就　　速疾無與倫",
        "from": "T20n1120A_001:0519b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每日應及時　　稱已離諸罪",
        "from": "T20n1120A_001:0519b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常脫一切苦　　淨土當現前",
        "from": "T20n1120A_001:0519b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纔誦眾福圓　　增吉祥明盛",
        "from": "T20n1120A_001:0519b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復陳閼伽契　　側印於器端",
        "from": "T20n1120A_001:0519b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "首應亦隨傾　　想禮稱唵句",
        "from": "T20n1120A_001:0519b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捧獻如儀則　　誦其本真言",
        "from": "T20n1120A_001:0519b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發願畢結前　　金剛熾盛日",
        "from": "T20n1120A_001:0519b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左旋想解界　　又作堅固縛",
        "from": "T20n1120A_001:0519b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忍願竪成針　　當心誦明曰",
        "from": "T20n1120A_001:0519b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "句畢近額上　　自鋒乃徐開",
        "from": "T20n1120A_001:0519b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想眾聖還宮　　護身隨所適",
        "from": "T20n1120A_001:0519b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲求先行者　　四時處道場",
        "from": "T20n1120A_001:0519c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦滿一洛叉　　次晝夜持念",
        "from": "T20n1120A_001:0519c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸尊當覆護　　然加行精勤",
        "from": "T20n1120A_001:0519c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又演念誦門　　四威儀修習",
        "from": "T20n1120A_001:0519c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前願拔濟　　無餘界有情",
        "from": "T20n1120A_001:0519c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦誦初所陳　　菩提心密語",
        "from": "T20n1120A_001:0519c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又當稱婀字　　用妙適悅言",
        "from": "T20n1120A_001:0519c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了諸法不生　　如初住空觀",
        "from": "T20n1120A_001:0519c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓明五鋒杵　　及成就己身",
        "from": "T20n1120A_001:0519c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同薩埵真儀　　分明諦觀想",
        "from": "T20n1120A_001:0519c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "半跏右押左　　金剛掌遂合",
        "from": "T20n1120A_001:0519c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅固縛又陳　　開心遍入印",
        "from": "T20n1120A_001:0519c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三昧拳亦爾　　金剛慢印旋",
        "from": "T20n1120A_001:0520a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五佛印寶冠　　繫鬘垂帶想",
        "from": "T20n1120A_001:0520a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "參差當拍掌　　三昧契護持",
        "from": "T20n1120A_001:0520a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此印請尊　　降於己身內",
        "from": "T20n1120A_001:0520a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八供養雲集　　如前安立之",
        "from": "T20n1120A_001:0520a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆依於大空　　布位當觀念",
        "from": "T20n1120A_001:0520a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分明羅列已　　金剛慢契陳",
        "from": "T20n1120A_001:0520a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其餘供養尊　　皆用羯磨印",
        "from": "T20n1120A_001:0520a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今但標契號　　其相前所申",
        "from": "T20n1120A_001:0520a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆誦本真言　　并運心供養",
        "from": "T20n1120A_001:0520a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀身色空已　　是真覩本尊",
        "from": "T20n1120A_001:0520a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如斯勝解之　　決定無疑惑",
        "from": "T20n1120A_001:0520a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因緣有情類　　悟本皆成空",
        "from": "T20n1120A_001:0520a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復建同尊儀　　如是瑜伽住",
        "from": "T20n1120A_001:0520a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡遇莊嚴事　　悅意殊特流",
        "from": "T20n1120A_001:0520a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "泯彼皆觀空　　復次再安立",
        "from": "T20n1120A_001:0520a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆成清淨已　　想供養本尊",
        "from": "T20n1120A_001:0520a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并稱唵字明　　同普賢受用",
        "from": "T20n1120A_001:0520a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又陳先行法　　隨處而修習",
        "from": "T20n1120A_001:0520a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數滿一洛叉　　如前晝夜誦",
        "from": "T20n1120A_001:0520a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若專精一月　　兩月或半月",
        "from": "T20n1120A_001:0520a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "警相當現前　　隨意求成就",
        "from": "T20n1120A_001:0520a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樂登種佛位　　菩薩及本尊",
        "from": "T20n1120A_001:0520a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信已即彼身　　纔決定便證",
        "from": "T20n1120A_001:0520a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖障重懶惰　　此生速獲之",
        "from": "T20n1120A_001:0520a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大樂微妙門　　仁者應尊奉",
        "from": "T20n1120A_001:0520a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛頂勝初瑜伽經中略出大樂金剛薩埵念誦儀軌",
        "from": "T20n1120A_001:0520a28_100",
        "context_with_next_sentence": false
    }
]