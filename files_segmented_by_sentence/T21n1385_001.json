[
    {
        "source_sentence": "佛說俱枳羅陀羅尼經",
        "from": "T21n1385_001:0905b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試光祿卿",
        "from": "T21n1385_001:0905b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明教大師臣法賢奉　詔譯",
        "from": "T21n1385_001:0905b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T21n1385_001:0905b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在舍衛國祇樹給孤獨園。",
        "from": "T21n1385_001:0905b11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大比丘眾俱。",
        "from": "T21n1385_001:0905b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有天人阿修羅乾闥婆等俱來會坐。",
        "from": "T21n1385_001:0905b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛告尊者阿難言。",
        "from": "T21n1385_001:0905b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當諦聽我有陀羅尼名俱枳羅。",
        "from": "T21n1385_001:0905b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大威力能作吉祥。",
        "from": "T21n1385_001:0905b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能破部多及惡曜等之所執持。",
        "from": "T21n1385_001:0905b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及能除瘥一切疾病。",
        "from": "T21n1385_001:0905b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂食病寒熱病。",
        "from": "T21n1385_001:0905b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭痛半痛欬瘷喘氣。",
        "from": "T21n1385_001:0905b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至痔病白淋之病。",
        "from": "T21n1385_001:0905b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及種種瘡痍等。",
        "from": "T21n1385_001:0905b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難至於眾生命欲斷者亦能延續。",
        "from": "T21n1385_001:0905b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由是我今為汝宣說。",
        "from": "T21n1385_001:0905b19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難若復有人聞此陀羅尼。",
        "from": "T21n1385_001:0905b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發堅固心讀誦受持。",
        "from": "T21n1385_001:0905b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人所獲善利無量無邊。",
        "from": "T21n1385_001:0905b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊即說陀羅尼曰。",
        "from": "T21n1385_001:0905b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛𭔞切身下同他引一唵引壹里蜜里二唧里彌引三嚩囉酤引計隷引室哩二合末底四崑拏隷引五農訥毘六農訥毘七印捺囉二合嚩日囉二合引儗儞二合暮哩儞二合八薩哩嚩二合嚩日囉二合引儗儞二合暮哩儞二合九沒囉二合賀摩二合莎哩二合娑嚩二合引賀引十野他引杌嚕二合引沒馱十一薩埵引曩引尾囉引誐室哩二合引瑟吒二合十二薩哩嚩二合達哩摩二合引𧹞引十三誐拏鉢囉二合嚩囉十四商伽薩爹嚩引計曳二合引那十五遏儞引那薩爹嚩計曳二合引那十六摩摩薩哩嚩二合薩埵引曩引十七遏悉當十八閉當十九珂儞當二十阿引莎引儞多引儞二十一三藐酤半音窣契引那二十二波哩拏引莽誐蹉覩娑嚩二合引賀引二十三爾時世尊說此陀羅尼已。",
        "from": "T21n1385_001:0905c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告阿難言。",
        "from": "T21n1385_001:0905c07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是陀羅尼所在之處。",
        "from": "T21n1385_001:0905c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我不見有天上人間一切梵魔沙門婆羅門等敢有違越。",
        "from": "T21n1385_001:0905c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當受持為他人說。",
        "from": "T21n1385_001:0905c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俾使多人獲大利益。",
        "from": "T21n1385_001:0905c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難及與會中天人阿修羅乾闥婆等。",
        "from": "T21n1385_001:0905c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛所說歡喜信受作禮而退。",
        "from": "T21n1385_001:0905c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說俱枳羅陀羅尼經",
        "from": "T21n1385_001:0905c13_100",
        "context_with_next_sentence": false
    }
]