[
    {
        "source_sentence": "地藏菩薩儀軌",
        "from": "T20n1158_001:0652a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中天竺輸婆迦羅奉　詔譯",
        "from": "T20n1158_001:0652a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時釋迦牟尼佛。在𭛟羅提耶山。與無量諸",
        "from": "T20n1158_001:0652a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地藏菩薩十俱胝比丘比丘尼及天龍八部諸鬼神俱無量百千大眾圍繞而為說法。",
        "from": "T20n1158_001:0652a15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時大眾中有一大士名曰地藏。",
        "from": "T20n1158_001:0652a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偏袒右肩右膝著地。",
        "from": "T20n1158_001:0652a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌恭敬白佛言。",
        "from": "T20n1158_001:0652a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我說神呪欲利益一切眾生。",
        "from": "T20n1158_001:0652a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊欲說神呪。",
        "from": "T20n1158_001:0652a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈哀聽許。",
        "from": "T20n1158_001:0652a18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時佛告地藏菩薩言。",
        "from": "T20n1158_001:0652a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等速說利益一切有情。",
        "from": "T20n1158_001:0652a19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時地藏菩薩騰虛空讚歎歸命世尊。",
        "from": "T20n1158_001:0652a20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在空中亦現無量神通。",
        "from": "T20n1158_001:0652a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以神口說呪曰。",
        "from": "T20n1158_001:0652a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵闇摩他𭌩摩爾俱苾俱苾三曼多娑婆賀次說心呪。",
        "from": "T20n1158_001:0652a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵炎曼他啫娑婆賀次心中心呪。",
        "from": "T20n1158_001:0652a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵礘呬二合纔說此呪。",
        "from": "T20n1158_001:0652a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十地等大菩薩。",
        "from": "T20n1158_001:0652a25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "失本心迷惑。",
        "from": "T20n1158_001:0652a25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天龍八部藥叉鬼神驚走倒地。",
        "from": "T20n1158_001:0652a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日月星宿失光隱蔽空。",
        "from": "T20n1158_001:0652a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雨寶華繽紛。",
        "from": "T20n1158_001:0652a27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "窮劫說呪力不可說盡。",
        "from": "T20n1158_001:0652a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說畫像法。",
        "from": "T20n1158_001:0652a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作聲聞形像。",
        "from": "T20n1158_001:0652a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著袈裟端覆左肩。",
        "from": "T20n1158_001:0652a29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持盈華形。",
        "from": "T20n1158_001:0652b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手施無畏令坐蓮華。",
        "from": "T20n1158_001:0652b01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復居座大士像頂著天冠著袈裟。",
        "from": "T20n1158_001:0652b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持蓮華茶。",
        "from": "T20n1158_001:0652b02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手如先令安坐九品蓮臺傳云九品者九重也亦云八葉中臺也。",
        "from": "T20n1158_001:0652b03_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說印呪印二羽三補吒。",
        "from": "T20n1158_001:0652b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戒方內掌來去。",
        "from": "T20n1158_001:0652b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T20n1158_001:0652b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵炎摩智利娑婆賀次說普供養印。",
        "from": "T20n1158_001:0652b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二羽三補吒。",
        "from": "T20n1158_001:0652b06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪智立來去。",
        "from": "T20n1158_001:0652b06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵喃惹摩尼娑嚩賀次總說總印。",
        "from": "T20n1158_001:0652b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二羽三補吒。",
        "from": "T20n1158_001:0652b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力立來去。",
        "from": "T20n1158_001:0652b08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T20n1158_001:0652b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵𭌅只儞耶娑嚩二合賀次請讚印。",
        "from": "T20n1158_001:0652b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二羽三補吒。",
        "from": "T20n1158_001:0652b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力來去次撥遣以觀手禪進彈。",
        "from": "T20n1158_001:0652b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說成就法。",
        "from": "T20n1158_001:0652b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若念得大福德。",
        "from": "T20n1158_001:0652b12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持阿迦木護摩七萬遍。",
        "from": "T20n1158_001:0652b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若念得五穀成就。",
        "from": "T20n1158_001:0652b13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以稻實華護摩。",
        "from": "T20n1158_001:0652b13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若念他人福德。",
        "from": "T20n1158_001:0652b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取得其家竈土護摩。",
        "from": "T20n1158_001:0652b14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若念得高位二德。",
        "from": "T20n1158_001:0652b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以比哩華護摩。",
        "from": "T20n1158_001:0652b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若念生生世世怨敵伏。",
        "from": "T20n1158_001:0652b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以苦練木護摩。",
        "from": "T20n1158_001:0652b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若念惡靈邪家伏。",
        "from": "T20n1158_001:0652b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以毒味護摩。",
        "from": "T20n1158_001:0652b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若念顛狂痾病除愈。",
        "from": "T20n1158_001:0652b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮實草護摩。",
        "from": "T20n1158_001:0652b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三萬遍。",
        "from": "T20n1158_001:0652b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若念滅罪生善生身後生極樂以草護摩三萬遍。",
        "from": "T20n1158_001:0652b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若念惡人呪咀還著本人。",
        "from": "T20n1158_001:0652b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以苦草投火中護摩三萬遍。",
        "from": "T20n1158_001:0652b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若念得無量榮果俸祿。",
        "from": "T20n1158_001:0652b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白華鳥草護摩三萬遍。",
        "from": "T20n1158_001:0652b21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若念三昧辨財德者。",
        "from": "T20n1158_001:0652b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以骨婁草護摩三萬遍。",
        "from": "T20n1158_001:0652b22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若念化度一切眾生令離苦得樂。",
        "from": "T20n1158_001:0652b23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以白芥子護摩三萬遍。",
        "from": "T20n1158_001:0652b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若念化度無佛世界眾生。",
        "from": "T20n1158_001:0652b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以尸迦草護摩三七萬遍。",
        "from": "T20n1158_001:0652b25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若念枯田畠五穀生者。",
        "from": "T20n1158_001:0652b25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持古蔓菁。",
        "from": "T20n1158_001:0652b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持散。",
        "from": "T20n1158_001:0652b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若愁萬病者。",
        "from": "T20n1158_001:0652b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牛膝草護摩三萬遍。",
        "from": "T20n1158_001:0652b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若愁三陰四熱種種病。",
        "from": "T20n1158_001:0652b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以降乾各門草護摩。",
        "from": "T20n1158_001:0652b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若夫妻不和者。",
        "from": "T20n1158_001:0652b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比罪草護摩三萬遍和合。",
        "from": "T20n1158_001:0652b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若惡友善友相違者。",
        "from": "T20n1158_001:0652b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藤葉草護摩三萬遍。",
        "from": "T20n1158_001:0652c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若白月八日十四日。",
        "from": "T20n1158_001:0652c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十五日以上法修必得成就。",
        "from": "T20n1158_001:0652c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地藏菩薩儀軌",
        "from": "T20n1158_001:0652c03_100",
        "context_with_next_sentence": false
    }
]