[
    {
        "source_sentence": "供養護世八天法",
        "from": "T21n1295_001:0380a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "青龍寺沙門法全集",
        "from": "T21n1295_001:0380a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘇婆呼經云。修真言行者。日日供養護方天",
        "from": "T21n1295_001:0380a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神。",
        "from": "T21n1295_001:0380a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則無災難。",
        "from": "T21n1295_001:0380a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於道場外八方。",
        "from": "T21n1295_001:0380a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敷茅草或荷葉。",
        "from": "T21n1295_001:0380a12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或塗圓壇為十位。",
        "from": "T21n1295_001:0380a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於帝釋左右安梵天地天。",
        "from": "T21n1295_001:0380a13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若道場外無置位處。",
        "from": "T21n1295_001:0380a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於道場前閑淨處為方界。",
        "from": "T21n1295_001:0380a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中在八方。",
        "from": "T21n1295_001:0380a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中央安置梵天地天。",
        "from": "T21n1295_001:0380a15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以施十方天。",
        "from": "T21n1295_001:0380a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食應用雜粥。",
        "from": "T21n1295_001:0380a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂粳米油麻。",
        "from": "T21n1295_001:0380a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先置一淨葉令極清淨。",
        "from": "T21n1295_001:0380a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香美盛一器中。",
        "from": "T21n1295_001:0380a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每坐先置一淨葉或淨荼椀。",
        "from": "T21n1295_001:0380a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以淨瓶盛香水。",
        "from": "T21n1295_001:0380a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後䠒跪合掌。",
        "from": "T21n1295_001:0380a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "召請十方世天。",
        "from": "T21n1295_001:0380a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謹請東方憍尸迦天主與諸眷屬來降於此。",
        "from": "T21n1295_001:0380a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我所辦供願垂納受。",
        "from": "T21n1295_001:0380a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謹請東南方火天山等與諸眷屬來降於此。",
        "from": "T21n1295_001:0380a22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我所辦供願垂納受。",
        "from": "T21n1295_001:0380a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謹請南方琰摩天主。",
        "from": "T21n1295_001:0380a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅剎主。",
        "from": "T21n1295_001:0380a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天部多天王。",
        "from": "T21n1295_001:0380a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水天龍王。",
        "from": "T21n1295_001:0380a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風神王等。",
        "from": "T21n1295_001:0380a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多聞天王。",
        "from": "T21n1295_001:0380a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伊舍那天王。",
        "from": "T21n1295_001:0380a25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大梵天王。",
        "from": "T21n1295_001:0380a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謹請地居所有諸大神王與諸眷屬來降於此。",
        "from": "T21n1295_001:0380a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我所辦供願垂納受。",
        "from": "T21n1295_001:0380a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願冥加衛護於我。",
        "from": "T21n1295_001:0380a29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心想本方諮白。",
        "from": "T21n1295_001:0380a29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "退請。",
        "from": "T21n1295_001:0380b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以梵名請世天。",
        "from": "T21n1295_001:0380b01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東北伊舍那眷屬部多眾戟印三昧拳火竪屈風背。",
        "from": "T21n1295_001:0380b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伊舍那真言曰。",
        "from": "T21n1295_001:0380b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三去滿多沒馱喃引嚕心捺囉二合野娑嚩二合賀引觀想護方天王各從天宮降臨來此。",
        "from": "T21n1295_001:0380b06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依本方位摽心令住。",
        "from": "T21n1295_001:0380b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝釋印。",
        "from": "T21n1295_001:0380b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內縛二風申如針。",
        "from": "T21n1295_001:0380b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝釋天王真言曰。",
        "from": "T21n1295_001:0380b08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三去滿多沒馱喃引鑠吃羅二合也娑嚩二合賀引火仙。",
        "from": "T21n1295_001:0380b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定掌火空相捻三角。",
        "from": "T21n1295_001:0380b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惠竪四輪空度橫在掌中。",
        "from": "T21n1295_001:0380b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風屈三召。",
        "from": "T21n1295_001:0380b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火天真言曰。",
        "from": "T21n1295_001:0380b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱喃阿𣝅曩二合曳娑嚩二合賀焰摩。",
        "from": "T21n1295_001:0380b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定惠合地風背入月。",
        "from": "T21n1295_001:0380b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "焰摩王真言曰。",
        "from": "T21n1295_001:0380b14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱喃縛無背反嚩娑嚩多野二合娑嚩二合賀羅剎恐怖形。",
        "from": "T21n1295_001:0380b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西南方持刀。",
        "from": "T21n1295_001:0380b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1295_001:0380b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼多沒馱喃羅入吃察二合娑地跛多娑嚩二合賀水天執羂索。",
        "from": "T21n1295_001:0380b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1295_001:0380b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多母馱喃阿播鉢多曳娑嚩二合賀縛庾風天憧智拳地水竪。",
        "from": "T21n1295_001:0380b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風天真言曰。",
        "from": "T21n1295_001:0380b22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼多沒馱喃嚩引野吠娑嚩賀多聞虛心合二地入掌交。",
        "from": "T21n1295_001:0380b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空竪風側屈一寸不相著。",
        "from": "T21n1295_001:0380b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多聞天王真言曰。",
        "from": "T21n1295_001:0380b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多母馱喃味室囉二合摩拏野娑縛賀梵天持紅蓮三昧空持水。",
        "from": "T21n1295_001:0380b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵王真言曰。",
        "from": "T21n1295_001:0380b28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多母馱喃鉢羅二合若鉢哆曳娑縛賀地神持寶瓶。",
        "from": "T21n1295_001:0380c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地神真言曰。",
        "from": "T21n1295_001:0380c02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼多母馱喃鉢囉二合體丁以反吠曳娑嚩二合賀日天福智仰水入空持剎火輪欲相並水火自相持。",
        "from": "T21n1295_001:0380c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1295_001:0380c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多母馱喃阿儞怛也二合野娑嚩二合賀月天三昧手持於白蓮華。",
        "from": "T21n1295_001:0380c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空捻火初節。",
        "from": "T21n1295_001:0380c09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1295_001:0380c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼多母馱喃戰捺囉二合野娑婆賀稱名敬白。",
        "from": "T21n1295_001:0380c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護方天王十二宮神。",
        "from": "T21n1295_001:0380c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九執大天二十八宿。",
        "from": "T21n1295_001:0380c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "業道冥宮本命宿主。",
        "from": "T21n1295_001:0380c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今遇此災變。",
        "from": "T21n1295_001:0380c13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "某事相凌敬謝天眾。",
        "from": "T21n1295_001:0380c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "順佛教勅。",
        "from": "T21n1295_001:0380c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受我迎請。",
        "from": "T21n1295_001:0380c14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉來赴會響此。",
        "from": "T21n1295_001:0380c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "單誠發歡喜心。",
        "from": "T21n1295_001:0380c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為我某甲息除災障。",
        "from": "T21n1295_001:0380c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增長福壽。",
        "from": "T21n1295_001:0380c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或云。",
        "from": "T21n1295_001:0380c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從今日起首習真言。",
        "from": "T21n1295_001:0380c16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我承大聖護念力故。",
        "from": "T21n1295_001:0380c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拔濟我等一切有情輪迴苦業。",
        "from": "T21n1295_001:0380c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願護世天主遊空宿曜。",
        "from": "T21n1295_001:0380c18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "順佛教令輪施我無畏令安樂住。",
        "from": "T21n1295_001:0380c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應供養淨香水。",
        "from": "T21n1295_001:0380c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寫少水葉上以獻。",
        "from": "T21n1295_001:0380c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1295_001:0380c20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿密哩二合帝吽泮吒塗香真言曰。",
        "from": "T21n1295_001:0380c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中名指彈少塗香獻淡。",
        "from": "T21n1295_001:0380c22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼多沒馱引喃引尾戍馱誐□納婆二合嚩娑嚩二合賀花鬘真言曰。",
        "from": "T21n1295_001:0380c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相反仰蓮掌置額右旋轉。",
        "from": "T21n1295_001:0380c25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱喃摩賀引妹怛哩也二合毘庾二合訥𮓌帝娑嚩二合賀焚香真言曰。",
        "from": "T21n1295_001:0380c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地水火相背。",
        "from": "T21n1295_001:0380c28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二風側相合。",
        "from": "T21n1295_001:0380c28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空捻風輪上節開。",
        "from": "T21n1295_001:0380c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱喃□摩馱怛嚩二合拏𮓌帝娑嚩二合賀飲食真言曰。",
        "from": "T21n1295_001:0381a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密合。",
        "from": "T21n1295_001:0381a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱喃阿囉囉迦囉囉末隣上捺娜𭤶沫捺禰摩賀沫瀝娑嚩二合賀燈明真言曰。",
        "from": "T21n1295_001:0381a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右羽為拳竪。",
        "from": "T21n1295_001:0381a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火輪空押風捻火中節。",
        "from": "T21n1295_001:0381a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱喃怛他引𮓌多引囉𭥷二合娑叵二合囉儜嚩婆引娑曩誐誐𭸧那哩也二合娑嚩二合賀虛空藏菩薩普供養真言曰。",
        "from": "T21n1295_001:0381a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫薩嚩怛他𮓌帝驃一尾濕嚩二合目契弊薩嚩他欠𭇩娜𮓌帝娑頗引儞𤚥誐誐娜鉤娑婆二合賀觀想護方天王各從天宮降臨來此。",
        "from": "T21n1295_001:0381a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依本方位摽心令住。",
        "from": "T21n1295_001:0381a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱名敬白。",
        "from": "T21n1295_001:0381a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護方天王十二宮神。",
        "from": "T21n1295_001:0381a16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九執大天二十八宿。",
        "from": "T21n1295_001:0381a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "業道冥宮本命宿主。",
        "from": "T21n1295_001:0381a17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今遇此災變。",
        "from": "T21n1295_001:0381a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "某事相凌敬謝天眾。",
        "from": "T21n1295_001:0381a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "順佛教勅。",
        "from": "T21n1295_001:0381a18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受我迎請。",
        "from": "T21n1295_001:0381a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉來赴會響此。",
        "from": "T21n1295_001:0381a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "單誠發歡喜心。",
        "from": "T21n1295_001:0381a19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為我某息除災障。",
        "from": "T21n1295_001:0381a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增長福壽或云。",
        "from": "T21n1295_001:0381a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "某從今日起首若干日來。",
        "from": "T21n1295_001:0381a20_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦某真言。",
        "from": "T21n1295_001:0381a20_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其間守護道場。",
        "from": "T21n1295_001:0381a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅除障難。",
        "from": "T21n1295_001:0381a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘事某白我承大聖護念力故。",
        "from": "T21n1295_001:0381a21_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拔濟我等及一切有情輪迴苦業。",
        "from": "T21n1295_001:0381a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願護方天主遊空宿曜。",
        "from": "T21n1295_001:0381a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "順佛教令輪故施我無畏令安樂住。",
        "from": "T21n1295_001:0381a23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請白了已。",
        "from": "T21n1295_001:0381a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應供養淨瓶香水葉上以獻。",
        "from": "T21n1295_001:0381a24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1295_001:0381a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱引喃引印捺羅二合邪遏鉗次以中名指彈獻少塗香以獻即言獻淡。",
        "from": "T21n1295_001:0381a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以一花置之於坐次。",
        "from": "T21n1295_001:0381a28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "献花名補澁二合甘補甘反焚董□香。",
        "from": "T21n1295_001:0381a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於座前梵語燒香。",
        "from": "T21n1295_001:0381a29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號之度引補甘二合。",
        "from": "T21n1295_001:0381b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次卒一杓粥置於葉上。",
        "from": "T21n1295_001:0381b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又言未臨。",
        "from": "T21n1295_001:0381b02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次用小蠟燭插在粥上。",
        "from": "T21n1295_001:0381b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵云儞引補甘二合布引惹室哩二合引泥去嚩補怛囉二合寫扇底俱嚕娑嚩二合引賀。",
        "from": "T21n1295_001:0381b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每位從水至燭獻畢然向。",
        "from": "T21n1295_001:0381b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其次其燭作意獻諸位。",
        "from": "T21n1295_001:0381b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未遍已來莫令燭滅。",
        "from": "T21n1295_001:0381b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或須助伴驅使數人。",
        "from": "T21n1295_001:0381b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各執一事以祇供。",
        "from": "T21n1295_001:0381b08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一一自取。",
        "from": "T21n1295_001:0381b08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恐燭不終法事似不吉祥。",
        "from": "T21n1295_001:0381b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "再白求事。",
        "from": "T21n1295_001:0381b09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或可施繞。",
        "from": "T21n1295_001:0381b09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或復安坐。",
        "from": "T21n1295_001:0381b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念大吉祥真言三七遍。",
        "from": "T21n1295_001:0381b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又念佛慈護真言一百八遍。",
        "from": "T21n1295_001:0381b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將此持誦饒益諸天。",
        "from": "T21n1295_001:0381b11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願諸天樂轉更增勝。",
        "from": "T21n1295_001:0381b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供獻之間多不如法。",
        "from": "T21n1295_001:0381b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願慈悲施我歡喜。",
        "from": "T21n1295_001:0381b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師應執爐遍乞歡喜求請無災。",
        "from": "T21n1295_001:0381b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說偈□明發遣天主諸天自在威神力。",
        "from": "T21n1295_001:0381b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今□求者速成就。",
        "from": "T21n1295_001:0381b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願天主還本宮後。",
        "from": "T21n1295_001:0381b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若請時垂哀赴。",
        "from": "T21n1295_001:0381b15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引吉里吉里嚩日羅二合目娑嚩二合賀引次施一切鬼神食。",
        "from": "T21n1295_001:0381b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘇婆呼經云。",
        "from": "T21n1295_001:0381b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次蘇婆呼。",
        "from": "T21n1295_001:0381b17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若念誦人欲得成就作持諸法令無障難得悉地者。",
        "from": "T21n1295_001:0381b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以諸飲食祭祀諸天修羅藥叉龍王迦嚕拏共命鳥羯吒布單那乾闥婆部多諸鬼魅等。",
        "from": "T21n1295_001:0381b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居地上或處虛空。",
        "from": "T21n1295_001:0381b21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令念誦人無諸難事。",
        "from": "T21n1295_001:0381b21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "意願所求皆悉滿足。",
        "from": "T21n1295_001:0381b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言契印牢結護。",
        "from": "T21n1295_001:0381b22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右膝著地啟請言曰。",
        "from": "T21n1295_001:0381b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居妙高山諸天部多。",
        "from": "T21n1295_001:0381b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居歡喜園及餘天宮。",
        "from": "T21n1295_001:0381b24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居日月。",
        "from": "T21n1295_001:0381b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居河海或陂澤泉井。",
        "from": "T21n1295_001:0381b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居村落。",
        "from": "T21n1295_001:0381b25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居神廟。",
        "from": "T21n1295_001:0381b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居空室。",
        "from": "T21n1295_001:0381b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居天室。",
        "from": "T21n1295_001:0381b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或住伽藍制帝。",
        "from": "T21n1295_001:0381b26_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居外道草菴。",
        "from": "T21n1295_001:0381b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居家室。",
        "from": "T21n1295_001:0381b27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居庫藏。",
        "from": "T21n1295_001:0381b27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或住街巷。",
        "from": "T21n1295_001:0381b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居四□道邊。",
        "from": "T21n1295_001:0381b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或依獨樹。",
        "from": "T21n1295_001:0381b28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或在大路。",
        "from": "T21n1295_001:0381b28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或住塚間。",
        "from": "T21n1295_001:0381b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居屍多林。",
        "from": "T21n1295_001:0381b29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或寄住大林。",
        "from": "T21n1295_001:0381b29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居師子大虫遊行之處。",
        "from": "T21n1295_001:0381c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或住大砂磧中。",
        "from": "T21n1295_001:0381c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居海洲諸上妙處。",
        "from": "T21n1295_001:0381c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諮居與諸眷屬降臨來此。",
        "from": "T21n1295_001:0381c02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我所營辦花鬘塗香飲食及妙燈明。",
        "from": "T21n1295_001:0381c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願垂影嚮。",
        "from": "T21n1295_001:0381c03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我所求願事滿足。",
        "from": "T21n1295_001:0381c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其果即以銅器或淨瓫。",
        "from": "T21n1295_001:0381c04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置種種甘美飲食菓子甜脆等。",
        "from": "T21n1295_001:0381c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆須擘碎。",
        "from": "T21n1295_001:0381c05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦施焰口餓鬼食真言。",
        "from": "T21n1295_001:0381c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起慈悲心。",
        "from": "T21n1295_001:0381c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "己手按盆。",
        "from": "T21n1295_001:0381c06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持三二七遍。",
        "from": "T21n1295_001:0381c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又念妙色身如來真言。",
        "from": "T21n1295_001:0381c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持水灑。",
        "from": "T21n1295_001:0381c07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又念普供養印明。",
        "from": "T21n1295_001:0381c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至誠運求所禮花香飲食廣多細妙。",
        "from": "T21n1295_001:0381c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又念偈曰。",
        "from": "T21n1295_001:0381c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以我所修福　　普霑於鬼趣",
        "from": "T21n1295_001:0381c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食已免飢苦　　捨身生樂處",
        "from": "T21n1295_001:0381c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即將障外或於宅□□方散施。",
        "from": "T21n1295_001:0381c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或施長流水中。",
        "from": "T21n1295_001:0381c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施與無名□鬼神食之。",
        "from": "T21n1295_001:0381c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1295_001:0381c13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多母馱引南引印娜囉二合耶二仡哩二合賀拏二合伊𤚥引三遏鉗四巘淡五補澁鉗補甘反度引補甘二合引麼臨八儞引補甘二合布引若室哩二合引十泥嚩補怛囉二合寫十一扇底俱嚕娑嚩二合引賀引十二總云。",
        "from": "T21n1295_001:0381c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命諸佛及帝釋。",
        "from": "T21n1295_001:0381c19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受此遏伽塗香花鬘燒香粥食及妙燈明供養。",
        "from": "T21n1295_001:0381c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉祥天子與某甲作息災。",
        "from": "T21n1295_001:0381c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福智增盛。",
        "from": "T21n1295_001:0381c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉祥圓滿。",
        "from": "T21n1295_001:0381c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故言　娑嚩二合引賀。",
        "from": "T21n1295_001:0381c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施十方天王真言印契等。",
        "from": "T21n1295_001:0381c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡受諸法及一期念誦畢及護摩灌頂等。",
        "from": "T21n1295_001:0381c24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆須施諸天速得無難。",
        "from": "T21n1295_001:0381c25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌曰。",
        "from": "T21n1295_001:0381c25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀心法界生　　成本忿怒壇",
        "from": "T21n1295_001:0381c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三隅娜誐形　　猛熾徹梵天",
        "from": "T21n1295_001:0381c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三部次□□　　引索四召請",
        "from": "T21n1295_001:0381c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成身誦三遍　　或復一□□",
        "from": "T21n1295_001:0381c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "事畢發願已　　後結本方尊",
        "from": "T21n1295_001:0382a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各誦本真言　　送聖護方天",
        "from": "T21n1295_001:0382a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依法念誦畢　　歌讚發願已",
        "from": "T21n1295_001:0382a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解界送聖天　　方出道場外",
        "from": "T21n1295_001:0382a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八隅塗圓壇　　而為十聖位",
        "from": "T21n1295_001:0382a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於帝釋左右　　置梵天地天",
        "from": "T21n1295_001:0382a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或復於龍方　　梵地相對置",
        "from": "T21n1295_001:0382a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日天月天位　　曜宿皆隨轉",
        "from": "T21n1295_001:0382a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中位觀四臂　　不動忿怒王",
        "from": "T21n1295_001:0382a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩手金剛拳　　風地曲如鉤",
        "from": "T21n1295_001:0382a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以安口兩邊　　右鉤左持索",
        "from": "T21n1295_001:0382a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須彌山為坐　　身外現八峯",
        "from": "T21n1295_001:0382a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利銳金剛輪　　洪滿紺青色",
        "from": "T21n1295_001:0382a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓬頭垂髮索　　威焰如劫燒",
        "from": "T21n1295_001:0382a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勢如𮓌嚕拏　　赫奕無能對",
        "from": "T21n1295_001:0382a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魔羅及鉢底　　攝怖而奔竄",
        "from": "T21n1295_001:0382a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘昔大誓力　　現茲暴惡形",
        "from": "T21n1295_001:0382a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吞嘩諸障惱　　安慰修行者",
        "from": "T21n1295_001:0382a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "布列方位已　　供養以雜粥",
        "from": "T21n1295_001:0382a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以粳米油麻　　菉豆相和煮",
        "from": "T21n1295_001:0382a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然於諸壇位　　各置一瓷器",
        "from": "T21n1295_001:0382a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以淨香水　　少傾於器中",
        "from": "T21n1295_001:0382a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次奉以□□　　用指而彈指",
        "from": "T21n1295_001:0382a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又奉諸名花　　次燒香□□",
        "from": "T21n1295_001:0382a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然以一杓粥　　皆以殷重心",
        "from": "T21n1295_001:0382a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各加持三遍　　次執小蠟燭",
        "from": "T21n1295_001:0382a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用攝於粥上　　事未畢已來",
        "from": "T21n1295_001:0382a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此燭勿令滅　　於圓寂字上",
        "from": "T21n1295_001:0382a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加所求願言　　聖力密加持",
        "from": "T21n1295_001:0382a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所作皆悉地　　斯乃助修行",
        "from": "T21n1295_001:0382b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獨期願亦得　　若欲請施時",
        "from": "T21n1295_001:0382b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "標心令安位　　依法而供養",
        "from": "T21n1295_001:0382b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "決定無疑念　　願者皆成就",
        "from": "T21n1295_001:0382b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九執日為主　　諸宿月為主",
        "from": "T21n1295_001:0382b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵地沒捺囉　　今當次第說",
        "from": "T21n1295_001:0382b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八葉鉢頭摩　　沒囉賀摩印",
        "from": "T21n1295_001:0382b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十度如閼伽　　地天沒捺囉",
        "from": "T21n1295_001:0382b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以定握惠腕　　惠竪舒五輪",
        "from": "T21n1295_001:0382b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空輪入月橫　　風狀鉤來去",
        "from": "T21n1295_001:0382b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火天請送印　　將禪捻戒度",
        "from": "T21n1295_001:0382b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向外以三彈　　是遣沒捺羅",
        "from": "T21n1295_001:0382b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言如經說。",
        "from": "T21n1295_001:0382b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵名歎如來十號功德怛他誐姤如來囉𮋖應供三藐三母馱正遍知尾儞耶二合月□□行播拏足索誐姤善逝路迦□□姤拏怛囉二合無上士補嚕灑娜𭤶耶些羅體調御丁以反捨些跢丈夫泥嚩天難者摩拏史耶人南者母度佛薄誐鑁世尊天供次第先於道場外。",
        "from": "T21n1295_001:0382b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敷茅或蓮葉或青草等。",
        "from": "T21n1295_001:0382b22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗圓壇并辦備六種具蠟燭等。",
        "from": "T21n1295_001:0382b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次著座三部被甲護身。",
        "from": "T21n1295_001:0382b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次加持香水用枳里枳羅印明二十一遍。",
        "from": "T21n1295_001:0382b24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散灑四方供物等次用羯磨印加持七遍。",
        "from": "T21n1295_001:0382b25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次加持壇用枳里枳羅明。",
        "from": "T21n1295_001:0382b25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七遍若二十一遍次淨地如大儀次五大願并陳事由。",
        "from": "T21n1295_001:0382b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及誦般若經典饒□□天等。",
        "from": "T21n1295_001:0382b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次大輪金剛印明地壇等□觀想壇中􄚲字變成獨古杵。",
        "from": "T21n1295_001:0382b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "杵變成四臂明王如護摩說次四字明如大儀軌想本來住尊加被壇中尊冥合無二也。",
        "from": "T21n1295_001:0382c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次隨方召請誦大鉤召印明。",
        "from": "T21n1295_001:0382c01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1295_001:0382c01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼沒馱引喃一阿去息呼薩嚩怛囉二合鉢囉二合底訶諦二怛他𮓌黨矩奢三昌地折哩也二合鉢哩布囉迦四娑嚩二合賀引次誦梵名勸請　次䠒跪誦漠語勸謁如法全儀軌。",
        "from": "T21n1295_001:0382c05_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若繁者不誦好也　次誦四智讚。",
        "from": "T21n1295_001:0382c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次捧閼伽奉獻不動明王　次座印。",
        "from": "T21n1295_001:0382c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次網。",
        "from": "T21n1295_001:0382c07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次炎。",
        "from": "T21n1295_001:0382c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次塗花燒。",
        "from": "T21n1295_001:0382c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如常念誦奉獻不動尊　次一一供獻諸天等隨方位各各可置一器每遏伽等日誦各各本明三　□□□□□□□□□□。",
        "from": "T21n1295_001:0382c09_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次普供養明。",
        "from": "T21n1295_001:0382c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結印加持五處。",
        "from": "T21n1295_001:0382c10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次不動慈救真言一百八遍。",
        "from": "T21n1295_001:0382c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慇懃祈願數了。",
        "from": "T21n1295_001:0382c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前結護懺悔。",
        "from": "T21n1295_001:0382c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次事主天真言一百八遍。",
        "from": "T21n1295_001:0382c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一千八十遍。",
        "from": "T21n1295_001:0382c12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可念為灌護施主也。",
        "from": "T21n1295_001:0382c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次念如來慈護真言一百八遍。",
        "from": "T21n1295_001:0382c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又誦大吉祥天真言一百八遍回向之義也。",
        "from": "T21n1295_001:0382c14_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次後供養奉獻不動明王。",
        "from": "T21n1295_001:0382c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次誦普供養明供養明王□海會塵剎諸尊護世天等。",
        "from": "T21n1295_001:0382c16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次四智讚及天龍八部讚。",
        "from": "T21n1295_001:0382c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次迴向。",
        "from": "T21n1295_001:0382c17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次解界。",
        "from": "T21n1295_001:0382c17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如例一一解。",
        "from": "T21n1295_001:0382c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次閼伽。",
        "from": "T21n1295_001:0382c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次發遣。",
        "from": "T21n1295_001:0382c18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次三部護身。",
        "from": "T21n1295_001:0382c18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如先初出道場了。",
        "from": "T21n1295_001:0382c19_2",
        "context_with_next_sentence": true
    }
]