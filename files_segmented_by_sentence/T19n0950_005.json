[
    {
        "source_sentence": "菩提場所說一字頂輪王經卷第",
        "from": "T19n0950_005:0220b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五",
        "from": "T19n0950_005:0220b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿",
        "from": "T19n0950_005:0220b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "肅國公食邑三千戶賜紫贈司空諡大鑒正號大廣智大興善寺三藏沙門不空奉　詔譯11 無能勝加持品無能勝加持品第十一爾時世尊觀金剛手祕密主復以伽他而誥之曰。",
        "from": "T19n0950_005:0220c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當於未來世　　有情劣精進",
        "from": "T19n0950_005:0220c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我慢嗔恚癡　　無慚矯慳悋",
        "from": "T19n0950_005:0220c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能依儀軌　　修習真言行",
        "from": "T19n0950_005:0220c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是持誦之人　　護摩所加持",
        "from": "T19n0950_005:0220c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸魔悉惑亂　　悉無是思惟",
        "from": "T19n0950_005:0220c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以明獲成就　　虛受諸勤勞",
        "from": "T19n0950_005:0220c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常作是思惟　　愚夫常是說",
        "from": "T19n0950_005:0220c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為息彼障故　　亦除諸魔羅",
        "from": "T19n0950_005:0220c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今說此大明　　先佛之所說",
        "from": "T19n0950_005:0220c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利益諸有情　　是無能勝明",
        "from": "T19n0950_005:0220c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人常憶念　　隨時住等引",
        "from": "T19n0950_005:0220c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼諸魔障者　　悉皆得除滅",
        "from": "T19n0950_005:0220c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說真言句　　爾時金剛手",
        "from": "T19n0950_005:0220c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祕密藥叉主　　心生大歡喜",
        "from": "T19n0950_005:0220c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂禮於世尊　　大覺智莊嚴",
        "from": "T19n0950_005:0220c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大無能勝　　是明我願聽",
        "from": "T19n0950_005:0220c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊即說大無能勝陀羅尼曰。",
        "from": "T19n0950_005:0220c19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是陀羅尼　　世間悉皆聞",
        "from": "T19n0950_005:0221a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是大無能勝　　能壞一切魔",
        "from": "T19n0950_005:0221a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能增勤勇力　　則住三昧形",
        "from": "T19n0950_005:0221a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為無能勝　　說彼大心明",
        "from": "T19n0950_005:0221a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大力極勇猛　　不異於前明",
        "from": "T19n0950_005:0221a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是心世尊說真言曰。",
        "from": "T19n0950_005:0221a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此心真言　　應等正覺說",
        "from": "T19n0950_005:0221b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七佛之世尊　　顯揚諸功德",
        "from": "T19n0950_005:0221b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說是大明　　利益修行者",
        "from": "T19n0950_005:0221b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普遍諸世界　　六種而振動",
        "from": "T19n0950_005:0221b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切魔宮殿　　悉皆大振動",
        "from": "T19n0950_005:0221b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛手此真言句。",
        "from": "T19n0950_005:0221b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸佛所說。",
        "from": "T19n0950_005:0221b16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利益眾生故。",
        "from": "T19n0950_005:0221b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祕密主或持誦輪王真言者。",
        "from": "T19n0950_005:0221b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或持餘真言者以此真言加持結縷。",
        "from": "T19n0950_005:0221b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或結袈裟角。",
        "from": "T19n0950_005:0221b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或結頂髮。",
        "from": "T19n0950_005:0221b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或樺皮上書帶頸臂。",
        "from": "T19n0950_005:0221b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼人速疾易得成就。",
        "from": "T19n0950_005:0221b19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本尊速現其前。",
        "from": "T19n0950_005:0221b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦時若能憶持。",
        "from": "T19n0950_005:0221b20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛手我不見天世魔世沙門婆羅門眾中。",
        "from": "T19n0950_005:0221b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若此真言加護。",
        "from": "T19n0950_005:0221b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若穢者若淨者前。",
        "from": "T19n0950_005:0221b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人若非人或魔子或必舍遮。",
        "from": "T19n0950_005:0221b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或毘那也迦或藥叉或鳩槃茶或羅剎娑。",
        "from": "T19n0950_005:0221b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或餘類有情欲來障難。",
        "from": "T19n0950_005:0221b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是思惟。",
        "from": "T19n0950_005:0221b24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於阿吒迦嚩底丁以反王宮不得入。",
        "from": "T19n0950_005:0221b25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有違越此明清淨修行者。",
        "from": "T19n0950_005:0221b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼悉皆違逆金剛種族及自種族并親族朋友。",
        "from": "T19n0950_005:0221b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不容住於彼。",
        "from": "T19n0950_005:0221b27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛手此明真言有大威力。",
        "from": "T19n0950_005:0221b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切事業應作加護。",
        "from": "T19n0950_005:0221b28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應供正遍知印可。",
        "from": "T19n0950_005:0221b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸菩薩印可。",
        "from": "T19n0950_005:0221b29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "12 證學法品菩提場所說一字頂輪王經證學法品第十二爾時世尊知無盡法界已。",
        "from": "T19n0950_005:0221c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除遣一切諸障。",
        "from": "T19n0950_005:0221c03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復誥金剛手菩薩言。",
        "from": "T19n0950_005:0221c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛手若善男子善女人苾芻苾芻尼。",
        "from": "T19n0950_005:0221c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲修習佛頂不思議印三摩地。",
        "from": "T19n0950_005:0221c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼苾芻住苾芻律儀。",
        "from": "T19n0950_005:0221c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慇懃而護持。",
        "from": "T19n0950_005:0221c06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苾芻尼住苾芻尼律儀。",
        "from": "T19n0950_005:0221c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塢波塞迦住塢婆塞迦律儀。",
        "from": "T19n0950_005:0221c07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塢波斯迦住塢波斯迦律儀。",
        "from": "T19n0950_005:0221c08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若是彼善男子修真言行者。",
        "from": "T19n0950_005:0221c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼先應入曼茶羅授三歸依發菩提心。",
        "from": "T19n0950_005:0221c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應成就十善業道如說修真言行極善作意。",
        "from": "T19n0950_005:0221c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "親近承事善友。",
        "from": "T19n0950_005:0221c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常修六念。",
        "from": "T19n0950_005:0221c11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應觀法界如虛空自性。",
        "from": "T19n0950_005:0221c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應善修習入般若波羅蜜境界。",
        "from": "T19n0950_005:0221c12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此觀行不欺誑不放逸。",
        "from": "T19n0950_005:0221c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善應隨三世佛菩薩行行。",
        "from": "T19n0950_005:0221c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住於阿蘭若。",
        "from": "T19n0950_005:0221c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不顧戀一切身命。",
        "from": "T19n0950_005:0221c14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三時善應授三歸菩提心律儀戒。",
        "from": "T19n0950_005:0221c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所聞甚深佛法憶持修行。",
        "from": "T19n0950_005:0221c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善修四攝。",
        "from": "T19n0950_005:0221c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於如來窣覩波前。",
        "from": "T19n0950_005:0221c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常塗曼荼羅。",
        "from": "T19n0950_005:0222a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於真言儀軌常精進。",
        "from": "T19n0950_005:0222a01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作窣覩波身口專精修行。",
        "from": "T19n0950_005:0222a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不怒不躁不掉舉。",
        "from": "T19n0950_005:0222a02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口不多語不雜亂語不欺誑他。",
        "from": "T19n0950_005:0222a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸有情常行恭敬愛樂心。",
        "from": "T19n0950_005:0222a03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善知如來密意之說。",
        "from": "T19n0950_005:0222a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我略說。",
        "from": "T19n0950_005:0222a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行者常懷勇猛大精進意。",
        "from": "T19n0950_005:0222a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安立一切有情於佛菩薩道。",
        "from": "T19n0950_005:0222a05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若修佛頂王真言行者。",
        "from": "T19n0950_005:0222a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若修餘真言行者。",
        "from": "T19n0950_005:0222a06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應如所說功德。",
        "from": "T19n0950_005:0222a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善應修行成就。",
        "from": "T19n0950_005:0222a07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如方廣經典所說真言行。",
        "from": "T19n0950_005:0222a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當修習。",
        "from": "T19n0950_005:0222a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各住自律儀善應護持。",
        "from": "T19n0950_005:0222a08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復誥金剛手。",
        "from": "T19n0950_005:0222a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如說修佛頂真言行者。",
        "from": "T19n0950_005:0222a09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已得成就身。",
        "from": "T19n0950_005:0222a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如初出日輪。",
        "from": "T19n0950_005:0222a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真金瓔珞臂釧。",
        "from": "T19n0950_005:0222a10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作閻浮檀金色。",
        "from": "T19n0950_005:0222a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切嚴具莊嚴其身。",
        "from": "T19n0950_005:0222a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著天妙衣具諸相好。",
        "from": "T19n0950_005:0222a11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縱廣周停身相奇特。",
        "from": "T19n0950_005:0222a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百千光明莊嚴。",
        "from": "T19n0950_005:0222a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓光一尋超過日輪。",
        "from": "T19n0950_005:0222a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "映奪一切色身。",
        "from": "T19n0950_005:0222a13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次金剛手。",
        "from": "T19n0950_005:0222a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就持明仙。",
        "from": "T19n0950_005:0222a14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纔見令一切眾生喜悅。",
        "from": "T19n0950_005:0222a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如如意樹令滿一切所求。",
        "from": "T19n0950_005:0222a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次金剛手成就輪王佛頂菩薩。",
        "from": "T19n0950_005:0222a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至於地獄雨種種天妙飲食。",
        "from": "T19n0950_005:0222a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能滿一切眾生所須。",
        "from": "T19n0950_005:0222a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有所希望者皆得滿足。",
        "from": "T19n0950_005:0222a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我略說彼有大威德。",
        "from": "T19n0950_005:0222a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛手成就輪王真言者。",
        "from": "T19n0950_005:0222a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆滿一切有情意樂。",
        "from": "T19n0950_005:0222a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由心起念則令滿足。",
        "from": "T19n0950_005:0222a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼得輪王成就之人。",
        "from": "T19n0950_005:0222a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住十地菩薩。",
        "from": "T19n0950_005:0222a20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不敢違越其教令。",
        "from": "T19n0950_005:0222a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛手此一字輪王真言。",
        "from": "T19n0950_005:0222a21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切真言中為王。",
        "from": "T19n0950_005:0222a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大明王主。",
        "from": "T19n0950_005:0222a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若修行滅除一切業障。",
        "from": "T19n0950_005:0222a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦滅除一切惡趣之業。",
        "from": "T19n0950_005:0222a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得成此真言。",
        "from": "T19n0950_005:0222a23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切神通悉皆現前。",
        "from": "T19n0950_005:0222a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纔瞬目頃往於色究竟天。",
        "from": "T19n0950_005:0222a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切佛菩薩緣覺聲聞稱讚歡喜。",
        "from": "T19n0950_005:0222a25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得一切菩薩行。",
        "from": "T19n0950_005:0222a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於餘世界自在遊行。",
        "from": "T19n0950_005:0222a26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切有情。",
        "from": "T19n0950_005:0222a26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨其意趣。",
        "from": "T19n0950_005:0222a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種音聲而為說法。",
        "from": "T19n0950_005:0222a27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至我略說。",
        "from": "T19n0950_005:0222a27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於無量無邊世界有情。",
        "from": "T19n0950_005:0222a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "希奇行色最勝廣大獲得成就。",
        "from": "T19n0950_005:0222a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時如來說伽他曰。",
        "from": "T19n0950_005:0222a29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種鬪戰空自在　　如栽寶性而照曜",
        "from": "T19n0950_005:0222b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若青蓮華在池開　　彼勝驍勇威光色",
        "from": "T19n0950_005:0222b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼人悉皆超世明　　毘鈕真言不可及",
        "from": "T19n0950_005:0222b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊復誥金剛手祕密主。",
        "from": "T19n0950_005:0222b04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說伽他曰。",
        "from": "T19n0950_005:0222b04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略說普通法　　祕密地居人",
        "from": "T19n0950_005:0222b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明者先行後　　指示相最勝",
        "from": "T19n0950_005:0222b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地方說三種　　卑濕及乾燥",
        "from": "T19n0950_005:0222b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并以於高原　　於明天所居",
        "from": "T19n0950_005:0222b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名曰為勝地　　中方說三種",
        "from": "T19n0950_005:0222b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求成就之地　　皆通於三種",
        "from": "T19n0950_005:0222b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智者應觀察　　淨不淨兩并",
        "from": "T19n0950_005:0222b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天妙復三種　　於此一一中",
        "from": "T19n0950_005:0222b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各分為三種　　河池海山王",
        "from": "T19n0950_005:0222b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱最勝成就　　淨不淨德俱",
        "from": "T19n0950_005:0222b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名中成就處　　若是屍林地",
        "from": "T19n0950_005:0222b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名不淨處　　此教一切處",
        "from": "T19n0950_005:0222b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就處三種　　惡王賊飢儉",
        "from": "T19n0950_005:0222b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是處不應居　　行者有障難",
        "from": "T19n0950_005:0222b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼地不應住　　極寒熱雨處",
        "from": "T19n0950_005:0222b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此教悉制　　三時應念誦",
        "from": "T19n0950_005:0222b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長養意樂故　　應攝三種時",
        "from": "T19n0950_005:0222b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復告金剛手　　遠離於祕密",
        "from": "T19n0950_005:0222b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就不可得　　於此經教中",
        "from": "T19n0950_005:0222b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就故說密　　護摩爐差別",
        "from": "T19n0950_005:0222b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應祕密而作　　息災等三種",
        "from": "T19n0950_005:0222b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一處不應作　　若一處護摩",
        "from": "T19n0950_005:0222b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護摩爐必謬　　若於調伏爐",
        "from": "T19n0950_005:0222b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應作息災　　如器中有毒",
        "from": "T19n0950_005:0222b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盛乳必當壞　　審觀三種事",
        "from": "T19n0950_005:0222c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故說三種爐　　餘教亦說三",
        "from": "T19n0950_005:0222c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爐作是分別　　於此應用之",
        "from": "T19n0950_005:0222c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故不相違　　明屈屢草芽",
        "from": "T19n0950_005:0222c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "許用於牛酥　　優曇鉢天木",
        "from": "T19n0950_005:0222c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及以於乳木　　并用欝金香",
        "from": "T19n0950_005:0222c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三時作護摩　　為求息災故",
        "from": "T19n0950_005:0222c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲得種種利　　若被竊藥物",
        "from": "T19n0950_005:0222c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應用黑油麻　　和蜜常用之",
        "from": "T19n0950_005:0222c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及波羅奢木　　及與天木等",
        "from": "T19n0950_005:0222c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應用白芥子　　護摩而稱讚",
        "from": "T19n0950_005:0222c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸三種法　　而用酥護摩",
        "from": "T19n0950_005:0222c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊釋迦牟尼。",
        "from": "T19n0950_005:0222c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復告金剛手祕密主言。",
        "from": "T19n0950_005:0222c13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此中修行教王。",
        "from": "T19n0950_005:0222c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為有情利益故。",
        "from": "T19n0950_005:0222c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說伽他曰。",
        "from": "T19n0950_005:0222c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是真言明　　種種大威德",
        "from": "T19n0950_005:0222c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修習佛頂王　　種種真言明",
        "from": "T19n0950_005:0222c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量大奇特　　并佛眼等明",
        "from": "T19n0950_005:0222c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就諸義利　　及與印契等",
        "from": "T19n0950_005:0222c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我先已宣說　　普通真言王",
        "from": "T19n0950_005:0222c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為求成就者　　獲得果報故",
        "from": "T19n0950_005:0222c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今說印契　　為求悉地故",
        "from": "T19n0950_005:0222c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一類說多種　　次第我今說",
        "from": "T19n0950_005:0222c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普通佛頂印以二手內相叉作拳。",
        "from": "T19n0950_005:0222c25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二中指相合屈上節。",
        "from": "T19n0950_005:0222c25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普通一切佛頂印。",
        "from": "T19n0950_005:0222c26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能成就一切義利。",
        "from": "T19n0950_005:0222c26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由見此印契　　如親覩諸佛",
        "from": "T19n0950_005:0222c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難調諸藥叉　　龍阿酥羅眾",
        "from": "T19n0950_005:0222c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸羅剎　　由此印威德",
        "from": "T19n0950_005:0222c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉融而驚怖　　此是大真言",
        "from": "T19n0950_005:0223a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切佛頂心曩謨三滿多　沒駄南唵吒嚕二合滿馱四娑嚩二合引訶復說伽他曰。",
        "from": "T19n0950_005:0223a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若得此印契　　能獲諸安樂",
        "from": "T19n0950_005:0223a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國王等世間　　於彼常利益",
        "from": "T19n0950_005:0223a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲求法利益　　決定而獲得",
        "from": "T19n0950_005:0223a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若得此印契　　諸苦悉消滅",
        "from": "T19n0950_005:0223a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此一切佛頂根本印。",
        "from": "T19n0950_005:0223a10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作一切事業。",
        "from": "T19n0950_005:0223a10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行者護諸根者。",
        "from": "T19n0950_005:0223a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此根本印。",
        "from": "T19n0950_005:0223a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用中指端來往。",
        "from": "T19n0950_005:0223a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即名迎請印。",
        "from": "T19n0950_005:0223a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切普通先所說。",
        "from": "T19n0950_005:0223a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以各自真言。",
        "from": "T19n0950_005:0223a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用結此根本印。",
        "from": "T19n0950_005:0223a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通一切處用。",
        "from": "T19n0950_005:0223a13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗香花燒香飲食燈明等以此印用之。",
        "from": "T19n0950_005:0223a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即用前印。",
        "from": "T19n0950_005:0223a14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指峯如環狀。",
        "from": "T19n0950_005:0223a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是迎火天印。",
        "from": "T19n0950_005:0223a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言如先已說。",
        "from": "T19n0950_005:0223a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若發遣火天時。",
        "from": "T19n0950_005:0223a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印向外掣。",
        "from": "T19n0950_005:0223a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成發遣火天印。",
        "from": "T19n0950_005:0223a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又即此印准前辦事佛頂印。",
        "from": "T19n0950_005:0223a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右中指頭。",
        "from": "T19n0950_005:0223a17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈上節。",
        "from": "T19n0950_005:0223a17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "跓蹙左中指面上節。",
        "from": "T19n0950_005:0223a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是摧壞頂印。",
        "from": "T19n0950_005:0223a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能作奇特事　　能作一切事",
        "from": "T19n0950_005:0223a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護身結界處　　應當而受用",
        "from": "T19n0950_005:0223a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又移左中指屈上節蹙跓。",
        "from": "T19n0950_005:0223a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右中指面上節。",
        "from": "T19n0950_005:0223a21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是摧毀頂印。",
        "from": "T19n0950_005:0223a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言如先已說。",
        "from": "T19n0950_005:0223a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此名摧毀頂　　能調難調者",
        "from": "T19n0950_005:0223a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在於大障難　　以此應護身",
        "from": "T19n0950_005:0223a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前普通印。",
        "from": "T19n0950_005:0223a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈右中指第三節拄左中指第一節文。",
        "from": "T19n0950_005:0223a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能淨處所故　　用此摧毀頂",
        "from": "T19n0950_005:0223a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若求成就時　　結此護處所",
        "from": "T19n0950_005:0223a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "移左指如前　　以此印護處",
        "from": "T19n0950_005:0223a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸佛頂心　　應用摧毀頂",
        "from": "T19n0950_005:0223b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用以自灌頂　　以此印常用",
        "from": "T19n0950_005:0223b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人得此印　　能淨念誦室",
        "from": "T19n0950_005:0223b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常於澡洗時　　修行者應用",
        "from": "T19n0950_005:0223b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼人無諸障　　誦是真言故",
        "from": "T19n0950_005:0223b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次第而用之　　本部三昧耶",
        "from": "T19n0950_005:0223b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常用如此印　　修習真言者",
        "from": "T19n0950_005:0223b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼人無諸魔　　於此佛頂教",
        "from": "T19n0950_005:0223b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛作如是說即前印二中指於手背上。",
        "from": "T19n0950_005:0223b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相押如環。",
        "from": "T19n0950_005:0223b10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此無能勝頂　　能滅一切罪",
        "from": "T19n0950_005:0223b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言已先說　　能除諸惡夢",
        "from": "T19n0950_005:0223b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能成吉祥事　　應用此大印",
        "from": "T19n0950_005:0223b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當欲寢臥時　　自身若常誦",
        "from": "T19n0950_005:0223b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能滅種種障　　我今而略說",
        "from": "T19n0950_005:0223b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣說有無量　　於此我略說",
        "from": "T19n0950_005:0223b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為修佛頂者　　共佛眼真言",
        "from": "T19n0950_005:0223b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而誦求悉地　　一切諸會中",
        "from": "T19n0950_005:0223b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修諸真言說解脫　　一切如來及菩薩",
        "from": "T19n0950_005:0223b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得諸安樂獲義利　　增加精進及大力",
        "from": "T19n0950_005:0223b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有情利益勤修習　　悉除一切諸疑惑",
        "from": "T19n0950_005:0223b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故金剛祕密主　　諦聽我為汝宣說",
        "from": "T19n0950_005:0223b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我已略說義相應　　此是祕密修明者",
        "from": "T19n0950_005:0223b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三時護摩以天木　　油麻及酥乳相和",
        "from": "T19n0950_005:0223b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此歡喜真言王　　當說成就真言主",
        "from": "T19n0950_005:0223b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就念誦及護摩　　三種相資而演說",
        "from": "T19n0950_005:0223b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此一一修行中　　念誦修行說三種",
        "from": "T19n0950_005:0223b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身口及意次第說　　以此希望增益事",
        "from": "T19n0950_005:0223b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說三種應當知　　天上遊空及地居",
        "from": "T19n0950_005:0223b30_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為彼求成有三種　　為修三種之種類",
        "from": "T19n0950_005:0223c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就求欲及求財　　并與求法而念誦",
        "from": "T19n0950_005:0223c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨其悉地發勤勇　　為求一切成就故",
        "from": "T19n0950_005:0223c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善應依法作制底　　正見大悲求成就",
        "from": "T19n0950_005:0223c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼人成就亦不難　　現世獲得勝安樂",
        "from": "T19n0950_005:0223c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他世必獲於解脫　　古昔多人得成就",
        "from": "T19n0950_005:0223c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由修頂王大奇特　　我曾修此佛頂王",
        "from": "T19n0950_005:0223c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊釋迦牟尼如來。",
        "from": "T19n0950_005:0223c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以佛眼觀無量無邊世界。",
        "from": "T19n0950_005:0223c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復告金剛手言說伽他曰。",
        "from": "T19n0950_005:0223c09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸教中已說　　律儀與軌則",
        "from": "T19n0950_005:0223c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能作及所作　　於此教法中",
        "from": "T19n0950_005:0223c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當而修行　　以彼聖甘露",
        "from": "T19n0950_005:0223c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "軍茶利明王　　通修於三部",
        "from": "T19n0950_005:0223c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我說儀軌法　　常當而修行",
        "from": "T19n0950_005:0223c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由彼真言威　　一切障悉除",
        "from": "T19n0950_005:0223c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明王經所說　　忿怒王印契",
        "from": "T19n0950_005:0223c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼中諸儀軌　　悉皆此中用",
        "from": "T19n0950_005:0223c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應食葱蒜　　羅蔔及𮏄子",
        "from": "T19n0950_005:0223c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不以油塗身　　亦不應食油",
        "from": "T19n0950_005:0223c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有不淨食　　餘教中所制",
        "from": "T19n0950_005:0223c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切不應食　　求悉地行者",
        "from": "T19n0950_005:0223c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常求淨身故　　以無能勝明",
        "from": "T19n0950_005:0223c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應用於五淨　　半月半月用",
        "from": "T19n0950_005:0223c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所餘諸教說　　悉皆而修行",
        "from": "T19n0950_005:0223c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此我略說　　說餘經教中",
        "from": "T19n0950_005:0223c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此不廣說　　一切諸如來",
        "from": "T19n0950_005:0223c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說真言法性　　諸佛及菩薩",
        "from": "T19n0950_005:0223c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曾修亦曾說　　住彼真言形",
        "from": "T19n0950_005:0223c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遊行於世間　　廣作諸義利",
        "from": "T19n0950_005:0223c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為彼劣慧者　　盡說其威德",
        "from": "T19n0950_005:0224a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今少分說　　稱讚其功德",
        "from": "T19n0950_005:0224a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而於百劫中　　不能說輪王",
        "from": "T19n0950_005:0224a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奇特之法性　　此功德無盡",
        "from": "T19n0950_005:0224a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無盡無所得　　若得此教王",
        "from": "T19n0950_005:0224a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼人同如來　　亦同於菩薩",
        "from": "T19n0950_005:0224a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天蘇囉禮敬　　心得不退轉",
        "from": "T19n0950_005:0224a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常恒獲如是　　先世所積集",
        "from": "T19n0950_005:0224a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩提之資糧　　皆由祕密主",
        "from": "T19n0950_005:0224a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大威神之力　　當知彼有情",
        "from": "T19n0950_005:0224a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常得清淨身　　若得此教王",
        "from": "T19n0950_005:0224a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆得一切　　證成兩足尊",
        "from": "T19n0950_005:0224a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說是經已。",
        "from": "T19n0950_005:0224a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛手祕密主諸大菩薩等苾芻。",
        "from": "T19n0950_005:0224a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及一切世間天龍藥叉乾闥婆等。",
        "from": "T19n0950_005:0224a14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛所說歡喜奉行。",
        "from": "T19n0950_005:0224a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩提場所說一字頂輪王經卷第五",
        "from": "T19n0950_005:0224a16_100",
        "context_with_next_sentence": false
    }
]