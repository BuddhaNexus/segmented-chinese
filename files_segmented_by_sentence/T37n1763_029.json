[
    {
        "source_sentence": "3大般涅槃經集解卷第二十九",
        "from": "T37n1763_029:0480c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　出功德天黑闇女譬　釋死苦",
        "from": "T37n1763_029:0480c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖行品之第三",
        "from": "T37n1763_029:0480c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉！",
        "from": "T37n1763_029:0480c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生之根本凡有如是至觀於所生已見過患。",
        "from": "T37n1763_029:0480c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案僧亮曰：",
        "from": "T37n1763_029:0480c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以凡夫顛倒貪生而惡老死，明菩薩不爾也。",
        "from": "T37n1763_029:0480c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰：",
        "from": "T37n1763_029:0480c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三，寄觀行門明八苦相也。",
        "from": "T37n1763_029:0480c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中有六譬，皆明凡夫貪生；",
        "from": "T37n1763_029:0480c09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而菩薩見過，棄而不樂也。",
        "from": "T37n1763_029:0480c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰：",
        "from": "T37n1763_029:0480c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此下第二，觀過也，有六譬也。",
        "from": "T37n1763_029:0480c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉！",
        "from": "T37n1763_029:0480c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如有女人入於他舍至散華供養恭敬禮拜。",
        "from": "T37n1763_029:0480c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案僧亮曰：",
        "from": "T37n1763_029:0480c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為菩薩作譬也。",
        "from": "T37n1763_029:0480c13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩愛生情薄，見生死過，則俱厭也。",
        "from": "T37n1763_029:0480c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生是貪悅之本，譬女。",
        "from": "T37n1763_029:0480c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五陰是生所住處，譬舍。",
        "from": "T37n1763_029:0480c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘因得果，譬之他也。",
        "from": "T37n1763_029:0480c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生理可樂，譬之端正。",
        "from": "T37n1763_029:0480c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智以御身，譬之舍主。",
        "from": "T37n1763_029:0480c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名以表實，譬問名也。",
        "from": "T37n1763_029:0480c17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生是善果，其因淨故，譬功德也。",
        "from": "T37n1763_029:0480c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果非不淨，譬也。",
        "from": "T37n1763_029:0480c18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "考名責實，問所作也。",
        "from": "T37n1763_029:0480c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有生有實，譬能與也。",
        "from": "T37n1763_029:0480c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復於門外更見一女至汝若不去當斷汝命。",
        "from": "T37n1763_029:0480c20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案僧亮曰：",
        "from": "T37n1763_029:0480c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨生名死，譬門外也。",
        "from": "T37n1763_029:0480c21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "反生易顯，不須辭費也。",
        "from": "T37n1763_029:0480c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即持利刀者，醫藥救生，譬持利刀，以防死也。",
        "from": "T37n1763_029:0480c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰：",
        "from": "T37n1763_029:0480c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "死是人之所惡，悲情內故，譬之門外也。",
        "from": "T37n1763_029:0480c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "女人答言：",
        "from": "T37n1763_029:0480c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝甚愚癡至我皆不用各隨意去。",
        "from": "T37n1763_029:0480c25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案智秀曰：",
        "from": "T37n1763_029:0480c26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生在報前，譬姊。",
        "from": "T37n1763_029:0480c26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "死居報後，譬之妹也。",
        "from": "T37n1763_029:0480c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時二女便共相將至是故俱請令住我家。",
        "from": "T37n1763_029:0480c28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案僧宗曰：",
        "from": "T37n1763_029:0480c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡夫也，無法財，故喻貧也。",
        "from": "T37n1763_029:0480c29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉！",
        "from": "T37n1763_029:0481a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩摩訶薩亦復如是至是故貪愛生死二法。",
        "from": "T37n1763_029:0481a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案智秀曰：",
        "from": "T37n1763_029:0481a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雙合譬也。",
        "from": "T37n1763_029:0481a02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰：",
        "from": "T37n1763_029:0481a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此第一，以二女譬凡夫，與菩薩相對也。",
        "from": "T37n1763_029:0481a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次迦葉！",
        "from": "T37n1763_029:0481a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如婆羅門至如彼童子取菓還棄。",
        "from": "T37n1763_029:0481a04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案僧亮曰：",
        "from": "T37n1763_029:0481a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幼稚童子，譬始業菩薩見苦不深，未全忘生，如取菓已捨；",
        "from": "T37n1763_029:0481a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若智行俱深，則不取不捨也。",
        "from": "T37n1763_029:0481a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰：",
        "from": "T37n1763_029:0481a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩是高行之族，以譬婆羅門子。",
        "from": "T37n1763_029:0481a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曠劫為地獄所苦，絕人天食也。",
        "from": "T37n1763_029:0481a08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "糞譬陰身。",
        "from": "T37n1763_029:0481a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菓譬生也。",
        "from": "T37n1763_029:0481a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欣生之情，譬欲取也。",
        "from": "T37n1763_029:0481a09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生之為過，聖之所說，譬智訶也。",
        "from": "T37n1763_029:0481a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知貪生之患，譬𧹞然有愧也。",
        "from": "T37n1763_029:0481a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為生天中有五欲樂，譬淨洗也。",
        "from": "T37n1763_029:0481a11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終不愛者，譬還棄也。",
        "from": "T37n1763_029:0481a12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰：",
        "from": "T37n1763_029:0481a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此第二，以童子譬小菩薩，與大菩薩對也。",
        "from": "T37n1763_029:0481a13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次迦葉！",
        "from": "T37n1763_029:0481a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如有人至以其不見老病死故。",
        "from": "T37n1763_029:0481a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案僧亮曰。",
        "from": "T37n1763_029:0481a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四衢道，譬佛四無量無齊限也。",
        "from": "T37n1763_029:0481a15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "器譬經藏。",
        "from": "T37n1763_029:0481a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食譬施戒定。",
        "from": "T37n1763_029:0481a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛為在家之人，初讚施戒，生死之樂；",
        "from": "T37n1763_029:0481a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後說無常，讚涅槃樂，此法始終皆佛說。",
        "from": "T37n1763_029:0481a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡夫貪生，不見後過；",
        "from": "T37n1763_029:0481a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖人見過，不生貪也。",
        "from": "T37n1763_029:0481a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又四衢譬四辨。",
        "from": "T37n1763_029:0481a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "器譬名味句身。",
        "from": "T37n1763_029:0481a19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食譬施戒定也。",
        "from": "T37n1763_029:0481a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賣之譬勸行也。",
        "from": "T37n1763_029:0481a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠來飢虛，譬經三途久也。",
        "from": "T37n1763_029:0481a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是何物者，稱其所求，問以定之。",
        "from": "T37n1763_029:0481a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食主答言者，戒定能得天人之樂，後必無常也。",
        "from": "T37n1763_029:0481a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人聞已者，不為菩薩說也。",
        "from": "T37n1763_029:0481a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多與我價者，生天不受四趣之苦也。",
        "from": "T37n1763_029:0481a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰：",
        "from": "T37n1763_029:0481a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說法聚人之處，譬四衢也。",
        "from": "T37n1763_029:0481a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化主說人天因果，取人利養如賣也。",
        "from": "T37n1763_029:0481a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩雖知天中報妙，見有死患，不欲生故。",
        "from": "T37n1763_029:0481a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喻如彼人雖欲買食，為一惡故，所以遂捨也。",
        "from": "T37n1763_029:0481a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰：",
        "from": "T37n1763_029:0481a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此第三，以賣食譬釋教也。",
        "from": "T37n1763_029:0481a28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂若生死之果如此者，聖人何意說此五戒十善受人天報耶？",
        "from": "T37n1763_029:0481b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是善不受義之誠證也。",
        "from": "T37n1763_029:0481b01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此教本接下愚，若不聞人天之樂，凡夫豈背行道耶？",
        "from": "T37n1763_029:0481b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次迦葉！",
        "from": "T37n1763_029:0481b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如毒樹至下至十歲俱亦受苦。",
        "from": "T37n1763_029:0481b04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案僧亮曰：",
        "from": "T37n1763_029:0481b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此二譬同顯有生有死、脩短無異耳。",
        "from": "T37n1763_029:0481b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰：",
        "from": "T37n1763_029:0481b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四第五二譬，偏據生死果報唯苦而無樂。",
        "from": "T37n1763_029:0481b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次迦葉！",
        "from": "T37n1763_029:0481b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如譬嶮岸上有草至大涅槃經觀於生苦。",
        "from": "T37n1763_029:0481b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案僧亮曰：",
        "from": "T37n1763_029:0481b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬菩薩尚不貪天上報，況復欣此人中果耶？",
        "from": "T37n1763_029:0481b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰：",
        "from": "T37n1763_029:0481b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "險岸，譬五陰身也。",
        "from": "T37n1763_029:0481b10_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "草覆，譬無明也。",
        "from": "T37n1763_029:0481b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "岸邊甘露，譬妙藥報也。",
        "from": "T37n1763_029:0481b11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大坑，譬死苦也。",
        "from": "T37n1763_029:0481b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即便取者，貪樂果故，脩善因也。",
        "from": "T37n1763_029:0481b12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脚跌者，永淪生死也。",
        "from": "T37n1763_029:0481b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰：",
        "from": "T37n1763_029:0481b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為外道作譬也。",
        "from": "T37n1763_029:0481b13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "險岸譬非想也，草覆譬彼生死果報，而外道不知，計為涅槃也。",
        "from": "T37n1763_029:0481b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甘露者，譬禪定樂也。",
        "from": "T37n1763_029:0481b15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽千年者，八萬劫也。",
        "from": "T37n1763_029:0481b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貪其味者，著禪定也。",
        "from": "T37n1763_029:0481b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深坑者，生死苦也。",
        "from": "T37n1763_029:0481b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脚跌者，外道以非想為涅槃，失理故墮三塗也。",
        "from": "T37n1763_029:0481b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉！",
        "from": "T37n1763_029:0481b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何菩薩摩訶薩至大涅槃經觀於老苦。",
        "from": "T37n1763_029:0481b19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案僧亮曰：",
        "from": "T37n1763_029:0481b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆與衰壞作譬也。",
        "from": "T37n1763_029:0481b20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王譬壯色，敵國譬老，他土人民以譬憶念少盛之事也。",
        "from": "T37n1763_029:0481b21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉！",
        "from": "T37n1763_029:0481b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何菩薩摩訶薩至大涅槃經觀於病苦。",
        "from": "T37n1763_029:0481b22_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案僧亮曰：",
        "from": "T37n1763_029:0481b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端正譬健色，夫人譬愛結，王譬病也，挑一目等譬壞盛色。",
        "from": "T37n1763_029:0481b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉！",
        "from": "T37n1763_029:0481b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何菩薩摩訶薩至住於大乘大涅槃呪。",
        "from": "T37n1763_029:0481b25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案僧亮曰：",
        "from": "T37n1763_029:0481b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯除二禪者，凡死苦一至，無不惱亂，故以災為譬。",
        "from": "T37n1763_029:0481b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩死至，心不亂也。",
        "from": "T37n1763_029:0481b27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩羅毒蛇其色黑，若觸人衣，戒人衣觸其行處皆死。",
        "from": "T37n1763_029:0481b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名觸毒。",
        "from": "T37n1763_029:0481b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿竭多星，八月出也，因星結呪，消四毒也。",
        "from": "T37n1763_029:0481c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰：",
        "from": "T37n1763_029:0481c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "病之為苦，破壞浮囊實為最苦也。",
        "from": "T37n1763_029:0481c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次迦葉！",
        "from": "T37n1763_029:0481c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如有人至大涅槃經觀於死苦。",
        "from": "T37n1763_029:0481c03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案僧亮曰：",
        "from": "T37n1763_029:0481c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上來說死而未說苦。",
        "from": "T37n1763_029:0481c04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今言於險難處無有資糧者，諸受根滅，不受根起，名曰死也。",
        "from": "T37n1763_029:0481c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不受根起，則生塗斷絕，譬險難也。",
        "from": "T37n1763_029:0481c06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受根不起，不起則無法，無法則不可資用，譬無資糧也。",
        "from": "T37n1763_029:0481c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來塗茫茫，莫知所之，譬懸遠也。",
        "from": "T37n1763_029:0481c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五根皆滅，意識獨行，譬無伴侶。",
        "from": "T37n1763_029:0481c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "業風所飄，求生不住，譬常行也。",
        "from": "T37n1763_029:0481c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五道未分，譬不知邊際也。",
        "from": "T37n1763_029:0481c10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遇得生緣，乘癡受生，譬無燈明也。",
        "from": "T37n1763_029:0481c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上來諸譬說欲受生之苦，自此下譬死之苦也。",
        "from": "T37n1763_029:0481c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說一切苦皆由受生。",
        "from": "T37n1763_029:0481c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心滅名死，滅不由戶，譬無門也。",
        "from": "T37n1763_029:0481c13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不受根起，譬有處所也。",
        "from": "T37n1763_029:0481c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不覺苦樂，譬無痛處。",
        "from": "T37n1763_029:0481c14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "醫藥不加，譬不可治也。",
        "from": "T37n1763_029:0481c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸根漸滅，強力所不制，譬無遮止也。",
        "from": "T37n1763_029:0481c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受根已滅，不可更生，譬不得脫也。",
        "from": "T37n1763_029:0481c16_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生滅根異，外相不改，譬無破壞也。",
        "from": "T37n1763_029:0481c17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人不喜見，譬愁毒也。",
        "from": "T37n1763_029:0481c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能害人，譬非惡色也。",
        "from": "T37n1763_029:0481c18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人懼其禍，譬怖畏也。",
        "from": "T37n1763_029:0481c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是根皆滅，譬在身也。",
        "from": "T37n1763_029:0481c19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅不可見，譬不可覺知也。",
        "from": "T37n1763_029:0481c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大般涅槃經集解卷第二十九",
        "from": "T37n1763_029:0481c21_100",
        "context_with_next_sentence": false
    }
]