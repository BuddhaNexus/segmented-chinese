        "source_sentence": "三總結。",
        "from": "T85n2801_003:0886a11_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三依鈍根得沙門果及善巧立第三道分三。",
        "from": "T85n2801_003:0886a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一依得有學果不得無學果立道分三。",
        "from": "T85n2801_003:0886a13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一立因。",
        "from": "T85n2801_003:0886a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二顯果。",
        "from": "T85n2801_003:0886a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三明不證無學果。",
        "from": "T85n2801_003:0886a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二依善巧等立道分二。",
        "from": "T85n2801_003:0886a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一立因。",
        "from": "T85n2801_003:0886a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二顯果。",
        "from": "T85n2801_003:0886a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三總結　第三明習成相分二。",
        "from": "T85n2801_003:0886a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一問。",
        "from": "T85n2801_003:0886a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二答分三。",
        "from": "T85n2801_003:0886a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一依初道立果分二。",
        "from": "T85n2801_003:0886a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一立因。",
        "from": "T85n2801_003:0886a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二顯果。",
        "from": "T85n2801_003:0886a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二依第二三道立果分二。",
        "from": "T85n2801_003:0886a17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一立因。",
        "from": "T85n2801_003:0886a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二顯果分二。",
        "from": "T85n2801_003:0886a18_9",
