        "source_sentence": "二顯果。",
        "from": "T85n2801_003:0886a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三結二依中根順決釋分及六善巧立第二道相分三。",
        "from": "T85n2801_003:0886a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一依順決釋分立道分二。",
        "from": "T85n2801_003:0886a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一立因。",
        "from": "T85n2801_003:0886a10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二顯果。",
        "from": "T85n2801_003:0886a10_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二依善巧立道分二。",
        "from": "T85n2801_003:0886a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一立因。",
        "from": "T85n2801_003:0886a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二顯果。",
        "from": "T85n2801_003:0886a11_16",
        "context_with_next_sentence": true
    },
