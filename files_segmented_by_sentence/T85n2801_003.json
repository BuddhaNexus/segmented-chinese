[
    {
        "source_sentence": "瑜伽師論本地分中獨覺地分門記一卷",
        "from": "T85n2801_003:0885c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本事分獨覺地本地分第二廣解分十七中第十四釋獨覺地",
        "from": "T85n2801_003:0885c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分四。一明生起　二略答。三列名分五如論。",
        "from": "T85n2801_003:0885c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四別釋分五　第一明種姓相分三。",
        "from": "T85n2801_003:0885c29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一總問。",
        "from": "T85n2801_003:0885c29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二略答。",
        "from": "T85n2801_003:0886a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三別釋分三。",
        "from": "T85n2801_003:0886a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一依薄塵明種姓分二。",
        "from": "T85n2801_003:0886a01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一立因。",
        "from": "T85n2801_003:0886a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二顯果分二。",
        "from": "T85n2801_003:0886a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一不利他。",
        "from": "T85n2801_003:0886a02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二貪自利。",
        "from": "T85n2801_003:0886a02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二依薄悲明種姓分二。",
        "from": "T85n2801_003:0886a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一立因。",
        "from": "T85n2801_003:0886a03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二顯果分二。",
        "from": "T85n2801_003:0886a03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一不利他。",
        "from": "T85n2801_003:0886a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二著自利。",
        "from": "T85n2801_003:0886a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三依中根慢行明種姓分二。",
        "from": "T85n2801_003:0886a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一立因。",
        "from": "T85n2801_003:0886a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二顯果分二。",
        "from": "T85n2801_003:0886a05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一明慢行果。",
        "from": "T85n2801_003:0886a05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二明中根果　第二明修道相分三。",
        "from": "T85n2801_003:0886a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一總問。",
