        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二依部行喻明共相雜住分二。",
        "from": "T85n2801_003:0886a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一明雜住義。",
        "from": "T85n2801_003:0886a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二指文　第五明遊行相分二。",
        "from": "T85n2801_003:0886a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一問。",
        "from": "T85n2801_003:0886a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二答分三。",
        "from": "T85n2801_003:0886a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一明善護身等功德。",
        "from": "T85n2801_003:0886a24_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二明不以語言化利有情因由分三。",
        "from": "T85n2801_003:0886a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一明正義。",
        "from": "T85n2801_003:0886a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二徵。",
        "from": "T85n2801_003:0886a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三答所以分二如論。",
        "from": "T85n2801_003:0886a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三明前三俱是趣寂之義。",
        "from": "T85n2801_003:0886a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已上獨覺地分門記究竟。",
        "from": "T85n2801_003:0886a28_10",
        "context_with_next_sentence": true
    }
