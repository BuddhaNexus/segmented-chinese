        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三別釋分三。",
        "from": "T85n2801_003:0886a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一明依利根求證及於蘊等善巧立初道相分三。",
        "from": "T85n2801_003:0886a08_8",
        "context_with_next_sentence": true
