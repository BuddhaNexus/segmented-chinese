        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二答分二。",
        "from": "T85n2801_003:0886a20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一依麟角喻明安住勝解等作意分二。",
        "from": "T85n2801_003:0886a21_15",
        "context_with_next_sentence": true
    },
