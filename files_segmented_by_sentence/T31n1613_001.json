{
    "T31n1613_001:0850c14_0": "大乘廣五蘊論一卷",
    "T31n1613_001:0850c16_0": "安慧菩薩造",
    "T31n1613_001:0850c17_0": "大唐中天竺國三藏地婆訶羅",
    "T31n1613_001:0850c18_0": "奉　詔譯",
    "T31n1613_001:0850c19_0": "佛說五蘊。",
    "T31n1613_001:0850c19_1": "謂色蘊。",
    "T31n1613_001:0850c19_2": "受蘊。",
    "T31n1613_001:0850c19_3": "想蘊。",
    "T31n1613_001:0850c19_4": "行蘊。",
    "T31n1613_001:0850c19_5": "識蘊。",
    "T31n1613_001:0850c19_6": "云何色蘊謂四大種。",
    "T31n1613_001:0850c20_7": "及大種所造色。",
    "T31n1613_001:0850c20_8": "云何四大種。",
    "T31n1613_001:0850c21_9": "謂地界。",
    "T31n1613_001:0850c21_10": "水界。",
    "T31n1613_001:0850c21_11": "火界。",
    "T31n1613_001:0850c21_12": "風界。",
    "T31n1613_001:0850c21_13": "此復云何。",
    "T31n1613_001:0850c21_14": "謂地堅性。",
    "T31n1613_001:0850c22_15": "水濕性。",
    "T31n1613_001:0850c22_16": "火煖性。",
    "T31n1613_001:0850c22_17": "風輕性。",
    "T31n1613_001:0850c22_18": "界者。",
    "T31n1613_001:0850c22_19": "能持自性所造色故。",
    "T31n1613_001:0850c24_0": "云何四大所造色。",
    "T31n1613_001:0850c24_1": "謂眼根。",
    "T31n1613_001:0850c24_2": "耳根。",
    "T31n1613_001:0850c24_3": "鼻根。",
    "T31n1613_001:0850c24_4": "舌根。",
    "T31n1613_001:0850c25_0": "身根。",
    "T31n1613_001:0850c25_1": "色聲香味。",
    "T31n1613_001:0850c25_2": "及觸一分。",
    "T31n1613_001:0850c25_3": "無表色等。",
    "T31n1613_001:0850c25_4": "造者因義。",
    "T31n1613_001:0850c26_5": "根者最勝自在義。",
    "T31n1613_001:0850c26_6": "主義。",
    "T31n1613_001:0850c26_7": "增上義。",
    "T31n1613_001:0850c26_8": "是為根義。",
    "T31n1613_001:0850c27_9": "所言主義。",
    "T31n1613_001:0850c27_10": "與誰為主。",
    "T31n1613_001:0850c27_11": "謂即眼根。",
    "T31n1613_001:0850c27_12": "與眼識為主。",
    "T31n1613_001:0850c28_13": "生眼識故。",
    "T31n1613_001:0850c28_14": "如是乃至身根。",
    "T31n1613_001:0850c28_15": "與身識為主。",
    "T31n1613_001:0850c29_0": "生身識故。",
    "T31n1613_001:0851a01_0": "云何眼根。",
    "T31n1613_001:0851a01_1": "謂以色為境。",
    "T31n1613_001:0851a01_2": "淨色為性。",
    "T31n1613_001:0851a01_3": "謂於眼中。",
    "T31n1613_001:0851a02_0": "一分淨色如淨醍醐。",
    "T31n1613_001:0851a02_1": "此性有故。",
    "T31n1613_001:0851a02_2": "眼識得生。",
    "T31n1613_001:0851a02_3": "無即不生。",
    "T31n1613_001:0851a04_0": "云何耳根。",
    "T31n1613_001:0851a04_1": "謂以聲為境。",
    "T31n1613_001:0851a04_2": "淨色為性。",
    "T31n1613_001:0851a04_3": "謂於耳中。",
    "T31n1613_001:0851a05_0": "一分淨色。",
    "T31n1613_001:0851a05_1": "此性有故。",
    "T31n1613_001:0851a05_2": "耳識得生。",
    "T31n1613_001:0851a05_3": "無即不生。",
    "T31n1613_001:0851a06_0": "云何鼻根。",
    "T31n1613_001:0851a06_1": "謂以香為境。",
    "T31n1613_001:0851a06_2": "淨色為性。",
    "T31n1613_001:0851a06_3": "謂於鼻中。",
    "T31n1613_001:0851a07_0": "一分淨色。",
    "T31n1613_001:0851a07_1": "此性有故。",
    "T31n1613_001:0851a07_2": "鼻識得生。",
    "T31n1613_001:0851a07_3": "無即不生。",
    "T31n1613_001:0851a08_0": "云何舌根。",
    "T31n1613_001:0851a08_1": "謂以味為境。",
    "T31n1613_001:0851a08_2": "淨色為性。",
    "T31n1613_001:0851a08_3": "謂於舌上。",
    "T31n1613_001:0851a09_0": "周遍淨色。",
    "T31n1613_001:0851a09_1": "有說。",
    "T31n1613_001:0851a09_2": "此於舌上。",
    "T31n1613_001:0851a09_3": "有少不遍。",
    "T31n1613_001:0851a09_4": "如一毛端。",
    "T31n1613_001:0851a10_5": "此性有故。",
    "T31n1613_001:0851a10_6": "舌識得生。",
    "T31n1613_001:0851a10_7": "無即不生。",
    "T31n1613_001:0851a11_0": "云何身根。",
    "T31n1613_001:0851a11_1": "謂以觸為境。",
    "T31n1613_001:0851a11_2": "淨色為性。",
    "T31n1613_001:0851a11_3": "謂於身中。",
    "T31n1613_001:0851a12_0": "周遍淨色。",
    "T31n1613_001:0851a12_1": "此性有故。",
    "T31n1613_001:0851a12_2": "身識得生。",
    "T31n1613_001:0851a12_3": "無即不生。",
    "T31n1613_001:0851a13_0": "云何色。",
    "T31n1613_001:0851a13_1": "謂眼之境。",
    "T31n1613_001:0851a13_2": "顯色。",
    "T31n1613_001:0851a13_3": "形色。",
    "T31n1613_001:0851a13_4": "及表色等。",
    "T31n1613_001:0851a13_5": "顯色有四種。",
    "T31n1613_001:0851a14_6": "謂青黃赤白。",
    "T31n1613_001:0851a14_7": "形色。",
    "T31n1613_001:0851a14_8": "謂長短等。",
    "T31n1613_001:0851a15_0": "云何聲。",
    "T31n1613_001:0851a15_1": "謂耳之境。",
    "T31n1613_001:0851a15_2": "執受大種因聲。",
    "T31n1613_001:0851a15_3": "非執受大種因聲。",
    "T31n1613_001:0851a16_4": "俱大種因聲。",
    "T31n1613_001:0851a17_0": "諸心心法。",
    "T31n1613_001:0851a17_1": "是能執受。",
    "T31n1613_001:0851a17_2": "蠢動之類。",
    "T31n1613_001:0851a17_3": "是所執受。",
    "T31n1613_001:0851a18_0": "執受大種因聲者。",
    "T31n1613_001:0851a18_1": "如手相擊。",
    "T31n1613_001:0851a18_2": "語言等聲。",
    "T31n1613_001:0851a18_3": "非執受大種因聲者。",
    "T31n1613_001:0851a19_4": "如風林駛水等聲。",
    "T31n1613_001:0851a19_5": "俱大種因聲者。",
    "T31n1613_001:0851a20_6": "如手擊鼓等聲。",
    "T31n1613_001:0851a21_0": "云何香。",
    "T31n1613_001:0851a21_1": "謂鼻之境。",
    "T31n1613_001:0851a21_2": "好香．惡香．平等香。",
    "T31n1613_001:0851a21_3": "好香者。",
    "T31n1613_001:0851a22_4": "謂與鼻合時。",
    "T31n1613_001:0851a22_5": "於蘊相續。",
    "T31n1613_001:0851a22_6": "有所順益。",
    "T31n1613_001:0851a22_7": "惡香者。",
    "T31n1613_001:0851a23_8": "謂與鼻合時。",
    "T31n1613_001:0851a23_9": "於蘊相續。",
    "T31n1613_001:0851a23_10": "有所違損。",
    "T31n1613_001:0851a23_11": "平等香者。",
    "T31n1613_001:0851a24_12": "謂與鼻合時。",
    "T31n1613_001:0851a24_13": "無所損益。",
    "T31n1613_001:0851a25_0": "云何味。",
    "T31n1613_001:0851a25_1": "謂舌之境。",
    "T31n1613_001:0851a25_2": "甘醋醎辛苦淡等。",
    "T31n1613_001:0851a26_0": "云何觸一分。",
    "T31n1613_001:0851a26_1": "謂身之境。",
    "T31n1613_001:0851a26_2": "除大種。",
    "T31n1613_001:0851a26_3": "謂滑性．澁性．重性．輕性．冷飢渴等。",
    "T31n1613_001:0851a28_0": "滑謂細軟。",
    "T31n1613_001:0851a28_1": "澁謂麁強。",
    "T31n1613_001:0851a28_2": "重謂可稱。",
    "T31n1613_001:0851a28_3": "輕謂反是。",
    "T31n1613_001:0851a28_4": "煖欲為冷。",
    "T31n1613_001:0851a29_5": "觸是冷因。",
    "T31n1613_001:0851a29_6": "此即於因。",
    "T31n1613_001:0851a29_7": "立其果稱。",
    "T31n1613_001:0851a29_8": "如說諸佛出世樂。",
    "T31n1613_001:0851b01_9": "演說正法樂。",
    "T31n1613_001:0851b01_10": "眾僧和合樂。",
    "T31n1613_001:0851b01_11": "同修精進樂。",
    "T31n1613_001:0851b02_12": "精進勤苦。",
    "T31n1613_001:0851b02_13": "雖是樂因。",
    "T31n1613_001:0851b02_14": "即說為樂。",
    "T31n1613_001:0851b02_15": "此亦如是。",
    "T31n1613_001:0851b03_16": "欲食為飢。",
    "T31n1613_001:0851b03_17": "欲飲為渴。",
    "T31n1613_001:0851b03_18": "說亦如是。",
    "T31n1613_001:0851b03_19": "已說七種造觸。",
    "T31n1613_001:0851b04_20": "及前四大十一種等。",
    "T31n1613_001:0851b05_0": "云何無表色等。",
    "T31n1613_001:0851b05_1": "謂有表業。",
    "T31n1613_001:0851b05_2": "三摩地所生。",
    "T31n1613_001:0851b05_3": "無見無對色等。",
    "T31n1613_001:0851b06_4": "有表業者。",
    "T31n1613_001:0851b06_5": "謂身語表。",
    "T31n1613_001:0851b06_6": "此通善不善無記性。",
    "T31n1613_001:0851b07_7": "所生色者。",
    "T31n1613_001:0851b07_8": "謂即從彼善不善表所生之色。",
    "T31n1613_001:0851b08_9": "此不可顯示故名無表。",
    "T31n1613_001:0851b08_10": "三摩地所生色者。",
    "T31n1613_001:0851b09_11": "謂四靜慮所生色等。",
    "T31n1613_001:0851b09_12": "此無表色。",
    "T31n1613_001:0851b09_13": "是所造性。",
    "T31n1613_001:0851b10_0": "名善律儀。",
    "T31n1613_001:0851b10_1": "不善律儀等亦名業。",
    "T31n1613_001:0851b10_2": "亦名種子。",
    "T31n1613_001:0851b10_3": "如是諸色。",
    "T31n1613_001:0851b11_4": "略為三種。",
    "T31n1613_001:0851b11_5": "一者可見有對。",
    "T31n1613_001:0851b11_6": "二者不可見有對。",
    "T31n1613_001:0851b12_7": "三者不可見無對。",
    "T31n1613_001:0851b12_8": "是中可見有對者。",
    "T31n1613_001:0851b13_0": "謂顯色等。",
    "T31n1613_001:0851b13_1": "不可見有對者。",
    "T31n1613_001:0851b13_2": "謂眼根等。",
    "T31n1613_001:0851b13_3": "不可見無對者。",
    "T31n1613_001:0851b14_4": "謂無表色等。",
    "T31n1613_001:0851b15_0": "云何受蘊。",
    "T31n1613_001:0851b15_1": "受有三種。",
    "T31n1613_001:0851b15_2": "謂樂受．苦受．不苦不樂受。",
    "T31n1613_001:0851b16_3": "樂受者。",
    "T31n1613_001:0851b16_4": "謂此滅時。",
    "T31n1613_001:0851b16_5": "有和合欲。",
    "T31n1613_001:0851b16_6": "苦受者。",
    "T31n1613_001:0851b17_0": "謂此生時。",
    "T31n1613_001:0851b17_1": "有乖離欲。",
    "T31n1613_001:0851b17_2": "不苦不樂受者。",
    "T31n1613_001:0851b17_3": "謂無二欲。",
    "T31n1613_001:0851b18_4": "無二欲者。",
    "T31n1613_001:0851b18_5": "謂無和合。",
    "T31n1613_001:0851b18_6": "及乖離欲。",
    "T31n1613_001:0851b18_7": "受謂識之領納。",
    "T31n1613_001:0851b20_0": "云何想蘊。",
    "T31n1613_001:0851b20_1": "謂能增勝。",
    "T31n1613_001:0851b20_2": "取諸境相。",
    "T31n1613_001:0851b20_3": "增勝取者。",
    "T31n1613_001:0851b20_4": "謂勝力能取。",
    "T31n1613_001:0851b21_5": "如大力者。",
    "T31n1613_001:0851b21_6": "說名勝力。",
    "T31n1613_001:0851b22_0": "云何行蘊。",
    "T31n1613_001:0851b22_1": "謂除受想。",
    "T31n1613_001:0851b22_2": "諸餘心法。",
    "T31n1613_001:0851b22_3": "及心不相應行。",
    "T31n1613_001:0851b23_4": "云何餘心法。",
    "T31n1613_001:0851b23_5": "謂與心相應諸行。",
    "T31n1613_001:0851b23_6": "觸．作意．思．欲．勝解．念．三摩地．慧．信．慚．愧．無貪．無瞋．無癡．精進．輕安．不放逸．捨．不害．貪．瞋．慢．無明．見．疑．無慚．無愧．昏沈．掉舉．不信．懈怠．放逸．失念．散亂．不正知．惡作．睡眠．尋伺。",
    "T31n1613_001:0851b29_0": "是諸心法。",
    "T31n1613_001:0851b29_1": "五是遍行。",
    "T31n1613_001:0851c01_0": "此遍一切善不善無記心故名遍行。",
    "T31n1613_001:0851c02_0": "五是別境。",
    "T31n1613_001:0851c03_0": "此五一一。",
    "T31n1613_001:0851c03_1": "於差別境。",
    "T31n1613_001:0851c03_2": "展轉決定。",
    "T31n1613_001:0851c03_3": "性不相離。",
    "T31n1613_001:0851c03_4": "是中有一。",
    "T31n1613_001:0851c04_5": "必有一切。",
    "T31n1613_001:0851c05_0": "十一為善。",
    "T31n1613_001:0851c05_1": "六為煩惱。",
    "T31n1613_001:0851c05_2": "餘是隨煩惱。",
    "T31n1613_001:0851c05_3": "四為不定。",
    "T31n1613_001:0851c06_0": "此不定四。",
    "T31n1613_001:0851c06_1": "非正隨煩惱以通善及無記性故。",
    "T31n1613_001:0851c07_0": "觸等體性及業。",
    "T31n1613_001:0851c07_1": "應當解釋。",
    "T31n1613_001:0851c08_0": "云何觸。",
    "T31n1613_001:0851c08_1": "謂三和合。",
    "T31n1613_001:0851c08_2": "分別為性。",
    "T31n1613_001:0851c09_0": "三和。",
    "T31n1613_001:0851c09_1": "謂眼色識。",
    "T31n1613_001:0851c09_2": "如是等。",
    "T31n1613_001:0851c09_3": "此諸和合心心法生故名為觸。",
    "T31n1613_001:0851c10_4": "與受所依為業。",
    "T31n1613_001:0851c11_0": "云何作意。",
    "T31n1613_001:0851c11_1": "謂令心發悟為性。",
    "T31n1613_001:0851c11_2": "令心心法現前警動。",
    "T31n1613_001:0851c12_3": "是憶念義任持攀緣心為業。",
    "T31n1613_001:0851c13_0": "云何思。",
    "T31n1613_001:0851c13_1": "謂於功德過失。",
    "T31n1613_001:0851c13_2": "及以俱非。",
    "T31n1613_001:0851c13_3": "令心造作意業為性。",
    "T31n1613_001:0851c15_0": "此性若有。",
    "T31n1613_001:0851c15_1": "識攀緣用。",
    "T31n1613_001:0851c15_2": "即現在前。",
    "T31n1613_001:0851c15_3": "猶如磁石引鐵令動。",
    "T31n1613_001:0851c16_4": "能推善不善無記心為業。",
    "T31n1613_001:0851c17_0": "云何欲。",
    "T31n1613_001:0851c17_1": "謂於可愛樂事。",
    "T31n1613_001:0851c17_2": "希望為性。",
    "T31n1613_001:0851c17_3": "愛樂事者。",
    "T31n1613_001:0851c18_0": "所謂可愛見聞等事。",
    "T31n1613_001:0851c18_1": "是願樂希求之義能與精進所依為業。",
    "T31n1613_001:0851c20_0": "云何勝解。",
    "T31n1613_001:0851c20_1": "謂於決定境。",
    "T31n1613_001:0851c20_2": "如所了知。",
    "T31n1613_001:0851c20_3": "印可為性。",
    "T31n1613_001:0851c21_0": "決定境者。",
    "T31n1613_001:0851c21_1": "謂於五蘊等。",
    "T31n1613_001:0851c21_2": "如日親說。",
    "T31n1613_001:0851c21_3": "色如聚沫。",
    "T31n1613_001:0851c22_4": "受如水泡。",
    "T31n1613_001:0851c22_5": "想如陽炎。",
    "T31n1613_001:0851c22_6": "行如芭蕉。",
    "T31n1613_001:0851c22_7": "識如幻境。",
    "T31n1613_001:0851c23_8": "如是決定。",
    "T31n1613_001:0851c23_9": "或如諸法所住自相。",
    "T31n1613_001:0851c23_10": "謂即如是而生決定。",
    "T31n1613_001:0851c24_11": "言決定者即印持義。",
    "T31n1613_001:0851c24_12": "餘無引轉為業。",
    "T31n1613_001:0851c25_13": "此增勝故。",
    "T31n1613_001:0851c25_14": "餘所不能引。",
    "T31n1613_001:0851c26_0": "云何念。",
    "T31n1613_001:0851c26_1": "謂於慣習事。",
    "T31n1613_001:0851c26_2": "心不忘失。",
    "T31n1613_001:0851c26_3": "明記為性。",
    "T31n1613_001:0851c26_4": "慣習事者。",
    "T31n1613_001:0851c27_5": "謂曾所習行。",
    "T31n1613_001:0851c27_6": "與不散亂所依為業。",
    "T31n1613_001:0851c28_0": "云何三摩地。",
    "T31n1613_001:0851c28_1": "謂於所觀事。",
    "T31n1613_001:0851c28_2": "心一境性。",
    "T31n1613_001:0851c28_3": "所觀事者。",
    "T31n1613_001:0851c29_4": "謂五蘊等。",
    "T31n1613_001:0851c29_5": "及無常苦空無我等。",
    "T31n1613_001:0851c29_6": "心一境者。",
    "T31n1613_001:0852a01_7": "是專注義。",
    "T31n1613_001:0852a01_8": "與智所依為業。",
    "T31n1613_001:0852a01_9": "由心定故。",
    "T31n1613_001:0852a01_10": "如實了知。",
    "T31n1613_001:0852a03_0": "云何慧。",
    "T31n1613_001:0852a03_1": "謂即於彼擇法為性。",
    "T31n1613_001:0852a03_2": "或如理所引。",
    "T31n1613_001:0852a03_3": "或不如理所引。",
    "T31n1613_001:0852a04_4": "或俱非所引。",
    "T31n1613_001:0852a05_0": "即於彼者。",
    "T31n1613_001:0852a05_1": "謂所觀事。",
    "T31n1613_001:0852a05_2": "擇法者。",
    "T31n1613_001:0852a05_3": "謂於諸法自相共相由慧簡擇。",
    "T31n1613_001:0852a06_4": "得決定故。",
    "T31n1613_001:0852a06_5": "如理所引者。",
    "T31n1613_001:0852a06_6": "謂佛弟子。",
    "T31n1613_001:0852a07_7": "不如理所引者。",
    "T31n1613_001:0852a07_8": "謂諸外道。",
    "T31n1613_001:0852a07_9": "俱非所引者。",
    "T31n1613_001:0852a08_0": "謂餘眾生。",
    "T31n1613_001:0852a08_1": "斷疑為業。",
    "T31n1613_001:0852a08_2": "慧能簡擇。",
    "T31n1613_001:0852a08_3": "於諸法中。",
    "T31n1613_001:0852a08_4": "得決定故。",
    "T31n1613_001:0852a10_0": "云何信。",
    "T31n1613_001:0852a10_1": "謂於業果諸諦寶等。",
    "T31n1613_001:0852a10_2": "深正符順。",
    "T31n1613_001:0852a10_3": "心淨為性。",
    "T31n1613_001:0852a12_0": "於業者。",
    "T31n1613_001:0852a12_1": "謂福。",
    "T31n1613_001:0852a12_2": "非福。",
    "T31n1613_001:0852a12_3": "不動業。",
    "T31n1613_001:0852a12_4": "於果者。",
    "T31n1613_001:0852a12_5": "謂須陀洹。",
    "T31n1613_001:0852a13_6": "斯陀含。",
    "T31n1613_001:0852a13_7": "阿那含。",
    "T31n1613_001:0852a13_8": "阿羅漢果。",
    "T31n1613_001:0852a13_9": "於諦者。",
    "T31n1613_001:0852a13_10": "謂苦集滅道諦。",
    "T31n1613_001:0852a14_11": "於寶者。",
    "T31n1613_001:0852a14_12": "謂佛法僧寶。",
    "T31n1613_001:0852a14_13": "於如是業果等。",
    "T31n1613_001:0852a15_14": "極相符順。",
    "T31n1613_001:0852a15_15": "亦名清淨。",
    "T31n1613_001:0852a15_16": "及希求義。",
    "T31n1613_001:0852a15_17": "與欲所依為業。",
    "T31n1613_001:0852a17_0": "云何慚。",
    "T31n1613_001:0852a17_1": "謂自增上。",
    "T31n1613_001:0852a17_2": "及法增上。",
    "T31n1613_001:0852a17_3": "於所作罪。",
    "T31n1613_001:0852a17_4": "羞恥為性。",
    "T31n1613_001:0852a18_5": "罪謂過失。",
    "T31n1613_001:0852a18_6": "智者所厭患故。",
    "T31n1613_001:0852a18_7": "羞恥者。",
    "T31n1613_001:0852a18_8": "謂不作眾罪。",
    "T31n1613_001:0852a19_9": "防息惡行。",
    "T31n1613_001:0852a19_10": "所依為業。",
    "T31n1613_001:0852a20_0": "云何愧。",
    "T31n1613_001:0852a20_1": "謂他增上。",
    "T31n1613_001:0852a20_2": "於所作罪。",
    "T31n1613_001:0852a20_3": "羞恥為性。",
    "T31n1613_001:0852a20_4": "他增上者謂怖畏責罰。",
    "T31n1613_001:0852a21_5": "及議論等。",
    "T31n1613_001:0852a21_6": "所有罪失。",
    "T31n1613_001:0852a21_7": "羞恥於他。",
    "T31n1613_001:0852a22_8": "業如慚說。",
    "T31n1613_001:0852a23_0": "云何無貪。",
    "T31n1613_001:0852a23_1": "謂貪對治。",
    "T31n1613_001:0852a23_2": "令深厭患。",
    "T31n1613_001:0852a23_3": "無著為性。",
    "T31n1613_001:0852a23_4": "謂於諸有。",
    "T31n1613_001:0852a24_5": "及有資具。",
    "T31n1613_001:0852a24_6": "染著為貪。",
    "T31n1613_001:0852a24_7": "彼之對治。",
    "T31n1613_001:0852a24_8": "說為無貪。",
    "T31n1613_001:0852a25_9": "此即於有。",
    "T31n1613_001:0852a25_10": "及有資具。",
    "T31n1613_001:0852a25_11": "無染著義。",
    "T31n1613_001:0852a25_12": "遍知生死諸過失故。",
    "T31n1613_001:0852a26_13": "名為厭患。",
    "T31n1613_001:0852a26_14": "惡行不起。",
    "T31n1613_001:0852a26_15": "所依為業。",
    "T31n1613_001:0852a28_0": "云何無瞋。",
    "T31n1613_001:0852a28_1": "謂瞋對治。",
    "T31n1613_001:0852a28_2": "以慈為性。",
    "T31n1613_001:0852a28_3": "謂於眾生。",
    "T31n1613_001:0852a28_4": "不損害義。",
    "T31n1613_001:0852a29_5": "業如無貪說。",
    "T31n1613_001:0852b01_0": "云何無癡。",
    "T31n1613_001:0852b01_1": "謂癡對治。",
    "T31n1613_001:0852b01_2": "如實正行為性。",
    "T31n1613_001:0852b01_3": "如實者略。",
    "T31n1613_001:0852b02_4": "謂四聖諦廣。",
    "T31n1613_001:0852b02_5": "謂十二緣起。",
    "T31n1613_001:0852b02_6": "於彼加行。",
    "T31n1613_001:0852b02_7": "是正知義。",
    "T31n1613_001:0852b03_8": "業亦如無貪說。",
    "T31n1613_001:0852b04_0": "云何精進。",
    "T31n1613_001:0852b04_1": "謂懈怠對治。",
    "T31n1613_001:0852b04_2": "善品現前。",
    "T31n1613_001:0852b04_3": "勤勇為性。",
    "T31n1613_001:0852b05_0": "謂若被甲。",
    "T31n1613_001:0852b05_1": "若加行。",
    "T31n1613_001:0852b05_2": "若無怯弱。",
    "T31n1613_001:0852b05_3": "若不退轉。",
    "T31n1613_001:0852b05_4": "若無喜足。",
    "T31n1613_001:0852b06_5": "是如此義。",
    "T31n1613_001:0852b06_6": "圓滿成就。",
    "T31n1613_001:0852b06_7": "善法為業。",
    "T31n1613_001:0852b07_0": "云何輕安。",
    "T31n1613_001:0852b07_1": "謂麁重對治。",
    "T31n1613_001:0852b07_2": "身心調暢。",
    "T31n1613_001:0852b07_3": "堪能為性。",
    "T31n1613_001:0852b08_0": "謂能棄捨十不善行。",
    "T31n1613_001:0852b08_1": "除障為業。",
    "T31n1613_001:0852b08_2": "由此力故。",
    "T31n1613_001:0852b08_3": "除一切障。",
    "T31n1613_001:0852b09_4": "轉捨麁重。",
    "T31n1613_001:0852b10_0": "云何不放逸。",
    "T31n1613_001:0852b10_1": "謂放逸對治。",
    "T31n1613_001:0852b10_2": "依止無貪。",
    "T31n1613_001:0852b10_3": "乃至精進。",
    "T31n1613_001:0852b11_4": "捨諸不善。",
    "T31n1613_001:0852b11_5": "修彼對治諸善法故。",
    "T31n1613_001:0852b11_6": "謂貪瞋癡。",
    "T31n1613_001:0852b12_0": "及以懈怠。",
    "T31n1613_001:0852b12_1": "名為放逸。",
    "T31n1613_001:0852b12_2": "對治彼故。",
    "T31n1613_001:0852b12_3": "是不放逸。",
    "T31n1613_001:0852b12_4": "謂依無貪．無瞋．無癡．精進四法。",
    "T31n1613_001:0852b13_5": "對治不善法。",
    "T31n1613_001:0852b14_0": "修習善法故。",
    "T31n1613_001:0852b14_1": "世出世間正行所依為業。",
    "T31n1613_001:0852b15_0": "云何捨。",
    "T31n1613_001:0852b15_1": "謂依如是無貪。",
    "T31n1613_001:0852b15_2": "無瞋。",
    "T31n1613_001:0852b15_3": "乃至精進。",
    "T31n1613_001:0852b15_4": "獲得心平等性。",
    "T31n1613_001:0852b16_5": "心正直性。",
    "T31n1613_001:0852b16_6": "心無功用性。",
    "T31n1613_001:0852b16_7": "又復由此。",
    "T31n1613_001:0852b17_0": "離諸雜染法。",
    "T31n1613_001:0852b17_1": "安住清淨法。",
    "T31n1613_001:0852b17_2": "謂依無貪。",
    "T31n1613_001:0852b17_3": "無瞋無癡。",
    "T31n1613_001:0852b18_4": "精進性故。",
    "T31n1613_001:0852b18_5": "或時遠離昏沈掉舉諸過失故。",
    "T31n1613_001:0852b19_0": "初得心平等。",
    "T31n1613_001:0852b19_1": "或時任運無勉勵故。",
    "T31n1613_001:0852b19_2": "次得心正直。",
    "T31n1613_001:0852b20_3": "或時遠離諸雜染故。",
    "T31n1613_001:0852b20_4": "最後獲得心無功用。",
    "T31n1613_001:0852b21_0": "業如不放逸說。",
    "T31n1613_001:0852b22_0": "云何不害。",
    "T31n1613_001:0852b22_1": "謂害對治。",
    "T31n1613_001:0852b22_2": "以悲為性。",
    "T31n1613_001:0852b22_3": "謂由悲故。",
    "T31n1613_001:0852b22_4": "不害群生。",
    "T31n1613_001:0852b23_5": "是無瞋分。",
    "T31n1613_001:0852b23_6": "不損惱為業。",
    "T31n1613_001:0852b24_0": "云何貪。",
    "T31n1613_001:0852b24_1": "謂於五取蘊。",
    "T31n1613_001:0852b24_2": "染愛耽著為性。",
    "T31n1613_001:0852b24_3": "謂此纏縛。",
    "T31n1613_001:0852b25_4": "輪迴三界。",
    "T31n1613_001:0852b25_5": "生苦為業。",
    "T31n1613_001:0852b25_6": "由愛力故。",
    "T31n1613_001:0852b25_7": "生五取蘊。",
    "T31n1613_001:0852b27_0": "云何瞋。",
    "T31n1613_001:0852b27_1": "謂於群生。",
    "T31n1613_001:0852b27_2": "損害為性。",
    "T31n1613_001:0852b27_3": "住不安隱。",
    "T31n1613_001:0852b27_4": "及惡行所依為業。",
    "T31n1613_001:0852b28_5": "不安隱者。",
    "T31n1613_001:0852b28_6": "謂損害他。",
    "T31n1613_001:0852b28_7": "自住苦故。",
    "T31n1613_001:0852b29_0": "云何慢。",
    "T31n1613_001:0852b29_1": "慢有七種。",
    "T31n1613_001:0852b29_2": "謂慢．過慢．過過慢．我慢．增上慢．卑慢．邪慢。",
    "T31n1613_001:0852c01_3": "云何慢。",
    "T31n1613_001:0852c01_4": "謂於劣計己勝。",
    "T31n1613_001:0852c02_0": "或於等計己等。",
    "T31n1613_001:0852c02_1": "如是心高舉為性。",
    "T31n1613_001:0852c02_2": "云何過慢。",
    "T31n1613_001:0852c03_0": "謂於等計己勝。",
    "T31n1613_001:0852c03_1": "或於勝計己等。",
    "T31n1613_001:0852c03_2": "如是心高舉為性。",
    "T31n1613_001:0852c04_3": "云何過過慢。",
    "T31n1613_001:0852c04_4": "謂於勝計己勝。",
    "T31n1613_001:0852c04_5": "如是心高舉為性。",
    "T31n1613_001:0852c05_6": "云何我慢。",
    "T31n1613_001:0852c05_7": "謂於五取蘊。",
    "T31n1613_001:0852c05_8": "隨計為我。",
    "T31n1613_001:0852c05_9": "或為我所。",
    "T31n1613_001:0852c06_10": "如是心高舉為性。",
    "T31n1613_001:0852c06_11": "云何增上慢。",
    "T31n1613_001:0852c06_12": "謂未得增上殊勝所證之法。",
    "T31n1613_001:0852c07_13": "謂我已得。",
    "T31n1613_001:0852c07_14": "如是心高舉為性。",
    "T31n1613_001:0852c08_15": "增上殊勝所證法者。",
    "T31n1613_001:0852c08_16": "謂諸聖果。",
    "T31n1613_001:0852c08_17": "及三摩地。",
    "T31n1613_001:0852c09_18": "三摩鉢底等。",
    "T31n1613_001:0852c09_19": "於彼未得。",
    "T31n1613_001:0852c09_20": "謂我已得。",
    "T31n1613_001:0852c09_21": "而自矜倨。",
    "T31n1613_001:0852c10_22": "云何卑慢謂於多分殊勝。",
    "T31n1613_001:0852c10_23": "計己少分下劣。",
    "T31n1613_001:0852c11_24": "如是心高舉為性。",
    "T31n1613_001:0852c11_25": "云何邪慢。",
    "T31n1613_001:0852c11_26": "謂實無德。",
    "T31n1613_001:0852c11_27": "計己有德。",
    "T31n1613_001:0852c12_28": "如是心高舉為性。",
    "T31n1613_001:0852c12_29": "下生敬重所依為業。",
    "T31n1613_001:0852c13_30": "謂於尊者。",
    "T31n1613_001:0852c13_31": "及有德者。",
    "T31n1613_001:0852c13_32": "而起倨慠。",
    "T31n1613_001:0852c13_33": "不生崇重。",
    "T31n1613_001:0852c15_0": "云何無明。",
    "T31n1613_001:0852c15_1": "謂於業果諦寶。",
    "T31n1613_001:0852c15_2": "無智為性。",
    "T31n1613_001:0852c15_3": "此有二種。",
    "T31n1613_001:0852c16_4": "一者俱生。",
    "T31n1613_001:0852c16_5": "二者分別。",
    "T31n1613_001:0852c16_6": "又欲界貪瞋。",
    "T31n1613_001:0852c16_7": "及以無明。",
    "T31n1613_001:0852c17_8": "為三不善根。",
    "T31n1613_001:0852c17_9": "謂貪不善根。",
    "T31n1613_001:0852c17_10": "瞋不善根。",
    "T31n1613_001:0852c17_11": "癡不善根。",
    "T31n1613_001:0852c18_12": "此復俱生。",
    "T31n1613_001:0852c18_13": "不俱生。",
    "T31n1613_001:0852c18_14": "分別所起。",
    "T31n1613_001:0852c18_15": "俱生者。",
    "T31n1613_001:0852c19_16": "謂禽獸等。",
    "T31n1613_001:0852c19_17": "不俱生者。",
    "T31n1613_001:0852c19_18": "謂貪相應等。",
    "T31n1613_001:0852c19_19": "分別者。",
    "T31n1613_001:0852c20_20": "謂諸見相應與虛妄決定。",
    "T31n1613_001:0852c20_21": "疑煩惱所依為業。",
    "T31n1613_001:0852c22_0": "云何見。",
    "T31n1613_001:0852c22_1": "見有五種。",
    "T31n1613_001:0852c22_2": "謂薩迦耶見．邊執見．邪見．見取．戒取。",
    "T31n1613_001:0852c23_3": "云何薩迦耶見。",
    "T31n1613_001:0852c23_4": "謂於五取蘊。",
    "T31n1613_001:0852c24_5": "隨執為我。",
    "T31n1613_001:0852c24_6": "或為我所。",
    "T31n1613_001:0852c24_7": "染慧為性。",
    "T31n1613_001:0852c24_8": "薩謂敗壞義。",
    "T31n1613_001:0852c25_9": "迦耶謂和合積聚義。",
    "T31n1613_001:0852c25_10": "即於此中。",
    "T31n1613_001:0852c25_11": "見一見常。",
    "T31n1613_001:0852c26_0": "異蘊有我蘊。",
    "T31n1613_001:0852c26_1": "為我所等。",
    "T31n1613_001:0852c26_2": "何故復如是說。",
    "T31n1613_001:0852c26_3": "謂薩者破常想。",
    "T31n1613_001:0852c27_4": "迦耶破一想。",
    "T31n1613_001:0852c27_5": "無常積集。",
    "T31n1613_001:0852c27_6": "是中無我及我所故。",
    "T31n1613_001:0852c28_7": "染慧者。",
    "T31n1613_001:0852c28_8": "謂煩惱俱。",
    "T31n1613_001:0852c28_9": "一切見品所依為業。",
    "T31n1613_001:0853a01_0": "云何邊執見。",
    "T31n1613_001:0853a01_1": "謂薩迦耶見增上力故。",
    "T31n1613_001:0853a01_2": "即於所取。",
    "T31n1613_001:0853a02_3": "或執為常。",
    "T31n1613_001:0853a02_4": "或執為斷。",
    "T31n1613_001:0853a02_5": "染慧為性。",
    "T31n1613_001:0853a02_6": "常邊者。",
    "T31n1613_001:0853a03_0": "謂執我自在。",
    "T31n1613_001:0853a03_1": "為遍常等。",
    "T31n1613_001:0853a03_2": "斷邊者。",
    "T31n1613_001:0853a03_3": "謂執有作者丈夫等彼死已不復生。",
    "T31n1613_001:0853a04_4": "如瓶既破更無盛用。",
    "T31n1613_001:0853a05_0": "障中道出離為業。",
    "T31n1613_001:0853a06_0": "云何邪見。",
    "T31n1613_001:0853a06_1": "謂謗因果。",
    "T31n1613_001:0853a06_2": "或謗作用。",
    "T31n1613_001:0853a06_3": "或壞善事。",
    "T31n1613_001:0853a06_4": "染慧為性。",
    "T31n1613_001:0853a07_5": "謗因者。",
    "T31n1613_001:0853a07_6": "因謂業煩惱性。",
    "T31n1613_001:0853a07_7": "合有五支。",
    "T31n1613_001:0853a07_8": "煩惱有三種。",
    "T31n1613_001:0853a08_9": "謂無明愛取。",
    "T31n1613_001:0853a08_10": "業有二種謂行及有。",
    "T31n1613_001:0853a09_0": "有者。",
    "T31n1613_001:0853a09_1": "謂依阿賴耶識諸業種子此亦名業。",
    "T31n1613_001:0853a09_2": "如世尊說。",
    "T31n1613_001:0853a10_3": "阿難。",
    "T31n1613_001:0853a10_4": "若業能與未來果彼亦名有。",
    "T31n1613_001:0853a10_5": "如是等。",
    "T31n1613_001:0853a11_6": "此謗名為謗因。",
    "T31n1613_001:0853a11_7": "謗果者。",
    "T31n1613_001:0853a11_8": "果有七支。",
    "T31n1613_001:0853a11_9": "謂識名色六處觸受生老死。",
    "T31n1613_001:0853a12_10": "此謗為謗果。",
    "T31n1613_001:0853a12_11": "或復謗無善行惡行。",
    "T31n1613_001:0853a13_12": "名為謗因。",
    "T31n1613_001:0853a13_13": "謗無善行惡行果報。",
    "T31n1613_001:0853a14_0": "名為謗果。",
    "T31n1613_001:0853a14_1": "謗無此世他世。",
    "T31n1613_001:0853a14_2": "無父無母。",
    "T31n1613_001:0853a14_3": "無化生眾生。",
    "T31n1613_001:0853a15_4": "此謗為謗作用。",
    "T31n1613_001:0853a15_5": "謂從此世往他世作用。",
    "T31n1613_001:0853a16_0": "種子任持作用。",
    "T31n1613_001:0853a16_1": "結生相續作用等。",
    "T31n1613_001:0853a16_2": "謗無世間阿羅漢等。",
    "T31n1613_001:0853a17_3": "為壞善事。",
    "T31n1613_001:0853a17_4": "斷善根為業。",
    "T31n1613_001:0853a17_5": "不善根堅固所依為業。",
    "T31n1613_001:0853a18_6": "又生不善。",
    "T31n1613_001:0853a18_7": "不生善為業。",
    "T31n1613_001:0853a19_0": "云何見取。",
    "T31n1613_001:0853a19_1": "謂於三見。",
    "T31n1613_001:0853a19_2": "及所依蘊。",
    "T31n1613_001:0853a19_3": "隨計為最為上為勝為極。",
    "T31n1613_001:0853a20_4": "染慧為性。",
    "T31n1613_001:0853a20_5": "三見者。",
    "T31n1613_001:0853a20_6": "謂薩迦耶。",
    "T31n1613_001:0853a21_0": "邊執。",
    "T31n1613_001:0853a21_1": "邪見。",
    "T31n1613_001:0853a21_2": "所依蘊者。",
    "T31n1613_001:0853a21_3": "即彼諸見所依之蘊。",
    "T31n1613_001:0853a21_4": "業如邪見說。",
    "T31n1613_001:0853a23_0": "云何戒禁取。",
    "T31n1613_001:0853a23_1": "謂於戒禁。",
    "T31n1613_001:0853a23_2": "及所依蘊。",
    "T31n1613_001:0853a23_3": "隨計為清淨為解脫。",
    "T31n1613_001:0853a24_4": "為出離。",
    "T31n1613_001:0853a24_5": "染慧為性。",
    "T31n1613_001:0853a24_6": "戒者。",
    "T31n1613_001:0853a24_7": "謂以惡見為先。",
    "T31n1613_001:0853a25_8": "離七種惡。",
    "T31n1613_001:0853a25_9": "禁者。",
    "T31n1613_001:0853a25_10": "謂牛狗等禁。",
    "T31n1613_001:0853a25_11": "及自拔髮。",
    "T31n1613_001:0853a26_12": "執三支杖僧佉定慧等。",
    "T31n1613_001:0853a26_13": "此非解脫之因。",
    "T31n1613_001:0853a27_0": "又計大自在或計世主。",
    "T31n1613_001:0853a27_1": "及入水火等。",
    "T31n1613_001:0853a27_2": "此非生天之因。",
    "T31n1613_001:0853a28_3": "如是等。",
    "T31n1613_001:0853a28_4": "彼計為因。",
    "T31n1613_001:0853a28_5": "所依蘊者。",
    "T31n1613_001:0853a28_6": "謂即戒禁所依之蘊。",
    "T31n1613_001:0853a29_7": "清淨者。",
    "T31n1613_001:0853a29_8": "謂即說此無間方便。",
    "T31n1613_001:0853a29_9": "以為清淨。",
    "T31n1613_001:0853b01_10": "解脫者。",
    "T31n1613_001:0853b01_11": "謂即以此解脫。",
    "T31n1613_001:0853b01_12": "煩惱出離者。",
    "T31n1613_001:0853b02_0": "謂即以此出離生死。",
    "T31n1613_001:0853b02_1": "是如此義。",
    "T31n1613_001:0853b02_2": "能與無果唐勞疲苦所依為業。",
    "T31n1613_001:0853b03_3": "無果唐勞者。",
    "T31n1613_001:0853b03_4": "謂此不能獲出苦義。",
    "T31n1613_001:0853b05_0": "云何疑。",
    "T31n1613_001:0853b05_1": "謂於諦寶等。",
    "T31n1613_001:0853b05_2": "為有為無。",
    "T31n1613_001:0853b05_3": "猶預為性。",
    "T31n1613_001:0853b06_0": "不生善法。",
    "T31n1613_001:0853b06_1": "所依為業。",
    "T31n1613_001:0853b06_2": "諸煩惱中。",
    "T31n1613_001:0853b06_3": "後三見及疑。",
    "T31n1613_001:0853b07_0": "唯分別起。",
    "T31n1613_001:0853b07_1": "餘通俱生。",
    "T31n1613_001:0853b07_2": "及分別起。",
    "T31n1613_001:0853b08_0": "云何忿。",
    "T31n1613_001:0853b08_1": "謂依現前不饒益事。",
    "T31n1613_001:0853b08_2": "心憤為性。",
    "T31n1613_001:0853b08_3": "能與暴惡。",
    "T31n1613_001:0853b09_4": "執持鞭杖。",
    "T31n1613_001:0853b09_5": "所依為業。",
    "T31n1613_001:0853b10_0": "云何恨。",
    "T31n1613_001:0853b10_1": "謂忿為先。",
    "T31n1613_001:0853b10_2": "結怨不捨為性。",
    "T31n1613_001:0853b10_3": "能與不忍。",
    "T31n1613_001:0853b11_0": "所依為業。",
    "T31n1613_001:0853b12_0": "云何覆。",
    "T31n1613_001:0853b12_1": "謂於過失。",
    "T31n1613_001:0853b12_2": "隱藏為性謂藏隱罪故。",
    "T31n1613_001:0853b12_3": "他正教誨時。",
    "T31n1613_001:0853b13_4": "不能發露。",
    "T31n1613_001:0853b13_5": "是癡之分。",
    "T31n1613_001:0853b13_6": "能與追悔。",
    "T31n1613_001:0853b14_0": "不安隱住。",
    "T31n1613_001:0853b14_1": "所依為業。",
    "T31n1613_001:0853b15_0": "云何惱。",
    "T31n1613_001:0853b15_1": "謂發暴惡言。",
    "T31n1613_001:0853b15_2": "陵犯為性。",
    "T31n1613_001:0853b15_3": "忿恨為先。",
    "T31n1613_001:0853b15_4": "心起損害。",
    "T31n1613_001:0853b16_5": "暴惡言者。",
    "T31n1613_001:0853b16_6": "謂切害麁獷。",
    "T31n1613_001:0853b16_7": "能與憂苦。",
    "T31n1613_001:0853b16_8": "不安隱住。",
    "T31n1613_001:0853b17_9": "所依為業。",
    "T31n1613_001:0853b17_10": "又能發生非福為業。",
    "T31n1613_001:0853b17_11": "起惡名稱為業。",
    "T31n1613_001:0853b19_0": "云何嫉。",
    "T31n1613_001:0853b19_1": "謂於他盛事。",
    "T31n1613_001:0853b19_2": "心妬為性。",
    "T31n1613_001:0853b19_3": "為名利故。",
    "T31n1613_001:0853b19_4": "於他盛事。",
    "T31n1613_001:0853b20_5": "不堪忍耐。",
    "T31n1613_001:0853b20_6": "妬忌心生。",
    "T31n1613_001:0853b20_7": "自住憂苦。",
    "T31n1613_001:0853b20_8": "所依為業。",
    "T31n1613_001:0853b22_0": "云何慳。",
    "T31n1613_001:0853b22_1": "謂施相違。",
    "T31n1613_001:0853b22_2": "心悋為性。",
    "T31n1613_001:0853b22_3": "謂於財等。",
    "T31n1613_001:0853b22_4": "生悋惜故。",
    "T31n1613_001:0853b23_5": "不能惠施。",
    "T31n1613_001:0853b23_6": "如是為慳。",
    "T31n1613_001:0853b23_7": "心遍執著利養眾具。",
    "T31n1613_001:0853b24_8": "是貪之分。",
    "T31n1613_001:0853b24_9": "與無厭足。",
    "T31n1613_001:0853b24_10": "所依為業。",
    "T31n1613_001:0853b24_11": "無厭足者。",
    "T31n1613_001:0853b25_12": "由慳悋故。",
    "T31n1613_001:0853b25_13": "非所用物。",
    "T31n1613_001:0853b25_14": "猶恒積聚。",
    "T31n1613_001:0853b26_0": "云何誑。",
    "T31n1613_001:0853b26_1": "謂矯妄於他詐現不實功德為性。",
    "T31n1613_001:0853b26_2": "是貪之分。",
    "T31n1613_001:0853b27_3": "能與邪命。",
    "T31n1613_001:0853b27_4": "所依為業。",
    "T31n1613_001:0853b28_0": "云何諂。",
    "T31n1613_001:0853b28_1": "謂矯設方便。",
    "T31n1613_001:0853b28_2": "隱己過惡。",
    "T31n1613_001:0853b28_3": "心曲為性。",
    "T31n1613_001:0853b28_4": "謂於名利。",
    "T31n1613_001:0853b29_5": "有所計著。",
    "T31n1613_001:0853b29_6": "是貪癡分。",
    "T31n1613_001:0853b29_7": "障正教誨為業。",
    "T31n1613_001:0853c01_0": "復由有罪。",
    "T31n1613_001:0853c01_1": "不自如實發露歸懺。",
    "T31n1613_001:0853c01_2": "不任教授。",
    "T31n1613_001:0853c02_0": "云何憍。",
    "T31n1613_001:0853c02_1": "謂於盛事。",
    "T31n1613_001:0853c02_2": "染著倨傲。",
    "T31n1613_001:0853c02_3": "能盡為性。",
    "T31n1613_001:0853c02_4": "盛事者。",
    "T31n1613_001:0853c03_5": "謂有漏盛事。",
    "T31n1613_001:0853c03_6": "染著倨傲者。",
    "T31n1613_001:0853c03_7": "謂於染愛。",
    "T31n1613_001:0853c03_8": "悅豫矜恃。",
    "T31n1613_001:0853c04_9": "是貪之分。",
    "T31n1613_001:0853c04_10": "能盡者。",
    "T31n1613_001:0853c04_11": "謂此能盡諸善根故。",
    "T31n1613_001:0853c06_0": "云何害。",
    "T31n1613_001:0853c06_1": "謂於眾生。",
    "T31n1613_001:0853c06_2": "損惱為性。",
    "T31n1613_001:0853c06_3": "是瞋之分。",
    "T31n1613_001:0853c06_4": "損惱者。",
    "T31n1613_001:0853c07_5": "謂加鞭杖等。",
    "T31n1613_001:0853c07_6": "即此所依為業。",
    "T31n1613_001:0853c08_0": "云何為慚。",
    "T31n1613_001:0853c08_1": "謂所作罪。",
    "T31n1613_001:0853c08_2": "不自羞恥為性。",
    "T31n1613_001:0853c08_3": "一切煩惱。",
    "T31n1613_001:0853c09_4": "及隨煩惱。",
    "T31n1613_001:0853c09_5": "助伴為業。",
    "T31n1613_001:0853c10_0": "云何無愧。",
    "T31n1613_001:0853c10_1": "謂所作罪。",
    "T31n1613_001:0853c10_2": "不羞他為性。",
    "T31n1613_001:0853c10_3": "業如無慚說。",
    "T31n1613_001:0853c12_0": "云何昏沈。",
    "T31n1613_001:0853c12_1": "謂心不調暢。",
    "T31n1613_001:0853c12_2": "無所堪任。",
    "T31n1613_001:0853c12_3": "蒙昧為性。",
    "T31n1613_001:0853c13_4": "是癡之分。",
    "T31n1613_001:0853c13_5": "與一切煩惱。",
    "T31n1613_001:0853c13_6": "及隨煩惱。",
    "T31n1613_001:0853c13_7": "所依為業。",
    "T31n1613_001:0853c15_0": "云何掉舉。",
    "T31n1613_001:0853c15_1": "謂隨憶念喜樂等事。",
    "T31n1613_001:0853c15_2": "心不寂靜為性。",
    "T31n1613_001:0853c16_3": "應知憶念先所遊戲歡笑等事。",
    "T31n1613_001:0853c16_4": "心不寂靜。",
    "T31n1613_001:0853c17_0": "是貪之分。",
    "T31n1613_001:0853c17_1": "障奢摩他為業。",
    "T31n1613_001:0853c18_0": "云何不信。",
    "T31n1613_001:0853c18_1": "謂信所治。",
    "T31n1613_001:0853c18_2": "於業果等。",
    "T31n1613_001:0853c18_3": "不正信順。",
    "T31n1613_001:0853c18_4": "心不清淨為性。",
    "T31n1613_001:0853c19_5": "能與懈怠。",
    "T31n1613_001:0853c19_6": "所依為業。",
    "T31n1613_001:0853c20_0": "云何懈怠。",
    "T31n1613_001:0853c20_1": "謂精進所治。",
    "T31n1613_001:0853c20_2": "於諸善品。",
    "T31n1613_001:0853c20_3": "心不勇進為性。",
    "T31n1613_001:0853c21_4": "能障勤修眾善為業。",
    "T31n1613_001:0853c22_0": "云何放逸。",
    "T31n1613_001:0853c22_1": "謂依貪瞋癡懈怠故。",
    "T31n1613_001:0853c22_2": "於諸煩惱。",
    "T31n1613_001:0853c22_3": "心不防護。",
    "T31n1613_001:0853c23_4": "於諸善品。",
    "T31n1613_001:0853c23_5": "不能修習為性。",
    "T31n1613_001:0853c23_6": "不善增長。",
    "T31n1613_001:0853c24_0": "善法退失。",
    "T31n1613_001:0853c24_1": "所依為業。",
    "T31n1613_001:0853c25_0": "云何失念。",
    "T31n1613_001:0853c25_1": "謂染污念。",
    "T31n1613_001:0853c25_2": "於諸善法。",
    "T31n1613_001:0853c25_3": "不能明記為性。",
    "T31n1613_001:0853c26_4": "染污念者。",
    "T31n1613_001:0853c26_5": "謂煩惱俱。",
    "T31n1613_001:0853c26_6": "於善不明記者。",
    "T31n1613_001:0853c26_7": "謂於正教授。",
    "T31n1613_001:0853c27_8": "不能憶持義能與散亂所依為業。",
    "T31n1613_001:0853c28_0": "云何散亂。",
    "T31n1613_001:0853c28_1": "謂貪瞋癡分。",
    "T31n1613_001:0853c28_2": "令心心法流散為性。",
    "T31n1613_001:0853c29_0": "能障離欲為業。",
    "T31n1613_001:0854a01_0": "云何不正知。",
    "T31n1613_001:0854a01_1": "謂煩惱相應慧。",
    "T31n1613_001:0854a01_2": "能起不正身語意行為性。",
    "T31n1613_001:0854a02_3": "違犯律行所依為業。",
    "T31n1613_001:0854a02_4": "謂於去來等。",
    "T31n1613_001:0854a03_0": "不正觀察故。",
    "T31n1613_001:0854a03_1": "而不能知應作不應作。",
    "T31n1613_001:0854a03_2": "致犯律儀。",
    "T31n1613_001:0854a05_0": "云何惡作。",
    "T31n1613_001:0854a05_1": "謂心變悔為性。",
    "T31n1613_001:0854a05_2": "謂惡所作故名惡作。",
    "T31n1613_001:0854a06_3": "此惡作體非即變悔。",
    "T31n1613_001:0854a06_4": "由先惡所作。",
    "T31n1613_001:0854a06_5": "後起追悔故。",
    "T31n1613_001:0854a07_6": "此即以果從因為目。",
    "T31n1613_001:0854a07_7": "故名惡作。",
    "T31n1613_001:0854a07_8": "譬如六觸處說為先業。",
    "T31n1613_001:0854a08_9": "此有二位。",
    "T31n1613_001:0854a08_10": "謂善不善。",
    "T31n1613_001:0854a08_11": "於二位中。",
    "T31n1613_001:0854a09_12": "復各有二。",
    "T31n1613_001:0854a09_13": "若善位中。",
    "T31n1613_001:0854a09_14": "先不作善。",
    "T31n1613_001:0854a09_15": "後起悔心。",
    "T31n1613_001:0854a10_16": "彼因是善。",
    "T31n1613_001:0854a10_17": "悔亦是善。",
    "T31n1613_001:0854a10_18": "若先作惡。",
    "T31n1613_001:0854a10_19": "後起悔心。",
    "T31n1613_001:0854a11_20": "彼因不善。",
    "T31n1613_001:0854a11_21": "悔即是善。",
    "T31n1613_001:0854a11_22": "若不善位。",
    "T31n1613_001:0854a11_23": "先不作惡。",
    "T31n1613_001:0854a12_24": "後起悔心。",
    "T31n1613_001:0854a12_25": "彼因不善。",
    "T31n1613_001:0854a12_26": "悔亦不善。",
    "T31n1613_001:0854a12_27": "若先作善。",
    "T31n1613_001:0854a13_28": "後起悔心。",
    "T31n1613_001:0854a13_29": "彼因是善。",
    "T31n1613_001:0854a13_30": "悔是不善。",
    "T31n1613_001:0854a14_0": "云何睡眠。",
    "T31n1613_001:0854a14_1": "謂不自在轉。",
    "T31n1613_001:0854a14_2": "昧略為性。",
    "T31n1613_001:0854a14_3": "不自在者。",
    "T31n1613_001:0854a15_0": "謂令心等不自在轉。",
    "T31n1613_001:0854a15_1": "是癡之分。",
    "T31n1613_001:0854a15_2": "又此自性不自在故。",
    "T31n1613_001:0854a16_3": "令心心法極成昧略。",
    "T31n1613_001:0854a16_4": "此善不善。",
    "T31n1613_001:0854a16_5": "及無記性。",
    "T31n1613_001:0854a17_6": "能與過失。",
    "T31n1613_001:0854a17_7": "所依為業。",
    "T31n1613_001:0854a18_0": "云何尋。",
    "T31n1613_001:0854a18_1": "謂思慧差別。",
    "T31n1613_001:0854a18_2": "意言尋求。",
    "T31n1613_001:0854a18_3": "令心麁相分別為性。",
    "T31n1613_001:0854a19_4": "意言者。",
    "T31n1613_001:0854a19_5": "謂是意識。",
    "T31n1613_001:0854a19_6": "是中或依思。",
    "T31n1613_001:0854a19_7": "或依慧而起。",
    "T31n1613_001:0854a20_8": "分別麁相者。",
    "T31n1613_001:0854a20_9": "謂尋求瓶衣車乘等之麁相。",
    "T31n1613_001:0854a21_10": "樂觸苦觸等所依為業。",
    "T31n1613_001:0854a22_0": "云何伺。",
    "T31n1613_001:0854a22_1": "謂思慧差別。",
    "T31n1613_001:0854a22_2": "意言伺察。",
    "T31n1613_001:0854a22_3": "令心細相分別為性。",
    "T31n1613_001:0854a23_4": "細相者。",
    "T31n1613_001:0854a23_5": "謂於瓶衣等。",
    "T31n1613_001:0854a23_6": "分別細相成不成等差別之義。",
    "T31n1613_001:0854a25_0": "云何心不相應行。",
    "T31n1613_001:0854a25_1": "謂依色心等。",
    "T31n1613_001:0854a25_2": "分位假立。",
    "T31n1613_001:0854a25_3": "謂此與彼不可施設。",
    "T31n1613_001:0854a26_4": "異不異性。",
    "T31n1613_001:0854a26_5": "此復云何。",
    "T31n1613_001:0854a26_6": "謂得．無想定．滅盡定．無想天．命根．眾同分．生．老．住．無常．名身．句身．文身．異生性。",
    "T31n1613_001:0854a28_7": "如是等。",
    "T31n1613_001:0854a29_0": "云何得。",
    "T31n1613_001:0854a29_1": "謂若獲。",
    "T31n1613_001:0854a29_2": "若成就。",
    "T31n1613_001:0854a29_3": "此復三種。",
    "T31n1613_001:0854a29_4": "謂種子成就。",
    "T31n1613_001:0854b01_5": "自在成就。",
    "T31n1613_001:0854b01_6": "現起成就。",
    "T31n1613_001:0854b01_7": "如其所應。",
    "T31n1613_001:0854b02_0": "云何無想定。",
    "T31n1613_001:0854b02_1": "謂離遍淨染未離上染。",
    "T31n1613_001:0854b02_2": "以出離想作意為先所有不恒行心心法滅為性。",
    "T31n1613_001:0854b04_0": "云何滅盡定。",
    "T31n1613_001:0854b04_1": "謂已離無所有處染。",
    "T31n1613_001:0854b04_2": "從第一有更起勝進。",
    "T31n1613_001:0854b05_3": "暫止息想作意為先。",
    "T31n1613_001:0854b05_4": "所有不恒行。",
    "T31n1613_001:0854b06_0": "及恒行一分。",
    "T31n1613_001:0854b06_1": "心心法滅為性不恒行。",
    "T31n1613_001:0854b06_2": "謂六轉識。",
    "T31n1613_001:0854b07_3": "恒行。",
    "T31n1613_001:0854b07_4": "謂攝藏識。",
    "T31n1613_001:0854b07_5": "及染污意。",
    "T31n1613_001:0854b07_6": "是中六轉識品。",
    "T31n1613_001:0854b08_7": "及染污意滅。",
    "T31n1613_001:0854b08_8": "皆滅盡定。",
    "T31n1613_001:0854b09_0": "云何無想天。",
    "T31n1613_001:0854b09_1": "謂無想定所得之果。",
    "T31n1613_001:0854b09_2": "生彼天已。",
    "T31n1613_001:0854b10_0": "所有不恒行。",
    "T31n1613_001:0854b10_1": "心心法滅為性。",
    "T31n1613_001:0854b11_0": "云何命根。",
    "T31n1613_001:0854b11_1": "謂於眾同分。",
    "T31n1613_001:0854b11_2": "先業所引住時分限為性。",
    "T31n1613_001:0854b13_0": "云何眾同分。",
    "T31n1613_001:0854b13_1": "謂諸群生各各自類相似為性。",
    "T31n1613_001:0854b14_0": "云何生。",
    "T31n1613_001:0854b14_1": "謂於眾同分。",
    "T31n1613_001:0854b14_2": "所有諸行。",
    "T31n1613_001:0854b14_3": "本無今有為性。",
    "T31n1613_001:0854b16_0": "云何老。",
    "T31n1613_001:0854b16_1": "謂彼諸行相續。",
    "T31n1613_001:0854b16_2": "變壞為性。",
    "T31n1613_001:0854b17_0": "云何住。",
    "T31n1613_001:0854b17_1": "謂彼諸行相續。",
    "T31n1613_001:0854b17_2": "隨轉為性。",
    "T31n1613_001:0854b18_0": "云何無常。",
    "T31n1613_001:0854b18_1": "謂彼諸行相續。",
    "T31n1613_001:0854b18_2": "謝滅為性。",
    "T31n1613_001:0854b19_0": "云何名身。",
    "T31n1613_001:0854b19_1": "謂於諸法自性。",
    "T31n1613_001:0854b19_2": "增語為性。",
    "T31n1613_001:0854b19_3": "如說眼等。",
    "T31n1613_001:0854b21_0": "云何句身。",
    "T31n1613_001:0854b21_1": "謂於諸法差別增語為性。",
    "T31n1613_001:0854b21_2": "如說諸行無常等。",
    "T31n1613_001:0854b23_0": "云何文身。",
    "T31n1613_001:0854b23_1": "謂即諸字。",
    "T31n1613_001:0854b23_2": "此能表了前二性故。",
    "T31n1613_001:0854b23_3": "亦名顯。",
    "T31n1613_001:0854b24_4": "謂名句所依。",
    "T31n1613_001:0854b24_5": "顯了義故。",
    "T31n1613_001:0854b24_6": "亦名字。",
    "T31n1613_001:0854b24_7": "謂無異轉故。",
    "T31n1613_001:0854b25_8": "前二性者。",
    "T31n1613_001:0854b25_9": "謂詮自性及以差別。",
    "T31n1613_001:0854b25_10": "顯謂顯了。",
    "T31n1613_001:0854b27_0": "云何異生性。",
    "T31n1613_001:0854b27_1": "謂於聖法。",
    "T31n1613_001:0854b27_2": "不得為性。",
    "T31n1613_001:0854b28_0": "云何識蘊。",
    "T31n1613_001:0854b28_1": "謂於所緣。",
    "T31n1613_001:0854b28_2": "了別為性。",
    "T31n1613_001:0854b28_3": "亦名心。",
    "T31n1613_001:0854b28_4": "能採集故。",
    "T31n1613_001:0854b29_5": "亦名意。",
    "T31n1613_001:0854b29_6": "意所攝故。",
    "T31n1613_001:0854b29_7": "若最勝心。",
    "T31n1613_001:0854b29_8": "即阿賴耶識。",
    "T31n1613_001:0854c01_9": "此能採集諸行種子故。",
    "T31n1613_001:0854c01_10": "又此行相不可分別。",
    "T31n1613_001:0854c02_11": "前後一類相續轉故。",
    "T31n1613_001:0854c02_12": "又由此識從滅盡定。",
    "T31n1613_001:0854c03_0": "無想定。",
    "T31n1613_001:0854c03_1": "無想天起者。",
    "T31n1613_001:0854c03_2": "了別境界轉識。",
    "T31n1613_001:0854c03_3": "復生待所緣緣差別轉故。",
    "T31n1613_001:0854c04_4": "數數間斷還復生起。",
    "T31n1613_001:0854c04_5": "又令生死流轉迴還故。",
    "T31n1613_001:0854c05_6": "阿賴耶識者。",
    "T31n1613_001:0854c05_7": "謂能攝藏一切種子。",
    "T31n1613_001:0854c06_8": "又能攝藏我慢相故。",
    "T31n1613_001:0854c06_9": "又復緣身為境界故。",
    "T31n1613_001:0854c07_10": "又此亦名阿陀那識。",
    "T31n1613_001:0854c07_11": "執持身故。",
    "T31n1613_001:0854c07_12": "最勝意者。",
    "T31n1613_001:0854c08_13": "謂緣藏識為境之識。",
    "T31n1613_001:0854c08_14": "恒與我癡．我見．我慢．我愛相應。",
    "T31n1613_001:0854c09_15": "前後一類相續隨轉。",
    "T31n1613_001:0854c09_16": "除阿羅漢聖道。",
    "T31n1613_001:0854c10_17": "滅定現在前位。",
    "T31n1613_001:0854c10_18": "如是六轉識。",
    "T31n1613_001:0854c10_19": "及染污意。",
    "T31n1613_001:0854c11_0": "阿賴耶識。",
    "T31n1613_001:0854c11_1": "此八名識蘊。",
    "T31n1613_001:0854c12_0": "問蘊為何義。",
    "T31n1613_001:0854c12_1": "答積聚是蘊義。",
    "T31n1613_001:0854c12_2": "謂世間相續品類趣處差別色等總略攝故。",
    "T31n1613_001:0854c13_3": "如世尊說。",
    "T31n1613_001:0854c13_4": "比丘所有色。",
    "T31n1613_001:0854c14_5": "若過去。",
    "T31n1613_001:0854c14_6": "若未來。",
    "T31n1613_001:0854c14_7": "若現在。",
    "T31n1613_001:0854c14_8": "若內若外。",
    "T31n1613_001:0854c15_0": "若麁若細。",
    "T31n1613_001:0854c15_1": "若勝若劣。",
    "T31n1613_001:0854c15_2": "若近若遠。",
    "T31n1613_001:0854c15_3": "如是總攝為一色蘊。",
    "T31n1613_001:0854c17_0": "復有十二處。",
    "T31n1613_001:0854c17_1": "謂眼處．色處．耳處．聲處．鼻處．香處．舌處．味處．身處．觸處．意處．法處。",
    "T31n1613_001:0854c18_2": "眼等五處。",
    "T31n1613_001:0854c19_3": "及色聲香味處。",
    "T31n1613_001:0854c19_4": "如前已釋。",
    "T31n1613_001:0854c19_5": "觸處謂諸大種及一分觸。",
    "T31n1613_001:0854c20_6": "意處即是識蘊。",
    "T31n1613_001:0854c20_7": "法處謂受想行蘊。",
    "T31n1613_001:0854c21_8": "并無表色等。",
    "T31n1613_001:0854c21_9": "及諸無為。",
    "T31n1613_001:0854c22_0": "云何無為。",
    "T31n1613_001:0854c22_1": "謂虛空無為．非擇滅無為．擇滅無為。",
    "T31n1613_001:0854c23_2": "及真如等。",
    "T31n1613_001:0854c23_3": "虛空者。",
    "T31n1613_001:0854c23_4": "謂容受諸色。",
    "T31n1613_001:0854c23_5": "非擇滅者。",
    "T31n1613_001:0854c24_6": "謂若滅非離繫。",
    "T31n1613_001:0854c25_0": "云何非離繫。",
    "T31n1613_001:0854c25_1": "謂離煩惱對治諸蘊畢竟不生。",
    "T31n1613_001:0854c26_0": "云何擇滅。",
    "T31n1613_001:0854c26_1": "謂若滅是離繫。",
    "T31n1613_001:0854c27_0": "云何離繫。",
    "T31n1613_001:0854c27_1": "謂煩惱對治諸蘊畢竟不生。",
    "T31n1613_001:0854c28_0": "云何真如。",
    "T31n1613_001:0854c28_1": "謂諸法法性法無我性。",
    "T31n1613_001:0854c29_0": "問處為何義。",
    "T31n1613_001:0854c29_1": "答諸識生長門是處義。",
    "T31n1613_001:0855a01_0": "復有十八界。",
    "T31n1613_001:0855a01_1": "謂眼界．色界．眼識界．耳界．聲界．耳識界．鼻界．香界．鼻識界．舌界．味界．舌識界．身界．觸界．身識界．意界．法界．意識界。",
    "T31n1613_001:0855a04_0": "眼等諸界。",
    "T31n1613_001:0855a04_1": "及色等諸界。",
    "T31n1613_001:0855a04_2": "如處中說。",
    "T31n1613_001:0855a04_3": "六識界者。",
    "T31n1613_001:0855a05_0": "謂依眼等根。",
    "T31n1613_001:0855a05_1": "緣色等境。",
    "T31n1613_001:0855a05_2": "了別為性。",
    "T31n1613_001:0855a05_3": "意界者。",
    "T31n1613_001:0855a05_4": "即彼無間滅等。",
    "T31n1613_001:0855a06_5": "為顯第六識依止。",
    "T31n1613_001:0855a06_6": "及廣建立十八界故。",
    "T31n1613_001:0855a07_7": "如是色蘊即十處十界及法處法界一分。",
    "T31n1613_001:0855a08_8": "識蘊即意處及七心界。",
    "T31n1613_001:0855a08_9": "餘三蘊及色蘊一分。",
    "T31n1613_001:0855a09_10": "并諸無為。",
    "T31n1613_001:0855a09_11": "即法處法界。",
    "T31n1613_001:0855a10_0": "問界為何義。",
    "T31n1613_001:0855a10_1": "答任持無作用性自相是界義。",
    "T31n1613_001:0855a11_0": "問以何義故說蘊界處等。",
    "T31n1613_001:0855a11_1": "答對治三種我執故。",
    "T31n1613_001:0855a12_2": "所謂一性我執。",
    "T31n1613_001:0855a12_3": "受者我執。",
    "T31n1613_001:0855a12_4": "作者我執。",
    "T31n1613_001:0855a12_5": "如其次第。",
    "T31n1613_001:0855a14_0": "復次此十八界幾有色。",
    "T31n1613_001:0855a14_1": "謂十界一少分。",
    "T31n1613_001:0855a14_2": "即色蘊自性。",
    "T31n1613_001:0855a15_3": "幾無色。",
    "T31n1613_001:0855a15_4": "謂所餘界。",
    "T31n1613_001:0855a16_0": "幾有見。",
    "T31n1613_001:0855a16_1": "謂一色界。",
    "T31n1613_001:0855a16_2": "幾無見。",
    "T31n1613_001:0855a16_3": "謂所餘界。",
    "T31n1613_001:0855a17_0": "幾有對。",
    "T31n1613_001:0855a17_1": "謂十色界。",
    "T31n1613_001:0855a17_2": "若彼於此有所礙故。",
    "T31n1613_001:0855a17_3": "幾無對。",
    "T31n1613_001:0855a18_4": "謂所餘界。",
    "T31n1613_001:0855a19_0": "幾有漏。",
    "T31n1613_001:0855a19_1": "謂十五界。",
    "T31n1613_001:0855a19_2": "及後三少分。",
    "T31n1613_001:0855a19_3": "謂於是處煩惱起故。",
    "T31n1613_001:0855a20_4": "現所行處故。",
    "T31n1613_001:0855a20_5": "幾無漏。",
    "T31n1613_001:0855a20_6": "謂後三少分。",
    "T31n1613_001:0855a20_7": "幾欲界繫。",
    "T31n1613_001:0855a21_8": "謂一切。",
    "T31n1613_001:0855a21_9": "幾色界繫。",
    "T31n1613_001:0855a21_10": "謂十四。",
    "T31n1613_001:0855a21_11": "除香味及鼻舌識。",
    "T31n1613_001:0855a22_12": "幾無色界繫。",
    "T31n1613_001:0855a22_13": "謂後三。",
    "T31n1613_001:0855a22_14": "幾不繫。",
    "T31n1613_001:0855a22_15": "謂即彼無漏。",
    "T31n1613_001:0855a23_16": "幾蘊所攝。",
    "T31n1613_001:0855a23_17": "謂除無為。",
    "T31n1613_001:0855a23_18": "幾取蘊所攝。",
    "T31n1613_001:0855a24_19": "謂有漏。",
    "T31n1613_001:0855a24_20": "幾善。",
    "T31n1613_001:0855a24_21": "幾不善。",
    "T31n1613_001:0855a24_22": "幾無記。",
    "T31n1613_001:0855a24_23": "謂十通三性。",
    "T31n1613_001:0855a25_24": "七心界色聲。",
    "T31n1613_001:0855a25_25": "及法界一分。",
    "T31n1613_001:0855a25_26": "八無記性。",
    "T31n1613_001:0855a25_27": "幾是內。",
    "T31n1613_001:0855a26_28": "謂十二。",
    "T31n1613_001:0855a26_29": "除色聲香味觸及法界。",
    "T31n1613_001:0855a26_30": "幾是外。",
    "T31n1613_001:0855a27_0": "謂所餘六。",
    "T31n1613_001:0855a28_0": "幾有緣。",
    "T31n1613_001:0855a28_1": "謂七心界及法界少分。",
    "T31n1613_001:0855a28_2": "心所法性。",
    "T31n1613_001:0855a28_3": "幾無緣。",
    "T31n1613_001:0855a29_4": "謂餘十及法界少分。",
    "T31n1613_001:0855b01_0": "幾有分別。",
    "T31n1613_001:0855b01_1": "謂意識界。",
    "T31n1613_001:0855b01_2": "意界。",
    "T31n1613_001:0855b01_3": "及法界少分。",
    "T31n1613_001:0855b02_0": "幾有執受。",
    "T31n1613_001:0855b02_1": "謂五內界。",
    "T31n1613_001:0855b02_2": "及四界少分。",
    "T31n1613_001:0855b02_3": "謂色香味觸。",
    "T31n1613_001:0855b03_4": "幾非執受。",
    "T31n1613_001:0855b03_5": "謂餘九及四少分。",
    "T31n1613_001:0855b04_0": "幾同分。",
    "T31n1613_001:0855b04_1": "謂五內有色界。",
    "T31n1613_001:0855b04_2": "與彼自識等境界故。",
    "T31n1613_001:0855b05_0": "幾彼同分。",
    "T31n1613_001:0855b05_1": "謂彼自識空時。",
    "T31n1613_001:0855b05_2": "與自類等故。",
    "T31n1613_001:0855b06_0": "大乘廣五蘊論一卷"
}