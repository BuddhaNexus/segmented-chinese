[
    {
        "source_sentence": "撰集三藏及雜藏傳",
        "from": "T49n2026_001:0001a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不載譯人名附東晉錄",
        "from": "T49n2026_001:0001a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛涅槃後迦葉阿難於摩竭國僧伽尸城北",
        "from": "T49n2026_001:0001a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "撰集三藏及雜藏傳。",
        "from": "T49n2026_001:0001a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先禮佛已　　禮法眾僧　　各受集法",
        "from": "T49n2026_001:0001a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此諸法典　　除去五蓋　　一心聽受",
        "from": "T49n2026_001:0001a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說所聚法　　如阿難說　　當共信樂",
        "from": "T49n2026_001:0001a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是阿難智　　與佛同等　　聽集此法",
        "from": "T49n2026_001:0001a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如佛涅槃　　阿難付法　　愍念眾生",
        "from": "T49n2026_001:0001a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護持諸法　　如視世尊　　無上導士",
        "from": "T49n2026_001:0001a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "視阿難等　　福田無過　　設非阿難",
        "from": "T49n2026_001:0001a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋種涅槃　　正法滅盡　　并及三藏",
        "from": "T49n2026_001:0001a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛雨諸法　　哀念天人　　承以完器",
        "from": "T49n2026_001:0001a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難受持　　寄付慇懃　　阿難之法",
        "from": "T49n2026_001:0001a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此人乃復　　終不漏脫　　所聚法名",
        "from": "T49n2026_001:0001a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分別等法　　如師子吼　　阿難所說",
        "from": "T49n2026_001:0001a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一於前　　比丘各好　　阿難獨立",
        "from": "T49n2026_001:0001a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛自稱譽　　若過世智　　若復俗智",
        "from": "T49n2026_001:0001a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切皆知　　阿難如海　　汝等為此",
        "from": "T49n2026_001:0001a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信向阿難　　聽所集法　　悉斷眾苦",
        "from": "T49n2026_001:0001a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世明涅槃　　地為震動　　山海涌沸",
        "from": "T49n2026_001:0001a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天人哭泣　　世尊出晚　　涅槃何早",
        "from": "T49n2026_001:0001a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人天孤遺　　諸道荒塞　　神通徹視",
        "from": "T49n2026_001:0001a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神足羅漢　　皆詣拘夷　　供養世尊",
        "from": "T49n2026_001:0001a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八萬四千　　過於無漏　　天人所敬",
        "from": "T49n2026_001:0001a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福田無上　　閻浮利地　　羅漢悉來",
        "from": "T49n2026_001:0001a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊涅槃　　聚會於中　　別有不還",
        "from": "T49n2026_001:0001b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得頻來果　　溝港進學　　此輩一倍",
        "from": "T49n2026_001:0001b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡行比丘　　無數百千　　舉手哭行",
        "from": "T49n2026_001:0001b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊那去　　我等盲聾　　三毒未除",
        "from": "T49n2026_001:0001b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沒於五道　　誰當拔我　　比丘尼千",
        "from": "T49n2026_001:0001b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數三十五　　中有神通　　有漏無漏",
        "from": "T49n2026_001:0001b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝釋梵天　　及無數天　　文陀華伎",
        "from": "T49n2026_001:0001b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速持來到　　四天大王　　各有將從",
        "from": "T49n2026_001:0001b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "栴檀珠瓔　　速齎來詣　　及諸欲天",
        "from": "T49n2026_001:0001b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有色無色　　九十八億　　咸詣拘夷",
        "from": "T49n2026_001:0001b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無畏釋王　　八萬牙象　　名馬八萬",
        "from": "T49n2026_001:0001b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡詣拘夷　　舍衛月王　　強勇聰明",
        "from": "T49n2026_001:0001b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名象七萬　　皆到拘夷　　阿闍世王",
        "from": "T49n2026_001:0001b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勇猛信佛　　象七萬半　　悲泣來詣",
        "from": "T49n2026_001:0001b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "槃闍梨王　　名明端政　　象馬六萬",
        "from": "T49n2026_001:0001b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉詣拘夷　　名愍王煖　　欝光親厚",
        "from": "T49n2026_001:0001b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾各五萬　　皆來集會　　西香眾國",
        "from": "T49n2026_001:0001b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸王嚴仗　　四色軍服　　數百千眾",
        "from": "T49n2026_001:0001b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速疾來至　　欲見佛尸　　龍王泣啼",
        "from": "T49n2026_001:0001b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "追尋諸王　　維耶離眾　　象馬車乘",
        "from": "T49n2026_001:0001b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如諸天比　　來詣拘夷　　凡眾無數",
        "from": "T49n2026_001:0001b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并及清信　　覩佛盡今　　當覲涅槃",
        "from": "T49n2026_001:0001b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世界眾生　　三十六億　　皆詣拘夷",
        "from": "T49n2026_001:0001b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "齎所喪具　　方百由延　　拘夷處中",
        "from": "T49n2026_001:0001b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天人滿中　　地無空缺　　諸天散華",
        "from": "T49n2026_001:0001b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養佛尸　　鼓天伎樂　　雨諸花香",
        "from": "T49n2026_001:0001b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸王官屬　　來詣佛尸　　皆禮佛足",
        "from": "T49n2026_001:0001b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圍繞而立　　諸王號泣　　舉手悲號",
        "from": "T49n2026_001:0001b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不復見佛　　為眾說法　　阿闍世哭",
        "from": "T49n2026_001:0001b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來嗚佛足　　我五逆人　　持我付誰",
        "from": "T49n2026_001:0001c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸王繞尸　　散寶滿上　　並作眾樂",
        "from": "T49n2026_001:0001c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香泥塗床　　舉尸鐵棺　　灌滿麻油",
        "from": "T49n2026_001:0001c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地為震動　　諸王號哭　　諸天墮淚",
        "from": "T49n2026_001:0001c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "渧現於地　　天王雨淚　　普皆哭泣",
        "from": "T49n2026_001:0001c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天栴檀薪　　如意雜香　　積之于地",
        "from": "T49n2026_001:0001c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸王四輩　　大迦葉等　　僧眾坐上",
        "from": "T49n2026_001:0001c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生所供　　此大福田　　迦葉眾首",
        "from": "T49n2026_001:0001c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "丘尼賢者　　天帝人王　　各共然尸",
        "from": "T49n2026_001:0001c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "登燒身時　　天人號哭　　嗚呼痛哉",
        "from": "T49n2026_001:0001c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等如何　　七日供養　　諸天盡受",
        "from": "T49n2026_001:0001c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分為八分　　著甖第九　　各還本所",
        "from": "T49n2026_001:0001c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王及凡民　　諸天還上　　八部神歸",
        "from": "T49n2026_001:0001c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八分餘者　　天龍神得　　拘夷力人",
        "from": "T49n2026_001:0001c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得分立塔　　八分八塔　　第九甖塔",
        "from": "T49n2026_001:0001c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "炭塔第十　　閻浮所起　　迦葉僧首",
        "from": "T49n2026_001:0001c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行出拘夷　　詣摩竭國　　聚會眾僧",
        "from": "T49n2026_001:0001c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉語僧　　比丘莫行　　當共集會",
        "from": "T49n2026_001:0001c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿令法壞　　於虛空中　　迦葉鳴揵",
        "from": "T49n2026_001:0001c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲使法久　　勝於諸魔　　迦葉語眾",
        "from": "T49n2026_001:0001c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及天與人　　今當集法　　令眾生安",
        "from": "T49n2026_001:0001c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛雖涅槃　　四諦故存　　八道猶在",
        "from": "T49n2026_001:0001c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可獲涅槃　　先佛長壽　　人民有福",
        "from": "T49n2026_001:0001c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遺法日近　　不久便盡　　今人短命",
        "from": "T49n2026_001:0001c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛出第七　　涅槃之後　　法那久存",
        "from": "T49n2026_001:0001c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不還頻來　　見道未成　　今集真僧",
        "from": "T49n2026_001:0001c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此不得入　　天眼神足　　六通無我",
        "from": "T49n2026_001:0001c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切羅漢　　我今集之　　眾聞教聲",
        "from": "T49n2026_001:0001c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即各來集　　八十千眾　　皆是無漏",
        "from": "T49n2026_001:0001c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉問曰　　弟阿那律　　遍覩羅漢",
        "from": "T49n2026_001:0002a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誰不來者　　那律便觀　　大千世界",
        "from": "T49n2026_001:0002a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見忉利天　　有憍桓鉢　　律白迦葉",
        "from": "T49n2026_001:0002a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憍桓忉利　　羅漢無漏　　今不來會",
        "from": "T49n2026_001:0002a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉命召　　而亦不來　　世尊涅槃",
        "from": "T49n2026_001:0002a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世空何求　　迦葉心念　　比丘能有",
        "from": "T49n2026_001:0002a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "撰佛所說　　經法者不　　遍看不見",
        "from": "T49n2026_001:0002a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能集十二　　部經法義　　佛所說者",
        "from": "T49n2026_001:0002a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯有阿難　　是須陀洹　　當為設宜",
        "from": "T49n2026_001:0002a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使成無漏　　上座方便　　觀阿難心",
        "from": "T49n2026_001:0002a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知有慈愍　　得道未久　　迦葉語難",
        "from": "T49n2026_001:0002a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得入眾　　汝不應入　　今當出去",
        "from": "T49n2026_001:0002a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難白曰　　何耶上座　　我於三尊",
        "from": "T49n2026_001:0002a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有何過失　　迦葉答曰　　弟欲知耶",
        "from": "T49n2026_001:0002a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝於佛眾　　有大過失　　坐汝佛法",
        "from": "T49n2026_001:0002a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "減於千歲　　由汝勸佛　　度於母人",
        "from": "T49n2026_001:0002a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "細微之戒　　佛欲分別　　汝何不問",
        "from": "T49n2026_001:0002a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "細微戒耶　　爾時何念　　輕慢於戒",
        "from": "T49n2026_001:0002a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛已涅槃　　今當問誰　　蹈佛大衣",
        "from": "T49n2026_001:0002a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛渴索水　　汝竟不與　　非是過耶",
        "from": "T49n2026_001:0002a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此比過多　　是汝所作　　非口所陳",
        "from": "T49n2026_001:0002a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故當出　　阿難長歎　　悲惋墮淚",
        "from": "T49n2026_001:0002a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛方便終　　當何恃怙　　於此便去",
        "from": "T49n2026_001:0002a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐一樹下　　感結漏盡　　佛法由興",
        "from": "T49n2026_001:0002a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無數億天　　圍遶阿難　　來詣大會",
        "from": "T49n2026_001:0002a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師子無畏　　迦葉遙見　　便謂眾僧",
        "from": "T49n2026_001:0002a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆當速起　　阿難今至　　八萬餘眾",
        "from": "T49n2026_001:0002a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆是無垢　　迦葉上首　　叉手立迎",
        "from": "T49n2026_001:0002a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉舉手　　善來阿難　　便上高座",
        "from": "T49n2026_001:0002a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修理眾僧　　真大阿難　　為眾人眼",
        "from": "T49n2026_001:0002b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "侍佛已竟　　瞿曇福成　　汝觀此僧",
        "from": "T49n2026_001:0002b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并觀天眾　　世間久病　　有三苦患",
        "from": "T49n2026_001:0002b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間無主　　道御涅槃　　快共慈心",
        "from": "T49n2026_001:0002b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為眾說法　　阿難默然　　迦葉便請",
        "from": "T49n2026_001:0002b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上座欣笑　　作師子吼　　迦葉心念",
        "from": "T49n2026_001:0002b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相望能說　　阿難今日　　濟度天人",
        "from": "T49n2026_001:0002b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉舉聲　　大命眾生　　欲度世者",
        "from": "T49n2026_001:0002b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆來詣此　　如佛所說　　種種諸法",
        "from": "T49n2026_001:0002b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除眾生苦　　阿難當說　　一切知法",
        "from": "T49n2026_001:0002b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如散雜華　　阿難當撰　　分別三藏",
        "from": "T49n2026_001:0002b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來說法　　隨眾所欲　　高座阿難",
        "from": "T49n2026_001:0002b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復當敷演　　十方當聞　　天龍鬼神",
        "from": "T49n2026_001:0002b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四部弟子　　聞命即至　　如人熱渴",
        "from": "T49n2026_001:0002b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思想飲水　　奔走趣河　　當於中飲",
        "from": "T49n2026_001:0002b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天龍鬼神　　四輩弟子　　來趣阿難",
        "from": "T49n2026_001:0002b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中聞法　　餘無數眾　　進學見道",
        "from": "T49n2026_001:0002b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頻來不還　　尋聲後到　　阿那含道",
        "from": "T49n2026_001:0002b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二萬一千　　斯陀含眾　　四萬二千",
        "from": "T49n2026_001:0002b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須陀洹僧　　八萬四千　　此等後到",
        "from": "T49n2026_001:0002b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦欲聽法　　諸王皆集　　群臣兵眾",
        "from": "T49n2026_001:0002b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難儀容　　眾覩咸歡　　大眾次坐",
        "from": "T49n2026_001:0002b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方十二延　　阿難在中　　如月滿明",
        "from": "T49n2026_001:0002b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝釋在右　　梵天在左　　侍於阿難",
        "from": "T49n2026_001:0002b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如佛在時　　釋說偈讚　　天子欣悅",
        "from": "T49n2026_001:0002b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覩大會故　　阿難無畏　　如轉法輪",
        "from": "T49n2026_001:0002b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圍遶佛時　　瞻阿難顏　　儀容巍巍",
        "from": "T49n2026_001:0002b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵天亦爾　　請於如來　　世尊說法",
        "from": "T49n2026_001:0002b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難如是　　魔聞名聲　　亦來到此",
        "from": "T49n2026_001:0002b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并將妻子　　及臣兵眾　　波旬覩見",
        "from": "T49n2026_001:0002c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若干種眾　　阿難在中　　如日光明",
        "from": "T49n2026_001:0002c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難髻出　　儀似山頂　　項有日光",
        "from": "T49n2026_001:0002c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照於眾會　　難陀髻出　　迦葉項光",
        "from": "T49n2026_001:0002c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那律徹視　　觀於大千　　諸王在會",
        "from": "T49n2026_001:0002c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并及群臣　　眾人千億　　皆在此中",
        "from": "T49n2026_001:0002c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見會甚樂　　阿難勇猛　　波旬愁毒",
        "from": "T49n2026_001:0002c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心懷戰慓　　波旬心念　　一佛滅度",
        "from": "T49n2026_001:0002c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更有三出　　佛力勢大　　瞿曇涅槃",
        "from": "T49n2026_001:0002c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂呼得脫　　此三所得　　其處甚大",
        "from": "T49n2026_001:0002c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見佛滅度　　心甚喜悅　　瞿曇法衰",
        "from": "T49n2026_001:0002c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無怨仇對　　當設方宜　　滅此殘法",
        "from": "T49n2026_001:0002c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四部弟子　　及諸國王　　魔便出教",
        "from": "T49n2026_001:0002c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勅師子將　　速合四兵　　盡滅此法",
        "from": "T49n2026_001:0002c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即起化兵　　四種將主　　圍遶大會",
        "from": "T49n2026_001:0002c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出可畏聲　　收捕道人　　清信男女",
        "from": "T49n2026_001:0002c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誅殺諸王　　壞裂道場　　眾會驚愕",
        "from": "T49n2026_001:0002c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四輩心念　　此何從出　　未成懷疑",
        "from": "T49n2026_001:0002c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸王聞聲　　皆懷驚怖　　見魔兵眾",
        "from": "T49n2026_001:0002c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各自嚴仗　　阿難心惟　　誰來相嬈",
        "from": "T49n2026_001:0002c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覩此兵眾　　乃知魔為　　阿難便笑",
        "from": "T49n2026_001:0002c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勅王頓駕　　此亂眾者　　我自降之",
        "from": "T49n2026_001:0002c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難以慧力　　葉以進力　　申手執魔",
        "from": "T49n2026_001:0002c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三尸繫咽　　第一人尸　　第二狗尸",
        "from": "T49n2026_001:0002c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三蛇尸　　膖爛難近　　魔便首情",
        "from": "T49n2026_001:0002c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉見收　　羅漢應當　　困於人耶",
        "from": "T49n2026_001:0002c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉我前　　極撓嬈佛　　世尊未曾",
        "from": "T49n2026_001:0002c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見困如今　　答佛哀愍　　被大慈鎧",
        "from": "T49n2026_001:0002c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終不加害　　於諸群生　　我等集法",
        "from": "T49n2026_001:0002c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲令久興　　汝何為來　　撓攪我眾",
        "from": "T49n2026_001:0003a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魔即叉手　　啟阿難曰　　且俱放我",
        "from": "T49n2026_001:0003a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不復嬈人　　阿難使誓　　迦葉亦爾",
        "from": "T49n2026_001:0003a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若後亂眾　　尸還枷頸　　三尸化去",
        "from": "T49n2026_001:0003a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "波旬得免　　魔便愁怖　　別立一面",
        "from": "T49n2026_001:0003a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天稱善　　佛法得勝　　遺法久存",
        "from": "T49n2026_001:0003a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常當勝魔　　迦葉勅眾　　皆當靖定",
        "from": "T49n2026_001:0003a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難今說　　如佛所演　　諸王普起",
        "from": "T49n2026_001:0003a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "叉手待之　　諸天悉悅　　賢者視顏",
        "from": "T49n2026_001:0003a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉語難　　說經時到　　發此寶箱",
        "from": "T49n2026_001:0003a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯說上法　　何說增一　　何說增十",
        "from": "T49n2026_001:0003a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何說本起　　何說諸界　　阿難長笑",
        "from": "T49n2026_001:0003a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師子振欠　　四顧眾坐　　說聞如是",
        "from": "T49n2026_001:0003a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說一時已　　地為震動　　一億天人",
        "from": "T49n2026_001:0003a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逮得法眼　　舍衛增一　　名彼增十",
        "from": "T49n2026_001:0003a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋中本起　　魔竭諸界　　餘經亦爾",
        "from": "T49n2026_001:0003a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處處演說　　阿難以經　　為大眾說",
        "from": "T49n2026_001:0003a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡集諸經　　以為一藏　　律為二藏",
        "from": "T49n2026_001:0003a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大法三藏　　經錄阿含　　戒律大法",
        "from": "T49n2026_001:0003a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三分正等　　以為三藏　　已說大本",
        "from": "T49n2026_001:0003a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "錄諸異法　　合集眾雜　　復為一藏",
        "from": "T49n2026_001:0003a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別經四分　　名作阿含　　增一中含",
        "from": "T49n2026_001:0003a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長雜四含　　毘尼隨法　　犯次可生",
        "from": "T49n2026_001:0003a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中者久童　　苦行在後　　大法諸分",
        "from": "T49n2026_001:0003a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作所生名　　分別第一　　然後各異",
        "from": "T49n2026_001:0003a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增一中含　　長雜四含　　迦葉問難",
        "from": "T49n2026_001:0003a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此義何謂　　難答一一　　比丘念佛",
        "from": "T49n2026_001:0003a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以是調意　　故名增一　　此後二法",
        "from": "T49n2026_001:0003a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思惟善心　　兩法便生　　止意分別",
        "from": "T49n2026_001:0003a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三處三知　　宿命漏盡　　四處四諦",
        "from": "T49n2026_001:0003b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五處五根　　六處六大　　七處七覺",
        "from": "T49n2026_001:0003b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八處八懅　　九處九止　　十處十力",
        "from": "T49n2026_001:0003b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十一處經　　名放牛兒　　慈經斷後",
        "from": "T49n2026_001:0003b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增一經終　　從此義中　　當一一解",
        "from": "T49n2026_001:0003b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經若干　　故名增一　　猶如畫師",
        "from": "T49n2026_001:0003b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分部色像　　是一增一　　種種撰合",
        "from": "T49n2026_001:0003b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如藥無限　　隨病和合　　名某藥丸",
        "from": "T49n2026_001:0003b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名增一　　一一縷綖　　經緯成布",
        "from": "T49n2026_001:0003b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以一一說　　成於增一　　如合諸物",
        "from": "T49n2026_001:0003b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名空集音　　種種諸經　　故名增一",
        "from": "T49n2026_001:0003b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如草木　　埿土起牆　　圍覆於空",
        "from": "T49n2026_001:0003b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾乃成舍　　如是施戒　　生天涅槃",
        "from": "T49n2026_001:0003b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以義圍遶　　故名增一　　如一種泥",
        "from": "T49n2026_001:0003b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成數種器　　是一增一　　種種義合",
        "from": "T49n2026_001:0003b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從此當知　　種種義說　　此經盡出",
        "from": "T49n2026_001:0003b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當持增一　　亦不大長　　亦不至短",
        "from": "T49n2026_001:0003b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結義得偶　　名中阿含　　字亦不麁",
        "from": "T49n2026_001:0003b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不極細　　言義正等　　是故名中",
        "from": "T49n2026_001:0003b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除去上下　　說於中法　　棄於彼我",
        "from": "T49n2026_001:0003b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名中含　　小凶大凶　　癡疑盡壞",
        "from": "T49n2026_001:0003b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以觀正諦　　是故名中　　於中長說",
        "from": "T49n2026_001:0003b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并及先世　　劫世流轉　　是故曰長",
        "from": "T49n2026_001:0003b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "計於諸止　　天上快樂　　聞者歡喜",
        "from": "T49n2026_001:0003b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名曰長　　七世過佛　　及攬大乘",
        "from": "T49n2026_001:0003b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛之涅槃　　是故曰長　　諸寶計數",
        "from": "T49n2026_001:0003b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多有轉輪　　諸王喜聞　　故名曰長",
        "from": "T49n2026_001:0003b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此法當據　　學之喜忘　　欲斷諸結",
        "from": "T49n2026_001:0003b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故曰雜　　此法等含　　義味共俱",
        "from": "T49n2026_001:0003b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞之斷疑　　故名等含　　是修行地",
        "from": "T49n2026_001:0003c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪智所趣　　等見諸法　　是名等含",
        "from": "T49n2026_001:0003c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡此經中　　撮行兩端　　聞者多疑",
        "from": "T49n2026_001:0003c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名等含　　部外雜經　　諸天讚偈",
        "from": "T49n2026_001:0003c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆入其中　　故名等含　　附近法者",
        "from": "T49n2026_001:0003c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得好淨意　　斷諸諍訟　　故名戒律",
        "from": "T49n2026_001:0003c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分別中戒　　得淨精進　　聞者皆調",
        "from": "T49n2026_001:0003c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名戒律　　比丘行是　　得益於中",
        "from": "T49n2026_001:0003c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破碎眾結　　故名戒律　　忍諸結垢",
        "from": "T49n2026_001:0003c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比丘學是　　除諸惡行　　是名戒律",
        "from": "T49n2026_001:0003c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀視諸法　　從法得益　　墮甘露地",
        "from": "T49n2026_001:0003c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名毘尼　　迦栴造竟　　持用呈佛",
        "from": "T49n2026_001:0003c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言上法　　當名上法　　於中破癡",
        "from": "T49n2026_001:0003c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "益於世間　　此眾經明　　故名大法",
        "from": "T49n2026_001:0003c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總持外道　　斷於貢高　　眾法牙旗",
        "from": "T49n2026_001:0003c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名大法　　譬如明燈　　照於眾物",
        "from": "T49n2026_001:0003c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以見諸形　　故名大法　　此眾經義",
        "from": "T49n2026_001:0003c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如芒甘露　　是諸法味　　此大法義",
        "from": "T49n2026_001:0003c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸經戒律　　勤思惟持　　勿令放捨",
        "from": "T49n2026_001:0003c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫縛三藏　　分別字義　　比丘諸天",
        "from": "T49n2026_001:0003c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千萬稱善　　迦葉復問　　云何四藏",
        "from": "T49n2026_001:0003c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難可說　　為眾生故　　阿難答曰",
        "from": "T49n2026_001:0003c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此說各異　　隨眾意行　　是名雜藏",
        "from": "T49n2026_001:0003c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說宿緣　　羅漢亦說　　天梵外道",
        "from": "T49n2026_001:0003c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名雜藏　　中多偈頌　　問十二緣",
        "from": "T49n2026_001:0003c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此各異入　　是名雜藏　　三阿僧祇",
        "from": "T49n2026_001:0003c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩生中　　所生作緣　　故名三藏",
        "from": "T49n2026_001:0003c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中多宿緣　　多出所生　　與阿含異",
        "from": "T49n2026_001:0003c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名雜藏　　雜藏之法　　讚菩薩生",
        "from": "T49n2026_001:0003c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中諸義　　多於三藏　　都合諸法",
        "from": "T49n2026_001:0004a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結在一處　　何等比丘　　能盡持者",
        "from": "T49n2026_001:0004a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當來世時　　比丘多愚　　此輩不能",
        "from": "T49n2026_001:0004a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡持三藏　　後當作師　　從經出頌",
        "from": "T49n2026_001:0004a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此益增　　是故不合　　處處有喜",
        "from": "T49n2026_001:0004a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四阿含者　　或喜毘尼　　又喜大法",
        "from": "T49n2026_001:0004a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或喜外頌　　或喜雜藏　　故不一名",
        "from": "T49n2026_001:0004a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡說諸法　　結四阿含　　集錄諸數",
        "from": "T49n2026_001:0004a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并律大法　　聚為三藏　　聞是法已",
        "from": "T49n2026_001:0004a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天神及人　　三千比丘　　逮得漏盡",
        "from": "T49n2026_001:0004a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不還八千　　頻來十千　　無數天人",
        "from": "T49n2026_001:0004a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得見道迹　　此法久住　　為天人故",
        "from": "T49n2026_001:0004a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸王常勝　　盡受百秋　　一切天人",
        "from": "T49n2026_001:0004a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸王比丘　　皆共稱善　　如阿難說",
        "from": "T49n2026_001:0004a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "集法已訖　　天人各還　　四輩弟子",
        "from": "T49n2026_001:0004a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆歸本所佛涅槃後。",
        "from": "T49n2026_001:0004a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉阿難等。",
        "from": "T49n2026_001:0004a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於摩竭國僧伽尸城北。",
        "from": "T49n2026_001:0004a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造集三藏正經及雜藏經。",
        "from": "T49n2026_001:0004a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常所云四篋者。",
        "from": "T49n2026_001:0004a18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合雜言也。",
        "from": "T49n2026_001:0004a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡二百首盧。",
        "from": "T49n2026_001:0004a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上增一阿含。",
        "from": "T49n2026_001:0004a19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從一至十為十一處經者。",
        "from": "T49n2026_001:0004a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "撰諸十一事經。",
        "from": "T49n2026_001:0004a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以放牛兒十一事經為始。",
        "from": "T49n2026_001:0004a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以行慈十一事經為終。",
        "from": "T49n2026_001:0004a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因其所引便出其經。",
        "from": "T49n2026_001:0004a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以事相連故合為一卷。",
        "from": "T49n2026_001:0004a22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此放牛經者。",
        "from": "T49n2026_001:0004a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說放牛十一事。",
        "from": "T49n2026_001:0004a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以況比丘道具十一行。",
        "from": "T49n2026_001:0004a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成道樹根栽枝葉茂盛多所覆蔭。",
        "from": "T49n2026_001:0004a24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因放牛兒於坐發念。",
        "from": "T49n2026_001:0004a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛知其意故說十一事以所行者。",
        "from": "T49n2026_001:0004a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放牛者即解便逮羅漢。",
        "from": "T49n2026_001:0004a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "撰集三藏及雜藏傳",
        "from": "T49n2026_001:0004a27_100",
        "context_with_next_sentence": false
    }
]