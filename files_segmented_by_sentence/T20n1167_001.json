[
    {
        "source_sentence": "八大菩薩曼荼羅經",
        "from": "T20n1167_001:0675a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T20n1167_001:0675a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶贈司空諡大鑒正號",
        "from": "T20n1167_001:0675a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大廣智大興善寺三藏法師不空奉　詔譯如是我聞。",
        "from": "T20n1167_001:0675a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時薄伽梵。",
        "from": "T20n1167_001:0675a25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住補怛落伽山聖觀自在菩薩宮殿。",
        "from": "T20n1167_001:0675a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與百千俱胝那庾多菩薩前後圍遶。",
        "from": "T20n1167_001:0675a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時眾中有一菩薩。",
        "from": "T20n1167_001:0675a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名曰寶藏月光。",
        "from": "T20n1167_001:0675a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從座而起整理衣服。",
        "from": "T20n1167_001:0675a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偏袒右肩曲躬合掌。",
        "from": "T20n1167_001:0675a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白言薄伽梵我有少疑。",
        "from": "T20n1167_001:0675a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願如來聽許諮問。",
        "from": "T20n1167_001:0675b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是寶藏月光菩薩言。",
        "from": "T20n1167_001:0675b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人。",
        "from": "T20n1167_001:0675b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作八曼荼羅者云何建立。",
        "from": "T20n1167_001:0675b02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復依何法起無量福。",
        "from": "T20n1167_001:0675b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令修行者速證菩提。",
        "from": "T20n1167_001:0675b03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時如來讚寶藏月光菩薩言。",
        "from": "T20n1167_001:0675b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉善男子。",
        "from": "T20n1167_001:0675b04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能問如是甚深之義。",
        "from": "T20n1167_001:0675b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而為利益無量無邊有情與安樂故。",
        "from": "T20n1167_001:0675b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及能淨除三惡趣故。",
        "from": "T20n1167_001:0675b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為證無比無上智故。",
        "from": "T20n1167_001:0675b06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今善聽。",
        "from": "T20n1167_001:0675b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若諸有情纔聞此密言者。",
        "from": "T20n1167_001:0675b07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得長壽樂。",
        "from": "T20n1167_001:0675b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子有八曼荼羅。",
        "from": "T20n1167_001:0675b08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是八大菩薩甚深法要。",
        "from": "T20n1167_001:0675b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有有情。",
        "from": "T20n1167_001:0675b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依法建立此八曼荼羅一遍者。",
        "from": "T20n1167_001:0675b09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有十惡五逆謗方等經皆悉銷滅。",
        "from": "T20n1167_001:0675b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切所求義利勝願悉得成就。",
        "from": "T20n1167_001:0675b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說如來密言曰。",
        "from": "T20n1167_001:0675b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵摩訶尾囉娑嚩二合引賀引即曼荼囉中。",
        "from": "T20n1167_001:0675b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想於如來真金色身。",
        "from": "T20n1167_001:0675b13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十二相坐蓮華臺。",
        "from": "T20n1167_001:0675b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說觀自在菩薩密言曰。",
        "from": "T20n1167_001:0675b14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吽纈唎二合郝鉢納麼二合室哩二合曳娑嚩二合引訶即想曼荼羅中。",
        "from": "T20n1167_001:0675b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖觀自在赤色身。",
        "from": "T20n1167_001:0675b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持蓮華右手施願。",
        "from": "T20n1167_001:0675b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭冠中有無量壽如來。",
        "from": "T20n1167_001:0675b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說慈氏菩薩密言曰。",
        "from": "T20n1167_001:0675b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每訶哩爾娑嚩二合引訶於觀自在菩薩後。",
        "from": "T20n1167_001:0675b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想慈氏菩薩。",
        "from": "T20n1167_001:0675b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金色身左手執軍持右手施無畏。",
        "from": "T20n1167_001:0675b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冠中有窣堵波半跏坐。",
        "from": "T20n1167_001:0675b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說虛空藏菩薩密言曰。",
        "from": "T20n1167_001:0675b23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿引孽婆也　娑嚩二合引訶於佛背後。",
        "from": "T20n1167_001:0675b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想虛空藏菩薩。",
        "from": "T20n1167_001:0675b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持寶安於心上。",
        "from": "T20n1167_001:0675b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手施流出無量寶。",
        "from": "T20n1167_001:0675b26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說普賢菩薩密言曰。",
        "from": "T20n1167_001:0675b27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纈唎二合惹也娑嚩二合引訶虛空藏菩薩左邊。",
        "from": "T20n1167_001:0675b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想普賢菩薩。",
        "from": "T20n1167_001:0675b29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戴五佛冠金色身。",
        "from": "T20n1167_001:0675c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持劍左手施願。",
        "from": "T20n1167_001:0675c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "半跏而坐。",
        "from": "T20n1167_001:0675c01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說金剛手菩薩密言曰。",
        "from": "T20n1167_001:0675c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵鑁囉嚩娑嚩二合引訶於如來左邊。",
        "from": "T20n1167_001:0675c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想金剛手菩薩。",
        "from": "T20n1167_001:0675c04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手執金剛杵左手安於胯。",
        "from": "T20n1167_001:0675c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戴五佛冠。",
        "from": "T20n1167_001:0675c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身青色半跏而坐。",
        "from": "T20n1167_001:0675c05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說曼殊室利菩薩密言曰。",
        "from": "T20n1167_001:0675c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "室利二合闍㘕誐娑嚩二合引訶於金剛手菩薩前。",
        "from": "T20n1167_001:0675c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想曼殊室利童真菩薩。",
        "from": "T20n1167_001:0675c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五髻童子形。",
        "from": "T20n1167_001:0675c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手執青蓮花。",
        "from": "T20n1167_001:0675c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "花中有五股金剛杵。",
        "from": "T20n1167_001:0675c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手作施願。",
        "from": "T20n1167_001:0675c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身金色半跏而坐。",
        "from": "T20n1167_001:0675c10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說除蓋障菩薩密言曰。",
        "from": "T20n1167_001:0675c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "匿伐囉拏娑嚩二合訶引於曼殊室唎菩薩右。",
        "from": "T20n1167_001:0675c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想除蓋障菩薩。",
        "from": "T20n1167_001:0675c13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金色身。",
        "from": "T20n1167_001:0675c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持如意幢。",
        "from": "T20n1167_001:0675c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手施願半跏而坐。",
        "from": "T20n1167_001:0675c14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說地藏菩薩密言曰。",
        "from": "T20n1167_001:0675c15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乞灑二合訶囉惹娑嚩二合引賀引於如來前。",
        "from": "T20n1167_001:0675c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想地藏菩薩。",
        "from": "T20n1167_001:0675c17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭冠瓔珞面貌熙怡寂靜。",
        "from": "T20n1167_001:0675c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愍念一切有情。",
        "from": "T20n1167_001:0675c18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手安臍下拓鉢。",
        "from": "T20n1167_001:0675c18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手覆掌向下。",
        "from": "T20n1167_001:0675c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指捻頭指作安慰一切有情想。",
        "from": "T20n1167_001:0675c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此八大菩薩曼荼羅供養觀行法。",
        "from": "T20n1167_001:0675c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人。",
        "from": "T20n1167_001:0675c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持此八曼荼羅經。",
        "from": "T20n1167_001:0675c21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切業障悉皆銷滅。",
        "from": "T20n1167_001:0675c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速證無上正等菩提。",
        "from": "T20n1167_001:0675c22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說是經已諸大菩薩及聲聞眾。",
        "from": "T20n1167_001:0675c23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切天龍八部聞佛所說歡喜奉行。",
        "from": "T20n1167_001:0675c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八大菩薩讚。",
        "from": "T20n1167_001:0675c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓寂宮城門　　能摧戶扇者",
        "from": "T20n1167_001:0675c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛法受用　　救世我頂禮",
        "from": "T20n1167_001:0675c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自手流清水　　能除餓鬼渴",
        "from": "T20n1167_001:0675c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三界如意樹　　頂禮蓮華手",
        "from": "T20n1167_001:0675c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大慈水為心　　能息瞋恚火",
        "from": "T20n1167_001:0676a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂禮慈氏尊　　能斷欲弓弦",
        "from": "T20n1167_001:0676a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空藏妙慧　　虛空寂靜尊",
        "from": "T20n1167_001:0676a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生死流解脫　　頂禮佛心子",
        "from": "T20n1167_001:0676a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無邊有情惑　　能息無益心",
        "from": "T20n1167_001:0676a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普賢我頂禮　　善逝上首子",
        "from": "T20n1167_001:0676a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塵勞盡僮僕　　超勝摩羅軍",
        "from": "T20n1167_001:0676a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂禮金剛手　　能說一切明",
        "from": "T20n1167_001:0676a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂禮妙吉祥　　持妙童子形",
        "from": "T20n1167_001:0676a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舒遍智慧燈　　攘奪三界冥",
        "from": "T20n1167_001:0676a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切除蓋障　　是故我頂禮",
        "from": "T20n1167_001:0676a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無盡智慧尊　　能生無竭辯",
        "from": "T20n1167_001:0676a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如地諸有情　　所依一不斷",
        "from": "T20n1167_001:0676a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅慧悲愍藏　　地藏我頂禮",
        "from": "T20n1167_001:0676a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此真善逝子　　讚揚所獲福",
        "from": "T20n1167_001:0676a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此諸有情　　如彼成讚器",
        "from": "T20n1167_001:0676a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八大菩薩曼荼羅經此中八位文錯今依衡函尊勝佛頂念誦法中所明方位而正之云。",
