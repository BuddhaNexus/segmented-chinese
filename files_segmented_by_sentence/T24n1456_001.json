[
    {
        "source_sentence": "根本說一切有部毘奈耶．尼陀那",
        "from": "T24n1456_001:0517c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目得迦攝頌一卷",
        "from": "T24n1456_001:0517c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏法師義淨奉　制譯",
        "from": "T24n1456_001:0517c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尼陀那五十二頌。",
        "from": "T24n1456_001:0517c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目得迦四十八頌。",
        "from": "T24n1456_001:0517c07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大門總攝頌曰：",
        "from": "T24n1456_001:0517c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初明受近圓，　　次分亡人物，圓壇并戶鉤，　　菩薩像五門。",
        "from": "T24n1456_001:0517c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別門初總攝頌曰此有十事，盡不截皮：",
        "from": "T24n1456_001:0517c10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "近圓知日數，　　界別不入地，",
        "from": "T24n1456_001:0517c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "界邊五眾居，　　不截皮生肉。",
        "from": "T24n1456_001:0517c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一子攝頌曰：",
        "from": "T24n1456_001:0517c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "近圓男女狀，　　非近圓為師；",
        "from": "T24n1456_001:0517c13_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難等十無師，　　莫授我七歲。",
        "from": "T24n1456_001:0517c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二子攝頌曰：",
        "from": "T24n1456_001:0517c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日數每應知，　　告白夜須減；",
        "from": "T24n1456_001:0517c15_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六日十八日，　　說戒不應頻。",
        "from": "T24n1456_001:0517c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三子攝頌曰：",
        "from": "T24n1456_001:0517c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "界別不告淨，　　亦不為羯磨；",
        "from": "T24n1456_001:0517c17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘空不持欲，　　解前方結後。",
        "from": "T24n1456_001:0517c18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四子攝頌曰：",
        "from": "T24n1456_001:0517c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不入界捨界出放光說二頌，　　樹界有世尊；",
        "from": "T24n1456_001:0517c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不越及可越，　　羯磨者身死。",
        "from": "T24n1456_001:0517c20_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五子攝頌曰：",
        "from": "T24n1456_001:0517c21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地牆等秉事，　　結界無與欲；",
        "from": "T24n1456_001:0517c21_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但於一處坐，　　得為四羯磨。",
        "from": "T24n1456_001:0517c22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六子攝頌曰：",
        "from": "T24n1456_001:0517c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大界兩驛半，　　下水上山顛；",
        "from": "T24n1456_001:0517c23_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "異見明相過，　　五眾受七日。",
        "from": "T24n1456_001:0517c24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第七子攝頌曰：",
        "from": "T24n1456_001:0517c25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五眾坐安居，　　親等請日去；",
        "from": "T24n1456_001:0517c25_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於經有疑問，　　求解者應行。",
        "from": "T24n1456_001:0517c26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八子攝頌曰：",
        "from": "T24n1456_001:0517c27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "假令不截衣，　　有緣皆得著；",
        "from": "T24n1456_001:0517c27_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣可隨身量，　　若短作篅衣。",
        "from": "T24n1456_001:0517c28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第九子攝頌曰：",
        "from": "T24n1456_001:0517c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不畜五種皮，　　由有過失故；",
        "from": "T24n1456_001:0517c29_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開許得用處，　　齊坐臥容身。",
        "from": "T24n1456_001:0518a01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十子攝頌曰：",
        "from": "T24n1456_001:0518a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生肉及諸醋，　　有五種不同；",
        "from": "T24n1456_001:0518a02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "痔病爪不傷，　　迴施知希望。",
        "from": "T24n1456_001:0518a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別門第二總攝頌曰：",
        "from": "T24n1456_001:0518a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分亡及唱導，　　張衣授學人；",
        "from": "T24n1456_001:0518a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重作收攝驅，　　求寂同牆上。",
        "from": "T24n1456_001:0518a05_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一子攝頌曰：",
        "from": "T24n1456_001:0518a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分亡者衣物，　　互無應互取；",
        "from": "T24n1456_001:0518a06_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見鬪應須諫，　　隨頭向處分。",
        "from": "T24n1456_001:0518a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二子攝頌曰：",
        "from": "T24n1456_001:0518a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唱導乘與車，　　得衣應舉掌；",
        "from": "T24n1456_001:0518a08_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧伽獲衣利，　　凡聖可同分。",
        "from": "T24n1456_001:0518a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三子攝頌曰：",
        "from": "T24n1456_001:0518a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有張有不張，　　有出有不出；",
        "from": "T24n1456_001:0518a10_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若在於界外，　　聞生隨喜心。",
        "from": "T24n1456_001:0518a11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四子攝頌曰：",
        "from": "T24n1456_001:0518a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "授學等不秉，　　作法不成訶；",
        "from": "T24n1456_001:0518a12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二人成訶，　　不淨犯根本。",
        "from": "T24n1456_001:0518a13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五子攝頌曰：",
        "from": "T24n1456_001:0518a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更應重作法，　　勿使求寂行；",
        "from": "T24n1456_001:0518a14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "守護善用心，　　見處離聞處。",
        "from": "T24n1456_001:0518a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六子攝頌曰：",
        "from": "T24n1456_001:0518a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "收攝於界內，　　於眾心降伏；",
        "from": "T24n1456_001:0518a16_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "截柱及門框，　　尼等同驅擯。",
        "from": "T24n1456_001:0518a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第七子攝頌曰：",
        "from": "T24n1456_001:0518a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破戒應驅逐，　　伏處亦皆除；",
        "from": "T24n1456_001:0518a18_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惱俗應收謝，　　餘眾咸同此。",
        "from": "T24n1456_001:0518a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八子攝頌曰：",
        "from": "T24n1456_001:0518a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與求寂令怖，　　為受成近圓；",
        "from": "T24n1456_001:0518a20_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五法成就時，　　五夏離依去。",
        "from": "T24n1456_001:0518a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第九子攝頌曰：",
        "from": "T24n1456_001:0518a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同分非同分，　　有齊限及無；",
        "from": "T24n1456_001:0518a22_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有覆無覆殊，　　名一種便異。",
        "from": "T24n1456_001:0518a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十子攝頌曰：",
        "from": "T24n1456_001:0518a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不牆上行法，　　非於一二三；",
        "from": "T24n1456_001:0518a24_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不對破戒人，　　不取授學欲。",
        "from": "T24n1456_001:0518a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別門第三總攝頌曰：",
        "from": "T24n1456_001:0518a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓壇求寂墮，　　一衣烟藥器；",
        "from": "T24n1456_001:0518a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鐵椎髮及門，　　不應隨鐵作。",
        "from": "T24n1456_001:0518a27_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一子攝頌曰：",
        "from": "T24n1456_001:0518a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓壇及天廟，　　兩驛半依止；",
        "from": "T24n1456_001:0518a28_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無鉢不度人，　　鉢等不書字。",
        "from": "T24n1456_001:0518a29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二子攝頌曰：",
        "from": "T24n1456_001:0518b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求寂墮鉢破，　　開餘存念者；",
        "from": "T24n1456_001:0518b01_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作二種薰籠，　　并隨所須物。",
        "from": "T24n1456_001:0518b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三子攝頌曰：",
        "from": "T24n1456_001:0518b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一衣不互作，　　澡浴可遮人；",
        "from": "T24n1456_001:0518b03_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於褥不剃頭，　　病人隨服食。",
        "from": "T24n1456_001:0518b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四子攝頌曰：",
        "from": "T24n1456_001:0518b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烟筒壞色衣，　　鼻筒飲水器；",
        "from": "T24n1456_001:0518b05_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "針筒非寶物，　　眼藥合并椎。",
        "from": "T24n1456_001:0518b06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五子攝頌曰：",
        "from": "T24n1456_001:0518b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥器及𣰽毺，　　承足枮瀉藥；",
        "from": "T24n1456_001:0518b07_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苾芻不應作，　　當擇死人衣。",
        "from": "T24n1456_001:0518b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六子攝頌曰：",
        "from": "T24n1456_001:0518b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鐵椎并杵杓，　　身自不負擔；",
        "from": "T24n1456_001:0518b09_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以食供父母，　　毛毯不充衣。",
        "from": "T24n1456_001:0518b10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第七子攝頌曰：",
        "from": "T24n1456_001:0518b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髮爪窣覩波，　　任作鮮白色；",
        "from": "T24n1456_001:0518b11_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意安燈處，　　一畔出高簷。",
        "from": "T24n1456_001:0518b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八子攝頌曰：",
        "from": "T24n1456_001:0518b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "門戶并簷屋，　　及以塔下基；",
        "from": "T24n1456_001:0518b13_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "赤石紫礦塗，　　此等皆隨作。",
        "from": "T24n1456_001:0518b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第九子攝頌曰：",
        "from": "T24n1456_001:0518b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應以橛釘，　　及昇窣覩波；",
        "from": "T24n1456_001:0518b15_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開許金銀花，　　塔上以舍蓋。",
        "from": "T24n1456_001:0518b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十子攝頌曰：",
        "from": "T24n1456_001:0518b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鐵作窣覩波，　　及以金銀等；",
        "from": "T24n1456_001:0518b17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "許幡旗供養，　　并可用香油。",
        "from": "T24n1456_001:0518b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別門第四總攝頌曰：",
        "from": "T24n1456_001:0518b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戶鐶隨處用，　　霑衣大小便；",
        "from": "T24n1456_001:0518b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "染衣揁認衣，　　賒衣果無淨。",
        "from": "T24n1456_001:0518b20_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一子攝頌曰：",
        "from": "T24n1456_001:0518b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戶鐶倚帶網，　　取米為眾食；",
        "from": "T24n1456_001:0518b21_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寺內作私房，　　居人應受用。",
        "from": "T24n1456_001:0518b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二子攝頌曰：",
        "from": "T24n1456_001:0518b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨處當用物，　　營作人所須；",
        "from": "T24n1456_001:0518b23_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "器具食燈油，　　隨施主應用。",
        "from": "T24n1456_001:0518b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三子攝頌曰：",
        "from": "T24n1456_001:0518b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令雨霑僧物，　　夜半共分床；",
        "from": "T24n1456_001:0518b25_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小座並依年，　　敷席咸同此。",
        "from": "T24n1456_001:0518b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四子攝頌曰：",
        "from": "T24n1456_001:0518b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大小便利處，　　經行不惱他；",
        "from": "T24n1456_001:0518b27_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洗足及拭鞋，　　釜篦不奪用。",
        "from": "T24n1456_001:0518b28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五子攝頌曰：",
        "from": "T24n1456_001:0518b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "染釜及水瓶，　　僧鉢并飲器；",
        "from": "T24n1456_001:0518b29_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀石爪鼻物，　　支床不問年。",
        "from": "T24n1456_001:0518c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六子攝頌曰：",
        "from": "T24n1456_001:0518c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羯恥那衣揁，　　絣線正縫時；",
        "from": "T24n1456_001:0518c02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "染汁雜物等，　　用時不應奪。",
        "from": "T24n1456_001:0518c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第七子攝頌曰：",
        "from": "T24n1456_001:0518c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外道覆認衣，　　作記死時施；",
        "from": "T24n1456_001:0518c04_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有五種親友，　　得法獨應行。",
        "from": "T24n1456_001:0518c05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八子攝頌曰：",
        "from": "T24n1456_001:0518c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賒取他衣去，　　及為他和市；",
        "from": "T24n1456_001:0518c06_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不高下買衣，　　應二三酬價。",
        "from": "T24n1456_001:0518c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第九子攝頌曰：",
        "from": "T24n1456_001:0518c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果園差修理，　　四種不應分；",
        "from": "T24n1456_001:0518c08_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果熟現前分，　　觀時莫諠戲。",
        "from": "T24n1456_001:0518c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十子攝頌曰：",
        "from": "T24n1456_001:0518c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無淨人自行，　　自取不應食；",
        "from": "T24n1456_001:0518c10_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不選開其病，　　結界證耕人。",
        "from": "T24n1456_001:0518c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別門第五總攝頌曰：",
        "from": "T24n1456_001:0518c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩像供養，　　吉祥大眾食；",
        "from": "T24n1456_001:0518c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大會草稕居，　　集僧鳴大鼓。",
        "from": "T24n1456_001:0518c13_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一子攝頌曰：",
        "from": "T24n1456_001:0518c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聽為菩薩像，　　復許五種旗；",
        "from": "T24n1456_001:0518c14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為座置尊儀，　　鐵干隨意作。",
        "from": "T24n1456_001:0518c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二子攝頌曰：",
        "from": "T24n1456_001:0518c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養菩薩像，　　并作諸瓔珞；",
        "from": "T24n1456_001:0518c16_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗香及車輿，　　作傘蓋旗幡。",
        "from": "T24n1456_001:0518c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三子攝頌曰：",
        "from": "T24n1456_001:0518c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉祥并供養，　　花鬘及香合；",
        "from": "T24n1456_001:0518c18_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸人大集時，　　晝開門夜關。",
        "from": "T24n1456_001:0518c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四子攝頌曰：",
        "from": "T24n1456_001:0518c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大眾集會食，　　薜舍佉月生；",
        "from": "T24n1456_001:0518c20_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香臺五六年，　　並應為大會。",
        "from": "T24n1456_001:0518c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五子攝頌曰：",
        "from": "T24n1456_001:0518c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大會為草稕，　　不應雜亂坐；",
        "from": "T24n1456_001:0518c22_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應打揵稚鼓，　　告時令普知。",
        "from": "T24n1456_001:0518c23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六子攝頌曰：",
        "from": "T24n1456_001:0518c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "集僧鳴大鼓，　　供了去幢幡；",
        "from": "T24n1456_001:0518c24_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若多獲珍財，　　隨應悉分與。",
        "from": "T24n1456_001:0518c25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大門總攝頌曰目得迦四十八頌：",
        "from": "T24n1456_001:0518c26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最初為懺謝，　　第二定屬物，第三資具衣，　　目得迦總頌。",
        "from": "T24n1456_001:0518c27_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別門第一總攝頌曰：",
        "from": "T24n1456_001:0518c28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懺謝草田中，　　合免王影勝；",
        "from": "T24n1456_001:0518c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "糖酥根等聽，　　狗肉盞甘蔗。",
        "from": "T24n1456_001:0518c29_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一子攝頌曰：",
        "from": "T24n1456_001:0519a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懺謝非近圓，　　觀求寂相貌；",
        "from": "T24n1456_001:0519a01_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苾芻與尼法，　　若互秉皆成。",
        "from": "T24n1456_001:0519a02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二子攝頌曰：",
        "from": "T24n1456_001:0519a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "草田村略說，　　生心褒灑陀；",
        "from": "T24n1456_001:0519a03_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賊縛不同愆，　　六開僧教罪。",
        "from": "T24n1456_001:0519a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三子攝頌曰：",
        "from": "T24n1456_001:0519a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合免者應放，　　穿渠遣眾行；",
        "from": "T24n1456_001:0519a05_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日至四旬，　　皮肉皆不淨。",
        "from": "T24n1456_001:0519a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四子攝頌曰：",
        "from": "T24n1456_001:0519a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "影勝王床施，　　王母物入僧；",
        "from": "T24n1456_001:0519a07_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烏燕鶴鷲雕，　　苾芻不應食。",
        "from": "T24n1456_001:0519a08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五子攝頌曰：",
        "from": "T24n1456_001:0519a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "狗肉不應噉，　　并食屍鳥獸；",
        "from": "T24n1456_001:0519a09_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及以同蹄畜，　　亦不食獼猴。",
        "from": "T24n1456_001:0519a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六子攝頌曰：",
        "from": "T24n1456_001:0519a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小盞及衣角，　　皮葉等有過；",
        "from": "T24n1456_001:0519a11_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除其鐵一種，　　餘物任情為。",
        "from": "T24n1456_001:0519a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第七子攝頌曰：",
        "from": "T24n1456_001:0519a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甘蔗酪肉麻，　　藥有四種別；",
        "from": "T24n1456_001:0519a13_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大麻蔓菁粥，　　根等粥應飡。",
        "from": "T24n1456_001:0519a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八子攝頌曰：",
        "from": "T24n1456_001:0519a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開許沙糖飲，　　得為七日藥；",
        "from": "T24n1456_001:0519a15_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生心為五事，　　益彼應共分。",
        "from": "T24n1456_001:0519a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第九子攝頌曰：",
        "from": "T24n1456_001:0519a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "醫教應服酥，　　油及餘殘觸；",
        "from": "T24n1456_001:0519a17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并開眼藥合，　　除十為淨厨。",
        "from": "T24n1456_001:0519a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十子攝頌曰：",
        "from": "T24n1456_001:0519a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "根莖葉花果，　　皆應淡酒浸；",
        "from": "T24n1456_001:0519a19_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水攪而飲用，　　并許其異食。",
        "from": "T24n1456_001:0519a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別門第二總攝頌曰：",
        "from": "T24n1456_001:0519a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定物有主處，　　須問憍薩羅；",
        "from": "T24n1456_001:0519a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從像豫先差，　　大減會尼眾。",
        "from": "T24n1456_001:0519a22_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一子攝頌曰：",
        "from": "T24n1456_001:0519a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定物不應移，　　莫拾賊遺物；",
        "from": "T24n1456_001:0519a23_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屍林亦復爾，　　隨許並應收。",
        "from": "T24n1456_001:0519a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二子攝頌曰：",
        "from": "T24n1456_001:0519a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有主天廟物，　　苾芻不應取；",
        "from": "T24n1456_001:0519a25_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "看病人不應，　　勸他捨法服。",
        "from": "T24n1456_001:0519a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三子攝頌曰：",
        "from": "T24n1456_001:0519a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "物須問施主，　　眾利可平分；",
        "from": "T24n1456_001:0519a27_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大合均分，　　餘眾應加減。",
        "from": "T24n1456_001:0519a28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四子攝頌曰：",
        "from": "T24n1456_001:0519a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憍薩羅白疊，　　佛子因飡󰉓；",
        "from": "T24n1456_001:0519a29_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "室利笈多緣，　　廣論營造事。",
        "from": "T24n1456_001:0519b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五子攝頌曰：",
        "from": "T24n1456_001:0519b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從像入城中，　　受吉祥施物；",
        "from": "T24n1456_001:0519b02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旗鼓隨情設，　　苾芻皆不應。",
        "from": "T24n1456_001:0519b03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六子攝頌曰：",
        "from": "T24n1456_001:0519b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豫先為唱令，　　五眾從行城；",
        "from": "T24n1456_001:0519b04_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應差掌物人，　　尼無別輪法。",
        "from": "T24n1456_001:0519b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第七子攝頌曰：",
        "from": "T24n1456_001:0519b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應差分物人，　　上座宜准價；",
        "from": "T24n1456_001:0519b06_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得輒酬直，　　索價返還衣。",
        "from": "T24n1456_001:0519b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八子攝頌曰：",
        "from": "T24n1456_001:0519b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寺大減其層，　　將衣者應用；",
        "from": "T24n1456_001:0519b08_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恐怖若止息，　　准式用僧祇。",
        "from": "T24n1456_001:0519b09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第九子攝頌曰：",
        "from": "T24n1456_001:0519b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有大聚會，　　鳴鼓集眾僧；",
        "from": "T24n1456_001:0519b10_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾大別為行，　　檢校人先食。",
        "from": "T24n1456_001:0519b11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十子攝頌曰：",
        "from": "T24n1456_001:0519b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡於尼眾首，　　應安一空座；",
        "from": "T24n1456_001:0519b12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為待餘苾芻，　　孤苦勿增價。",
        "from": "T24n1456_001:0519b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三別門總攝頌曰：",
        "from": "T24n1456_001:0519b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "資具衣愚癡，　　若差不用俗；",
        "from": "T24n1456_001:0519b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正作長者施，　　剃刀窣覩波；",
        "from": "T24n1456_001:0519b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飯酪葉承水，　　及洗鉢等事；",
        "from": "T24n1456_001:0519b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此之十二頌，　　總攝要應知。",
        "from": "T24n1456_001:0519b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一子攝頌曰：",
        "from": "T24n1456_001:0519b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十三資具物，　　牒名而守持；",
        "from": "T24n1456_001:0519b19_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自餘諸長衣，　　委寄應分別。",
        "from": "T24n1456_001:0519b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二子攝頌曰：",
        "from": "T24n1456_001:0519b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "癡不了三藏，　　此等十二人；",
        "from": "T24n1456_001:0519b21_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "失性復本時，　　訶言應採錄。",
        "from": "T24n1456_001:0519b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三子攝頌曰：",
        "from": "T24n1456_001:0519b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若差十二人，　　斯語成訶法；",
        "from": "T24n1456_001:0519b23_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受時言我俗，　　此不成近圓。",
        "from": "T24n1456_001:0519b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四子攝頌曰：",
        "from": "T24n1456_001:0519b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不用五種脂，　　隨應為說戒；",
        "from": "T24n1456_001:0519b25_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因億耳開粥，　　王田眾應受。",
        "from": "T24n1456_001:0519b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五子攝頌曰：",
        "from": "T24n1456_001:0519b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俗人求寂等，　　並不合同坐；",
        "from": "T24n1456_001:0519b27_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩學有難緣，　　同處非成過。",
        "from": "T24n1456_001:0519b28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六子攝頌曰：",
        "from": "T24n1456_001:0519b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正作不令起，　　隨年坐染盆；",
        "from": "T24n1456_001:0519b29_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應共護僧園，　　勿燒營作木。",
        "from": "T24n1456_001:0519c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第七子攝頌曰：",
        "from": "T24n1456_001:0519c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長者所施物，　　問已應留舉；",
        "from": "T24n1456_001:0519c02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨處莫廢他，　　洗身方入寺。",
        "from": "T24n1456_001:0519c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八子攝頌曰：",
        "from": "T24n1456_001:0519c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剃刀并鑷子，　　用竟不應留；",
        "from": "T24n1456_001:0519c04_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便利若了時，　　無宜室中住。",
        "from": "T24n1456_001:0519c05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第九子攝頌曰：",
        "from": "T24n1456_001:0519c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "窣覩波圍繞，　　廣陳諸聖迹；",
        "from": "T24n1456_001:0519c06_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "濁水隨應飲，　　若醎分別知。",
        "from": "T24n1456_001:0519c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十子攝頌曰：",
        "from": "T24n1456_001:0519c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餅酪等非污，　　亦可內瓶中；",
        "from": "T24n1456_001:0519c08_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洗足五種瓨，　　齊何名口淨。",
        "from": "T24n1456_001:0519c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "葉手承注口，　　多疑流鉢中；",
        "from": "T24n1456_001:0519c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉糧持渡河，　　縱觸非成過。",
        "from": "T24n1456_001:0519c10_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洗鉢應用心，　　他觸問方受；",
        "from": "T24n1456_001:0519c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "換食持糧等，　　無難並還遮。",
        "from": "T24n1456_001:0519c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別門第四總攝頌曰：",
        "from": "T24n1456_001:0519c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與田分不應，　　赤體定物施；",
        "from": "T24n1456_001:0519c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧衣字還往，　　甘蔗果客裙。",
        "from": "T24n1456_001:0519c14_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一子攝頌曰：",
        "from": "T24n1456_001:0519c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與田分相助，　　車船沸自取；",
        "from": "T24n1456_001:0519c15_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳥󰝾蠅無慚，　　制底信少欲。",
        "from": "T24n1456_001:0519c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二子攝頌曰：",
        "from": "T24n1456_001:0519c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應令賊住，　　及以黃門等；",
        "from": "T24n1456_001:0519c17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至授學人，　　行籌破僧眾。",
        "from": "T24n1456_001:0519c18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三子攝頌曰：",
        "from": "T24n1456_001:0519c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不赤體披衣，　　冒雨向厨內；",
        "from": "T24n1456_001:0519c19_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便利宜縫補，　　和泥福久增。",
        "from": "T24n1456_001:0519c20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四子攝頌曰：",
        "from": "T24n1456_001:0520a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定物施此中，　　不應餘處食；",
        "from": "T24n1456_001:0520a01_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有將去者，　　並須依價還。",
        "from": "T24n1456_001:0520a02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五子攝頌曰：",
        "from": "T24n1456_001:0520a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧衣題施主，　　別人施私記；",
        "from": "T24n1456_001:0520a03_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "氍氀許別人，　　尼夏應修理。",
        "from": "T24n1456_001:0520a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六子攝頌曰：",
        "from": "T24n1456_001:0520a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若還往衣物，　　送來應為受；",
        "from": "T24n1456_001:0520a05_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為眾取他財，　　將眾物還價。",
        "from": "T24n1456_001:0520a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第七子攝頌曰：",
        "from": "T24n1456_001:0520a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甘蔗等平分，　　不應分口腹；",
        "from": "T24n1456_001:0520a07_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四事無分法，　　臥具夜不行。",
        "from": "T24n1456_001:0520a08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八子攝頌曰：",
        "from": "T24n1456_001:0520a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果由藥叉施，　　淨之方受食；",
        "from": "T24n1456_001:0520a09_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘者為漿飲，　　不燒地燈臺。",
        "from": "T24n1456_001:0520a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第九子攝頌曰：",
        "from": "T24n1456_001:0520a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "客舊宜詳審，　　授受分明付；",
        "from": "T24n1456_001:0520a11_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五開應總閉，　　肘短可隨身。",
        "from": "T24n1456_001:0520a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十子攝頌曰：",
        "from": "T24n1456_001:0520a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "裙及僧脚欹，　　香泥污衣洗；",
        "from": "T24n1456_001:0520a13_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取食除多分，　　須知十種塵。",
        "from": "T24n1456_001:0520a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "根本說一切有部毘奈耶尼陀那目得迦攝頌一卷大唐景龍四年歲次庚戌四月壬午朔十五日景申三藏法師大德沙門義淨宣釋梵本并綴文正字翻經沙門吐火羅大德達摩秫唐證梵義翻經沙門中天竺國大德校努證梵義翻經沙門罽賓國大德達摩難陀證梵文翻經沙門淄州大雲寺大德慧沼證義翻經沙門洛州崇光寺大德律師道琳證義翻經沙門福壽寺寺主大德利明證義翻經沙門洛州太平寺大德律師道恪證義翻經沙門大薦福寺大德勝莊證義翻經沙門相州禪河寺大德玄傘證義筆受翻經沙門大薦福寺大德律師智積證義正字翻經沙門德州大雲寺寺主慧傘證義翻經沙門西涼州白塔寺大德慧積讀梵本翻經婆羅門右驍衛翊府中郎員外置宿衛臣李釋迦讀梵本翻經婆羅門東天竺國左屯翊府中郎將員外置同正員臣瞿金剛證譯翻經婆羅門東天竺國大首領臣伊舍羅證梵本翻經婆羅門東天竺國左執戟直中書省臣度頗具讀梵本翻經婆羅門龍播國大達官准三品臣李輸羅證譯金紫光祿大夫守尚書左僕射同中書門下三品上柱國館國公臣韋洰源等，及修文館學士二十六人同監判官朝散大夫行著作佐郎臣劉令植，使金紫光祿大夫行祕書監檢校殿中監兼知內外閑厩隴右三使上柱國嗣虢王臣邕",
        "from": "T24n1456_001:0520b21_100",
        "context_with_next_sentence": false
    }
]