[
    {
        "source_sentence": "大乘緣生論一卷聖者欝楞迦造",
        "from": "T32n1653_001:0486b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿",
        "from": "T32n1653_001:0486b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "肅國公食邑三千戶賜紫贈司空",
        "from": "T32n1653_001:0486b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從一生於三，　　從三轉生六，",
        "from": "T32n1653_001:0486b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六二二更六，　　從六亦生六。",
        "from": "T32n1653_001:0486b10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從六有於三，　　此三復有三，",
        "from": "T32n1653_001:0486b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三復生於四，　　四復生於三。",
        "from": "T32n1653_001:0486b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從三生於一，　　彼一復生七，",
        "from": "T32n1653_001:0486b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中所有苦，　　牟尼說皆攝。",
        "from": "T32n1653_001:0486b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二種差別，　　智士說為空，",
        "from": "T32n1653_001:0486b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣生支力故，　　應知十二法。",
        "from": "T32n1653_001:0486b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無知與業識，　　名色根三和，",
        "from": "T32n1653_001:0486b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "領渴及以取，　　集生熟次終。",
        "from": "T32n1653_001:0486b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初八九煩惱，　　第二第十業，",
        "from": "T32n1653_001:0486b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘七皆是苦，　　三攝十二法。",
        "from": "T32n1653_001:0486b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初二是過去，　　後二未來時，",
        "from": "T32n1653_001:0486b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘八是現在，　　此謂三時法。",
        "from": "T32n1653_001:0486b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煩惱業感報，　　報還生煩惱，",
        "from": "T32n1653_001:0486b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煩惱復生業，　　亦由業有報。",
        "from": "T32n1653_001:0486b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離惱何有業？",
        "from": "T32n1653_001:0486b25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　離業何有報？",
        "from": "T32n1653_001:0486b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無報則離惱，　　此三各寂滅。",
        "from": "T32n1653_001:0486b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五支因生果，　　名為煩惱業，",
        "from": "T32n1653_001:0486b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七支以為果，　　七種苦應知。",
        "from": "T32n1653_001:0486b28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因中空無果，　　果中亦無因，",
        "from": "T32n1653_001:0486b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因中亦無因，　　果中亦無果，",
        "from": "T32n1653_001:0486c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智者空相應。",
        "from": "T32n1653_001:0486c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因果合故有，　　煩惱業果合，",
        "from": "T32n1653_001:0486c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應許為六分。",
        "from": "T32n1653_001:0486c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二節及三略，　　因果雜為節，",
        "from": "T32n1653_001:0486c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三四節總略。",
        "from": "T32n1653_001:0486c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦位有五法，　　作者及藏界，",
        "from": "T32n1653_001:0486c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "境轉生流行。",
        "from": "T32n1653_001:0486c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "等流果為二，　　相應根分中，",
        "from": "T32n1653_001:0486c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一三二分。",
        "from": "T32n1653_001:0486c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉出等流果，　　相應餘分中，",
        "from": "T32n1653_001:0486c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二一一一法。",
        "from": "T32n1653_001:0486c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和合故緣生，　　無眾生無命，",
        "from": "T32n1653_001:0486c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空無慧以知。",
        "from": "T32n1653_001:0486c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無我無我中，　　四種無知空，",
        "from": "T32n1653_001:0486c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘支亦如是，　　斷常二邊離，",
        "from": "T32n1653_001:0486c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此即是中道。",
        "from": "T32n1653_001:0486c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覺體是諸佛，　　覺已於眾中，",
        "from": "T32n1653_001:0486c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖仙說無我。",
        "from": "T32n1653_001:0486c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "導師說此義，　　迦栴延經說，",
        "from": "T32n1653_001:0486c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正見及空見，　　破邏遇拏經張宿名也，",
        "from": "T32n1653_001:0486c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦說殊勝空。",
        "from": "T32n1653_001:0486c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼知空相應，　　緣生若不知，",
        "from": "T32n1653_001:0486c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不知彼空。",
        "from": "T32n1653_001:0486c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於蘊不生厭，　　彼名惡趣空，",
        "from": "T32n1653_001:0486c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則迷緣生義。",
        "from": "T32n1653_001:0486c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離慢彼知空，　　及厭於蘊故，",
        "from": "T32n1653_001:0486c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不迷於業果。",
        "from": "T32n1653_001:0486c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦非不緣此，　　空緣當有此，",
        "from": "T32n1653_001:0486c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "業報受用具。",
        "from": "T32n1653_001:0487a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先已說緣生，　　彼煩惱業苦，",
        "from": "T32n1653_001:0487a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三中如法攝，　　從三生於二，",
        "from": "T32n1653_001:0487a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從二生於七，　　從七復生三。",
        "from": "T32n1653_001:0487a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有輪如是轉，　　一切皆因果，",
        "from": "T32n1653_001:0487a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從空生於空，　　從法生於法。",
        "from": "T32n1653_001:0487a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藉緣生煩惱，　　藉緣亦生業，",
        "from": "T32n1653_001:0487a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藉緣亦生報，　　無一不有緣。",
        "from": "T32n1653_001:0487a08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦燈印鏡音，　　日光種子醋，",
        "from": "T32n1653_001:0487a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘊續不移時，　　智慧觀彼二。",
        "from": "T32n1653_001:0487a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣生三十論本竟。",
        "from": "T32n1653_001:0487a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣生三十論，我當隨順次第解釋。",
        "from": "T32n1653_001:0487a12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從一生於三，　　從三轉生六，",
        "from": "T32n1653_001:0487a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六二二更六，　　從六亦生六。",
        "from": "T32n1653_001:0487a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從一生於三者謂無知，此無知者說名無明。",
        "from": "T32n1653_001:0487a15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於苦集滅道中不覺知故名為無知，由故則有福、非福、不動，說名三行。",
        "from": "T32n1653_001:0487a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及身行、口行、心行等從其轉生。",
        "from": "T32n1653_001:0487a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從三轉生六者，從三行生六識身，所謂眼識、耳識、鼻識、舌識、身識、意識。",
        "from": "T32n1653_001:0487a19_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六二者，彼六識身轉生二種，所謂名、色。",
        "from": "T32n1653_001:0487a20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二更六者，名色二種轉生六處，所謂眼處、耳處、鼻處、舌處、身處、意處。",
        "from": "T32n1653_001:0487a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從六亦生六者，從彼六處轉生六觸，所謂眼觸、耳觸、鼻觸、舌觸、身觸、意觸。",
        "from": "T32n1653_001:0487a23_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從六有於三，　　此三復有三，",
        "from": "T32n1653_001:0487a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三復生於四，　　四復生於三。",
        "from": "T32n1653_001:0487a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從六有於三者，從彼六觸轉生三受，所謂樂受、苦受、不苦不樂受。",
        "from": "T32n1653_001:0487a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此三復有三者，還從彼等三受轉生三種愛，所謂欲愛、有愛、無有愛。",
        "from": "T32n1653_001:0487a28_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從三復生於四者，從彼三種愛轉生四取，所謂欲取、見取、戒禁取、我語取。",
        "from": "T32n1653_001:0487b01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從四復生於三者，從彼四取轉生三有，所謂欲有、色有、無色有。",
        "from": "T32n1653_001:0487b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從三生於一，　　彼一復生七，",
        "from": "T32n1653_001:0487b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中所有苦，　　牟尼說皆攝。",
        "from": "T32n1653_001:0487b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從三生於一者，還以彼等三有作緣，生當來一種生。",
        "from": "T32n1653_001:0487b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼一復生七者，還從一當有老、死、愁、歎、苦、憂、惱等七種。",
        "from": "T32n1653_001:0487b08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中所有苦牟尼說皆攝者，於中無明為始、苦為終，無量種苦，世尊略說皆此所攝。",
        "from": "T32n1653_001:0487b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二種差別，　　智士說為空，",
        "from": "T32n1653_001:0487b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣生支力故，　　應知十二法。",
        "from": "T32n1653_001:0487b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二種差別智士說為空者，此無知等差別有十二支，彼一切皆自性空，應當知如此所說唯是空。",
        "from": "T32n1653_001:0487b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法從空生、空從法生，法由緣生支法故。",
        "from": "T32n1653_001:0487b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當知十二法者，若以次第生支力故，彼十二法如是應知。",
        "from": "T32n1653_001:0487b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼中迷惑相者是無明，彼行句處積集當有相者是行，彼識句處次受生支轉出相者是識，彼名色句處名身色身和合相者是名色，彼六處句安置根相者是六處，彼觸句處眼色識共聚相者是觸，彼受句處愛非愛顛倒受用相者是受，彼愛句處無厭足相者是愛，彼取句處執持攝取相者是取，彼有句處名身色身相者是有，彼生句處蘊生起相者是生，彼老句處成熟相者是老，彼死句處命根斷者是死，彼愁句處怱遽相者是愁，彼歎句處哭聲者是歎，彼苦句處身逼惱相者是苦，彼憂句處心逼惱相者是憂，彼諸熱惱句處損害相者是惱。",
        "from": "T32n1653_001:0487c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無知與業識，　　名色根三和，",
        "from": "T32n1653_001:0487c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "領渴及以取，　　集出熟後邊。",
        "from": "T32n1653_001:0487c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中無知者是無明，業者行，識者是了別，名色五蘊聚，根者是處，三和者是觸，領納者是受，渴者是愛，取者是執持，受用者是有，起者是生，熟者是老，後邊者是死。",
        "from": "T32n1653_001:0487c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此等差別相攝，我當次第說之。",
        "from": "T32n1653_001:0487c08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中煩惱業差別。",
        "from": "T32n1653_001:0487c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初八九煩惱，　　第二第十業，",
        "from": "T32n1653_001:0487c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘七皆是苦，　　三攝十二法。",
        "from": "T32n1653_001:0487c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三煩惱者，無明、愛、取。",
        "from": "T32n1653_001:0487c12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二業者，行，有。",
        "from": "T32n1653_001:0487c12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七報者，識、名色、六處、觸、受、生、老死等。",
        "from": "T32n1653_001:0487c13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此十二法三種所攝。",
        "from": "T32n1653_001:0487c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又時差別。",
        "from": "T32n1653_001:0487c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初二是過去，　　後二未來時，",
        "from": "T32n1653_001:0487c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘八是現在，　　此謂三時法。",
        "from": "T32n1653_001:0487c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無明、行初二種過去時，生、老死後二種未來，識、名色、六處、觸、受、愛、取、有八種現在時。",
        "from": "T32n1653_001:0487c18_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此等各各次第相生。",
        "from": "T32n1653_001:0487c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煩惱業感報，　　報還生煩惱，",
        "from": "T32n1653_001:0487c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煩惱復生業，　　亦由業有報。",
        "from": "T32n1653_001:0487c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煩惱、業、報三種如前所說。",
        "from": "T32n1653_001:0487c22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由彼煩惱故有業，由業故有報，還由報故有煩惱，由煩惱故有業，由業故有報。",
        "from": "T32n1653_001:0487c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰：",
        "from": "T32n1653_001:0487c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由煩惱盡各各寂滅，其義云何？",
        "from": "T32n1653_001:0487c25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答言：",
        "from": "T32n1653_001:0487c25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離惱何有業？",
        "from": "T32n1653_001:0487c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　離業何有報？",
        "from": "T32n1653_001:0487c26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無報則離惱，　　此三各寂滅。",
        "from": "T32n1653_001:0487c27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若其此心無煩惱染則不集業，若不作業則不受報，若滅報者亦不生煩惱，如是此三各各寂滅。",
        "from": "T32n1653_001:0488a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此等有因果分。",
        "from": "T32n1653_001:0488a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五支因生果，　　名為煩惱業，",
        "from": "T32n1653_001:0488a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七分以為果，　　七種苦應知。",
        "from": "T32n1653_001:0488a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五種因名為煩惱業者，如前所說，無明、行、愛、取、有是也。",
        "from": "T32n1653_001:0488a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七種果轉生者，亦如前所說七種苦，所謂識、名色、六處、觸、受、生、老死是也。",
        "from": "T32n1653_001:0488a07_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此因果二種空。",
        "from": "T32n1653_001:0488a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因中空無果，　　果中亦無因，",
        "from": "T32n1653_001:0488a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因中亦無因，　　果中亦無果，",
        "from": "T32n1653_001:0488a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智者空相應梵本一偈今為一句矣。",
        "from": "T32n1653_001:0488a11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若此所說因果二種，於中若因空果亦空，果空因亦空。",
        "from": "T32n1653_001:0488a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因空因亦空，果空果亦空，於此四句際當與相應。",
        "from": "T32n1653_001:0488a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此更有分別。",
        "from": "T32n1653_001:0488a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世中四種支，　　因果合故有，",
        "from": "T32n1653_001:0488a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煩惱業果合，　　應許為六支。",
        "from": "T32n1653_001:0488a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世中四種支因果合故有者，所說三世五種因，共七種果，總略為四種，次第有四種分。",
        "from": "T32n1653_001:0488a19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中無明、行，過去時二法為初分。",
        "from": "T32n1653_001:0488a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識、名色、六處、觸、受，現在時為第二分。",
        "from": "T32n1653_001:0488a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛、取、有，亦是現在時為第三分。",
        "from": "T32n1653_001:0488a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生、老死，未來時二法為第四分。",
        "from": "T32n1653_001:0488a22_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此謂四種分也。",
        "from": "T32n1653_001:0488a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煩惱業果結許為六分者，煩惱、業、報三種，結為二根，則為六分。",
        "from": "T32n1653_001:0488a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中無明乃至受以無明為根，愛乃至老死為愛根。",
        "from": "T32n1653_001:0488a25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無明根中無明是煩惱分，行是業分，識、名色、六處、觸、受是報分。",
        "from": "T32n1653_001:0488a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛根中愛取是煩惱分，有是業分，生、老死是報分。",
        "from": "T32n1653_001:0488a28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又節分總略。",
        "from": "T32n1653_001:0488a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有節所攝故，　　二節及三略，",
        "from": "T32n1653_001:0488b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因果雜為節，　　三四節總略。",
        "from": "T32n1653_001:0488b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有節為本，發起二節，所謂有、生兩間是第一節，行、識兩間是第二節。",
        "from": "T32n1653_001:0488b04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此二節並為業果節。",
        "from": "T32n1653_001:0488b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受、愛中因果共雜是第三節。",
        "from": "T32n1653_001:0488b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此之三節，復為四種總略。",
        "from": "T32n1653_001:0488b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無明、行二種是第一總略。",
        "from": "T32n1653_001:0488b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識、名色、六處、觸、受五種是第二總略。",
        "from": "T32n1653_001:0488b07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛、取、有三種是第三總略。",
        "from": "T32n1653_001:0488b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生、老死二種是第四總略。",
        "from": "T32n1653_001:0488b08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此謂三節及四總略。",
        "from": "T32n1653_001:0488b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此等法中位時差別。",
        "from": "T32n1653_001:0488b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二三二三二，　　苦位有五法，",
        "from": "T32n1653_001:0488b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作者及藏界，　　境轉生流行。",
        "from": "T32n1653_001:0488b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法者，無明、行說為二種。",
        "from": "T32n1653_001:0488b13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識、名色、六處說為三種。",
        "from": "T32n1653_001:0488b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觸、受說為二種。",
        "from": "T32n1653_001:0488b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛、取、有說為三種。",
        "from": "T32n1653_001:0488b14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又二者，生、老死說為二種。",
        "from": "T32n1653_001:0488b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此等五法，是苦位中作者、胎藏、境界、發轉、出生、於中流行，如數當知。",
        "from": "T32n1653_001:0488b16_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中無明、行二種說為苦位中作者，識、名色、六處三種說為苦位中胎藏，觸、受二種說為苦位中境界，愛、取、有三種說為苦位中發轉，生老、死二種說為苦位中流行。",
        "from": "T32n1653_001:0488b20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又果差別。",
        "from": "T32n1653_001:0488b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迷惑發起果，　　報流果為二，",
        "from": "T32n1653_001:0488b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相應根分中，　　一一三二分。",
        "from": "T32n1653_001:0488b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前所說，此無明根及愛根，於無明根初分中迷惑發起報等流，名四種果。",
        "from": "T32n1653_001:0488b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一三二數分之道，隨其次第當與相應。",
        "from": "T32n1653_001:0488b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中無明是迷惑果，行是發起果，識、名色、六處是報果，觸、受是等流果。",
        "from": "T32n1653_001:0488b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有餘殘果。",
        "from": "T32n1653_001:0488b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "熱惱缺短果，　　轉出等流果，",
        "from": "T32n1653_001:0488c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相應餘分中，　　二一一一法。",
        "from": "T32n1653_001:0488c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前所說第二愛根分中，熱惱、缺短、轉生、等流果等，隨其數分二一一一，於此法中當與相應。",
        "from": "T32n1653_001:0488c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中愛取是熱惱果，有是缺短果，生是轉出果，老死是等流果。",
        "from": "T32n1653_001:0488c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是此等則有八果。",
        "from": "T32n1653_001:0488c06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此有十二種，　　和合故緣生，",
        "from": "T32n1653_001:0488c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無眾生無命，　　空無以慧知。",
        "from": "T32n1653_001:0488c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是無明古譯無明，今無知乃正也為初，老死為後，有十二支，和合勝故各各緣生。",
        "from": "T32n1653_001:0488c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而無眾生、無壽命、空無，以慧應知。",
        "from": "T32n1653_001:0488c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中無眾生者，以不牢固故。",
        "from": "T32n1653_001:0488c11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無壽命者，以無我故。",
        "from": "T32n1653_001:0488c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空者，無作者，以無作者故。",
        "from": "T32n1653_001:0488c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無我無我所，　　無我無我中，",
        "from": "T32n1653_001:0488c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四種無知空，　　餘支亦如是。",
        "from": "T32n1653_001:0488c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無知是無我。",
        "from": "T32n1653_001:0488c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中無知是無我所，以無我故無我中無無知。",
        "from": "T32n1653_001:0488c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四種無知無我所中亦無無知空。",
        "from": "T32n1653_001:0488c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如四種次第無知空，如是行等餘支亦皆是空，應當知之。",
        "from": "T32n1653_001:0488c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷常二邊離，　　此即是中道，",
        "from": "T32n1653_001:0488c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若覺已成就，　　覺體是諸佛。",
        "from": "T32n1653_001:0488c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有是常執、無是斷執，此二邊由此生，緣故生彼。",
        "from": "T32n1653_001:0488c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼諸有中，若離二邊即契中道。",
        "from": "T32n1653_001:0488c23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不知此是義，則諸外道墮於二邊。",
        "from": "T32n1653_001:0488c24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若覺悟已是則一切諸佛，如佛於世間能成就非餘。",
        "from": "T32n1653_001:0488c25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覺已於眾中，　　聖仙說無我，",
        "from": "T32n1653_001:0488c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曾於城喻經，　　導師說此義。",
        "from": "T32n1653_001:0488c27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼亦是此中道覺已，於諸眾中，佛說無我無我所。",
        "from": "T32n1653_001:0488c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等比丘當知，謂著我我所，愚童凡夫寡聞之類。",
        "from": "T32n1653_001:0489a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨假施設中復我及我所。",
        "from": "T32n1653_001:0489a01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比丘生時但苦生，滅時但苦滅。",
        "from": "T32n1653_001:0489a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如城喻經中導師已說義。",
        "from": "T32n1653_001:0489a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又：",
        "from": "T32n1653_001:0489a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦旃延經說，　　正見及空見，",
        "from": "T32n1653_001:0489a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破邏具拏經，　　亦說殊勝空。",
        "from": "T32n1653_001:0489a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此等三經及以餘處，如是之相世尊已廣說。",
        "from": "T32n1653_001:0489a06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣生若正知，　　彼知空相應，",
        "from": "T32n1653_001:0489a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣生若不知，　　亦不知彼空。",
        "from": "T32n1653_001:0489a08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於前所說緣生若有正知，彼知無異。",
        "from": "T32n1653_001:0489a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼復何知？",
        "from": "T32n1653_001:0489a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂知於空。",
        "from": "T32n1653_001:0489a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣生若不知亦不知空者，於此緣生若其不知，亦於彼空不能解入應知之。",
        "from": "T32n1653_001:0489a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於空若起慢，　　則不厭於蘊，",
        "from": "T32n1653_001:0489a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有彼無見，　　則迷緣生義。",
        "from": "T32n1653_001:0489a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於空若起慢則不厭，若起空慢則於五蘊中不生厭離。",
        "from": "T32n1653_001:0489a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有彼無見則迷緣生義者，若復由於無見迷此緣生義故，則於四種見中隨取何見，一者斷見、二者常見、三者自在化語、四者一切宿業作。",
        "from": "T32n1653_001:0489a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣生不迷故，　　離慢彼知空，",
        "from": "T32n1653_001:0489a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及厭於蘊故，　　不迷於業果。",
        "from": "T32n1653_001:0489a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣生不迷故離慢彼知空者，於前所說各各緣生中若無迷心，及於執取我我所中若得離慢，彼則如法能入於空。",
        "from": "T32n1653_001:0489a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及厭於蘊故不迷於業果者，五蘊中執取我我所故，則遍世間輪轉不息。",
        "from": "T32n1653_001:0489a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彼蘊中厭離故，於業果相續則無顛倒亦不迷惑。",
        "from": "T32n1653_001:0489a27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此義云何？",
        "from": "T32n1653_001:0489a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "業作緣續生，　　亦非不緣此，",
        "from": "T32n1653_001:0489a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空緣當有此，　　業報受用具。",
        "from": "T32n1653_001:0489b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "業作緣續生亦非不緣此者，煩惱業報如前所說，彼以如是善不善業，推遣眾生傍及上下相續而生，若非此業則不作緣。",
        "from": "T32n1653_001:0489b04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不然者，則不作業受報、已作業而失。",
        "from": "T32n1653_001:0489b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空緣當有此業報受用具者，若由此等善不善業有報受用，自性是空、本無有我，作緣發生彼性空亦應當知。",
        "from": "T32n1653_001:0489b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼義今更略說。",
        "from": "T32n1653_001:0489b08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二支差別，　　前已說緣生，",
        "from": "T32n1653_001:0489b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼煩惱業苦，　　三中如法攝。",
        "from": "T32n1653_001:0489b10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無明為初、老死為後，是十二支緣生差別，如前所說。",
        "from": "T32n1653_001:0489b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼中三是煩惱，二是業，七是苦，皆已攝入。",
        "from": "T32n1653_001:0489b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從三生於二，　　從二生於七，",
        "from": "T32n1653_001:0489b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從七復生三，　　有輪如是轉。",
        "from": "T32n1653_001:0489b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無明、愛、取三種所生行有二種，彼二所生識、名色、六處、觸、受、生、老死七支。",
        "from": "T32n1653_001:0489b17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼七支中如前所說還生三種，彼三復其二更七。",
        "from": "T32n1653_001:0489b18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故二種次第不斷，此之有輪如是轉。",
        "from": "T32n1653_001:0489b19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因果生諸世，　　無別有眾生，",
        "from": "T32n1653_001:0489b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯是於空法，　　還自生空法。",
        "from": "T32n1653_001:0489b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因果生諸世無別有眾生者，無明、行、愛、取、有五種名因，識、名色、六處、觸、受、生、老死七種名果。",
        "from": "T32n1653_001:0489b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此等所有普遍世間，若我、若眾生、若壽、若生者、若丈夫、若人、若作者，是等分別唯虛誑，應當知之。",
        "from": "T32n1653_001:0489b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼云何生？",
        "from": "T32n1653_001:0489b26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯是於空法還自生空法，謂自性空中假名煩惱業果，唯有空假名煩惱業果法生，此是其義。",
        "from": "T32n1653_001:0489b28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藉緣生煩惱，　　藉緣亦生業，",
        "from": "T32n1653_001:0489b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藉緣亦生報，　　無一不有緣。",
        "from": "T32n1653_001:0489c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有煩惱則有種種無量業，及種種業所生果報，彼皆因共緣，應當知之無有一法無因緣者。",
        "from": "T32n1653_001:0489c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又為明彼義，今更說譬喻。",
        "from": "T32n1653_001:0489c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦燈印鏡音，　　日光種子醋，",
        "from": "T32n1653_001:0489c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾續不移轉，　　智應觀彼二。",
        "from": "T32n1653_001:0489c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如誦，有教誦者、受誦者，所有教誦不移轉受誦，何故教誦者仍安，安故其教誦者亦不相續。",
        "from": "T32n1653_001:0489c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故？",
        "from": "T32n1653_001:0489c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自不自故。",
        "from": "T32n1653_001:0489c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如燈次第生，非是初燈移轉，亦非第二無因而生。",
        "from": "T32n1653_001:0489c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是印與像二種、面與鏡二種、音與響二種、日與火二種、種子與芽二種、醋與舌唌二種。",
        "from": "T32n1653_001:0489c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二種此等所有皆不移轉，亦非不生，亦非無因而生。",
        "from": "T32n1653_001:0489c13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼二種五蘊相續次第轉，非初蘊而移轉，而第二蘊亦非不生，亦非無因而生。",
        "from": "T32n1653_001:0489c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智者於此蘊相續次第不移轉，應當正觀。",
        "from": "T32n1653_001:0489c16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又內外相應有十種，皆當知。",
        "from": "T32n1653_001:0489c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中外十種者，一者非常故、二者非斷故、三者不移轉故、四者因果相繫無中間故、五者非彼體故、六者非別異故、七者無作者故、八者非無因故、九者剎那滅故、十者同類果相繫故。",
        "from": "T32n1653_001:0489c21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼外所有種子滅無餘故、非常牙出生故、非斷種子滅無餘已、其牙本無今有生故、不移轉彼所相續無有斷絕因果相繼故、無中間種子牙差別故、非彼體從出生故、非別異因緣和合故、無作者種子為因故、非無因種子牙莖枝葉花菓等展轉相生故、剎那滅甜醋醎苦辛澁隨因差別果轉出故。",
        "from": "T32n1653_001:0489c28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同類果相繫，於中內十種者，一者死邊蘊滅無餘故非常；",
        "from": "T32n1653_001:0489c29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者得次生支蘊故非斷；",
        "from": "T32n1653_001:0490a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者死邊蘊滅無餘已次生支蘊本無今有生故不移轉；",
        "from": "T32n1653_001:0490a02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者蘊相續無有斷絕，因果相繼故無中間；",
        "from": "T32n1653_001:0490a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者死邊次生支蘊差別故非彼體；",
        "from": "T32n1653_001:0490a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六者從彼出生故非別異；",
        "from": "T32n1653_001:0490a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七者因緣和合故無作者；",
        "from": "T32n1653_001:0490a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八者煩惱業為因故非無因；",
        "from": "T32n1653_001:0490a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九者迦邏羅、頞浮陀、箄尸、伽那、奄佉、出胎、嬰孩、童子、少年、長宿等，展轉相生故剎那滅；",
        "from": "T32n1653_001:0490a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十者善不善薰。",
        "from": "T32n1653_001:0490a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨因差別果轉出故，同類果相繫。",
        "from": "T32n1653_001:0490a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又有三偈：",
        "from": "T32n1653_001:0490a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如燈焰轉生，　　識身亦如是，",
        "from": "T32n1653_001:0490a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前際與後際，　　亦無有積集。",
        "from": "T32n1653_001:0490a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不生亦有生，　　破壞不和合，",
        "from": "T32n1653_001:0490a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所生亦無住，　　而此作業轉。",
        "from": "T32n1653_001:0490a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於彼緣生，　　而能觀知空，",
        "from": "T32n1653_001:0490a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若知彼施設，　　則契於中道。",
        "from": "T32n1653_001:0490a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中無明、行、愛、取、有是為集諦，識、名色、六處、觸、受、生、老死是為苦諦。",
        "from": "T32n1653_001:0490a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼十二支，道諦者令彼滅證方便，所謂念處、正勤、如意足、根、力、覺支、八聖道名為道諦。",
        "from": "T32n1653_001:0490a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大乘緣生論一卷",
        "from": "T32n1653_001:0490a21_100",
        "context_with_next_sentence": false
    }
]