[
    {
        "source_sentence": "十二天供儀軌合兩所文為軌已出護摩軌",
        "from": "T21n1298_001:0385b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於道場八方敷茅草。或蓮荷葉或餘青草。或",
        "from": "T21n1298_001:0385b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗圓壇合為十位。於帝釋左右置梵天地天",
        "from": "T21n1298_001:0385b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "位。",
        "from": "T21n1298_001:0385b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與八方而十。",
        "from": "T21n1298_001:0385b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若道場內無置位處。",
        "from": "T21n1298_001:0385b10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於道場前用淨處別為方界。",
        "from": "T21n1298_001:0385b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中布八方。",
        "from": "T21n1298_001:0385b11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中央作兩位。",
        "from": "T21n1298_001:0385b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置梵天地天。",
        "from": "T21n1298_001:0385b12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以施十方天。",
        "from": "T21n1298_001:0385b12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食應用雜粥。",
        "from": "T21n1298_001:0385b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂梗米油麻菉豆相和煮。",
        "from": "T21n1298_001:0385b13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令極清淨。",
        "from": "T21n1298_001:0385b13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香義盛器中。",
        "from": "T21n1298_001:0385b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每座先置一淨葉。",
        "from": "T21n1298_001:0385b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "巡環卒置葉上。",
        "from": "T21n1298_001:0385b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以淨瓶盛水。",
        "from": "T21n1298_001:0385b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即寫少香水於葉上以獻。",
        "from": "T21n1298_001:0385b15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以右手中無名二指彈少塗香。",
        "from": "T21n1298_001:0385b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次獻一花置之於座次獻燒香。",
        "from": "T21n1298_001:0385b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次爐梵香。",
        "from": "T21n1298_001:0385b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於座前獻諸座。",
        "from": "T21n1298_001:0385b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同此一爐。",
        "from": "T21n1298_001:0385b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次卒一杓粥置葉上以獻。",
        "from": "T21n1298_001:0385b18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次用小蠟燭或紙燭以獻。",
        "from": "T21n1298_001:0385b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便插粥上。",
        "from": "T21n1298_001:0385b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從香水至燭。",
        "from": "T21n1298_001:0385b19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各以本真言加持三遍。",
        "from": "T21n1298_001:0385b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每位從水至燭獻畢。",
        "from": "T21n1298_001:0385b20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然向。",
        "from": "T21n1298_001:0385b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次其燭作意獻諸位。",
        "from": "T21n1298_001:0385b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未遍已來不用令滅。",
        "from": "T21n1298_001:0385b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須助伴或驅使數人。",
        "from": "T21n1298_001:0385b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各執一物以供事。",
        "from": "T21n1298_001:0385b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一一自取即燭必不終事。",
        "from": "T21n1298_001:0385b23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每位於薩嚩訶上加所求願語　東方帝釋乘白象王。",
        "from": "T21n1298_001:0385b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住五色雲中。",
        "from": "T21n1298_001:0385b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身作金色。",
        "from": "T21n1298_001:0385b25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持三股當心。",
        "from": "T21n1298_001:0385b25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手托左胯。",
        "from": "T21n1298_001:0385b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左脚垂下。",
        "from": "T21n1298_001:0385b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三天女各手持蓮花。",
        "from": "T21n1298_001:0385b26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或以盤盛雜花等。",
        "from": "T21n1298_001:0385b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盤盛青蓮花。",
        "from": "T21n1298_001:0385b27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印相者右手作拳安腰。",
        "from": "T21n1298_001:0385b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手五指直竪相著。",
        "from": "T21n1298_001:0385b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地水二指屈中節。",
        "from": "T21n1298_001:0385b28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以風著火背。",
        "from": "T21n1298_001:0385b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空屈中節。",
        "from": "T21n1298_001:0385b29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰　曩莫三曼多沒馱南因捺羅耶娑嚩二合賀　東南方火天乘青羊。",
        "from": "T21n1298_001:0385c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "赤肉色遍身火焰。",
        "from": "T21n1298_001:0385c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右二手一持青竹一持軍持。",
        "from": "T21n1298_001:0385c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左二手一楊掌一持念珠。",
        "from": "T21n1298_001:0385c03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有二天女持天花。",
        "from": "T21n1298_001:0385c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左右置苦行仙。",
        "from": "T21n1298_001:0385c04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "垂左脚蹉右足。",
        "from": "T21n1298_001:0385c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印相者左手作拳安腰左。",
        "from": "T21n1298_001:0385c05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手五指直竪相著。",
        "from": "T21n1298_001:0385c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈空納著掌中。",
        "from": "T21n1298_001:0385c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風屈中節。",
        "from": "T21n1298_001:0385c06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與娑婆訶相招。",
        "from": "T21n1298_001:0385c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1298_001:0385c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三滿多沒馱南．阿哦那二合曳娑嚩二合引訶引南方焰摩天乘水牛。",
        "from": "T21n1298_001:0385c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手執人頭幢左手仰掌。",
        "from": "T21n1298_001:0385c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有二天女侍。",
        "from": "T21n1298_001:0385c09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二鬼使者持刀捧戟。",
        "from": "T21n1298_001:0385c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "赤黑色垂右脚。",
        "from": "T21n1298_001:0385c10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印相者先合掌。",
        "from": "T21n1298_001:0385c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二地屈中節背相著。",
        "from": "T21n1298_001:0385c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二風亦屈背著。",
        "from": "T21n1298_001:0385c11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大指各押風中節即成。",
        "from": "T21n1298_001:0385c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1298_001:0385c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三滿多沒馱南焰摩耶娑嚩二合訶西南方羅剎主天乘白師子。",
        "from": "T21n1298_001:0385c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身著甲冑。",
        "from": "T21n1298_001:0385c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持刀令竪。",
        "from": "T21n1298_001:0385c14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手大指押中小二指。",
        "from": "T21n1298_001:0385c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "赤肉色。",
        "from": "T21n1298_001:0385c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二天女侍左右。",
        "from": "T21n1298_001:0385c15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二羅剎鬼。",
        "from": "T21n1298_001:0385c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持三股戟。",
        "from": "T21n1298_001:0385c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印相者右手作拳安腰右。",
        "from": "T21n1298_001:0385c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手五指竪相著屈地水中節。",
        "from": "T21n1298_001:0385c17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以空端押地水甲即成。",
        "from": "T21n1298_001:0385c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1298_001:0385c18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三滿多沒馱南乃哩底曳二合娑嚩二合賀西方水天住於水中乘龜。",
        "from": "T21n1298_001:0385c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淺綠色。",
        "from": "T21n1298_001:0385c20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手取刀。",
        "from": "T21n1298_001:0385c20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持龍索。",
        "from": "T21n1298_001:0385c20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冠上有五龍。",
        "from": "T21n1298_001:0385c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四天女持妙花。",
        "from": "T21n1298_001:0385c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印相者右手如前。",
        "from": "T21n1298_001:0385c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手握拳。",
        "from": "T21n1298_001:0385c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空勿入掌中。",
        "from": "T21n1298_001:0385c22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風即直竪。",
        "from": "T21n1298_001:0385c22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中節少屈即成。",
        "from": "T21n1298_001:0385c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1298_001:0385c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三滿多沒馱南嚩嚕拏野娑嚩二合訶西北方風天雲中乘麞著甲冑。",
        "from": "T21n1298_001:0385c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手托胯。",
        "from": "T21n1298_001:0385c25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手執獨股頭創。",
        "from": "T21n1298_001:0385c25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "創上有緋幡。",
        "from": "T21n1298_001:0385c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二天女侍之。",
        "from": "T21n1298_001:0385c26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并藥叉眾印相者右手如前。",
        "from": "T21n1298_001:0385c26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手五指直竪相著。",
        "from": "T21n1298_001:0385c27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地水屈中節即成。",
        "from": "T21n1298_001:0385c27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1298_001:0385c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三滿多沒馱南嚩耶吠娑嚩二合訶北方毘沙門天坐二鬼上。",
        "from": "T21n1298_001:0385c29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身著甲冑。",
        "from": "T21n1298_001:0385c29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手掌捧塔。",
        "from": "T21n1298_001:0386a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手執寶捧。",
        "from": "T21n1298_001:0386a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身金色。",
        "from": "T21n1298_001:0386a01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二天女持寶花等。",
        "from": "T21n1298_001:0386a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印相者先合掌。",
        "from": "T21n1298_001:0386a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十指內相叉。",
        "from": "T21n1298_001:0386a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二水竪頭相跓。",
        "from": "T21n1298_001:0386a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二風竪微屈。",
        "from": "T21n1298_001:0386a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左空入右掌中。",
        "from": "T21n1298_001:0386a03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "押左火甲。",
        "from": "T21n1298_001:0386a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右空越左空背入左掌中。",
        "from": "T21n1298_001:0386a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "押右火甲。",
        "from": "T21n1298_001:0386a04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二風與訶相招。",
        "from": "T21n1298_001:0386a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1298_001:0386a05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三滿多沒馱南吠室囉二合嚩拏野娑嚩訶東北方伊舍那天舊云魔醯首羅天。",
        "from": "T21n1298_001:0386a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦云大自在天。",
        "from": "T21n1298_001:0386a07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘黃豐牛。",
        "from": "T21n1298_001:0386a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持劫波杯盛血。",
        "from": "T21n1298_001:0386a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持三戟創。",
        "from": "T21n1298_001:0386a08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淺青肉色三目忿怒。",
        "from": "T21n1298_001:0386a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二牙上出。",
        "from": "T21n1298_001:0386a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髑髏為瓔珞。",
        "from": "T21n1298_001:0386a09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭冠中有二仰月。",
        "from": "T21n1298_001:0386a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二天女持花。",
        "from": "T21n1298_001:0386a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印相者右手作拳安腰右。",
        "from": "T21n1298_001:0386a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手五指直竪相著。",
        "from": "T21n1298_001:0386a11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地水二指屈中節。",
        "from": "T21n1298_001:0386a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火風空三指各少相去即成。",
        "from": "T21n1298_001:0386a12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1298_001:0386a12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三漫多馱南伊舍曩曳娑嚩二合訶上方梵天印相者右手作拳安右腰。",
        "from": "T21n1298_001:0386a14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手五指相著竪之少屈。",
        "from": "T21n1298_001:0386a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其高少許過肩。",
        "from": "T21n1298_001:0386a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成蓮花想。",
        "from": "T21n1298_001:0386a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1298_001:0386a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三滿多沒馱南沒羅二合訶摩二合寧娑嚩二合訶下方地天印相者先合掌。",
        "from": "T21n1298_001:0386a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風以下四指頭相踁。",
        "from": "T21n1298_001:0386a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前方大開。",
        "from": "T21n1298_001:0386a18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二空各著風側。",
        "from": "T21n1298_001:0386a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1298_001:0386a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三滿多沒馱南畢哩二合體微曳二合娑嚩二合訶日天印相者不異前地天印。",
        "from": "T21n1298_001:0386a21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但以二空頭。",
        "from": "T21n1298_001:0386a21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各跓水下節即成。",
        "from": "T21n1298_001:0386a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1298_001:0386a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三滿多沒馱南阿去儞底也二合娑嚩二合訶月天印相者如前梵天印。",
        "from": "T21n1298_001:0386a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但仰掌。",
        "from": "T21n1298_001:0386a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中有月潔白相。",
        "from": "T21n1298_001:0386a24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1298_001:0386a24_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三曼多沒馱南戰捺羅二合去也娑嚩二合訶七曜印相者先合掌十指相著。",
        "from": "T21n1298_001:0386a26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二風與二空極相開。",
        "from": "T21n1298_001:0386a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1298_001:0386a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三曼多沒馱南蘖羅二合醯濕嚩二合哩耶鉢囉二合跛多而偷二合底羅摩耶娑嚩二合訶二十八宿印相者先虛心合掌。",
        "from": "T21n1298_001:0386b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二火外相叉。",
        "from": "T21n1298_001:0386b01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二空亦相叉。",
        "from": "T21n1298_001:0386b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1298_001:0386b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三曼多沒馱南諾乞灑二合怛羅二合涅蘇那儞曳娑嚩訶於八方中加兩位。",
        "from": "T21n1298_001:0386b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上下天對。",
        "from": "T21n1298_001:0386b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曜東宿西。",
        "from": "T21n1298_001:0386b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸獻並同。",
        "from": "T21n1298_001:0386b04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若別有所願。",
        "from": "T21n1298_001:0386b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獨用亦得。",
        "from": "T21n1298_001:0386b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若護摩壇中。",
        "from": "T21n1298_001:0386b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各依本方標心令住亦設位云云。",
        "from": "T21n1298_001:0386b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壇中心置四臂不動尊。",
        "from": "T21n1298_001:0386b06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "青肉色。",
        "from": "T21n1298_001:0386b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手金剛拳。",
        "from": "T21n1298_001:0386b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭指小指各曲如鉤形。",
        "from": "T21n1298_001:0386b07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安口兩角。",
        "from": "T21n1298_001:0386b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牙相如牙。",
        "from": "T21n1298_001:0386b08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持刀令竪。",
        "from": "T21n1298_001:0386b08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左持索半跏右押左。",
        "from": "T21n1298_001:0386b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐盤石上。",
        "from": "T21n1298_001:0386b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "威焰光明遍身如火云云。",
        "from": "T21n1298_001:0386b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今作圖位。",
        "from": "T21n1298_001:0386b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但文立圓壇云云。",
        "from": "T21n1298_001:0386b10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨時可作之耳。",
        "from": "T21n1298_001:0386b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北面而可修之。",
        "from": "T21n1298_001:0386b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或圖　中台五大明王十二天次第以不動五大明王為十二天主之十二天之次可供諸天妃次可供當土諸名神。",
        "from": "T21n1298_001:0386c12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求覓洛東智積院動潮僧正之御本。",
        "from": "T21n1298_001:0386c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令寫之而挍。",
        "from": "T21n1298_001:0386c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以上木。",
        "from": "T21n1298_001:0386c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此軌薄草決及金寶集等引以證。",
        "from": "T21n1298_001:0386c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　　　長谷寺留學沙門　快道誌",
        "from": "T21n1298_001:0386c18_0",
        "context_with_next_sentence": true
    }
]