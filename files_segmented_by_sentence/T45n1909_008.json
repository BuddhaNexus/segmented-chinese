[
    {
        "source_sentence": "慈悲道場懺法卷第八",
        "from": "T45n1909_008:0954b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "4 奉為阿脩羅道一切善神禮佛奉為阿脩羅道一切善神禮佛第四",
        "from": "T45n1909_008:0954b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今日道場同業大眾。重復至誠五體投地。奉",
        "from": "T45n1909_008:0954b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為十方盡虛空界一切阿脩羅王。",
        "from": "T45n1909_008:0954b06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切阿脩羅。",
        "from": "T45n1909_008:0954b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各及眷屬。",
        "from": "T45n1909_008:0954b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又奉為十方盡虛空界一切聰明。",
        "from": "T45n1909_008:0954b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正直天地虛空主善罰惡守護持呪。",
        "from": "T45n1909_008:0954b08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八部神王八部神將。",
        "from": "T45n1909_008:0954b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至若內若外若近若遠。",
        "from": "T45n1909_008:0954b09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東西南北四維上下。",
        "from": "T45n1909_008:0954b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍空法界有大神足力。",
        "from": "T45n1909_008:0954b10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大威德力。",
        "from": "T45n1909_008:0954b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是十方八部神王八部神將各及眷屬。",
        "from": "T45n1909_008:0954b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命敬禮一切世間大慈悲父。",
        "from": "T45n1909_008:0954b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無彌勒佛　南無釋迦牟尼佛　南無寶名佛　南無眾清淨佛　南無無邊名佛　南無不虛光佛　南無聖天佛　南無智王佛　南無金剛眾佛　南無善障佛　南無建慈佛　南無華國佛　南無法意佛　南無風行佛　南無善思名佛　南無多明佛　南無密眾佛　南無功德守佛　南無利意佛　南無無懼佛　南無堅觀佛　南無住法佛　南無珠足佛　南無解脫德佛　南無妙身佛　南無善高佛　南無普德佛　南無無邊身菩薩　南無觀世音菩薩又復歸命如是十方盡虛空界一切三寶。",
        "from": "T45n1909_008:0954b24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願以慈悲力同加覆護。",
        "from": "T45n1909_008:0954b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願阿脩羅王一切阿脩羅。",
        "from": "T45n1909_008:0954b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各及眷屬又願聰明正直。",
        "from": "T45n1909_008:0954b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天地虛空。",
        "from": "T45n1909_008:0954b26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "主善罰惡守護持呪。",
        "from": "T45n1909_008:0954b27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八部神王八部神將。",
        "from": "T45n1909_008:0954b27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各及眷屬。",
        "from": "T45n1909_008:0954b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解脫客塵清淨緣障。",
        "from": "T45n1909_008:0954b28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發起大乘修無礙道。",
        "from": "T45n1909_008:0954b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四無量心。",
        "from": "T45n1909_008:0954b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六波羅蜜常得現前。",
        "from": "T45n1909_008:0954b29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四辯六通如意自在。",
        "from": "T45n1909_008:0954c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒以慈悲救護眾生。",
        "from": "T45n1909_008:0954c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行菩薩道入佛智慧。",
        "from": "T45n1909_008:0954c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "度金剛心成等正覺。",
        "from": "T45n1909_008:0954c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "5 奉為龍王禮佛奉為龍王禮佛第五今日道場同業大眾。",
        "from": "T45n1909_008:0954c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重復至誠五體投地。",
        "from": "T45n1909_008:0954c04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉為十方盡虛空界一切不可思議龍王。",
        "from": "T45n1909_008:0954c05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙化龍王頭化提龍王。",
        "from": "T45n1909_008:0954c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五方龍王。",
        "from": "T45n1909_008:0954c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天龍王地龍王。",
        "from": "T45n1909_008:0954c06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "山龍王海龍王。",
        "from": "T45n1909_008:0954c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日宮龍王月宮龍王。",
        "from": "T45n1909_008:0954c07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "星宮龍王歲時龍王。",
        "from": "T45n1909_008:0954c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "青海龍王。",
        "from": "T45n1909_008:0954c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護形命龍王。",
        "from": "T45n1909_008:0954c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護眾生龍王。",
        "from": "T45n1909_008:0954c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至十方若內若外。",
        "from": "T45n1909_008:0954c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若近若遠。",
        "from": "T45n1909_008:0954c09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東西南北四維上下遍空法界有大神足力有大威德力。",
        "from": "T45n1909_008:0954c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是一切龍王一切龍神。",
        "from": "T45n1909_008:0954c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各及眷屬。",
        "from": "T45n1909_008:0954c11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命敬禮一切世間大慈悲父。",
        "from": "T45n1909_008:0954c12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無彌勒佛　南無釋迦牟尼佛　南無妙智佛　南無梵財佛　南無實音佛　南無正智佛　南無力得佛　南無師子意佛　南無華相佛　南無積智佛　南無華齒佛　南無功德藏佛　南無名實佛　南無希有名佛　南無上戒佛　南無無畏佛　南無日明佛　南無梵壽佛　南無一切天佛　南無樂智佛　南無寶天佛　南無珠藏佛　南無德流布佛　南無智王佛　南無無縛佛　南無堅法佛　南無天德佛南無無邊身菩薩　南無觀世音菩薩又復歸命如是十方盡虛空界一切三寶。",
        "from": "T45n1909_008:0954c24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願以慈悲力同加攝受。",
        "from": "T45n1909_008:0954c25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願諸龍王各及眷屬增輝光明神力自在。",
        "from": "T45n1909_008:0954c26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以無相解斷除緣障。",
        "from": "T45n1909_008:0954c26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永離惡趣常生淨土。",
        "from": "T45n1909_008:0954c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四無量心六波羅蜜。",
        "from": "T45n1909_008:0954c27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常得現前。",
        "from": "T45n1909_008:0954c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四無礙辯六神通力。",
        "from": "T45n1909_008:0954c28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨心自在。",
        "from": "T45n1909_008:0954c28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以慈悲心拯接一切。",
        "from": "T45n1909_008:0954c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙行莊嚴過法雲地。",
        "from": "T45n1909_008:0954c29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入金剛心成等正覺。",
        "from": "T45n1909_008:0955a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "6 奉為魔王禮佛奉為魔王禮佛第六今日道場同業大眾。",
        "from": "T45n1909_008:0955a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重復至誠。",
        "from": "T45n1909_008:0955a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉為大魔王五帝大魔乃至東西南北四維上下盡虛空界一切魔王各及眷屬。",
        "from": "T45n1909_008:0955a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命敬禮一切世間大慈悲父。",
        "from": "T45n1909_008:0955a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無彌勒佛　南無釋迦牟尼佛　南無梵牟尼佛　南無安詳行佛　南無勤精進佛　南無炎肩佛　南無大威德佛　南無薝蔔華佛　南無歡喜佛　南無善眾佛　南無帝幢佛　南無大愛佛　南無須蔓色佛　南無眾妙佛　南無可樂佛　南無善定義佛　南無牛王佛　南無妙臂佛　南無大車佛　南無滿願佛　南無德光佛　南無寶音佛　南無金剛軍佛　南無富貴佛　南無勢力行佛　南無師子力佛　南無淨目佛南無無邊身菩薩　南無觀世音菩薩又復歸命如是十方盡虛空界一切三寶。",
        "from": "T45n1909_008:0955a19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願以慈悲力同加覆護。",
        "from": "T45n1909_008:0955a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願大魔王五帝大魔一切魔王各及眷屬。",
        "from": "T45n1909_008:0955a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無始已來至于今日。",
        "from": "T45n1909_008:0955a21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切緣障皆得清淨。",
        "from": "T45n1909_008:0955a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切罪業皆得銷滅。",
        "from": "T45n1909_008:0955a22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾苦皆得解脫。",
        "from": "T45n1909_008:0955a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四無量心六波羅蜜常得現前。",
        "from": "T45n1909_008:0955a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四無礙智六神通力如意自在。",
        "from": "T45n1909_008:0955a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行菩薩道不休不息。",
        "from": "T45n1909_008:0955a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先度眾生然後作佛。",
        "from": "T45n1909_008:0955a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "7 為人道禮佛為人道禮佛第七今日道場同業大眾。",
        "from": "T45n1909_008:0955a27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相與已得。",
        "from": "T45n1909_008:0955a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉為諸天諸仙。",
        "from": "T45n1909_008:0955a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍神八部。",
        "from": "T45n1909_008:0955a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮佛竟。",
        "from": "T45n1909_008:0955a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應奉為人道一切人王禮佛報恩。",
        "from": "T45n1909_008:0955a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又為父母師長一切人民。",
        "from": "T45n1909_008:0955a29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故爾。",
        "from": "T45n1909_008:0955b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無國王一切眾生無所依附。",
        "from": "T45n1909_008:0955b01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由大王故一切得住。",
        "from": "T45n1909_008:0955b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行國王地飲國王水。",
        "from": "T45n1909_008:0955b02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸餘利益不可具說。",
        "from": "T45n1909_008:0955b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大眾宜各起報恩心。",
        "from": "T45n1909_008:0955b03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經言。",
        "from": "T45n1909_008:0955b03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能一日一夜六時忍苦為欲利益奉報恩者。",
        "from": "T45n1909_008:0955b04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當發起如是等心習行慈悲。",
        "from": "T45n1909_008:0955b05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以是願力。",
        "from": "T45n1909_008:0955b05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "報國王恩。",
        "from": "T45n1909_008:0955b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "報施主恩。",
        "from": "T45n1909_008:0955b06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又當念行道報父母慈育之恩。",
        "from": "T45n1909_008:0955b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又當行道報師長慈訓之恩。",
        "from": "T45n1909_008:0955b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次復運心念如來恩。",
        "from": "T45n1909_008:0955b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能至心常念不絕者。",
        "from": "T45n1909_008:0955b08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等人得入道疾。",
        "from": "T45n1909_008:0955b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "1 奉為國王禮佛奉為國王禮佛第一今日道場同業大眾。",
        "from": "T45n1909_008:0955b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛大聖慈恩開誘慇懃如此令知恩報恩。",
        "from": "T45n1909_008:0955b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等今日既仰賴國王。",
        "from": "T45n1909_008:0955b12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於末世中興顯佛法。",
        "from": "T45n1909_008:0955b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種供養不惜財寶。",
        "from": "T45n1909_008:0955b13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "率土臣民望風歸附。",
        "from": "T45n1909_008:0955b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又令出家之人安身行道。",
        "from": "T45n1909_008:0955b14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行住坐臥初無留難。",
        "from": "T45n1909_008:0955b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡百不預唯獎以善。",
        "from": "T45n1909_008:0955b15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆願我等速出生死。",
        "from": "T45n1909_008:0955b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闡無量法門開人天正路。",
        "from": "T45n1909_008:0955b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而國王有如此恩德。",
        "from": "T45n1909_008:0955b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈得不人人禮拜奉報其恩相與至心等一痛切。",
        "from": "T45n1909_008:0955b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉為國王。",
        "from": "T45n1909_008:0955b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸依世間大慈悲父。",
        "from": "T45n1909_008:0955b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無彌勒佛　南無釋迦牟尼佛　南無迦葉佛　南無淨意佛　南無知次第佛　南無猛威德佛　南無大光明佛　南無日光曜佛　南無淨藏佛　南無分別威佛　南無無損佛　南無密日佛　南無月光佛　南無持明佛　南無善寂行佛　南無不動佛　南無大請佛　南無德法佛　南無莊嚴王佛　南無高出佛　南無炎熾佛　南無華德佛　南無寶嚴佛　南無上善佛　南無寶上佛　南無利慧佛　南無嚴土佛南無無邊身菩薩　南無觀世音菩薩又復歸命如是十方盡虛空界一切三寶。",
        "from": "T45n1909_008:0955c03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願以慈悲力同加攝受。",
        "from": "T45n1909_008:0955c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願當今皇帝舊云大梁皇帝聖體康御。",
        "from": "T45n1909_008:0955c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天威振遠帝基永固。",
        "from": "T45n1909_008:0955c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧命無窮慈霑無際。",
        "from": "T45n1909_008:0955c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有識歸心菩薩盛化。",
        "from": "T45n1909_008:0955c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天人讚仰四等六度。",
        "from": "T45n1909_008:0955c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日夜增明四無礙辯。",
        "from": "T45n1909_008:0955c07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樂說無盡。",
        "from": "T45n1909_008:0955c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得八自在具六神通。",
        "from": "T45n1909_008:0955c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三昧總持。",
        "from": "T45n1909_008:0955c08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應念現前慈悲即世恩遍六道。",
        "from": "T45n1909_008:0955c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬行早圓速登正覺。",
        "from": "T45n1909_008:0955c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "2 奉為諸王禮佛奉為諸王禮佛第二今日道場同業大眾。",
        "from": "T45n1909_008:0955c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重復至誠五體投地奉為。",
        "from": "T45n1909_008:0955c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皇太子殿下。",
        "from": "T45n1909_008:0955c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分土諸王舊云臨川諸王各及眷屬。",
        "from": "T45n1909_008:0955c14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸依世間大慈悲父。",
        "from": "T45n1909_008:0955c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無彌勒佛　南無釋迦牟尼佛　南無海德佛　南無梵相佛　南無月蓋佛　南無多炎佛　南無違藍王佛　南無智稱佛　南無覺想佛　南無功德光佛　南無聲流布佛　南無滿月佛　南無華光佛　南無善戒佛　南無燈王佛　南無電光佛　南無光王佛　南無光明佛　南無具足讚佛　南無華藏佛　南無弗沙佛　南無日端嚴佛　南無淨善佛　南無威猛軍佛　南無福威德佛　南無力行佛　南無羅睺天佛南無無邊身菩薩　南無觀世音菩薩又復歸依如是十方盡虛空界一切三寶。",
        "from": "T45n1909_008:0956a05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願以慈悲力同加覆護。",
        "from": "T45n1909_008:0956a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願皇太子殿下。",
        "from": "T45n1909_008:0956a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分土諸王各及眷屬。",
        "from": "T45n1909_008:0956a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身心安樂妙算無窮。",
        "from": "T45n1909_008:0956a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行大乘道入佛智慧。",
        "from": "T45n1909_008:0956a08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被四弘誓不捨一切。",
        "from": "T45n1909_008:0956a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四等六度常得現前。",
        "from": "T45n1909_008:0956a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六通三達善識根性。",
        "from": "T45n1909_008:0956a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具二莊嚴神力自在。",
        "from": "T45n1909_008:0956a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行如來慈攝化四生。",
        "from": "T45n1909_008:0956a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "3 奉為父母禮佛奉為父母禮佛第三今日道場同業大眾。",
        "from": "T45n1909_008:0956a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次復應須五體投地奉為父母育養之恩。",
        "from": "T45n1909_008:0956a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懷抱乳哺愛重情深。",
        "from": "T45n1909_008:0956a14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寧自危身安立其子。",
        "from": "T45n1909_008:0956a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至年長大訓以仁禮。",
        "from": "T45n1909_008:0956a15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洗掌求師願通經義。",
        "from": "T45n1909_008:0956a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時刻不忘企及人流。",
        "from": "T45n1909_008:0956a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所當供給不悋家寶。",
        "from": "T45n1909_008:0956a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念思慮結有亦成病臥不安席。",
        "from": "T45n1909_008:0956a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常憶其子。",
        "from": "T45n1909_008:0956a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天下恩重世實無二。",
        "from": "T45n1909_008:0956a18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以佛言。",
        "from": "T45n1909_008:0956a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天下之恩莫過父母。",
        "from": "T45n1909_008:0956a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫捨家人未能得道。",
        "from": "T45n1909_008:0956a19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯勤學業為善莫廢。",
        "from": "T45n1909_008:0956a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "積德不止。",
        "from": "T45n1909_008:0956a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必能報恩相與至心等一痛切五體投地。",
        "from": "T45n1909_008:0956a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各自奉為有識神已來至于今日。",
        "from": "T45n1909_008:0956a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經生父母歷劫親緣一切眷屬。",
        "from": "T45n1909_008:0956a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸依世間大慈悲父。",
        "from": "T45n1909_008:0956a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無彌勒佛　南無釋迦牟尼佛　南無智聚佛　南無調御佛　南無如王佛　南無華相佛　南無羅睺羅佛　南無大藥佛　南無宿王佛　南無華王佛　南無德手佛　南無得叉迦佛　南無流布王佛　南無日光佛　南無法藏佛　南無妙音佛　南無德主佛　南無金剛眾佛　南無慧頂佛　南無善住佛　南無意行佛　南無梵音佛　南無師子佛　南無雷音佛　南無通相佛　南無安隱佛　南無慧陰佛　南無無邊身菩薩　南無觀世音菩薩又復歸依如是十方盡虛空界一切三寶。",
        "from": "T45n1909_008:0956b06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願以慈悲力同加攝受。",
        "from": "T45n1909_008:0956b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願父母親緣各及眷屬。",
        "from": "T45n1909_008:0956b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從今日去至于菩提。",
        "from": "T45n1909_008:0956b08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切罪障皆得除滅。",
        "from": "T45n1909_008:0956b08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾苦畢竟解脫。",
        "from": "T45n1909_008:0956b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結習煩惱永得清淨。",
        "from": "T45n1909_008:0956b09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長辭四趣自在往生。",
        "from": "T45n1909_008:0956b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "親侍諸佛現前受記。",
        "from": "T45n1909_008:0956b10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四無量心六波羅蜜。",
        "from": "T45n1909_008:0956b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常不離行四無礙智。",
        "from": "T45n1909_008:0956b11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六神通力如意自在。",
        "from": "T45n1909_008:0956b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得佛十力相好嚴身。",
        "from": "T45n1909_008:0956b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同坐道場成等正覺。",
        "from": "T45n1909_008:0956b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "4 各為過去父母禮佛各為過去父母禮佛第四今日道場同業大眾。",
        "from": "T45n1909_008:0956b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其中若有父母少便孤背難復再遇。",
        "from": "T45n1909_008:0956b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬劫悠然既未得天眼。",
        "from": "T45n1909_008:0956b16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "死生智明不知父母捨報神識更生何道。",
        "from": "T45n1909_008:0956b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯當競設福力功德追而報恩。",
        "from": "T45n1909_008:0956b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為善不止功成必致。",
        "from": "T45n1909_008:0956b18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經言。",
        "from": "T45n1909_008:0956b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為亡人作福如餉遠人。",
        "from": "T45n1909_008:0956b19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若生人天增益功德。",
        "from": "T45n1909_008:0956b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若處三途。",
        "from": "T45n1909_008:0956b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或在八難速令解脫。",
        "from": "T45n1909_008:0956b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生若值佛受正法教永離眾苦憂畏悉除七世久遠歷劫親緣十方眾生同得解脫。",
        "from": "T45n1909_008:0956b22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為智者至慈至孝最上報恩。",
        "from": "T45n1909_008:0956b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相與今日若有過去者應當悲泣追懷懊惱嗚呼哽慟奉為過去父母歷劫親緣五體投地。",
        "from": "T45n1909_008:0956b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸依世間大慈悲父。",
        "from": "T45n1909_008:0956b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無彌勒佛　南無釋迦牟尼佛　南無梵王佛　南無大牛王佛　南無梨陀目佛　南無龍德佛　南無寶相佛　南無莊嚴佛　南無不沒音佛　南無華持佛　南無音得佛　南無師子佛　南無莊嚴辭佛　南無勇智佛　南無華積佛　南無華開佛　南無力行佛　南無德積佛　南無上形色佛　南無明曜佛　南無月燈佛　南無威德王佛　南無菩提王佛　南無無盡佛　南無菩提眼佛　南無身充滿佛　南無慧國佛南無無邊身菩薩　南無觀世音菩薩又復歸依如是十方。",
        "from": "T45n1909_008:0956c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡虛空界一切三寶。",
        "from": "T45n1909_008:0956c10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願以慈悲力救護拯接。",
        "from": "T45n1909_008:0956c11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願大眾各有過去父母歷劫眷屬。",
        "from": "T45n1909_008:0956c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從今日去至于道場。",
        "from": "T45n1909_008:0956c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切罪緣皆得消殄。",
        "from": "T45n1909_008:0956c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切苦果永得除滅。",
        "from": "T45n1909_008:0956c13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煩惱結業畢竟清淨。",
        "from": "T45n1909_008:0956c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷三障緣無五怖畏。",
        "from": "T45n1909_008:0956c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行菩薩道廣化一切。",
        "from": "T45n1909_008:0956c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八解洗心四弘被物。",
        "from": "T45n1909_008:0956c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面奉慈顏諮承妙旨。",
        "from": "T45n1909_008:0956c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不起本處盡諸有漏。",
        "from": "T45n1909_008:0956c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨念逍遙遍諸佛土。",
        "from": "T45n1909_008:0956c16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行願早登速成正覺。",
        "from": "T45n1909_008:0956c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "5 奉為師長禮佛奉為師長禮佛第五今日道場同業大眾。",
        "from": "T45n1909_008:0956c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相與已為父母親緣禮佛竟。",
        "from": "T45n1909_008:0956c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次復應念師長恩德。",
        "from": "T45n1909_008:0956c20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虔誠禮佛何以故爾。",
        "from": "T45n1909_008:0956c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父母雖復生育我等。",
        "from": "T45n1909_008:0956c21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能令我速離惡趣。",
        "from": "T45n1909_008:0956c21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師長於我恩德無量。",
        "from": "T45n1909_008:0957a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大慈獎諭恒使修善。",
        "from": "T45n1909_008:0957a01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願出生死到於彼岸。",
        "from": "T45n1909_008:0957a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每事利益令得見佛。",
        "from": "T45n1909_008:0957a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除煩惱結永處無為。",
        "from": "T45n1909_008:0957a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如此至德誰能上報。",
        "from": "T45n1909_008:0957a03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能終身行道正可自利非報師恩。",
        "from": "T45n1909_008:0957a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故佛言。",
        "from": "T45n1909_008:0957a04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善知識者莫過師長。",
        "from": "T45n1909_008:0957a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既能自度亦復度人。",
        "from": "T45n1909_008:0957a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相與今日稟得出家。",
        "from": "T45n1909_008:0957a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受具足戒。",
        "from": "T45n1909_008:0957a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此之重恩從師長得。",
        "from": "T45n1909_008:0957a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈不人人追念此恩。",
        "from": "T45n1909_008:0957a07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相與至心等一痛切五體投地。",
        "from": "T45n1909_008:0957a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉為和上阿闍梨同壇尊證上中下座各及眷屬。",
        "from": "T45n1909_008:0957a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸依世間大慈悲父。",
        "from": "T45n1909_008:0957a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無彌勒佛　南無釋迦牟尼佛　南無最上佛　南無清涼照佛　南無慧德佛　南無妙音佛　南無導師佛　南無無礙藏佛　南無上施佛　南無大尊佛　南無智力勢佛　南無大焰佛　南無帝王佛　南無制力佛　南無威德佛　南無善明佛　南無名聞佛　南無端嚴佛　南無無塵垢佛　南無威儀佛　南無師子軍佛　南無天王佛　南無名聲佛　南無殊勝佛　南無大藏佛　南無福德光佛　南無梵聞佛南無無邊身菩薩　南無觀世音菩薩又復歸依如是十方盡虛空界一切三寶。",
        "from": "T45n1909_008:0957a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願以慈悲力同加攝受。",
        "from": "T45n1909_008:0957a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願和上阿闍梨同壇尊證上中下座各及眷屬。",
        "from": "T45n1909_008:0957a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從今日去至坐道場。",
        "from": "T45n1909_008:0957a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切罪障皆得清淨。",
        "from": "T45n1909_008:0957a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾苦悉得解脫。",
        "from": "T45n1909_008:0957a24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切煩惱皆得斷除。",
        "from": "T45n1909_008:0957a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨念往生諸佛淨土。",
        "from": "T45n1909_008:0957a25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩提行願皆悉具足。",
        "from": "T45n1909_008:0957a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "財施無盡法施無盡。",
        "from": "T45n1909_008:0957a26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福德無盡安樂無盡。",
        "from": "T45n1909_008:0957a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽命無盡智慧無盡。",
        "from": "T45n1909_008:0957a27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四無量心六波羅蜜。",
        "from": "T45n1909_008:0957a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常得現前。",
        "from": "T45n1909_008:0957a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四無礙智六神通力。",
        "from": "T45n1909_008:0957a28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如意自在。",
        "from": "T45n1909_008:0957a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住首楞嚴三昧得金剛身。",
        "from": "T45n1909_008:0957a29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不捨本誓還救眾生。",
        "from": "T45n1909_008:0957b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "6 為十方比丘比丘尼禮佛為十方比丘比丘尼禮佛第六今日道場同業大眾。",
        "from": "T45n1909_008:0957b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此禮拜之次。",
        "from": "T45n1909_008:0957b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重復增到五體投地。",
        "from": "T45n1909_008:0957b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普為十方盡虛空界。",
        "from": "T45n1909_008:0957b04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現在未來一切。",
        "from": "T45n1909_008:0957b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比丘比丘尼。",
        "from": "T45n1909_008:0957b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "式叉摩那。",
        "from": "T45n1909_008:0957b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙彌沙彌尼各及眷屬。",
        "from": "T45n1909_008:0957b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又為十方盡虛空界一切優婆塞優婆夷各及眷屬。",
        "from": "T45n1909_008:0957b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又為從來信施檀越。",
        "from": "T45n1909_008:0957b07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善惡知識有緣無緣各及眷屬。",
        "from": "T45n1909_008:0957b08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是人道一切人類各及眷屬。",
        "from": "T45n1909_008:0957b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "某甲等。",
        "from": "T45n1909_008:0957b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今日以慈悲心。",
        "from": "T45n1909_008:0957b09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普為歸依世間大慈悲父。",
        "from": "T45n1909_008:0957b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無彌勒佛　南無釋迦牟尼佛　南無燈王佛　南無智頂佛　南無上天佛　南無地王佛　南無至解脫佛　南無金髻佛　南無羅睺日佛　南無莫能勝佛　南無牟尼淨佛　南無善光佛　南無金齊佛　南無眾德天王佛　南無法益佛　南無德臂佛　南無鴦伽陀佛　南無美妙惠佛　南無微意佛　南無諸威德佛　南無師子髮佛　南無解脫相佛　南無威相佛　南無斷流佛　南無慧藏佛　南無智聚佛　南無無礙讚佛南無無邊身菩薩　南無觀世音菩薩又復歸依如是十方盡虛空界一切三寶。",
        "from": "T45n1909_008:0957b23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願以慈悲力同加覆護。",
        "from": "T45n1909_008:0957b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願十方盡虛空界一切比丘比丘尼式叉摩那沙彌沙彌尼各及眷屬。",
        "from": "T45n1909_008:0957b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又願十方一切優婆塞優婆夷。",
        "from": "T45n1909_008:0957b26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各及眷屬。",
        "from": "T45n1909_008:0957b26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又願從來信施壇越善惡知識有緣無緣各及眷屬。",
        "from": "T45n1909_008:0957b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至一切人道一切人類。",
        "from": "T45n1909_008:0957b28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無始已來至于今日。",
        "from": "T45n1909_008:0957b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切煩惱皆得斷除。",
        "from": "T45n1909_008:0957b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切緣障皆得清淨。",
        "from": "T45n1909_008:0957c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切罪業皆得消滅。",
        "from": "T45n1909_008:0957c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾苦皆得解脫。",
        "from": "T45n1909_008:0957c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離三障業無五怖畏。",
        "from": "T45n1909_008:0957c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四無量心六波羅蜜。",
        "from": "T45n1909_008:0957c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常得現前。",
        "from": "T45n1909_008:0957c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四無礙智六神通力如意自在。",
        "from": "T45n1909_008:0957c03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行菩薩行入一乘道。",
        "from": "T45n1909_008:0957c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "度脫無邊一切眾生。",
        "from": "T45n1909_008:0957c04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "7 為十方過去比丘比丘尼禮佛為十方過去比丘比丘尼禮佛第七今日道場同業大眾。",
        "from": "T45n1909_008:0957c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重復至誠五體投地。",
        "from": "T45n1909_008:0957c06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "代為十方盡虛空界一切過去比丘比丘尼。",
        "from": "T45n1909_008:0957c07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "式叉摩那沙彌沙彌尼。",
        "from": "T45n1909_008:0957c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過去優婆塞優婆夷。",
        "from": "T45n1909_008:0957c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣及十方一切人道一切人類有命過者各及眷屬。",
        "from": "T45n1909_008:0957c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "某甲等今日以慈悲心。",
        "from": "T45n1909_008:0957c10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "等諸佛心同諸佛願。",
        "from": "T45n1909_008:0957c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普為歸依世間大慈悲父。",
        "from": "T45n1909_008:0957c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無彌勒佛　南無釋迦牟尼佛　南無寶聚佛　南無善高佛　南無山王相佛　南無法頂佛　南無解脫德佛　南無善端嚴佛　南無吉身佛　南無愛語佛　南無師子利佛　南無和樓那佛　南無師子法佛　南無法力佛　南無愛樂佛　南無讚不動佛　南無眾明王佛　南無覺悟眾生佛　南無妙眼佛　南無意住義佛　南無光照佛　南無香德佛　南無令喜佛　南無不虛行佛　南無滅恚佛　南無上色佛　南無善步佛南無無邊身菩薩　南無觀世音菩薩又復歸依如是十方盡虛空界一切三寶。",
        "from": "T45n1909_008:0957c24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願以慈悲力救護拯接。",
        "from": "T45n1909_008:0957c25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願過去一切比丘比丘尼。",
        "from": "T45n1909_008:0957c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "式叉摩那沙彌沙彌尼。",
        "from": "T45n1909_008:0957c26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各及眷屬。",
        "from": "T45n1909_008:0957c26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又願過去一切優婆塞優婆夷。",
        "from": "T45n1909_008:0957c27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各及眷屬。",
        "from": "T45n1909_008:0957c27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有地獄道苦今日即得解脫。",
        "from": "T45n1909_008:0957c28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有畜生道苦今日即得解脫。",
        "from": "T45n1909_008:0957c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有鬼神道苦今日即得解脫。",
        "from": "T45n1909_008:0957c29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離八難地受八福生。",
        "from": "T45n1909_008:0958a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永捨惡道長生淨土。",
        "from": "T45n1909_008:0958a01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "財施無盡法施無盡。",
        "from": "T45n1909_008:0958a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福德無盡安樂無盡。",
        "from": "T45n1909_008:0958a02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽命無盡智慧無盡。",
        "from": "T45n1909_008:0958a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四無量心六波羅蜜。",
        "from": "T45n1909_008:0958a03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常得現前。",
        "from": "T45n1909_008:0958a03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四無礙智六神通力。",
        "from": "T45n1909_008:0958a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如意自在。",
        "from": "T45n1909_008:0958a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常得見佛聞法。",
        "from": "T45n1909_008:0958a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行菩薩道勇猛精進。",
        "from": "T45n1909_008:0958a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不休不息。",
        "from": "T45n1909_008:0958a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至進修成阿耨多羅三藐三菩提。",
        "from": "T45n1909_008:0958a06_10",
