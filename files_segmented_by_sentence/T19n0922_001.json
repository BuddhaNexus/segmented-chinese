[
    {
        "source_sentence": "藥師瑠璃光如來消災除難念誦",
        "from": "T19n0922_001:0020b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儀軌",
        "from": "T19n0922_001:0020b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先淨身口意　　歸命佛法僧",
        "from": "T19n0922_001:0020b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮遍照尊　　十方諸聖眾",
        "from": "T19n0922_001:0020b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今略開演　　祕密消災法",
        "from": "T19n0922_001:0020b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此法世尊說　　最勝最第一",
        "from": "T19n0922_001:0020b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速出離生死　　疾證大菩提",
        "from": "T19n0922_001:0020b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為順眾生界　　及說除災難",
        "from": "T19n0922_001:0020b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增敬降伏法　　女人懷難月",
        "from": "T19n0922_001:0020b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "產危難生子　　及遭疾患者",
        "from": "T19n0922_001:0020b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神鬼作禍殃　　建立曼拏攞",
        "from": "T19n0922_001:0020b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塑畫本尊像　　燈燃四十九",
        "from": "T19n0922_001:0020b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養瑠璃尊　　晝夜恒照明",
        "from": "T19n0922_001:0020b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉香花菓食　　五色成幡蓋",
        "from": "T19n0922_001:0020b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放水陸眾生　　日滿四十九",
        "from": "T19n0922_001:0020b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持五色線　　繫於病人項",
        "from": "T19n0922_001:0020b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲修如是法　　智者先從師",
        "from": "T19n0922_001:0020b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受三昧耶戒　　發大菩提心",
        "from": "T19n0922_001:0020b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得持明灌頂　　阿闍梨印可",
        "from": "T19n0922_001:0020b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後乃修持　　山林閑靜處",
        "from": "T19n0922_001:0020b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "河池及海岸　　或自居住處",
        "from": "T19n0922_001:0020b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗拭曼拏攞　　方圓隨本意",
        "from": "T19n0922_001:0020b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置二閼伽水　　安排下七位",
        "from": "T19n0922_001:0020b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒香花燈明　　飲食塗香等",
        "from": "T19n0922_001:0020b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陳設壇四邊　　懺悔并隨喜",
        "from": "T19n0922_001:0020b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勸請發願等　　回向諸功德",
        "from": "T19n0922_001:0020b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相應成悉地　　順教而修習",
        "from": "T19n0922_001:0020b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛部心密印　　定慧應內縛",
        "from": "T19n0922_001:0020c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雙慧俱申竪　　觀佛滿虛空",
        "from": "T19n0922_001:0020c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言七遍　　頂上而散印",
        "from": "T19n0922_001:0020c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0922_001:0020c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此加持故　　諸佛悉雲集",
        "from": "T19n0922_001:0020c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光明照行人　　離障速成就",
        "from": "T19n0922_001:0020c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結蓮華部　　心印如前契",
        "from": "T19n0922_001:0020c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈定輪入掌　　慧輪而建立",
        "from": "T19n0922_001:0020c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應觀觀自在　　與蓮華眷屬",
        "from": "T19n0922_001:0020c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在諸如來右　　念真言七遍",
        "from": "T19n0922_001:0020c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此加持故　　菩薩眾雲集",
        "from": "T19n0922_001:0020c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行人為同事　　所求皆滿足",
        "from": "T19n0922_001:0020c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結金剛部　　心印亦如前",
        "from": "T19n0922_001:0020c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈慧輪入掌　　定輪而竪建",
        "from": "T19n0922_001:0020c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應觀金剛手　　一切持金剛",
        "from": "T19n0922_001:0020c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住在如來右　　念真言七遍",
        "from": "T19n0922_001:0020c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此加持故　　金剛眾雲集",
        "from": "T19n0922_001:0020c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "承佛本悲願　　衛護修行者",
        "from": "T19n0922_001:0020c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三業如金剛　　堅固無能壞",
        "from": "T19n0922_001:0021a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結被甲印　　止觀各為拳",
        "from": "T19n0922_001:0021a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持輪橫在掌　　止拳而安心",
        "from": "T19n0922_001:0021a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀拳加五處　　額兩肩心喉",
        "from": "T19n0922_001:0021a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此印真言　　加持被甲故",
        "from": "T19n0922_001:0021a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸魔不陵逼　　速疾獲成就",
        "from": "T19n0922_001:0021a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切佛頂中　　是印大威德",
        "from": "T19n0922_001:0021a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纔結印護身　　人天皆敬仰",
        "from": "T19n0922_001:0021a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切嶮難處　　及諸災難時",
        "from": "T19n0922_001:0021a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆獲安樂　　次志心虔懇",
        "from": "T19n0922_001:0021a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舒奢摩他輪　　毘鉢含那持",
        "from": "T19n0922_001:0021a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印地念真言　　加持滿七遍",
        "from": "T19n0922_001:0021a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉此雜染所　　成淨妙佛土",
        "from": "T19n0922_001:0021a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此加持故　　成本尊淨土",
        "from": "T19n0922_001:0021a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大寶宮殿　　種種持莊嚴",
        "from": "T19n0922_001:0021a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於寶宮殿中　　想藥師如來",
        "from": "T19n0922_001:0021a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩眾圍繞　　而觀曼拏攞",
        "from": "T19n0922_001:0021a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結迎請印　　用前佛部心",
        "from": "T19n0922_001:0021a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二輪向身招　　念真言三遍",
        "from": "T19n0922_001:0021a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此加持故　　一切作障者",
        "from": "T19n0922_001:0021a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆而遠走　　恭敬修行者",
        "from": "T19n0922_001:0021a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次獻閼伽水　　時花汎其上",
        "from": "T19n0922_001:0021a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶藥香種子　　置之於水內",
        "from": "T19n0922_001:0021b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捧至頂上獻　　念真言七遍",
        "from": "T19n0922_001:0021b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想浴聖眾足　　稱所求之願",
        "from": "T19n0922_001:0021b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由獻香水故　　離垢獲清淨",
        "from": "T19n0922_001:0021b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當得灌頂地　　證如來法身",
        "from": "T19n0922_001:0021b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結獻座印　　五頂開敷花",
        "from": "T19n0922_001:0021b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想從印流出　　蓮華師子座",
        "from": "T19n0922_001:0021b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此真言印　　本尊并眷屬",
        "from": "T19n0922_001:0021b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各受寶蓮華　　金剛師子座",
        "from": "T19n0922_001:0021b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結普供養　　大印之儀則",
        "from": "T19n0922_001:0021b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸頂初分交　　從印中流出",
        "from": "T19n0922_001:0021b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種諸供養　　燒香花燈明",
        "from": "T19n0922_001:0021b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗香飲食等　　眾伎樂雲海",
        "from": "T19n0922_001:0021b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臺繖寶樓閣　　寶座等莊嚴",
        "from": "T19n0922_001:0021b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勝妙幢幡蓋　　賢瓶眾香水",
        "from": "T19n0922_001:0021b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆從印流出　　遍虛空法界",
        "from": "T19n0922_001:0021b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養諸如來　　菩薩及聖眾",
        "from": "T19n0922_001:0021b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便以三等力　　真實妙伽陀",
        "from": "T19n0922_001:0021b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誠心念三遍　　一切皆成就",
        "from": "T19n0922_001:0021b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以我功德力　　如來加持力",
        "from": "T19n0922_001:0021b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及以法界力　　普供養而住",
        "from": "T19n0922_001:0021b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後結佛大慈　　護印之軌儀",
        "from": "T19n0922_001:0021c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五頂堅固縛　　勝輪頂竪合",
        "from": "T19n0922_001:0021c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印心額喉頂　　等諸佛自體",
        "from": "T19n0922_001:0021c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此加持故　　一切魔障難",
        "from": "T19n0922_001:0021c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及惡人相害　　疾起於慈心",
        "from": "T19n0922_001:0021c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結佛眼印　　應以三補吒",
        "from": "T19n0922_001:0021c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二蓋持光背　　猶如笑眼形",
        "from": "T19n0922_001:0021c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二輪各持　　二光中節文",
        "from": "T19n0922_001:0021c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印自身五處　　佛母常加護",
        "from": "T19n0922_001:0021c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此印真言　　加持威力故",
        "from": "T19n0922_001:0021c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能滿一切願　　除一切不祥",
        "from": "T19n0922_001:0021c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生一切福德　　滅一切罪障",
        "from": "T19n0922_001:0021c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令諸有情　　見聞獲安樂",
        "from": "T19n0922_001:0021c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次稱揚讚歎　　本尊之功德",
        "from": "T19n0922_001:0021c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命滿月界　　淨妙瑠璃尊",
        "from": "T19n0922_001:0021c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法藥救人天　　因中十二願",
        "from": "T19n0922_001:0021c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈悲弘誓廣　　願度諸含生",
        "from": "T19n0922_001:0021c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今申讚揚　　志心頭面禮",
        "from": "T19n0922_001:0021c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "根本之密印　　二羽內相叉",
        "from": "T19n0922_001:0021c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩腕稍相去　　開張三二寸",
        "from": "T19n0922_001:0021c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由是本尊故　　誦真言遍數",
        "from": "T19n0922_001:0022a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七遍至百八　　散印於頂上",
        "from": "T19n0922_001:0022a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陳所祈願心　　對彼本尊前",
        "from": "T19n0922_001:0022a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願希垂照矚　　殄災除橫死",
        "from": "T19n0922_001:0022a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次即取數珠　　盤置雙掌內",
        "from": "T19n0922_001:0022a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持成七遍　　捧珠安頂上",
        "from": "T19n0922_001:0022a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此真言印　　加持念珠故",
        "from": "T19n0922_001:0022a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從母珠初起　　一遍捻一珠",
        "from": "T19n0922_001:0022a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言末字畢　　至母珠却迴",
        "from": "T19n0922_001:0022a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應越母珠　　驀過越法罪",
        "from": "T19n0922_001:0022a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣教萬八千　　略之一百八",
        "from": "T19n0922_001:0022a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "限數既終畢　　再捧珠加持",
        "from": "T19n0922_001:0022a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱所祈求事　　志心普迴施",
        "from": "T19n0922_001:0022a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是法印呪。",
        "from": "T19n0922_001:0022a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人等。",
        "from": "T19n0922_001:0022a29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多諸障難一切疾病。",
        "from": "T19n0922_001:0022b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "女人難月產厄。",
        "from": "T19n0922_001:0022b01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願欲轉禍求福。",
        "from": "T19n0922_001:0022b01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并患鬼神病難差者。",
        "from": "T19n0922_001:0022b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以五色線搓索。",
        "from": "T19n0922_001:0022b02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪繫病人項及手足腰腹等。",
        "from": "T19n0922_001:0022b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仍須請七僧。",
        "from": "T19n0922_001:0022b03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建置道場造本尊像。",
        "from": "T19n0922_001:0022b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寫藥師經六時行道。",
        "from": "T19n0922_001:0022b04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燃七層燈。",
        "from": "T19n0922_001:0022b04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造五色幡四十九尺。",
        "from": "T19n0922_001:0022b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日轉經四十九遍。",
        "from": "T19n0922_001:0022b05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放水陸生命四十九頭。",
        "from": "T19n0922_001:0022b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時花菓子殷勤供養。",
        "from": "T19n0922_001:0022b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪五色線發願。",
        "from": "T19n0922_001:0022b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以印拄於線上。",
        "from": "T19n0922_001:0022b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更呪四十九遍。",
        "from": "T19n0922_001:0022b07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結四十九結。",
        "from": "T19n0922_001:0022b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復念此真言。",
        "from": "T19n0922_001:0022b08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持真言曰。",
        "from": "T19n0922_001:0022b08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復讚禮本尊　　結本尊密印",
        "from": "T19n0922_001:0022b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念真言七遍　　復獻閼伽水",
        "from": "T19n0922_001:0022b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前申供養　　辨事佛頂印",
        "from": "T19n0922_001:0022b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念明句三遍　　左旋而解界",
        "from": "T19n0922_001:0022b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉送於聖眾　　如前迎請印",
        "from": "T19n0922_001:0022b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二輪向外撥　　想像虛空中",
        "from": "T19n0922_001:0022b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還歸於淨土　　安居寶宮殿",
        "from": "T19n0922_001:0022b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉送本尊已　　即結佛慈護",
        "from": "T19n0922_001:0022b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三昧耶密契　　加持印四處",
        "from": "T19n0922_001:0022b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復結佛眼印　　真言印五處",
        "from": "T19n0922_001:0022b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結三部心印　　各本明三遍",
        "from": "T19n0922_001:0022b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復結甲護身　　真言印五處",
        "from": "T19n0922_001:0022b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮佛發大願　　隨意任經行",
        "from": "T19n0922_001:0022c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥師如來消災除難念誦儀軌藥師瑠璃光如來消災除難念誦儀軌一部。",
        "from": "T19n0922_001:0022c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一行阿闍梨撰。",
        "from": "T19n0922_001:0022c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東大寺奝然弟子祚壹之所請也。",
        "from": "T19n0922_001:0022c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而今所刻者。",
        "from": "T19n0922_001:0022c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "靈雲開山淨嚴和尚之挍本。",
        "from": "T19n0922_001:0022c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所希壽諸梓以布普天。",
        "from": "T19n0922_001:0022c06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九橫宏難一時消。",
        "from": "T19n0922_001:0022c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四大沈痾剎那除。",
        "from": "T19n0922_001:0022c07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時享保星舍甲寅臘月吉日和州長谷輪下沙門無等謹識",
        "from": "T19n0922_001:0022c09_100",
        "context_with_next_sentence": false
    }
]