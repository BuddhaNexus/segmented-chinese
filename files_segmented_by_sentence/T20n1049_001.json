[
    {
        "source_sentence": "聖六字增壽大明陀羅尼經",
        "from": "T20n1049_001:0046b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試鴻臚卿",
        "from": "T20n1049_001:0046b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳法大師臣施護奉　詔譯",
        "from": "T20n1049_001:0046b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T20n1049_001:0046b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在舍衛國祇樹給孤獨園。",
        "from": "T20n1049_001:0046b19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時尊者阿難有大疾病。",
        "from": "T20n1049_001:0046b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛自知已即詣彼所。",
        "from": "T20n1049_001:0046b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敷座而坐告阿難曰。",
        "from": "T20n1049_001:0046b21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今諦聽我有六字大明陀羅尼。",
        "from": "T20n1049_001:0046b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能消災患增益壽命。",
        "from": "T20n1049_001:0046b22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝若受持非但自身。",
        "from": "T20n1049_001:0046b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復令四眾苾芻苾芻尼優婆塞優婆夷。",
        "from": "T20n1049_001:0046b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長夜安隱遠離眾苦。",
        "from": "T20n1049_001:0046b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次阿難此六字大明陀羅尼。",
        "from": "T20n1049_001:0046b25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七十七俱胝佛。",
        "from": "T20n1049_001:0046b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并六大威德師同所宣說。",
        "from": "T20n1049_001:0046b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六大師者。",
        "from": "T20n1049_001:0046b26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一如來應正等覺。",
        "from": "T20n1049_001:0046b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二帝釋天主。",
        "from": "T20n1049_001:0046b27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三多聞天王。",
        "from": "T20n1049_001:0046b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四持國天王。",
        "from": "T20n1049_001:0046b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五增長天王。",
        "from": "T20n1049_001:0046b28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六廣目天王。",
        "from": "T20n1049_001:0046b28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是聖賢異口同音。",
        "from": "T20n1049_001:0046b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說陀羅尼曰。",
        "from": "T20n1049_001:0046b29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難底黎難底黎難覩哩去都摩哩半拏哩俱㘓致引摩度摩帝引娑嚩二合引賀引佛告阿難。",
        "from": "T20n1049_001:0046c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此六字大明章句有大威力。",
        "from": "T20n1049_001:0046c03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人王法難中驚怖。",
        "from": "T20n1049_001:0046c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大水難中驚怖大火難中驚怖。",
        "from": "T20n1049_001:0046c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賊劫難中驚怖冤家難中驚怖。",
        "from": "T20n1049_001:0046c05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾惡難中驚怖鬪戰難中驚怖惡曜難中驚怖。",
        "from": "T20n1049_001:0046c06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是諸難害身之時。",
        "from": "T20n1049_001:0046c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心稱念大明章句。",
        "from": "T20n1049_001:0046c07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擁護某甲令得解脫。",
        "from": "T20n1049_001:0046c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是語已是諸眾難速得消除。",
        "from": "T20n1049_001:0046c08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次阿難若諸有情。",
        "from": "T20n1049_001:0046c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "患諸疼痛頭痛項痛眼耳鼻痛。",
        "from": "T20n1049_001:0046c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牙齒舌痛脣口頰痛。",
        "from": "T20n1049_001:0046c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "胸脇背痛心痛肚痛。",
        "from": "T20n1049_001:0046c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "腰痛胯痛遍身疼痛。",
        "from": "T20n1049_001:0046c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及瀉痢痔瘻風黃痰癊諸惡重病。",
        "from": "T20n1049_001:0046c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前稱念大明章句。",
        "from": "T20n1049_001:0046c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛大威德令一切日月星曜。",
        "from": "T20n1049_001:0046c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅漢聖賢發真實言。",
        "from": "T20n1049_001:0046c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與某甲弟子應作擁護。",
        "from": "T20n1049_001:0046c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "息除災患令得安樂。",
        "from": "T20n1049_001:0046c14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有刀劍毒藥。",
        "from": "T20n1049_001:0046c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虎狼師子蚖蛇蝮蝎。",
        "from": "T20n1049_001:0046c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸惡禽獸皆不為害。",
        "from": "T20n1049_001:0046c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瘧病不著亦不中夭。",
        "from": "T20n1049_001:0046c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至阿波娑摩囉部多毘舍左。",
        "from": "T20n1049_001:0046c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳩盤茶等一切鬼將。",
        "from": "T20n1049_001:0046c17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆遠離不敢為患。",
        "from": "T20n1049_001:0046c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次阿難若諸有情鬼魅所著。",
        "from": "T20n1049_001:0046c19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "連年積月而不捨離。",
        "from": "T20n1049_001:0046c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此真言加持於線繫患人手。",
        "from": "T20n1049_001:0046c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時金剛手大藥叉主。",
        "from": "T20n1049_001:0046c21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以忿怒力破鬼魅頭令作七分。",
        "from": "T20n1049_001:0046c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又令大智舍利弗大神通目乾連。",
        "from": "T20n1049_001:0046c22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持戒羅睺羅及汝阿難陀。",
        "from": "T20n1049_001:0046c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆來擁護令得安隱。",
        "from": "T20n1049_001:0046c23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不爾者須彌山王。",
        "from": "T20n1049_001:0046c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離於本處大海枯竭。",
        "from": "T20n1049_001:0046c24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日月墮落大地崩裂。",
        "from": "T20n1049_001:0046c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來應正等覺無有妄語。",
        "from": "T20n1049_001:0046c25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難此六字大明陀羅尼。",
        "from": "T20n1049_001:0046c26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神通威德得未曾有。",
        "from": "T20n1049_001:0046c26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若隨喜聽聞。",
        "from": "T20n1049_001:0046c27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人恒得長壽無病眾惡不侵。",
        "from": "T20n1049_001:0046c27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況受持讀誦書寫供養。",
        "from": "T20n1049_001:0046c28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名成就最上增益之法。",
        "from": "T20n1049_001:0046c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難聞已信受奉行。",
        "from": "T20n1049_001:0046c29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖六字增壽大明陀羅尼經",
        "from": "T20n1049_001:0047a01_100",
        "context_with_next_sentence": false
    }
]