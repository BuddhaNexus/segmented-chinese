[
    {
        "source_sentence": "貝多樹下思惟十二因緣經亦名聞城",
        "from": "T16n0713_001:0826b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二因緣經",
        "from": "T16n0713_001:0826b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吳月氏優婆塞支謙譯",
        "from": "T16n0713_001:0826b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞如是：",
        "from": "T16n0713_001:0826b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在舍衛國，止祇樹給孤獨園。",
        "from": "T16n0713_001:0826b08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時佛告：",
        "from": "T16n0713_001:0826b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「比丘！",
        "from": "T16n0713_001:0826b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」比丘便應：",
        "from": "T16n0713_001:0826b09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「唯然。",
        "from": "T16n0713_001:0826b09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」比丘從佛聽。",
        "from": "T16n0713_001:0826b09_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛便說言：",
        "from": "T16n0713_001:0826b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「諸比丘！",
        "from": "T16n0713_001:0826b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我本未得佛道為菩薩時，為念是：",
        "from": "T16n0713_001:0826b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『咄！",
        "from": "T16n0713_001:0826b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是世間極劇，為生、老、死，為往受但苦。",
        "from": "T16n0713_001:0826b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當何時從老、死得要也？",
        "from": "T16n0713_001:0826b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』「諸比丘！",
        "from": "T16n0713_001:0826b12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便自思惟，得是意：",
        "from": "T16n0713_001:0826b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『何以故有老死？",
        "from": "T16n0713_001:0826b13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦何因緣復老死？",
        "from": "T16n0713_001:0826b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』比丘！",
        "from": "T16n0713_001:0826b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便念本，得應意：",
        "from": "T16n0713_001:0826b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『生故為有老死，亦生因緣復老死。",
        "from": "T16n0713_001:0826b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』「比丘！",
        "from": "T16n0713_001:0826b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便思惟念：",
        "from": "T16n0713_001:0826b15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『何以故為生？",
        "from": "T16n0713_001:0826b15_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦何因緣復生？",
        "from": "T16n0713_001:0826b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』比丘！",
        "from": "T16n0713_001:0826b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便自思惟案本念，便得是應意：",
        "from": "T16n0713_001:0826b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『為有故生，亦因緣有復生。",
        "from": "T16n0713_001:0826b17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』「比丘！",
        "from": "T16n0713_001:0826b17_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便思惟，生是意：",
        "from": "T16n0713_001:0826b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『何以故為有有？",
        "from": "T16n0713_001:0826b18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦何因緣復有？",
        "from": "T16n0713_001:0826b18_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』比丘！",
        "from": "T16n0713_001:0826b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便思惟案本念，便生應是意：",
        "from": "T16n0713_001:0826b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『為受故有有，亦受因緣復有。",
        "from": "T16n0713_001:0826b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』「比丘！",
        "from": "T16n0713_001:0826b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便思惟：",
        "from": "T16n0713_001:0826b20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『是何以故受有？",
        "from": "T16n0713_001:0826b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦何因緣復受？",
        "from": "T16n0713_001:0826b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』比丘！",
        "from": "T16n0713_001:0826b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便案本念，得是應意：",
        "from": "T16n0713_001:0826b21_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『為愛故受，亦愛因緣復受。",
        "from": "T16n0713_001:0826b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』「比丘！",
        "from": "T16n0713_001:0826b22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便思惟念：",
        "from": "T16n0713_001:0826b22_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『何以故為愛有？",
        "from": "T16n0713_001:0826b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦何因緣復愛？",
        "from": "T16n0713_001:0826b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』比丘！",
        "from": "T16n0713_001:0826b23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便思惟案本念，得要：",
        "from": "T16n0713_001:0826b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『痛樂故愛，亦痛樂因緣復愛。",
        "from": "T16n0713_001:0826b24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』「比丘！",
        "from": "T16n0713_001:0826b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便思惟念：",
        "from": "T16n0713_001:0826b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『何以痛樂有？",
        "from": "T16n0713_001:0826b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦何因緣痛樂復有？",
        "from": "T16n0713_001:0826b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』比丘！",
        "from": "T16n0713_001:0826b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便思惟案本念，得是應意：",
        "from": "T16n0713_001:0826b26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『更故痛樂有，亦更因緣痛樂復痛樂。",
        "from": "T16n0713_001:0826b27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』「比丘！",
        "from": "T16n0713_001:0826b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便思惟念：",
        "from": "T16n0713_001:0826b27_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『何以故有更？",
        "from": "T16n0713_001:0826b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦何因緣復更？",
        "from": "T16n0713_001:0826b28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』比丘！",
        "from": "T16n0713_001:0826b28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便思惟案本念，便生應意：",
        "from": "T16n0713_001:0826b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『六入故有更，亦六入因緣復更。",
        "from": "T16n0713_001:0826c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』「比丘！",
        "from": "T16n0713_001:0826c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便思惟念：",
        "from": "T16n0713_001:0826c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『何以故有六入？",
        "from": "T16n0713_001:0826c01_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦何因緣復有六入？",
        "from": "T16n0713_001:0826c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』比丘！",
        "from": "T16n0713_001:0826c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便思惟案本念，得是應意：",
        "from": "T16n0713_001:0826c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『名像故有六入，亦名像因緣復有六入。",
        "from": "T16n0713_001:0826c03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』「比丘！",
        "from": "T16n0713_001:0826c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便思惟念：",
        "from": "T16n0713_001:0826c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『何以故名像有？",
        "from": "T16n0713_001:0826c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦何因緣復有名像？",
        "from": "T16n0713_001:0826c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』比丘！",
        "from": "T16n0713_001:0826c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便思惟案本念，得是應意：",
        "from": "T16n0713_001:0826c05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『識故為名像有，亦識因緣復有名像。",
        "from": "T16n0713_001:0826c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』「比丘！",
        "from": "T16n0713_001:0826c06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便思惟念：",
        "from": "T16n0713_001:0826c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『何以故有識？",
        "from": "T16n0713_001:0826c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦何因緣復有識？",
        "from": "T16n0713_001:0826c07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』比丘！",
        "from": "T16n0713_001:0826c07_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便思惟案本念，有是應意：",
        "from": "T16n0713_001:0826c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『名像故為有識，亦名像因緣復識。",
        "from": "T16n0713_001:0826c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』「比丘！",
        "from": "T16n0713_001:0826c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便思惟，生是意：",
        "from": "T16n0713_001:0826c09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『是何等咄是識，還不復前在？",
        "from": "T16n0713_001:0826c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名像因緣識，亦識因緣名像；",
        "from": "T16n0713_001:0826c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名像因緣六入，六入因緣更；",
        "from": "T16n0713_001:0826c11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更因緣痛，痛因緣愛；",
        "from": "T16n0713_001:0826c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛因緣受，受因緣有；",
        "from": "T16n0713_001:0826c12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有因緣生，生因緣老死。",
        "from": "T16n0713_001:0826c13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憂、哭、苦、不可意愁，從是致有。",
        "from": "T16n0713_001:0826c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是但為從五陰，一切苦從習生。",
        "from": "T16n0713_001:0826c14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』「比丘！",
        "from": "T16n0713_001:0826c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便自思惟念：",
        "from": "T16n0713_001:0826c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『何以故無有老死？",
        "from": "T16n0713_001:0826c15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦何故老死滅盡？",
        "from": "T16n0713_001:0826c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』比丘！",
        "from": "T16n0713_001:0826c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便案本念思惟，得是應可意：",
        "from": "T16n0713_001:0826c16_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『無有生亦不老死已，生盡，老死亦盡。",
        "from": "T16n0713_001:0826c17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』「比丘！",
        "from": "T16n0713_001:0826c17_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便自思惟念：",
        "from": "T16n0713_001:0826c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『何以故不復生？",
        "from": "T16n0713_001:0826c18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦何因緣復生盡？",
        "from": "T16n0713_001:0826c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』比丘！",
        "from": "T16n0713_001:0826c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便思惟案本念，生是應意：",
        "from": "T16n0713_001:0826c19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『有無是有者，便不復生；",
        "from": "T16n0713_001:0826c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有盡，復生盡。",
        "from": "T16n0713_001:0826c20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』「比丘！",
        "from": "T16n0713_001:0826c20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便思惟念：",
        "from": "T16n0713_001:0826c20_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『何等無有為有無有？",
        "from": "T16n0713_001:0826c21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等盡為復有盡？",
        "from": "T16n0713_001:0826c21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』諸比丘！",
        "from": "T16n0713_001:0826c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便思惟案本念，自生應意：",
        "from": "T16n0713_001:0826c22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『受無有，便有無有；",
        "from": "T16n0713_001:0826c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受已盡，便有盡。",
        "from": "T16n0713_001:0826c23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』「諸比丘！",
        "from": "T16n0713_001:0826c23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便思惟念：",
        "from": "T16n0713_001:0826c23_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『何等無有令受無有？",
        "from": "T16n0713_001:0826c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦何等盡令受盡？",
        "from": "T16n0713_001:0826c24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』諸比丘！",
        "from": "T16n0713_001:0826c24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便思惟案本念，得應意：",
        "from": "T16n0713_001:0826c25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『無有愛，亦無有受；",
        "from": "T16n0713_001:0826c25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛已盡，便受盡。",
        "from": "T16n0713_001:0826c26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』「諸比丘！",
        "from": "T16n0713_001:0826c26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便思惟念：",
        "from": "T16n0713_001:0826c26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『何無有為愛無有？",
        "from": "T16n0713_001:0826c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何盡復是愛盡？",
        "from": "T16n0713_001:0826c27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』諸比丘！",
        "from": "T16n0713_001:0826c27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便思惟案本念，意生如應：",
        "from": "T16n0713_001:0826c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『痛痒無有，便愛無有；",
        "from": "T16n0713_001:0826c28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "痛痒盡，則愛盡。",
        "from": "T16n0713_001:0826c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』「諸比丘！",
        "from": "T16n0713_001:0826c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便思惟念：",
        "from": "T16n0713_001:0826c29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『何等無有則痛痒無有？",
        "from": "T16n0713_001:0826c29_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等盡是痛痒盡？",
        "from": "T16n0713_001:0827a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』便自思惟案本念，得是應：",
        "from": "T16n0713_001:0827a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『更樂無有，則痛痒無有；",
        "from": "T16n0713_001:0827a02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更樂盡，復痛痒盡。",
        "from": "T16n0713_001:0827a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』「便復思惟念：",
        "from": "T16n0713_001:0827a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『何等無有更樂無有？",
        "from": "T16n0713_001:0827a03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等盡是更樂盡？",
        "from": "T16n0713_001:0827a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』便復思惟念本，得應意：",
        "from": "T16n0713_001:0827a04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『六入無有，即更樂無有；",
        "from": "T16n0713_001:0827a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六入盡，更樂盡。",
        "from": "T16n0713_001:0827a05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』「便復思惟念：",
        "from": "T16n0713_001:0827a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『何等無有六入無有？",
        "from": "T16n0713_001:0827a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等盡則六入盡？",
        "from": "T16n0713_001:0827a06_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』便復思惟案本念，有是應意：",
        "from": "T16n0713_001:0827a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『名像無有，則六入無有；",
        "from": "T16n0713_001:0827a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名像盡，復六入盡。",
        "from": "T16n0713_001:0827a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』「便復思惟念：",
        "from": "T16n0713_001:0827a08_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『何等無有為名像無有？",
        "from": "T16n0713_001:0827a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等盡復名像盡？",
        "from": "T16n0713_001:0827a09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』便復思惟案本念，有是意應：",
        "from": "T16n0713_001:0827a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『無有識，亦無有名像；",
        "from": "T16n0713_001:0827a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識盡，復名像盡。",
        "from": "T16n0713_001:0827a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』「便復思惟念：",
        "from": "T16n0713_001:0827a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『是何等無有則識無有？",
        "from": "T16n0713_001:0827a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等盡復識盡？",
        "from": "T16n0713_001:0827a12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』便復思惟案本念，得是應意：",
        "from": "T16n0713_001:0827a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『無有殃種，識亦無有；",
        "from": "T16n0713_001:0827a13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殃種已盡，則識盡。",
        "from": "T16n0713_001:0827a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』「便復思惟念：",
        "from": "T16n0713_001:0827a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『何等無有為殃種無有？",
        "from": "T16n0713_001:0827a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等盡為殃種盡？",
        "from": "T16n0713_001:0827a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』便復思惟念本，得應意：",
        "from": "T16n0713_001:0827a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『無有癡，亦無有殃種；",
        "from": "T16n0713_001:0827a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "癡已盡，則殃種盡；",
        "from": "T16n0713_001:0827a16_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殃種已盡，識盡；",
        "from": "T16n0713_001:0827a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識盡，名像盡；",
        "from": "T16n0713_001:0827a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名像盡，六入盡；",
        "from": "T16n0713_001:0827a17_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六入盡，更盡；",
        "from": "T16n0713_001:0827a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更盡，痛痒盡；",
        "from": "T16n0713_001:0827a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "痛痒盡，愛盡；",
        "from": "T16n0713_001:0827a18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛盡，成就盡；",
        "from": "T16n0713_001:0827a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就盡，有盡；",
        "from": "T16n0713_001:0827a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有盡，生盡；",
        "from": "T16n0713_001:0827a19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生盡，老死盡，憂、愁、苦、不可意劇便盡。",
        "from": "T16n0713_001:0827a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是最無有量苦陰便盡。",
        "from": "T16n0713_001:0827a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "』」佛告諸比丘：",
        "from": "T16n0713_001:0827a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「彼時念是，以自得故道所。",
        "from": "T16n0713_001:0827a22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛從是往，得便隨；",
        "from": "T16n0713_001:0827a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已隨，便知老死，亦知老死習，亦知老死滅，亦知老死行令度。",
        "from": "T16n0713_001:0827a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生亦爾，有亦爾，受亦爾，愛亦爾，痛痒亦爾，更亦爾，六入亦爾，名像亦爾，識亦爾，殃種亦爾。",
        "from": "T16n0713_001:0827a26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便知癡，亦知癡習本，亦知何癡盡，亦知受行令癡盡。",
        "from": "T16n0713_001:0827a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬，比丘！",
        "from": "T16n0713_001:0827a28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人在空澤間、閑處行，便見故道、故有行者迹，人便隨迹；",
        "from": "T16n0713_001:0827a29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已隨迹，便見故城、好足園、好足饒樹、好足池、好足河、好山塹、亦好處熾饒園樂，便念是：",
        "from": "T16n0713_001:0827b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『若我今往，當白王：",
        "from": "T16n0713_001:0827b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「我為行在空澤間、閑處，便見故道、故人行處，便隨已，隨見故城、好足園、好足饒樹、好足池、好河、好山塹，亦好處熾饒園樂，可王居。",
        "from": "T16n0713_001:0827b05_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」』王便取居。",
        "from": "T16n0713_001:0827b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "却後稍嚴，稍增熾，多人饒。",
        "from": "T16n0713_001:0827b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」佛告諸比丘：",
        "from": "T16n0713_001:0827b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「我亦如是，得故道所。",
        "from": "T16n0713_001:0827b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛本從是行者，我便隨；",
        "from": "T16n0713_001:0827b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已隨，便知老死從所，亦知老死從聚，亦知老死何盡，亦知老死何行得度世；",
        "from": "T16n0713_001:0827b09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知生亦爾，知有亦爾，知受亦爾，知愛亦爾，知痛痒亦爾，知更亦爾，知六入亦爾，知名像亦爾，知識亦爾，知作行亦爾；",
        "from": "T16n0713_001:0827b12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知癡，亦知癡從生，亦知從何癡滅，亦知何行除癡度世。",
        "from": "T16n0713_001:0827b13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」佛便告比丘、比丘尼、優婆塞、優婆夷：",
        "from": "T16n0713_001:0827b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「若比丘為比丘如有應受行，便得道，不失道，能巧道。",
        "from": "T16n0713_001:0827b15_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比丘尼亦爾，優婆塞亦爾，優婆夷亦爾。",
        "from": "T16n0713_001:0827b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若諦受正行，便隨道，得道。",
        "from": "T16n0713_001:0827b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能如應法行，如是無為行者，增多方至。",
        "from": "T16n0713_001:0827b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」天亦人已見佛說如是宜，比丘取著意，佛行者受。",
        "from": "T16n0713_001:0827b19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貝多樹下思惟十二因緣經",
        "from": "T16n0713_001:0827b20_100",
        "context_with_next_sentence": false
    }
]