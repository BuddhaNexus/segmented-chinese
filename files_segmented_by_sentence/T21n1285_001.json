[
    {
        "source_sentence": "佛說寶賢陀羅尼經",
        "from": "T21n1285_001:0353c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試光祿卿",
        "from": "T21n1285_001:0353c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明教大師臣法賢奉　詔譯",
        "from": "T21n1285_001:0353c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T21n1285_001:0353c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在舍衛國祇樹給孤獨園。",
        "from": "T21n1285_001:0353c29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大菩薩眾俱。",
        "from": "T21n1285_001:0354a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時有大夜叉主名曰寶賢。",
        "from": "T21n1285_001:0354a01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來詣佛所。",
        "from": "T21n1285_001:0354a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到已頭面禮足。",
        "from": "T21n1285_001:0354a02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌恭敬而白佛言。",
        "from": "T21n1285_001:0354a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我有大祕密心陀羅尼樂欲宣說。",
        "from": "T21n1285_001:0354a03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊加哀覆護。",
        "from": "T21n1285_001:0354a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊此陀羅尼。",
        "from": "T21n1285_001:0354a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有比丘比丘尼優婆塞優婆夷等。",
        "from": "T21n1285_001:0354a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日三時至心誦念者。",
        "from": "T21n1285_001:0354a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為彼人常作依怙。",
        "from": "T21n1285_001:0354a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切事皆得吉祥。",
        "from": "T21n1285_001:0354a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至飲食衣服臥具金銀珍寶財穀等我常供給。",
        "from": "T21n1285_001:0354a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及作一切勝利悉使成就。",
        "from": "T21n1285_001:0354a08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦令一切人眾普得愛敬。",
        "from": "T21n1285_001:0354a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯除婬怒癡等惡不善法。",
        "from": "T21n1285_001:0354a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘諸所作悉皆隨意。",
        "from": "T21n1285_001:0354a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊知此寶賢大夜叉主誠心信重。",
        "from": "T21n1285_001:0354a11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲安樂一切眾生貧苦惱故。",
        "from": "T21n1285_001:0354a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而告之言。",
        "from": "T21n1285_001:0354a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉寶賢大夜叉主。",
        "from": "T21n1285_001:0354a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能為利益諸眾生故。",
        "from": "T21n1285_001:0354a13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦樂聞隨意宣說。",
        "from": "T21n1285_001:0354a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時寶賢大夜叉主。",
        "from": "T21n1285_001:0354a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說陀羅尼。",
        "from": "T21n1285_001:0354a14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰。",
        "from": "T21n1285_001:0354a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那謨引囉怛那二合怛囉二合夜引野引一那謨引摩引尼跋捺囉二合野二摩賀引藥叉西那鉢多曳三薩曳替曩四呬哩摩引尼跋捺囉二合五呬哩呬哩摩引尼跋捺囉二合六枳哩摩引尼跋捺囉二合七枳哩枳哩摩引尼跋捺囉二合八唧哩摩引尼跋捺囉二合九唧哩唧哩摩引尼跋捺囉二合十祖路摩引尼跋捺囉二合十一祖路祖路摩引尼跋捺囉二合十二祖嚕摩引尼跋捺囉二合十三祖嚕祖嚕摩引尼跋捺囉二合十四覩嚕摩引尼跋捺囉二合十五覩嚕覩嚕摩引尼跋捺囉二合十六酤嚕摩引尼跋捺囉二合十七酤嚕酤嚕摩引尼跋捺囉二合十八蘇嚕摩引尼跋捺囉二合十九蘇嚕蘇嚕摩引尼跋捺囉二合二十薩哩嚩二合阿哩湯二合彌娑引達野莎引賀引二十一怛𭔞切身他引二十二鉢多泥二十三蘇鉢多泥二十四蘇嚕彌二十五蘇末帝二十六蘇囉替二十七速剎摩二合閉呬哩計引二十八呬哩哥引哩二十九布蘭尼二合引悉馱跋捺哩二合三十呬哩呬哩莎引賀引三十一伊呬酤引儞瑟計二合三十二伊呬細引儞瑟計二合引三十三伊呬虞引尼瑟計二合莎引賀引三十四是時寶賢大夜叉主。",
        "from": "T21n1285_001:0354b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是陀羅尼已。",
        "from": "T21n1285_001:0354b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復白佛言。",
        "from": "T21n1285_001:0354b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊此陀羅尼。",
        "from": "T21n1285_001:0354b07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人依法持誦七遍。",
        "from": "T21n1285_001:0354b07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人所作勝利皆得成就。",
        "from": "T21n1285_001:0354b08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復有人於白月十五日潔淨持戒。",
        "from": "T21n1285_001:0354b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日三時焚沈檀香。",
        "from": "T21n1285_001:0354b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此陀羅尼滿八千遍。",
        "from": "T21n1285_001:0354b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人隨願獲得金銀珍寶悉皆如意。",
        "from": "T21n1285_001:0354b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時大夜叉主。",
        "from": "T21n1285_001:0354b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是陀羅尼已禮佛而退。",
        "from": "T21n1285_001:0354b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說是經已。",
        "from": "T21n1285_001:0354b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸菩薩眾聞佛所說。",
        "from": "T21n1285_001:0354b13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆大歡喜信受奉行。",
        "from": "T21n1285_001:0354b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說寶賢陀羅尼經",
        "from": "T21n1285_001:0354b15_100",
        "context_with_next_sentence": false
    }
]