[
    {
        "source_sentence": "佛說妙色陀羅尼經",
        "from": "T21n1386_001:0905c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試光祿卿",
        "from": "T21n1386_001:0905c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明教大師臣法賢奉　詔譯",
        "from": "T21n1386_001:0905c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛告阿難言。",
        "from": "T21n1386_001:0905c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有陀羅尼名曰妙色。",
        "from": "T21n1386_001:0905c21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃是三世諸佛同共宣說能與眾生作大利益。",
        "from": "T21n1386_001:0905c22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人聞是陀羅尼。",
        "from": "T21n1386_001:0905c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生難遭想發勇猛心。",
        "from": "T21n1386_001:0905c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讀誦受持供養恭敬。",
        "from": "T21n1386_001:0905c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人理世獲大福聚。",
        "from": "T21n1386_001:0905c24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晝夜安隱。",
        "from": "T21n1386_001:0905c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復有人以大悲心。",
        "from": "T21n1386_001:0905c25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於寂靜處持種種飲食而為出生。",
        "from": "T21n1386_001:0905c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此陀羅尼七遍。",
        "from": "T21n1386_001:0905c26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持已作如是言。",
        "from": "T21n1386_001:0905c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今出生祭於世間一切惡趣諸鬼。",
        "from": "T21n1386_001:0905c27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願食此生者速離惡趣。",
        "from": "T21n1386_001:0905c28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是言時即三彈指。",
        "from": "T21n1386_001:0905c28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想彼諸鬼得此食者各各飽滿。",
        "from": "T21n1386_001:0905c29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變妙色身發菩提心。",
        "from": "T21n1386_001:0906a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至當來漸成佛果。",
        "from": "T21n1386_001:0906a01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說陀羅尼曰。",
        "from": "T21n1386_001:0906a01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那謨引婆誐嚩帝引一句蘇嚕播引野二怛他引誐多引野三阿囉曷二合帝引三藐訖三二合沒馱引野四怛𭔞切身他引五唵引蘇嚕蘇嚕六鉢囉二合蘇嚕鉢囉二合蘇嚕七三摩二合囉三摩二合囉八婆囉婆囉九三婆囉三婆囉十薩哩嚩二合必隷二合引多必舍引左引曩引十一阿引賀引囕捺那引彌娑嚩二合引賀引十二爾時阿難聞佛說已。",
        "from": "T21n1386_001:0906a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡喜信受作禮而退。",
        "from": "T21n1386_001:0906a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說妙色陀羅尼經",
        "from": "T21n1386_001:0906a10_100",
        "context_with_next_sentence": false
    }
]