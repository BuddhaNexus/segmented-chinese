[
    {
        "source_sentence": "瑜伽論第三十四卷手記",
        "from": "T85n2802_004:0945a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "談述　福慧",
        "from": "T85n2802_004:0945a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言無常行者。謂內外諸行生滅故　言苦行",
        "from": "T85n2802_004:0945a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "者。",
        "from": "T85n2802_004:0945a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂逼迫性故　言空行者。",
        "from": "T85n2802_004:0945a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "違我見故　言無我行者。",
        "from": "T85n2802_004:0945a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂違我見故　言因引者。",
        "from": "T85n2802_004:0945a07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂業煩惱是能生法故　言集行者。",
        "from": "T85n2802_004:0945a08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有多種故　言起引者。",
        "from": "T85n2802_004:0945a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒資產故　言緣行者。",
        "from": "T85n2802_004:0945a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各別助故　言滅行者。",
        "from": "T85n2802_004:0945a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸蘊盡故　言靜行者。",
        "from": "T85n2802_004:0945a10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生老等三相息故　言妙行者。",
        "from": "T85n2802_004:0945a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無眾患故　言離行者。",
        "from": "T85n2802_004:0945a11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脫眾災故　言道行者。",
        "from": "T85n2802_004:0945a12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通行義故　言如行者。",
        "from": "T85n2802_004:0945a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "契正理故　言行行者。",
        "from": "T85n2802_004:0945a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正趣向故　言出行者。",
        "from": "T85n2802_004:0945a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能永超故　言長時安住者。",
        "from": "T85n2802_004:0945a14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂劫末壞已來也　言七日經者。",
        "from": "T85n2802_004:0945a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂小乘經名也。",
        "from": "T85n2802_004:0945a15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼經廣說三界壞時。",
        "from": "T85n2802_004:0945a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七日頓現故也　言不背者。",
        "from": "T85n2802_004:0945a16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂現見不隱義　言不由他緣者。",
        "from": "T85n2802_004:0945a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂現量故不用他人指示而見也　言諸光明者。",
        "from": "T85n2802_004:0945a18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂日光明等也　言一分位等者。",
        "from": "T85n2802_004:0945a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂八位變異也　言雜染者。",
        "from": "T85n2802_004:0945a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂貪等諸煩惱變異故　言謂緣樂受等者。",
        "from": "T85n2802_004:0945a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於三受明變異也　言又有會者等者。",
        "from": "T85n2802_004:0945a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂依煩惱立變異也　言如是眼識等者。",
        "from": "T85n2802_004:0945a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於六識明變異也　言或人所作者。",
        "from": "T85n2802_004:0945a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂怨家人等而損害故　言餘如前說者。",
        "from": "T85n2802_004:0945a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂徵答所以二門如前分位所作門同也　言醴水者。",
        "from": "T85n2802_004:0945a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂甘美水。",
        "from": "T85n2802_004:0945a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其水如酒能益老病也　言殉利者。",
        "from": "T85n2802_004:0945a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂求營等也　言餘如前說者。",
        "from": "T85n2802_004:0945a27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂徵答同前說故也　言彼諸色行等者。",
        "from": "T85n2802_004:0945a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂內外緣生諸行雖不假因緣自然而有。",
        "from": "T85n2802_004:0945a29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅壞無常其性微細現量不見。",
        "from": "T85n2802_004:0945b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要假比量方乃得見。",
        "from": "T85n2802_004:0945b01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故第三比量明也　成立生滅義中　言謂彼諸行要有剎那等者。",
        "from": "T85n2802_004:0945b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂諸外道執微塵勝性等是常住義。",
        "from": "T85n2802_004:0945b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及內法中一類小乘執內外諸行三剎那六剎那等常安住。",
        "from": "T85n2802_004:0945b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂破如是邪辟諸執。",
        "from": "T85n2802_004:0945b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故論文云謂彼諸行等也　言非如是住者。",
        "from": "T85n2802_004:0945b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂內外緣生實。",
        "from": "T85n2802_004:0945b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若從於微塵勝性等言非如也　而生起者。",
        "from": "T85n2802_004:0945b08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體是常故則無前後。",
        "from": "T85n2802_004:0945b08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變異生滅而可得故　言生已不待滅壞因緣等者。",
        "from": "T85n2802_004:0945b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂內外緣生諸行。",
        "from": "T85n2802_004:0945b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生起之時假因託緣方得生起。",
        "from": "T85n2802_004:0945b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅壞之時不假因緣自然滅盡。",
        "from": "T85n2802_004:0945b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如有世人以石擲空即假人力。",
        "from": "T85n2802_004:0945b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "石墮之時不假人功任運墮落。",
        "from": "T85n2802_004:0945b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中道理亦復如是　等難中　言如是所有變異因緣等者。",
        "from": "T85n2802_004:0945b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中有難。",
        "from": "T85n2802_004:0945b14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若內外緣生諸行滅盡之時無有因者。",
        "from": "T85n2802_004:0945b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前所說八種變異因緣豈不具其滅壞因耶。",
        "from": "T85n2802_004:0945b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故論答云如是所有等也。",
        "from": "T85n2802_004:0945b17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中意說。",
        "from": "T85n2802_004:0945b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前所立八種變異是內外緣生變異生起因緣非是滅壞因也。",
        "from": "T85n2802_004:0945b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如錘破瓶是碎壞因非滅微塵因。",
        "from": "T85n2802_004:0945b19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中道理亦復如是　言俱滅壞者。",
        "from": "T85n2802_004:0945b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂內外緣生因之與緣俱滅壞已。",
        "from": "T85n2802_004:0945b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後不相似行方乃生超。",
        "from": "T85n2802_004:0945b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故八種變異因緣是生起因非是滅因。",
        "from": "T85n2802_004:0945b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若是滅因者。",
        "from": "T85n2802_004:0945b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如盛年相狀滅時者之相狀不合得有。",
        "from": "T85n2802_004:0945b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何故得有。",
        "from": "T85n2802_004:0945b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故當知八種變異是生起因非滅因也　言或有諸行等者。",
        "from": "T85n2802_004:0945b25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是立之辭。",
        "from": "T85n2802_004:0945b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難之意者。",
        "from": "T85n2802_004:0945b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖八種變異因緣是內外緣生諸行生起之因非滅壞因者。",
        "from": "T85n2802_004:0945b27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此非遍義。",
        "from": "T85n2802_004:0945b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故。",
        "from": "T85n2802_004:0945b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世有一分有為諸行與因俱滅非是生起。",
        "from": "T85n2802_004:0945b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如火煎水。",
        "from": "T85n2802_004:0945b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "焚燒器世與因俱滅非生起故　言彼亦因緣等者。",
        "from": "T85n2802_004:0945c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂破前成因喻也。",
        "from": "T85n2802_004:0945c01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中意顯。",
        "from": "T85n2802_004:0945c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火之功用德能煎水。",
        "from": "T85n2802_004:0945c02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "焚燒器世無其盡水等功。",
        "from": "T85n2802_004:0945c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故汝言一分有為與因俱滅不應道理。",
        "from": "T85n2802_004:0945c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故論云不由其火等也　言他世者。",
        "from": "T85n2802_004:0945c04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂過未二世也　言如是一切者。",
        "from": "T85n2802_004:0945c05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂上好惡等七種差別也　言先有等者。",
        "from": "T85n2802_004:0945c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂因滅果生同剎那故。",
        "from": "T85n2802_004:0945c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若自在先者即因義不成也　言三增上者。",
        "from": "T85n2802_004:0945c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂初淨信是聖教量。",
        "from": "T85n2802_004:0945c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘二現量比量。",
        "from": "T85n2802_004:0945c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如論應知　言為者。",
        "from": "T85n2802_004:0945c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂與下四文一一相合　言退失者。",
        "from": "T85n2802_004:0945c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦與下四文一一相合。",
        "from": "T85n2802_004:0945c10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂退失主性等也　言惡作加行者。",
        "from": "T85n2802_004:0945c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂不好藏護也　言法性者。",
        "from": "T85n2802_004:0945c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂變異等三行法爾道理必成假故。",
        "from": "T85n2802_004:0945c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如擲石於空必當墮落。",
        "from": "T85n2802_004:0945c13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中道理亦復如是　言不意行等者。",
        "from": "T85n2802_004:0945c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂苦受順入諸苦蘊取入苦行故。",
        "from": "T85n2802_004:0945c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故下文云順苦諸蘊等也　言不安穩行者。",
        "from": "T85n2802_004:0945c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂行苦不安穩故也　言有漏有取等者。",
        "from": "T85n2802_004:0945c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂能依非苦非樂受也　言順非苦樂等者。",
        "from": "T85n2802_004:0945c18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂受所依處也　言唯有其見者。",
        "from": "T85n2802_004:0945c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂唯有斷常等見無我人等也　言自相者。",
        "from": "T85n2802_004:0945c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂地水等各各自相也　言愛能引苦等者。",
        "from": "T85n2802_004:0945c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂業煩惱為因故能引三界苦果故也。",
        "from": "T85n2802_004:0945c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下三文皆以業煩惱立也　言能攝受者。",
        "from": "T85n2802_004:0945c22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂愛能受諸苦種子故　言招引者。",
        "from": "T85n2802_004:0945c23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂因集緣起四法皆能招苦果五趣中也　言取因者。",
        "from": "T85n2802_004:0945c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂三愛也　言招集者。",
        "from": "T85n2802_004:0945c25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂取緣三界有也　言上首者。",
        "from": "T85n2802_004:0945c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂生也　言生為緣者。",
        "from": "T85n2802_004:0945c26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂由生故有老死等也　言謂正了知等者。",
        "from": "T85n2802_004:0945c27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂愛名煩惱隨眠也　言所依者。",
        "from": "T85n2802_004:0945c28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂五趣蘊也。",
        "from": "T85n2802_004:0945c28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此愛隨眠與五趣蘊為所依故。",
        "from": "T85n2802_004:0945c29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "招集當來起緣法故。",
        "from": "T85n2802_004:0946a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煩惱隨眠是能成因。",
        "from": "T85n2802_004:0946a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "集起緣三是所成法　言所生纏者。",
        "from": "T85n2802_004:0946a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂下所說後有愛等三種也。",
        "from": "T85n2802_004:0946a02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此三種纏從前煩惱隨眠之所生故。",
        "from": "T85n2802_004:0946a03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故云彼所生也　言三種者。",
        "from": "T85n2802_004:0946a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前煩惱隨眠及三種愛也　言所知境者。",
        "from": "T85n2802_004:0946a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂苦集滅諦也。",
        "from": "T85n2802_004:0946a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由正見等故能通尋求生死涅槃過患功德故　言四門者。",
        "from": "T85n2802_004:0946a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂道如行出。",
        "from": "T85n2802_004:0946a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆由正見等遠離故能得隨轉　言比度者　言不同分者。",
        "from": "T85n2802_004:0946a08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂上二界也　言比度者。",
        "from": "T85n2802_004:0946a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂欲界苦諦下無常與五行而觀苦。",
        "from": "T85n2802_004:0946a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上二界無變異無常及別離無常。",
        "from": "T85n2802_004:0946a10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合會無常一分通上界。",
        "from": "T85n2802_004:0946a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但有滅壞無常。",
        "from": "T85n2802_004:0946a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及法性無常一分外十六種無常及內十五種無常上界亦無。",
        "from": "T85n2802_004:0946a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三苦行相中上界但有不安穩行苦也。",
        "from": "T85n2802_004:0946a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空無我相同於欲界。",
        "from": "T85n2802_004:0946a14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "集諦中上界無嗔。",
        "from": "T85n2802_004:0946a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘准欲界。",
        "from": "T85n2802_004:0946a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅道二行相亦准欲界而比度之　言謂彼所有等者。",
        "from": "T85n2802_004:0946a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂苦集二諦遍上界故　言一切種者。",
        "from": "T85n2802_004:0946a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上界亦有不動行種　言一切時者。",
        "from": "T85n2802_004:0946a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂三世時也　言如是法者。",
        "from": "T85n2802_004:0946a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即苦集法也　言如是理者。",
        "from": "T85n2802_004:0946a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂苦集因果理也　言如是住者。",
        "from": "T85n2802_004:0946a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂苦集二諦是染性故　言所有道者。",
        "from": "T85n2802_004:0946a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八聖道中上界無正命正業。",
        "from": "T85n2802_004:0946a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘同欲界　言若觀行者等者。",
        "from": "T85n2802_004:0946a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於欲界十六觀行得決定故　言復於諸諦等者。",
        "from": "T85n2802_004:0946a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於上二界諸諦中起勝解也　言所說法者。",
        "from": "T85n2802_004:0946a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂前無常等七種無邊際法也　言更無有餘等者。",
        "from": "T85n2802_004:0946a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂聖道未現前已表更無有法而能息滅生死作方便也　言復於如是等者。",
        "from": "T85n2802_004:0946a27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂勝解作意行者能於三有四生之中深厭怖故。",
        "from": "T85n2802_004:0946a28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即苦集二諦也　言及於等者。",
        "from": "T85n2802_004:0946a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即滅道二諦也　言隨喜一行者。",
        "from": "T85n2802_004:0946b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂滅道二諦各四行中隨於一中心願樂故　言彼於長夜等者。",
        "from": "T85n2802_004:0946b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂從無始時來資長積集五塵種子故。",
        "from": "T85n2802_004:0946b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖樂涅槃心有退轉　言作是思非等者。",
        "from": "T85n2802_004:0946b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是我慢也　言當復流轉者。",
        "from": "T85n2802_004:0946b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不對治當更流轉。",
        "from": "T85n2802_004:0946b05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由我對治故更不流轉。",
        "from": "T85n2802_004:0946b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等十二種法唯我所有。",
        "from": "T85n2802_004:0946b06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘皆無者是名我慢長也　言以慧通達者。",
        "from": "T85n2802_004:0946b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂苦類等智也　言外所知境者。",
        "from": "T85n2802_004:0946b08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂五及十二種我慢也　言隨所生起等者。",
        "from": "T85n2802_004:0946b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若彼行人觀上界四諦時。",
        "from": "T85n2802_004:0946b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前所說障礙諸法隨有生起。",
        "from": "T85n2802_004:0946b10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由對治力尋便謝滅。",
        "from": "T85n2802_004:0946b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對治之心無間生起　言第二法者。",
        "from": "T85n2802_004:0946b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂有二種。",
        "from": "T85n2802_004:0946b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一蘊法。",
        "from": "T85n2802_004:0946b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二我法捨故　言妙慧者。",
        "from": "T85n2802_004:0946b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂能觀十六行相之慧也　言正智得生者。",
        "from": "T85n2802_004:0946b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂類智得生也　言妙是行等者。",
        "from": "T85n2802_004:0946b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂三界四諦下共有八忍。",
        "from": "T85n2802_004:0946b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勝解作意行者證得下品八忍三昧之時。",
        "from": "T85n2802_004:0946b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以三昧力發生人空無分別智。",
        "from": "T85n2802_004:0946b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "焚燒無始煩惱業薪。",
        "from": "T85n2802_004:0946b17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智火耎相初現前故名耎位也。",
        "from": "T85n2802_004:0946b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂忍二位依中上忍隨應說之。",
        "from": "T85n2802_004:0946b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中不立八智者。",
        "from": "T85n2802_004:0946b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八智是無間道是因位故。",
        "from": "T85n2802_004:0946b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中不別。",
        "from": "T85n2802_004:0946b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八忍是解脫道是果位故。",
        "from": "T85n2802_004:0946b20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中明也　言能緣者。",
        "from": "T85n2802_004:0946b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂人空智也　言所緣者。",
        "from": "T85n2802_004:0946b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂四諦十六行相　言觀心所有加行者。",
        "from": "T85n2802_004:0946b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂四諦十六行相有分別影像也　言住無加行者。",
        "from": "T85n2802_004:0946b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂四諦十六行無分別影像也　言非滅者。",
        "from": "T85n2802_004:0946b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "業煩惱種猶未斷故　言而非無緣者。",
        "from": "T85n2802_004:0946b25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂有無分別奢摩他影像現前故　言非美睡眠等者。",
        "from": "T85n2802_004:0946b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂彼行者住無間定時。",
        "from": "T85n2802_004:0946b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非周蜜蜂三昧惛醉。",
        "from": "T85n2802_004:0946b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯有分明智觀雙運道現前故　言從此無間等者。",
        "from": "T85n2802_004:0946b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂世第一法無間生起遠離作意也　言起內作意者。",
        "from": "T85n2802_004:0946c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂遠離作意也　言如其次第者。",
        "from": "T85n2802_004:0946c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂先觀欲界。",
        "from": "T85n2802_004:0946c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次觀上二界十六行相也　言現見等者。",
        "from": "T85n2802_004:0946c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂欲界四諦也　言非現見者。",
        "from": "T85n2802_004:0946c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂上二界也　言無分別者。",
        "from": "T85n2802_004:0946c04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂十六行相無分別影像也　言決定智者。",
        "from": "T85n2802_004:0946c05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂現見智也　言此永斷故等者。",
        "from": "T85n2802_004:0946c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於世道中離欲界九品惑盡者。",
        "from": "T85n2802_004:0946c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今於此中入諦現觀得不還果　言彼以前說等者。",
        "from": "T85n2802_004:0946c08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂如前說於世道中離欲之人有十二種。",
        "from": "T85n2802_004:0946c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前第七門說相狀。",
        "from": "T85n2802_004:0946c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此亦如是。",
        "from": "T85n2802_004:0946c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若爾與世道離欲有何差別。",
        "from": "T85n2802_004:0946c10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故論答云然於此中等也。",
        "from": "T85n2802_004:0946c11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂世道者雖生色界。",
        "from": "T85n2802_004:0946c11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從彼涅槃還生欲界。",
        "from": "T85n2802_004:0946c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若出世道從彼涅槃便取無學。",
        "from": "T85n2802_004:0946c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不復還來生於欲界。",
        "from": "T85n2802_004:0946c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有此差別也　言若先倍離等者。",
        "from": "T85n2802_004:0946c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂欲界九品惑中斷前六品故為位也　言能知智者。",
        "from": "T85n2802_004:0946c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂能緣人空之智與所緣十六行相和合相對故也　言此亦成就等者。",
        "from": "T85n2802_004:0946c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖世道中有十二種相狀。",
        "from": "T85n2802_004:0946c17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此亦有十八種相狀。",
        "from": "T85n2802_004:0946c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "超世道故也　言雖法智者。",
        "from": "T85n2802_004:0946c18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂了人空智　言非斷智者。",
        "from": "T85n2802_004:0946c19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂預流人離斷見道智也　言非常智者。",
        "from": "T85n2802_004:0946c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂預流人離常見也　言得緣生智者。",
        "from": "T85n2802_004:0946c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂了緣生橫如串化無實我也　言唯自法見等者。",
        "from": "T85n2802_004:0946c22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂四諦法隨次配釋　言四證淨者。",
        "from": "T85n2802_004:0946c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即四淨也。",
        "from": "T85n2802_004:0946c23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂三寶及聖所愛惑無礙也　言第八有者。",
        "from": "T85n2802_004:0946c24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂預流人極鈍根者唯有七生並證無學故無第八也　言如所得道者。",
        "from": "T85n2802_004:0946c26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂四諦十六行相也　言然有差別者。",
        "from": "T85n2802_004:0946c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂預流人若起煩惱。",
        "from": "T85n2802_004:0946c27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由上中下三品對治方能除遣。",
        "from": "T85n2802_004:0946c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此一來人若由失念煩惱起時。",
        "from": "T85n2802_004:0946c28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微劣對治便能除遣。",
        "from": "T85n2802_004:0946c29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故差別也　言上品者。",
        "from": "T85n2802_004:0946c29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於上上五欲境界中起微劣貪也。",
        "from": "T85n2802_004:0947a01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若初果已前人於下品境界中便起下品貪等。",
        "from": "T85n2802_004:0947a02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此亦是其差別之相也　言一切道者。",
        "from": "T85n2802_004:0947a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂一來乃至無學向一切皆由攝樂遠離加行究竟三作意故。",
        "from": "T85n2802_004:0947a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於所得道而正修習　言謂由定地等者。",
        "from": "T85n2802_004:0947a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂出世道所起之智是有為故。",
        "from": "T85n2802_004:0947a06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故所修善亦名有為　言增長者。",
        "from": "T85n2802_004:0947a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初心也。",
        "from": "T85n2802_004:0947a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言無間者。",
        "from": "T85n2802_004:0947a07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中心也　言殷重者。",
        "from": "T85n2802_004:0947a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後心也。",
        "from": "T85n2802_004:0947a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此三心令所修習所有善法而會合故　言殊勝善法者。",
        "from": "T85n2802_004:0947a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂三解六神通皆由修習二十賢善定之所得故○言安住如前者。",
        "from": "T85n2802_004:0947a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂安住等九種行如修所瑜伽處已說也　言修亦如前等者。",
        "from": "T85n2802_004:0947a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂有四種修相亦修瑜伽處說也　言盡智者。",
        "from": "T85n2802_004:0947a13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無間道　言無生智者。",
        "from": "T85n2802_004:0947a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂解脫道也　言彼於爾時等者。",
        "from": "T85n2802_004:0947a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂證金剛喻定時也　言證自在義者。",
        "from": "T85n2802_004:0947a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂如諸佛證自義故　言空住等者。",
        "from": "T85n2802_004:0947a16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂空解解門等隨次配之　言慈悲等者。",
        "from": "T85n2802_004:0947a17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於世道修作梵住因故　言關鍵者。",
        "from": "T85n2802_004:0947a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂由業煩惱關鍵故。",
        "from": "T85n2802_004:0947a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸有情不能趣入。",
        "from": "T85n2802_004:0947a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "涅槃云城　言深坑者。",
        "from": "T85n2802_004:0947a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂生死深坑也　言伊師迦去此云籬𥬰。",
        "from": "T85n2802_004:0947a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂三界業煩惱也　言高慢者。",
        "from": "T85n2802_004:0947a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂我慢也　言五支者。",
        "from": "T85n2802_004:0947a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂殺等五也　言六支者。",
        "from": "T85n2802_004:0947a22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂密護根門等也。",
        "from": "T85n2802_004:0947a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前所說　言四所依者。",
        "from": "T85n2802_004:0947a23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂依智不依識。",
        "from": "T85n2802_004:0947a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依義不依語。",
        "from": "T85n2802_004:0947a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依了義經不依不了義經。",
        "from": "T85n2802_004:0947a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依法不依人　言身行倚息者。",
        "from": "T85n2802_004:0947a25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂與輕安相應也　言已親近者。",
        "from": "T85n2802_004:0947a26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂與聖道相應也　言無上丈夫者。",
        "from": "T85n2802_004:0947a27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂小乘中佛是羅漢故名無上也　言其心等者。",
        "from": "T85n2802_004:0947a28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂羅漢心猶如虛空及淨水等。",
        "from": "T85n2802_004:0947a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有香嗅不能傾動故。",
        "from": "T85n2802_004:0947a29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蕃本云。",
        "from": "T85n2802_004:0947b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其心如空。",
        "from": "T85n2802_004:0947b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "假使塗其栴檀及與斧鑿。",
        "from": "T85n2802_004:0947b01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此二中其心平等。",
        "from": "T85n2802_004:0947b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘文可見　言此中都無者。",
        "from": "T85n2802_004:0947b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如生死無有流轉。",
        "from": "T85n2802_004:0947b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於涅槃中亦不貪著　言猶如等者。",
        "from": "T85n2802_004:0947b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂一切法皆依名句文建立。",
        "from": "T85n2802_004:0947b04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼四果皆依種姓等四瑜伽建立。",
        "from": "T85n2802_004:0947b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故喻名句等也　言獨覺地者。",
        "from": "T85n2802_004:0947b06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂種姓發心修行得果四瑜伽處與聲聞同。",
        "from": "T85n2802_004:0947b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何故此中列五相耶。",
        "from": "T85n2802_004:0947b07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "T85n2802_004:0947b07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂謂住及行並是得瑜伽處故與上無別　言不愛憒鬧者。",
        "from": "T85n2802_004:0947b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂修自利行也　言樂寂靜者。",
        "from": "T85n2802_004:0947b09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂不樂利他也　言依說正法及於少思務者。",
        "from": "T85n2802_004:0947b10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與自利及不利他隨次相合　言希願無師者。",
        "from": "T85n2802_004:0947b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由慢行故也　言無敵者。",
        "from": "T85n2802_004:0947b12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由中根故也。",
        "from": "T85n2802_004:0947b12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂獨成菩提時無諸天魔為怨敵故也　言經百劫者。",
        "from": "T85n2802_004:0947b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於百大却中一一皆值諸佛出世修六善巧故　言親近乘事者。",
        "from": "T85n2802_004:0947b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初心也　言成熟相續及專心者。",
        "from": "T85n2802_004:0947b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中後心也　言順決擇等者。",
        "from": "T85n2802_004:0947b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中有問。",
        "from": "T85n2802_004:0947b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四決擇分合有四種。",
        "from": "T85n2802_004:0947b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何故此中但說三耶。",
        "from": "T85n2802_004:0947b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "T85n2802_004:0947b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由世第一與證現觀預流之果極隣近故。",
        "from": "T85n2802_004:0947b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此未證得　言得沙門果者。",
        "from": "T85n2802_004:0947b19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂第二第三果也　言復有一類等者。",
        "from": "T85n2802_004:0947b20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百劫修道同前所說　言復有類等者。",
        "from": "T85n2802_004:0947b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂利根故頓證無學。",
        "from": "T85n2802_004:0947b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此由中下故漸證無學。",
        "from": "T85n2802_004:0947b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故論文言或證法現觀等也　言麟角喻者。",
        "from": "T85n2802_004:0947b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此顯獨一無侶義也　後言部行喻者。",
        "from": "T85n2802_004:0947b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此顯有部儻義。",
        "from": "T85n2802_004:0947b24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂一類修獨覺者猶如群鳥。",
        "from": "T85n2802_004:0947b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而修習故方證果故也。",
        "from": "T85n2802_004:0947b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此位獨覺修之時。",
        "from": "T85n2802_004:0947b26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樂與眾人雜住修故。",
        "from": "T85n2802_004:0947b26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得果之時或十或百乃至千萬一時出現。",
        "from": "T85n2802_004:0947b27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於虛空中現十八變化利有情也　言樂甚深者。",
        "from": "T85n2802_004:0947b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂樂觀順流逆流也　言樂觀察者。",
        "from": "T85n2802_004:0947b29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂觀緣起緣性也　言安住等者。",
        "from": "T85n2802_004:0947c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂樂三解脫門也　言獨勝者。",
        "from": "T85n2802_004:0947c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂即獨覺也　言所餘等者。",
        "from": "T85n2802_004:0947c02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住雖有異。",
        "from": "T85n2802_004:0947c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所觀之相與前無別故　言謂一切等者。",
        "from": "T85n2802_004:0947c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂三種獨覺入村邑時。",
        "from": "T85n2802_004:0947c04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一善護身威儀濟正護惡剎等。",
        "from": "T85n2802_004:0947c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二善護餘根等不隨色等故。",
        "from": "T85n2802_004:0947c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三善護意根謂住正念故　言下劣等者。",
        "from": "T85n2802_004:0947c06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂外道等下劣有情邊若不廣說現神變必不發心。",
        "from": "T85n2802_004:0947c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施衣食等。",
        "from": "T85n2802_004:0947c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼由施故於現世中獲大果報。",
        "from": "T85n2802_004:0947c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瑜伽論第三十四卷手記竟",
        "from": "T85n2802_004:0947c10_100",
        "context_with_next_sentence": false
    }
]