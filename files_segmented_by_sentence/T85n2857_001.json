[
    {
        "source_sentence": "索法號義𮗿諷誦文",
        "from": "T85n2857_001:1306b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏以暫乖寢膳臥疾經時。朝風晝觸於帡幃。",
        "from": "T85n2857_001:1306b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愁雲暮結於庭際。鴛鸞帳下邕邕而憂色潛",
        "from": "T85n2857_001:1306b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生。",
        "from": "T85n2857_001:1306b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翡翠簷間漠漠而清煙亂起。",
        "from": "T85n2857_001:1306b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "病愈意□公謙君子洛之英才。",
        "from": "T85n2857_001:1306b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常懷三義之心每有斷金之美。",
        "from": "T85n2857_001:1306b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頓因璃文動玉質瘵。",
        "from": "T85n2857_001:1306b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身羸如雪六月之花。",
        "from": "T85n2857_001:1306b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貌顇九秋之葉。",
        "from": "T85n2857_001:1306b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂乃仰憑皇覺齋青眼以虔誠信啟金父仁蒙素毫而遠照喜得。",
        "from": "T85n2857_001:1306b16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六根清泰如月照秋江。",
        "from": "T85n2857_001:1306b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三障雲消似紅蓮舒於碧沼。",
        "from": "T85n2857_001:1306b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是如信誠不昧。",
        "from": "T85n2857_001:1306b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "靈鑒昭彰。",
        "from": "T85n2857_001:1306b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈雲布而熱惱涼。",
        "from": "T85n2857_001:1306b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惠影臨而沈痾頹息。",
        "from": "T85n2857_001:1306b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既蒙諸佛衛護念。",
        "from": "T85n2857_001:1306b19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寧無慶賀之心。",
        "from": "T85n2857_001:1306b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "表於此晨用由慶讚女莊嚴。",
        "from": "T85n2857_001:1306b20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願碧山與壽紅樹增春。",
        "from": "T85n2857_001:1306b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必期鏤玉之獲展星河之慶。",
        "from": "T85n2857_001:1306b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願蓮花點於性海。",
        "from": "T85n2857_001:1306b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明月照於心臺。",
        "from": "T85n2857_001:1306b23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常居翠𣓠之年。",
        "from": "T85n2857_001:1306b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永鎖芙蓉之帳　伏願青絲不變紅粉增春。",
        "from": "T85n2857_001:1306b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "德齊嵒下之松。",
        "from": "T85n2857_001:1306b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽等月中之桂　伏願綠眉狀月長分八字之鮮。",
        "from": "T85n2857_001:1306b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玉貌如春獨占春之色　朗君子　伏願文搖五彩之筆。",
        "from": "T85n2857_001:1306b27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高視洪流覺畜九經之書。",
        "from": "T85n2857_001:1306b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "低看鮑謝。",
        "from": "T85n2857_001:1306b28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "折東堂之仙桂香。",
        "from": "T85n2857_001:1306b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發子中馳儒之曲。",
        "from": "T85n2857_001:1306b29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "墳名傳海內祿蕚相映。",
        "from": "T85n2857_001:1306c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玉樹蓮芳同禮移庭。",
        "from": "T85n2857_001:1306c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長光膝下女莊嚴松羅益翠結鳳彩而長卷庭。",
        "from": "T85n2857_001:1306c02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玉蓮芳欝仙桂而恒泰　夫人伏願浩海常流資景福而逾遠。",
        "from": "T85n2857_001:1306c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽山聳峻等群岳而轉高　武莊嚴金璋永󱫧紫綬長榮。",
        "from": "T85n2857_001:1306c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "財盈四海之珍福。",
        "from": "T85n2857_001:1306c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五侯之俸事清吉。",
        "from": "T85n2857_001:1306c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "榮祿日新所謀長。",
        "from": "T85n2857_001:1306c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂於□動靜永康於元吉識亡考。",
        "from": "T85n2857_001:1306c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北意將為北。",
        "from": "T85n2857_001:1306c07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當扇枕慶及溫清。",
        "from": "T85n2857_001:1306c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何期忽翻浪以傾舟俄庭風而滅燭。",
        "from": "T85n2857_001:1306c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孤子想幽冥之氣。",
        "from": "T85n2857_001:1306c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竭思膝下以增悲。",
        "from": "T85n2857_001:1306c09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未申獻橘之誠。",
        "from": "T85n2857_001:1306c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空攀傳衣之意。",
        "from": "T85n2857_001:1306c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思立身之孝道□地無追。",
        "from": "T85n2857_001:1306c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悲風樹之不停。",
        "from": "T85n2857_001:1306c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "痛寒泉之永隔　亡莊嚴。",
        "from": "T85n2857_001:1306c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願瑠璃殿內踏香砌以經行。",
        "from": "T85n2857_001:1306c12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶樹林間擿仙花而奉　又碧池授記紅蓮化生。",
        "from": "T85n2857_001:1306c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法水潤心香風動識　伏願甘露臺側生聞般若之音。",
        "from": "T85n2857_001:1306c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解脫林中慶現龍花之會　伏願五雲捧足逍遙歸解脫之宮。",
        "from": "T85n2857_001:1306c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百福資靈放曠出愛纏之苦。",
        "from": "T85n2857_001:1306c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十歲已下男子芙蓉灼灼。",
        "from": "T85n2857_001:1306c17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可頹芳顏秋月亭亭。",
        "from": "T85n2857_001:1306c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同奇麗質親羅。",
        "from": "T85n2857_001:1306c18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喜門風之望鄉隣嘆巷陌之珍。",
        "from": "T85n2857_001:1306c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□氣欲成而忽消。",
        "from": "T85n2857_001:1306c19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "花正芳而忽墜。",
        "from": "T85n2857_001:1306c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "致使嚴父慈哭愛子以長愁樣蕚蓮枝痛雁行之空闊夕雲操操　女莊嚴。",
        "from": "T85n2857_001:1306c21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性閑皎月體淨秋霜。",
        "from": "T85n2857_001:1306c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幼質也麗南國之風姿。",
        "from": "T85n2857_001:1306c22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮儀也蓋西施美貌。",
        "from": "T85n2857_001:1306c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本冀門榮碎碎𣓠光益宗枝四德傳芳輝榮九族奈河黃天不祐籌俄終。",
        "from": "T85n2857_001:1306c24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮花靉而桂影沈暉。",
        "from": "T85n2857_001:1306c25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "芳樹彫而蘭姿罷郁。",
        "from": "T85n2857_001:1306c25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念恒娥奔月宮之長往嗟。",
        "from": "T85n2857_001:1306c26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遊水流還海以不歸哀傷。",
        "from": "T85n2857_001:1306c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父母之酸痛結姻親之念　文歎德。",
        "from": "T85n2857_001:1306c27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "量宏弘深榮欽禮樂。",
        "from": "T85n2857_001:1306c28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "志超今古動合神明。",
        "from": "T85n2857_001:1306c28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "氣高玉壘之雲。",
        "from": "T85n2857_001:1306c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仁量闊滄渲之水　武德歎。",
        "from": "T85n2857_001:1306c29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風雲稟性星像間生。",
        "from": "T85n2857_001:1307a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信立義存仁深德重。",
        "from": "T85n2857_001:1307a01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "秉文秉武為紫府之良才。",
        "from": "T85n2857_001:1307a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡忠盡貞作無我之心腹。",
        "from": "T85n2857_001:1307a02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不然者曷得軍城見重職掌班聲振人寰名芳邑　莊嚴尚書。",
        "from": "T85n2857_001:1307a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願德光金簡為世上之股肱。",
        "from": "T85n2857_001:1307a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聲振玉堦顯名彰於日下　又願般若智水灌注身田。",
        "from": "T85n2857_001:1307a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旃檀香風資助性海。",
        "from": "T85n2857_001:1307a06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "榮名剋著保祿高遷。",
        "from": "T85n2857_001:1307a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金紫與日月而爭輝。",
        "from": "T85n2857_001:1307a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福壽比山河而永固　女人念誦。",
        "from": "T85n2857_001:1307a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高臺寶鏡雲侵絕代之容。",
        "from": "T85n2857_001:1307a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咲日瓊花霧捹南國之貌。",
        "from": "T85n2857_001:1307a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃知功德之嚴淨託佛力以潛。",
        "from": "T85n2857_001:1307a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "消施上件之名衣表傾誠。",
        "from": "T85n2857_001:1307a10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於投奈苑啟首金人。",
        "from": "T85n2857_001:1307a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "會七辨之緇流。",
        "from": "T85n2857_001:1307a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "演千身之化佛。",
        "from": "T85n2857_001:1307a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是可以修塵累於□劫。",
        "from": "T85n2857_001:1307a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是可以逾熱惱。",
        "from": "T85n2857_001:1307a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於□今晨深願既發。",
        "from": "T85n2857_001:1307a13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於裏心功德必資於貴體。",
        "from": "T85n2857_001:1307a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願諸天甘露承懇疑而隣臨。",
        "from": "T85n2857_001:1307a14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大聖加威注醍醐於頂內。",
        "from": "T85n2857_001:1307a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有多生累止葉憑念以消除現世愆尤賴殊因而殄滅。",
        "from": "T85n2857_001:1307a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大唐元和七年六月　慰言語間時再査",
        "from": "T85n2857_001:1307a17_100",
        "context_with_next_sentence": false
    }
]