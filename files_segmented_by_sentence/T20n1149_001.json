[
    {
        "source_sentence": "五大虛空藏菩薩速疾大神驗祕",
        "from": "T20n1149_001:0607a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密式經一卷",
        "from": "T20n1149_001:0607a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛智所譯",
        "from": "T20n1149_001:0607a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我依瑜伽經所說　　為像末薄福比丘",
        "from": "T20n1149_001:0607a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "久持諸佛祕密語　　若其福智不現前",
        "from": "T20n1149_001:0607a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及以無官無位倫　　貴姓高人福因薄",
        "from": "T20n1149_001:0607b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為化度一切眾生　　此顯明極祕密法",
        "from": "T20n1149_001:0607b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三世諸佛神驗道　　速疾甚深究竟法",
        "from": "T20n1149_001:0607b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋尊遺法比丘等　　隨力而財寶布施",
        "from": "T20n1149_001:0607b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為菩提果名薄貢　　傳受慎念可修行",
        "from": "T20n1149_001:0607b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若布施不至恭敬　　受道修行不感應",
        "from": "T20n1149_001:0607b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲顯此法大驗力　　先當建立祕密盤",
        "from": "T20n1149_001:0607b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白檀若柏桂。",
        "from": "T20n1149_001:0607b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又經百歲靈木。",
        "from": "T20n1149_001:0607b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用天盤造圓。",
        "from": "T20n1149_001:0607b08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方二寸五分又三寸。",
        "from": "T20n1149_001:0607b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地盤四方六寸又七寸。",
        "from": "T20n1149_001:0607b09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又天色皆經青。",
        "from": "T20n1149_001:0607b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地內院黃。",
        "from": "T20n1149_001:0607b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中院青。",
        "from": "T20n1149_001:0607b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外院赤。",
        "from": "T20n1149_001:0607b10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下方四面空青下黃。",
        "from": "T20n1149_001:0607b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天厚一寸三分。",
        "from": "T20n1149_001:0607b11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地厚一寸。",
        "from": "T20n1149_001:0607b11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又八分作之。",
        "from": "T20n1149_001:0607b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天盤東面。",
        "from": "T20n1149_001:0607b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福智虛空藏菩薩像畫。",
        "from": "T20n1149_001:0607b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐白寶蓮華。",
        "from": "T20n1149_001:0607b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戴五佛寶冠。",
        "from": "T20n1149_001:0607b13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以種種瓔珞身嚴飾。",
        "from": "T20n1149_001:0607b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手施無畏。",
        "from": "T20n1149_001:0607b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持寶蓮華。",
        "from": "T20n1149_001:0607b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華上有羯磨杵。",
        "from": "T20n1149_001:0607b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形色黃。",
        "from": "T20n1149_001:0607b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南方有怛洛字。",
        "from": "T20n1149_001:0607b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變成能滿虛空藏菩薩。",
        "from": "T20n1149_001:0607b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐赤蓮華。",
        "from": "T20n1149_001:0607b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂有五佛寶冠。",
        "from": "T20n1149_001:0607b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莊嚴如前。",
        "from": "T20n1149_001:0607b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形色赤肉。",
        "from": "T20n1149_001:0607b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持寶劍。",
        "from": "T20n1149_001:0607b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左執青蓮華。",
        "from": "T20n1149_001:0607b17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華上有如意寶珠。",
        "from": "T20n1149_001:0607b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "珠迴有火焰。",
        "from": "T20n1149_001:0607b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方有紇哩字。",
        "from": "T20n1149_001:0607b18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變成施願虛空藏菩薩。",
        "from": "T20n1149_001:0607b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐紫蓮華。",
        "from": "T20n1149_001:0607b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶冠如前莊嚴如前。",
        "from": "T20n1149_001:0607b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌當胸前。",
        "from": "T20n1149_001:0607b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形色紅波利。",
        "from": "T20n1149_001:0607b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方有噁字。",
        "from": "T20n1149_001:0607b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變成無垢虛空藏菩薩。",
        "from": "T20n1149_001:0607b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐白蓮華。",
        "from": "T20n1149_001:0607b21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶冠如前莊嚴如前。",
        "from": "T20n1149_001:0607b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手施無畏。",
        "from": "T20n1149_001:0607b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持寶蓮華。",
        "from": "T20n1149_001:0607b22_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華。",
        "from": "T20n1149_001:0607b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上有月輪。",
        "from": "T20n1149_001:0607b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形色水白。",
        "from": "T20n1149_001:0607b23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中央有鑁字。",
        "from": "T20n1149_001:0607b23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變成解脫虛空藏菩薩。",
        "from": "T20n1149_001:0607b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶冠如前莊嚴如前。",
        "from": "T20n1149_001:0607b24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐黃白蓮華。",
        "from": "T20n1149_001:0607b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手施無畏。",
        "from": "T20n1149_001:0607b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持寶蓮華。",
        "from": "T20n1149_001:0607b25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華上有獨鈷杵。",
        "from": "T20n1149_001:0607b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色黃。",
        "from": "T20n1149_001:0607b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次天盤頂上畫七星王形像。",
        "from": "T20n1149_001:0607b26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次地盤內院八方畫八天形像。",
        "from": "T20n1149_001:0607b27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四角四天王形像如例。",
        "from": "T20n1149_001:0607b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東方帝釋天像如例。",
        "from": "T20n1149_001:0607b28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南方焰魔天像如例。",
        "from": "T20n1149_001:0607b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方梵天形如例。",
        "from": "T20n1149_001:0607b29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方毘沙門天像如例。",
        "from": "T20n1149_001:0607b29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次中院二十八宿畫。",
        "from": "T20n1149_001:0607c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東方青色青衣男形七人持器仗。",
        "from": "T20n1149_001:0607c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南方赤色赤衣男形七人持器仗。",
        "from": "T20n1149_001:0607c02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方白色白衣男形七人持器仗。",
        "from": "T20n1149_001:0607c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方黑色黑衣男形七人持器仗。",
        "from": "T20n1149_001:0607c04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次外院三十六禽畫。",
        "from": "T20n1149_001:0607c04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始自甲乙。",
        "from": "T20n1149_001:0607c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸夜叉部尋之。",
        "from": "T20n1149_001:0607c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其形像畫。",
        "from": "T20n1149_001:0607c05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若名字畫色形。",
        "from": "T20n1149_001:0607c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨方角耳。",
        "from": "T20n1149_001:0607c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內院地黃。",
        "from": "T20n1149_001:0607c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中院地青。",
        "from": "T20n1149_001:0607c06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外院地赤。",
        "from": "T20n1149_001:0607c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知之綵畫矣。",
        "from": "T20n1149_001:0607c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "2 成就悉地品五大虛空藏菩薩成就悉地品第二根本最勝心陀羅尼曰。",
        "from": "T20n1149_001:0607c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南牟阿迦去引捨舒可反二揭魚羯反婆去引耶全可反三唵四阿唎迦入摩唎六𭋼唎七莎嚩訶八東方菩薩呪曰。",
        "from": "T20n1149_001:0607c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日羅二合吽欲求官位正命者南方菩薩呪曰。",
        "from": "T20n1149_001:0607c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日羅二合曩怛洛二合欲求富貴智惠者西方菩薩呪曰。",
        "from": "T20n1149_001:0607c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日羅二合紇哩入欲求敬愛者北方菩薩呪曰。",
        "from": "T20n1149_001:0607c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日羅二合噁欲求解脫息災者中央菩薩呪曰。",
        "from": "T20n1149_001:0607c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日羅二合鑁欲求遂一切意願者印契如瑜伽經說。",
        "from": "T20n1149_001:0607c22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行者先相應壇場建立。",
        "from": "T20n1149_001:0607c22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意樂處。",
        "from": "T20n1149_001:0607c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但可擇求寂靜處。",
        "from": "T20n1149_001:0607c23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離憒鬧污穢不淨童男童女不信沙彌貧窮垢衣者勿親。",
        "from": "T20n1149_001:0607c24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求成就庭。",
        "from": "T20n1149_001:0607c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "好好謹慎。",
        "from": "T20n1149_001:0607c25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "調備供具。",
        "from": "T20n1149_001:0607c25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉置本尊盤。",
        "from": "T20n1149_001:0607c25_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增上向方隨願誦滿。",
        "from": "T20n1149_001:0607c26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晝夜三時根本明一百萬遍。",
        "from": "T20n1149_001:0607c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即獲得大智惠。",
        "from": "T20n1149_001:0607c27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得人中第一極大安樂。",
        "from": "T20n1149_001:0607c27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲極一切大自在者。",
        "from": "T20n1149_001:0607c28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨方調備供具護摩。",
        "from": "T20n1149_001:0607c28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切所求皆悉成就。",
        "from": "T20n1149_001:0607c29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護摩相應物等。",
        "from": "T20n1149_001:0607c29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如說諸德悉地品但祕術可決師口傳。",
        "from": "T20n1149_001:0608a01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "像末弟子於法有輕熳心。",
        "from": "T20n1149_001:0608a01_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輙不授之。",
        "from": "T20n1149_001:0608a01_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依繁不記之若人欲得大智惠者。",
        "from": "T20n1149_001:0608a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以南方菩薩加毘沙門天。",
        "from": "T20n1149_001:0608a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈之必得之。",
        "from": "T20n1149_001:0608a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得國王大愛念者。",
        "from": "T20n1149_001:0608a03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以西方菩薩加毘沙門天。",
        "from": "T20n1149_001:0608a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈之必得之。",
        "from": "T20n1149_001:0608a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得王后采女乃至諸貴女極愛者。",
        "from": "T20n1149_001:0608a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前菩薩加前天。",
        "from": "T20n1149_001:0608a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈之必得之。",
        "from": "T20n1149_001:0608a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得大臣公卿愛念者。",
        "from": "T20n1149_001:0608a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以前菩薩如前天。",
        "from": "T20n1149_001:0608a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈之必得之。",
        "from": "T20n1149_001:0608a07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得大福長者成急者。",
        "from": "T20n1149_001:0608a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以南方菩薩加前天。",
        "from": "T20n1149_001:0608a08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈必得之。",
        "from": "T20n1149_001:0608a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得大官位歸依尊重者。",
        "from": "T20n1149_001:0608a09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以東方菩薩加帝釋天。",
        "from": "T20n1149_001:0608a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈必得之。",
        "from": "T20n1149_001:0608a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得勝諸人天下自在者。",
        "from": "T20n1149_001:0608a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以北方菩薩加梵天。",
        "from": "T20n1149_001:0608a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈之必得之。",
        "from": "T20n1149_001:0608a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得遂一切所望出世間事者。",
        "from": "T20n1149_001:0608a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以中方菩薩加東方天。",
        "from": "T20n1149_001:0608a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈必得。",
        "from": "T20n1149_001:0608a13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得示他人大靈驗者。",
        "from": "T20n1149_001:0608a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以南方菩薩加西北方天。",
        "from": "T20n1149_001:0608a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈之必令爾。",
        "from": "T20n1149_001:0608a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得大驗利益一切眾生者。",
        "from": "T20n1149_001:0608a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以東方菩薩加西南方天。",
        "from": "T20n1149_001:0608a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈之必令爾。",
        "from": "T20n1149_001:0608a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得官爵職祿者。",
        "from": "T20n1149_001:0608a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以東方菩薩加毘沙門天。",
        "from": "T20n1149_001:0608a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈之得。",
        "from": "T20n1149_001:0608a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得一切人愛念者。",
        "from": "T20n1149_001:0608a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以西方菩薩加前天。",
        "from": "T20n1149_001:0608a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈之必得之。",
        "from": "T20n1149_001:0608a19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得勝論議者。",
        "from": "T20n1149_001:0608a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以南方菩薩加梵天。",
        "from": "T20n1149_001:0608a20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈必令得之。",
        "from": "T20n1149_001:0608a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得惡人降伏者。",
        "from": "T20n1149_001:0608a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以北方菩薩加西南天。",
        "from": "T20n1149_001:0608a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈必令爾。",
        "from": "T20n1149_001:0608a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得止熱病者。",
        "from": "T20n1149_001:0608a22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以中方菩薩加西北方天。",
        "from": "T20n1149_001:0608a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈之必除之。",
        "from": "T20n1149_001:0608a23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得止癊病瘇疳癰病者。",
        "from": "T20n1149_001:0608a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以北方菩薩加西南方天。",
        "from": "T20n1149_001:0608a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈必除之。",
        "from": "T20n1149_001:0608a25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得除官位有障難者。",
        "from": "T20n1149_001:0608a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以東方菩薩加西北天。",
        "from": "T20n1149_001:0608a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈必令得。",
        "from": "T20n1149_001:0608a26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得成一切法師王者。",
        "from": "T20n1149_001:0608a27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以南方菩薩加東南方天。",
        "from": "T20n1149_001:0608a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈必得。",
        "from": "T20n1149_001:0608a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得諸佛菩薩護念者。",
        "from": "T20n1149_001:0608a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以中方菩薩加前天。",
        "from": "T20n1149_001:0608a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈必得。",
        "from": "T20n1149_001:0608a29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得諸富人愛念者。",
        "from": "T20n1149_001:0608b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以南方菩薩加毘沙門天。",
        "from": "T20n1149_001:0608b01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈必得。",
        "from": "T20n1149_001:0608b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得天狐地狐結縛者。",
        "from": "T20n1149_001:0608b02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以東方菩薩加西南天。",
        "from": "T20n1149_001:0608b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈之。",
        "from": "T20n1149_001:0608b03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得除滅一切鬼神所著者。",
        "from": "T20n1149_001:0608b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以北方菩薩加焰魔天。",
        "from": "T20n1149_001:0608b04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈之。",
        "from": "T20n1149_001:0608b04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得諸人決定業除滅者。",
        "from": "T20n1149_001:0608b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以東方菩薩加前天。",
        "from": "T20n1149_001:0608b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈之。",
        "from": "T20n1149_001:0608b06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得諸小兒短命夭壽消除者。",
        "from": "T20n1149_001:0608b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以東方菩薩加前天。",
        "from": "T20n1149_001:0608b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈之。",
        "from": "T20n1149_001:0608b07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得一切女人難產速生者。",
        "from": "T20n1149_001:0608b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以北方菩薩加前天。",
        "from": "T20n1149_001:0608b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈之。",
        "from": "T20n1149_001:0608b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得止一切惡鬼被惡毒顛倒者。",
        "from": "T20n1149_001:0608b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以東方菩薩加前天。",
        "from": "T20n1149_001:0608b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈之。",
        "from": "T20n1149_001:0608b10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得返惡人呪咀者。",
        "from": "T20n1149_001:0608b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以北方菩薩加西南天。",
        "from": "T20n1149_001:0608b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈之必得。",
        "from": "T20n1149_001:0608b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得止一切災火之難者。",
        "from": "T20n1149_001:0608b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以中方菩薩加西北方天。",
        "from": "T20n1149_001:0608b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈之。",
        "from": "T20n1149_001:0608b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得除旱魃之難者。",
        "from": "T20n1149_001:0608b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以中方菩薩加帝釋天。",
        "from": "T20n1149_001:0608b14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈之。",
        "from": "T20n1149_001:0608b14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得除疫病流行之災者。",
        "from": "T20n1149_001:0608b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以北方菩薩加焰魔天。",
        "from": "T20n1149_001:0608b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈之。",
        "from": "T20n1149_001:0608b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得極樂往生上品上生者。",
        "from": "T20n1149_001:0608b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以西方菩薩加焰魔天。",
        "from": "T20n1149_001:0608b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈之。",
        "from": "T20n1149_001:0608b17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得拔濟七世父母六親并一切眾生地獄苦極樂往生者。",
        "from": "T20n1149_001:0608b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以西方菩薩加梵天。",
        "from": "T20n1149_001:0608b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈之。",
        "from": "T20n1149_001:0608b19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得夫婦相愛者。",
        "from": "T20n1149_001:0608b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以西方菩薩加毘沙門天。",
        "from": "T20n1149_001:0608b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書男女姓名和合天地間置封之。",
        "from": "T20n1149_001:0608b21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈之。",
        "from": "T20n1149_001:0608b21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得夫婦令相離惡中者。",
        "from": "T20n1149_001:0608b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以北方菩薩加西南天。",
        "from": "T20n1149_001:0608b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈之。",
        "from": "T20n1149_001:0608b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得夫女相離如本愛念者。",
        "from": "T20n1149_001:0608b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以西方菩薩加毘沙門天。",
        "from": "T20n1149_001:0608b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈之。",
        "from": "T20n1149_001:0608b24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得一切財物為盜紛失返得者。",
        "from": "T20n1149_001:0608b25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以北方菩薩加焰魔天。",
        "from": "T20n1149_001:0608b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈之必令得。",
        "from": "T20n1149_001:0608b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得從者并牛馬逃逝者。",
        "from": "T20n1149_001:0608b27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以南方菩薩加梵天。",
        "from": "T20n1149_001:0608b27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈則得已上祕術條四十歟。",
        "from": "T20n1149_001:0608b28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持蘇密作法子細。",
        "from": "T20n1149_001:0608b29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求聞持儀軌如說。",
        "from": "T20n1149_001:0608b29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尋之知之。",
        "from": "T20n1149_001:0608c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "件法天竺國於菩提所傳也。",
        "from": "T20n1149_001:0608c02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其調度不多類。",
        "from": "T20n1149_001:0608c02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時人皆號驗法焉。",
        "from": "T20n1149_001:0608c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持供養物用馬頭明王印真言。",
        "from": "T20n1149_001:0608c03_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿密栗妬納婆縛吽發吒娑婆訶大輪壇印　二手各作拳。",
        "from": "T20n1149_001:0608c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地風各釣結當胸。",
        "from": "T20n1149_001:0608c05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言三遍轉之　次加持壇　次以印入自口　真言曰。",
        "from": "T20n1149_001:0608c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵縛日羅斫羯羅惹吽鑁斛念珠加持真言唵毘盧遮那摩黎娑婆訶次誦旋轉念珠明曰。",
        "from": "T20n1149_001:0608c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日羅二合獄呬耶二合惹跛三去摩曳引吽吉祥天小呪唵摩訶支利曳婆娑婆訶天王梵字呪唵毘智須多迦悉者娑婆訶四天王通呪唵漸婆羅奢舉多羅耶娑婆訶使者呪唵豆留豆留地利地利度露度露迦他耶娑婆訶大三摩耶印真言曰　二手作金剛拳背相合。",
        "from": "T20n1149_001:0608c22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "申二風頭相拄。",
        "from": "T20n1149_001:0608c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印自心至頂。",
        "from": "T20n1149_001:0608c23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三返舉之。",
        "from": "T20n1149_001:0608c23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即頂散之。",
        "from": "T20n1149_001:0608c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼多沒馱喃達麼馱睹薩嚩二合婆嚩二合句唅廣大不空摩尼普供養印　二手金剛合掌。",
        "from": "T20n1149_001:0608c27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指捻蹙如寶形。",
        "from": "T20n1149_001:0608c28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並竪二大指即成。",
        "from": "T20n1149_001:0608c28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦不空摩尼供養真言曰。",
        "from": "T20n1149_001:0608c29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿上謨引伽去布惹自羅反一摩抳二尼那反跛納摩二合縛日隷二合怛他去蘖多尾路引枳帝三三去滿多鉢羅二合薩羅吽引四次取獨古。",
        "from": "T20n1149_001:0609a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手拳安左腰。",
        "from": "T20n1149_001:0609a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦金剛杵真言三遍。",
        "from": "T20n1149_001:0609a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以杵左右各三遍轉。",
        "from": "T20n1149_001:0609a05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "投上三度。",
        "from": "T20n1149_001:0609a05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持五處。",
        "from": "T20n1149_001:0609a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安右乳上。",
        "from": "T20n1149_001:0609a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次執鈴左手。",
        "from": "T20n1149_001:0609a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言三遍了。",
        "from": "T20n1149_001:0609a06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五度振上。",
        "from": "T20n1149_001:0609a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左耳間二度振上。",
        "from": "T20n1149_001:0609a07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次閒斷二度振上。",
        "from": "T20n1149_001:0609a07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以杵各左右三轉。",
        "from": "T20n1149_001:0609a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持五處。",
        "from": "T20n1149_001:0609a08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以杵鈴共安本所。",
        "from": "T20n1149_001:0609a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "杵真言曰。",
        "from": "T20n1149_001:0609a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日羅播儞吽鈴真言曰。",
        "from": "T20n1149_001:0609a10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日羅健吒覩使也斛次奉獻閼伽印明如常　次虛空藏四方結如常　次金剛牆如常　次灑淨真言如常五大虛空藏儀軌於仁和寺皆明寺。",
        "from": "T20n1149_001:0609a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以拇尾山法鼓臺本書寫畢　于時天明二壬寅年秋八月四日夜。",
        "from": "T20n1149_001:0609a16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燈下閣筆。",
        "from": "T20n1149_001:0609a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覺禪抄云。",
        "from": "T20n1149_001:0609a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "式經一卷真偽可尋。",
        "from": "T20n1149_001:0609a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　智積院東武慈忍享和元年次辛酉八月二日。",
        "from": "T20n1149_001:0609a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以智積慈忍師本令寫。",
        "from": "T20n1149_001:0609a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而挍正加點以印行。",
        "from": "T20n1149_001:0609a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　豐山長谷寺總持院快道林常誌享和三年癸亥三月小盡日甲子　一校加筆訖　慈順",
        "from": "T20n1149_001:0609a24_100",
        "context_with_next_sentence": false
    }
]