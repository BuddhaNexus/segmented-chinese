[
    {
        "source_sentence": "佛說摩利支天陀羅尼呪經",
        "from": "T21n1256_001:0261b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "失譯人名今附梁錄",
        "from": "T21n1256_001:0261b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。一時婆伽婆。在舍衛國祇樹給孤",
        "from": "T21n1256_001:0261b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獨園。",
        "from": "T21n1256_001:0261c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大比丘眾千二百五十人俱。",
        "from": "T21n1256_001:0261c01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告諸比丘。",
        "from": "T21n1256_001:0261c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有天名摩利支天。",
        "from": "T21n1256_001:0261c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常行日月前。",
        "from": "T21n1256_001:0261c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼摩利支天。",
        "from": "T21n1256_001:0261c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能見無人能捉。",
        "from": "T21n1256_001:0261c03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為人欺誑不為人縛。",
        "from": "T21n1256_001:0261c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為人債其財物。",
        "from": "T21n1256_001:0261c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為怨家能得其便。",
        "from": "T21n1256_001:0261c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告諸比丘。",
        "from": "T21n1256_001:0261c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人知彼摩利支天名者。",
        "from": "T21n1256_001:0261c06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼人亦不可見亦不可捉。",
        "from": "T21n1256_001:0261c07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為人欺誑不為人債其財物。",
        "from": "T21n1256_001:0261c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為怨家能得其便。",
        "from": "T21n1256_001:0261c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告諸比丘。",
        "from": "T21n1256_001:0261c08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人。",
        "from": "T21n1256_001:0261c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知彼摩利支天名者。",
        "from": "T21n1256_001:0261c09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應作是言。",
        "from": "T21n1256_001:0261c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我弟子某乙。",
        "from": "T21n1256_001:0261c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知彼摩利支天名故。",
        "from": "T21n1256_001:0261c10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能見我無人能捉我。",
        "from": "T21n1256_001:0261c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為人能欺誑我不為人能縛我。",
        "from": "T21n1256_001:0261c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為人能債我財物。",
        "from": "T21n1256_001:0261c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為怨家能得我便。",
        "from": "T21n1256_001:0261c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告諸比丘。",
        "from": "T21n1256_001:0261c13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是摩利支天有陀羅尼呪。",
        "from": "T21n1256_001:0261c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能守護人即說呪曰。",
        "from": "T21n1256_001:0261c14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多姪他遏迦摩私末迦摩私支婆羅摩私支婆羅摩私摩訶支婆羅摩私安陀利陀那摩私於行路中護我。",
        "from": "T21n1256_001:0261c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非行路中護我。",
        "from": "T21n1256_001:0261c18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晝日中護我夜中護我。",
        "from": "T21n1256_001:0261c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於怨家中護我於王難中護我。",
        "from": "T21n1256_001:0261c19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於賊難中護我。",
        "from": "T21n1256_001:0261c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於水難中護我於火難中護我。",
        "from": "T21n1256_001:0261c20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於疫病中護我於阿鳩隷阿鳩隷無利支帝。",
        "from": "T21n1256_001:0261c21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉利吉利安帝安帝於一切處一切時。",
        "from": "T21n1256_001:0261c22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護我弟子某乙。",
        "from": "T21n1256_001:0261c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娑婆訶。",
        "from": "T21n1256_001:0261c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告諸比丘。",
        "from": "T21n1256_001:0261c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人。",
        "from": "T21n1256_001:0261c24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比丘比丘尼優婆塞優婆夷。",
        "from": "T21n1256_001:0261c25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國王大臣及諸人民。",
        "from": "T21n1256_001:0261c25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞是摩利支天陀羅尼呪。",
        "from": "T21n1256_001:0261c26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心受持者。",
        "from": "T21n1256_001:0261c26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為如上諸惡所害。",
        "from": "T21n1256_001:0261c27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告諸比丘。",
        "from": "T21n1256_001:0261c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人。",
        "from": "T21n1256_001:0261c28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書寫是經受持讀誦者。",
        "from": "T21n1256_001:0261c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心齋戒淨治一室。",
        "from": "T21n1256_001:0261c29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以香泥塗地。",
        "from": "T21n1256_001:0262a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七日七夜誦持是摩利支天陀羅尼呪。",
        "from": "T21n1256_001:0262a01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿一百八遍。",
        "from": "T21n1256_001:0262a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所經諸陣一切怨賊。",
        "from": "T21n1256_001:0262a02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並皆息刃。",
        "from": "T21n1256_001:0262a02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行時書寫是陀羅尼。",
        "from": "T21n1256_001:0262a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有著髻中若著衣中隨身行。",
        "from": "T21n1256_001:0262a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸惡不能加害。",
        "from": "T21n1256_001:0262a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆退散無敢當者。",
        "from": "T21n1256_001:0262a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遇疾病。",
        "from": "T21n1256_001:0262a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當請一淨戒比丘及比丘尼優婆塞優婆夷。",
        "from": "T21n1256_001:0262a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前淨治一室香泥塗地。",
        "from": "T21n1256_001:0262a06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒種種名香設七盤果餅。",
        "from": "T21n1256_001:0262a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "布五色壓設五色飯。",
        "from": "T21n1256_001:0262a07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請摩利支天。",
        "from": "T21n1256_001:0262a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然燈續明七日七夜。",
        "from": "T21n1256_001:0262a08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦是摩利支天陀羅尼呪經。",
        "from": "T21n1256_001:0262a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿二百遍。",
        "from": "T21n1256_001:0262a09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切病鬼皆生慈心。",
        "from": "T21n1256_001:0262a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放於病人即得除差。",
        "from": "T21n1256_001:0262a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遭縣官所拘錄者。",
        "from": "T21n1256_001:0262a10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦如前淨室如法設供敷座。",
        "from": "T21n1256_001:0262a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然燈續明七日七夜。",
        "from": "T21n1256_001:0262a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦是摩利支天陀羅尼呪經五百遍。",
        "from": "T21n1256_001:0262a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得如願已設齋散座。",
        "from": "T21n1256_001:0262a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切厄難無不滅除。",
        "from": "T21n1256_001:0262a13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時諸比丘聞佛所說。",
        "from": "T21n1256_001:0262a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆大歡喜信受奉持。",
        "from": "T21n1256_001:0262a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說摩利支天陀羅尼呪經",
        "from": "T21n1256_001:0262a15_100",
        "context_with_next_sentence": false
    }
]