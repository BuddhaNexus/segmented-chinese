[
    {
        "source_sentence": "那羅延天共阿修羅王鬪戰法",
        "from": "T21n1281_001:0342b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶思惟三藏譯",
        "from": "T21n1281_001:0342b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時那羅延天在於須彌山頂上。集諸天大",
        "from": "T21n1281_001:0342b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾梵天王及大自在天王天帝釋等。",
        "from": "T21n1281_001:0342b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有無量天龍夜叉乾闥婆緊那羅摩呼羅伽人非人等。",
        "from": "T21n1281_001:0342b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有無數鬼神等。",
        "from": "T21n1281_001:0342b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共會說法。",
        "from": "T21n1281_001:0342b21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其甘露妙藥及和合安置瓶中。",
        "from": "T21n1281_001:0342b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時阿修羅王變形入其瓶中。",
        "from": "T21n1281_001:0342b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食其甘露妙藥。",
        "from": "T21n1281_001:0342b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是那羅延天見阿修羅王入其瓶中。",
        "from": "T21n1281_001:0342b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以其金輪就於瓶中側之。",
        "from": "T21n1281_001:0342b24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其阿修羅王半身遂斷。",
        "from": "T21n1281_001:0342b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿修羅作病者。",
        "from": "T21n1281_001:0342b25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取白芥子安悉香胡麻蘇蜜相和。",
        "from": "T21n1281_001:0342b26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取佉陀羅木長□指。",
        "from": "T21n1281_001:0342b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一百八段。",
        "from": "T21n1281_001:0342b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一呪一燒。",
        "from": "T21n1281_001:0342b27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱彼□人名字。",
        "from": "T21n1281_001:0342b27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得除愈。",
        "from": "T21n1281_001:0342b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說印法。",
        "from": "T21n1281_001:0342b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護身印。",
        "from": "T21n1281_001:0342c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二小指相叉。",
        "from": "T21n1281_001:0342c01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於掌中二無名指押小指上。",
        "from": "T21n1281_001:0342c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又二中指直竪。",
        "from": "T21n1281_001:0342c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指捻二中指上。",
        "from": "T21n1281_001:0342c02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指捻中指中節。",
        "from": "T21n1281_001:0342c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右轉三匝即名護身。",
        "from": "T21n1281_001:0342c03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後作一切法事。",
        "from": "T21n1281_001:0342c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有障礙。",
        "from": "T21n1281_001:0342c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀印。",
        "from": "T21n1281_001:0342c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手小指名指□上節。",
        "from": "T21n1281_001:0342c05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大母指押小指無名指節上。",
        "from": "T21n1281_001:0342c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中指頭指直竪向上。",
        "from": "T21n1281_001:0342c06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輪印。",
        "from": "T21n1281_001:0342c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定手風空相捻圓如輪狀。",
        "from": "T21n1281_001:0342c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上擲三度向所障處打□□一切阿修羅皆悉斷。",
        "from": "T21n1281_001:0342c08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次火印。",
        "from": "T21n1281_001:0342c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定手作金剛拳。",
        "from": "T21n1281_001:0342c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手直舒五指微動。",
        "from": "T21n1281_001:0342c09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左頭指柱右手心能燒一切設覩嚕心爛以印大焰向彼指之。",
        "from": "T21n1281_001:0342c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想火燒彼如是并護摩必使母陀也。",
        "from": "T21n1281_001:0342c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　咸通五年七月五日比丘禪□□",
        "from": "T21n1281_001:0342c14_0",
        "context_with_next_sentence": true
    }
]