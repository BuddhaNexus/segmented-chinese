[
    {
        "source_sentence": "大雲輪請雨經卷下",
        "from": "T19n0991_002:0497a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隋天竺三藏那連提耶舍譯",
        "from": "T19n0991_002:0497a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊說是諸佛如來名已。告於無邊",
        "from": "T19n0991_002:0497a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莊嚴海雲威德輪蓋龍王作如是言。",
        "from": "T19n0991_002:0497a09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝大龍王。",
        "from": "T19n0991_002:0497a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此諸佛名汝等一切諸龍眷屬。",
        "from": "T19n0991_002:0497a10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能誦持稱彼佛名及禮拜者。",
        "from": "T19n0991_002:0497a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸龍所有苦厄。",
        "from": "T19n0991_002:0497a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆悉解脫普獲安樂。",
        "from": "T19n0991_002:0497a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得安樂已即能於此閻浮提中。",
        "from": "T19n0991_002:0497a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風雨隨時令諸藥草樹木叢林。",
        "from": "T19n0991_002:0497a13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆生長五穀成熟。",
        "from": "T19n0991_002:0497a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時娑婆三千大千世界之主。",
        "from": "T19n0991_002:0497a15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無邊莊嚴海雲威德輪蓋龍王。",
        "from": "T19n0991_002:0497a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復白佛言。",
        "from": "T19n0991_002:0497a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我今啟請諸佛所說陀羅尼句。",
        "from": "T19n0991_002:0497a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令於未來末世之時。",
        "from": "T19n0991_002:0497a17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "閻浮提內若有亢旱不降雨處。",
        "from": "T19n0991_002:0497a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此神呪即當降雨。",
        "from": "T19n0991_002:0497a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飢饉惡世多饒疾疫。",
        "from": "T19n0991_002:0497a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非法亂行人民恐怖。",
        "from": "T19n0991_002:0497a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妖星變怪災厲相續。",
        "from": "T19n0991_002:0497a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有如是等無量苦惱。",
        "from": "T19n0991_002:0497a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以佛力故悉得滅除。",
        "from": "T19n0991_002:0497a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊大慈悲愍諸眾生故。",
        "from": "T19n0991_002:0497a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住持為說所有神呪陀羅尼句。",
        "from": "T19n0991_002:0497a22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告諸龍知能使諸天歡喜踊躍。",
        "from": "T19n0991_002:0497a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復能破散一切諸魔。",
        "from": "T19n0991_002:0497a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生身中所有苦難之事。",
        "from": "T19n0991_002:0497a24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并及惡星變怪災障。",
        "from": "T19n0991_002:0497a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆除滅。",
        "from": "T19n0991_002:0497a25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復如來曾說五種雨障之災。",
        "from": "T19n0991_002:0497a26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦皆消滅彼障除已。",
        "from": "T19n0991_002:0497a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即能使此閻浮提內雨澤以時。",
        "from": "T19n0991_002:0497a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願如來為我等說。",
        "from": "T19n0991_002:0497a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊。",
        "from": "T19n0991_002:0497a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞此無邊莊嚴海雲威德輪蓋龍王如是語已。",
        "from": "T19n0991_002:0497b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即讚歎言善哉善哉汝大龍王。",
        "from": "T19n0991_002:0497b01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今亦如諸佛饒益一切眾生憐愍安樂。",
        "from": "T19n0991_002:0497b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能請如來說此神呪。",
        "from": "T19n0991_002:0497b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝大龍王諦聽諦聽善思念之。",
        "from": "T19n0991_002:0497b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當為汝說於往昔從彼大悲雲生如來所聞。",
        "from": "T19n0991_002:0497b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "震吼奮迅勇猛幢陀羅尼。",
        "from": "T19n0991_002:0497b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過去諸佛已曾宣說威神加護。",
        "from": "T19n0991_002:0497b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今亦當隨順而說。",
        "from": "T19n0991_002:0497b06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利益一切諸眾生故憐愍與樂。",
        "from": "T19n0991_002:0497b07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於未來世若炎旱時能令降雨。",
        "from": "T19n0991_002:0497b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若汎雨時亦能令止。",
        "from": "T19n0991_002:0497b08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飢饉疾疫悉能除滅。",
        "from": "T19n0991_002:0497b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普告諸龍令使知聞。",
        "from": "T19n0991_002:0497b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復令諸天歡喜踊躍。",
        "from": "T19n0991_002:0497b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散一切魔安隱眾生。",
        "from": "T19n0991_002:0497b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T19n0991_002:0497b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛緻他摩訶若那引婆婆引薩尼一尼梨低殊引洛敧彌去聲二提利茶引毘迦囉摩跋闍羅引僧伽怛膩三波羅摩毘囉闍四埿摩求那鷄經岐反兜引修梨耶引波羅鞞五毘摩嵐引伽耶師六婆囉引婆囉去聲七三婆囉引三婆囉去聲八豆潭徒感反鞞去聲九呵那呵那十摩訶波羅薜蒲詣反十一毘頭多摸訶陀迦隷十二波囉若伽囉輸悌十三波梨富婁那引迷帝隷迷怛利引帝囉引摩那娑揵提去聲十四彌多羅浮馱利十五社羅社羅十六社羅社羅十七社羅浮馱利十八蒲登伽俱蘇迷去聲十九達捨婆利二十遮鬪薨賒引阿囉提二十一頞瑟吒達舍毘尼迦佛陀引達迷去聲二十二輸頗摩帝二十三分若羅翅二十四叔迦羅引達摩引三摩泥比二十五鉗毘梨二十六毘羅闍悉鷄經岐反二十七毘富茶毘舍沙波羅鉢帝二十八尼囉蘇羅引婆引達彌二十九薩婆盧迦引匙引瑟吒三十失梨沙吒引三十一波羅波羅婆引兮唎三十二阿奴引怛唎三十三阿僧祇三十四陀囉陀囉三十五地唎地唎三十六豆漏豆漏三十七賒塞多引摩帝三十八賒塞多引波蔽三十九遮羅遮羅四十旨唎旨唎四十一呪漏呪漏四十二波羅遮引佛陀喃引南去聲摩帝四十三摩訶般利若引波引羅引蜜帝莎引呵四十四南無智海毘盧遮那藏如來。",
        "from": "T19n0991_002:0497c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無一切諸菩提薩埵。",
        "from": "T19n0991_002:0497c07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時一切諸龍王等。",
        "from": "T19n0991_002:0497c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為降雨故受持此呪。",
        "from": "T19n0991_002:0497c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若後末世惡災行時能令不起。",
        "from": "T19n0991_002:0497c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復一切諸佛菩薩真實力故。",
        "from": "T19n0991_002:0497c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂復勅諸一切龍等。",
        "from": "T19n0991_002:0497c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於閻浮提所祈請處。",
        "from": "T19n0991_002:0497c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降澍大雨除滅五種雨之障礙。",
        "from": "T19n0991_002:0497c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而說呪曰。",
        "from": "T19n0991_002:0497c12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多緻他一娑邏娑邏二四唎四唎三素漏素漏四那引伽喃去聲五闍婆闍婆一句並去聲六侍毘侍毘並去聲七樹附樹附八佛實力故。",
        "from": "T19n0991_002:0497c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大龍王等速來在於閻浮提內。",
        "from": "T19n0991_002:0497c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所祈請處降澍大雨而說呪曰。",
        "from": "T19n0991_002:0497c17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遮羅遮羅並去聲一至利至利二朱漏朱漏三佛實力故。",
        "from": "T19n0991_002:0497c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咄諸龍王於閻浮提。",
        "from": "T19n0991_002:0497c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請雨國內降澍大雨。",
        "from": "T19n0991_002:0497c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而說呪曰。",
        "from": "T19n0991_002:0497c20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婆邏婆邏並去聲一避利避利避字並白利反二復漏復漏三諸佛菩薩威神之力。",
        "from": "T19n0991_002:0497c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大乘真實行業力故。",
        "from": "T19n0991_002:0497c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸龍王等速來至此。",
        "from": "T19n0991_002:0497c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各各憶念諸如來法及菩薩行。",
        "from": "T19n0991_002:0497c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起於慈心悲心喜心及以捨心。",
        "from": "T19n0991_002:0497c25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而說呪曰。",
        "from": "T19n0991_002:0497c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婆邏婆邏一毘梨毘梨二蒲盧蒲盧三大意氣龍王慈心正念妙密佛法。",
        "from": "T19n0991_002:0497c28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持大雲雨速來至此。",
        "from": "T19n0991_002:0498a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而說呪曰。",
        "from": "T19n0991_002:0498a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伽茶伽茶一祁墀祁墀二瞿厨瞿厨三一切諸佛真實力故。",
        "from": "T19n0991_002:0498a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大健瞋者大疾行者睒電舌者。",
        "from": "T19n0991_002:0498a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "治諸惡毒來起慈心。",
        "from": "T19n0991_002:0498a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於閻浮提請雨國內。",
        "from": "T19n0991_002:0498a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降澍大雨莎引呵。",
        "from": "T19n0991_002:0498a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又說呪曰。",
        "from": "T19n0991_002:0498a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛吒怛吒吒並去聲一底致底致二鬪晝鬪晝三金剛密迹真實力故。",
        "from": "T19n0991_002:0498a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭上戴大摩尼天冠。",
        "from": "T19n0991_002:0498a07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蛇身相者念三寶力。",
        "from": "T19n0991_002:0498a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於閻浮提此請雨國內降澍大雨莎引呵。",
        "from": "T19n0991_002:0498a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又說呪曰。",
        "from": "T19n0991_002:0498a09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦羅迦羅一繼利繼利二句漏句漏三佛實力故。",
        "from": "T19n0991_002:0498a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛密跡勅彼一切澍大水者乘大雲者。",
        "from": "T19n0991_002:0498a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起慈悲心悉來於此閻浮提中請雨國內。",
        "from": "T19n0991_002:0498a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降澍大雨又說呪曰。",
        "from": "T19n0991_002:0498a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何邏邏何邏羅一兮利履兮利履二候漏塿婁苟反候漏塿三三世諸佛真實力故。",
        "from": "T19n0991_002:0498a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令一切諸龍眷屬捨於睡眠。",
        "from": "T19n0991_002:0498a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又說呪曰。",
        "from": "T19n0991_002:0498a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伽磨伽磨一姞寐姞寐二求牟求牟三莎呵我勅一切諸龍王等。",
        "from": "T19n0991_002:0498a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起大慈心為菩提本而說呪曰。",
        "from": "T19n0991_002:0498a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那囉那囉一尼梨尼梨二奴漏奴漏三莎呵咄咄龍等種種異形。",
        "from": "T19n0991_002:0498a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千頭可畏赤眼大力大蛇身者。",
        "from": "T19n0991_002:0498a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今勅汝。",
        "from": "T19n0991_002:0498a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當憶念最上慈悲威神功德滅煩惱者。",
        "from": "T19n0991_002:0498a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸佛如來名字。",
        "from": "T19n0991_002:0498a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而說呪曰。",
        "from": "T19n0991_002:0498a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "揭其謁反茶去聲揭茶一耆穉耆穉二崛住崛住三莎呵無礙勇健奪於世間人色力者。",
        "from": "T19n0991_002:0498a28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於閻浮提請雨國內。",
        "from": "T19n0991_002:0498b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降澍大雨而說呪曰。",
        "from": "T19n0991_002:0498b01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍囉舍囉一尸利尸利二輸入聲嚧輸嚧莎呵一切諸天真實力故。",
        "from": "T19n0991_002:0498b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咄諸大龍念自種姓。",
        "from": "T19n0991_002:0498b03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速來於此閻浮提中請雨國內。",
        "from": "T19n0991_002:0498b04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降澍大雨莎呵。",
        "from": "T19n0991_002:0498b04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大梵天王實行力故。",
        "from": "T19n0991_002:0498b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸龍王於閻浮提請雨國內。",
        "from": "T19n0991_002:0498b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降澍大雨莎呵。",
        "from": "T19n0991_002:0498b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天主帝釋實行力故。",
        "from": "T19n0991_002:0498b07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸龍王於閻浮提請雨國內。",
        "from": "T19n0991_002:0498b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降澍大雨莎呵。",
        "from": "T19n0991_002:0498b08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四大天王實行力故。",
        "from": "T19n0991_002:0498b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸龍王於閻浮提請雨國內。",
        "from": "T19n0991_002:0498b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降澍大雨莎呵。",
        "from": "T19n0991_002:0498b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八人實行力故。",
        "from": "T19n0991_002:0498b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸龍王於閻浮提請雨國內。",
        "from": "T19n0991_002:0498b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降澍大雨莎呵。",
        "from": "T19n0991_002:0498b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須陀洹實行力故。",
        "from": "T19n0991_002:0498b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸龍王於閻浮提請雨國內。",
        "from": "T19n0991_002:0498b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降澍大雨莎呵。",
        "from": "T19n0991_002:0498b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斯陀含實行力故。",
        "from": "T19n0991_002:0498b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸龍王於閻浮提請雨國內。",
        "from": "T19n0991_002:0498b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降澍大雨莎呵。",
        "from": "T19n0991_002:0498b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿那含實行力故。",
        "from": "T19n0991_002:0498b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸龍王於閻浮提請雨國內降澍大雨莎呵。",
        "from": "T19n0991_002:0498b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿羅漢實行力故。",
        "from": "T19n0991_002:0498b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸龍王於閻浮提請雨國內降澍大雨莎呵。",
        "from": "T19n0991_002:0498b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辟支佛實行力故。",
        "from": "T19n0991_002:0498b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸龍王於閻浮提請雨國內降澍大雨莎呵。",
        "from": "T19n0991_002:0498b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩實行力故。",
        "from": "T19n0991_002:0498b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸龍王於閻浮提請雨國內降澍大雨莎呵。",
        "from": "T19n0991_002:0498b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛實行力故。",
        "from": "T19n0991_002:0498b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸龍王於閻浮提請雨國內降澍大雨莎呵。",
        "from": "T19n0991_002:0498b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸天實行力故令速除滅災障苦惱莎呵。",
        "from": "T19n0991_002:0498b27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸龍實行力故能速降雨潤此大地莎呵。",
        "from": "T19n0991_002:0498b28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切夜叉實行力故能速覆護一切眾生莎呵。",
        "from": "T19n0991_002:0498c01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切揵闥婆實行力故能速除滅一切眾生所有憂惱莎呵。",
        "from": "T19n0991_002:0498c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切阿修羅實行力故能速迴轉惡星變怪莎呵。",
        "from": "T19n0991_002:0498c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切迦樓羅實行力故於諸龍邊起大慈悲降澍大雨莎呵。",
        "from": "T19n0991_002:0498c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切緊那羅實行力故速能滅除一切眾生諸重罪業令起踊躍莎呵。",
        "from": "T19n0991_002:0498c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切摩睺羅伽實行力故能降大雨普使充足滅除五種雨之障礙莎呵。",
        "from": "T19n0991_002:0498c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切善男子善女人實行力故善能覆護一切眾生莎呵。",
        "from": "T19n0991_002:0498c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又說呪曰。",
        "from": "T19n0991_002:0498c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦邏迦邏一抧利抧利二句嚧句嚧去聲三陀囉陀囉一地利地利二豆漏豆嚧三那吒那吒一膩年一反𰈂膩𰈂膩二奴晝奴晝三持大雲雨疾行之者如雲者。",
        "from": "T19n0991_002:0498c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著雲衣者生雲中者。",
        "from": "T19n0991_002:0498c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能作雲者雲雷響者。",
        "from": "T19n0991_002:0498c19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住雲中者雲天冠者。",
        "from": "T19n0991_002:0498c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雲莊嚴者乘大雲者。",
        "from": "T19n0991_002:0498c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雲中隱者雲中藏者。",
        "from": "T19n0991_002:0498c20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被雲髮者耀雲光者。",
        "from": "T19n0991_002:0498c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雲圍遶者處大雲者。",
        "from": "T19n0991_002:0498c21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雲瓔珞者能奪五穀精氣之者。",
        "from": "T19n0991_002:0498c22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住在深山叢林中者。",
        "from": "T19n0991_002:0498c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊者龍母名分陀羅大雲威德喜樂尊大龍王。",
        "from": "T19n0991_002:0498c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身體清涼持大風輪。",
        "from": "T19n0991_002:0498c24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛實行力故。",
        "from": "T19n0991_002:0498c24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放六味雨而說呪曰。",
        "from": "T19n0991_002:0498c25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伽邏伽邏一岐利岐利二求漏求漏三其利尼其利尼四求磨求磨求磨求磨求磨求磨求磨求磨求磨凡九求磨五九頭龍母。",
        "from": "T19n0991_002:0499a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勅告首冠大雲閃電華冠之者。",
        "from": "T19n0991_002:0499a02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持一切龍者服雲衣者。",
        "from": "T19n0991_002:0499a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝諸境界毒氣者乘雲嚴者。",
        "from": "T19n0991_002:0499a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雷聲遠震能告諸龍者大雲圍遶者。",
        "from": "T19n0991_002:0499a04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛實行力故。",
        "from": "T19n0991_002:0499a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令閻浮提請雨國內。",
        "from": "T19n0991_002:0499a05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降澍大雨使令充足莎呵。",
        "from": "T19n0991_002:0499a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又說呪曰。",
        "from": "T19n0991_002:0499a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "野邏野邏一逸利逸利二喻屢喻屢三樹屢樹屢四嗜利嗜利五社邏社邏社社邏六求茶求茶求求茶七伽茶伽茶八耆遲耆遲九呵邏呵邏十醯利醯利十一牟漏牟漏十二多邏多邏十三帝利帝利十四兜漏兜漏十五阿那阿那十六陀呵陀呵十七鉢遮鉢遮十八祁利祁利十九醯那醯那二十求利陀二十一末利陀二十二鉢囉末利二十三彌勒菩薩告勅令除一切雨障莎呵又說呪曰。",
        "from": "T19n0991_002:0499a16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛提佛提一浮佛提浮佛提二令諸眾生持佛功德。",
        "from": "T19n0991_002:0499a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除滅一切障業重罪。",
        "from": "T19n0991_002:0499a18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而說呪曰。",
        "from": "T19n0991_002:0499a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陀羅尼一馱離二輸婆摩帝三求那伽囉鉢囉鉢泥四摩呵若奴盧枳去聲五輸說羅引達彌六薩底夜波羅引底若七摩訶耶那殊引瑟𰈂八阿殊引瑟𰈂九盧歌㖿引瑟𰈂十婆伽婆帝佛陀彌帝隷十一阿鉢羅夜薩婆差多羅尼十二叔訖離施十三卑當婆離十四那茶羅引婆引私膩十五頭頭隷頭頭漏十六賒摩賒摩十七羶多引摩那賜十八除一切雨障莎呵。",
        "from": "T19n0991_002:0499a28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三世諸佛真實力故大慈心故。",
        "from": "T19n0991_002:0499a29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正行精進心故。",
        "from": "T19n0991_002:0499b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勅召一切諸大龍王莎呵。",
        "from": "T19n0991_002:0499b01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我勅無邊海莊嚴威德輪蓋龍王。",
        "from": "T19n0991_002:0499b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於閻浮提請雨國內降澍大雨莎呵。",
        "from": "T19n0991_002:0499b03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我勅難陀優波難陀龍王於閻浮提請雨國內降澍大雨莎呵。",
        "from": "T19n0991_002:0499b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我勅娑伽龍王於閻浮提請雨國內降澍大雨莎呵。",
        "from": "T19n0991_002:0499b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我勅阿耨達多龍王於閻浮提請雨國內降澍大雨莎呵。",
        "from": "T19n0991_002:0499b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我勅摩那斯龍王於閻浮提請雨國內降澍大雨莎呵。",
        "from": "T19n0991_002:0499b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我勅婆婁那龍王於閻浮提請雨國內降澍大雨莎呵。",
        "from": "T19n0991_002:0499b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我勅德叉迦龍王於閻浮提請雨國內降澍大雨莎呵。",
        "from": "T19n0991_002:0499b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我勅提頭賴吒龍王於閻浮提請雨國內降澍大雨莎呵。",
        "from": "T19n0991_002:0499b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我勅婆修吉龍王於閻浮提請雨國內降澍大雨莎呵。",
        "from": "T19n0991_002:0499b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我勅目真隣陀龍王於閻浮提請雨國內降澍大雨莎呵。",
        "from": "T19n0991_002:0499b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我勅伊羅跋那龍王於閻浮提請雨國內降澍大雨莎呵。",
        "from": "T19n0991_002:0499b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我勅分茶羅龍王於閻浮提請雨國內降澍大雨莎呵。",
        "from": "T19n0991_002:0499b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我勅大威光龍王於閻浮提請雨國內降澍大雨莎呵。",
        "from": "T19n0991_002:0499b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我勅威賢龍王於閻浮提請雨國內降澍大雨莎呵。",
        "from": "T19n0991_002:0499b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我勅電冠龍王於閻浮提請雨國內降澍大雨莎呵。",
        "from": "T19n0991_002:0499c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我勅大摩尼髻龍王於閻浮提請雨國內降澍大雨莎呵。",
        "from": "T19n0991_002:0499c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我勅戴摩尼髻龍王於閻浮提請雨國內降澍大雨莎呵。",
        "from": "T19n0991_002:0499c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我勅光髻龍王於閻浮提請雨國內降澍大雨莎呵。",
        "from": "T19n0991_002:0499c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我勅是等一切龍王於閻浮提請雨國內。",
        "from": "T19n0991_002:0499c09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降澍大雨訖。",
        "from": "T19n0991_002:0499c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又說呪曰。",
        "from": "T19n0991_002:0499c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那祇那祇瞿羅引摩引柰賜三那伽咥喜梨反四梨陀易頭摩鳩隷五郁伽羅盂路曬六波羅旃陀低豉七毘氀姞利八阿尸引毘師九阿咥引瞿十訖栗瑟那去崩引伽隷十一旃引遮隷十二盧羅引嗜薜十三摩訶頗那引𠳷隷𠳷羅引波施十四勞陀羅引波引尸膩十五頭沖薜十六波羅波羅十七庇利庇利十八富路富路十九毘私引哹匹尤反婁闍膩二十浮路浮路二十一摩訶蒲祇二十二摩尼達隷二十三匹利匹利二十四副漏副漏二十五破邏破邏二十六跋利沙跋利沙二十七閻藍浮引陀隷二十八睒浮睒浮二十九婆羅引訶翅三十那吒引碪薜三十一那吒引碪薜三十二忡忡忡忡薜三十三彌伽波羅引薜三十四彌伽婆引咥膩三十五茶迦茶迦茶迦三十六茶沈薜三十七伽那去伽那去三十八尸棄膩三十九迦那迦那四十伽那伽那四十一摩訶那伽引伽那去四十二尼囉引怛藍四十三糅引波闍羅四十四得迦紇唎四十五摩訶那伽引紇利陀引曳四十六瞿摩瞿摩瞿摩波引耶四十七頞悉低迦引承伽唎四十八浮承引伽彌四十九毘迦吒僧迦吒瞿隷五十毘私孚盧闍泥五十一毘折時列反林引婆泥五十二我今召集此會一切諸龍王等。",
        "from": "T19n0991_002:0500a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於閻浮提請雨國內降澍大雨。",
        "from": "T19n0991_002:0500a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸佛如來力故。",
        "from": "T19n0991_002:0500a05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三世諸佛真實力故。",
        "from": "T19n0991_002:0500a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈悲心故莎呵。",
        "from": "T19n0991_002:0500a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊說此呪已告龍王言。",
        "from": "T19n0991_002:0500a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若天旱時欲請雨者。",
        "from": "T19n0991_002:0500a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其請雨主必於一切諸眾生等起慈悲心。",
        "from": "T19n0991_002:0500a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有比丘及比丘尼必須戒行本來清淨。",
        "from": "T19n0991_002:0500a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若曾違犯尼薩耆罪乃至眾學。",
        "from": "T19n0991_002:0500a10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆須已前七日七夜殷重懺悔。",
        "from": "T19n0991_002:0500a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若在俗人亦須於前七日七夜。",
        "from": "T19n0991_002:0500a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日別須受八關齋戒。",
        "from": "T19n0991_002:0500a12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至請雨行道之日。",
        "from": "T19n0991_002:0500a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉須清淨無得懈慢。",
        "from": "T19n0991_002:0500a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當於空地實淨土上。",
        "from": "T19n0991_002:0500a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除去沙礫無諸棘草。",
        "from": "T19n0991_002:0500a14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方十二步以為道場場中起壇方十步。",
        "from": "T19n0991_002:0500a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壇高一尺用𤚩牛糞。",
        "from": "T19n0991_002:0500a15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取新淨者周匝泥壇。",
        "from": "T19n0991_002:0500a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於壇中央施一高座。",
        "from": "T19n0991_002:0500a16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "座上敷設新青淨褥張新青帳。",
        "from": "T19n0991_002:0500a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從高座東量三肘外。",
        "from": "T19n0991_002:0500a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用牛糞汁畫作龍王一身三頭。",
        "from": "T19n0991_002:0500a18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦畫彼龍左右眷屬圍遶。",
        "from": "T19n0991_002:0500a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍王從高座南量五肘外。",
        "from": "T19n0991_002:0500a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫作龍王一身五頭。",
        "from": "T19n0991_002:0500a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦畫諸龍左右圍遶。",
        "from": "T19n0991_002:0500a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從高座西量七肘外。",
        "from": "T19n0991_002:0500a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫作龍王一身七頭。",
        "from": "T19n0991_002:0500a22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦畫諸龍左右圍遶。",
        "from": "T19n0991_002:0500a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從高座北量九肘外。",
        "from": "T19n0991_002:0500a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫作龍王一身九頭。",
        "from": "T19n0991_002:0500a24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦畫諸龍左右圍遶。",
        "from": "T19n0991_002:0500a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其壇四角安四華瓶各容三升。",
        "from": "T19n0991_002:0500a26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以金精或復石黛。",
        "from": "T19n0991_002:0500a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和水令清悉使瓶滿。",
        "from": "T19n0991_002:0500a27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種華木諸雜華蘂插著瓶內。",
        "from": "T19n0991_002:0500a28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道場四門各各置一大妙香爐。",
        "from": "T19n0991_002:0500a29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒種種香熏陸沈水蘇合栴檀及安息等。",
        "from": "T19n0991_002:0500b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四角各懸青幡七枚。",
        "from": "T19n0991_002:0500b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合二十八幡各長一丈。",
        "from": "T19n0991_002:0500b02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然蘇油燈亦隨幡數。",
        "from": "T19n0991_002:0500b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設諸雜果蘇酪乳糜安置四面。",
        "from": "T19n0991_002:0500b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸龍王前散華燒香勿令斷絕。",
        "from": "T19n0991_002:0500b04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其果飲食及以瓶水。",
        "from": "T19n0991_002:0500b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日別使新不得隔宿。",
        "from": "T19n0991_002:0500b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒以晨朝日初出時施設供具。",
        "from": "T19n0991_002:0500b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讀經之者一日三時香湯澡浴。",
        "from": "T19n0991_002:0500b07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著新青衣持齋靜思。",
        "from": "T19n0991_002:0500b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟食蘇酪及乳糜等粳米果菜。",
        "from": "T19n0991_002:0500b08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若大小便又須澡浴。",
        "from": "T19n0991_002:0500b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昇高座時先禮十方一切諸佛。",
        "from": "T19n0991_002:0500b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒香散花奉請十方一切諸佛諸大菩薩。",
        "from": "T19n0991_002:0500b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及以一切諸天龍王。",
        "from": "T19n0991_002:0500b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其誦呪人為護身故。",
        "from": "T19n0991_002:0500b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或呪淨水或呪白灰。",
        "from": "T19n0991_002:0500b13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自心繫念以結場界。",
        "from": "T19n0991_002:0500b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於一步乃至多步。",
        "from": "T19n0991_002:0500b14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若水若灰用為界畔。",
        "from": "T19n0991_002:0500b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又呪縷綖為結。",
        "from": "T19n0991_002:0500b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫頂及以手足。",
        "from": "T19n0991_002:0500b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當呪水時散灑項上及灑額上。",
        "from": "T19n0991_002:0500b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應作是念。",
        "from": "T19n0991_002:0500b17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有惡心者不得入此壇場界內。",
        "from": "T19n0991_002:0500b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其誦呪者於諸眾生恒起慈心勿生惡念。",
        "from": "T19n0991_002:0500b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又願以此禮佛念誦及諸功德。",
        "from": "T19n0991_002:0500b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迴施一切諸天龍王。",
        "from": "T19n0991_002:0500b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并及含識有形之類。",
        "from": "T19n0991_002:0500b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昇法座時高聲讀誦此經及呪晝夜不絕。",
        "from": "T19n0991_002:0500b22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一七日若二七日。",
        "from": "T19n0991_002:0500b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠至三七日必降甘雨。",
        "from": "T19n0991_002:0500b23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除不專念無慈心人及穢濁者。",
        "from": "T19n0991_002:0500b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告龍王。",
        "from": "T19n0991_002:0500b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "海水潮來尚可盈縮。",
        "from": "T19n0991_002:0500b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此言真實決定不虛。",
        "from": "T19n0991_002:0500b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時諸龍王蒙佛教已。",
        "from": "T19n0991_002:0500b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡喜踊躍頂禮奉行。",
        "from": "T19n0991_002:0500b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大雲輪請雨經卷下",
        "from": "T19n0991_002:0500b28_100",
        "context_with_next_sentence": false
    }
]