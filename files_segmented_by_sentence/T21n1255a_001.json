[
    {
        "source_sentence": "佛說摩利支天菩薩陀羅尼經",
        "from": "T21n1255a_001:0259b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大興善寺三藏沙門不空奉　詔譯",
        "from": "T21n1255a_001:0259b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。一時佛在舍衛國祇樹給孤獨園。",
        "from": "T21n1255a_001:0259b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大阿羅漢千二百五十人俱。",
        "from": "T21n1255a_001:0259b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有無量大菩薩眾。",
        "from": "T21n1255a_001:0259b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彌勒菩薩。",
        "from": "T21n1255a_001:0259b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曼殊室利菩薩。",
        "from": "T21n1255a_001:0259b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀世音菩薩。",
        "from": "T21n1255a_001:0259b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而為上首。",
        "from": "T21n1255a_001:0259b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及摩利支等諸天。",
        "from": "T21n1255a_001:0259b20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍八部前後圍遶。",
        "from": "T21n1255a_001:0259b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時舍利弗即從座起。",
        "from": "T21n1255a_001:0259b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偏袒右肩右膝著地。",
        "from": "T21n1255a_001:0259b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌向佛。",
        "from": "T21n1255a_001:0259b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而白佛言。",
        "from": "T21n1255a_001:0259b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊於未來世。",
        "from": "T21n1255a_001:0259b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "末世眾生作何等法得脫諸難。",
        "from": "T21n1255a_001:0259b23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告舍利弗。",
        "from": "T21n1255a_001:0259b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諦聽諦聽。",
        "from": "T21n1255a_001:0259b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今為汝說於此事。",
        "from": "T21n1255a_001:0259b24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時會眾歡喜踊躍。",
        "from": "T21n1255a_001:0259b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重勸請佛。",
        "from": "T21n1255a_001:0259b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告諸比丘。",
        "from": "T21n1255a_001:0259b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日前有天。",
        "from": "T21n1255a_001:0259b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名摩利支。",
        "from": "T21n1255a_001:0259b26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大神通自在之法。",
        "from": "T21n1255a_001:0259b27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常行日前。",
        "from": "T21n1255a_001:0259b27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日不見彼。",
        "from": "T21n1255a_001:0259b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼能見日。",
        "from": "T21n1255a_001:0259b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能見。",
        "from": "T21n1255a_001:0259b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能知。",
        "from": "T21n1255a_001:0259b28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能捉。",
        "from": "T21n1255a_001:0259b28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能害。",
        "from": "T21n1255a_001:0259b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人欺誑。",
        "from": "T21n1255a_001:0259b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能縛。",
        "from": "T21n1255a_001:0259b29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能債其財物。",
        "from": "T21n1255a_001:0259b29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能罰。",
        "from": "T21n1255a_001:0259c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不畏怨家能得其便。",
        "from": "T21n1255a_001:0259c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告諸比丘。",
        "from": "T21n1255a_001:0259c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人知彼摩利支天名者。",
        "from": "T21n1255a_001:0259c02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼人亦不可見亦不可知。",
        "from": "T21n1255a_001:0259c03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不可捉亦不可害。",
        "from": "T21n1255a_001:0259c03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不為人欺誑亦不為人縛。",
        "from": "T21n1255a_001:0259c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不為人債其財物。",
        "from": "T21n1255a_001:0259c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不為人責罰。",
        "from": "T21n1255a_001:0259c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不為怨家能得其便。",
        "from": "T21n1255a_001:0259c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告諸比丘。",
        "from": "T21n1255a_001:0259c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人知彼摩利支天名者。",
        "from": "T21n1255a_001:0259c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應作是言。",
        "from": "T21n1255a_001:0259c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我弟子某甲知彼摩利支天名故。",
        "from": "T21n1255a_001:0259c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能見我。",
        "from": "T21n1255a_001:0259c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能知我。",
        "from": "T21n1255a_001:0259c09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能捉我。",
        "from": "T21n1255a_001:0259c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能害我。",
        "from": "T21n1255a_001:0259c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能欺誑我。",
        "from": "T21n1255a_001:0259c10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能縛我。",
        "from": "T21n1255a_001:0259c10_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能債我財物。",
        "from": "T21n1255a_001:0259c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能責罰我。",
        "from": "T21n1255a_001:0259c11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不為怨家能得我便。",
        "from": "T21n1255a_001:0259c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此呪有大神力。",
        "from": "T21n1255a_001:0259c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所作成就。",
        "from": "T21n1255a_001:0259c12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破一切惡。",
        "from": "T21n1255a_001:0259c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若用結界百由旬內一切諸惡無敢入者。",
        "from": "T21n1255a_001:0259c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊即說呪曰。",
        "from": "T21n1255a_001:0259c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無佛陀耶一南無達摩耶二南無僧伽耶三怛姪他四遏囉迦末斯五摩囉迦末斯六蘇途末斯七支鉢囉末斯八摩訶支鉢囉末斯九摩利支夜末斯十安怛陀那夜末斯十一那謨粹都底十二莎訶十三王難中護我。",
        "from": "T21n1255a_001:0259c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賊難中護我。",
        "from": "T21n1255a_001:0259c20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行路中護我。",
        "from": "T21n1255a_001:0259c20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "失於道路曠野中護我。",
        "from": "T21n1255a_001:0259c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晝日護我。",
        "from": "T21n1255a_001:0259c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜中護我。",
        "from": "T21n1255a_001:0259c21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水難中護我。",
        "from": "T21n1255a_001:0259c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火難中護我。",
        "from": "T21n1255a_001:0259c22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅剎難中護我。",
        "from": "T21n1255a_001:0259c22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "茶枳儞鬼難中護我。",
        "from": "T21n1255a_001:0259c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒藥難中護我。",
        "from": "T21n1255a_001:0259c23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛實語護我。",
        "from": "T21n1255a_001:0259c23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法實語護我。",
        "from": "T21n1255a_001:0259c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧實語護我。",
        "from": "T21n1255a_001:0259c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天實語護我。",
        "from": "T21n1255a_001:0259c24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仙人實語護我。",
        "from": "T21n1255a_001:0259c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛姪他一阿羅拘隷二阿囉拘隷三雞栗底知約反四薩婆迦囉醯蔽蔽亞反下同五薩蒲跛突瑟䶩二合蔽六薩婆伊都波達羅鞞蔽七囉上音叉囉上音叉自稱名字八莎呵九奉請摩利支天呪。",
        "from": "T21n1255a_001:0260a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名摩利支天身呪。",
        "from": "T21n1255a_001:0260a01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1255a_001:0260a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨囉跢那二合跢囉二合夜耶一摩唎支唎馱去音耶二摩婆上帝移沙上彌三跢姪他四婆上囉去梨五婆馱梨六婆囉呵目溪七薩婆上徒瑟誓去二合八槃上馱槃上馱九娑婆呵十佛告諸比丘。",
        "from": "T21n1255a_001:0260a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人識彼摩利支天菩薩者。",
        "from": "T21n1255a_001:0260a07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除一切障難王難賊難。",
        "from": "T21n1255a_001:0260a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猛獸毒蟲之難。",
        "from": "T21n1255a_001:0260a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水火等難。",
        "from": "T21n1255a_001:0260a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲行此法者。",
        "from": "T21n1255a_001:0260a09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切法中此法最勝。",
        "from": "T21n1255a_001:0260a09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持此呪者面向百踰闍那。",
        "from": "T21n1255a_001:0260a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切鬼神惡人無能得其便者。",
        "from": "T21n1255a_001:0260a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於難中行時。",
        "from": "T21n1255a_001:0260a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晨起誦前身呪。",
        "from": "T21n1255a_001:0260a11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪一掬水。",
        "from": "T21n1255a_001:0260a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四方散灑及灑自身。",
        "from": "T21n1255a_001:0260a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若衣襟若衣袂若袈裟角。",
        "from": "T21n1255a_001:0260a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一呪一結總作三結。",
        "from": "T21n1255a_001:0260a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即往難中行連誦前二大呪而行。",
        "from": "T21n1255a_001:0260a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有一切事難軍防主者悉皆迷醉。",
        "from": "T21n1255a_001:0260a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "都無覺知之者。",
        "from": "T21n1255a_001:0260a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說是經已。",
        "from": "T21n1255a_001:0260a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告諸比丘比丘尼。",
        "from": "T21n1255a_001:0260a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "優婆塞優婆夷。",
        "from": "T21n1255a_001:0260a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國王大臣及諸人民等。",
        "from": "T21n1255a_001:0260a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛說是摩利支天陀羅尼。",
        "from": "T21n1255a_001:0260a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心受持之者。",
        "from": "T21n1255a_001:0260a18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人不為一切諸惡所害。",
        "from": "T21n1255a_001:0260a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸比丘若有人。",
        "from": "T21n1255a_001:0260a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能書寫讀誦受持之者。",
        "from": "T21n1255a_001:0260a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若著髻中。",
        "from": "T21n1255a_001:0260a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若著衣中。",
        "from": "T21n1255a_001:0260a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨身而行。",
        "from": "T21n1255a_001:0260a20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸惡悉皆退散無敢當者。",
        "from": "T21n1255a_001:0260a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸四眾天龍八部禮佛而退。",
        "from": "T21n1255a_001:0260a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡喜奉行。",
        "from": "T21n1255a_001:0260a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說摩利支天菩薩陀羅尼經",
        "from": "T21n1255a_001:0260a23_100",
        "context_with_next_sentence": false
    }
]