[
    {
        "source_sentence": "佛母般若波羅蜜多圓集要義論",
        "from": "T25n1518_001:0912c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大域龍菩薩造",
        "from": "T25n1518_001:0912c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝奉大夫試光祿",
        "from": "T25n1518_001:0912c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卿傳法大師賜紫臣施護等奉　詔譯歸命妙吉祥童真菩薩摩訶薩等。",
        "from": "T25n1518_001:0912c13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "般若等成就，　　無二智如來，",
        "from": "T25n1518_001:0912c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼中義相應，　　彼聲教道二。",
        "from": "T25n1518_001:0912c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依止及作用，　　事業同起修，",
        "from": "T25n1518_001:0912c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分別相及罪，　　稱讚如次說。",
        "from": "T25n1518_001:0912c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具信以為體，　　師資互證說，",
        "from": "T25n1518_001:0912c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說時說處等，　　得自量成就。",
        "from": "T25n1518_001:0912c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說法者應知，　　世間時處二，",
        "from": "T25n1518_001:0912c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說者有同證，　　然後得如量。",
        "from": "T25n1518_001:0912c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如是集，　　我聞等所說，",
        "from": "T25n1518_001:0912c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和合如是義，　　最上三十二。",
        "from": "T25n1518_001:0912c23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分別十六相，　　空如其次第，",
        "from": "T25n1518_001:0912c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八千頌中說，　　了異方便說。",
        "from": "T25n1518_001:0912c25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此八千頌，　　如說義無減，",
        "from": "T25n1518_001:0912c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨所樂頌略，　　如是義如說。",
        "from": "T25n1518_001:0912c27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩我不見，　　此說實寂默，",
        "from": "T25n1518_001:0912c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能受內諸事，　　彼說即為空。",
        "from": "T25n1518_001:0912c29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色及色自性，　　此說亦復空，",
        "from": "T25n1518_001:0913a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此等外諸處，　　所受分皆止。",
        "from": "T25n1518_001:0913a02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色等相彼身，　　安住及相離，",
        "from": "T25n1518_001:0913a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向義若彼見，　　彼內即無實。",
        "from": "T25n1518_001:0913a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼諸內空性，　　自性亦復空，",
        "from": "T25n1518_001:0913a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有識相種，　　即起我悲智。",
        "from": "T25n1518_001:0913a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不生亦不滅，　　有情此等明，",
        "from": "T25n1518_001:0913a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有情生死欲，　　彼說即為空。",
        "from": "T25n1518_001:0913a08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛法不可見，　　菩薩法亦然，",
        "from": "T25n1518_001:0913a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此等如所說，　　空彼十力等。",
        "from": "T25n1518_001:0913a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別別所有法，　　此說遍計性，",
        "from": "T25n1518_001:0913a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼勝義非有，　　諸法如是說。",
        "from": "T25n1518_001:0913a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼我等見斷，　　大士畢竟作，",
        "from": "T25n1518_001:0913a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而彼人無我，　　佛一切處說。",
        "from": "T25n1518_001:0913a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切法不生，　　此所說亦然，",
        "from": "T25n1518_001:0913a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宣說法無我，　　一切處實說。",
        "from": "T25n1518_001:0913a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有罪及無罪，　　不增亦不減，",
        "from": "T25n1518_001:0913a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸有為無為，　　所有諸善止。",
        "from": "T25n1518_001:0913a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸善空性中，　　彼出亦無盡，",
        "from": "T25n1518_001:0913a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此遍計分別，　　彼普攝為空。",
        "from": "T25n1518_001:0913a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十種心散亂，　　心散亂異處，",
        "from": "T25n1518_001:0913a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愚不得相應，　　無二智不成。",
        "from": "T25n1518_001:0913a22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼止息互相，　　為能所對治，",
        "from": "T25n1518_001:0913a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於般若教中，　　彼圓集所說。",
        "from": "T25n1518_001:0913a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有菩薩有，　　此無相分別，",
        "from": "T25n1518_001:0913a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散亂止息師，　　說彼世俗蘊。",
        "from": "T25n1518_001:0913a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此八千頌等，　　從初語次第，",
        "from": "T25n1518_001:0913a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至了畢皆止，　　說無相分別。",
        "from": "T25n1518_001:0913a28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因言不如是，　　此唯說事相，",
        "from": "T25n1518_001:0913a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵網等經中，　　知一切如理。",
        "from": "T25n1518_001:0913b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩我不見，　　而此等廣大，",
        "from": "T25n1518_001:0913b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊此止遣，　　有相分別亂。",
        "from": "T25n1518_001:0913b03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不見彼名，　　境界行亦然，",
        "from": "T25n1518_001:0913b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼蘊一切處，　　皆不見菩薩。",
        "from": "T25n1518_001:0913b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此止遣遍計，　　普攝此所說，",
        "from": "T25n1518_001:0913b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘一切智因，　　慧分別諸相。",
        "from": "T25n1518_001:0913b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "般若波羅蜜，　　說三種依止，",
        "from": "T25n1518_001:0913b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂遍計依他，　　及圓成實性。",
        "from": "T25n1518_001:0913b09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無此等說句，　　一切遍計止，",
        "from": "T25n1518_001:0913b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幻喻等見邊，　　此說依他性。",
        "from": "T25n1518_001:0913b11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有四種清淨，　　說圓成實性，",
        "from": "T25n1518_001:0913b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "般若波羅蜜，　　佛無別異說。",
        "from": "T25n1518_001:0913b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十分別散亂，　　對治如次說，",
        "from": "T25n1518_001:0913b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此三種知已，　　若即若離說。",
        "from": "T25n1518_001:0913b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如初語圓成，　　依他及遍計，",
        "from": "T25n1518_001:0913b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無相分別色，　　彼散亂止遣。",
        "from": "T25n1518_001:0913b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼佛亦菩提，　　不見說者等，",
        "from": "T25n1518_001:0913b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至了畢此知，　　止遣遍計性。",
        "from": "T25n1518_001:0913b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自性空彼色，　　俱相何所有，",
        "from": "T25n1518_001:0913b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此別異語中，　　了知已彼止。",
        "from": "T25n1518_001:0913b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此不空故空，　　如是語所說，",
        "from": "T25n1518_001:0913b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸毀謗分別，　　一切說皆止。",
        "from": "T25n1518_001:0913b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如幻亦然佛，　　彼如夢亦然，",
        "from": "T25n1518_001:0913b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是如次知，　　智語邊決定。",
        "from": "T25n1518_001:0913b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸同等所作，　　此說佛如幻，",
        "from": "T25n1518_001:0913b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幻喻等言等，　　此說依他性。",
        "from": "T25n1518_001:0913b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若諸異生智，　　彼自性清淨，",
        "from": "T25n1518_001:0913b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故說彼佛言，　　菩薩亦如佛。",
        "from": "T25n1518_001:0913b29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自性自色覆，　　彼無明因作，",
        "from": "T25n1518_001:0913c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如幻別異現，　　果如夢棄捨。",
        "from": "T25n1518_001:0913c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無二別異說，　　果等定毀謗，",
        "from": "T25n1518_001:0913c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毀謗諸分別，　　彼毀謗此說。",
        "from": "T25n1518_001:0913c04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色空非和合，　　彼互相違礙，",
        "from": "T25n1518_001:0913c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無色無空名，　　色相自和合。",
        "from": "T25n1518_001:0913c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此一性分別，　　對治種種性，",
        "from": "T25n1518_001:0913c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空不異彼色，　　彼空何所有。",
        "from": "T25n1518_001:0913c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此無實所現，　　彼無明所起，",
        "from": "T25n1518_001:0913c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此無實能表，　　彼說無明故。",
        "from": "T25n1518_001:0913c10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此如是說色，　　般若波羅蜜，",
        "from": "T25n1518_001:0913c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無二二如是，　　二分別對治。",
        "from": "T25n1518_001:0913c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如理言淨性，　　亦然不可得，",
        "from": "T25n1518_001:0913c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性無性違等，　　種種性定見。",
        "from": "T25n1518_001:0913c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此色唯名，　　真實無自性，",
        "from": "T25n1518_001:0913c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼自性分別，　　容受即當止。",
        "from": "T25n1518_001:0913c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色及色自性，　　空如先所說，",
        "from": "T25n1518_001:0913c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼自性俱相，　　分別此止遣。",
        "from": "T25n1518_001:0913c18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不生不滅等，　　所有諸法觀，",
        "from": "T25n1518_001:0913c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言若散異，　　彼差別分別。",
        "from": "T25n1518_001:0913c20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛假名言等，　　彼法若分別，",
        "from": "T25n1518_001:0913c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聲義二非合，　　彼非自性意。",
        "from": "T25n1518_001:0913c22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "般若波羅蜜，　　佛菩薩亦然，",
        "from": "T25n1518_001:0913c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此所說唯名，　　離實義分別。",
        "from": "T25n1518_001:0913c24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有聲義止，　　此非事止遣，",
        "from": "T25n1518_001:0913c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是餘亦知，　　語中義決定。",
        "from": "T25n1518_001:0913c26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此無所得正，　　一切名實知，",
        "from": "T25n1518_001:0913c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如義性如是，　　不止遣彼聲。",
        "from": "T25n1518_001:0913c28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須菩提二離，　　聲聲義如是，",
        "from": "T25n1518_001:0913c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩無有名，　　我見此有說。",
        "from": "T25n1518_001:0914a01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "般若波羅蜜，　　語無決定生，",
        "from": "T25n1518_001:0914a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伺察唯智者，　　此義微妙慧。",
        "from": "T25n1518_001:0914a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相續義除遣，　　若別義分別，",
        "from": "T25n1518_001:0914a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "般若波羅蜜，　　彼言說如響。",
        "from": "T25n1518_001:0914a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總略如是義，　　般若等依止，",
        "from": "T25n1518_001:0914a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是義循環，　　復別義依止。",
        "from": "T25n1518_001:0914a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "般若波羅蜜，　　正攝八千頌，",
        "from": "T25n1518_001:0914a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼所得福蘊，　　皆從般若生。",
        "from": "T25n1518_001:0914a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛母般若波羅蜜多圓集要義論",
        "from": "T25n1518_001:0914a10_100",
        "context_with_next_sentence": false
    }
]