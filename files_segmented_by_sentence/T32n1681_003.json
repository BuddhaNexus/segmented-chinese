[
    {
        "source_sentence": "佛吉祥德讚卷下",
        "from": "T32n1681_003:0766c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊者寂友造",
        "from": "T32n1681_003:0766c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命作大師子吼，　　出生一切善行門，",
        "from": "T32n1681_003:0766c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切性如實知，　　無勝知見遍一切，",
        "from": "T32n1681_003:0766c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善化眾生時無間，　　宣說無邊諸法門，",
        "from": "T32n1681_003:0766c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最先破彼無明卵，　　隨覺一切諸法者。",
        "from": "T32n1681_003:0766c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覺了眾生大正士，　　覺悟微妙諸法尊，",
        "from": "T32n1681_003:0766c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於無我理已覺明，　　善覺諸業自性者。",
        "from": "T32n1681_003:0766c10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覺了一切界分量，　　了知生滅本來性，",
        "from": "T32n1681_003:0766c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸苦法如實知，　　善悟真實自性者。",
        "from": "T32n1681_003:0766c12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善能覺悟無嬈惱，　　世間自性悉覺知，",
        "from": "T32n1681_003:0766c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破諸惡見智開明，　　覺了一切因果者。",
        "from": "T32n1681_003:0766c14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覺了三界出離相，　　故復修諸極難行，",
        "from": "T32n1681_003:0766c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身無疲倦執相工，　　然於小法不厭離，",
        "from": "T32n1681_003:0766c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畢竟方便善攝益，　　宣說中道諸法門，",
        "from": "T32n1681_003:0766c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉釋迦牟尼尊，　　頂禮歸命應供者。",
        "from": "T32n1681_003:0766c18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最上說者寂默者，　　最上最勝我大師，",
        "from": "T32n1681_003:0766c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善攝眾生諸意樂，　　於諸事相悉不取。",
        "from": "T32n1681_003:0766c20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足一種稱讚者，　　善能成就四攝法，",
        "from": "T32n1681_003:0766c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六和敬法善宣揚，　　六常行行已圓滿。",
        "from": "T32n1681_003:0766c22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安立甚深正知見，　　得諸等持寂靜善，",
        "from": "T32n1681_003:0766c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善法中無放逸心，　　已證先佛所成道。",
        "from": "T32n1681_003:0766c24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覺了甚深正道者，　　開發甚深智光明，",
        "from": "T32n1681_003:0766c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彼無尋無伺道，　　稽首如來證悟者。",
        "from": "T32n1681_003:0766c26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總聚智者所知境，　　微細甚深悉覺知，",
        "from": "T32n1681_003:0766c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯示二種涅槃界，　　盡證一切涅槃道。",
        "from": "T32n1681_003:0766c28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生長夜起虛妄，　　佛方便說無妄法，",
        "from": "T32n1681_003:0766c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生沒在生死泥，　　佛為垂手善接度，",
        "from": "T32n1681_003:0767a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生墮在惡趣者，　　佛方便力為拯拔，",
        "from": "T32n1681_003:0767a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸眾生起生等怖，　　佛為引示無畏處。",
        "from": "T32n1681_003:0767a03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自具廣大威神力，　　身現一切色相寶，",
        "from": "T32n1681_003:0767a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內藏心寶妙圓明，　　宣說無比正法寶。",
        "from": "T32n1681_003:0767a05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛不為他所攝伏，　　一切無能違佛者，",
        "from": "T32n1681_003:0767a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安住一切歡喜門，　　佛一切相皆圓滿，",
        "from": "T32n1681_003:0767a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無上沙門正真行，　　如蓮華開智清淨，",
        "from": "T32n1681_003:0767a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普集梵行盡無餘，　　稽首梵行已立者。",
        "from": "T32n1681_003:0767a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖明世間圍陀典，　　不壞一切聖法教，",
        "from": "T32n1681_003:0767a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洗滌一切諸罪垢，　　獲得第一增上法。",
        "from": "T32n1681_003:0767a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善能成就持明法，　　能善引示寂靜處，",
        "from": "T32n1681_003:0767a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "導師開示善樂門，　　輪迴曠野為善導。",
        "from": "T32n1681_003:0767a13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常離一切諸暗冥，　　開熙怡眼常觀視，",
        "from": "T32n1681_003:0767a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畢竟無染清淨尊，　　罪福無動行解脫。",
        "from": "T32n1681_003:0767a15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能於自身觀空性，　　愛見已盡善無染，",
        "from": "T32n1681_003:0767a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸欲境離想心，　　離染煩惱及分別。",
        "from": "T32n1681_003:0767a17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心住寂靜諸事業，　　默然法中得解脫，",
        "from": "T32n1681_003:0767a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最上寂靜大牟尼，　　一切清淨無染者。",
        "from": "T32n1681_003:0767a19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出世功德已廣大，　　善布世間大明照，",
        "from": "T32n1681_003:0767a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間妄境悉能觀，　　圓滿真實諸法想。",
        "from": "T32n1681_003:0767a21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛繩已斷神通具，　　善發智慧大光明，",
        "from": "T32n1681_003:0767a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破彼一切依著心，　　於諸意樂皆寂靜。",
        "from": "T32n1681_003:0767a23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來已離諸過失，　　所應敬禮及供養，",
        "from": "T32n1681_003:0767a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切無利惡法除，　　一切利益相應者。",
        "from": "T32n1681_003:0767a25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛與眾生為善友，　　無足二足第一者，",
        "from": "T32n1681_003:0767a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安住念慧諸性中，　　善具無忘失法者。",
        "from": "T32n1681_003:0767a27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛於義無義自性，　　和合依止而不著，",
        "from": "T32n1681_003:0767a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生死此岸怖已除，　　能證涅槃彼岸樂。",
        "from": "T32n1681_003:0767a29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛善依止正道行，　　聽聞無謬心無減，",
        "from": "T32n1681_003:0767b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸清淨中佛最尊，　　廣大色相慧光照。",
        "from": "T32n1681_003:0767b02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間所有耽欲味，　　佛智久已善出離，",
        "from": "T32n1681_003:0767b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智慧稱讚涅槃門，　　稽首如來決定說。",
        "from": "T32n1681_003:0767b04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無染見及無染思，　　無染語并無染業，",
        "from": "T32n1681_003:0767b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無染命與無染勤，　　無染念兼無染定。",
        "from": "T32n1681_003:0767b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解脫無染智無染，　　於戒定慧善安住，",
        "from": "T32n1681_003:0767b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切結縛悉斷除，　　破諸煩惱顛倒處。",
        "from": "T32n1681_003:0767b08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來梵行所出生，　　已住最上清涼地，",
        "from": "T32n1681_003:0767b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切所作寂靜尊，　　不著世間敬愛事。",
        "from": "T32n1681_003:0767b10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已能斷除五分結，　　復能具足六分法，",
        "from": "T32n1681_003:0767b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起一平等護念心，　　四依成就而無缺。",
        "from": "T32n1681_003:0767b12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "純一真實無別異，　　平等棄捨世所愛，",
        "from": "T32n1681_003:0767b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無倒思惟真正心，　　已得一切輕安者。",
        "from": "T32n1681_003:0767b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心善解脫慧解脫，　　純一梵行善安立，",
        "from": "T32n1681_003:0767b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來最上勝丈夫，　　是故歸命廣稱讚。",
        "from": "T32n1681_003:0767b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稽首勝大阿羅漢，　　離名離相離分位，",
        "from": "T32n1681_003:0767b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出過名相分位門，　　名相分位皆寂靜，",
        "from": "T32n1681_003:0767b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善見心能息欲焰，　　欲境貪愛染皆除。",
        "from": "T32n1681_003:0767b19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸欲染離含藏，　　及離一切欲過失，",
        "from": "T32n1681_003:0767b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善修七種觀想法，　　善說菩提分法門，",
        "from": "T32n1681_003:0767b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切已能勝伏他，　　佛是勇猛無畏者。",
        "from": "T32n1681_003:0767b22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離癡斷染勝中勝，　　戒力增上無罪者，",
        "from": "T32n1681_003:0767b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具善智慧微妙心，　　內攝種種功德行，",
        "from": "T32n1681_003:0767b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無疑離染常喜足，　　已息世間諸妄源，",
        "from": "T32n1681_003:0767b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大沙門事作已成，　　稽首真實歡喜者。",
        "from": "T32n1681_003:0767b26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛為摩耨惹中勝，　　已於其身善觀察，",
        "from": "T32n1681_003:0767b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應得已得諸說門，　　稽首如來無比喻。",
        "from": "T32n1681_003:0767b28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正念勝觀一切覺，　　不為他伏能伏他，",
        "from": "T32n1681_003:0767b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已得寂靜無所為，　　稽首牛王勝上者。",
        "from": "T32n1681_003:0767c01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量甚深住寂默，　　正智常行安樂行，",
        "from": "T32n1681_003:0767c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如其法律妙威儀，　　稽首身嚴心寂靜。",
        "from": "T32n1681_003:0767c03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸行妙圓語善說，　　身現頭陀難行相，",
        "from": "T32n1681_003:0767c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言無戲論住正真，　　稽首離貪正命士。",
        "from": "T32n1681_003:0767c05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來尊勝復自在，　　猶如帝釋天中勝，",
        "from": "T32n1681_003:0767c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所向正順復善觀，　　語言謙下而和美。",
        "from": "T32n1681_003:0767c07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切清淨清淨者，　　善愛無著廣清淨，",
        "from": "T32n1681_003:0767c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成熟功德香充滿，　　稽首已到最上處。",
        "from": "T32n1681_003:0767c09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛是最上大仙王，　　廣積功德滿無減，",
        "from": "T32n1681_003:0767c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨行已圓諂已除，　　稽首正智光明聚。",
        "from": "T32n1681_003:0767c11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身心清淨復輕安，　　已息一切諸冤對，",
        "from": "T32n1681_003:0767c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寂慧廣慧大慧尊，　　塞忿恚源常歡喜，",
        "from": "T32n1681_003:0767c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "補特伽羅中無勝，　　不可稱量離諸著，",
        "from": "T32n1681_003:0767c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "句身通達已無疑，　　稽首能仁善解者。",
        "from": "T32n1681_003:0767c15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教化調伏中最上，　　無上可愛作光明，",
        "from": "T32n1681_003:0767c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無求無慢無愛著，　　稽首無誑清淨者。",
        "from": "T32n1681_003:0767c17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已離暗翳無所染，　　補特伽羅中最上，",
        "from": "T32n1681_003:0767c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已得善寂大名稱，　　稽首無著無纏縛。",
        "from": "T32n1681_003:0767c19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迷惑耽著久已離，　　無我取相我見除，",
        "from": "T32n1681_003:0767c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初中後善法宣揚，　　善文善義皆圓滿。",
        "from": "T32n1681_003:0767c21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生處苦無疲懈，　　佛方便故令休息，",
        "from": "T32n1681_003:0767c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生不趣出離門，　　佛方便故令出離，",
        "from": "T32n1681_003:0767c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來不住於滅法，　　常行救度攝世間，",
        "from": "T32n1681_003:0767c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復於廣大正法門，　　稽首大智悉包攝。",
        "from": "T32n1681_003:0767c25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已息邪妄諸分別，　　善離一切尋求道，",
        "from": "T32n1681_003:0767c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "催諸惡法號勝人，　　離七種染圓梵行，",
        "from": "T32n1681_003:0767c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如空欲泥不能染，　　清淨常依止梵行。",
        "from": "T32n1681_003:0767c28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖除染法了性空，　　出過語言心無著，",
        "from": "T32n1681_003:0767c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛常安住身念性，　　佛心已過於二邊，",
        "from": "T32n1681_003:0768a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正智深密勇力尊，　　吉祥門中善身者。",
        "from": "T32n1681_003:0768a02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稽首大悲不思議，　　一切所作自通力，",
        "from": "T32n1681_003:0768a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他劣弱者助營修，　　他善美者必隨順。",
        "from": "T32n1681_003:0768a04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛是金剛堅固身，　　從真實處所出生，",
        "from": "T32n1681_003:0768a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安住一切相應門，　　得大涅槃最上樂。",
        "from": "T32n1681_003:0768a06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來已得最上利，　　棄捨自樂而不著，",
        "from": "T32n1681_003:0768a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "引示世間安隱處，　　廣為眾生說正道。",
        "from": "T32n1681_003:0768a08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命大覺尊，　　現證正覺道，",
        "from": "T32n1681_003:0768a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不修梵行者，　　佛善為建立。",
        "from": "T32n1681_003:0768a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛是知道者，　　識道說道者，",
        "from": "T32n1681_003:0768a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛為正道尊，　　諸道所歸向。",
        "from": "T32n1681_003:0768a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛從照明生，　　復是智生者，",
        "from": "T32n1681_003:0768a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義生及法生，　　善說明了義。",
        "from": "T32n1681_003:0768a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸義出離者，　　人中大師子，",
        "from": "T32n1681_003:0768a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人中大象龍，　　人中大仙王，",
        "from": "T32n1681_003:0768a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人中正知者，　　人中智勇尊，",
        "from": "T32n1681_003:0768a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人中勝無比，　　人中極最上，",
        "from": "T32n1681_003:0768a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人中殊妙士，　　人中白蓮華。",
        "from": "T32n1681_003:0768a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無怖及無驚，　　無怯亦無懼，",
        "from": "T32n1681_003:0768a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛是離怖者，　　無畏無奔競。",
        "from": "T32n1681_003:0768a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怖畏險難除，　　斷滅諸怖難，",
        "from": "T32n1681_003:0768a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自出過怖境，　　復令他亦出，",
        "from": "T32n1681_003:0768a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自斷滅怖難，　　復令他亦斷，",
        "from": "T32n1681_003:0768a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自渡怖難海，　　復令他亦渡。",
        "from": "T32n1681_003:0768a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離身毛喜豎，　　離身毛悚立，",
        "from": "T32n1681_003:0768a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛勝無比等，　　亦復無等等。",
        "from": "T32n1681_003:0768a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功德不可稱，　　已過於稱量，",
        "from": "T32n1681_003:0768a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復不可取像，　　亦無各別分。",
        "from": "T32n1681_003:0768a29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勝補特伽羅，　　唯一而無二，",
        "from": "T32n1681_003:0768b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛是自然智，　　而無同等者。",
        "from": "T32n1681_003:0768b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣利諸眾生，　　稽首一切智，",
        "from": "T32n1681_003:0768b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天人世間，　　梵魔沙門中，",
        "from": "T32n1681_003:0768b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛是最上尊，　　復為無勝者。",
        "from": "T32n1681_003:0768b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛寶甚希有，　　而亦復難得，",
        "from": "T32n1681_003:0768b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遇佛寶出生，　　實希有難得。",
        "from": "T32n1681_003:0768b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛是廣大眼，　　復為大光明，",
        "from": "T32n1681_003:0768b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大曜大明照，　　大燈大光炬，",
        "from": "T32n1681_003:0768b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無暗大熾明，　　已得最上法，",
        "from": "T32n1681_003:0768b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "根力覺道圓，　　不與聲聞共。",
        "from": "T32n1681_003:0768b11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無利益事，　　如來即不生，",
        "from": "T32n1681_003:0768b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無利不現化，　　無利亦不隱。",
        "from": "T32n1681_003:0768b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛為諸群品，　　利益故出生，",
        "from": "T32n1681_003:0768b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悲愍諸世間，　　作諸利樂事，",
        "from": "T32n1681_003:0768b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸天人中，　　佛是正見者，",
        "from": "T32n1681_003:0768b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以正道法律，　　普教示一切。",
        "from": "T32n1681_003:0768b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復一切眾中，　　佛為所愛尊，",
        "from": "T32n1681_003:0768b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸起敬愛心，　　作尊重觀想，",
        "from": "T32n1681_003:0768b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛是所親近，　　及隨從恭敬，",
        "from": "T32n1681_003:0768b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂禮見真如，　　吉祥最上寶。",
        "from": "T32n1681_003:0768b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛慧眼光明，　　復從慧眼生，",
        "from": "T32n1681_003:0768b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開發慧焰明，　　持廣大慧炬，",
        "from": "T32n1681_003:0768b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燃慧燈普耀，　　大慧破諸暗。",
        "from": "T32n1681_003:0768b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切性自性，　　如來悉照了，",
        "from": "T32n1681_003:0768b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來勝慧根，　　微妙復最上。",
        "from": "T32n1681_003:0768b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來大慧力，　　而不可屈伏，",
        "from": "T32n1681_003:0768b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "積無盡慧財，　　具無價慧寶，",
        "from": "T32n1681_003:0768b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持快利慧刀，　　執勝慧器仗，",
        "from": "T32n1681_003:0768b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "秉鋒利慧劍，　　慧無墮無減。",
        "from": "T32n1681_003:0768c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛善開正慧，　　覺了一切法，",
        "from": "T32n1681_003:0768c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣慧為宮殿，　　正智者安處。",
        "from": "T32n1681_003:0768c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅固慧為牆，　　周匝而密護，",
        "from": "T32n1681_003:0768c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不思議正慧，　　為階梯進趣。",
        "from": "T32n1681_003:0768c05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無屈伏勝尊，　　無異心安住，",
        "from": "T32n1681_003:0768c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為他所伏，　　亦伏無所取，",
        "from": "T32n1681_003:0768c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而於三界中，　　所應供養者。",
        "from": "T32n1681_003:0768c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切現觀察，　　一切無所著，",
        "from": "T32n1681_003:0768c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所應知已知，　　所應離已離，",
        "from": "T32n1681_003:0768c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所應得已得，　　勝所作已作，",
        "from": "T32n1681_003:0768c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "己事悉周圓，　　為世間教授。",
        "from": "T32n1681_003:0768c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛是大聖者，　　精進未曾有，",
        "from": "T32n1681_003:0768c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已安立清淨，　　希有廣大行，",
        "from": "T32n1681_003:0768c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現證最上道，　　希有難思法，",
        "from": "T32n1681_003:0768c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲得取上句，　　飲涅槃甘露。",
        "from": "T32n1681_003:0768c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拔除愛網根，　　絕諸過失本，",
        "from": "T32n1681_003:0768c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善行無別異，　　無染法能著，",
        "from": "T32n1681_003:0768c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善生及善體，　　勝善而出家，",
        "from": "T32n1681_003:0768c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善本所從來，　　稽首善來者。",
        "from": "T32n1681_003:0768c20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無動不思議，　　智者智中尊，",
        "from": "T32n1681_003:0768c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諂曲過染除，　　已渡煩惱海，",
        "from": "T32n1681_003:0768c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相應行解脫，　　解脫諸纏縛，",
        "from": "T32n1681_003:0768c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已得無取心，　　已盡一切漏，",
        "from": "T32n1681_003:0768c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已盡過失邊，　　已破一切毒，",
        "from": "T32n1681_003:0768c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已斷一切障，　　盡燒諸見網。",
        "from": "T32n1681_003:0768c26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有邪妄言，　　及邪妄見聞，",
        "from": "T32n1681_003:0768c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "邪念邪作事，　　佛一切已離。",
        "from": "T32n1681_003:0768c28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘊處界諸法，　　隨廣略善說，",
        "from": "T32n1681_003:0768c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依止淨無縛，　　諸依止中勝。",
        "from": "T32n1681_003:0769a01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無救者為救，　　無歸者作歸，",
        "from": "T32n1681_003:0769a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸無趣向者，　　佛為作趣向。",
        "from": "T32n1681_003:0769a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已渡生死海，　　遍入諸境界，",
        "from": "T32n1681_003:0769a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所應現化處，　　一切皆能到。",
        "from": "T32n1681_003:0769a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸墮惡趣者，　　佛常知常思，",
        "from": "T32n1681_003:0769a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以善方便，　　而普為救度。",
        "from": "T32n1681_003:0769a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無貪及無瞋，　　無癡善根具，",
        "from": "T32n1681_003:0769a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大身常住身，　　悲愍利益者。",
        "from": "T32n1681_003:0769a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身被釋種服，　　廣作吉祥事，",
        "from": "T32n1681_003:0769a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "希有精進力，　　大智正識者。",
        "from": "T32n1681_003:0769a11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戒忍法真實，　　內心常精進，",
        "from": "T32n1681_003:0769a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣大復甚深，　　具足諸功德，",
        "from": "T32n1681_003:0769a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光明大照耀，　　調伏心潔白，",
        "from": "T32n1681_003:0769a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶湛水澄清，　　善種現圓滿。",
        "from": "T32n1681_003:0769a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間清淨眼，　　青優鉢羅華，",
        "from": "T32n1681_003:0769a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣大而殊妙，　　周遍十方界，",
        "from": "T32n1681_003:0769a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寂止性潤澤，　　一切智妙月，",
        "from": "T32n1681_003:0769a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛德廣無邊，　　是故我稱讚。",
        "from": "T32n1681_003:0769a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛吉祥德讚卷下",
        "from": "T32n1681_003:0769a20_100",
        "context_with_next_sentence": false
    }
]