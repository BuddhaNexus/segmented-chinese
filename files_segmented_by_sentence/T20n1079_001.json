[
    {
        "source_sentence": "七俱胝獨部法",
        "from": "T20n1079_001:0187a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏沙門無畏譯",
        "from": "T20n1079_001:0187a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總攝二十五部大漫荼羅印。以二手無名指",
        "from": "T20n1079_001:0187a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小指。",
        "from": "T20n1079_001:0187a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相叉於內。",
        "from": "T20n1079_001:0187a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指直竪相拄。",
        "from": "T20n1079_001:0187a25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指附二。",
        "from": "T20n1079_001:0187a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中指第一節。",
        "from": "T20n1079_001:0187a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大母指捻左右手無名指中節。",
        "from": "T20n1079_001:0187a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有召請。",
        "from": "T20n1079_001:0187a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指來去。",
        "from": "T20n1079_001:0187a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言此呪及印能滅十惡五逆一切重罪。",
        "from": "T20n1079_001:0187a28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就一切白法功德。",
        "from": "T20n1079_001:0187a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作此法不簡在家出家。",
        "from": "T20n1079_001:0187a29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若在家人飲酒食肉有妻子不簡淨穢。",
        "from": "T20n1079_001:0187b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但依我法無不成就。",
        "from": "T20n1079_001:0187b01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一壇法。",
        "from": "T20n1079_001:0187b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二念誦法。",
        "from": "T20n1079_001:0187b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三成驗法。",
        "from": "T20n1079_001:0187b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四廣明自在法。",
        "from": "T20n1079_001:0187b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五天得大神足。",
        "from": "T20n1079_001:0187b03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "1 壇法第一壇法佛言若求成就先作壇法不同諸部廣修供養。",
        "from": "T20n1079_001:0187b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掘地作壇香泥塗之所建立。",
        "from": "T20n1079_001:0187b05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但以一新淨境未曾用者。",
        "from": "T20n1079_001:0187b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於佛像前月十五日夜。",
        "from": "T20n1079_001:0187b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨力供養。",
        "from": "T20n1079_001:0187b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒安悉香及清淨水。",
        "from": "T20n1079_001:0187b07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先當靜心無所思惟。",
        "from": "T20n1079_001:0187b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後結印誦呪。",
        "from": "T20n1079_001:0187b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪鏡一百八遍。",
        "from": "T20n1079_001:0187b08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以囊匣盛鏡常持相隨。",
        "from": "T20n1079_001:0187b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲誦但將此鏡。",
        "from": "T20n1079_001:0187b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置於面前結印誦呪。",
        "from": "T20n1079_001:0187b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依鏡為壇即得成就。",
        "from": "T20n1079_001:0187b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "2 念誦法第二念誦法佛言欲持此法於十五日夜。",
        "from": "T20n1079_001:0187b11_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清淨沐浴著新淨衣。",
        "from": "T20n1079_001:0187b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面向東方半跏正坐。",
        "from": "T20n1079_001:0187b12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置鏡在前。",
        "from": "T20n1079_001:0187b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨力香華清淨水諸物。",
        "from": "T20n1079_001:0187b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先當靜心絕思。",
        "from": "T20n1079_001:0187b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後結印印於心上。",
        "from": "T20n1079_001:0187b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此呪一百八遍。",
        "from": "T20n1079_001:0187b14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持此呪時。",
        "from": "T20n1079_001:0187b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能使短命者長命。",
        "from": "T20n1079_001:0187b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加摩羅病尚得除差。",
        "from": "T20n1079_001:0187b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況餘病。",
        "from": "T20n1079_001:0187b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不消差無有是處。",
        "from": "T20n1079_001:0187b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每月一日八日。",
        "from": "T20n1079_001:0187b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十四日十五日。",
        "from": "T20n1079_001:0187b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十八日二十三日。",
        "from": "T20n1079_001:0187b17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十四日二十八日。",
        "from": "T20n1079_001:0187b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十九日三十日。",
        "from": "T20n1079_001:0187b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是十日每於平明。",
        "from": "T20n1079_001:0187b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清水嗽口面向東方。",
        "from": "T20n1079_001:0187b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此呪一百八遍然後飲食。",
        "from": "T20n1079_001:0187b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縱有妻子不斷辛肉亦當成驗。",
        "from": "T20n1079_001:0187b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "3 成驗法第三成驗法每月十八日以境及以結印念誦。",
        "from": "T20n1079_001:0187b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除十齊日不須對鏡及以結印。",
        "from": "T20n1079_001:0187b23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但於平朝未辛肉時。",
        "from": "T20n1079_001:0187b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦持此呪一百八遍已。",
        "from": "T20n1079_001:0187b24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是不絕四十九日。",
        "from": "T20n1079_001:0187b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每有善惡吉祥災變。",
        "from": "T20n1079_001:0187b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "準提菩薩令二聖者隨其人。",
        "from": "T20n1079_001:0187b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有善惡心之事呪七遍。",
        "from": "T20n1079_001:0187b26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洗面灑身心想作歡喜想。",
        "from": "T20n1079_001:0187b27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令國王大臣長者生恭敬心。",
        "from": "T20n1079_001:0187b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是即歡喜如臣敬君。",
        "from": "T20n1079_001:0187b28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如父愛子心。",
        "from": "T20n1079_001:0187b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨其所欲。",
        "from": "T20n1079_001:0187b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至身命皆無所惜悉得成就。",
        "from": "T20n1079_001:0187b29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有短命長病眾生。",
        "from": "T20n1079_001:0187c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月十五日夜曉燒安悉香。",
        "from": "T20n1079_001:0187c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言一千遍。",
        "from": "T20n1079_001:0187c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魔鬼失心狂走。",
        "from": "T20n1079_001:0187c02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "狐𤢌惡鬼皆於鏡中見形。",
        "from": "T20n1079_001:0187c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教殺即殺教放即放。",
        "from": "T20n1079_001:0187c03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更不再來增壽無量。",
        "from": "T20n1079_001:0187c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人無福無相。",
        "from": "T20n1079_001:0187c04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求官無遷貧苦所逼。",
        "from": "T20n1079_001:0187c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每十齊日常誦此呪。",
        "from": "T20n1079_001:0187c05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令現世得轉輪王位。",
        "from": "T20n1079_001:0187c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求官克當稱遂。",
        "from": "T20n1079_001:0187c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲行及欲作事。",
        "from": "T20n1079_001:0187c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先當思惟準提聖者。",
        "from": "T20n1079_001:0187c07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心念此呪具滿七遍。",
        "from": "T20n1079_001:0187c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若可營為端身少頃。",
        "from": "T20n1079_001:0187c08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其身自然搖動。",
        "from": "T20n1079_001:0187c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即知克遂及以吉慶。",
        "from": "T20n1079_001:0187c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若身剛強向前向後。",
        "from": "T20n1079_001:0187c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即知不遂必有災難。",
        "from": "T20n1079_001:0187c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲召四天王梵王帝釋二十八部天波旬等。",
        "from": "T20n1079_001:0187c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對鏡結印頭指來去。",
        "from": "T20n1079_001:0187c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於清淨處高聲誦二十一遍。",
        "from": "T20n1079_001:0187c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢聖閻羅王鬼眾。",
        "from": "T20n1079_001:0187c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨情必至不敢前却。",
        "from": "T20n1079_001:0187c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有驅使隨情皆得。",
        "from": "T20n1079_001:0187c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲知此法成與不成。",
        "from": "T20n1079_001:0187c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具滿七遍依法誦呪。",
        "from": "T20n1079_001:0187c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夢中見佛菩薩及以華菓。",
        "from": "T20n1079_001:0187c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口吐黑物復吃白物。",
        "from": "T20n1079_001:0187c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即知成就。",
        "from": "T20n1079_001:0187c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "4 廣明自在法第四廣明自在法佛言此陀羅尼有大勢力。",
        "from": "T20n1079_001:0187c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "移須彌山及大海水。",
        "from": "T20n1079_001:0187c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪乾枯木能生華菓。",
        "from": "T20n1079_001:0187c18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若常誦持水不能溺火不能燒。",
        "from": "T20n1079_001:0187c19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒藥力兵怨病皆不能害。",
        "from": "T20n1079_001:0187c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若知地中有寶藏物。",
        "from": "T20n1079_001:0187c20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結印誦呪具滿七遍。",
        "from": "T20n1079_001:0187c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地中伏藏自然涌出。",
        "from": "T20n1079_001:0187c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意所欲皆得充足。",
        "from": "T20n1079_001:0187c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "軍陳賊難結印印指之。",
        "from": "T20n1079_001:0187c22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨所皆悉退散。",
        "from": "T20n1079_001:0187c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有卒為鬼神傷死。",
        "from": "T20n1079_001:0187c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結印誦呪七遍。",
        "from": "T20n1079_001:0187c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印印心卒死重病即令却活。",
        "from": "T20n1079_001:0187c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宅舍不安鬼神作禍。",
        "from": "T20n1079_001:0187c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪土四塊鎮之即去。",
        "from": "T20n1079_001:0187c25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若又有人六親不和人不愛念。",
        "from": "T20n1079_001:0187c26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依法誦呪所向和合。",
        "from": "T20n1079_001:0187c26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞名見身皆生歡喜欲不捨離。",
        "from": "T20n1079_001:0187c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猛火之中以印指火即消滅。",
        "from": "T20n1079_001:0187c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印指水波浪皆靜。",
        "from": "T20n1079_001:0187c28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡所求願無不稱遂。",
        "from": "T20n1079_001:0187c28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況更能結印齊戒依法持誦。",
        "from": "T20n1079_001:0187c29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不轉肉身往四方淨土。",
        "from": "T20n1079_001:0188a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "5 天得大神足第五天得大神足若求長生及勅諸仙取其仙藥。",
        "from": "T20n1079_001:0188a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於古塔前及深山中。",
        "from": "T20n1079_001:0188a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或淨房內依鏡為壇。",
        "from": "T20n1079_001:0188a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具滿七日或三七日。",
        "from": "T20n1079_001:0188a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒青蓮華和安悉香。",
        "from": "T20n1079_001:0188a04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於初夜分誦呪一百八遍即當睡眠。",
        "from": "T20n1079_001:0188a05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於夢中食仙藥或攝方法。",
        "from": "T20n1079_001:0188a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於鏡前面有五色光光中有藥隨意食之即成仙道。",
        "from": "T20n1079_001:0188a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸餘功力百千萬億。",
        "from": "T20n1079_001:0188a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說不能盡。",
        "from": "T20n1079_001:0188a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "準提別法準提別法先以左手頭指無名指小指三指。",
        "from": "T20n1079_001:0188a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在於掌中。",
        "from": "T20n1079_001:0188a10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大母指。",
        "from": "T20n1079_001:0188a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捻頭指無名指小指甲上。",
        "from": "T20n1079_001:0188a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手亦然。",
        "from": "T20n1079_001:0188a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相合二中指頭相拄誦呪。",
        "from": "T20n1079_001:0188a12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結界印。",
        "from": "T20n1079_001:0188a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左右二手。",
        "from": "T20n1079_001:0188a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中指無名指向內相叉。",
        "from": "T20n1079_001:0188a13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二小指直竪。",
        "from": "T20n1079_001:0188a13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指相拄。",
        "from": "T20n1079_001:0188a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手大指各捻頭指中節誦呪。",
        "from": "T20n1079_001:0188a14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護身印。",
        "from": "T20n1079_001:0188a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手合掌。",
        "from": "T20n1079_001:0188a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指向外相叉。",
        "from": "T20n1079_001:0188a15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指並捻頭指中節上。",
        "from": "T20n1079_001:0188a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中指已下三指直竪合。",
        "from": "T20n1079_001:0188a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若逢盜賊將印頂戴。",
        "from": "T20n1079_001:0188a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先印左肩次印右。",
        "from": "T20n1079_001:0188a17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印心前印頭下印眉問。",
        "from": "T20n1079_001:0188a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此不敢侵。",
        "from": "T20n1079_001:0188a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "治病印。",
        "from": "T20n1079_001:0188a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指二中指四指。",
        "from": "T20n1079_001:0188a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向內相叉。",
        "from": "T20n1079_001:0188a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二無名指二小指四指。",
        "from": "T20n1079_001:0188a19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向外相拄合掌。",
        "from": "T20n1079_001:0188a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指並捻頭指中節。",
        "from": "T20n1079_001:0188a20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總攝印。",
        "from": "T20n1079_001:0188a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左右小指無名指叉入掌中。",
        "from": "T20n1079_001:0188a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指直竪頭相合。",
        "from": "T20n1079_001:0188a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開著二頭指少屈。",
        "from": "T20n1079_001:0188a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大母指壓無名指。",
        "from": "T20n1079_001:0188a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "召請時頭指來去。",
        "from": "T20n1079_001:0188a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "似華臺坐。",
        "from": "T20n1079_001:0188a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦呪呪七遍。",
        "from": "T20n1079_001:0188a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用印處兩膊咽喉下眉間髮際。",
        "from": "T20n1079_001:0188a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復即此印當心誦呪一百八遍。",
        "from": "T20n1079_001:0188a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破天魔印。",
        "from": "T20n1079_001:0188a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左右手四指。",
        "from": "T20n1079_001:0188a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壓大母指內掌中。",
        "from": "T20n1079_001:0188a26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "急把拳擬之即是。",
        "from": "T20n1079_001:0188a26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有恐怖處。",
        "from": "T20n1079_001:0188a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疑有鬼魅毒龍。",
        "from": "T20n1079_001:0188a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即作此印瞋聲誦呪。",
        "from": "T20n1079_001:0188a27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請鬼神印。",
        "from": "T20n1079_001:0188a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手中指壓大母指按壇。",
        "from": "T20n1079_001:0188a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以上餘指皆舒如牙似。",
        "from": "T20n1079_001:0188a29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微曲頭指小指來去。",
        "from": "T20n1079_001:0188a29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦呪七遍。",
        "from": "T20n1079_001:0188b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三遍一切鬼神即來。",
        "from": "T20n1079_001:0188b01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右七俱胝獨部軌。",
        "from": "T20n1079_001:0188b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔有律師。",
        "from": "T20n1079_001:0188b02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見略題之本。",
        "from": "T20n1079_001:0188b02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言不簡在家出家飲酒食肉有妻子不簡淨穢。",
        "from": "T20n1079_001:0188b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謬讀文以為偽文。",
        "from": "T20n1079_001:0188b04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天明中沙門亮雄者辨其謬。",
        "from": "T20n1079_001:0188b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以具題之本為正本。",
        "from": "T20n1079_001:0188b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其略題之本為草稿未治。",
        "from": "T20n1079_001:0188b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙門慧亮者。",
        "from": "T20n1079_001:0188b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印施具題之本行世。",
        "from": "T20n1079_001:0188b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以予視之非矣。",
        "from": "T20n1079_001:0188b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賴瑜薄草決。",
        "from": "T20n1079_001:0188b07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祕鈔問答等。",
        "from": "T20n1079_001:0188b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "引用此軌。",
        "from": "T20n1079_001:0188b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今歲集洛北清和院。",
        "from": "T20n1079_001:0188b08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洛東智積院。",
        "from": "T20n1079_001:0188b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "河州延命寺等所藏眾本。",
        "from": "T20n1079_001:0188b09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當山愛染院大宣等挍合。",
        "from": "T20n1079_001:0188b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "予亦訂正。",
        "from": "T20n1079_001:0188b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以兩本俱印版。",
        "from": "T20n1079_001:0188b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙音輪下總持院　快道　　誌",
        "from": "T20n1079_001:0188b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一校了　　　　　　　　　　慈順",
        "from": "T20n1079_001:0188b13_0",
        "context_with_next_sentence": true
    }
]