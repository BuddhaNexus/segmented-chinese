[
    {
        "source_sentence": "金剛頂普賢瑜伽大教王經大",
        "from": "T20n1121_001:0521b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樂不空金剛薩埵一切時方成",
        "from": "T20n1121_001:0521b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就儀",
        "from": "T20n1121_001:0521b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稽首禮大密　　從持金剛生",
        "from": "T20n1121_001:0521b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言易成就　　我說念誦儀",
        "from": "T20n1121_001:0521b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密語有先應　　發心盡利樂",
        "from": "T20n1121_001:0521b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無餘有情界　　隨意稱誦此",
        "from": "T20n1121_001:0521b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性成就密言　　應發菩提心",
        "from": "T20n1121_001:0521b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如上名願菩提心。",
        "from": "T20n1121_001:0521b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由纔誦密言　　發菩提心故",
        "from": "T20n1121_001:0521b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彌除一切障　　獲諸樂悅意",
        "from": "T20n1121_001:0521b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不被眾魔阻　　等同正遍知",
        "from": "T20n1121_001:0521b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受諸世間供　　復觀一切法",
        "from": "T20n1121_001:0521b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即徹見本心　　圓白如滿月",
        "from": "T20n1121_001:0521b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五峯素金剛　　踴立月輪面",
        "from": "T20n1121_001:0521b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即觀智金剛　　變成婆伽梵",
        "from": "T20n1121_001:0521b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛薩埵身　　形服如皓月",
        "from": "T20n1121_001:0521b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐白蓮華臺　　五佛以嚴冠",
        "from": "T20n1121_001:0521b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "備飾裝嚴具　　背熾然赤光",
        "from": "T20n1121_001:0521b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依月輪中住　　又以五金剛",
        "from": "T20n1121_001:0521b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "裝嚴心月輪　　如是觀己身",
        "from": "T20n1121_001:0521b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二金剛拳　　左胯右當心",
        "from": "T20n1121_001:0521b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名金剛薩埵　　慢印吽迦羅",
        "from": "T20n1121_001:0521b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧趺押定股　　名普賢結跏",
        "from": "T20n1121_001:0521c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又結妙適悅　　三摩耶密印",
        "from": "T20n1121_001:0521c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸度聚應縛　　力智以為根",
        "from": "T20n1121_001:0521c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪形入彼門　　徐動安應逼",
        "from": "T20n1121_001:0521c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦素囉多　　塞怛鑁密語",
        "from": "T20n1121_001:0521c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此印密言　　聖有入己體",
        "from": "T20n1121_001:0521c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印心額喉頂　　便作三昧拳",
        "from": "T20n1121_001:0521c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪智並入中　　進力屈跓背",
        "from": "T20n1121_001:0521c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦唵嚩日羅　　母瑟智鑁已",
        "from": "T20n1121_001:0521c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "𢫱為二業拳　　誦薩縛悉地",
        "from": "T20n1121_001:0521c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復陳前幔印　　大樂金剛心",
        "from": "T20n1121_001:0521c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密語吽迦羅　　次合金剛掌",
        "from": "T20n1121_001:0521c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忍願如蓮合　　進力安初節",
        "from": "T20n1121_001:0521c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂額右旋置　　而成五佛冠",
        "from": "T20n1121_001:0521c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密語次當演　　頂中遍照佛",
        "from": "T20n1121_001:0521c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當誦唵㒒欠　　阿閦佛髮際",
        "from": "T20n1121_001:0521c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當誦嚩日囉　　薩埵成密語",
        "from": "T20n1121_001:0521c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂右寶生佛　　亦稱嚩日羅",
        "from": "T20n1121_001:0521c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅怛曩成旬　　後無量壽佛",
        "from": "T20n1121_001:0521c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又誦嚩日囉　　達磨密語成",
        "from": "T20n1121_001:0521c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂左誦不空　　成就佛密語",
        "from": "T20n1121_001:0521c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚩日囉羯磨　　分佛身勝印",
        "from": "T20n1121_001:0521c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為二金剛拳　　交臂當於額",
        "from": "T20n1121_001:0521c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分拳至玉杬　　進力節相繞",
        "from": "T20n1121_001:0521c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三繫金剛鬘　　分手兩耳傍",
        "from": "T20n1121_001:0521c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從檀慧歷散　　蘊藉垂繒帶",
        "from": "T20n1121_001:0521c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱誦嚩日囉　　莽攞𤚥鑁已",
        "from": "T20n1121_001:0521c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此印等成　　受具足灌頂",
        "from": "T20n1121_001:0521c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為說聖眾故　　參差掌三拍",
        "from": "T20n1121_001:0521c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦嚩日羅覩　　史也穀密語",
        "from": "T20n1121_001:0522a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由拍金剛掌　　速成薩埵體",
        "from": "T20n1121_001:0522a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當遍安立　　眷屬十六尊",
        "from": "T20n1121_001:0522a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圍繞大安樂　　眼前處於前",
        "from": "T20n1121_001:0522a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手持弓箭　　形服蓮紅色",
        "from": "T20n1121_001:0522a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二金剛拳　　彎弓如射儀",
        "from": "T20n1121_001:0522a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦弱嚩日囉　　爾哩瑟致娑",
        "from": "T20n1121_001:0522a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "也計麼吒句　　右計里枳羅",
        "from": "T20n1121_001:0522a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "交臂金剛拳　　右內把金剛",
        "from": "T20n1121_001:0522a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月色形蓮眼　　誦吽嚩日羅",
        "from": "T20n1121_001:0522a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "計里枳隷吽　　後更持幢幡",
        "from": "T20n1121_001:0522a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "麼迦羅為首　　慧拳竪臂肘",
        "from": "T20n1121_001:0522a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定拳力智水　　形眼類青蓮",
        "from": "T20n1121_001:0522a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鑁嚩曰哩抳　　娑麼囉囉吒",
        "from": "T20n1121_001:0522a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此密語已　　二拳且腰側",
        "from": "T20n1121_001:0522a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向左小低頭　　誦穀嚩日囉",
        "from": "T20n1121_001:0522a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦冥涅嚩哩　　怛覽密語已",
        "from": "T20n1121_001:0522a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛幔居左　　意體蓮華黃",
        "from": "T20n1121_001:0522a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前右隅時春　　二手捧華盤",
        "from": "T20n1121_001:0522a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形服蓮華白　　二拳仰上散",
        "from": "T20n1121_001:0522a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名華滿空界　　當誦唵末度",
        "from": "T20n1121_001:0522a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚩日囉具唵　　後右隅時雨",
        "from": "T20n1121_001:0522a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持鑪獻焚香　　身色類懸雲",
        "from": "T20n1121_001:0522a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覆二向下按　　香雲遍氣馥",
        "from": "T20n1121_001:0522a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦彼密語唵　　嚩日囉銘祗",
        "from": "T20n1121_001:0522a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愚魯愚魯已　　後左隅時秋",
        "from": "T20n1121_001:0522a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "赤色持燈燭　　禪智並相逼",
        "from": "T20n1121_001:0522a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛燈周擢　　即誦唵捨羅",
        "from": "T20n1121_001:0522a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚩曰哩揞句　　雨散拭自胸",
        "from": "T20n1121_001:0522a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃成塗香契　　誦唵嚩日羅",
        "from": "T20n1121_001:0522b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勢始隷吽吽　　捧獻塗香鉢",
        "from": "T20n1121_001:0522b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安住前左隅　　冬身蓮衣黃",
        "from": "T20n1121_001:0522b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於外曼荼羅　　四隅當安立",
        "from": "T20n1121_001:0522b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華方名嬉戲　　覆二拳當心",
        "from": "T20n1121_001:0522b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旋誦係囉底　　嚩日囉尾攞",
        "from": "T20n1121_001:0522b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉爾怛羅吒　　揚掌指口旁",
        "from": "T20n1121_001:0522b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "笑誦係羅底　　嚩日囉賀細",
        "from": "T20n1121_001:0522b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賀賀迦羅也　　名笑住雲外",
        "from": "T20n1121_001:0522b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "秋外金剛歌　　禪執梵箜篌",
        "from": "T20n1121_001:0522b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定拳力微屈　　彎臂如箜篌",
        "from": "T20n1121_001:0522b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧拳進亦然　　禪定臂膊間",
        "from": "T20n1121_001:0522b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歌唱係囉底　　嚩日羅儗帝",
        "from": "T20n1121_001:0522b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝帝密語成　　旋舞輪諸度",
        "from": "T20n1121_001:0522b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散拳還成拳　　左右頰轉上",
        "from": "T20n1121_001:0522b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛掌頂散　　誦係囉底嚩",
        "from": "T20n1121_001:0522b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日囉爾哩帝　　吠波吠波已",
        "from": "T20n1121_001:0522b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舞處冬外隅　　如是四供養",
        "from": "T20n1121_001:0522b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並同金色身　　檀慧相鉤結",
        "from": "T20n1121_001:0522b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "交臂申力度　　屈進初如鉤",
        "from": "T20n1121_001:0522b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當誦嚩曰𠻴　　矩勢弱密語",
        "from": "T20n1121_001:0522b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持鉤護前門　　即以進力跓",
        "from": "T20n1121_001:0522b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如環成羅索　　又誦嚩日羅",
        "from": "T20n1121_001:0522b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "播勢吽密語　　執索守右門",
        "from": "T20n1121_001:0522b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力極拘結　　踀即成乃誦",
        "from": "T20n1121_001:0522b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚩日囉商羯　　隷鑁密語已",
        "from": "T20n1121_001:0522b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中踀住後門　　不改前印相",
        "from": "T20n1121_001:0522b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當心微搖動　　名震金剛鈴",
        "from": "T20n1121_001:0522b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚩日囉健致　　穀密語誦已",
        "from": "T20n1121_001:0522b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執磬護左門　　四攝尊形色",
        "from": "T20n1121_001:0522c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應內四泥微　　如上十六尊",
        "from": "T20n1121_001:0522c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成爾妙安客　　美艶備恣態",
        "from": "T20n1121_001:0522c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處蓮普賢跏　　皆依月輪住",
        "from": "T20n1121_001:0522c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切莊嚴具　　而用授飾身",
        "from": "T20n1121_001:0522c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並載五佛冠　　裳蓮隨體色",
        "from": "T20n1121_001:0522c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆已染悅目　　瞻視持金剛",
        "from": "T20n1121_001:0522c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是安立已　　成□曼荼羅",
        "from": "T20n1121_001:0522c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼中如輪形　　八柱而嚴飾",
        "from": "T20n1121_001:0522c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛勝柱間　　月輪而莊嚴",
        "from": "T20n1121_001:0522c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼外曼荼羅　　四方應四門",
        "from": "T20n1121_001:0522c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四外門相應　　門戶交合聞",
        "from": "T20n1121_001:0522c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "填飾金剛寶　　蔓荼羅諸角",
        "from": "T20n1121_001:0522c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "半月而莊嚴　　瓔珞鈴繒幡",
        "from": "T20n1121_001:0522c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處處而懸烈　　微風吹搖激",
        "from": "T20n1121_001:0522c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出和雅暢音　　周圍金剛界",
        "from": "T20n1121_001:0522c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空為宮殿　　蔓荼羅處中",
        "from": "T20n1121_001:0522c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是諦思惟　　一切時方住",
        "from": "T20n1121_001:0522c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自心所安立　　自身為本尊",
        "from": "T20n1121_001:0522c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等供養　　圍繞獻己身",
        "from": "T20n1121_001:0522c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行住常隨逐　　所見一切色",
        "from": "T20n1121_001:0522c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀彼等皆空　　復思本尊身",
        "from": "T20n1121_001:0522c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦唵迦路目　　佉薩嚩達磨",
        "from": "T20n1121_001:0522c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拏摩爾努半　　曩怛襪密語",
        "from": "T20n1121_001:0522c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或草稱唵字　　自勝解思惟",
        "from": "T20n1121_001:0522c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗香燈焚香　　華鬘及飲食",
        "from": "T20n1121_001:0522c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣服寶幡蓋　　扇拂幡鈴現",
        "from": "T20n1121_001:0522c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歌舞勝宮殿　　遍諸虛空界",
        "from": "T20n1121_001:0522c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今皆變化　　一切勝供養",
        "from": "T20n1121_001:0522c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾聖受用之　　即誦四讚王",
        "from": "T20n1121_001:0523a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "射抱幢幔印　　輪唱金剛欲",
        "from": "T20n1121_001:0523a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薩嚩努邏誐　　蘇佉薩怛莾",
        "from": "T20n1121_001:0523a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩婆放前儀　　怛鑁嚩日囉",
        "from": "T20n1121_001:0523a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薩埵跛囉麼　　索囉跢妙陀",
        "from": "T20n1121_001:0523a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婆嚩銘磨訶　　蘇佉爾哩住",
        "from": "T20n1121_001:0523a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縒囉也那幢　　鉢囉底幡𭀍",
        "from": "T20n1121_001:0523a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉地者攞具　　鉢囉拏多慢",
        "from": "T20n1121_001:0523a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此金剛歌　　大樂隨愛樂",
        "from": "T20n1121_001:0523a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "適悅為堅固　　皆得如意成",
        "from": "T20n1121_001:0523a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以妙適悅言　　隨分誦婀字",
        "from": "T20n1121_001:0523a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸法本不生　　當知如是義",
        "from": "T20n1121_001:0523a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大樂體不空　　常誦吽迦羅",
        "from": "T20n1121_001:0523a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛孕法印　　隨意安密言",
        "from": "T20n1121_001:0523a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吽迦囉或餘　　每誦發光焰",
        "from": "T20n1121_001:0523a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍出毛孔中　　如素頗胝迦",
        "from": "T20n1121_001:0523a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日現皓月光　　身口心金剛",
        "from": "T20n1121_001:0523a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆作是諦觀　　成就諸教法",
        "from": "T20n1121_001:0523a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛語念誦　　脣齒合無聲",
        "from": "T20n1121_001:0523a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微動舌念誦　　作如是修習",
        "from": "T20n1121_001:0523a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至滿一月　　或二或六月",
        "from": "T20n1121_001:0523a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所觀相皆現　　自獲薩埵身",
        "from": "T20n1121_001:0523a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安聖眾身前　　以唵迦𡀔獻",
        "from": "T20n1121_001:0523a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又陳八供養　　歌誦四讚王",
        "from": "T20n1121_001:0523a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛嚩並合　　忍願度如幢",
        "from": "T20n1121_001:0523a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦唵訖哩姤　　嚩薩嚩薩埵",
        "from": "T20n1121_001:0523a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捺多也他努　　誐薩縒持挽",
        "from": "T20n1121_001:0523a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沒馱尾灑焰　　補曩邏誐麼",
        "from": "T20n1121_001:0523a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那色覩唵嚩　　日囉薩埵目",
        "from": "T20n1121_001:0523a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此密言已　　以印頂上散",
        "from": "T20n1121_001:0523b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拳送諸聖眾　　從頂而流出",
        "from": "T20n1121_001:0523b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲一切時　　依此儀作意",
        "from": "T20n1121_001:0523b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見一切色等　　皆想獻本尊",
        "from": "T20n1121_001:0523b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀身色空相　　即是我本尊",
        "from": "T20n1121_001:0523b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是思惟　　即名勝解眼",
        "from": "T20n1121_001:0523b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所覩彼彼色　　皆空觀又觀",
        "from": "T20n1121_001:0523b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛薩埵身　　此乃已與尊",
        "from": "T20n1121_001:0523b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知皆本空　　即同為一體",
        "from": "T20n1121_001:0523b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住如是瑜伽　　行住若坐臥",
        "from": "T20n1121_001:0523b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意念誦光　　十萬遍終目",
        "from": "T20n1121_001:0523b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡夜不寢息　　念誦至晨朝",
        "from": "T20n1121_001:0523b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又於一切時　　用前妙適悅",
        "from": "T20n1121_001:0523b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三麼耶及鬘　　各誦本密語",
        "from": "T20n1121_001:0523b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自身成本尊　　心安繫瑜伽",
        "from": "T20n1121_001:0523b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後當於四時　　道場中念誦",
        "from": "T20n1121_001:0523b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共滿十萬遍　　乃至末後夜",
        "from": "T20n1121_001:0523b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前不寢息　　至明念誦之",
        "from": "T20n1121_001:0523b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不久當成就　　本尊體佛體",
        "from": "T20n1121_001:0523b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剋獲尚不難　　何況諸悉地",
        "from": "T20n1121_001:0523b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善賢阿闍梨　　集此微妙義",
        "from": "T20n1121_001:0523b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我略說大經　　纘集極祕旨",
        "from": "T20n1121_001:0523b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不入曼荼羅　　擅用結罪垢",
        "from": "T20n1121_001:0523b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸教所說　　犯大重罪障",
        "from": "T20n1121_001:0523b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無能除滅者　　結金剛薩埵",
        "from": "T20n1121_001:0523b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大印盡能滅　　修此三昧者",
        "from": "T20n1121_001:0523b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "近意作諸法　　一切教不說",
        "from": "T20n1121_001:0523b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆隨意成就　　纔結大印時",
        "from": "T20n1121_001:0523b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來咸敬禮　　密語者即為",
        "from": "T20n1121_001:0523b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛之至宰　　大印威德力",
        "from": "T20n1121_001:0523c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寂勝無倫疋　　不作諸儀軌",
        "from": "T20n1121_001:0523c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯結一大印　　無能為障者",
        "from": "T20n1121_001:0523c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速疾得成就　　護本三麼耶",
        "from": "T20n1121_001:0523c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不令身心苦　　受樂取成就",
        "from": "T20n1121_001:0523c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普願諸有情　　信受入此法",
        "from": "T20n1121_001:0523c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆速成就　　金剛薩埵身",
        "from": "T20n1121_001:0523c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍照堅固體　　及諸隨願身",
        "from": "T20n1121_001:0523c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普賢瑜伽經大樂金剛薩埵成就儀軌大歷三年八月十四日西京青龍寺僧願力書寫梵漢字記應德元年九月二十八日辰剋於桂林房以前唐院唐本傳寫了件唐本千手儀軌在諦耳良　記之",
        "from": "T20n1121_001:0523c14_100",
        "context_with_next_sentence": false
    }
]