[
    {
        "source_sentence": "佛說七女觀經",
        "from": "T85n2913_001:1459a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞如是。一時佛說七女觀經。昔冥緣王有七",
        "from": "T85n2913_001:1459a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "女覺世非常。不著繒綵衣服。不著指鐶臂釧。",
        "from": "T85n2913_001:1459a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不捉香油塗身。",
        "from": "T85n2913_001:1459a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歲三長齋月六齋。",
        "from": "T85n2913_001:1459a17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姊妹相將詣父王殿前。",
        "from": "T85n2913_001:1459a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稽首白王。",
        "from": "T85n2913_001:1459a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "女等前身積罪無福。",
        "from": "T85n2913_001:1459a18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今值女身處。",
        "from": "T85n2913_001:1459a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其深宮眼無所見。",
        "from": "T85n2913_001:1459a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求王放女出城遊觀。",
        "from": "T85n2913_001:1459a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王告女言。",
        "from": "T85n2913_001:1459a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我作天下之文萬基之主。",
        "from": "T85n2913_001:1459a20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有苑園浴地華臺樓觀。",
        "from": "T85n2913_001:1459a21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦可觀之。",
        "from": "T85n2913_001:1459a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何用出城觀乎。",
        "from": "T85n2913_001:1459a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "女對王曰。",
        "from": "T85n2913_001:1459a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖有苑園浴池華臺樓觀。",
        "from": "T85n2913_001:1459a22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可悅樂耳。",
        "from": "T85n2913_001:1459a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非女遊觀之處。",
        "from": "T85n2913_001:1459a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王即聽之。",
        "from": "T85n2913_001:1459a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出城東門。",
        "from": "T85n2913_001:1459a23_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到其塚間。",
        "from": "T85n2913_001:1459a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見一死屍。",
        "from": "T85n2913_001:1459a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "膖脹爛臭。",
        "from": "T85n2913_001:1459a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "狐狼踐蹹。",
        "from": "T85n2913_001:1459a24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形骸異處。",
        "from": "T85n2913_001:1459a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姊妹相將。",
        "from": "T85n2913_001:1459a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遶屍三匝。",
        "from": "T85n2913_001:1459a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "涕零號泣。",
        "from": "T85n2913_001:1459a25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等女身會當如是。",
        "from": "T85n2913_001:1459a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姊妹七人各說一偈言。",
        "from": "T85n2913_001:1459a26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一女說偈言。",
        "from": "T85n2913_001:1459a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□重其身　　如似好寶珠",
        "from": "T85n2913_001:1459a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "企視鵝容步　　姿則欲入居",
        "from": "T85n2913_001:1459a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□死棄塚間　　禽獸競共疽",
        "from": "T85n2913_001:1459b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切皆如是　　何用於世殊",
        "from": "T85n2913_001:1459b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二女說偈言。",
        "from": "T85n2913_001:1459b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如一身居　　人去宅無光",
        "from": "T85n2913_001:1459b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身死如神誓　　乃知此非常",
        "from": "T85n2913_001:1459b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今覩死尸觀　　忽然遇犲狼",
        "from": "T85n2913_001:1459b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切皆如是　　安得有久長",
        "from": "T85n2913_001:1459b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三女說偈言。",
        "from": "T85n2913_001:1459b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如馬駕車　　車破而馬去",
        "from": "T85n2913_001:1459b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四大解散時　　魂神依何住",
        "from": "T85n2913_001:1459b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初集四大時　　來從何所處",
        "from": "T85n2913_001:1459b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃知此非常　　唯福可恃怙",
        "from": "T85n2913_001:1459b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四女說偈言。",
        "from": "T85n2913_001:1459b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如好城墎　　眾人樂安居",
        "from": "T85n2913_001:1459b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求未厭足　　何其忽空虛",
        "from": "T85n2913_001:1459b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人命如草木　　春長冬以枯",
        "from": "T85n2913_001:1459b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯有修福德　　智慧隨身軀",
        "from": "T85n2913_001:1459b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五女說偈言。",
        "from": "T85n2913_001:1459b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人之不修福　　譬如乘船渡",
        "from": "T85n2913_001:1459b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四大乖離時　　如船捨岸去",
        "from": "T85n2913_001:1459b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地定風不動　　火微水還故",
        "from": "T85n2913_001:1459b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身非神常宅　　安得久長處",
        "from": "T85n2913_001:1459b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六女說偈言。",
        "from": "T85n2913_001:1459b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如人好莊嚴　　形著諸繒綵",
        "from": "T85n2913_001:1459b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自縱取時榮　　故使欲人待",
        "from": "T85n2913_001:1459b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "侹然不動搖　　其神安所在",
        "from": "T85n2913_001:1459b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀世塚間屍　　將來宜應改",
        "from": "T85n2913_001:1459b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第七女說偈言。",
        "from": "T85n2913_001:1459b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如雀在瓶中　　羅縠覆其口",
        "from": "T85n2913_001:1459b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縠穿雀飛去　　神明隨所受",
        "from": "T85n2913_001:1459c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四大和合時　　如雀瓶中有",
        "from": "T85n2913_001:1459c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯有修善福　　智慧隨身首",
        "from": "T85n2913_001:1459c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四忉利天王從天來下。",
        "from": "T85n2913_001:1459c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到諸女前。",
        "from": "T85n2913_001:1459c04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問女言。",
        "from": "T85n2913_001:1459c04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等何所求欲使汝得之。",
        "from": "T85n2913_001:1459c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸女良久不答。",
        "from": "T85n2913_001:1459c05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復問女言。",
        "from": "T85n2913_001:1459c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等何所求欲使汝得之。",
        "from": "T85n2913_001:1459c06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一女言乞願無根無鬚無枝無葉。",
        "from": "T85n2913_001:1459c07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其中生是則為樂。",
        "from": "T85n2913_001:1459c07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一女復願。",
        "from": "T85n2913_001:1459c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常無五欲。",
        "from": "T85n2913_001:1459c08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常處寂靜。",
        "from": "T85n2913_001:1459c08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耳飡法音。",
        "from": "T85n2913_001:1459c08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目覩聖教。",
        "from": "T85n2913_001:1459c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是則為樂。",
        "from": "T85n2913_001:1459c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一女復願。",
        "from": "T85n2913_001:1459c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幽幽冥冥猶如虛空。",
        "from": "T85n2913_001:1459c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "登願玄覺。",
        "from": "T85n2913_001:1459c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常覩聖容。",
        "from": "T85n2913_001:1459c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是則為樂。",
        "from": "T85n2913_001:1459c10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天帝語諸女言。",
        "from": "T85n2913_001:1459c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "止止。",
        "from": "T85n2913_001:1459c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝之所願無想。",
        "from": "T85n2913_001:1459c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾不能得日月中王使汝得之。",
        "from": "T85n2913_001:1459c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是七女皆得道果。",
        "from": "T85n2913_001:1459c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說七女觀經",
        "from": "T85n2913_001:1459c13_100",
        "context_with_next_sentence": false
    }
]