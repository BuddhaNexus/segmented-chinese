[
    {
        "source_sentence": "佛說大乘八大曼拏羅經",
        "from": "T20n1168A_001:0676a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試光祿卿",
        "from": "T20n1168A_001:0676a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明教大師臣法賢奉　詔譯",
        "from": "T20n1168A_001:0676a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T20n1168A_001:0676a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在補陀落迦山聖觀自在菩薩宮中。",
        "from": "T20n1168A_001:0676a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有無數百千俱胝那由他菩薩恭敬圍繞。",
        "from": "T20n1168A_001:0676a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時會中有一菩薩名寶藏月光。",
        "from": "T20n1168A_001:0676a29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即從座起偏袒右肩。",
        "from": "T20n1168A_001:0676b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右膝著地合掌向佛。",
        "from": "T20n1168A_001:0676b01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而白佛言世尊如來應供正等覺。",
        "from": "T20n1168A_001:0676b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今有所問願佛慈愍斷我疑惑。",
        "from": "T20n1168A_001:0676b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告寶藏月光菩薩摩訶薩言。",
        "from": "T20n1168A_001:0676b04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今問我樂聞之法。",
        "from": "T20n1168A_001:0676b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是義云何當為汝說。",
        "from": "T20n1168A_001:0676b05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時寶藏月光菩薩摩訶薩。",
        "from": "T20n1168A_001:0676b06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛語已而白佛言。",
        "from": "T20n1168A_001:0676b06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊若有善男子善女人。",
        "from": "T20n1168A_001:0676b07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲作八大曼拏羅。",
        "from": "T20n1168A_001:0676b07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何受持恭敬供養。",
        "from": "T20n1168A_001:0676b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言善哉善哉。",
        "from": "T20n1168A_001:0676b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人。",
        "from": "T20n1168A_001:0676b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此八大曼拏羅。",
        "from": "T20n1168A_001:0676b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依法受持恭敬供養。",
        "from": "T20n1168A_001:0676b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大功德出生智慧利益無量。",
        "from": "T20n1168A_001:0676b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至三際眾生。",
        "from": "T20n1168A_001:0676b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆得成就無等正智。",
        "from": "T20n1168A_001:0676b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子汝今諦聽。",
        "from": "T20n1168A_001:0676b11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當為汝說此八大曼拏羅。",
        "from": "T20n1168A_001:0676b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具八大菩薩根本心大明。",
        "from": "T20n1168A_001:0676b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若常持誦恭敬供養。",
        "from": "T20n1168A_001:0676b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有五逆重罪。",
        "from": "T20n1168A_001:0676b13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸惡悉皆除滅。",
        "from": "T20n1168A_001:0676b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而能成就無上勝義。",
        "from": "T20n1168A_001:0676b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大明曰。",
        "from": "T20n1168A_001:0676b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引摩賀引尾囉娑嚩二合引賀引一此是佛心大明。",
        "from": "T20n1168A_001:0676b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安曼拏羅中心。",
        "from": "T20n1168A_001:0676b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此大明獻佛供養。",
        "from": "T20n1168A_001:0676b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次聖觀自在菩薩心大明曰。",
        "from": "T20n1168A_001:0676b19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引紇哩二合賀鉢捺摩二合室哩二合曳娑嚩二合引賀引一慈氏菩薩心大明曰。",
        "from": "T20n1168A_001:0676b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昧賀引囉尼娑嚩二合引賀引一虛空藏菩薩心大明曰。",
        "from": "T20n1168A_001:0676b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿誐哩婆二合引野娑嚩二合引賀引一普賢菩薩心大明曰。",
        "from": "T20n1168A_001:0676b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莎紇哩二合惹野娑嚩二合引賀引一金剛手菩薩心大明曰。",
        "from": "T20n1168A_001:0676b28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱尾囉娑嚩二合引賀引一曼殊室哩菩薩心大明曰。",
        "from": "T20n1168A_001:0676c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "室哩二合引暗囉伽娑嚩二合引賀引一除蓋障菩薩心大明曰。",
        "from": "T20n1168A_001:0676c03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儞娑囉嚩娑嚩二合引賀引一地藏菩薩心大明曰。",
        "from": "T20n1168A_001:0676c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "叱賀引囉娑嚩二合引賀引一如是八大菩薩根本心大明。",
        "from": "T20n1168A_001:0676c07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如來曼拏羅。",
        "from": "T20n1168A_001:0676c07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人。",
        "from": "T20n1168A_001:0676c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此曼拏羅依法受持。",
        "from": "T20n1168A_001:0676c08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "志心持誦根本大明。",
        "from": "T20n1168A_001:0676c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼人速得成就阿耨多羅三藐三菩提。",
        "from": "T20n1168A_001:0676c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說大乘八大曼拏羅經",
        "from": "T20n1168A_001:0676c11_100",
        "context_with_next_sentence": false
    }
]