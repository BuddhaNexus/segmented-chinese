[
    {
        "source_sentence": "佛說大白傘蓋總持陀羅尼經",
        "from": "T19n0977_001:0404a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元天竺俊辯大師唧𭊁銘得哩連",
        "from": "T19n0977_001:0404a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得囉磨寧及譯主僧真智等譯",
        "from": "T19n0977_001:0404a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮一切最妙上師。",
        "from": "T19n0977_001:0404a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫欲修習白傘蓋佛母者。",
        "from": "T19n0977_001:0404a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寂靜室內。",
        "from": "T19n0977_001:0404a08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於軟穩氈上坐已。",
        "from": "T19n0977_001:0404a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然發願云。",
        "from": "T19n0977_001:0404a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為六道一切有情。",
        "from": "T19n0977_001:0404a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於輪迴中令得解脫故。",
        "from": "T19n0977_001:0404a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願我成究竟正覺。",
        "from": "T19n0977_001:0404a10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而發願已。",
        "from": "T19n0977_001:0404a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面前空中想白傘蓋佛會。",
        "from": "T19n0977_001:0404a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼等處以真實心念三歸依已。",
        "from": "T19n0977_001:0404a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛會消融為光。",
        "from": "T19n0977_001:0404a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "融入自身自身成光。",
        "from": "T19n0977_001:0404a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後念莎末斡呪。",
        "from": "T19n0977_001:0404a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想一切皆空。",
        "from": "T19n0977_001:0404a13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其空中華月輪上。",
        "from": "T19n0977_001:0404a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想白色唵字。",
        "from": "T19n0977_001:0404a14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵字放光。",
        "from": "T19n0977_001:0404a14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其光復迴。",
        "from": "T19n0977_001:0404a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "字種變成白傘金柄。",
        "from": "T19n0977_001:0404a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "柄上嚴唵字。",
        "from": "T19n0977_001:0404a15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其字放光復回。",
        "from": "T19n0977_001:0404a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "字種變成白傘蓋佛母。",
        "from": "T19n0977_001:0404a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一面二臂具三目。",
        "from": "T19n0977_001:0404a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛跏趺而坐。",
        "from": "T19n0977_001:0404a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手作無怖畏印。",
        "from": "T19n0977_001:0404a17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手執白傘當胸。",
        "from": "T19n0977_001:0404a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚴飾種種瓔珞。",
        "from": "T19n0977_001:0404a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身色潔白如雪山上日光明照。",
        "from": "T19n0977_001:0404a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具喜悅相顯無自性。",
        "from": "T19n0977_001:0404a19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應觀如鏡中像然。",
        "from": "T19n0977_001:0404a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲誦呪時。",
        "from": "T19n0977_001:0404a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自己心中蓮華日輪上。",
        "from": "T19n0977_001:0404a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵字周圍繞心。",
        "from": "T19n0977_001:0404a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪及長短總持等於彼。",
        "from": "T19n0977_001:0404a21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放光遣除自他一切罪障。",
        "from": "T19n0977_001:0404a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及間斷等想已然後讀誦。",
        "from": "T19n0977_001:0404a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若疲倦時欲奉施食。",
        "from": "T19n0977_001:0404a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則面前置施食。",
        "from": "T19n0977_001:0404a23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念唵啞吽三字呪攝受。",
        "from": "T19n0977_001:0404a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變成甘露。",
        "from": "T19n0977_001:0404a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面前空中召請白傘蓋佛母為首。",
        "from": "T19n0977_001:0404a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并二十二山塚所居陰母。",
        "from": "T19n0977_001:0404a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及七種佛并十方正覺。",
        "from": "T19n0977_001:0404a26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三種具美淨梵帝釋伴繞等已。",
        "from": "T19n0977_001:0404a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想舌變成金剛光筒。",
        "from": "T19n0977_001:0404a27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦奉食呪曰。",
        "from": "T19n0977_001:0404a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩㗚斡二合怛達遏哆烏室禰折席𭊁怛末嘚哩吽發怛𠵱擔末哩渴渴渴兮渴兮誦三遍或五遍已。",
        "from": "T19n0977_001:0404b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然誦讚歎禱祝。",
        "from": "T19n0977_001:0404b02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求索願事等畢。",
        "from": "T19n0977_001:0404b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉送佛會。",
        "from": "T19n0977_001:0404b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其施食棄於淨處。",
        "from": "T19n0977_001:0404b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "回向善根矣。",
        "from": "T19n0977_001:0404b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "啞呤耶怛達遏哆烏室禰折西𭊁引怛末嘚哩捺麻啞末囉唧怛𭊁囉禰梵語聖一切如來頂髻中出白傘蓋佛母餘無能敵總持敬禮最上三寶華言。",
        "from": "T19n0977_001:0404b08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T19n0977_001:0404b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時出有壞。",
        "from": "T19n0977_001:0404b09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住三十三天善法妙好諸天所居之處。",
        "from": "T19n0977_001:0404b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大比丘并大菩提勇識及天主帝釋眾等集。",
        "from": "T19n0977_001:0404b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時出有壞坐蓮華座。",
        "from": "T19n0977_001:0404b11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入於普觀頂髻三昧。",
        "from": "T19n0977_001:0404b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速然出有壞從頂髻中。",
        "from": "T19n0977_001:0404b12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出現如是總持密呪法行。",
        "from": "T19n0977_001:0404b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮正覺及一切菩提勇識。",
        "from": "T19n0977_001:0404b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮正覺。",
        "from": "T19n0977_001:0404b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮妙法。",
        "from": "T19n0977_001:0404b14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮大眾。",
        "from": "T19n0977_001:0404b14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮七俱胝真實究竟正覺及聲聞大眾等。",
        "from": "T19n0977_001:0404b15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮所有世間壞怨等。",
        "from": "T19n0977_001:0404b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮所有預流等。",
        "from": "T19n0977_001:0404b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮所有一來等。",
        "from": "T19n0977_001:0404b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮所有不還等。",
        "from": "T19n0977_001:0404b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮所有世間真實超越等。",
        "from": "T19n0977_001:0404b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮所有入實者等。",
        "from": "T19n0977_001:0404b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮天仙呪咀及有加祐力能等。",
        "from": "T19n0977_001:0404b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮所有誦持明呪獲成就者等。",
        "from": "T19n0977_001:0404b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮淨梵。",
        "from": "T19n0977_001:0404b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮帝釋。",
        "from": "T19n0977_001:0404b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮緊威具美能令退屈苦行之主者等。",
        "from": "T19n0977_001:0404b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮具美嚴五手印無愛子之所歸敬處。",
        "from": "T19n0977_001:0404b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮具美能摧壞三層宮城住於墓地之中一切陰母所歸敬處。",
        "from": "T19n0977_001:0404b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮出有壞如來種佛。",
        "from": "T19n0977_001:0404b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮蓮華種佛。",
        "from": "T19n0977_001:0404b24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮金剛種佛。",
        "from": "T19n0977_001:0404b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮寶珠種佛。",
        "from": "T19n0977_001:0404b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮大象種佛。",
        "from": "T19n0977_001:0404b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮少童種佛。",
        "from": "T19n0977_001:0404b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮龍種佛。",
        "from": "T19n0977_001:0404b26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮勇固部器械王佛。",
        "from": "T19n0977_001:0404b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮無量光佛。",
        "from": "T19n0977_001:0404b27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮不動佛。",
        "from": "T19n0977_001:0404b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮藥師瑠璃光王佛。",
        "from": "T19n0977_001:0404b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮娑羅主王華實圓滿佛。",
        "from": "T19n0977_001:0404b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮釋迦牟尼佛。",
        "from": "T19n0977_001:0404b29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮寶上王佛。",
        "from": "T19n0977_001:0404b29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮最妙普賢佛。",
        "from": "T19n0977_001:0404c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮眾明主佛。",
        "from": "T19n0977_001:0404c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮目圓滿烏巴辣香上王佛。",
        "from": "T19n0977_001:0404c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼等處敬禮已。",
        "from": "T19n0977_001:0404c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出有壞母一切如來頂髻中出白傘蓋佛母餘無能敵大迴遮母。",
        "from": "T19n0977_001:0404c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此決斷一切出者邪魔。",
        "from": "T19n0977_001:0404c04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能決斷餘者一切明呪。",
        "from": "T19n0977_001:0404c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能迴遮非時橫夭。",
        "from": "T19n0977_001:0404c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能令有情解脫一切繫縛。",
        "from": "T19n0977_001:0404c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能迴遮一切憎嫌惡夢。",
        "from": "T19n0977_001:0404c06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能摧壞八萬四千邪魔。",
        "from": "T19n0977_001:0404c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能歡悅二十八宿。",
        "from": "T19n0977_001:0404c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能折伏八大房宿。",
        "from": "T19n0977_001:0404c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能迴遮一切冤讎。",
        "from": "T19n0977_001:0404c08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能摧壞最極暴惡一切憎嫌惡夢。",
        "from": "T19n0977_001:0404c09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能救度毒藥器械水火等難。",
        "from": "T19n0977_001:0404c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有能敵大緊母　　大掇朴母大力母",
        "from": "T19n0977_001:0404c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大熾然母大威力　　大白蓋母大力母",
        "from": "T19n0977_001:0404c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "熾然掛纓白衣母　　聖救度母具嗔皺",
        "from": "T19n0977_001:0404c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勝勢金剛稱念珠　　蓮華昭明金剛名",
        "from": "T19n0977_001:0404c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有能敵具念珠　　金剛牆等摧壞母",
        "from": "T19n0977_001:0404c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "柔善佛等供養母　　柔相威力具大母",
        "from": "T19n0977_001:0404c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖救度母大力母　　不歿金剛鐵鋜母",
        "from": "T19n0977_001:0404c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛少童持種母　　金剛手種金念珠",
        "from": "T19n0977_001:0404c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大赤色及寶珠母　　種明金剛稱頂髻",
        "from": "T19n0977_001:0404c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種相窈窕金剛母　　如金色光具眼母",
        "from": "T19n0977_001:0404c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛燭及白色母　　蓮華眼及月光母",
        "from": "T19n0977_001:0404c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手印聚彼等一切力故。",
        "from": "T19n0977_001:0404c22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願令擁護於我擁護於我。",
        "from": "T19n0977_001:0404c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵呤室遏捺不囉二合引折𭊁引也怛達遏哆烏室禰折席𭊁怛巴嘚哩二合吽𠹌隆二合拶沒末捺葛囉吽𠹌隆二合席怛沒末捺葛囉吽𠹌隆二合麻曷覓得也三合口末室渴捺葛囉吽𠹌隆二合撥囉覓能二合惹舌上三合口末室渴捺葛囉吽𠹌隆二合薩沒斡𬻅舌上室達捺能席擔沒末捺葛囉吽𠹌隆二合薩斡也室渴囉室渴薩屹囉曷捺覓得𭊁能薩捺葛囉吽𠹌隆二合拶𬻅囉室帝捺能屹囉曷薩曷悉囉覓𭊁能薩捺葛囉吽𠹌隆二合引啞室捺𠵴能折帝𭊁能捺色曷上腭引得得囉捺麻不囉薩怛捺葛囉吽𠹌隆二合引啞希怛捺麻麻渴屹囉曷捺能覓能捺能上腭薩捺葛囉吽𠹌隆二合引囉塞剋囉𪁪沒擁護於我出有壞母一切如來頂髻中出白傘蓋佛母。",
        "from": "T19n0977_001:0405a08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛頂髻大迴遮母。",
        "from": "T19n0977_001:0405a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具千大臂母。",
        "from": "T19n0977_001:0405a09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有千大首母。",
        "from": "T19n0977_001:0405a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具十萬俱胝目不二熾燃具種相金剛寬廣大白母。",
        "from": "T19n0977_001:0405a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "主宰三界中圍母。",
        "from": "T19n0977_001:0405a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切時中擁護於我擁護於我。",
        "from": "T19n0977_001:0405a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵國王難盜賊難火難水難毒藥難器械難外國軍兵難飢饉難冤讎難疾疫難霹靂難非時橫夭難地震動難星箭難國王刑罰難天難龍難閃電難飛空難惡獸忿怒難。",
        "from": "T19n0977_001:0405a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復天魔龍魔非天魔風神魔飛空魔尋香魔疑神魔大腹行魔施礙魔夜叉魔餓鬼魔空行母魔食肉魔出者魔瓶袋魔臭魔身臭魔令枯瘦魔令忘魔令顛狂魔令魘寐魔令鎮伏魔奎宿魔彼等一切之中願我獲得安穩吉祥。",
        "from": "T19n0977_001:0405a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又奪威力鬼奪容顏鬼食產宮鬼飲血鬼食胞胎鬼食肉鬼食脂鬼食髓鬼食脂衣鬼取命鬼食嘔吐鬼食大便鬼食小便鬼食竅流鬼食殘鬼食唾鬼食涕鬼食涎鬼食膿鬼食施食鬼食鬘鬼食香氣鬼食香鬼奪意鬼食華鬼食果鬼食苗鬼食燒施鬼等之中。",
        "from": "T19n0977_001:0405a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願我獲得安穩吉祥。",
        "from": "T19n0977_001:0405a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼等一切及一切魔所造明呪以此決斷將杵擊之遍遊行所造明呪以此決斷將杵擊之。",
        "from": "T19n0977_001:0405b01_17",
        "context_with_next_sentence": true
    },
