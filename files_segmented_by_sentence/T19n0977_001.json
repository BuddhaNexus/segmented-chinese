    {
        "source_sentence": "出者魔等。",
        "from": "T19n0977_001:0406a08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瓶袋魔等。",
        "from": "T19n0977_001:0406a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臭魔等。",
        "from": "T19n0977_001:0406a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身臭魔等。",
        "from": "T19n0977_001:0406a09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令枯瘦魔等。",
        "from": "T19n0977_001:0406a09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令顛狂魔等。",
        "from": "T19n0977_001:0406a09_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令厭魅魔等。",
        "from": "T19n0977_001:0406a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令忘魔等。",
        "from": "T19n0977_001:0406a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鎮伏魔等。",
        "from": "T19n0977_001:0406a10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空行母魔等。",
        "from": "T19n0977_001:0406a10_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奎宿魔等。",
        "from": "T19n0977_001:0406a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獄帝魔等。",
        "from": "T19n0977_001:0406a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陰母令喜魔等。",
        "from": "T19n0977_001:0406a11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍遊行具瓔魔等。",
        "from": "T19n0977_001:0406a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拔剌魔等。",
        "from": "T19n0977_001:0406a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復一日疫病。",
        "from": "T19n0977_001:0406a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦所有二日病三日病四日病七日病。",
        "from": "T19n0977_001:0406a13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒常疫病。",
        "from": "T19n0977_001:0406a13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無盡疫病。",
        "from": "T19n0977_001:0406a13_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瘌痛病出者依風起病。",
        "from": "T19n0977_001:0406a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依膽起病。",
        "from": "T19n0977_001:0406a14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依痰起病。",
        "from": "T19n0977_001:0406a14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依俱集起病。",
        "from": "T19n0977_001:0406a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切疾病身病等願令遣除。",
        "from": "T19n0977_001:0406a15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復身分病不進飲食病眼病鼻病口病項頸病心病咽喉病耳病齒病心熱惱病腦病半肋病背節病腹病腰病穀道病腿𦙶病脛病手病足病肢病眾肢病等。",
        "from": "T19n0977_001:0406a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願令遣除願令擁護。",
        "from": "T19n0977_001:0406a19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大白傘蓋佛母金剛頂髻大迴遮母。",
        "from": "T19n0977_001:0406a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此十二由旬內出者起屍者空行母者。",
        "from": "T19n0977_001:0406a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復疫病疥瘡癢瘡痘瘡癩瘡皴烈瘡痔瘡燒瘡疙癆瘡妙瘡。",
        "from": "T19n0977_001:0406a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又枯瘦恐怖病及寶毒及和合毒厭禱毒并火難水難。",
        "from": "T19n0977_001:0406a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又鬪爭結怨損害非時夭壽。",
        "from": "T19n0977_001:0406a24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復嘚哩二合麻布割蟲嘚哩辣怛蟲蝮蝎蚖蛇鼠狼獅子虎熊羆并熊狼水獸及猶如虻蠅取他性命。",
        "from": "T19n0977_001:0406a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼等一切明呪。",
        "from": "T19n0977_001:0406a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆繫縛一切威儀。",
        "from": "T19n0977_001:0406a27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫縛餘者一切明呪。",
        "from": "T19n0977_001:0406a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫縛一切諸魔明呪。",
        "from": "T19n0977_001:0406a28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆願令繫縛。",
        "from": "T19n0977_001:0406a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛得也達唵啞捺呤啞捺呤𠵴折得帝𠵴折得帝𠵴引囉末𠷌囉𭊁哩末上腭𭊁末𭊁末𠷌囉鉢禰發怛吽吽發怛發怛吽𠹌隆二合末捺發怛莎曷凡有行人。",
        "from": "T19n0977_001:0406b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此一切如來頂髻中出白傘蓋佛母餘無能敵大迴遮母。",
        "from": "T19n0977_001:0406b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或樺皮或白㲲或樹皮上書寫已。",
        "from": "T19n0977_001:0406b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或戴身上或項頸上。",
        "from": "T19n0977_001:0406b07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則能直至終身。",
        "from": "T19n0977_001:0406b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以毒不能害以器械不能害。",
        "from": "T19n0977_001:0406b08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以火不能焚以水不能漂。",
        "from": "T19n0977_001:0406b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以寶毒不能中以和毒不能害。",
        "from": "T19n0977_001:0406b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以呪毒不能壞非時夭壽不能侵。",
        "from": "T19n0977_001:0406b10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切冤魔及所有惡友等。",
        "from": "T19n0977_001:0406b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡一切處為悅愛所愛敬也。",
        "from": "T19n0977_001:0406b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又能恒河沙俱胝八萬四千金剛種等。",
        "from": "T19n0977_001:0406b12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦擁護亦救護亦覆護。",
        "from": "T19n0977_001:0406b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼等作悅意所愛敬之。",
        "from": "T19n0977_001:0406b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又能八萬四千大劫之中得宿命智。",
        "from": "T19n0977_001:0406b14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又世世生處不受施礙羅剎餓鬼臭及身臭等身。",
        "from": "T19n0977_001:0406b15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又不受人中貧窮之身。",
        "from": "T19n0977_001:0406b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又具足無量無數恒河沙數正覺出有壞之福祿也。",
        "from": "T19n0977_001:0406b17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又能一切如來頂髻中出白傘蓋佛母餘無能敵大迴遮母恒受持。",
        "from": "T19n0977_001:0406b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則不行梵行亦成梵行。",
        "from": "T19n0977_001:0406b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能忍則亦能成忍。",
        "from": "T19n0977_001:0406b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不清淨則亦為清淨。",
        "from": "T19n0977_001:0406b20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無近住戒得近住戒。",
        "from": "T19n0977_001:0406b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不持齋戒亦成齋戒。",
        "from": "T19n0977_001:0406b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設爾所造五無間罪則能清淨無餘。",
        "from": "T19n0977_001:0406b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往昔業障悉皆消滅。",
        "from": "T19n0977_001:0406b22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲女人求子。",
        "from": "T19n0977_001:0406b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則能受持一切如來頂髻中出白傘蓋佛母餘無能敵大迴遮母者。",
        "from": "T19n0977_001:0406b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲得具足壽命福德威力之子。",
        "from": "T19n0977_001:0406b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命終之後往生極樂世界。",
        "from": "T19n0977_001:0406b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又人病牛病畜病疫病。",
        "from": "T19n0977_001:0406b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及損害及惹病礙。",
        "from": "T19n0977_001:0406b26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及鬪戰餘他一切軍兵之中。",
        "from": "T19n0977_001:0406b27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則能以此一切如來頂髻中出白傘蓋佛母餘無能敵大迴遮母。",
        "from": "T19n0977_001:0406b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安置於幢頂上作廣大供養已。",
        "from": "T19n0977_001:0406b29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將幢置大城門上。",
        "from": "T19n0977_001:0406c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或宮宅之中或村坊之中。",
        "from": "T19n0977_001:0406c01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或聚落之中或川原之中或寂靜之處。",
        "from": "T19n0977_001:0406c02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於餘無能敵大迴遮母處作廣大供養。",
        "from": "T19n0977_001:0406c03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則能速然國界安寧。",
        "from": "T19n0977_001:0406c03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能柔善疫病礙與損害鬪爭。",
        "from": "T19n0977_001:0406c04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘他一切軍兵也。",
        "from": "T19n0977_001:0406c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "末𠷌囉鉢禰遣魔擁護呪寧引得也達唵室哆沒末上腭𭊁末上腭𭊁擁護於我擁護於我莎曷室渴囉室渴麻沒唵吽室哆沒末能𭊁末能𭊁末𠷌囉二合擁護於我囉室渴囉室渴麻沒末𠷌囉末禰英吽發怛莎曷恒常持心呪唵薩㗚末怛達遏哆烏室禰折啞斡浪屹帝摩冷𭊁叮𡁧囉室禰唵𪢚辣𪢚辣𭊁渴𭊁渴𭊁囉𭊁囉覓能𭊁囉覓能𭊁囉秦𭊁秦𭊁覓沒𭊁覓沒𭊁吽吽發怛發怛莎曷增長身親心呪唵薩㗚末怛達遏哆烏室禰折吽發怛發怛莎曷攝受呪寧得也達唵啞捺令啞捺令渴薩銘渴薩銘𠵴引囉𠵴引囉星烏禰榮切身星上同薩㗚末莫𭊁啞溺室達捺啞溺室提矴薩㗚末怛達遏哆烏室禰折席怛怛末嘚哩吽發怛莎曷堅甲呪吽麻麻吽禰莎曷應作明滿修習。",
        "from": "T19n0977_001:0406c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼所有龍王等依時降雨矣。",
        "from": "T19n0977_001:0406c26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正覺與菩提勇識。",
        "from": "T19n0977_001:0406c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天及非天并人。",
        "from": "T19n0977_001:0406c27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與尋香一切世間等。",
        "from": "T19n0977_001:0406c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆大歡喜出有壞所說之處現前讚揚。",
        "from": "T19n0977_001:0406c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵蓮華月輪妙座上　　金剛跏趺身色白",
        "from": "T19n0977_001:0407a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手當胸執白蓋　　具白傘處稱讚禮",
        "from": "T19n0977_001:0407a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手結於無畏印　　喜笑及身具窈窕",
        "from": "T19n0977_001:0407a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切正覺所攝受　　佛母汝處稱讚禮",
        "from": "T19n0977_001:0407a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切所伏有情處　　隨類化現種種身",
        "from": "T19n0977_001:0407a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大寶莊嚴極美妙　　悅意母處稱讚禮",
        "from": "T19n0977_001:0407a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵無有能敵大緊母　　大掇朴母大力母",
        "from": "T19n0977_001:0407a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大熾然母大威母　　大白蓋母大力母",
        "from": "T19n0977_001:0407a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "熾然掛纓白衣母　　聖救度母大嗔皺",
        "from": "T19n0977_001:0407a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖世金剛稱念珠　　蓮華昭明金剛名",
        "from": "T19n0977_001:0407a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有能敵具念珠　　金剛牆等摧壞母",
        "from": "T19n0977_001:0407a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "柔善佛等供養母　　柔相威力具大母",
        "from": "T19n0977_001:0407a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖救度母大力母　　不歿金剛鐵鋜母",
        "from": "T19n0977_001:0407a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛少童持種母　　金剛手種金念珠",
        "from": "T19n0977_001:0407a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大赤色及寶珠母　　種明金剛稱頂髻",
        "from": "T19n0977_001:0407a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種相窈窕金剛母　　如金色光具眼母",
        "from": "T19n0977_001:0407a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛燭及白色母　　蓮華目及月光母",
        "from": "T19n0977_001:0407a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手印聚處稱讚禮　　出有壞母白蓋母",
        "from": "T19n0977_001:0407a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋迦頂髻中出母　　金剛頂髻迴遮母",
        "from": "T19n0977_001:0407a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千臂千手大具母　　大俱胝之具眼母",
        "from": "T19n0977_001:0407a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不二熾燃具種相　　金剛廣博大白母",
        "from": "T19n0977_001:0407a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "主宰三界中圍母　　最極於我求擁護",
        "from": "T19n0977_001:0407a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵國王賊怖水火毒　　器械飢饉邪魔疾",
        "from": "T19n0977_001:0407a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "霹靂非時并夭壽　　地震國王刑罰等",
        "from": "T19n0977_001:0407b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "閃電飛空諸怖散　　惡獸虎等大難中",
        "from": "T19n0977_001:0407b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切時中乞覆護　　其天魔等諸魔礙",
        "from": "T19n0977_001:0407b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能奪威力并餓鬼　　風膽痰等大病中",
        "from": "T19n0977_001:0407b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切時中乞覆護　　貪癡嗔等諸煩惱",
        "from": "T19n0977_001:0407b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十不善業五無間　　所遮自性罪業等",
        "from": "T19n0977_001:0407b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡趣苦果怖畏中　　愚資我今求覆護",
        "from": "T19n0977_001:0407b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大慈悲之鐵鉤　　猶如愛子乞護持",
        "from": "T19n0977_001:0407b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切時中擁護我大白傘蓋總持讚歎禱祝偈竟",
        "from": "T19n0977_001:0407b10_100",
        "context_with_next_sentence": false
    }
]