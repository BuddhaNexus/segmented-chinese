[
    {
        "source_sentence": "瑜伽蓮華部念誦法一卷",
        "from": "T20n1032_001:0006c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T20n1032_001:0006c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空諡大鑒",
        "from": "T20n1032_001:0006c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正號大廣智大興善寺三藏沙門不空奉　詔譯初入道場。",
        "from": "T20n1032_001:0006c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至心頂禮懺悔發願回向等已。",
        "from": "T20n1032_001:0006c09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結蓮華合掌印　兩手指虛其掌。",
        "from": "T20n1032_001:0006c10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "似未開芙蓉。",
        "from": "T20n1032_001:0006c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦一切法清淨真言。",
        "from": "T20n1032_001:0006c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印心額喉頂上各一遍。",
        "from": "T20n1032_001:0006c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得清淨。",
        "from": "T20n1032_001:0006c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0006c12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩嚩二合婆嚩󰡊馱薩嚩達摩娑嚩二合婆嚩戍度唅然後右膝著地。",
        "from": "T20n1032_001:0006c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮華合掌置於頂上。",
        "from": "T20n1032_001:0006c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言想禮一切諸佛菩薩本尊足。",
        "from": "T20n1032_001:0006c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0006c16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵鉢娜麼二合勿微一反作此法已。",
        "from": "T20n1032_001:0006c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於一切諸佛菩薩。",
        "from": "T20n1032_001:0006c18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮事供養皆悉成就。",
        "from": "T20n1032_001:0006c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後結加趺坐或半加坐。",
        "from": "T20n1032_001:0006c19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結蓮華三昧耶印。",
        "from": "T20n1032_001:0006c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩手外相叉合掌。",
        "from": "T20n1032_001:0006c20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二大指二小指相並。",
        "from": "T20n1032_001:0007a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呈示一切諸佛菩薩本尊。",
        "from": "T20n1032_001:0007a01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即憶本誓願加持攝受。",
        "from": "T20n1032_001:0007a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0007a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉二合鉢娜麼二合三摩耶薩怛鑁三合次結極喜印。",
        "from": "T20n1032_001:0007a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前手勢。",
        "from": "T20n1032_001:0007a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但以二中指。",
        "from": "T20n1032_001:0007a05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "垂下於掌中。",
        "from": "T20n1032_001:0007a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以指面相合。",
        "from": "T20n1032_001:0007a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0007a06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵三摩耶斛素囉多薩怛鑁三合次結開心印。",
        "from": "T20n1032_001:0007a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀二乳上。",
        "from": "T20n1032_001:0007a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右怛囉二合引左吒字。",
        "from": "T20n1032_001:0007a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如戶樞。",
        "from": "T20n1032_001:0007a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以金剛縛三掣開。",
        "from": "T20n1032_001:0007a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以開自心猶如啟扇。",
        "from": "T20n1032_001:0007a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0007a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉二合滿馱怛囉二合吒次結入智印。",
        "from": "T20n1032_001:0007a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前金剛縛。",
        "from": "T20n1032_001:0007a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但以二大拇指屈入掌中。",
        "from": "T20n1032_001:0007a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀一肘前有白蓮花。",
        "from": "T20n1032_001:0007a13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上置惡字字有白光。",
        "from": "T20n1032_001:0007a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流入心中住白蓮花上。",
        "from": "T20n1032_001:0007a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0007a14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉二合引微舍惡急次結閉心門印。",
        "from": "T20n1032_001:0007a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前縛。",
        "from": "T20n1032_001:0007a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但以二頭指並拄二大拇指節。",
        "from": "T20n1032_001:0007a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以其印觸心。",
        "from": "T20n1032_001:0007a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0007a17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉二合引母瑟知𤚥即觀門閉次結定印。",
        "from": "T20n1032_001:0007a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩手相叉仰安臍下。",
        "from": "T20n1032_001:0007a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大拇指向上相捻。",
        "from": "T20n1032_001:0007a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦入三摩地真言曰。",
        "from": "T20n1032_001:0007a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵三摩地鉢娜彌二合紇唎二合次應端身正念入三摩地。",
        "from": "T20n1032_001:0007a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舌拄上腭止諸攀緣。",
        "from": "T20n1032_001:0007a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀內外一切法皆無所有。",
        "from": "T20n1032_001:0007a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若妄念多者應先數息。",
        "from": "T20n1032_001:0007a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數息法從一息至七息。",
        "from": "T20n1032_001:0007a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又從一至七相續不絕。",
        "from": "T20n1032_001:0007a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心無攀緣即不須數息。",
        "from": "T20n1032_001:0007a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深入清淨無所有處。",
        "from": "T20n1032_001:0007a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即觀諸佛遍滿虛空。",
        "from": "T20n1032_001:0007a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其身大小猶如胡麻具諸相好。",
        "from": "T20n1032_001:0007a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告行者言。",
        "from": "T20n1032_001:0007a27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子汝觀自心。",
        "from": "T20n1032_001:0007a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又觀己身遍禮佛足。",
        "from": "T20n1032_001:0007a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊云何觀心。",
        "from": "T20n1032_001:0007a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心何相貌。",
        "from": "T20n1032_001:0007a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛告言。",
        "from": "T20n1032_001:0007a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子心相難可測量。",
        "from": "T20n1032_001:0007b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "授與真言密誦觀察。",
        "from": "T20n1032_001:0007b01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0007b01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵質多鉢囉二合底微鄧迦嚕弭行者密誦諦觀己心猶如淨月。",
        "from": "T20n1032_001:0007b03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛告言。",
        "from": "T20n1032_001:0007b03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子汝心本如是。",
        "from": "T20n1032_001:0007b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但客塵所翳。",
        "from": "T20n1032_001:0007b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知此即是菩提心。",
        "from": "T20n1032_001:0007b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又授真言曰。",
        "from": "T20n1032_001:0007b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵冒地質多　畝怛波二合娜夜弭誦此真言。",
        "from": "T20n1032_001:0007b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諦觀心月轉更分明淨無瑕翳。",
        "from": "T20n1032_001:0007b07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於月輪中觀紇唎二合字。",
        "from": "T20n1032_001:0007b08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成一八葉白蓮花光明顯照。",
        "from": "T20n1032_001:0007b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0007b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵底瑟姹二合嚩日囉二合鉢娜麼二合次又觀此白蓮華遍周法界量同虛空。",
        "from": "T20n1032_001:0007b11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0007b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩癹二合囉嚩日囉二合鉢娜摩二合次觀白蓮華漸漸收斂。",
        "from": "T20n1032_001:0007b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空諸佛悉入其中量等己身。",
        "from": "T20n1032_001:0007b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0007b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵僧訶囉嚩日囉二合鉢娜摩二合即變蓮華成觀自在菩薩。",
        "from": "T20n1032_001:0007b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶冠瓔珞相好莊嚴。",
        "from": "T20n1032_001:0007b17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放大光明遍周法界。",
        "from": "T20n1032_001:0007b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冠上有無量壽佛本尊左手執白蓮花。",
        "from": "T20n1032_001:0007b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手作擘開蓮華勢。",
        "from": "T20n1032_001:0007b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0007b19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵三摩喻含摩訶三摩喻含薩婆怛他孽多毘三冒地嚩日囉二合鉢娜摩二合阿怛摩二合句含入是三昧者。",
        "from": "T20n1032_001:0007b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切天龍八部。",
        "from": "T20n1032_001:0007b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見行者身與觀自在等無差別。",
        "from": "T20n1032_001:0007b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除行者無量億劫生死重罪。",
        "from": "T20n1032_001:0007b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切勝願無不悉地。",
        "from": "T20n1032_001:0007b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現得圓滿金剛法身。",
        "from": "T20n1032_001:0007b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即觀此身便成正覺。",
        "from": "T20n1032_001:0007b26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結加持印。",
        "from": "T20n1032_001:0007b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外相叉金剛縛結已。",
        "from": "T20n1032_001:0007b27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即變屈二頭指。",
        "from": "T20n1032_001:0007b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捻二大拇指。",
        "from": "T20n1032_001:0007b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印心額喉頂各誦一遍。",
        "from": "T20n1032_001:0007b28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0007b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵紇唎二合薩婆迦哩阿地瑟姹二合薩嚩二合𤚥次結灌頂印。",
        "from": "T20n1032_001:0007c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌已。",
        "from": "T20n1032_001:0007c03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二大拇指。",
        "from": "T20n1032_001:0007c03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偃蹙八指如寶形。",
        "from": "T20n1032_001:0007c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置於頂上誦真言三遍。",
        "from": "T20n1032_001:0007c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想五如來冠以冠其首。",
        "from": "T20n1032_001:0007c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0007c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵紇唎二合摩抳鉢娜摩二合阿毘詵者薩嚩二合𤚥怛𡀔二合紇唎二合次結繫鬘印誦真言。",
        "from": "T20n1032_001:0007c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以前冠頂印。",
        "from": "T20n1032_001:0007c08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從額分二手。",
        "from": "T20n1032_001:0007c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於腦後三相繞如繫鬘。",
        "from": "T20n1032_001:0007c09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便向前耳邊下。",
        "from": "T20n1032_001:0007c09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從小指散垂如鬘帶勢。",
        "from": "T20n1032_001:0007c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0007c10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉二合鉢娜摩二合磨隸𤚥紇唎二合次結甲冑印。",
        "from": "T20n1032_001:0007c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結金剛拳如小兒握固。",
        "from": "T20n1032_001:0007c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二頭指各拄二大拇指節。",
        "from": "T20n1032_001:0007c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當心已。",
        "from": "T20n1032_001:0007c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即伸二頭指相掩一節許。",
        "from": "T20n1032_001:0007c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仍以右指左指上。",
        "from": "T20n1032_001:0007c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀右頭指面有唵字。",
        "from": "T20n1032_001:0007c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左頭指面有跕字。",
        "from": "T20n1032_001:0007c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口中仍誦此兩字真言不斷絕。",
        "from": "T20n1032_001:0007c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想其字並放綠色光。",
        "from": "T20n1032_001:0007c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如抽藕絲光不斷絕。",
        "from": "T20n1032_001:0007c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心前三繞。",
        "from": "T20n1032_001:0007c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "背上三繞。",
        "from": "T20n1032_001:0007c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又至臍又至兩膝。",
        "from": "T20n1032_001:0007c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又至腰又當心。",
        "from": "T20n1032_001:0007c18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次右肩左肩次至額上。",
        "from": "T20n1032_001:0007c18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又於腦後各三繞已。",
        "from": "T20n1032_001:0007c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "却結金剛拳印從小指散如垂天衣。",
        "from": "T20n1032_001:0007c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以二縛不得解。",
        "from": "T20n1032_001:0007c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但以掌三相拍。",
        "from": "T20n1032_001:0007c21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0007c21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉二合鉢娜摩二合覩史也二合斛次於壇中觀阿上字成月輪。",
        "from": "T20n1032_001:0007c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於月輪中觀紇唎二合字成本尊身。",
        "from": "T20n1032_001:0007c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放大光明。",
        "from": "T20n1032_001:0007c24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量眷屬普現於圓光內。",
        "from": "T20n1032_001:0007c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結鉤印。",
        "from": "T20n1032_001:0007c26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛縛舉右頭指。",
        "from": "T20n1032_001:0007c26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如鉤三招。",
        "from": "T20n1032_001:0007c26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一誦一招。",
        "from": "T20n1032_001:0007c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0007c27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿夜係弱次以索印。",
        "from": "T20n1032_001:0007c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前縛以二頭指相拄如環。",
        "from": "T20n1032_001:0007c29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "引本尊入所觀智身。",
        "from": "T20n1032_001:0008a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0008a01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿係吽吽次結鎖印。",
        "from": "T20n1032_001:0008a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前縛以二頭指及二大指。",
        "from": "T20n1032_001:0008a03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相串各相捻如聯鎖。",
        "from": "T20n1032_001:0008a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令本尊止住。",
        "from": "T20n1032_001:0008a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0008a04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵係薩普二合吒𤚥次結鈴印。",
        "from": "T20n1032_001:0008a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前縛屈二大拇指入掌中。",
        "from": "T20n1032_001:0008a06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三撼手如搖鈴。",
        "from": "T20n1032_001:0008a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令本尊歡喜。",
        "from": "T20n1032_001:0008a07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0008a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵尾舍耶斛引次結獻遏伽香水印。",
        "from": "T20n1032_001:0008a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦百字真言或餘讚歎。",
        "from": "T20n1032_001:0008a09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩手外相叉擘開掌。",
        "from": "T20n1032_001:0008a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二中指頭相合。",
        "from": "T20n1032_001:0008a10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二頭指微屈。",
        "from": "T20n1032_001:0008a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "去中指背一麥許不相著。",
        "from": "T20n1032_001:0008a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "狀如三鈷杵頭。",
        "from": "T20n1032_001:0008a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大拇指博著二頭指下側。",
        "from": "T20n1032_001:0008a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即想香水滿掬。",
        "from": "T20n1032_001:0008a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言向前寫灌洗本尊足。",
        "from": "T20n1032_001:0008a13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0008a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日𡀔二合娜迦坼本尊既至次結羯摩印。",
        "from": "T20n1032_001:0008a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手金剛拳當心著。",
        "from": "T20n1032_001:0008a16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想把白蓮華。",
        "from": "T20n1032_001:0008a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手亦金剛拳於上轉。",
        "from": "T20n1032_001:0008a17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想撥開蓮葉即身。",
        "from": "T20n1032_001:0008a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同本尊事業真言曰。",
        "from": "T20n1032_001:0008a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉二合達摩紇唎二合次結三昧耶印。",
        "from": "T20n1032_001:0008a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛縛屈二頭指。",
        "from": "T20n1032_001:0008a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大拇指押。",
        "from": "T20n1032_001:0008a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同已上加持印。",
        "from": "T20n1032_001:0008a21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令本尊憶本誓願加持攝護。",
        "from": "T20n1032_001:0008a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0008a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩嚩迦里誦三遍然後結金剛嬉戲內供養印。",
        "from": "T20n1032_001:0008a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛縛伸二大拇指。",
        "from": "T20n1032_001:0008a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偃向身相並當心。",
        "from": "T20n1032_001:0008a25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0008a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵摩訶囉底丁以反次結鬘。",
        "from": "T20n1032_001:0008a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前印更不改。",
        "from": "T20n1032_001:0008a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但以二臂相並直伸。",
        "from": "T20n1032_001:0008a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向前當額。",
        "from": "T20n1032_001:0008a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0008a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵𡀔波輸陛次結歌印。",
        "from": "T20n1032_001:0008b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不改前印。",
        "from": "T20n1032_001:0008b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從前至口解散。",
        "from": "T20n1032_001:0008b01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向前垂二手下。",
        "from": "T20n1032_001:0008b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如發歌音。",
        "from": "T20n1032_001:0008b02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0008b02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵󰡊嚧上輸律反下縷音二合怛囉二合燥溪次結舞印。",
        "from": "T20n1032_001:0008b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二金剛拳相旋繞。",
        "from": "T20n1032_001:0008b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各從小指散。",
        "from": "T20n1032_001:0008b04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便金剛合掌置於頂上金剛合掌者合掌令指頭右押左頭相交是也真言曰。",
        "from": "T20n1032_001:0008b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩嚩布而而曳反　已上內四供養次結焚香印。",
        "from": "T20n1032_001:0008b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以金剛縛向下解散。",
        "from": "T20n1032_001:0008b08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如焚香。",
        "from": "T20n1032_001:0008b08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0008b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵鉢囉二合訶囉二合儞寧去引次結華印。",
        "from": "T20n1032_001:0008b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以金剛縛向上解散。",
        "from": "T20n1032_001:0008b11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如散華。",
        "from": "T20n1032_001:0008b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0008b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵頗攞誐弭次結燈印。",
        "from": "T20n1032_001:0008b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前嬉戲。",
        "from": "T20n1032_001:0008b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但以二大拇指。",
        "from": "T20n1032_001:0008b14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭屈前相逼。",
        "from": "T20n1032_001:0008b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不得相著指。",
        "from": "T20n1032_001:0008b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0008b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵素帝惹仡哩二合次結塗香印。",
        "from": "T20n1032_001:0008b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以金剛縛解散。",
        "from": "T20n1032_001:0008b17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩胸上如塗香勢真言曰。",
        "from": "T20n1032_001:0008b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵素巘蕩儗妍以反　已上外四供養次結部心根本印。",
        "from": "T20n1032_001:0008b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌以二無名指二頭指。",
        "from": "T20n1032_001:0008b20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初分相交。",
        "from": "T20n1032_001:0008b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其二小指二大拇指擘開。",
        "from": "T20n1032_001:0008b21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦百字真言三遍。",
        "from": "T20n1032_001:0008b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三字半七遍已於頂上散。",
        "from": "T20n1032_001:0008b22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後執念珠念誦。",
        "from": "T20n1032_001:0008b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手把珠當心念誦。",
        "from": "T20n1032_001:0008b23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諦觀本尊放淨光明。",
        "from": "T20n1032_001:0008b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流注已頂照於心月。",
        "from": "T20n1032_001:0008b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月有白蓮華。",
        "from": "T20n1032_001:0008b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身與本尊色相無二。",
        "from": "T20n1032_001:0008b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如於明鏡自觀己身。",
        "from": "T20n1032_001:0008b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與本尊互成影像。",
        "from": "T20n1032_001:0008b26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍數終訖懺悔發願。",
        "from": "T20n1032_001:0008b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重獻三昧耶重結八供養。",
        "from": "T20n1032_001:0008b27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後發遣聖者復還本宮。",
        "from": "T20n1032_001:0008b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發遣者用前三昧耶印當口解。",
        "from": "T20n1032_001:0008b28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1032_001:0008c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵鉢娜摩二合薩怛縛二合穆輕然後復結加持灌頂甲冑拍印等。",
        "from": "T20n1032_001:0008c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後出道場。",
        "from": "T20n1032_001:0008c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或讀大乘經。",
        "from": "T20n1032_001:0008c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或印佛作塔。",
        "from": "T20n1032_001:0008c04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切時本尊三摩地不令間斷。",
        "from": "T20n1032_001:0008c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若觸穢處當觀頂上有法界生字。",
        "from": "T20n1032_001:0008c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放赤色光。",
        "from": "T20n1032_001:0008c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂㘕字。",
        "from": "T20n1032_001:0008c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於所食物皆加持此字。",
        "from": "T20n1032_001:0008c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即不成穢觸於一切供養香華。",
        "from": "T20n1032_001:0008c07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆加持此字放白色光。",
        "from": "T20n1032_001:0008c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即無穢觸。",
        "from": "T20n1032_001:0008c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所供養物皆遍法界。",
        "from": "T20n1032_001:0008c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮華部百字真言曰。",
        "from": "T20n1032_001:0008c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一鉢娜摩二合下同薩怛嚩二合下同三摩耶二摩努播羅耶三鉢娜麼二合薩怛嚩二合四怛尾弩波底瑟姹二合五涅哩二合住茶護反銘婆嚩六素覩使諭二合寐婆嚩七阿弩囉訖覩二合寐婆嚩八素補使諭二合寐婆嚩九薩嚩悉地寐鉢囉二合也瑳十薩嚩羯磨素者寐十一質多室唎二合藥十二句𡀔吽十三呵呵呵呵穀十四引薄伽梵十五薩嚩怛他孽多十六鉢娜麼二合十七麼寐悶者十八鉢娜寐引婆嚩十九摩訶三摩耶薩怛嚩二合二十紇唎二合引聲瑜伽蓮華部念誦法一卷",
        "from": "T20n1032_001:0008c21_100",
        "context_with_next_sentence": false
    }
]