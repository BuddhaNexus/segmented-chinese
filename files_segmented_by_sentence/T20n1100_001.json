[
    {
        "source_sentence": "葉衣觀自在菩薩經",
        "from": "T20n1100_001:0447a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T20n1100_001:0447a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空諡大鑒",
        "from": "T20n1100_001:0447a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正號大廣智大興善寺三藏沙門不空奉　詔譯爾時婆伽梵住極樂世界。",
        "from": "T20n1100_001:0447a09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與諸大眾宣說妙法。",
        "from": "T20n1100_001:0447a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時金剛手菩薩。",
        "from": "T20n1100_001:0447a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從座而起偏袒右肩。",
        "from": "T20n1100_001:0447a10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雙膝著地頂禮觀自在菩薩摩訶薩足。",
        "from": "T20n1100_001:0447a11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白觀自在菩薩言。",
        "from": "T20n1100_001:0447a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖者住大悲解脫如幻三昧。",
        "from": "T20n1100_001:0447a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除一切有情苦惱。",
        "from": "T20n1100_001:0447a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與世出世利益安樂。",
        "from": "T20n1100_001:0447a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "假使三千大千世界一切眾生。",
        "from": "T20n1100_001:0447a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同時有種種苦惱及八難苦。",
        "from": "T20n1100_001:0447a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或希望世間出世果報。",
        "from": "T20n1100_001:0447a15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能一心稱念觀自在菩薩摩訶薩名號。",
        "from": "T20n1100_001:0447a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應時不捨大悲誓願。",
        "from": "T20n1100_001:0447a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即現種種隨類之身。",
        "from": "T20n1100_001:0447a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能滿眾生一切勝願。",
        "from": "T20n1100_001:0447a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能護持國界拔濟苦難。",
        "from": "T20n1100_001:0447a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能攝受養育增長吉祥。",
        "from": "T20n1100_001:0447a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能遮止囚禁苦形。",
        "from": "T20n1100_001:0447a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能銷除蠱毒鬼魅及諸惡病。",
        "from": "T20n1100_001:0447a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能臨陣禁制刀杖。",
        "from": "T20n1100_001:0447a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能消除水火災難。",
        "from": "T20n1100_001:0447a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能斷除󰚏禱呪詛。",
        "from": "T20n1100_001:0447a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能結護方隅地界。",
        "from": "T20n1100_001:0447a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願聖者哀愍未來一切有情國王男女。",
        "from": "T20n1100_001:0447a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若淨信三寶護持佛法。",
        "from": "T20n1100_001:0447a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相承王業勿令斷絕。",
        "from": "T20n1100_001:0447a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為彼等故說軌儀陀羅尼加持方便。",
        "from": "T20n1100_001:0447a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時觀自在菩薩摩訶薩。",
        "from": "T20n1100_001:0447a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從座而起頂禮佛足。",
        "from": "T20n1100_001:0447a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右遶三匝還坐本處。",
        "from": "T20n1100_001:0447a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌向佛而白佛言。",
        "from": "T20n1100_001:0447a27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊哀愍加持。",
        "from": "T20n1100_001:0447a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我有葉衣觀自在菩薩摩訶薩陀羅尼。",
        "from": "T20n1100_001:0447a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除一切有情災禍。",
        "from": "T20n1100_001:0447a29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疫疾飢儉劫賊刀兵。",
        "from": "T20n1100_001:0447b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水旱不調宿曜失序。",
        "from": "T20n1100_001:0447b01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能增長福德國界豐盛人民安樂。",
        "from": "T20n1100_001:0447b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今欲說唯願聽許。",
        "from": "T20n1100_001:0447b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言善哉善哉隨汝意說。",
        "from": "T20n1100_001:0447b03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時觀自在菩薩。",
        "from": "T20n1100_001:0447b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "承佛威神而說陀羅尼曰。",
        "from": "T20n1100_001:0447b04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨囉怛曩二合怛囉二合夜引野一曩謨阿弭跢引婆去引野二怛他去引蘖跢引夜引囉賀二合帝三去藐三去沒馱引野三曩謨阿去引哩野二合引嚩路引枳帝濕嚩二合囉野四󰂛地薩怛嚩二合引野五摩訶薩怛嚩二合引野六摩賀迦引嚕抳尼整反迦引野七曩謨摩賀娑他二合去引麼鉢囉二合引跛跢二合引野八󰂛地薩怛嚩二合引野九摩賀薩怛嚩二合野十摩賀迦魯抳尼整反迦引野十一嚩引麼寧怛鑁二合引曩麼寫去引弭十二怛鑁二合引曩麼寫去引弭嚩引麼寧十三妣舍止鉢囉二合拏捨嚩哩十四鉢囉二合拏捨嚩哩妣舍止十五婆去誐嚩底丁以反十六跛捨跛囉輸上播捨馱哩抳十七夜引𩕳迦𩕳質十八婆去夜引𩕳聿二合答跛二合儞也二合麼引曩引𩕳聿二合答跛二合撚帝十九夜引入迦引室質二合儞泥以反多庾二十引夜引入迦引室質二合怛麼二合哩庾二十一夜引迦引室質二合摩賀引麼引哩庾二合二十二曳計質弩鉢捺囉二合嚩無跛反二十三曳計質弩播引夜引娑引入二十四曳計質捺地野二合引婆嚩准上引二十五曳計質弩跛薩虐引二十六塢跛薩誐三去滿馱引嚩引二十七嗢鉢撚帝二十八薩嚩引𩕳跢引𩕳薩嚩引娑嚲二合引二十九薩吠帝嚩引攞多三十噎武無後反鉢撚帝曩三十一半旎多娑多二合娜𡩋曩薩底曳二合三十二薩底也二合嚩引計引曩三十三惹自娜反惹惹惹三十四曀鼻室止二合半旎跢引三十五地瑟恥二合帶引漫怛囉二合鉢乃引三十六麼鼻聲麼准上薩嚩薩怛縛二合引難上者三十七囉迦愴二合引矩嚕三十八麌不井二合矩嚕三十九跛哩怛囉二合引喃矩嚕四十跛哩󰋲囉二合怛上矩嚕四十一跛哩播引攞曩矩嚕四十二扇引井矩嚕四十三娑嚕二合娑底野三合野曩矩嚕四十四難上拏跛哩賀引𠻴矩嚕四十五設娑怛囉三合跛哩賀𠻴矩嚕四十六尾灑怒引灑南矩嚕四十七尾灑曩引捨曩矩嚕四十八枲引麼引滿蕩矩嚕四十九馱囉抳滿蕩左矩嚕五十怛儞也二合他去引五十一阿密哩二合帝阿蜜哩二合姤納婆二合吠五十二阿濕嚩二合娑黨二合引霓五十三麼鼻聲引麼囉麼同前引麼囉五十四捨麼鉢囉二合捨麼五十五覩奴鼻聲尾覩奴准前五十六覩黎覩母黎娑嚩二合賀心真言曰。",
        "from": "T20n1100_001:0448a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一鉢囉二合拏捨嚩哩二吽發吒半聲三時觀自在菩薩。",
        "from": "T20n1100_001:0448a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此陀羅尼已。",
        "from": "T20n1100_001:0448a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊。",
        "from": "T20n1100_001:0448a05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人。",
        "from": "T20n1100_001:0448a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此陀羅尼一遍。",
        "from": "T20n1100_001:0448a06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即護自身。",
        "from": "T20n1100_001:0448a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦兩遍即護伴侶。",
        "from": "T20n1100_001:0448a07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦三遍能護一家。",
        "from": "T20n1100_001:0448a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦四遍護一聚落。",
        "from": "T20n1100_001:0448a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦五遍護一國界。",
        "from": "T20n1100_001:0448a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若國內疫病流行。",
        "from": "T20n1100_001:0448a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應取白㲲闊一肘半長二肘。",
        "from": "T20n1100_001:0448a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先令畫人潔淨齋戒。",
        "from": "T20n1100_001:0448a10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以瞿摩夷汁和少青碌。",
        "from": "T20n1100_001:0448a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以香膠和。",
        "from": "T20n1100_001:0448a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿用皮膠。",
        "from": "T20n1100_001:0448a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取鬼宿日。",
        "from": "T20n1100_001:0448a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫葉衣觀自在菩薩像。",
        "from": "T20n1100_001:0448a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其像作天女形。",
        "from": "T20n1100_001:0448a12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "首戴寶冠冠有無量壽佛。",
        "from": "T20n1100_001:0448a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瓔珞環釧莊嚴其身。",
        "from": "T20n1100_001:0448a13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身有圓光火焰圍遶。",
        "from": "T20n1100_001:0448a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "像有四臂右第一手當心持吉祥菓。",
        "from": "T20n1100_001:0448a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二手作施願手。",
        "from": "T20n1100_001:0448a15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左第一手持鉞斧。",
        "from": "T20n1100_001:0448a15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二手持羂索。",
        "from": "T20n1100_001:0448a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐蓮華上。",
        "from": "T20n1100_001:0448a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫像成已懸於竿上。",
        "from": "T20n1100_001:0448a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令一人執持。",
        "from": "T20n1100_001:0448a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執竿之人無間斷誦葉衣觀自在菩薩陀羅尼。",
        "from": "T20n1100_001:0448a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聲鼓鳴磬。",
        "from": "T20n1100_001:0448a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所擊之杖用樀枳王真言。",
        "from": "T20n1100_001:0448a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持二十一遍方乃擊之。",
        "from": "T20n1100_001:0448a19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1100_001:0448a19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵樀枳吽短聲弱又令二人誦讚。",
        "from": "T20n1100_001:0448a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一人誦吉慶讚。",
        "from": "T20n1100_001:0448a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一人誦吉祥讚。",
        "from": "T20n1100_001:0448a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令知法弟子三五人。",
        "from": "T20n1100_001:0448a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一人持香爐燒安悉香。",
        "from": "T20n1100_001:0448a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其香以葉衣觀自在菩薩心真言。",
        "from": "T20n1100_001:0448a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持一百八遍。",
        "from": "T20n1100_001:0448a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後取燒香煙勿令斷絕。",
        "from": "T20n1100_001:0448a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一人持賢瓶。",
        "from": "T20n1100_001:0448a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿盛香水插華菓樹枝。",
        "from": "T20n1100_001:0448a25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令持瓶人在前先行引像。",
        "from": "T20n1100_001:0448a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二人吹䗍。",
        "from": "T20n1100_001:0448a26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "引入王宮有旋一匝。",
        "from": "T20n1100_001:0448a26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南門而出復從東。",
        "from": "T20n1100_001:0448a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入却遶城內坊市一匝。",
        "from": "T20n1100_001:0448a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便城南門出。",
        "from": "T20n1100_001:0448a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "城南門外置一大水瓶。",
        "from": "T20n1100_001:0448a28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中置種種飲食雜菓及󰉓。",
        "from": "T20n1100_001:0448a29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿闍梨誦妙色身如來真言加持七遍。",
        "from": "T20n1100_001:0448b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後誦葉衣觀自在菩薩陀羅尼七遍。",
        "from": "T20n1100_001:0448b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於真言句中。",
        "from": "T20n1100_001:0448b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱國王名號加持。",
        "from": "T20n1100_001:0448b02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願國王界無諸災難。",
        "from": "T20n1100_001:0448b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後於路側曠野。",
        "from": "T20n1100_001:0448b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "棄擲水瓶令破。",
        "from": "T20n1100_001:0448b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是告言。",
        "from": "T20n1100_001:0448b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "閻魔界中行病鬼等。",
        "from": "T20n1100_001:0448b04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等受領此飲食。",
        "from": "T20n1100_001:0448b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復道而歸。",
        "from": "T20n1100_001:0448b05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸有情起大慈心。",
        "from": "T20n1100_001:0448b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令此國界無諸災難。",
        "from": "T20n1100_001:0448b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法欲求長壽無病者。",
        "from": "T20n1100_001:0448b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意大小。",
        "from": "T20n1100_001:0448b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "㲲上畫葉衣觀自在菩薩像。",
        "from": "T20n1100_001:0448b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於施願手下。",
        "from": "T20n1100_001:0448b08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫彼男女。",
        "from": "T20n1100_001:0448b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其像置道場中。",
        "from": "T20n1100_001:0448b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每日香華飲食旋繞供養發願。",
        "from": "T20n1100_001:0448b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常得加持滿其所願。",
        "from": "T20n1100_001:0448b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若國王男女難長難養。",
        "from": "T20n1100_001:0448b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或短壽疾病纏眠寢食不安。",
        "from": "T20n1100_001:0448b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆由宿業因緣生惡宿直。",
        "from": "T20n1100_001:0448b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或數被五曜陵逼本宿。",
        "from": "T20n1100_001:0448b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令身不安。",
        "from": "T20n1100_001:0448b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則於所居之處。",
        "from": "T20n1100_001:0448b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用牛黃或紙或素。",
        "from": "T20n1100_001:0448b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上書二十八大藥叉將真言。",
        "from": "T20n1100_001:0448b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帖四壁上。",
        "from": "T20n1100_001:0448b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先於東方壁上。",
        "from": "T20n1100_001:0448b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帖四大藥叉將真言。",
        "from": "T20n1100_001:0448b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從東北角起首。",
        "from": "T20n1100_001:0448b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂第一藥叉將真言曰。",
        "from": "T20n1100_001:0448b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵儞引囉伽二合吒抧吽弱娑嚩二合引訶第二藥叉將真言曰。",
        "from": "T20n1100_001:0448b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵蘇寗怛囉二合吒抧吽弱娑嚩二合訶第三藥叉將真言曰。",
        "from": "T20n1100_001:0448b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵布囉拏二合迦吒抧吽弱娑嚩二合訶第四藥叉將真言曰。",
        "from": "T20n1100_001:0448b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵迦比攞吒抧吽弱娑嚩二合訶次於南方壁上。",
        "from": "T20n1100_001:0448b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貼四大藥叉將真言。",
        "from": "T20n1100_001:0448b25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一藥叉將真言曰。",
        "from": "T20n1100_001:0448b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵僧伽吒抧吽弱娑嚩二合引訶引第二藥叉將真言曰。",
        "from": "T20n1100_001:0448b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵塢波僧伽吒抧吽弱娑嚩二合引訶引第三藥叉將真言曰。",
        "from": "T20n1100_001:0448c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵啇企羅吒抧吽弱娑嚩二合引訶引第四藥叉將真言曰。",
        "from": "T20n1100_001:0448c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵難上娜上吒抧吽弱娑嚩二合引訶引次於西方壁上。",
        "from": "T20n1100_001:0448c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帖四大藥叉將真言。",
        "from": "T20n1100_001:0448c05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一藥叉將真言曰。",
        "from": "T20n1100_001:0448c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵訶哩吒抧吽弱娑嚩二合引訶第二藥叉將真言曰。",
        "from": "T20n1100_001:0448c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵訶哩計奢吒抧吽弱娑嚩二合引訶引第三藥叉將真言曰。",
        "from": "T20n1100_001:0448c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵鉢囉二合僕吒抧吽弱娑嚩二合引訶引第四藥叉將真言曰。",
        "from": "T20n1100_001:0448c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵迦比羅吒抧吽弱娑嚩二合引訶引次於北方壁上。",
        "from": "T20n1100_001:0448c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帖四大藥叉將真言。",
        "from": "T20n1100_001:0448c14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一藥叉將真言曰。",
        "from": "T20n1100_001:0448c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵馱邏拏吒抧吽弱娑嚩二合引訶引第二藥叉將真言曰。",
        "from": "T20n1100_001:0448c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵馱邏難上那吒抧吽弱娑嚩二合引訶第三藥叉將真言曰。",
        "from": "T20n1100_001:0448c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵塢儞庾二合誐跛羅吒抧吽弱娑嚩二合引訶第四藥叉將真言曰。",
        "from": "T20n1100_001:0448c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵尾灑弩吒抧吽弱娑嚩二合引訶次於東北隅。",
        "from": "T20n1100_001:0449a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帖一大藥叉將真言曰。",
        "from": "T20n1100_001:0449a01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵半支迦吒抧吽弱娑嚩二合訶引次於東南隅。",
        "from": "T20n1100_001:0449a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帖一大藥叉將真言曰。",
        "from": "T20n1100_001:0449a03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵半左引羅巘拏吒抧吽弱娑嚩二合引訶引次於西南隅。",
        "from": "T20n1100_001:0449a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帖一大藥叉將真言曰。",
        "from": "T20n1100_001:0449a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵娑上跢儗哩吒抧吽弱娑嚩二合訶引次於西北隅。",
        "from": "T20n1100_001:0449a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帖一大藥叉將真言曰。",
        "from": "T20n1100_001:0449a07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵害麼嚩多吒抧吽弱娑嚩二合引訶次於下方足不踏處石上。",
        "from": "T20n1100_001:0449a09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鐫四大藥叉將真言。",
        "from": "T20n1100_001:0449a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置於四方地下。",
        "from": "T20n1100_001:0449a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東方地下一大藥叉將真言曰。",
        "from": "T20n1100_001:0449a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵步莫吒抧吽弱娑嚩二合訶南方地下一大藥叉將真言曰。",
        "from": "T20n1100_001:0449a13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵蘇步莫吒抧吽弱娑嚩二合訶西方地下一大藥叉將真言曰。",
        "from": "T20n1100_001:0449a15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵迦羅吒抧吽弱娑嚩二合訶北方地下一大藥叉將真言曰。",
        "from": "T20n1100_001:0449a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵塢波迦羅吒抧吽弱娑嚩二合引訶次於上方四隅舍上。",
        "from": "T20n1100_001:0449a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各帖一大藥叉將真言。",
        "from": "T20n1100_001:0449a19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東北隅舍上一大藥叉將真言曰。",
        "from": "T20n1100_001:0449a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵蘇哩也二合吒抧吽弱娑嚩二合引訶東南隅舍上一大藥叉將真言曰。",
        "from": "T20n1100_001:0449a22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿銀儞二合吒抧吽弱娑嚩二合引訶西南隅舍上一大藥叉將真言曰。",
        "from": "T20n1100_001:0449a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵蘇摩吒抧吽弱娑嚩二合引訶引西北隅舍上一大藥叉將真言曰。",
        "from": "T20n1100_001:0449a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩庾吒抧吽弱娑嚩二合引訶帖真言已。",
        "from": "T20n1100_001:0449a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於二十八大藥叉將位。",
        "from": "T20n1100_001:0449a28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各各以香塗一小壇。",
        "from": "T20n1100_001:0449a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壇上燒香雜華。",
        "from": "T20n1100_001:0449a29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲食燈燭閼伽虔誠啟告。",
        "from": "T20n1100_001:0449b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願二十八大藥叉將并諸眷屬。",
        "from": "T20n1100_001:0449b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各住本方護持守護某甲令除災禍不祥疾病夭壽。",
        "from": "T20n1100_001:0449b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲得色力增長聰慧。",
        "from": "T20n1100_001:0449b03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "威肅端嚴具足。",
        "from": "T20n1100_001:0449b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "易養易長壽命長遠。",
        "from": "T20n1100_001:0449b04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是加持已。",
        "from": "T20n1100_001:0449b04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十八大藥叉將不敢違越諸佛。",
        "from": "T20n1100_001:0449b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如觀自在菩薩及金剛手菩薩教勅。",
        "from": "T20n1100_001:0449b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晝夜擁護臥安覺安獲大威德。",
        "from": "T20n1100_001:0449b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有國王作此法者。",
        "from": "T20n1100_001:0449b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其王境內災疾消滅。",
        "from": "T20n1100_001:0449b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國土安寧人民歡樂。",
        "from": "T20n1100_001:0449b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法應畫本生宿直。",
        "from": "T20n1100_001:0449b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每月供養。",
        "from": "T20n1100_001:0449b09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作如是法者。",
        "from": "T20n1100_001:0449b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡宿直轉成吉祥。",
        "from": "T20n1100_001:0449b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以白檀香刻作葉衣觀自在菩薩像。",
        "from": "T20n1100_001:0449b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并於樺皮上書此真言。",
        "from": "T20n1100_001:0449b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共帶。",
        "from": "T20n1100_001:0449b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作此法取鬼宿直日受灌頂。",
        "from": "T20n1100_001:0449b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其灌頂瓶以繒繫項。",
        "from": "T20n1100_001:0449b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿盛香水。",
        "from": "T20n1100_001:0449b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水中著七寶及五種藥。",
        "from": "T20n1100_001:0449b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂娑訶者囉等。",
        "from": "T20n1100_001:0449b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娑訶泥嚩。",
        "from": "T20n1100_001:0449b14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建吒迦哩。",
        "from": "T20n1100_001:0449b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿哩訶底。",
        "from": "T20n1100_001:0449b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儗哩羯囉拏。",
        "from": "T20n1100_001:0449b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及五種子諸香等。",
        "from": "T20n1100_001:0449b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以葉衣觀自在陀羅尼。",
        "from": "T20n1100_001:0449b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持一百八遍。",
        "from": "T20n1100_001:0449b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以用灌頂洗諸障難。",
        "from": "T20n1100_001:0449b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灌頂已。",
        "from": "T20n1100_001:0449b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取一瓦椀盛種種飲食。",
        "from": "T20n1100_001:0449b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼男女頭上遶三匝。",
        "from": "T20n1100_001:0449b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令一知法遠送擲破。",
        "from": "T20n1100_001:0449b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結線索。",
        "from": "T20n1100_001:0449b19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以葉衣真言加持繫其頭上。",
        "from": "T20n1100_001:0449b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作如是法。",
        "from": "T20n1100_001:0449b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身上疾病鬼魅󰚏禱。",
        "from": "T20n1100_001:0449b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執曜淩逼本命宿所皆悉殄滅。",
        "from": "T20n1100_001:0449b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若人疫病。",
        "from": "T20n1100_001:0449b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取舍彌木此國無取楮木替之燃火然後酥護摩。",
        "from": "T20n1100_001:0449b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人髮人骨投一百八遍於火中燒。",
        "from": "T20n1100_001:0449b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七日已來每日供養。",
        "from": "T20n1100_001:0449b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "葉衣觀自在菩薩。",
        "from": "T20n1100_001:0449b25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護摩之時。",
        "from": "T20n1100_001:0449b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱彼國城名聚落名村坊名。",
        "from": "T20n1100_001:0449b26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切災難悉皆除滅。",
        "from": "T20n1100_001:0449b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是象疫馬疫牛疫水牛疫。",
        "from": "T20n1100_001:0449b27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各取本類骨本毛。",
        "from": "T20n1100_001:0449b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作護摩七日七夜亦皆災滅。",
        "from": "T20n1100_001:0449b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法取一瓶滿盛香水。",
        "from": "T20n1100_001:0449c01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言加持一百八遍以水淋彼畜。",
        "from": "T20n1100_001:0449c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切疫病悉皆消滅。",
        "from": "T20n1100_001:0449c02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若人頭痛。",
        "from": "T20n1100_001:0449c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取有香氣華加持一百八遍。",
        "from": "T20n1100_001:0449c03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令鼻嗅即得除愈。",
        "from": "T20n1100_001:0449c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若人患鬼。",
        "from": "T20n1100_001:0449c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魅取粳米粉。",
        "from": "T20n1100_001:0449c05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "担作彼魅形。",
        "from": "T20n1100_001:0449c05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以鑌刀誦段段截之。",
        "from": "T20n1100_001:0449c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七日護摩即得除差。",
        "from": "T20n1100_001:0449c06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若人患瘧。",
        "from": "T20n1100_001:0449c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一日二日三日。",
        "from": "T20n1100_001:0449c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至七日或長時。",
        "from": "T20n1100_001:0449c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "患瘧用牛黃書此真言。",
        "from": "T20n1100_001:0449c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戴即得除差。",
        "from": "T20n1100_001:0449c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法或嬰孩鬼魅。",
        "from": "T20n1100_001:0449c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書此真言帶則得除愈。",
        "from": "T20n1100_001:0449c10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法劫賊侵奪坊市村邑。",
        "from": "T20n1100_001:0449c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或欲遠遊路畏劫盜取佉陀羅木末。",
        "from": "T20n1100_001:0449c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護摩誦真言一百八遍。",
        "from": "T20n1100_001:0449c12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所去之處無諸障難。",
        "from": "T20n1100_001:0449c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若虫食苗稼。",
        "from": "T20n1100_001:0449c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取砂以真言加持一百八遍。",
        "from": "T20n1100_001:0449c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散於田中虫自遠去五穀豐熟。",
        "from": "T20n1100_001:0449c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "葉衣觀自在菩薩經",
        "from": "T20n1100_001:0449c16_100",
        "context_with_next_sentence": false
    }
]