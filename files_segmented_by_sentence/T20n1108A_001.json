[
    {
        "source_sentence": "聖救度佛母二十一種禮讚經",
        "from": "T20n1108A_001:0478c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翰林學士承旨中奉大夫",
        "from": "T20n1108A_001:0478c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安藏奉　詔譯",
        "from": "T20n1108A_001:0478c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵敬禮多哩速疾勇　　咄多哩者除怖畏",
        "from": "T20n1108A_001:0478c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咄哩能授諸勝義　　具莎訶字我讚禮",
        "from": "T20n1108A_001:0478c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮救度速勇母　　目如剎那電光照",
        "from": "T20n1108A_001:0478c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三世界尊蓮華面　　從妙華中現端嚴",
        "from": "T20n1108A_001:0478c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮百秋朗月母　　普遍圓滿無垢面",
        "from": "T20n1108A_001:0478c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如千星宿俱時聚　　殊勝威光超於彼",
        "from": "T20n1108A_001:0478c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮紫磨金色母　　妙蓮華手勝莊嚴",
        "from": "T20n1108A_001:0478c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施精勤行柔善靜　　忍辱禪定性無境",
        "from": "T20n1108A_001:0478c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮如來頂髻母　　最勝能滿無邊行",
        "from": "T20n1108A_001:0479a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得到彼岸盡無餘　　勝勢佛子極所愛",
        "from": "T20n1108A_001:0479a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮怛囉吽字母　　聲愛方所滿虛空",
        "from": "T20n1108A_001:0479a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運足遍履七世界　　悉能鉤召攝無餘",
        "from": "T20n1108A_001:0479a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮釋梵火天母　　風神自在眾俱集",
        "from": "T20n1108A_001:0479a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "部多起屍尋香等　　諸藥叉眾作稱歎",
        "from": "T20n1108A_001:0479a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮特囉胝發母　　於他加行極摧壞",
        "from": "T20n1108A_001:0479a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "展左踡右作足踏　　頂髻熾盛極明耀",
        "from": "T20n1108A_001:0479a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮都哩大緊母　　勇猛能摧怨魔類",
        "from": "T20n1108A_001:0479a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於蓮華面作顰眉　　摧壞一切冤家眾",
        "from": "T20n1108A_001:0479a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮三寶嚴印母　　手指當心威嚴相",
        "from": "T20n1108A_001:0479a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚴飾方輪盡無餘　　自身熾盛光聚種",
        "from": "T20n1108A_001:0479a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮威德歡悅母　　寶冠珠鬘眾光飾",
        "from": "T20n1108A_001:0479a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最極喜笑覩怛哩　　鎮世間魔作攝伏",
        "from": "T20n1108A_001:0479a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮守護眾地母　　亦能鉤召諸神眾",
        "from": "T20n1108A_001:0479a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "搖顰眉面吽聲字　　一切衰敗令度脫",
        "from": "T20n1108A_001:0479a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮頂冠月相母　　冠中現勝妙嚴光",
        "from": "T20n1108A_001:0479a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿彌陀佛髻中現　　常放眾妙寶光明",
        "from": "T20n1108A_001:0479a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮如盡劫火母　　安住熾盛頂髻中",
        "from": "T20n1108A_001:0479a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普遍喜悅半趺坐　　能摧滅壞惡冤輪",
        "from": "T20n1108A_001:0479a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮手按大地母　　以足踐蹋作鎮壓",
        "from": "T20n1108A_001:0479a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現顰眉面作吽聲　　能破七險鎮降伏",
        "from": "T20n1108A_001:0479a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮安隱柔善母　　涅槃寂滅最樂境",
        "from": "T20n1108A_001:0479a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莎訶命種以相應　　善能消滅大災禍",
        "from": "T20n1108A_001:0479a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮普遍極喜母　　諸怨支體令脫離",
        "from": "T20n1108A_001:0479a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十字呪句妙嚴布　　明呪吽聲常朗耀",
        "from": "T20n1108A_001:0479a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮都哩巴帝母　　足躡相勢吽字種",
        "from": "T20n1108A_001:0479a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彌嚕曼陀結辣薩　　於此三處能搖動",
        "from": "T20n1108A_001:0479a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮薩囉天海母　　手中執住神獸像",
        "from": "T20n1108A_001:0479a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦二怛囉作發聲　　能滅諸毒盡無餘",
        "from": "T20n1108A_001:0479b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮諸天集會母　　天緊那羅所依愛",
        "from": "T20n1108A_001:0479b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "威德歡悅若堅鎧　　滅除鬪諍及惡夢",
        "from": "T20n1108A_001:0479b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮日月廣圓母　　目覩猶勝普光照",
        "from": "T20n1108A_001:0479b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦二喝囉咄怛哩　　善除惡毒瘟熱病",
        "from": "T20n1108A_001:0479b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮具三真實母　　善靜威力皆具足",
        "from": "T20n1108A_001:0479b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥叉執魅尾怛辣　　都哩最極除災禍",
        "from": "T20n1108A_001:0479b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有智者勤精進　　至心誦此二十一",
        "from": "T20n1108A_001:0479b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救度尊處誠信禮　　是故讚嘆根本呪",
        "from": "T20n1108A_001:0479b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每晨旦起夕時禮　　憶念施諸勝無畏",
        "from": "T20n1108A_001:0479b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切罪業盡消除　　悉能超越諸惡趣",
        "from": "T20n1108A_001:0479b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此等速能得聰慧　　七俱胝佛所灌頂",
        "from": "T20n1108A_001:0479b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現世富貴壽延安　　當來趣向諸佛位",
        "from": "T20n1108A_001:0479b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有時誤服諸毒物　　或自然生或合成",
        "from": "T20n1108A_001:0479b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憶念聖尊真實力　　諸惡毒藥盡消滅",
        "from": "T20n1108A_001:0479b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或見他人遭鬼魅　　或發熱病受諸苦",
        "from": "T20n1108A_001:0479b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若轉此讚二三七　　彼諸苦惱悉蠲除",
        "from": "T20n1108A_001:0479b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲乞男女得男女　　求財寶位獲富饒",
        "from": "T20n1108A_001:0479b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善能圓滿隨意願　　一切障礙不能侵",
        "from": "T20n1108A_001:0479b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "根本十字真言􇩂唵􄕗多引􄙄哩􄕛咄入􄕗多􄙄哩􄕛都􄙄哩􅁥莎引􄚩訶引救度八難真言􇩂唵􄕘多引􄙄哩􄕛咄􄕗多􄙄哩􄕛都􄙄哩􄙺薩􅞣哩嚩二合􄖵琶􄗱耶􄖩那引􄑼舍􄖩儞􄙺薩􅞣哩嚩二合􄕺覩入􄑔枯入􄕘多引􄙄哩􄕍禰􅁥莎引􄚩訶引聖救度佛母二十一種禮讚經",
        "from": "T20n1108A_001:0479c01_100",
        "context_with_next_sentence": false
    }
]