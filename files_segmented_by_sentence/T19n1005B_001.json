[
    {
        "source_sentence": "寶樓閣經梵字真言",
        "from": "T19n1005B_001:0634b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "驚覺。",
        "from": "T19n1005B_001:0634b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇖵􄖩􄗰􄙺􅞣􄕗􄕤􄑘􄕝􄢢􄗫􄕙􆵕􄕙",
        "from": "T19n1005B_001:0634b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄕯􈇌􄕱􄙨􈁴􄗥􄕍􄙖􄦽􄷩􄕯􄗱􄙖􄧃􄗦􄗽􄚣􄡈􄙘􄩷􄖵􄙒􄚨􄖩􄙖􄦽􄑘􅝑􄩟􄙺􄗱􄩟􄙺􄗱􄙺􅞣􄗦􄗽􄗙􄙖􄖪􄖫􈋶􈋶􄚦􄕻􄗽􄙺􄕻􄗽􈎭􇧺􄗬􄩡􄙺􅞣􄕗􄕤􄑘􄕗驚覺。",
        "from": "T19n1005B_001:0634b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄙺􅞣􄕗􄕤􄑘􄕗􄗥􄕍􄙢􄩠􄕱􆻬􄺸􄙐􄺸􄙐􄕻􈆺􄕼􄕛􄑘􅝗􄗥􄕍􄗥􄕍􄗥􄚩􄗥􄕍􄕗􄕤􄑘􄕗􄷩􈇪􄕍􅁥􄚩􇖴根本。",
        "from": "T19n1005B_001:0634b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄖩􄗰􄙺􅞣􄕗􄕤􄑘􄕗􈁀􈁴􄙘􄖹􄙊􄑘􅝗􄗥􄕍􄪼􄗟􄕗􄕤􄕘􄖫􄕯􈋧􄖯􄗥􄕍􄗥􄕍􈂼􄪼􄗟􄙘􄗥􄙐􄙻􄑘􄗽􄑘􆫏􄙄􈅼􈅼􄺸􄙊􄺸􄙊􈎭􇧺􄙘􄙒􄑂􄕝􄙦􄡡􄕽􆵍􄕗􄑘􅝗􅁥􄚩􇖴心及隨心。",
        "from": "T19n1005B_001:0634b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄗥􄕍􄙖􄧅􈅼􇖴隨心。",
        "from": "T19n1005B_001:0634c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄗥􄕍􄾫􄙄􈅼􄗁􈁆坐。",
        "from": "T19n1005B_001:0634c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄗥􄕍􄑄􆧽􄙌􆿁􆿁􅁥􄚩􇖴結壇界。",
        "from": "T19n1005B_001:0634c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄗥􄕍􄙖􄒶􄗷􄕻􄗽􄕻􄗽􆿁􅁥􄚩􇖴結十方界。",
        "from": "T19n1005B_001:0634c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄺸􄙌􄕗􄗥􄕍􄙂􄑾􄗾􄣊􈍇􆿁􆿁􄗁􈁆􇖴碎毘那夜迦。",
        "from": "T19n1005B_001:0634c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄗥􄕍􄪼􄗚􄙖􄕙􄚨􄗽􄚨􄗽􆿁􆿁􄗁􈁆􅁥􄚩􇖴頂髻。",
        "from": "T19n1005B_001:0634c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄙖􄦽􄗥􄕍􄕙􆵋􄕙􆵋􆿁􆿁􄗁􈁆􇖴加持衣。",
        "from": "T19n1005B_001:0634c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄗥􄕍􄙘􄖹􄙐􄕽􄙀􄕽􄙀􆿁􄗁􈁆􇖴洗嗽。",
        "from": "T19n1005B_001:0634c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄙘􄝁􄙖􄕙􄚨􄗽􄚨􄗽􄗥􄚩􄗥􄕍􆿁􆿁􄗁􈁆􇖴洗浴。",
        "from": "T19n1005B_001:0634c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􈂼􄖫􅝝􄙊􄙖􄕙􄚨􄗽􄚨􄗽􄖶􄖿􄗧􄙌􄗧􆿁􅁥􄚩􇖴法護。",
        "from": "T19n1005B_001:0634c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄗥􄕍􄾬􄙀􆿁􆿁􄗁􈁆􇖴神線。",
        "from": "T19n1005B_001:0634c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄕽􄙀􄕽􄙀􄗥􄕋􄑀􄙀􆿁􆿁􄗁􈁆􇖴獻花。",
        "from": "T19n1005B_001:0634c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄙺􅞣􄕗􄕤􄑘􄕗􈁂􄒶􄗥􄕍􆿁􆿁􇖴塗香。",
        "from": "T19n1005B_001:0635a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄙺􅞣􄕗􄕤􄑘􄕗􄑘􄾫􄗥􄕍􆷱􄗽􄕋􆿁􇖴燒香。",
        "from": "T19n1005B_001:0635a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄺸􄙌􄕗􄗥􄕍􆿅􄫦􄑄􄓚􆷱􄗽􄕋􄙘􄑘􄕙􆿁􇖴燈。",
        "from": "T19n1005B_001:0635a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄺸􄙌􄕗􄙤􄑘􄙄􄕻􄙖􄙊􆿁􆿁􄗁􈁆􇖴食。",
        "from": "T19n1005B_001:0635a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄪼􄙖􄗾􄥟􄙖􄕙􄙺􄗽􄙺􄗽􆿁􆿁􇖴閼伽。",
        "from": "T19n1005B_001:0635a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄗥􄚩􄗥􄕍􈁂􄗽􄗱􄕻􄗽􄕻􄗽􆿁􆿁􇖴供物及食等。",
        "from": "T19n1005B_001:0635a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄺸􄙊􆷵􄗽􄑘􄑘􄖩􄪼􄕘􄗽􄕍􆿁􆿁􇖴加持念珠。",
        "from": "T19n1005B_001:0635a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄙂􄑾􄗽􄗥􄕍􄪼􄙖􇪱􄗱􆿁念誦先誦此真言。",
        "from": "T19n1005B_001:0635a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄙖􄦽􄗥􄕍􄑀􄗽􈆡􄑀􄙀􆿁􆿁􄗁􈁆􇖴結趺坐。",
        "from": "T19n1005B_001:0635a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􈂼􄪼􄙖􈃪􄕗􄙜􄑞􄗥􄕍􄗥􄕍􅁥􄚩􇖴驚覺一切如來。",
        "from": "T19n1005B_001:0635a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄙺􅞣􄕗􄕤􄑘􄕗􄙗􄗵􄒶􄙖􄕗􄗽􄕗􄗽􆿁􄗥􄕍􄑁􄖩􄖯􅁥􄚩􇖴請一切如來。",
        "from": "T19n1005B_001:0635a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􈂼􄙘􄪼􄙊􄪼􄙖􄙄􈃁􄙂􈃁􄙂􆿁􆿁􇖴求願。",
        "from": "T19n1005B_001:0635b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄙺􅞣􄕗􄕤􄑘􄕘􄕽􆵋􄖩􄙘􆽕􄙺􄗽􄚴􄕋􄙖􄧃􆿁􆿁􇖴求菩薩願。",
        "from": "T19n1005B_001:0635b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􈂼􄙘􄖹􄙊􄙖􄕯􄖯􄚨􄗽􄚨􄗽􆿁􆿁􇖴請一切天龍。",
        "from": "T19n1005B_001:0635b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􆿅􄗛􄙺􄗥􄗱􄙖􄧃􄕻􄗽􄕻􄗽􆿁􇖴請四天王等。",
        "from": "T19n1005B_001:0635b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄗥􄕍􄙜􄑘􄙖􄕙􆿁􇖴加持弟子。",
        "from": "T19n1005B_001:0635b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄙦􄗙􄗥􄕍􈂪􄙊􈂪􄙊􆿁􇖴令弟子入壇。",
        "from": "T19n1005B_001:0635b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄙺􅞣􄕗􄕤􄑘􄕗􄷩􄕯􄗱􄙖􄦿􄕍􄕻􄗾􄕻􄗾􆿁􆿁􇖴獻一切佛一切菩薩諸天等食。",
        "from": "T19n1005B_001:0635b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄙙􄗽􄒼􄑘􄑘􄖩􄙗􄚪􄕙􄙊􈂪􄙊􈂪􆿁􆿁􇖴訶送諸聖眾。",
        "from": "T19n1005B_001:0635b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄙺􅞣􄕗􄕤􄑘􄕗􄙖􄦽􄑄􄙒􄕱􄕝􅋌􄗽􄕍􄙘􄑘􄕝􄺸􄙊􄺸􄙊􆿁􆿁􅁥􄚩􇖴獻物護身。",
        "from": "T19n1005B_001:0635b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄗥􄕍􈂱􄗙􄖫􄙜􄑘􄙖􄕙􄗽􄚴􄗽􄚴􄗯􆿁􇖴為弟子灌頂。",
        "from": "T19n1005B_001:0635b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄗥􄚩􄙘􄖹􄙊􄪼􄕙􆵍􄕗􄙼􇧼􆿅􄗛􄙰􄑼􄗯􄙺􅞣􄕗􄕤􄑘􄕘􄗛􄙴􈇽􄗙􄗽􄗙􄗽􄚦􄗙􆿁􄚦􄗙􄗽􆿁􆿁􇖴法供養護摩。",
        "from": "T19n1005B_001:0635c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􅁥􄚩􄖵􄕙􄗞􅝕􄙡􆿁􆿁􄗁􈁆􇖴我今悉皆禮。",
        "from": "T19n1005B_001:0635c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄗥􄖫􄗥􄕍􄪼􄙖􄗽􄪼􄙖􄗽􄑜􈃃􄖵􅇟􄗥􄚩􄪼􄗟􅁥􄚩普遍光明如來心。",
        "from": "T19n1005B_001:0635c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄙺􅞣􄕗􄕤􄑘􄕘􄷩􄕯􄗱􄗥􄕍􄺸􄙊􄕝􆿆􄙘􆵍􄗱􆿁􇖴一切如來心印。",
        "from": "T19n1005B_001:0635c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄙺􅞣􄕗􄕤􄑘􄕗􄪼􄙖􄗽􄙂􄥟􄗥􄕍􈊹􇖴一切如來普遍大寶三昧耶。",
        "from": "T19n1005B_001:0635c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄙺􅞣􄕗􄕤􄑘􄕘􄗛􄚦􄙞􄕻􄖩􄙖􄧃􈊹􆿁􇖴一切如來莊嚴大寶光。",
        "from": "T19n1005B_001:0635c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄙺􅞣􄕗􄕤􄑘􄕗􄕽􆵌􄖩􄗥􄕍􄗥􄕍􈊹􆿁􄗁􈁆􇖴金剛師子座。",
        "from": "T19n1005B_001:0635c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄙺􅞣􄕗􄕤􄑘􄕗􄪼􄙖􄗽􄗥􄕍􄗽􄑾􄙄􆿁􈊹􄗁􈁆􇖴大寶出生灌頂。",
        "from": "T19n1005B_001:0635c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄙺􅞣􄕗􄕤􄑘􄕗􄙘􄗥􄙊􄚦􄗙􄙜􆿁􆿁􇖴摧魔熾然法輪神通加持。",
        "from": "T19n1005B_001:0635c25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄙺􅞣􄕘􄕤􄑘􄕗􄙺􄗥􄗱􄗥􄕍􄙖􄧃􆿁􆿁􇖴無能勝。",
        "from": "T19n1005B_001:0635c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄙺􅞣􄕘􄕤􄑘􄕘􄒶􄗱􆿅􄒸􄕗􄙖􄧃􆿁􆿁􇖴一切如來轉法輪。",
        "from": "T19n1005B_001:0636a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄙺􅞣􄕗􄕤􄑘􄕗􄕻􈆺􄕼􄕛􄗥􄚩􄗥􄕍􄙤􄑌􄙄􄚨􄗽􄚨􈊹􈊹􄗁􈁆􇖴金剛手。",
        "from": "T19n1005B_001:0636a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄙺􅞣􄕗􄕤􄑘􄕗􄗥􄚩􄙖􄦾􄺸􄗽􄕻􄗽􄕻􄗽􆿁􆿁􄗁􈁆􇖴寶金剛菩薩。",
        "from": "T19n1005B_001:0636a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􈃁􄙂􈃁􄙂􄗥􄕍􄗥􄕍􄗥􄚩􄗥􄕍􄙘􄝁􄟗􄕍􅁥􄚩􇖴四大天主。",
        "from": "T19n1005B_001:0636a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄙒􄑆􄖶􄙌􄕝􄒶􄗱􄒶􄗱􆿁􇖴吉祥天女。",
        "from": "T19n1005B_001:0636a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄙘􄗥􄗾􄥟􄙖􄕙􄙺􄗙􄗽􆿁鈎棄尼天女。",
        "from": "T19n1005B_001:0636a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄕹􈃰􄕍􄙘􄙺􄗽􆿁􇖴使者天女。",
        "from": "T19n1005B_001:0636a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􆿆􄑘􄗥􄗱􄕾􄗽􄗯􄛽􆽭􄚩􄙼􄖫􆿁􇖴曼荼羅中一切聖眾。",
        "from": "T19n1005B_001:0636a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴􄙺􄗥􆩗􄑁􄙊􄖵􄙀􄪼􄗽􄕍􄕻􄑀􄕻􄑀􆿁􆿁􇖴花齒天女。",
        "from": "T19n1005B_001:0636a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "御本書寫　　本云以根本御本書之云",
        "from": "T19n1005B_001:0636a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　　　　　　　金剛　　顯耀",
        "from": "T19n1005B_001:0636a28_0",
        "context_with_next_sentence": true
    }
]