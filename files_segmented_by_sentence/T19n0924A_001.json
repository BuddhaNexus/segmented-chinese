[
    {
        "source_sentence": "藥師如來念誦儀軌一卷",
        "from": "T19n0924A_001:0029b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大興善寺三藏沙門大廣智",
        "from": "T19n0924A_001:0029b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空奉　詔譯",
        "from": "T19n0924A_001:0029b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛一時在維耶離樂音樹下。",
        "from": "T19n0924A_001:0029b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大菩薩三萬六千比丘八萬人俱。",
        "from": "T19n0924A_001:0029b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及十二神王并諸眷屬天龍八部大神王。",
        "from": "T19n0924A_001:0029b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在如是等大眾會中。",
        "from": "T19n0924A_001:0029b09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此法已。",
        "from": "T19n0924A_001:0029b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大眾皆聞無不歡喜。",
        "from": "T19n0924A_001:0029b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名即為結願神呪即薄伽梵。",
        "from": "T19n0924A_001:0029b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說呪曰。",
        "from": "T19n0924A_001:0029b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南謨薄伽筏帝鞞殺社窶嚕蔽瑠璃鉢喇婆喝囉闍耶怛他揭多怛姪他唵鞞殺逝鞞殺逝鞞殺社三沒揭帝娑婆訶若有受持此真言。",
        "from": "T19n0924A_001:0029b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能拔身中過去生死一切重罪。",
        "from": "T19n0924A_001:0029b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不復經歷三途。",
        "from": "T19n0924A_001:0029b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "免離九橫超越眾苦。",
        "from": "T19n0924A_001:0029b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方世界隨處安樂。",
        "from": "T19n0924A_001:0029b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自在無礙法應如是。",
        "from": "T19n0924A_001:0029b17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人等。",
        "from": "T19n0924A_001:0029b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持讀誦是真言者。",
        "from": "T19n0924A_001:0029b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日夜精勤香湯洗浴著新淨衣。",
        "from": "T19n0924A_001:0029b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持諸禁戒如法。",
        "from": "T19n0924A_001:0029b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦滿真言十萬遍已。",
        "from": "T19n0924A_001:0029b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就清淨處如法治地。",
        "from": "T19n0924A_001:0029b20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以淨土築令平。",
        "from": "T19n0924A_001:0029b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以淨牛糞和檀香塗圓壇。",
        "from": "T19n0924A_001:0029b21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以種種雜寶莊嚴壇。",
        "from": "T19n0924A_001:0029b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安中心一藥師如來像如來左手令執藥器。",
        "from": "T19n0924A_001:0029b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名無價珠。",
        "from": "T19n0924A_001:0029b23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手令作結三界印。",
        "from": "T19n0924A_001:0029b23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一著袈裟結跏趺坐。",
        "from": "T19n0924A_001:0029b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令安蓮華臺。",
        "from": "T19n0924A_001:0029b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臺下十二神將。",
        "from": "T19n0924A_001:0029b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八萬四千眷屬上首。",
        "from": "T19n0924A_001:0029b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令　又令須蓮臺。",
        "from": "T19n0924A_001:0029b25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來威光中令住日光月光二菩薩。",
        "from": "T19n0924A_001:0029b26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是壇四方周匝五色。",
        "from": "T19n0924A_001:0029b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "近前安置二閼伽器商佉瓦器。",
        "from": "T19n0924A_001:0029b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意受用奉獻承事尊像。",
        "from": "T19n0924A_001:0029b28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "像前念誦四十九日間。",
        "from": "T19n0924A_001:0029b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若三七日。",
        "from": "T19n0924A_001:0029b29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫像壇四角安置賢瓶。",
        "from": "T19n0924A_001:0029b29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是晝夜七日內。",
        "from": "T19n0924A_001:0029c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦呪數滿百千萬遍。",
        "from": "T19n0924A_001:0029c01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求從心無量獲果報。",
        "from": "T19n0924A_001:0029c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除不至心。",
        "from": "T19n0924A_001:0029c02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法應如是。",
        "from": "T19n0924A_001:0029c02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有利益說不可盡。",
        "from": "T19n0924A_001:0029c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其餘功能窮劫不可說。",
        "from": "T19n0924A_001:0029c03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次說藥師如來根本印。",
        "from": "T19n0924A_001:0029c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左右手頭指以下八指。",
        "from": "T19n0924A_001:0029c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "反叉入於掌。",
        "from": "T19n0924A_001:0029c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大指來去。",
        "from": "T19n0924A_001:0029c05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T19n0924A_001:0029c05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵戰馱祇哩娑婆訶是名根本印次說閼伽印。",
        "from": "T19n0924A_001:0029c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手掌捧器頂戴。",
        "from": "T19n0924A_001:0029c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924A_001:0029c07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三曼多沒馱南羯羯哞三摩三摩娑婆訶次花座真言印　次說塗香印　二手合掌二頭指二大指端合如彈指。",
        "from": "T19n0924A_001:0029c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924A_001:0029c10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三曼多沒馱南巘吐羯羅耶娑婆訶次說花印　二手合掌二小開末。",
        "from": "T19n0924A_001:0029c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924A_001:0029c12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三曼多沒馱南沒社華娑婆訶次說燒香印　二手合掌山屈。",
        "from": "T19n0924A_001:0029c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924A_001:0029c14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵那謨仡羅仡羅曼拏微灑曳尸并寧娑婆訶次說花座印　二手背合以右腕押左真言曰。",
        "from": "T19n0924A_001:0029c16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵社皤羅娑蒲悉什皤羅末吒阿次說普供養印　二手金剛合掌。",
        "from": "T19n0924A_001:0029c18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924A_001:0029c18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三曼多沒馱南唵阿凡賀布灑摩尼娑婆訶次念誦　次說發遣　以禪智指取花投壇中方。",
        "from": "T19n0924A_001:0029c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924A_001:0029c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三曼多沒馱南阿蘖磋蘖磋娑婆訶是法印呪。",
        "from": "T19n0924A_001:0029c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能滅一切苦惱。",
        "from": "T19n0924A_001:0029c24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等。",
        "from": "T19n0924A_001:0029c24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多諸罪障。",
        "from": "T19n0924A_001:0029c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸婦女。",
        "from": "T19n0924A_001:0029c25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願欲轉禍。",
        "from": "T19n0924A_001:0029c25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依教作藥師像一軀。",
        "from": "T19n0924A_001:0029c25_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寫藥師經一卷。",
        "from": "T19n0924A_001:0029c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造幡四十九燈作七層形如車輪。",
        "from": "T19n0924A_001:0029c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安置像前。",
        "from": "T19n0924A_001:0029c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五色作索。",
        "from": "T19n0924A_001:0029c27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印柱之。",
        "from": "T19n0924A_001:0029c27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四十九結繫彼人身。",
        "from": "T19n0924A_001:0029c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又轉藥師經四十九卷。",
        "from": "T19n0924A_001:0029c28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有罪障皆得解脫。",
        "from": "T19n0924A_001:0029c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽命延長不遇橫苦即得安穩。",
        "from": "T19n0924A_001:0030a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼神之病。",
        "from": "T19n0924A_001:0030a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并即除愈。",
        "from": "T19n0924A_001:0030a01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲降伏惡人者。",
        "from": "T19n0924A_001:0030a01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作印以嗔心誦一百八遍。",
        "from": "T19n0924A_001:0030a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遙打一切外道破滅。",
        "from": "T19n0924A_001:0030a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人患心病者。",
        "from": "T19n0924A_001:0030a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持青木香塗心除愈。",
        "from": "T19n0924A_001:0030a03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人患頭病者。",
        "from": "T19n0924A_001:0030a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持桂皮二十遍。",
        "from": "T19n0924A_001:0030a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "服之除愈。",
        "from": "T19n0924A_001:0030a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人患腫黃者。",
        "from": "T19n0924A_001:0030a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持欝金香一百八遍塗除愈。",
        "from": "T19n0924A_001:0030a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲遠去怨家。",
        "from": "T19n0924A_001:0030a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持苦練子一百八遍。",
        "from": "T19n0924A_001:0030a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一呪一燒投火中燒。",
        "from": "T19n0924A_001:0030a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人即遠去。",
        "from": "T19n0924A_001:0030a07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患身體支節痛。",
        "from": "T19n0924A_001:0030a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪湯水一百八遍。",
        "from": "T19n0924A_001:0030a08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洗浴即得除愈。",
        "from": "T19n0924A_001:0030a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人每日早朝。",
        "from": "T19n0924A_001:0030a09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以水一掬呪七遍飲之。",
        "from": "T19n0924A_001:0030a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在身所有惡報悉得消滅。",
        "from": "T19n0924A_001:0030a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況無諸災厄者。",
        "from": "T19n0924A_001:0030a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸三業苦亦得除愈。",
        "from": "T19n0924A_001:0030a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并得壽命長遠。",
        "from": "T19n0924A_001:0030a11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若呪飲喫者。",
        "from": "T19n0924A_001:0030a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸毒不能為損。",
        "from": "T19n0924A_001:0030a12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見惡人及有怨家。",
        "from": "T19n0924A_001:0030a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當須念誦此呪。",
        "from": "T19n0924A_001:0030a13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有怨家起惡心者。",
        "from": "T19n0924A_001:0030a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當皆降伏惡心即滅慈心相向。",
        "from": "T19n0924A_001:0030a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有恐怖處。",
        "from": "T19n0924A_001:0030a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當須攝心念誦。",
        "from": "T19n0924A_001:0030a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲臥時。",
        "from": "T19n0924A_001:0030a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當誦此呪一百八遍。",
        "from": "T19n0924A_001:0030a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得好夢善知吉凶。",
        "from": "T19n0924A_001:0030a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人或患瘧病。",
        "from": "T19n0924A_001:0030a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持此呪者視患瘧人。",
        "from": "T19n0924A_001:0030a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "切誦此呪一千八遍。",
        "from": "T19n0924A_001:0030a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其患即除愈。",
        "from": "T19n0924A_001:0030a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥師如來念誦儀軌一卷保元三年二月二日酉時書了　恭賜大緣房法印御本書了留贈後賢共期佛慧耳　求法沙門覺成延寶二甲寅年九月十四日以高山寺法鼓臺之本於仁和寺書寫一校了　野澤末資淨嚴　三十六歲天明元年辛丑天潤五月以覺遠僧正本寫之　慈忍　享和元年辛酉紀號。",
        "from": "T19n0924A_001:0030a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求上件本訓國字。",
        "from": "T19n0924A_001:0030a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此藥師念誦軌。",
        "from": "T19n0924A_001:0030a28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元文年中既鑄梓。",
        "from": "T19n0924A_001:0030a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然彼缺脫之本。",
        "from": "T19n0924A_001:0030a29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯一紙闕。",
        "from": "T19n0924A_001:0030a29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故更校正以壽梓矣。",
        "from": "T19n0924A_001:0030b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　豐山長谷寓居沙門　快道誌一校加筆畢　享和癸亥四月十日　慈順",
        "from": "T19n0924A_001:0030b02_100",
        "context_with_next_sentence": false
    }
]