[
    {
        "source_sentence": "十一面觀自在菩薩祕密心經",
        "from": "T20n1069_003:0146a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "語建立道場儀軌卷下",
        "from": "T20n1069_003:0146a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T20n1069_003:0146a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空謚大鑒正號大廣智大興善寺三藏沙門不空奉　詔譯我今說成就處。",
        "from": "T20n1069_003:0146a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依教擇得地。",
        "from": "T20n1069_003:0146a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉日吉宿吉曜。",
        "from": "T20n1069_003:0146a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨其地離諸過患。",
        "from": "T20n1069_003:0146a25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晨朝歡喜心攝受地作辟除法。",
        "from": "T20n1069_003:0146a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應作是言。",
        "from": "T20n1069_003:0146a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有於此地方障礙者。",
        "from": "T20n1069_003:0146a26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應遠離。",
        "from": "T20n1069_003:0146a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "午時面南應作辟除法。",
        "from": "T20n1069_003:0146a27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒佉陀羅木。",
        "from": "T20n1069_003:0146a27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以芥子油投白芥子護摩。",
        "from": "T20n1069_003:0146a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用甘露軍荼利。",
        "from": "T20n1069_003:0146a28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛心密言曰。",
        "from": "T20n1069_003:0146a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿引密哩二合帝吽發復以濕嚩嚩訶密言。",
        "from": "T20n1069_003:0146b02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持水七遍灑地。",
        "from": "T20n1069_003:0146b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜應作息災護摩。",
        "from": "T20n1069_003:0146b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面北用心中心密言一百八遍。",
        "from": "T20n1069_003:0146b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及用濕嚩嚩訶密言。",
        "from": "T20n1069_003:0146b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手按地。",
        "from": "T20n1069_003:0146b04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦淨地密言一百八遍。",
        "from": "T20n1069_003:0146b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1069_003:0146b05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵步引欠平取地隨意大小。",
        "from": "T20n1069_003:0146b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或九肘十三肘或十六肘。",
        "from": "T20n1069_003:0146b07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深掘齊膝。",
        "from": "T20n1069_003:0146b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除其地中過患。",
        "from": "T20n1069_003:0146b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平治地分為九分。",
        "from": "T20n1069_003:0146b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中央置七寶五穀藥等。",
        "from": "T20n1069_003:0146b09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "好時日以印密言加持。",
        "from": "T20n1069_003:0146b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則應結十方界二手內相叉。",
        "from": "T20n1069_003:0146b10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指頭指小指各申相合。",
        "from": "T20n1069_003:0146b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旋轉十方密言曰。",
        "from": "T20n1069_003:0146b11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵入嚩二合里多路者儞吽以佉陀羅木作橛。",
        "from": "T20n1069_003:0146b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持一百八遍釘四角。",
        "from": "T20n1069_003:0146b13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佉陀羅木橛密言曰。",
        "from": "T20n1069_003:0146b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵虎魯唵虎魯吽泮以波囉捨木燃火。",
        "from": "T20n1069_003:0146b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以本尊密言和三甜。",
        "from": "T20n1069_003:0146b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護摩一百八遍。",
        "from": "T20n1069_003:0146b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則於道場中全身舍利塔東面。",
        "from": "T20n1069_003:0146b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安本尊像。",
        "from": "T20n1069_003:0146b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "像面向西。",
        "from": "T20n1069_003:0146b18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應習先行法。",
        "from": "T20n1069_003:0146b18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦以了欲出道場。",
        "from": "T20n1069_003:0146b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持處所。",
        "from": "T20n1069_003:0146b19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手作金剛拳。",
        "from": "T20n1069_003:0146b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪頭指旋轉十方。",
        "from": "T20n1069_003:0146b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則成堅固精室。",
        "from": "T20n1069_003:0146b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今說護摩　　密言諸儀則",
        "from": "T20n1069_003:0146b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普通令歡喜　　去念誦處所",
        "from": "T20n1069_003:0146b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不近亦不遠　　對彼道場前",
        "from": "T20n1069_003:0146b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護摩如契經　　先應獻部尊",
        "from": "T20n1069_003:0146b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次供養本尊　　供養火天已",
        "from": "T20n1069_003:0146b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後依所求　　息災作圓爐",
        "from": "T20n1069_003:0146b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增益應為方　　降伏應三角",
        "from": "T20n1069_003:0146b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬愛如蓮葉　　爐中應安置",
        "from": "T20n1069_003:0146b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輪金剛獨古　　第四金剛鉤",
        "from": "T20n1069_003:0146c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次第而建立觀自在菩薩通增益法護摩之時。",
        "from": "T20n1069_003:0146c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迎觀自在大勢至義成就大威德者。",
        "from": "T20n1069_003:0146c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安置於爐東邊。",
        "from": "T20n1069_003:0146c04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并持明仙一切藥叉及吉祥天。",
        "from": "T20n1069_003:0146c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應置爐南邊。",
        "from": "T20n1069_003:0146c05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又於爐北邊安佛并諸不退轉菩薩梵王并訶利底母。",
        "from": "T20n1069_003:0146c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求增益成就者。",
        "from": "T20n1069_003:0146c07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當供養。",
        "from": "T20n1069_003:0146c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應迎觀自在密言曰。",
        "from": "T20n1069_003:0146c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵吠那勿微一反娑嚩二合引訶次大勢至密言曰。",
        "from": "T20n1069_003:0146c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵底瑟咤二合底瑟咤二合下同摩訶娑佗二合引麼吠誐三麼也麼努娑麼二合囉吽泮娑嚩二合訶引次義成就密言曰。",
        "from": "T20n1069_003:0146c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵悉𠆙賀悉𠆙娑引馱也娑嚩二合引訶引持明仙密言曰。",
        "from": "T20n1069_003:0146c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵訖哩二合下同拏尾訖哩二合拏尾訖哩二合尼多引也娑嚩二合引訶引藥叉眾密言曰。",
        "from": "T20n1069_003:0147a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥乞叉多入一切吉祥心密言曰。",
        "from": "T20n1069_003:0147a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨摩訶室哩二合夜引也唵止哩弭里膩曳娑嚩二合引訶梵王密言曰。",
        "from": "T20n1069_003:0147a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵鉢納麼二合喻曩曳娑嚩二合引訶引一切佛菩薩密言曰。",
        "from": "T20n1069_003:0147a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫薩嚩沒馱󰂛地薩怛嚩二合引南引阿引尾引囉吽欠平西邊應置白衣觀自在密言曰。",
        "from": "T20n1069_003:0147a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵濕吠二合帝濕吠二合帝半拏囉嚩引悉儞娑嚩二合引訶引訶唎底母密言曰。",
        "from": "T20n1069_003:0147a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵努努摩引里迦呬帝娑嚩二合引訶引如上建立名為增益儀軌。",
        "from": "T20n1069_003:0147a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "息災面向北　　南邊應置矟",
        "from": "T20n1069_003:0147a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西安嚩素枳　　北置金剛印",
        "from": "T20n1069_003:0147a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爐東邊應置　　三戟叉大印",
        "from": "T20n1069_003:0147a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各以本密言　　呼召及發遣",
        "from": "T20n1069_003:0147a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "召三戟叉密言。",
        "from": "T20n1069_003:0147a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各以大指押小指甲。",
        "from": "T20n1069_003:0147a21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散餘三指如叉。",
        "from": "T20n1069_003:0147a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便相合之。",
        "from": "T20n1069_003:0147a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨囉怛曩二合怛囉二合夜也曩莫室戰二合拏嚩日囉二合播拏曳摩訶藥乞叉二合細曩跛多曳．曀係曳呬摩訶藥乞叉二合嚕捺囉二合下同婆孕羯羅三麼焰鉢羅二合底播引攞也阿引蘖縒嚕捺羅二合麼麼曼拏禮嚩日囉二合三麼也歷努播攞也伊只枳弭里枳弭里娑嚩二引引訶發遣密言曰。",
        "from": "T20n1069_003:0147b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘖縒蘖縒嚕捺囉二合娑嚩二合娑嚩二合南補曩囉誐莽曩引夜引囉他二合悉馱曳止里枳娑嚩二合引訶召嚩蘇枳龍王。",
        "from": "T20n1069_003:0147b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰如常拳。",
        "from": "T20n1069_003:0147b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指頭指相捻如環。",
        "from": "T20n1069_003:0147b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1069_003:0147b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚩引蘇枳曩引誐囉惹嚩日囉二合三麼也麼努娑麼二合囉阿引蘖縒答多嚩引魯迦引婆也避多麼曼荼覽勢典虎魯虎魯阿引蘖縒娑嚩二合引訶引發遣密言曰。",
        "from": "T20n1069_003:0147b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孽縒孽縒娑嚩二合引婆嚩南曩引誐囉惹枳孕二合迦哩麼麼迦引麼引娑密哩二合𠆙覩娑嚩二合引訶引請金剛杵密言。",
        "from": "T20n1069_003:0147b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉。",
        "from": "T20n1069_003:0147b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二中指相合。",
        "from": "T20n1069_003:0147b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二大指二小指屈。",
        "from": "T20n1069_003:0147b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指各附二中指背。",
        "from": "T20n1069_003:0147b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不相著。",
        "from": "T20n1069_003:0147b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿演嚩日囉二合麼訶具囉薩嚩咄瑟咤二合娑夜跛赦阿嚩地也二合薩嚩泥嚩引南引嚩日囉二合訶娑多二合婆孕羯囉阿引孽縒施仡囕二合麼麼迦引哩焰娑蜜哩三合𠆙覩印捺囉二合娑訶二合娑囉二合引乞叉二合覩魯覩魯阿引孽縒娑嚩二合引訶引發遣金剛杵密言曰。",
        "from": "T20n1069_003:0147b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孽縒孽縒娑嚩二合婆嚩南嚩日囉二合訶娑多二合摩訶麼羅阿鉢囉二合地哩二合沙夜二合阿素上囕孽薩嚩引曩尾近曩二合覩娑那引婆嚩引跛囉爾儞娑嚩二合引訶引請梵天密言。",
        "from": "T20n1069_003:0147c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手虛心合掌。",
        "from": "T20n1069_003:0147c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開屈頭中名等六指。",
        "from": "T20n1069_003:0147c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如蓮葉。",
        "from": "T20n1069_003:0147c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比多引摩訶嚩覽劍麼攞嚩曩儞嚩引徙阿孽縒嚩日囉二合曼拏攞三麼也麼努播攞也鉢納弭二合儞那鉢納麼二合嚩悉儞二合娑嚩二合引訶引次用請毘紐天密言。",
        "from": "T20n1069_003:0147c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持茅為環。",
        "from": "T20n1069_003:0147c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安爐右。",
        "from": "T20n1069_003:0147c07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發遣時應解請毘紐天密言。",
        "from": "T20n1069_003:0147c08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手反相叉。",
        "from": "T20n1069_003:0147c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指頭相拄。",
        "from": "T20n1069_003:0147c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安右掌中。",
        "from": "T20n1069_003:0147c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吠微愛反濕嚩二合泥嚩摩賀鉢囉二合訖穰二合摩訶尾哩也二合跛囉訖囉二合麼阿孽縒孽嚕拏莽引嚕呬也二合斫訖󰓕二合拏三麼也曼拏覽三麼也麼努播攞也娑嚩二合引訶引行者坐處右邊。",
        "from": "T20n1069_003:0148a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應置護摩支分。",
        "from": "T20n1069_003:0148a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酪蘇香花等。",
        "from": "T20n1069_003:0148a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一器中滿置。",
        "from": "T20n1069_003:0148a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種子應安右邊。",
        "from": "T20n1069_003:0148a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此水天密言加持水。",
        "from": "T20n1069_003:0148a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散灑。",
        "from": "T20n1069_003:0148a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1069_003:0148a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿演嚩嚕拏烏二合曩引誐跛哩嚩引𡀔曩引誐麼攞三麼儞庾二合底入冥儞儞引寧反引信者也娑吠二合底薩嚩烏那迦目佉引尾近曩二合莽引婆挽覩次取茅草密言曰。",
        "from": "T20n1069_003:0148a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "順敷吉祥茅　　東方為先首",
        "from": "T20n1069_003:0148a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南西最後北　　當以梢壓根",
        "from": "T20n1069_003:0148a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿以根壓梢　　散布諸名花",
        "from": "T20n1069_003:0148a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍嚴於茅上　　應用毘俱胝",
        "from": "T20n1069_003:0148a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印密言加持曰。",
        "from": "T20n1069_003:0148a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫薩嚩怛他孽帝毘庾二合囉賀二合毘藥二合三藐三沒第毘藥二合唵婆也曩引捨儞怛囉二合娑儞怛囉二合細怛囉二合娑也毘𠴊二合矩致多致吠多致吠多致濕吠二合帝惹致儞二合娑嚩二合引訶引印如前說。",
        "from": "T20n1069_003:0148a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以辦事密言灑火。",
        "from": "T20n1069_003:0148a26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應然木依教然火密言曰。",
        "from": "T20n1069_003:0148a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵入嚩二合攞吽或用濕嚩二合嚩訶密言。",
        "from": "T20n1069_003:0148a29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灑火。",
        "from": "T20n1069_003:0148b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或用軍荼利。",
        "from": "T20n1069_003:0148b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依護摩儀則。",
        "from": "T20n1069_003:0148b01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初中應用濕嚩嚩訶及軍荼利印密言。",
        "from": "T20n1069_003:0148b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以說。",
        "from": "T20n1069_003:0148b02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以本明密言加持花。",
        "from": "T20n1069_003:0148b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀想投於爐中。",
        "from": "T20n1069_003:0148b03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如教應思惟。",
        "from": "T20n1069_003:0148b03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先投三莖木。",
        "from": "T20n1069_003:0148b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先應請火天以本印。",
        "from": "T20n1069_003:0148b04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後作護摩。",
        "from": "T20n1069_003:0148b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手作施無畏。",
        "from": "T20n1069_003:0148b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微屈頭指招召。",
        "from": "T20n1069_003:0148b05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指屈右掌中密言曰。",
        "from": "T20n1069_003:0148b06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三滿多入嚩二引合攞摩訶入嚩二合引攞阿引儞底也二合三麼鉢囉二合婆娑惹引多吠那迦比羅慕引囉多曳阿孽縒曼拏覽沒度步佉者囉三娑嚩二合引訶引請已先以大杓三沃火。",
        "from": "T20n1069_003:0148b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後旋灑。",
        "from": "T20n1069_003:0148b11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與火天漱口次灑淨。",
        "from": "T20n1069_003:0148b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則應以小杓供養本尊。",
        "from": "T20n1069_003:0148b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以木兩頭搵酥投火中獻。",
        "from": "T20n1069_003:0148b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次油麻次酪乳蜜等。",
        "from": "T20n1069_003:0148b13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以後隨意。",
        "from": "T20n1069_003:0148b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸香藥應燒。",
        "from": "T20n1069_003:0148b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中以油麻粳米和酥燒為勝。",
        "from": "T20n1069_003:0148b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如上眾緣不具。",
        "from": "T20n1069_003:0148b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但燒酥密言後安娑嚩訶。",
        "from": "T20n1069_003:0148b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦得一切成就。",
        "from": "T20n1069_003:0148b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護摩已了。",
        "from": "T20n1069_003:0148b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火以水𭲝灑。",
        "from": "T20n1069_003:0148b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令聖眾漱口。",
        "from": "T20n1069_003:0148b17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為令聖眾歡喜故。",
        "from": "T20n1069_003:0148b17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用本部心加持閼伽而獻之。",
        "from": "T20n1069_003:0148b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定心合掌殷重心求悉地。",
        "from": "T20n1069_003:0148b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以閼伽奉送依法以水灑火用辦事密言。",
        "from": "T20n1069_003:0148b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有護摩殘物隨意樂供養外諸天。",
        "from": "T20n1069_003:0148b20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以前請火天印。",
        "from": "T20n1069_003:0148b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭指大指相捻。",
        "from": "T20n1069_003:0148b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發遣火天密言曰。",
        "from": "T20n1069_003:0148b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孽縒入嚩二合引攞娑那嘮捺囉二合阿鉢囉二合地唱二合史也二合蘇囉蘇囉孽縒娑嚩娑嚩南入嚩二合引攞三莽扇引井婆嚩底娑那引底儞枳抳嘮儞哩二合抳娑嚩二合引訶引獻閼伽奉送聖眾。",
        "from": "T20n1069_003:0148b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以前大護印密言左旋解界。",
        "from": "T20n1069_003:0148b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及寶輅印密言奉送聖眾。",
        "from": "T20n1069_003:0148b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今次第說　　護摩焰色相",
        "from": "T20n1069_003:0148c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密語者由知　　速疾獲悉地",
        "from": "T20n1069_003:0148c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如虹霓白色　　珊瑚光莊嚴",
        "from": "T20n1069_003:0148c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋妙滋潤　　煥爛若虹霓",
        "from": "T20n1069_003:0148c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "映赤吠瑠璃　　如護摩杓形",
        "from": "T20n1069_003:0148c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三叉杵吉子　　商佉蓮拂形",
        "from": "T20n1069_003:0148c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幢蓋羯囉捨　　娑嚩悉底迦",
        "from": "T20n1069_003:0148c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其聲如笛鼓　　妙香極悅意",
        "from": "T20n1069_003:0148c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見如是焰　　無垢離諸障",
        "from": "T20n1069_003:0148c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者應慇重　　應當求成就",
        "from": "T20n1069_003:0148c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "焰若一峯勝　　二峯為中相",
        "from": "T20n1069_003:0148c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三峯下成就　　求成者應察",
        "from": "T20n1069_003:0148c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說不成相　　弊惡障嚴飾",
        "from": "T20n1069_003:0148c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左旋極臭氣　　屍臭驢鳴聲",
        "from": "T20n1069_003:0148c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數吐焰斷絕　　烟聚令怖聲",
        "from": "T20n1069_003:0148c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "糞器形乾濕　　焰散舐掠形",
        "from": "T20n1069_003:0148c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見護摩相　　智者應審知",
        "from": "T20n1069_003:0149a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言者速疾　　辨事軍荼利",
        "from": "T20n1069_003:0149a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以水應數灑　　能除不吉祥",
        "from": "T20n1069_003:0149a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故一切時　　應用甘露尊",
        "from": "T20n1069_003:0149a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時觀自在菩薩摩訶薩說此法已。",
        "from": "T20n1069_003:0149a05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切大眾咸共讚言善哉善哉大士。",
        "from": "T20n1069_003:0149a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃能為欲利益安樂諸有情故。",
        "from": "T20n1069_003:0149a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此密語。",
        "from": "T20n1069_003:0149a07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等隨喜亦願受持。",
        "from": "T20n1069_003:0149a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時大眾歡喜踊躍遶佛三匝作禮而去。",
        "from": "T20n1069_003:0149a08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十一面觀自在菩薩心密言經建立道場儀軌卷下",
        "from": "T20n1069_003:0149a10_100",
        "context_with_next_sentence": false
    }
]