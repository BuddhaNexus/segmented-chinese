[
    {
        "source_sentence": "佛說灌頂呪宮宅神王守鎮左右經佛說灌頂呪宮宅神王守鎮左右",
        "from": "T21n1331_005:0508c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經卷第五",
        "from": "T21n1331_005:0508c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東晉天竺三藏帛尸梨蜜多羅譯",
        "from": "T21n1331_005:0508c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞如是。",
        "from": "T21n1331_005:0508c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在摩竭國。",
        "from": "T21n1331_005:0508c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與千二百五十比丘俱。",
        "from": "T21n1331_005:0508c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩萬七千人。",
        "from": "T21n1331_005:0508c07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清信士三萬人。",
        "from": "T21n1331_005:0508c07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清信女萬二千人。",
        "from": "T21n1331_005:0508c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天人民鬼神龍王不可稱計。",
        "from": "T21n1331_005:0508c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆悉來集聽佛說法莫不樂聞。",
        "from": "T21n1331_005:0508c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語目連。",
        "from": "T21n1331_005:0508c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "波羅閱國去城千里有七百餘家。",
        "from": "T21n1331_005:0508c10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未覩聖化不識真道。",
        "from": "T21n1331_005:0508c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多事神鬼山川樹神。",
        "from": "T21n1331_005:0508c11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及日月五星二十八宿水火神等。",
        "from": "T21n1331_005:0508c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉村奉事以為真正。",
        "from": "T21n1331_005:0508c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殺生祭祠以此為常。",
        "from": "T21n1331_005:0508c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時村民宗王名曰迦羅那。",
        "from": "T21n1331_005:0508c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "門室大小一百餘人。",
        "from": "T21n1331_005:0508c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "春分之中為惡鬼神所共嬈觸。",
        "from": "T21n1331_005:0508c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此迦羅那居山谷之邊。",
        "from": "T21n1331_005:0508c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或言山神嬈我。",
        "from": "T21n1331_005:0508c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或言樹神嬈我。",
        "from": "T21n1331_005:0508c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或言水火日月星宿神嬈我。",
        "from": "T21n1331_005:0508c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是念已生大愁苦。",
        "from": "T21n1331_005:0508c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時惡魔明旦復將五方鬼魅。",
        "from": "T21n1331_005:0508c19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉共并力摑地大呼。",
        "from": "T21n1331_005:0508c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恐怖迦羅那家室大小。",
        "from": "T21n1331_005:0508c20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有鬼神身如師子。",
        "from": "T21n1331_005:0508c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或如熊羆虎狼象龍牛馬犬狗猴猨之形不可具說。",
        "from": "T21n1331_005:0508c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或虫頭人身蚖蛇之形黿龜之手。",
        "from": "T21n1331_005:0508c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或六目三頭。",
        "from": "T21n1331_005:0508c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或一頭而三面齒牙爪距。",
        "from": "T21n1331_005:0508c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或擔山吐火雷電霹靂。",
        "from": "T21n1331_005:0508c24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "村民聞之莫不助憂。",
        "from": "T21n1331_005:0508c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然迦羅那心中煩憒語諸宗族。",
        "from": "T21n1331_005:0508c25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卿等當佐我作方計。",
        "from": "T21n1331_005:0508c26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時村中有學梵志道者。",
        "from": "T21n1331_005:0508c27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "語迦羅那言汝可請大學淨信梵志。",
        "from": "T21n1331_005:0508c28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦羅那答言可爾。",
        "from": "T21n1331_005:0508c28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便擇良日吉時設大供辦。",
        "from": "T21n1331_005:0508c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸梵志集來迦羅那大客堂上。",
        "from": "T21n1331_005:0509a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦羅那自行澡水。",
        "from": "T21n1331_005:0509a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行水已訖飯食竟。",
        "from": "T21n1331_005:0509a01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便敷小卑座從坐而起。",
        "from": "T21n1331_005:0509a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白諸大德淨信梵志。",
        "from": "T21n1331_005:0509a02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等從春分以來。",
        "from": "T21n1331_005:0509a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常為諸魔之所嬈觸。",
        "from": "T21n1331_005:0509a03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願諸梵志各出聖術。",
        "from": "T21n1331_005:0509a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以却鬼神災惡之變。",
        "from": "T21n1331_005:0509a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸大梵志各各皆言。",
        "from": "T21n1331_005:0509a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉當為汝說先師之法。",
        "from": "T21n1331_005:0509a05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔時波羅㮈國亦有此變。",
        "from": "T21n1331_005:0509a06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝可於村中平淨之地。",
        "from": "T21n1331_005:0509a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謝四山諸神及日月星宿神等。",
        "from": "T21n1331_005:0509a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當得百頭種種眾生各各異類。",
        "from": "T21n1331_005:0509a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并二小兒殺以祠天。",
        "from": "T21n1331_005:0509a08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝將家中大小眷屬。",
        "from": "T21n1331_005:0509a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詣於村中平淨地處祠祀壇所。",
        "from": "T21n1331_005:0509a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使人人跪拜。",
        "from": "T21n1331_005:0509a10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請命求護然後乃安。",
        "from": "T21n1331_005:0509a10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時迦羅那即如其言。",
        "from": "T21n1331_005:0509a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明旦具兩小兒及異類眾生種種各百頭。",
        "from": "T21n1331_005:0509a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平治村中便將家大小。",
        "from": "T21n1331_005:0509a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸眾生往彼祠壇所。",
        "from": "T21n1331_005:0509a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "象馬牛羊隨道悲鳴震動大地。",
        "from": "T21n1331_005:0509a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從東門出當就祭壇。",
        "from": "T21n1331_005:0509a14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是世尊大悲普濟一切眾生。",
        "from": "T21n1331_005:0509a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愍是迦羅那頑愚之甚。",
        "from": "T21n1331_005:0509a15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何興大惡意殺眾生命欲救一家。",
        "from": "T21n1331_005:0509a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便勅弟子大目揵連。",
        "from": "T21n1331_005:0509a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝可往為迦羅那。",
        "from": "T21n1331_005:0509a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說辟鬼神呪護迦羅那。",
        "from": "T21n1331_005:0509a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目連受教便承佛威神。",
        "from": "T21n1331_005:0509a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即為迦羅那召五方諸善神。",
        "from": "T21n1331_005:0509a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以衛護迦羅那門戶左右悉獲安隱。",
        "from": "T21n1331_005:0509a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辟除邪惡往來鬼神。",
        "from": "T21n1331_005:0509a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為汝作害者從今以去。",
        "from": "T21n1331_005:0509a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠汝村舍不使為害。",
        "from": "T21n1331_005:0509a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦羅那見目連現神飛來。",
        "from": "T21n1331_005:0509a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡喜踊躍頭面禮目連足下。",
        "from": "T21n1331_005:0509a22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是念言尊者不以我愚賤。",
        "from": "T21n1331_005:0509a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現神顧我度我災難。",
        "from": "T21n1331_005:0509a23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令得蘇息無復愁惱。",
        "from": "T21n1331_005:0509a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今便是蒙尊者威靈。",
        "from": "T21n1331_005:0509a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救度我危厄使諸惡魔隱藏不現。",
        "from": "T21n1331_005:0509a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦羅那說此語已。",
        "from": "T21n1331_005:0509a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸梵志輩悉見目連現神化來。",
        "from": "T21n1331_005:0509a26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為迦羅那說大神呪。",
        "from": "T21n1331_005:0509a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當呪之日天地大動。",
        "from": "T21n1331_005:0509a27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸梵志輩使迦羅那祭祀鬼神。",
        "from": "T21n1331_005:0509a28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲食之具并其座席。",
        "from": "T21n1331_005:0509a28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆飛颺在於虛空。",
        "from": "T21n1331_005:0509a29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸眾生輩各得解脫。",
        "from": "T21n1331_005:0509a29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人留斷隨行而去。",
        "from": "T21n1331_005:0509b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸梵志輩見目連現呪術。",
        "from": "T21n1331_005:0509b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變化威德如是。",
        "from": "T21n1331_005:0509b02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四眾咸然稽首目連足下。",
        "from": "T21n1331_005:0509b02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而問訊言尊者目連　宗事何師其法真妙乃如是乎。",
        "from": "T21n1331_005:0509b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摧滅我等鬼神災變。",
        "from": "T21n1331_005:0509b04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令一村人悉得蘇息。",
        "from": "T21n1331_005:0509b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆尊者恩使之然也。",
        "from": "T21n1331_005:0509b05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目連語迦羅那梵志四眾等。",
        "from": "T21n1331_005:0509b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今我師者天人之尊。",
        "from": "T21n1331_005:0509b06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智慧弘廣靡所不通。",
        "from": "T21n1331_005:0509b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深知來今過去之事。",
        "from": "T21n1331_005:0509b07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身長丈六紫磨金色。",
        "from": "T21n1331_005:0509b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十二相而自莊嚴。",
        "from": "T21n1331_005:0509b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "度人厄難無不解脫。",
        "from": "T21n1331_005:0509b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四眾聞說大聖威神。",
        "from": "T21n1331_005:0509b09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無不欲見世尊身者。",
        "from": "T21n1331_005:0509b09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦羅那白目連言。",
        "from": "T21n1331_005:0509b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今佛世尊可得見不。",
        "from": "T21n1331_005:0509b10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目連語言今佛世尊近在羅閱祇。",
        "from": "T21n1331_005:0509b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸天人四部弟子說真妙法。",
        "from": "T21n1331_005:0509b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦羅那語大目揵連。",
        "from": "T21n1331_005:0509b12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今便欲相隨往見世尊。",
        "from": "T21n1331_005:0509b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目連語言佛神通聖達。",
        "from": "T21n1331_005:0509b13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自見汝心不須去也。",
        "from": "T21n1331_005:0509b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但燒香散華。",
        "from": "T21n1331_005:0509b14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊今日現神化來亦爾。",
        "from": "T21n1331_005:0509b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不久迦羅那但燒香散華靜心念佛。",
        "from": "T21n1331_005:0509b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須臾之頃佛現神來。",
        "from": "T21n1331_005:0509b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將從千二百五十比丘四輩弟子。",
        "from": "T21n1331_005:0509b17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸菩薩無央數。",
        "from": "T21n1331_005:0509b17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉來集會迦羅那大客堂上。",
        "from": "T21n1331_005:0509b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "客堂自然廣博嚴好變為殿舍。",
        "from": "T21n1331_005:0509b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉以瑠璃為柱梁。",
        "from": "T21n1331_005:0509b19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七寶莊嚴懸雜幡蓋。",
        "from": "T21n1331_005:0509b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四面皆有諸小菩薩。",
        "from": "T21n1331_005:0509b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執持幢蓋侍立左右。",
        "from": "T21n1331_005:0509b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦羅那及梵志四眾等見佛。",
        "from": "T21n1331_005:0509b21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心中歡喜踊躍無量。",
        "from": "T21n1331_005:0509b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭面接佛足下稽首作禮。",
        "from": "T21n1331_005:0509b22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦羅那前進長跪叉手。",
        "from": "T21n1331_005:0509b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊。",
        "from": "T21n1331_005:0509b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我從歷劫以來。",
        "from": "T21n1331_005:0509b23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信邪倒見不識真正。",
        "from": "T21n1331_005:0509b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有危厄之日輒殺眾生請求福祐。",
        "from": "T21n1331_005:0509b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今者世尊大慈普濟。",
        "from": "T21n1331_005:0509b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救度眾生危厄之患。",
        "from": "T21n1331_005:0509b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆蒙解脫離諸恐怖。",
        "from": "T21n1331_005:0509b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆天中天目連恩也。",
        "from": "T21n1331_005:0509b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "室家大小及村中人悉獲度脫。",
        "from": "T21n1331_005:0509b27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦羅那及梵志四眾等。",
        "from": "T21n1331_005:0509b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從佛乞受戒法。",
        "from": "T21n1331_005:0509b28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛為廣說業行因緣得法眼淨。",
        "from": "T21n1331_005:0509b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宗室大小及村中人。",
        "from": "T21n1331_005:0509b29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞法音歡喜各發善心。",
        "from": "T21n1331_005:0509c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有受戒者有生天者。",
        "from": "T21n1331_005:0509c01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各隨業行得道不同爾時坐中有一菩薩名曰普觀。",
        "from": "T21n1331_005:0509c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從坐而起前白佛言。",
        "from": "T21n1331_005:0509c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊已為迦羅那。",
        "from": "T21n1331_005:0509c03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說五方辟鬼神呪。",
        "from": "T21n1331_005:0509c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "營護門戶鎮守左右竟。",
        "from": "T21n1331_005:0509c04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願更演。",
        "from": "T21n1331_005:0509c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向者目連所說章句。",
        "from": "T21n1331_005:0509c05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為未來世諸眾生輩。",
        "from": "T21n1331_005:0509c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有如迦羅那比惡鬼神所嬈者。",
        "from": "T21n1331_005:0509c06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當書持讀誦經文示於後世。",
        "from": "T21n1331_005:0509c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣宣流傳遠近使聞。",
        "from": "T21n1331_005:0509c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普得度脫無復憂懼。",
        "from": "T21n1331_005:0509c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告普觀菩薩。",
        "from": "T21n1331_005:0509c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝能為未來濁惡之世。",
        "from": "T21n1331_005:0509c09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重問灌頂無上章句神名字者。",
        "from": "T21n1331_005:0509c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諦聽憶念我當重演如是章句。",
        "from": "T21n1331_005:0509c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普觀菩薩言唯唯天尊。",
        "from": "T21n1331_005:0509c11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四眾渴仰唯願速說。",
        "from": "T21n1331_005:0509c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言灌頂章句其名如是。",
        "from": "T21n1331_005:0509c12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神名阿修呵比丘尼槃頭荷神名波梨梨波斯離次離神名優唦知具利揵陀離神名何泥梨移波泥梨羅神名阿吼羅迷比路波祇神名氣利彌提波羅彌提神名陀羅住留富那佉尼神名師阿羅陀羅慢陀羅神名具利揵陀利闍吒羅神名住利旃唦利嘻摩陀神名阿波提梨摩那斯爾神名脂波弗夢懼頭摩醯神名嘻蕪夢羅呵梨彌醯神名多遮枝嘻遮惟留賴神名差陀離遮難頭謣難神名貨樓差陀輸柔羅彌神名丘波題金波羅優候神名東波須摩羅提迦波神名伊提牟羅嘻遲佉神名阿提嘻惒遲比難邏神名沙摩頭伽優頭摩陀佛告普觀菩薩摩訶薩。",
        "from": "T21n1331_005:0510a05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東方有七千大神。",
        "from": "T21n1331_005:0510a05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其上首者有三七鬼神之王守護東方不令邪惡觸犯萬姓。",
        "from": "T21n1331_005:0510a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有鳥鳴野獸變怪災禍起時。",
        "from": "T21n1331_005:0510a07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種不吉惡夢眾衰。",
        "from": "T21n1331_005:0510a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "橫羅縣官枷鎖著身。",
        "from": "T21n1331_005:0510a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "危厄之人眾所憎賤。",
        "from": "T21n1331_005:0510a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當以此三七神王。",
        "from": "T21n1331_005:0510a09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辟除東方如是災厄。",
        "from": "T21n1331_005:0510a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使諸邪橫不得妄生。",
        "from": "T21n1331_005:0510a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此諸神王守護東方。",
        "from": "T21n1331_005:0510a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除去七十億雜魅之鬼為人作害者。",
        "from": "T21n1331_005:0510a11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞有善男子女人讀誦呼是七千鬼神上首名者。",
        "from": "T21n1331_005:0510a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七十億魅鬼馳走而去。",
        "from": "T21n1331_005:0510a13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一萬由旬不害人也。",
        "from": "T21n1331_005:0510a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當說其名獲吉祥福。",
        "from": "T21n1331_005:0510a14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灌頂章句其名如是汝善持之。",
        "from": "T21n1331_005:0510a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神名那頭化提阿梨鳩梨神名提多瞿佉難阿梨加神名迦梨波梨金摩多尼神名波奈羅迦荼支由羅神名沙彈吒迦勒迦摩夜神名校坻細耶般遮戒提神名坻梨提梨摩梨摩梨神名師佉准沙差尼河藍神名辟師霧耶欝次彌次神名雞頭貨𭏷蒲恥瘖浮神名突迦多極那于陀伎神名陀含屈兒沙門多頭神名伊梨寐提薜荔迦移神名鳩舍羅𬛜迦拘多𭏷神名因樓賴天沙伊多耨神名甄尼烏厨遮摩多梨神名摩隷吒遮夫樓擥俱神名羅闍迦提阿銖迦隷神名摩尼摩尼遮樓旃陀神名俱波樓閱叉阿羅域神名那羅那移閻那阿梨神名沙善般遮壚梨波梨神名旃遮賴摩休迦設樓神名遮梨架阿擥烏遲神名羅摩奴遮𬛜迦壚遮神名波羅斯奴遮斯叉奴神名遮羅揵波頭那爐遮神名乾提于呵梨攎闍佛告普觀菩薩摩訶薩。",
        "from": "T21n1331_005:0510b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南方有九千大神。",
        "from": "T21n1331_005:0510b15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其上首者有二十八神王。",
        "from": "T21n1331_005:0510b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能為一切辟除眾惡守持南方。",
        "from": "T21n1331_005:0510b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除去五溫疫毒之病飛尸邪忤殃咎之注。",
        "from": "T21n1331_005:0510b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口舌亂鬪災火變怪。",
        "from": "T21n1331_005:0510b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒蛇蚖蝮熱氣惡病悉出南方。",
        "from": "T21n1331_005:0510b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是二十八神王。",
        "from": "T21n1331_005:0510b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有人知其名字諷誦宣傳。",
        "from": "T21n1331_005:0510b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是二十八神王即為佐助。",
        "from": "T21n1331_005:0510b20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "消滅邪惡使一切毒厭笮不得行。",
        "from": "T21n1331_005:0510b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以知神王名字力故。",
        "from": "T21n1331_005:0510b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南方魅鬼七千億數。",
        "from": "T21n1331_005:0510b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一去萬由旬不能為害。",
        "from": "T21n1331_005:0510b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灌頂章句其名如是汝善持之。",
        "from": "T21n1331_005:0510b23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神名呵婁薩叉尸遮那神名藍波藍波貨那名神名蘇貨迦羅闍烏那陀神名摩奴羅摩遮檀樓迦神名劍浮者梨惒迦羅博神名惟舍羅遮繩迦陀羅神名遮稜迦移陀那羅浮神名迦俱絺闍羅給迦神名多羅舍脂慢鳩梨神名臀頭梨阿多四惒伎神名阿耆尼羅鵄祇迦遮神名菩提尼菩提多菩提神名恒多羅夜尼耶迷神名陀優陀優富阿陀羅佛告普觀菩薩摩訶薩。",
        "from": "T21n1331_005:0510c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方有六千大神。",
        "from": "T21n1331_005:0510c09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其上首者有二七神王。",
        "from": "T21n1331_005:0510c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "主持西方怨家逆賊伐王國土。",
        "from": "T21n1331_005:0510c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偷竊之人懷惡心者。",
        "from": "T21n1331_005:0510c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞有男子女人等輩。",
        "from": "T21n1331_005:0510c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呼二七神王名字之者。",
        "from": "T21n1331_005:0510c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即便退散惶惑而去。",
        "from": "T21n1331_005:0510c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五億魔魅退萬由旬不復害人也。",
        "from": "T21n1331_005:0510c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此二七神王灌頂章句其名如是汝善持之。",
        "from": "T21n1331_005:0510c14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神名那葉提樓比舍慢破神名沙伊摩陀阿仇伊陀神名破仇摩至多羅和架神名比牟坻羅泥架那紫神名迦蘭脾留波摩迦羅神名耶頭破舍羅首梨沙神名拘陀利比呵梨茤陀神名慢陀波沙多陀周留神名阿知和知阿那波提神名那知鳩那知提我沙神名摩呵迦和尼羅闍摩神名阿佉尼波陀那迦利神名阿比羅慢多羅波陀神名羅那多羅摩羅提離神名耽波羅提梨吼樓壽神名樓壽闍輦叉輦善叉神名魔呵留羅迦利區惒神名舍洹陀越阿耨三菩神名迦三耶魔呵阿輪神名拔陀沙羅慢陀羅阿神名迦柰國舍嘻遲比遲佛告普觀菩薩摩訶薩。",
        "from": "T21n1331_005:0511a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方九千大神。",
        "from": "T21n1331_005:0511a07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其上首者有三七神王。",
        "from": "T21n1331_005:0511a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "主持北方五萬億魅鬼及詐稱之神。",
        "from": "T21n1331_005:0511a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求人飲食者故氣之魔。",
        "from": "T21n1331_005:0511a09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞有男子女人等輩。",
        "from": "T21n1331_005:0511a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呼三七神王名號。",
        "from": "T21n1331_005:0511a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸魅鬼退散馳去不能為害。",
        "from": "T21n1331_005:0511a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠百千由旬移置無人之處還其所在。",
        "from": "T21n1331_005:0511a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灌頂章句其名如是。",
        "from": "T21n1331_005:0511a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝善持之。",
        "from": "T21n1331_005:0511a12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告普觀菩薩摩訶薩。",
        "from": "T21n1331_005:0511a13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我說四方諸神王竟。",
        "from": "T21n1331_005:0511a13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今復更演中央神王。",
        "from": "T21n1331_005:0511a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有二十萬以為眷屬。",
        "from": "T21n1331_005:0511a14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其上首者有十二神王。",
        "from": "T21n1331_005:0511a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能為一切無量眾生。",
        "from": "T21n1331_005:0511a15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除去四方災惡諸變。",
        "from": "T21n1331_005:0511a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳥鳴惡夢縣官之厄。",
        "from": "T21n1331_005:0511a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口舌鬪亂五溫之病。",
        "from": "T21n1331_005:0511a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒氣蛇蚖怨家債主。",
        "from": "T21n1331_005:0511a17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逆賊侵陵壞王國土。",
        "from": "T21n1331_005:0511a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他餘雜鬼厭禱呪咀。",
        "from": "T21n1331_005:0511a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魔邪鬼神及諸精魅。",
        "from": "T21n1331_005:0511a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見有男子女人等輩。",
        "from": "T21n1331_005:0511a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讀誦宣說此十二神王名字之時。",
        "from": "T21n1331_005:0511a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四方妖邪惡鬼等類無不弭伏。",
        "from": "T21n1331_005:0511a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復能為人作鎮護故。",
        "from": "T21n1331_005:0511a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今演說如是章句。",
        "from": "T21n1331_005:0511a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "示諸未聞普使受持此十二神王名字。",
        "from": "T21n1331_005:0511a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為人守鎮辟除凶惡。",
        "from": "T21n1331_005:0511a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灌頂章句其名如是汝善持之。",
        "from": "T21n1331_005:0511a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神名優婆參那仙嬭神名優婆僧那涅坻賒神名跋臭修婆羅毘坻神名多婆斯阿修羅婆履神名摩薩羅波拘蘭荼神名坻摩阿修羅婆履那神名佛亶坻和婆娑斯神名伽施陀膩為氏仇陀神名多羅浮多鞞闍曳神名婆視涅槃伽羅呵衍神名毘真那貳娑坻衍神名多羅哆斯陀尼波佛告普觀菩薩摩訶薩。",
        "from": "T21n1331_005:0511b08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今已說五方逐魔鬼竟。",
        "from": "T21n1331_005:0511b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取中央三萬大神上首者十二神王名字為守鎮法。",
        "from": "T21n1331_005:0511b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當以好函盛之。",
        "from": "T21n1331_005:0511b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "題四天王名字書函四邊。",
        "from": "T21n1331_005:0511b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱吾釋迦名號而封印之。",
        "from": "T21n1331_005:0511b11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "綵傘覆蓋安淨潔處。",
        "from": "T21n1331_005:0511b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有邪神惡鬼往來人宮宅中者。",
        "from": "T21n1331_005:0511b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見此神王名字鎮函之處。",
        "from": "T21n1331_005:0511b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫不退散馳走者也。",
        "from": "T21n1331_005:0511b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告普觀菩薩摩訶薩。",
        "from": "T21n1331_005:0511b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若後末世遭災禍者。",
        "from": "T21n1331_005:0511b15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸魔魅之所傷犯。",
        "from": "T21n1331_005:0511b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當淨身口意不噉雜食五辛之屬。",
        "from": "T21n1331_005:0511b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "齋戒一心禮敬十方三世諸佛。",
        "from": "T21n1331_005:0511b17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然十方燈燒雜名香膠香婆香安息香等。",
        "from": "T21n1331_005:0511b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懸五方逐魔神幡。",
        "from": "T21n1331_005:0511b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各長一丈四尺。",
        "from": "T21n1331_005:0511b19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上作鬼神之形怖諸浮遊魔鬼精魅。",
        "from": "T21n1331_005:0511b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當在人定之時。",
        "from": "T21n1331_005:0511b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "露出中庭讀此神呪。",
        "from": "T21n1331_005:0511b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以青銅之鏡照曜五方。",
        "from": "T21n1331_005:0511b21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使諸魔魅不得隱藏其形。",
        "from": "T21n1331_005:0511b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向五方燒香散諸名華。",
        "from": "T21n1331_005:0511b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師當專心一意。",
        "from": "T21n1331_005:0511b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是五方守護神名。",
        "from": "T21n1331_005:0511b23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一方至四十九遍誦是章句。",
        "from": "T21n1331_005:0511b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸惡魔聞見之者。",
        "from": "T21n1331_005:0511b24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫不驚惶馳走而去。",
        "from": "T21n1331_005:0511b25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不復為害遠百千由旬不嬈人也。",
        "from": "T21n1331_005:0511b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普觀菩薩白佛言。",
        "from": "T21n1331_005:0511b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊向所說幡蓋作鬼神形像者。",
        "from": "T21n1331_005:0511b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出何法中云何驅魔令其退散。",
        "from": "T21n1331_005:0511b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語普觀菩薩摩訶薩言。",
        "from": "T21n1331_005:0511b28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子我所說幡蓋作鬼神形像者。",
        "from": "T21n1331_005:0511b29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取上五方諸神之中第一者耳。",
        "from": "T21n1331_005:0511c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又問云何馳逐眾魔。",
        "from": "T21n1331_005:0511c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答言當以好淨器先呪五龍之水以灑五方。",
        "from": "T21n1331_005:0511c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸魅魔邪無不退散馳走而去。",
        "from": "T21n1331_005:0511c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "部伍營從形體裸露莫知藏匿。",
        "from": "T21n1331_005:0511c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大章句至真致重。",
        "from": "T21n1331_005:0511c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能消滅諸鬼神災橫之變。",
        "from": "T21n1331_005:0511c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使誦持者獲得無上吉祥之福。",
        "from": "T21n1331_005:0511c05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普觀菩薩復白佛言。",
        "from": "T21n1331_005:0511c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作諸雜術種種相貌不似耶也。",
        "from": "T21n1331_005:0511c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言不也。",
        "from": "T21n1331_005:0511c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我若在世不須如是相貌法也。",
        "from": "T21n1331_005:0511c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我既涅槃未來惡世。",
        "from": "T21n1331_005:0511c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五濁眾生信正者少。",
        "from": "T21n1331_005:0511c08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多習邪見不識真正。",
        "from": "T21n1331_005:0511c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為此等輩說是章句雜法之術。",
        "from": "T21n1331_005:0511c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以化群生故我出此雜碎章句非邪法也。",
        "from": "T21n1331_005:0511c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以化萬姓諸比丘輩不解我意。",
        "from": "T21n1331_005:0511c11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見有書持讀誦之者。",
        "from": "T21n1331_005:0511c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂此法言非佛真說。",
        "from": "T21n1331_005:0511c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起邪見想誹謗不信。",
        "from": "T21n1331_005:0511c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我已於前護比丘章。",
        "from": "T21n1331_005:0511c13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說諸誹謗愆咎之過。",
        "from": "T21n1331_005:0511c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見聞者唯應專修勿生不信。",
        "from": "T21n1331_005:0511c14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見有行者恭敬禮拜如大師想。",
        "from": "T21n1331_005:0511c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可獲大福至得佛道。",
        "from": "T21n1331_005:0511c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛又告普觀菩薩。",
        "from": "T21n1331_005:0511c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當宣布流傳此經。",
        "from": "T21n1331_005:0511c16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至吾法沒莫效外道誹謗我法。",
        "from": "T21n1331_005:0511c17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若起慢心即墮惡道。",
        "from": "T21n1331_005:0511c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "況復毀呰墮阿鼻地獄億劫無救。",
        "from": "T21n1331_005:0511c18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今故宣示勿有謗毀。",
        "from": "T21n1331_005:0511c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說如是。",
        "from": "T21n1331_005:0511c19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難長跪叉手白佛言。",
        "from": "T21n1331_005:0511c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "演說此經當何名之。",
        "from": "T21n1331_005:0511c20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語阿難此法名為灌頂章句宮宅神呪守鎮左右辟除邪惡使獲吉祥魔鬼敢當經。",
        "from": "T21n1331_005:0511c22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難汝好宣傳使諸四輩。",
        "from": "T21n1331_005:0511c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣宣流布利益萬姓。",
        "from": "T21n1331_005:0511c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得入正法無諸邪曲。",
        "from": "T21n1331_005:0511c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說此經已。",
        "from": "T21n1331_005:0511c24_7",
        "context_with_next_sentence": true
    },
    {
