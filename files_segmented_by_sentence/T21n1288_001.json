[
    {
        "source_sentence": "佛說最上祕密那拏天經卷上",
        "from": "T21n1288_001:0358a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝奉大夫試光祿卿",
        "from": "T21n1288_001:0358a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明教大師臣法賢奉　詔譯",
        "from": "T21n1288_001:0358a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "1 最上成就儀軌分最上成就儀軌分第一如是我聞。",
        "from": "T21n1288_001:0358a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在毘沙門宮。",
        "from": "T21n1288_001:0358a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大菩薩摩訶薩眾俱。",
        "from": "T21n1288_001:0358a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大祕密主金剛手菩薩而為上首。",
        "from": "T21n1288_001:0358a16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及無數天龍夜叉乾闥婆阿脩羅迦樓羅緊那羅摩睺羅伽。",
        "from": "T21n1288_001:0358a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及釋梵護世那羅延天大自在天。",
        "from": "T21n1288_001:0358a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶賢天滿賢天力天大力天。",
        "from": "T21n1288_001:0358a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難儞計說囉天摩賀迦羅天醫主天。",
        "from": "T21n1288_001:0358a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸天后等。",
        "from": "T21n1288_001:0358a20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各有無數百千眷屬俱。",
        "from": "T21n1288_001:0358a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有大羅剎眾。",
        "from": "T21n1288_001:0358a21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂金色羅剎主十頭羅剎主。",
        "from": "T21n1288_001:0358a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尾毘沙拏羅剎主寶密羅剎主。",
        "from": "T21n1288_001:0358a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行密羅剎主無邊密羅剎主。",
        "from": "T21n1288_001:0358a23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普遍密羅剎主上授羅剎主。",
        "from": "T21n1288_001:0358a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡牙羅剎主利牙羅剎主。",
        "from": "T21n1288_001:0358a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烏姹播捺羅剎主象頭羅剎主。",
        "from": "T21n1288_001:0358a25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等羅剎主各領百千俱胝眷屬而來集會。",
        "from": "T21n1288_001:0358a26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有大夜叉眾。",
        "from": "T21n1288_001:0358a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂人仙夜叉主阿吒嚩夜叉主。",
        "from": "T21n1288_001:0358a27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善調夜叉主發光夜叉主。",
        "from": "T21n1288_001:0358a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善容夜叉主妙光夜叉主。",
        "from": "T21n1288_001:0358a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙目夜叉主尾瑟努夜叉主。",
        "from": "T21n1288_001:0358a29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙髮夜叉主惡相夜叉主。",
        "from": "T21n1288_001:0358b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得勝夜叉主熾盛夜叉主。",
        "from": "T21n1288_001:0358b01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火頂夜叉主名稱夜叉主。",
        "from": "T21n1288_001:0358b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "半唧迦夜叉主花授夜叉主。",
        "from": "T21n1288_001:0358b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "花髻夜叉主寶髻夜叉主。",
        "from": "T21n1288_001:0358b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣財夜叉主善覺夜叉主。",
        "from": "T21n1288_001:0358b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善行夜叉主妙臂夜叉主。",
        "from": "T21n1288_001:0358b04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙嚴夜叉主珠牙夜叉主。",
        "from": "T21n1288_001:0358b05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酥哩野夜叉主日光夜叉主。",
        "from": "T21n1288_001:0358b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日喜夜叉主露齒夜叉主。",
        "from": "T21n1288_001:0358b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "熾盛光夜叉主畏面夜叉主普照夜叉主。",
        "from": "T21n1288_001:0358b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等夜叉主。",
        "from": "T21n1288_001:0358b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具大威力騰空自在。",
        "from": "T21n1288_001:0358b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各領無數百千俱胝。",
        "from": "T21n1288_001:0358b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眷屬而來集會。",
        "from": "T21n1288_001:0358b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有大持明天眾。",
        "from": "T21n1288_001:0358b09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至諸大仙眾皆來集會。",
        "from": "T21n1288_001:0358b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大毘沙門天王與百千俱胝眷屬圍遶世尊。",
        "from": "T21n1288_001:0358b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時有天名曰那拏。",
        "from": "T21n1288_001:0358b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色相殊妙面現微笑。",
        "from": "T21n1288_001:0358b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手持日月及諸器杖。",
        "from": "T21n1288_001:0358b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾寶嚴飾光踰日月。",
        "from": "T21n1288_001:0358b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以難陀烏波難陀二龍而為絡腋。",
        "from": "T21n1288_001:0358b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得叉迦龍以為腰絛。",
        "from": "T21n1288_001:0358b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大威力如那羅延。",
        "from": "T21n1288_001:0358b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦來集會坐於佛前。",
        "from": "T21n1288_001:0358b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時毘沙門天王從座而起。",
        "from": "T21n1288_001:0358b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以諸寶華及鉢訥摩華。",
        "from": "T21n1288_001:0358b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱母捺華奔拏利迦華。",
        "from": "T21n1288_001:0358b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "優鉢羅華搔巘提迦華。",
        "from": "T21n1288_001:0358b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚩哩始迦華瞻蔔迦華。",
        "from": "T21n1288_001:0358b18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曼陀羅華摩訶曼陀羅華。",
        "from": "T21n1288_001:0358b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以如是等水陸諸華供養於佛。",
        "from": "T21n1288_001:0358b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旋繞千匝禮佛足已。",
        "from": "T21n1288_001:0358b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而白佛言世尊。",
        "from": "T21n1288_001:0358b20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我見此會大夜叉眾大羅剎眾。",
        "from": "T21n1288_001:0358b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現種種相食噉人者。",
        "from": "T21n1288_001:0358b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復見大菩薩眾大持明天眾及大仙眾。",
        "from": "T21n1288_001:0358b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至天龍夜叉羅剎阿脩羅迦樓羅緊那羅摩睺羅伽如是等眾皆來集會圍遶世尊。",
        "from": "T21n1288_001:0358b24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大祥瑞我未曾見。",
        "from": "T21n1288_001:0358b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊今者欲說何法。",
        "from": "T21n1288_001:0358b25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為說祕密曼拏羅法。",
        "from": "T21n1288_001:0358b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為說三昧之法。",
        "from": "T21n1288_001:0358b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊今正是時願廣宣說。",
        "from": "T21n1288_001:0358b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊受毘沙門天王請已。",
        "from": "T21n1288_001:0358b28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而即告言。",
        "from": "T21n1288_001:0358b28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天王我見世間一切眾生。",
        "from": "T21n1288_001:0358b29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於晝夜中有大驚怖。",
        "from": "T21n1288_001:0358b29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓋為無量大惡夜叉羅剎。",
        "from": "T21n1288_001:0358c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伺求其便而作侵害。",
        "from": "T21n1288_001:0358c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天王彼等大惡夜叉羅剎。",
        "from": "T21n1288_001:0358c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今歸依我於我教中。",
        "from": "T21n1288_001:0358c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受三歸依及受戒法。",
        "from": "T21n1288_001:0358c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸有情不復侵害。",
        "from": "T21n1288_001:0358c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊如是。",
        "from": "T21n1288_001:0358c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安慰毘沙門天王已。",
        "from": "T21n1288_001:0358c05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即入調伏夜叉熾盛普光三摩地。",
        "from": "T21n1288_001:0358c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其定中身放大光。",
        "from": "T21n1288_001:0358c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其光普照三千大千世界。",
        "from": "T21n1288_001:0358c07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有一切大惡夜叉。",
        "from": "T21n1288_001:0358c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅剎毘舍左部多。",
        "from": "T21n1288_001:0358c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸惡龍乃至宿曜等。",
        "from": "T21n1288_001:0358c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛光照已悉皆警覺。",
        "from": "T21n1288_001:0358c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其光迴還遶佛三匝入於佛頂。",
        "from": "T21n1288_001:0358c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復從面門出七色光入那拏天頂。",
        "from": "T21n1288_001:0358c10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時那拏天光入頂已。",
        "from": "T21n1288_001:0358c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即現大身如須彌山。",
        "from": "T21n1288_001:0358c11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面忿怒相復大笑相。",
        "from": "T21n1288_001:0358c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而有千臂。",
        "from": "T21n1288_001:0358c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手持葛波羅及諸器仗。",
        "from": "T21n1288_001:0358c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以虎皮絡腋葛波羅而為莊嚴。",
        "from": "T21n1288_001:0358c13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光明熾盛具大威力。",
        "from": "T21n1288_001:0358c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是那拏天現此身時。",
        "from": "T21n1288_001:0358c14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大地震動覩者皆怖。",
        "from": "T21n1288_001:0358c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時那拏天合掌向佛。",
        "from": "T21n1288_001:0358c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白言世尊我有心明。",
        "from": "T21n1288_001:0358c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善能調伏阿脩羅眾。",
        "from": "T21n1288_001:0358c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及一切夜叉羅剎部多毘舍左等。",
        "from": "T21n1288_001:0358c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至大梵天王那羅延天大自在天咸令歸伏。",
        "from": "T21n1288_001:0358c18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能句召一切天龍之眾。",
        "from": "T21n1288_001:0358c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我此心明。",
        "from": "T21n1288_001:0358c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是一切持明者最上成就之法。",
        "from": "T21n1288_001:0358c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故我今於此會中。",
        "from": "T21n1288_001:0358c20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲對如來廣大宣說。",
        "from": "T21n1288_001:0358c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸持明者令得成就。",
        "from": "T21n1288_001:0358c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速滿一切眾生心中所願。",
        "from": "T21n1288_001:0358c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊又此心明。",
        "from": "T21n1288_001:0358c22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切處念誦皆得成就。",
        "from": "T21n1288_001:0358c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至得入一切曼拏羅及諸三昧。",
        "from": "T21n1288_001:0358c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有如來部金剛部寶部蓮華部羯磨部。",
        "from": "T21n1288_001:0358c24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此諸部所有一切曼拏羅。",
        "from": "T21n1288_001:0358c25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦我心明皆得成就。",
        "from": "T21n1288_001:0358c25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及一切三昧曼拏羅印法皆悉得入。",
        "from": "T21n1288_001:0358c26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至得大總持。",
        "from": "T21n1288_001:0358c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊於此諸部平等三昧曼拏羅法明言印相。",
        "from": "T21n1288_001:0358c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願世尊哀愍攝受聽許宣說。",
        "from": "T21n1288_001:0358c28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告那拏天言。",
        "from": "T21n1288_001:0358c29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有諸部平等三昧曼拏羅法明言印相。",
        "from": "T21n1288_001:0359a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦隨喜。",
        "from": "T21n1288_001:0359a01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝那拏天為一切持明者速當宣說。",
        "from": "T21n1288_001:0359a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時那拏天承佛聖旨。",
        "from": "T21n1288_001:0359a02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為利益故遶佛三匝。",
        "from": "T21n1288_001:0359a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住立佛前即說心大明曰。",
        "from": "T21n1288_001:0359a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿他引都引入嚩二合里多忽跢嚩呼必一怛鉢多二合葛那葛末尼室攞二翳呬曳二合呬酤必多阿吒吒賀引娑三末尼鉢囉二合朗嚩賀引囉引嘌馱二合賀引囉囉唧當四󱃟許葛切栗跢羅贊捺那引㜸嚕咄嚕瑟哥二合五薩訖哩二合多散那桵設離囕六摩賀引彌嚕曼捺囉馱囉七嚩囉嚴毘囉引蘇囉㜸拏儞蘇捺囊八馱囉馱囉馱囉尼馱囉九嚴毘囉提囉毘囉毘囉十末毘囉末毘囉十一阿蘇囉㜸拏儞蘇捺𧹞十二末哩捺二合囊怛哩惹二合囊十三尾薩普二合陵㜸阿引儞爹吉囉拏引十四鉢囉二合婆引耨囉唧多曼拏羅十五葛播攞嚩嚩唐十六惹吒引吒賀引娑酤朗十七儞哩末二合朗三勃囉二合忙十八娑㜸囉蘇迷嚕鉢哩嚩二合多十九多致多囕㜸二十薩㘑羅室阿囉二十一馱覩曬羅播捺那葛囕二十二散祖蘭拏二合那葛囕二十三阿謨伽挼引捺舍儞爹曼拏羅二十四設室吉囉拏毛㘑馱囉二十五阿左羅尾末羅二十六薩葛羅儒薩那二合引捺訖哩二合多二十七阿嚩婆引娑酤朗二十八鉢囉二合末他囊二十九阿蘇囉婆嚩那酤羅那引設囊三十沒囉二合󱃟民二合捺囉二合嚕捺囉二合那末塞訖哩三合當三十一曼禰當布𡁠當阿哩唧當三十二薩哩嚩二合尾儞也二合引馱囉那莫塞訖哩二合當三十三㗚沙搖沙地三十四悉馱巘馱哩嚩二合布𡁠當三十五摩賀引迷嚕毘拏羅馱囕三十六嚩囉野虞鉢微多馱囕三十七部穰㜸多叉葛三十八嚩引蘇吉娑㜸囉三十九難奴鉢難捺曼馱葛播羅四十迷珂攞朗訖哩二合多葛致四十一薩捺喻二合提哩二合多葛播攞朗訖哩二合多設離朗四十二室嚩鉢囉二合拶㘑多毛㘑馱囕四十三嚩引喻鉢嚩那微㜸惹嚩引哩剛二合四十四拶覩囕㜸作葛囉二合拶囉𧹞四十五珂㜸拶囉𧹞四十六阿鉢囉二合迷野嚩囉嚩日囉二合母薩羅四十七頻尼朅捺誐二合妬末羅作訖囉二合四十八阿悉母娑羅怛哩二合戍羅四十九母捺誐二合囉鉢囉二合賀引囉𧹞五十那拏酤鉢囕五十一嚩囉酤鉢囕五十二蘇微商蘇儞怛囕五十三蘇嚩葛怛囕三合五十四蘇虞呬也二合剛五十五悉馱細那引鉢釘五十六那拏酤鉢囕五十七摩引嚩㗚怛二合拽設也二合引蜜五十八怛𭔞切身他引唵翳呬曳二合呬婆誐鑁引五十九那拏酤鉢囉六十󱃟娑󱃟娑六十一末娑末娑六十二酤嚕酤嚕六十三咄嚕咄嚕六十四祖嚕祖嚕六十五尾哩尾哩六十六唧致唧致六十七尾唧致尾唧致六十八嚩日朗二合嚩日囉二合嚩嚩日朗二合六十九吽吽吽七十虎多󱃟那󱃟那七十一尾特網一合薩野尾特網二合薩野七十二尾捺囉二合引鉢野尾捺囉二合引鉢野七十三嗢蹉引捺野嗢蹉引捺野七十四酤鉢囉酤鉢囉七十五那拏酤鉢囉七十六嚩囉酤鉢囉七十七酤鉢囉酤鉢囉七十八虞呬也二合酤鉢囉七十九輸婆酤鉢囉八十藥叉酤鉢囉八十一虞呬也二合酤鉢囉八十二末哩捺二合末哩捺二合八十三㜸哩惹二合㜸哩惹二合八十四那哩左二合那哩左二合八十五󱃟哩阿引那野󱃟那八十六󱃟哩󱃟哩路左那八十七囉訖多二合路左那八十八儞羅路左那八十九悉多路左那囉訖多二合路左那九十訖哩二合瑟拏二合路左那九十一尾訖哩二合多路左那九十二禰嚩也二合路左那九十三蘇嚩蘭拏二合路左那九十四悉地路左那九十五迷伽瞿數努婆二合嚩母努婆二合嚩九十六賀攞󱃟羅設沒捺二合葛羅引鉢野九十七呬呬那拏酤鉢囉九十八係係那拏酤鉢囉酤鉢囉九十九薩囉酤鉢囉一百阿引尾捨阿引尾捨酤鉢囉酤鉢囉一百一嚩囉酤鉢囉一百二拶羅拶羅一百三毛㘑馱囉一百四左引羅野左引羅野嚩日朗二合一百五羅仵羅仵朅捺昂二合一百六俱嚕俱嚕一百七阿吒吒賀引娑引俱朗一百八三俱羅俱朗一百九葛羅葛羅一百十吉㘑吉㘑一百一十一俱路俱路一百一十二捫左捫左一百一十三設沒捺二合引俱朗一百一十四那拏母捺誐二合囉一百一十五那拏酤鉢囉一百一十六嚩囉嚩囉嚩日囉二合馱囉一百一十七鉢捺摩二合馱囉一百一十八母娑羅馱囉一百一十九作訖囉二合馱囉一百二十朗虞羅馱囉一百二十一鉢囉戍馱囉一百二十二播設馱囉一百二十三阿喻母娑羅鉢囉二合賀引囉拏馱囉一百二十四馱囉尼馱囉一百二十五贊捺囉二合蘇哩也二合摩引攞馱囉一百二十六諾叉怛囉三合跢囉引誐囉二合󱃟曼拏羅馱囉一百二十七挼捺捨引禰爹入嚩二合引攞帝惹葛囉一百二十八鉢囉二合婆引耨囉唧多曼拏羅一百二十九娑引㜸囉薩囉尾輸殺拏葛囉一百三十末且末葛囉室戍摩引囉一百三十一訖哩二合多播引設馱囉一百三十二阿蘇囉㜸拏引囊一百三十三末哩捺二合那葛囉一百三十四供伴引拏藥叉囉叉娑一百三十五紇哩二合捺喻那末二合捺那葛囕一百三十六薩怛鑁三合婆誐鑁那拏酤鉢囉酤鉢囉一百三十七那哩左二合那哩左二合一百三十八󱃟哩󱃟哩一百三十九󱃟哩路左那一百四十嚩囉路左那一百四十一路左那一百四十二尾訖哩二合多路左那一百四十三蘇嚩蘭拏二合路左那一百四十四悉馱路左那一百四十五阿引尾捨阿引尾捨一百四十六尸伽囕二合悉殿覩滿怛囉二合鉢捺娑嚩二合引賀引一百四十七爾時那拏天說此明已。",
        "from": "T21n1288_001:0360a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復白佛言。",
        "from": "T21n1288_001:0360a08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我此心明。",
        "from": "T21n1288_001:0360a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有行人依法持誦得精熟已。",
        "from": "T21n1288_001:0360a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉能成就千種之事。",
        "from": "T21n1288_001:0360a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有行人欲止雷雹者。",
        "from": "T21n1288_001:0360a10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用刀劍或石榴枝。",
        "from": "T21n1288_001:0360a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或羯囉尾囉枝或水或灰。",
        "from": "T21n1288_001:0360a11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等物。",
        "from": "T21n1288_001:0360a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此明加持一遍。",
        "from": "T21n1288_001:0360a12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然可用之即得止息。",
        "from": "T21n1288_001:0360a12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若有行人。",
        "from": "T21n1288_001:0360a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為彼諸惡夜叉羅剎所執持者。",
        "from": "T21n1288_001:0360a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遣除者當用安悉香。",
        "from": "T21n1288_001:0360a14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此明加持一遍。",
        "from": "T21n1288_001:0360a14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後焚燒熏彼執者。",
        "from": "T21n1288_001:0360a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞此香已彼即說事便得止息。",
        "from": "T21n1288_001:0360a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若復行人。",
        "from": "T21n1288_001:0360a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲句召一切天龍夜叉乾闥婆阿脩羅迦樓羅緊那羅摩睺羅伽。",
        "from": "T21n1288_001:0360a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及羅剎部多毘舍左等。",
        "from": "T21n1288_001:0360a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用安悉香。",
        "from": "T21n1288_001:0360a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此明加持一遍。",
        "from": "T21n1288_001:0360a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒之句召即至。",
        "from": "T21n1288_001:0360a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲發遣者。",
        "from": "T21n1288_001:0360a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦用安悉香誦此明。",
        "from": "T21n1288_001:0360a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持一遍如前焚之。",
        "from": "T21n1288_001:0360a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發遣即去。",
        "from": "T21n1288_001:0360a21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲令鉢說事及舉動者。",
        "from": "T21n1288_001:0360a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用鉢滿盛燒瓦灰。",
        "from": "T21n1288_001:0360a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安曼拏羅中誦此明。",
        "from": "T21n1288_001:0360a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持安悉香焚燒。",
        "from": "T21n1288_001:0360a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復誦此明一遍。",
        "from": "T21n1288_001:0360a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其鉢作人言。",
        "from": "T21n1288_001:0360a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為行人說事及能舉動。",
        "from": "T21n1288_001:0360a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲令閼伽瓶說事及舉動者。",
        "from": "T21n1288_001:0360a26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當選純白閼伽瓶潔淨新好者。",
        "from": "T21n1288_001:0360a27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用白檀及恭俱摩香等。",
        "from": "T21n1288_001:0360a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作塗香塗之。",
        "from": "T21n1288_001:0360a28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿盛淨水及插妙華。",
        "from": "T21n1288_001:0360a28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦心明加持一遍。",
        "from": "T21n1288_001:0360a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安曼拏羅中燒安悉香。",
        "from": "T21n1288_001:0360a29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦心明一遍。",
        "from": "T21n1288_001:0360b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其瓶同前說事。",
        "from": "T21n1288_001:0360b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復誦心明加持白芥子打瓶。",
        "from": "T21n1288_001:0360b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其瓶即舉離地八指。",
        "from": "T21n1288_001:0360b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意復還曼拏羅中。",
        "from": "T21n1288_001:0360b02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲令薩擔婆舉動者。",
        "from": "T21n1288_001:0360b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先用白檀及恭俱摩香等。",
        "from": "T21n1288_001:0360b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作塗香塗之。",
        "from": "T21n1288_001:0360b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後燒安悉香。",
        "from": "T21n1288_001:0360b04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彈指誦心明加持。",
        "from": "T21n1288_001:0360b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其薩擔婆自然舉動復還本處。",
        "from": "T21n1288_001:0360b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲令傍生說事者。",
        "from": "T21n1288_001:0360b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當燒安悉香熏灰及白芥子。",
        "from": "T21n1288_001:0360b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦心明加持二十一遍已。",
        "from": "T21n1288_001:0360b08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後復誦心明。",
        "from": "T21n1288_001:0360b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以灰芥子擲傍生身彼即說事。",
        "from": "T21n1288_001:0360b09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲令童女說事者。",
        "from": "T21n1288_001:0360b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用不墮地瞿摩夷。",
        "from": "T21n1288_001:0360b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗四方曼拏羅已。",
        "from": "T21n1288_001:0360b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然擇一童女相貌端嚴肢體具足者。",
        "from": "T21n1288_001:0360b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沐浴潔淨著新白衣。",
        "from": "T21n1288_001:0360b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐曼拏羅中。",
        "from": "T21n1288_001:0360b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以眾華供養燒安悉香。",
        "from": "T21n1288_001:0360b13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼即能說過現未來之事。",
        "from": "T21n1288_001:0360b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲令童子說事者。",
        "from": "T21n1288_001:0360b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用白芥子與唧帝灰相和。",
        "from": "T21n1288_001:0360b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦心明加持一遍。",
        "from": "T21n1288_001:0360b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒安悉香熏已擲童子頂。",
        "from": "T21n1288_001:0360b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼即說事。",
        "from": "T21n1288_001:0360b17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲制火不熱者。",
        "from": "T21n1288_001:0360b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用木柴誦心明。",
        "from": "T21n1288_001:0360b18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持一遍。",
        "from": "T21n1288_001:0360b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒安悉香熏已內於火中。",
        "from": "T21n1288_001:0360b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火即冷如雪。",
        "from": "T21n1288_001:0360b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲制諸邪呪者。",
        "from": "T21n1288_001:0360b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用潔淨白線。",
        "from": "T21n1288_001:0360b21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦心明加持一遍。",
        "from": "T21n1288_001:0360b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒安悉香熏已。",
        "from": "T21n1288_001:0360b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫行人左臂。",
        "from": "T21n1288_001:0360b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切邪呪悉皆制伏。",
        "from": "T21n1288_001:0360b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲令沒訥誐囉作成就法者。",
        "from": "T21n1288_001:0360b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用不墮地瞿摩夷塗曼拏羅。",
        "from": "T21n1288_001:0360b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以沒訥誐囉安曼拏羅中。",
        "from": "T21n1288_001:0360b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用眾華供養燒安悉香熏。",
        "from": "T21n1288_001:0360b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦心明加持一遍。",
        "from": "T21n1288_001:0360b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此沒訥誐囉能作成就之事。",
        "from": "T21n1288_001:0360b27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復能令隨意舉動。",
        "from": "T21n1288_001:0360b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此沒訥誐囉若入戰陣自然得勝。",
        "from": "T21n1288_001:0360b28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至樹林山石亦能摧壞。",
        "from": "T21n1288_001:0360b29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡是行人所作之事悉皆成就。",
        "from": "T21n1288_001:0360c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲令弓作成就法者。",
        "from": "T21n1288_001:0360c02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用安悉香誦心明加持一遍。",
        "from": "T21n1288_001:0360c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "焚燒熏之。",
        "from": "T21n1288_001:0360c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即能隨意作成就事。",
        "from": "T21n1288_001:0360c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲令三叉作成就法。",
        "from": "T21n1288_001:0360c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同前作曼拏羅。",
        "from": "T21n1288_001:0360c05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後誦心明一遍。",
        "from": "T21n1288_001:0360c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持安悉香焚燒熏三叉已。",
        "from": "T21n1288_001:0360c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安曼拏羅中復誦心明一遍。",
        "from": "T21n1288_001:0360c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時彼三叉即放光熾盛。",
        "from": "T21n1288_001:0360c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉動離地向於四方。",
        "from": "T21n1288_001:0360c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後迴還入曼拏羅中。",
        "from": "T21n1288_001:0360c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作成就法隨行人意。",
        "from": "T21n1288_001:0360c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲令劍作成就法者。",
        "from": "T21n1288_001:0360c10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦用安悉香誦心明。",
        "from": "T21n1288_001:0360c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持一遍焚燒熏之。",
        "from": "T21n1288_001:0360c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能作一切成就之事。",
        "from": "T21n1288_001:0360c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲令朅樁誐作成就法者。",
        "from": "T21n1288_001:0360c13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用唧帝灰誦心明。",
        "from": "T21n1288_001:0360c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持一遍塗朅樁誐。",
        "from": "T21n1288_001:0360c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即能作一切成就之事。",
        "from": "T21n1288_001:0360c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲令葛波羅作成就法者。",
        "from": "T21n1288_001:0360c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先依法塗曼拏羅已。",
        "from": "T21n1288_001:0360c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即依法擇取葛波羅潔淨者。",
        "from": "T21n1288_001:0360c17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用白檀香塗之。",
        "from": "T21n1288_001:0360c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安曼拏羅中。",
        "from": "T21n1288_001:0360c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒安悉香誦心明一遍加持。",
        "from": "T21n1288_001:0360c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即所作成就能令舉動。",
        "from": "T21n1288_001:0360c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復能說三世之事。",
        "from": "T21n1288_001:0360c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法當用利劍誦心明。",
        "from": "T21n1288_001:0360c21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持一遍燒安悉香熏。",
        "from": "T21n1288_001:0360c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後行人高聲誦吽字加持彼劍。",
        "from": "T21n1288_001:0360c22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劍即出光騰空而起。",
        "from": "T21n1288_001:0360c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復還本處。",
        "from": "T21n1288_001:0360c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若入戰陣彼敵退敗。",
        "from": "T21n1288_001:0360c23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲令天祠形像遷動者。",
        "from": "T21n1288_001:0360c24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彼像前燒安悉香。",
        "from": "T21n1288_001:0360c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦心明一遍。",
        "from": "T21n1288_001:0360c25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時彼天像隨意遷動。",
        "from": "T21n1288_001:0360c25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若隨方處地。",
        "from": "T21n1288_001:0360c26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行人疑有伏藏當欲取者。",
        "from": "T21n1288_001:0360c26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於彼地作曼拏羅。",
        "from": "T21n1288_001:0360c27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒安悉香誦心明一遍。",
        "from": "T21n1288_001:0360c27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏藏出現。",
        "from": "T21n1288_001:0360c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法或用白芥子誦心明加持一遍。",
        "from": "T21n1288_001:0360c29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此芥子隨處擲之。",
        "from": "T21n1288_001:0361a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或聚落中或空舍中。",
        "from": "T21n1288_001:0361a01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或天祠像前或塔廟之處。",
        "from": "T21n1288_001:0361a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至山巖龍潭等處。",
        "from": "T21n1288_001:0361a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨所擲處燒安悉香誦心明一遍。",
        "from": "T21n1288_001:0361a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼有伏藏皆悉出現。",
        "from": "T21n1288_001:0361a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法用白芥子誦心明加持一遍。",
        "from": "T21n1288_001:0361a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此芥子擲於象身。",
        "from": "T21n1288_001:0361a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及燒安悉香熏。",
        "from": "T21n1288_001:0361a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復誦心明一遍。",
        "from": "T21n1288_001:0361a06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼象隨行人意善能作事。",
        "from": "T21n1288_001:0361a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或令破設咄嚕亦得隨意。",
        "from": "T21n1288_001:0361a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲為人除瘧病及作擁護者。",
        "from": "T21n1288_001:0361a09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用灰及白芥子水和。",
        "from": "T21n1288_001:0361a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦心明一遍加持。",
        "from": "T21n1288_001:0361a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒安悉香熏已戴於身上。",
        "from": "T21n1288_001:0361a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瘧病即差及得擁護。",
        "from": "T21n1288_001:0361a11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲止一切怖者。",
        "from": "T21n1288_001:0361a12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用白線燒安悉香熏。",
        "from": "T21n1288_001:0361a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦心明加持一遍。",
        "from": "T21n1288_001:0361a13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戴於頸上諸怖即除。",
        "from": "T21n1288_001:0361a13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲除諸病者。",
        "from": "T21n1288_001:0361a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用安悉香誦心明加持一遍。",
        "from": "T21n1288_001:0361a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "焚燒熏病者身諸病即除。",
        "from": "T21n1288_001:0361a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲設咄嚕令得入嚩囉者。",
        "from": "T21n1288_001:0361a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用安悉香誦心明加持一遍。",
        "from": "T21n1288_001:0361a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即可令千數設咄嚕皆得入嚩囉。",
        "from": "T21n1288_001:0361a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲令設咄嚕左都哩他哥入嚩囉生者。",
        "from": "T21n1288_001:0361a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用忤摩娑與安悉香相和。",
        "from": "T21n1288_001:0361a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦心明加持一遍。",
        "from": "T21n1288_001:0361a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱設咄嚕名作護摩七遍即得左都哩他哥入嚩囉。",
        "from": "T21n1288_001:0361a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲令止息者。",
        "from": "T21n1288_001:0361a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦用安悉香誦心明加持一遍。",
        "from": "T21n1288_001:0361a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作護摩七遍即得止息。",
        "from": "T21n1288_001:0361a23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲令設咄嚕。",
        "from": "T21n1288_001:0361a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得屹囉賀執持者。",
        "from": "T21n1288_001:0361a24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用安悉香。",
        "from": "T21n1288_001:0361a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將設咄嚕名與心明同誦。",
        "from": "T21n1288_001:0361a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持七遍已。",
        "from": "T21n1288_001:0361a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擲入火內。",
        "from": "T21n1288_001:0361a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得屹囉賀執持。",
        "from": "T21n1288_001:0361a26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲令止息者。",
        "from": "T21n1288_001:0361a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用灰加持一遍。",
        "from": "T21n1288_001:0361a27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戴於頂上即得止息。",
        "from": "T21n1288_001:0361a27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲除諸瘧病者。",
        "from": "T21n1288_001:0361a28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用黑線誦心明加持一遍。",
        "from": "T21n1288_001:0361a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫臂即差。",
        "from": "T21n1288_001:0361a29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲除諸疾病者。",
        "from": "T21n1288_001:0361b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用七種穀及白芥子相和。",
        "from": "T21n1288_001:0361b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "搵酥誦心明作護摩即差。",
        "from": "T21n1288_001:0361b02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲解諸毒藥所中者。",
        "from": "T21n1288_001:0361b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用孔雀尾誦心明。",
        "from": "T21n1288_001:0361b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持已拂於彼身毒即解除。",
        "from": "T21n1288_001:0361b04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如上諸法皆用安悉香。",
        "from": "T21n1288_001:0361b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦心明加持悉得成就。",
        "from": "T21n1288_001:0361b05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說最上祕密那拏天經卷上",
        "from": "T21n1288_001:0361b06_100",
        "context_with_next_sentence": false
    }
]