[
    {
        "source_sentence": "景教三威蒙度讚",
        "from": "T54n2143_001:1288b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無上諸天深敬歎　　大地重念普安和",
        "from": "T54n2143_001:1288b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人元真性蒙依止　　三才慈父阿羅訶",
        "from": "T54n2143_001:1288b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切善眾至誠禮　　一切慧性稱讚歌",
        "from": "T54n2143_001:1288b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切含真盡歸仰　　蒙聖慈光救離魔",
        "from": "T54n2143_001:1288b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難尋無及正真常　　慈父明子淨風王",
        "from": "T54n2143_001:1288b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸帝中為師帝　　於諸世尊為法皇",
        "from": "T54n2143_001:1288b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常居妙明無畔界　　光威盡察有界疆",
        "from": "T54n2143_001:1288b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自始無人嘗得見　　復以色見不可相",
        "from": "T54n2143_001:1288b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟獨絕凝清淨德　　惟獨神威無等力",
        "from": "T54n2143_001:1288b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟獨不轉儼然存　　眾善根本復無極",
        "from": "T54n2143_001:1288b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今一切念慈恩　　歎彼妙樂照此國",
        "from": "T54n2143_001:1288b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彌施訶普尊大聖子　　廣度苦界救無億",
        "from": "T54n2143_001:1288b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常活命王慈喜羔　　大普耽苦不辭勞",
        "from": "T54n2143_001:1288b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願捨群生積重罪　　善護真性得無繇",
        "from": "T54n2143_001:1288b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖子端任父右座　　其座復超無𭨝高",
        "from": "T54n2143_001:1288b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大師願彼乞眾請　　降栰使免火江漂",
        "from": "T54n2143_001:1288b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大師是我等慈父　　大師是我等聖主",
        "from": "T54n2143_001:1288b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大師是我等法王　　大師能為普救度",
        "from": "T54n2143_001:1288b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大師慧力助諸𮊵　　諸目瞻仰不暫移",
        "from": "T54n2143_001:1288b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復與枯燋降甘露　　所有蒙潤善根滋",
        "from": "T54n2143_001:1288b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大聖普尊彌施訶　　我歎慈父海藏慈",
        "from": "T54n2143_001:1288b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大聖謙及淨風性　　清凝法耳不思議",
        "from": "T54n2143_001:1288b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　證身盧訶寧俱沙　　已上三身同歸一體",
        "from": "T54n2143_001:1288c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瑜罕難法王　盧伽法王　摩矩辭法王　明泰法王　牟世法王　多惠法王　景通法王　寶路法王　千眼法王　那寧逸法王　珉艶法王　摩薩吉思法王　宜和吉思法王　摩沒吉思法王　岑穩僧法王　二十四聖法王　憲難耶法王　賀薩耶法　彌沙曳法王　娑羅法王　瞿盧法王　報信法王敬禮常明皇樂經　宣元至本經　志玄安樂經　天寶藏經　多惠聖王經　阿思瞿利容經　渾元經　通真經　寶明經　傳化經　罄遺經　原靈經　述略經　三際經　徵詰經　寧思經　宣義經　師利海經　寶路法王經　刪河律經　藝利月思經　寧耶𮟖經　儀則律經　毘遏啟經　三威讚經　牟世法王經　伊利耶經　遏拂林經　報信法王經　彌施訶自在天地經　四門經　啟真經　摩薩吉斯經　慈利波經　烏沙那經謹案諸經目錄。",
        "from": "T54n2143_001:1288c21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大秦本教經都五百卅部。",
        "from": "T54n2143_001:1288c21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並是貝葉梵音。",
        "from": "T54n2143_001:1288c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐太宗皇帝貞觀九年。",
        "from": "T54n2143_001:1288c22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西域太德僧阿羅本屆于中夏。",
        "from": "T54n2143_001:1288c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並奏上本音。",
        "from": "T54n2143_001:1288c23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "房玄齡魏徵宣譯奏言。",
        "from": "T54n2143_001:1288c23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後召本教大德僧景淨。",
        "from": "T54n2143_001:1288c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譯得已上卅部卷。",
        "from": "T54n2143_001:1288c24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘大數具在貝皮夾。",
        "from": "T54n2143_001:1288c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶未翻譯。",
        "from": "T54n2143_001:1288c25_6",
        "context_with_next_sentence": true
    }
]