[
    {
        "source_sentence": "佛說金色迦那鉢底陀羅尼經",
        "from": "T21n1269_001:0303a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏金剛智奉　詔譯",
        "from": "T21n1269_001:0303a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。一時佛在舍衛國迦蘭竹林園。",
        "from": "T21n1269_001:0303a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大說法愍念一切眾生。",
        "from": "T21n1269_001:0303a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此陀羅尼。",
        "from": "T21n1269_001:0303a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屬金色迦那鉢底。",
        "from": "T21n1269_001:0303a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊即說真言曰。",
        "from": "T21n1269_001:0303a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨率都底一迦吒迦吒二底吒摩吒三起哩二合亹拏二合四起哩二合亹拏二合五畔惹畔惹二合六曩謨率都帝七護嚕捺八跛惹尼曳九娑縛賀十阿部多十一浪拏乞懺二合質多十二曷羅引娑摩誐磋底丁以反十三摩訶野十四摩賀悉底二合十五諾乞二合曩十六鉢羅二合句跛野引弭引十七矩嚕矩嚕十八祖嚕祖嚕十九謨嚕謨嚕二十曩謨曩謨二十一佛告舍利弗。",
        "from": "T21n1269_001:0303a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是金色迦那鉢底除障難真言。",
        "from": "T21n1269_001:0303a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若眾生受持真言者。",
        "from": "T21n1269_001:0303a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所作諸法無不成。",
        "from": "T21n1269_001:0303a22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大驗當知。",
        "from": "T21n1269_001:0303a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定有障難。",
        "from": "T21n1269_001:0303a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於白月若黑月二日八日十五日。",
        "from": "T21n1269_001:0303a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牛糞塗地作壇種種供養所作成就。",
        "from": "T21n1269_001:0303a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金色迦那鉢底白佛言。",
        "from": "T21n1269_001:0303a25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當擁護持此真言者。",
        "from": "T21n1269_001:0303a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生持此真言者。",
        "from": "T21n1269_001:0303a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "某人所須資財臥具衣服飲食金銀珍寶奴婢從不舊乏少。",
        "from": "T21n1269_001:0303a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切真言法速令成所。",
        "from": "T21n1269_001:0303a28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作就如意。",
        "from": "T21n1269_001:0303a28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其像形人身象頭六臂。",
        "from": "T21n1269_001:0303a29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於白㲲上畫之。",
        "from": "T21n1269_001:0303a29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刻作用得。",
        "from": "T21n1269_001:0303b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白檀紫檀苦練木通用。",
        "from": "T21n1269_001:0303b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘木不得。",
        "from": "T21n1269_001:0303b01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若綵色勿用皮膠。",
        "from": "T21n1269_001:0303b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須用香汁及有汁木。",
        "from": "T21n1269_001:0303b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其身正立。",
        "from": "T21n1269_001:0303b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鼻向右曲。",
        "from": "T21n1269_001:0303b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左上手把刀。",
        "from": "T21n1269_001:0303b03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次手把歡喜團。",
        "from": "T21n1269_001:0303b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下手把劍棒。",
        "from": "T21n1269_001:0303b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次手把縛折羅。",
        "from": "T21n1269_001:0303b04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下手把索。",
        "from": "T21n1269_001:0303b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身作金色。",
        "from": "T21n1269_001:0303b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脚蹈金山。",
        "from": "T21n1269_001:0303b05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭上五色雲。",
        "from": "T21n1269_001:0303b05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雲內有四天王及諸仙散華。",
        "from": "T21n1269_001:0303b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左邊有俱摩羅淨軍。",
        "from": "T21n1269_001:0303b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右邊有阿吒薄俱元率大將。",
        "from": "T21n1269_001:0303b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向下左畫美女。",
        "from": "T21n1269_001:0303b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "音樂供養立。",
        "from": "T21n1269_001:0303b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右畫四大藥叉。",
        "from": "T21n1269_001:0303b08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各執器仗一半頭一猪頭一象頭一馬頭。",
        "from": "T21n1269_001:0303b09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆著虎皮褌。",
        "from": "T21n1269_001:0303b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作像之時莫令見雜人等。",
        "from": "T21n1269_001:0303b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書畢。",
        "from": "T21n1269_001:0303b10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒香清淨不食五辛。",
        "from": "T21n1269_001:0303b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於淨室中安置其壇。",
        "from": "T21n1269_001:0303b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方圓大小隨意作之。",
        "from": "T21n1269_001:0303b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壇中燒安悉香白膠沈水香等。",
        "from": "T21n1269_001:0303b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用隨餅乳粥歡喜團蘇蜜菓子種種供養。",
        "from": "T21n1269_001:0303b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所作隨心。",
        "from": "T21n1269_001:0303b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若鬼神難調伏者。",
        "from": "T21n1269_001:0303b14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "搓白線呪之。",
        "from": "T21n1269_001:0303b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一遍一結滿四十九結。",
        "from": "T21n1269_001:0303b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫左臂。",
        "from": "T21n1269_001:0303b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切鬼悉皆被縛一受持人。",
        "from": "T21n1269_001:0303b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不須邪婬妄語作誑。",
        "from": "T21n1269_001:0303b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其禁食已止芸薹葫荽五辛。",
        "from": "T21n1269_001:0303b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得食。",
        "from": "T21n1269_001:0303b17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故入色孝家生產處。",
        "from": "T21n1269_001:0303b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印法并通用。",
        "from": "T21n1269_001:0303b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸毘那翼迦法通用。",
        "from": "T21n1269_001:0303b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿親惡人。",
        "from": "T21n1269_001:0303b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同行者施之無妨。",
        "from": "T21n1269_001:0303b19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金色迦那鉢底經",
        "from": "T21n1269_001:0303b20_100",
        "context_with_next_sentence": false
    }
]