[
    {
        "source_sentence": "佛說灌頂百結神王護身呪經佛說灌頂百結神王護身呪經卷",
        "from": "T21n1331_004:0504c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四",
        "from": "T21n1331_004:0504c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東晉天竺三藏帛尸梨蜜多羅譯",
        "from": "T21n1331_004:0504c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞如是。",
        "from": "T21n1331_004:0504c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在羅閱祇國大精舍中。",
        "from": "T21n1331_004:0504c17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有千二百五十弟子各住餘室。",
        "from": "T21n1331_004:0504c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛獨一房自思念言。",
        "from": "T21n1331_004:0504c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我涅槃後四部弟子。",
        "from": "T21n1331_004:0504c19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持戒不具多所毀犯。",
        "from": "T21n1331_004:0504c19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造作非法不行十善。",
        "from": "T21n1331_004:0504c20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我法既滅末世之中。",
        "from": "T21n1331_004:0504c20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼魔亂起行諸邪惡嬈惱人民。",
        "from": "T21n1331_004:0504c21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又有毒龍吐毒害人。",
        "from": "T21n1331_004:0504c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當云何而辟除之作是念已默然而住。",
        "from": "T21n1331_004:0504c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是天帝知佛所念。",
        "from": "T21n1331_004:0504c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從坐而起如人屈申臂頃。",
        "from": "T21n1331_004:0504c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來至佛所稽首佛足。",
        "from": "T21n1331_004:0504c24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作禮畢已長跪叉手。",
        "from": "T21n1331_004:0504c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前請佛言惟願世尊愍濟眾生。",
        "from": "T21n1331_004:0504c25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為說無上灌頂章句大神王名字。",
        "from": "T21n1331_004:0504c26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "守護萬姓故。",
        "from": "T21n1331_004:0504c26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使得安隱離諸危厄度於邪惡。",
        "from": "T21n1331_004:0504c27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使諸魔鬼不得作害。",
        "from": "T21n1331_004:0504c27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結願神名常在左右。",
        "from": "T21n1331_004:0504c28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為人防惡使毒不行。",
        "from": "T21n1331_004:0504c28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所請如是惟願演之。",
        "from": "T21n1331_004:0504c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告天帝釋善哉善哉諦聽諦受。",
        "from": "T21n1331_004:0505a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾今為汝而演說之。",
        "from": "T21n1331_004:0505a01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸世間一切眾生。",
        "from": "T21n1331_004:0505a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有受三自歸者。",
        "from": "T21n1331_004:0505a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡帶持此百大神王名。",
        "from": "T21n1331_004:0505a02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以護人身辟除邪惡。",
        "from": "T21n1331_004:0505a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使萬毒不行百姓安寧。",
        "from": "T21n1331_004:0505a03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若干億神恒沙數鬼皆不得留住。",
        "from": "T21n1331_004:0505a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶神名者身中有鬼神在其身中不去者。",
        "from": "T21n1331_004:0505a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四天王當遣使者。",
        "from": "T21n1331_004:0505a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持金剛杵碎頭作七分。",
        "from": "T21n1331_004:0505a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天帝釋白佛言。",
        "from": "T21n1331_004:0505a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊所稱譽者其法微妙。",
        "from": "T21n1331_004:0505a07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願演說化於未聞。",
        "from": "T21n1331_004:0505a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語天帝此灌頂章句結願呪經。",
        "from": "T21n1331_004:0505a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甚深微妙不可妄說。",
        "from": "T21n1331_004:0505a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛世尊不妄宣授度與人也。",
        "from": "T21n1331_004:0505a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今我出世值於五惡。",
        "from": "T21n1331_004:0505a10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我若不說此結願呪經者。",
        "from": "T21n1331_004:0505a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸弊小鬼互來嬈人。",
        "from": "T21n1331_004:0505a11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覓人飲食求人長短。",
        "from": "T21n1331_004:0505a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今為汝及一切人而演說之。",
        "from": "T21n1331_004:0505a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語天帝釋言。",
        "from": "T21n1331_004:0505a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有百神王今在須彌山頂上居止。",
        "from": "T21n1331_004:0505a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我以威神召其使來。",
        "from": "T21n1331_004:0505a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面勅神王以護汝等。",
        "from": "T21n1331_004:0505a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不令遭橫結縷呼其名字。",
        "from": "T21n1331_004:0505a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸神王常當在汝左右為汝作護。",
        "from": "T21n1331_004:0505a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天帝釋言其名何等。",
        "from": "T21n1331_004:0505a17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言。",
        "from": "T21n1331_004:0505a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神名伊利寐鞮　字德無礙神名尼烏厨遮　字棄自大神名波羅那頭　字遊安寧神名無和遮樓　字歸正化神名薩多波羅　字救脫厄神名嘻摩和頭　字暉光照神名天樓眸俱　字宣言教神名阿羅或駒　字開達明神名那羅那移　字隨順彼是九神王當以威神為某作護辟除凶惡無諸惱患。",
        "from": "T21n1331_004:0505a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他餘鬼神不得其便。",
        "from": "T21n1331_004:0505a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠百由旬無相嬈害。",
        "from": "T21n1331_004:0505b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶持結願神王名字。",
        "from": "T21n1331_004:0505b01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外諸惡魔無不除却。",
        "from": "T21n1331_004:0505b01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲善利安令得吉祥。",
        "from": "T21n1331_004:0505b02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神名摩醯首羅　字威靈帝神名摩尼拔陀　字威伏行神名富那拔陀　字集至誠神名金毘羅陀　字威如王神名質多斯那　字知敬上神名賓頭盧伽　字立不動神名車鉢羅婆　字忍德脫是七神王當以已之威力。",
        "from": "T21n1331_004:0505b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共擁護某除不吉祥。",
        "from": "T21n1331_004:0505b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳥鳴惡夢野獸變怪。",
        "from": "T21n1331_004:0505b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因衰嬈人者不得害某。",
        "from": "T21n1331_004:0505b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶持結願神名字故獲福如是。",
        "from": "T21n1331_004:0505b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神名曇摩拔羅　字學帝王神名摩竭波羅　字除曲心神名繡利密多　字有功勳神名勒那翅奢　字調和平神名斂摩舍帝　字伏眾根神名奢羅密帝　字獨處快神名醯摩拔陀　字應念至是七神王當以威神之力為某作護。",
        "from": "T21n1331_004:0505b20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若入江海湖池溪谷。",
        "from": "T21n1331_004:0505b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水中雜毒蛟龍之屬。",
        "from": "T21n1331_004:0505b21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懷惡心者風波起時。",
        "from": "T21n1331_004:0505b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以某帶持結願神名。",
        "from": "T21n1331_004:0505b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然安隱過度厄難。",
        "from": "T21n1331_004:0505b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所到安寧吉利度岸。",
        "from": "T21n1331_004:0505b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神名薩多琦梨　字大力天神名波梨羅睺　字勇猛進神名毘摩質多　字響高遠神名睒摩利子　字瑛雄德神名波阿梨子　字威武盛神名佉羅騫陀　字吼如雷神名鳩羅檀提　字戰無敵是七神王當以威德在所作護。",
        "from": "T21n1331_004:0505c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若入異道聚會之中。",
        "from": "T21n1331_004:0505c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲食有毒蠱道所中。",
        "from": "T21n1331_004:0505c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食其飲食自然消化毒為不行。",
        "from": "T21n1331_004:0505c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶持結願神王力故。",
        "from": "T21n1331_004:0505c04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現世獲福其報如是。",
        "from": "T21n1331_004:0505c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神名和耆羅桓　字堅不動神名摩尼鉢羅　字演暢音神名阿波提羅　字喜無懼神名曇無利羅　字赫嚴飾神名揵陀尼吁　字堅住行神名劬摩和羅　字清淨明神名摩呵闍離　字越諸難是七神王當以威神為某作護。",
        "from": "T21n1331_004:0505c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有怨家以毒惡心妬賢嫉能。",
        "from": "T21n1331_004:0505c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或諍財寶已為咎恨鬪諍不已。",
        "from": "T21n1331_004:0505c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是七神王即以威力解除二家。",
        "from": "T21n1331_004:0505c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使其和解俱生善心惡意悉滅。",
        "from": "T21n1331_004:0505c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶持結縷神王名字現報如是。",
        "from": "T21n1331_004:0505c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神名闍那阿梨　字善威光神名耆羅斯耨　字事業得神名勒迦設婁　字除煩惱神名摩遮多梨　字補天位神名羅摩奴遮　字開正路神名𬛜迦盧遮　字力堅固神名阿梨盧黃　字離諸趣是七神王當以威神佐助左右。",
        "from": "T21n1331_004:0505c25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "治生賣買常獲倍利。",
        "from": "T21n1331_004:0505c26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不與怨家盜賊惡伴之所牽引。",
        "from": "T21n1331_004:0505c26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所在遊行見者悉喜。",
        "from": "T21n1331_004:0505c27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和悅相向無瞋惱色。",
        "from": "T21n1331_004:0505c27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶持結縷神王名故現報如是。",
        "from": "T21n1331_004:0505c28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神名鴛那攎遮　字除怖懼神名惟舍羅遮　字超出難神名蠅迦陀羅　字覆天地神名遮羅珞迦　字自無畏神名臀頭梨迦　字德如山神名那惟師尼　字思無愛神名陀優陀羅　字住無畏是七神王當以威神為某作護。",
        "from": "T21n1331_004:0506a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辟除縣官危厄之難。",
        "from": "T21n1331_004:0506a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若合死事鞭撻便除。",
        "from": "T21n1331_004:0506a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "枷鎖杻械杖楚之罰。",
        "from": "T21n1331_004:0506a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "罵辱唾吐即便除解。",
        "from": "T21n1331_004:0506a09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以某帶持神名力故現報如是。",
        "from": "T21n1331_004:0506a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神名富阿陀羅　字勝幡懸神名沙羅和羅　字弘聖言神名那羅訶摩　字施無盡神名醯波舍摩　字將導安神名阿抄牟羅　字覺無常神名比抄倚耶　字分別解神名至羅和移　字善勸吉是七神王當以威神主產生者。",
        "from": "T21n1331_004:0506a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不令他餘邪惡鬼神閉其生門帶持結願神名力故。",
        "from": "T21n1331_004:0506a19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到產之日身體安寧。",
        "from": "T21n1331_004:0506a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無諸痛惱兒則易生。",
        "from": "T21n1331_004:0506a20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左右善神扶佐生者。",
        "from": "T21n1331_004:0506a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶持結縷獲福如是。",
        "from": "T21n1331_004:0506a21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神名抄摩如提　字滿無及神名首抄和提　字威如天神名阿抄惒泥　字辯無喻神名脾羅摩尼　字無復疑神名修吼摩耶　字降小魔神名提摩陀伊　字解生死神名架抄優陀　字拔愛根是七神王當以威神。",
        "from": "T21n1331_004:0506a29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "主五溫山海之中諸小弊龍。",
        "from": "T21n1331_004:0506b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各吐惡毒五色之氣。",
        "from": "T21n1331_004:0506b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "侵陵萬民彼輒頭痛。",
        "from": "T21n1331_004:0506b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身體寒熱或時致死。",
        "from": "T21n1331_004:0506b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以某帶持結願力故。",
        "from": "T21n1331_004:0506b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡龍攝毒不復害人。",
        "from": "T21n1331_004:0506b03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶持結願縷現報如是。",
        "from": "T21n1331_004:0506b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神名半那波提　字精勇健神名莎羅波提　字救世者神名沙和迦羅　字離邪行神名尼遲樓𭏷　字滅思想神名波羅般然　字威靈祇神名嘻蘭譚耆　字如江海神名金羅伊頭　字不畏兵是七神王當以威神。",
        "from": "T21n1331_004:0506b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "主逐詐稱浮遊鬼神名籍未定者。",
        "from": "T21n1331_004:0506b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天上不攝地獄不受來往世間。",
        "from": "T21n1331_004:0506b13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若在城邑聚落村里。",
        "from": "T21n1331_004:0506b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作諸變怪恐動人情。",
        "from": "T21n1331_004:0506b14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愚人惶怖為設福食謂之為神。",
        "from": "T21n1331_004:0506b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或言汝命屬我作諸疾痛保汝不死。",
        "from": "T21n1331_004:0506b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以某帶持結縷神名。",
        "from": "T21n1331_004:0506b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此諸惡鬼不復嬈近索人飲食。",
        "from": "T21n1331_004:0506b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶持結願福德力故現報如是。",
        "from": "T21n1331_004:0506b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神名脂難雷耶　字通達顯神名捐摩質樓　字堅正幢神名倪和無呵　字至大祇神名懼頭摩醯　字最上勝神名差陀離遮　字久建行神名難頭謣難　字吉安寧神名嘻遲嘻遲　字勢力強是七神王當以威神。",
        "from": "T21n1331_004:0506b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "主治飛尸客氣之鬼復連注鬼。",
        "from": "T21n1331_004:0506b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有魅鬼來入人身者使意擾亂。",
        "from": "T21n1331_004:0506b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "湯藥針炙悉不得治呪術不行者。",
        "from": "T21n1331_004:0506b28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此鬼流轉在人百節五脈之中。",
        "from": "T21n1331_004:0506b29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使人惶惑諸治不差者。",
        "from": "T21n1331_004:0506b29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以某帶持結願神名。",
        "from": "T21n1331_004:0506c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此諸注鬼不能為害。",
        "from": "T21n1331_004:0506c01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結願福德現報如是。",
        "from": "T21n1331_004:0506c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神名阿波利移字慧無窮神名較坻細耶　字斷諸結神名摩梨摩羅　字住正臣神名阿泥梨移　字積財施神名波泥梨移　字不捨願神名阿吼羅迷　字除垢穢神名阿都摩梨　字誠信篤是七神王當以威神。",
        "from": "T21n1331_004:0506c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "主入溪谷山野之中。",
        "from": "T21n1331_004:0506c10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒蛇蚖蝮諸雜毒虫。",
        "from": "T21n1331_004:0506c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "象龍熊羆虎狼禽獸。",
        "from": "T21n1331_004:0506c11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種恐畏噉人血肉者。",
        "from": "T21n1331_004:0506c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以某帶持結願神名。",
        "from": "T21n1331_004:0506c12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常隨左右為某作護。",
        "from": "T21n1331_004:0506c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除却諸惡無眾衰惱。",
        "from": "T21n1331_004:0506c13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶持結願現報如是。",
        "from": "T21n1331_004:0506c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神名阿迦至提　字正真治神名修波梨阿　字正住安神名䚷羅卑羅　字伏魔魅神名阿呵閉羅　字教令從神名卑羅摩䟞　字朗赫照神名師羅摩提　字嚴整住神名他飢梨尼　字立安明是七神王當以威神。",
        "from": "T21n1331_004:0506c22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "主治逆賊侵陵境土。",
        "from": "T21n1331_004:0506c22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劫奪人財寶偷竊為意。",
        "from": "T21n1331_004:0506c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恆生惡念不絕。",
        "from": "T21n1331_004:0506c23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以貪求故壞王國土。",
        "from": "T21n1331_004:0506c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "村營市里更相嬈害。",
        "from": "T21n1331_004:0506c24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以某帶持結願神名。",
        "from": "T21n1331_004:0506c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方怨賊劫盜等侶。",
        "from": "T21n1331_004:0506c25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然消滅不能為害。",
        "from": "T21n1331_004:0506c26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶持結願經故現報如是。",
        "from": "T21n1331_004:0506c26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神名尼陀槃尼　字殊勝彼神名尼拔散尼　字深寂滅神名摩呵曼那　字辭章句神名㝹陀梨那　字曉了度神名漚那提奴　字吉定安神名漚那提陀　字虛空住神名漚彌提屠　字法呪術是七神王當以威神。",
        "from": "T21n1331_004:0507a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護國土之中有。",
        "from": "T21n1331_004:0507a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雜毒之氣以擊人身。",
        "from": "T21n1331_004:0507a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被輒寒熱起諸苞腫或乃徹骨。",
        "from": "T21n1331_004:0507a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至潰之日濃血臭爛不可得近。",
        "from": "T21n1331_004:0507a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唾呪不行或時致死。",
        "from": "T21n1331_004:0507a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "末世之中生此毒害。",
        "from": "T21n1331_004:0507a08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以某帶持結願經故。",
        "from": "T21n1331_004:0507a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雜氣之毒不害某身。",
        "from": "T21n1331_004:0507a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辟除萬惡魔邪敢當。",
        "from": "T21n1331_004:0507a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶持結願現報如是。",
        "from": "T21n1331_004:0507a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告天帝釋若男子女人。",
        "from": "T21n1331_004:0507a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為邪鬼所得便者。",
        "from": "T21n1331_004:0507a11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當洗手嗽口清淨。",
        "from": "T21n1331_004:0507a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正心敬禮諸佛三寶。",
        "from": "T21n1331_004:0507a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒諸名香膠香婆香安息香等。",
        "from": "T21n1331_004:0507a13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香汁泥地縱廣七尺。",
        "from": "T21n1331_004:0507a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散五色華然十方燈。",
        "from": "T21n1331_004:0507a14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以五色之縷各七條。",
        "from": "T21n1331_004:0507a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長七尺而左索之。",
        "from": "T21n1331_004:0507a15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "存百神王名字亦並結之。",
        "from": "T21n1331_004:0507a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人受者當長跪叉手。",
        "from": "T21n1331_004:0507a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "專心三寶莫念東西南北方事。",
        "from": "T21n1331_004:0507a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦復莫念家室之事。",
        "from": "T21n1331_004:0507a17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聽師所言稽首頂受諸鬼神王。",
        "from": "T21n1331_004:0507a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當以已之威力為男子女人作大護助。",
        "from": "T21n1331_004:0507a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行來出入帶在身上。",
        "from": "T21n1331_004:0507a19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸鬼魔輩聞見之者。",
        "from": "T21n1331_004:0507a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馳散亡走各自隱藏不近人也。",
        "from": "T21n1331_004:0507a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告天帝釋此灌頂章句百神王。",
        "from": "T21n1331_004:0507a22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不但結縷呼其名而已。",
        "from": "T21n1331_004:0507a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人危厄眾難所惱。",
        "from": "T21n1331_004:0507a23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨其所主存呼憶念。",
        "from": "T21n1331_004:0507a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常在人左右不離須臾。",
        "from": "T21n1331_004:0507a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書持名字厭除不祥。",
        "from": "T21n1331_004:0507a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨神擁護百怪消滅。",
        "from": "T21n1331_004:0507a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魔邪敢當無不吉祥。",
        "from": "T21n1331_004:0507a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過度眾厄壽命延長。",
        "from": "T21n1331_004:0507a26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說灌頂章句百神王名字時。",
        "from": "T21n1331_004:0507a27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有八菩薩從他方來。",
        "from": "T21n1331_004:0507a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稽首佛足而問佛言。",
        "from": "T21n1331_004:0507a28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是神名餘諸神王少有及者。",
        "from": "T21n1331_004:0507a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天中天怛薩阿竭乃說是神名。",
        "from": "T21n1331_004:0507b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸菩薩樂精進者行無懈怠。",
        "from": "T21n1331_004:0507b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛般泥洹後是諸神王。",
        "from": "T21n1331_004:0507b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當久在閻浮利內不。",
        "from": "T21n1331_004:0507b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告八菩薩我般泥洹後。",
        "from": "T21n1331_004:0507b03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸神王當現在世二百歲。",
        "from": "T21n1331_004:0507b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至三百歲其後不復現。",
        "from": "T21n1331_004:0507b04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "却後濁亂世佛經欲滅時。",
        "from": "T21n1331_004:0507b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸比丘輩不復用法威儀戒律。",
        "from": "T21n1331_004:0507b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒相嫉妬毀滅正化。",
        "from": "T21n1331_004:0507b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至吾法沒國國相伐。",
        "from": "T21n1331_004:0507b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是結縷神名當復現耳。",
        "from": "T21n1331_004:0507b07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛法漸末有諸少學比丘。",
        "from": "T21n1331_004:0507b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用佛威神故讀是經典。",
        "from": "T21n1331_004:0507b08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "老大比丘不欲聽聞。",
        "from": "T21n1331_004:0507b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂諸少學諸比丘輩。",
        "from": "T21n1331_004:0507b09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝破佛法行此邪見。",
        "from": "T21n1331_004:0507b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當墮鬼神道及地獄中。",
        "from": "T21n1331_004:0507b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知佛法於此漸滅。",
        "from": "T21n1331_004:0507b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時摩訶須薩惒菩薩憍曰兜菩薩。",
        "from": "T21n1331_004:0507b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那羅達菩薩。",
        "from": "T21n1331_004:0507b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須深彌菩薩。",
        "from": "T21n1331_004:0507b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和林調菩薩。",
        "from": "T21n1331_004:0507b13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共白佛言。",
        "from": "T21n1331_004:0507b13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天中天般泥洹却後世時。",
        "from": "T21n1331_004:0507b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是經卷者我輩護持使佛道久存。",
        "from": "T21n1331_004:0507b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人未聞是經典者。",
        "from": "T21n1331_004:0507b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當授度諷誦宣傳持是經典。",
        "from": "T21n1331_004:0507b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告阿難及天帝釋四部弟子。",
        "from": "T21n1331_004:0507b17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是颰陀和於五百菩薩人中之師當持正法。",
        "from": "T21n1331_004:0507b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合會隨順教莫不從。",
        "from": "T21n1331_004:0507b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡喜心隨順心清潔心却欲心。",
        "from": "T21n1331_004:0507b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時五百人皆叉手立佛前。",
        "from": "T21n1331_004:0507b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "颰陀和菩薩白佛言。",
        "from": "T21n1331_004:0507b20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩持幾事得行是神呪。",
        "from": "T21n1331_004:0507b21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言有四事能行是神呪。",
        "from": "T21n1331_004:0507b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者不信餘道。",
        "from": "T21n1331_004:0507b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者斷除愛欲。",
        "from": "T21n1331_004:0507b22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者如法修行。",
        "from": "T21n1331_004:0507b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者無所貪生。",
        "from": "T21n1331_004:0507b23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若菩薩行是四事。",
        "from": "T21n1331_004:0507b23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃能得是灌頂章句結願大呪經。",
        "from": "T21n1331_004:0507b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告八菩薩摩訶薩。",
        "from": "T21n1331_004:0507b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若男子女人行是神呪。",
        "from": "T21n1331_004:0507b25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書持讀誦為他人說。",
        "from": "T21n1331_004:0507b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今世現得五百功德。",
        "from": "T21n1331_004:0507b26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如慈心比丘。",
        "from": "T21n1331_004:0507b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終不中毒亦不中兵。",
        "from": "T21n1331_004:0507b27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火不能燒水不能漂。",
        "from": "T21n1331_004:0507b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝王不能得其便也。",
        "from": "T21n1331_004:0507b28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是男子女人。",
        "from": "T21n1331_004:0507b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行是結願呪經有如是功德。",
        "from": "T21n1331_004:0507b29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為水火所害。",
        "from": "T21n1331_004:0507c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若龍若師子虎狼。",
        "from": "T21n1331_004:0507c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若薜荔若鳩洹鬼神喜嬈人者。",
        "from": "T21n1331_004:0507c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲殺人若奪人財寶。",
        "from": "T21n1331_004:0507c02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若壞人禪定設中害者使不得便。",
        "from": "T21n1331_004:0507c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言如我所語無有異也。",
        "from": "T21n1331_004:0507c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟除宿命不請耳。",
        "from": "T21n1331_004:0507c04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人淨持齋戒行是呪經。",
        "from": "T21n1331_004:0507c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不患目痛身體無病。",
        "from": "T21n1331_004:0507c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心意無憂終無厄難。",
        "from": "T21n1331_004:0507c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若近死事皆得解脫無復惱害。",
        "from": "T21n1331_004:0507c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若男子女人修行是神呪者。",
        "from": "T21n1331_004:0507c07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為天護敬亦復稱譽讚歎。",
        "from": "T21n1331_004:0507c08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其善龍王鬼神閱叉揵陀羅阿須淪迦樓羅真陀羅摩睺勒鬼神王人非人等。",
        "from": "T21n1331_004:0507c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各共敬愛稱揚其善。",
        "from": "T21n1331_004:0507c10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸佛菩薩聲聞聖眾之所護念。",
        "from": "T21n1331_004:0507c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若命終時是八大菩薩迎其精神。",
        "from": "T21n1331_004:0507c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往生西方自在隨意。",
        "from": "T21n1331_004:0507c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是天尊以頌讚曰。",
        "from": "T21n1331_004:0507c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我說灌頂章句義　　辟除邪惡精魅鬼",
        "from": "T21n1331_004:0507c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘他鬼神不得便　　結縷神名常擁護",
        "from": "T21n1331_004:0507c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳥獸變怪野孤鳴　　帶持誦者災禍滅",
        "from": "T21n1331_004:0507c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若乘船行江海中　　水中鮫龍魚黿鼈",
        "from": "T21n1331_004:0507c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種雜類無央數　　終不為害安隱度",
        "from": "T21n1331_004:0507c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若入異道蠱毒家　　飲食有藥自消化",
        "from": "T21n1331_004:0507c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以帶持故能除惡　　生嫉妬者使滅恨",
        "from": "T21n1331_004:0507c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怨家為伏喜相向　　債主寬意不諍財",
        "from": "T21n1331_004:0507c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "買賣萬倍利自來　　盜賊不引恐懼除",
        "from": "T21n1331_004:0507c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縣官死事莫憂慮　　帶持結願唾即除",
        "from": "T21n1331_004:0507c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有產者輒生男　　設生女者皆端嚴",
        "from": "T21n1331_004:0507c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生門有鬼化為護　　小小危厄無不度",
        "from": "T21n1331_004:0507c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魅鬼雖嬈善神佐　　離諸危難無恐怖",
        "from": "T21n1331_004:0507c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "海中弊龍吐惡氣　　雖欲侵陵無能害",
        "from": "T21n1331_004:0507c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "浮游鬼神覓人食　　見持結願皆消息",
        "from": "T21n1331_004:0507c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "注連魔魅入人腹　　誦持結願皆自出",
        "from": "T21n1331_004:0507c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "溪谷曠野雜毒虫　　以帶持故無不降",
        "from": "T21n1331_004:0508a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他方怨賊奪人財　　見有誦者復道歸",
        "from": "T21n1331_004:0508a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俗惡末世生毒腫　　帶持結願無不滅",
        "from": "T21n1331_004:0508a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有男子學誦是　　佛說神呪結願經",
        "from": "T21n1331_004:0508a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "假使欲歎其功德　　譬如恒邊減一沙",
        "from": "T21n1331_004:0508a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀劍矛戟不傷身　　盜賊怨家無能害",
        "from": "T21n1331_004:0508a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國王大臣喜悅向　　學此神呪福如是",
        "from": "T21n1331_004:0508a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蚖蛇含毒誠可畏　　見彼誦者毒疾除",
        "from": "T21n1331_004:0508a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不復瞋恚吐惡氣　　持此神呪福如是",
        "from": "T21n1331_004:0508a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怨讎嫌恨莫能害　　天龍鬼神真陀羅",
        "from": "T21n1331_004:0508a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覩其威光默然信　　學誦此典德如是",
        "from": "T21n1331_004:0508a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "山野弊狼及大蟒　　師子猛虎鹿蝦蟇",
        "from": "T21n1331_004:0508a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無傷害心悉藏毒　　眾惡消滅魔敢當",
        "from": "T21n1331_004:0508a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弊惡鬼神將人魂　　諸天人民懷害心",
        "from": "T21n1331_004:0508a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "感其威神自然伏　　學此結願無所畏",
        "from": "T21n1331_004:0508a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人不病無苦痛　　耳目聰明無闇塞",
        "from": "T21n1331_004:0508a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言辭辯慧有殊傑　　行是神典能除惡",
        "from": "T21n1331_004:0508a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人終不墮地獄　　離餓鬼道及畜生",
        "from": "T21n1331_004:0508a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世世所生識宿命　　學誦此經人所敬",
        "from": "T21n1331_004:0508a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼神揵陀共擁護　　諸天四王亦如是",
        "from": "T21n1331_004:0508a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及阿須淪摩睺勒　　行此神呪福能護",
        "from": "T21n1331_004:0508a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天悉共頌其德　　天人龍鬼甄陀羅",
        "from": "T21n1331_004:0508a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛嗟歎令如願　　諷誦說經為人故",
        "from": "T21n1331_004:0508a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人道意不退轉　　法慧之義而無盡",
        "from": "T21n1331_004:0508a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姿顏美艶無與等　　誦習此經開化人",
        "from": "T21n1331_004:0508a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國國相伐民荒亂　　飢饉荐臻懷苦窮",
        "from": "T21n1331_004:0508a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終不於中夭其命　　能誦此經化人者",
        "from": "T21n1331_004:0508a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勇猛降伏諸魔事　　心無所畏毛不竪",
        "from": "T21n1331_004:0508a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其功德行不思議　　降伏外道令入正",
        "from": "T21n1331_004:0508a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妖蠱幻化及符呪　　穢濁邪道不正行",
        "from": "T21n1331_004:0508b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持是神呪莫能中　　用愛樂法建立故",
        "from": "T21n1331_004:0508b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切悉共歌其德　　具足空慧佛弟子",
        "from": "T21n1331_004:0508b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後當來在末世　　手得結願神呪經",
        "from": "T21n1331_004:0508b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常行精進喜踊躍　　同心和悅奉此法",
        "from": "T21n1331_004:0508b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持經卷講諷誦　　令此法言無遺失",
        "from": "T21n1331_004:0508b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在所宣傳令廣知　　法言流布有利益",
        "from": "T21n1331_004:0508b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊說此偈已告阿難言。",
        "from": "T21n1331_004:0508b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "假使有人令面見佛至心供養衣被飯飴。",
        "from": "T21n1331_004:0508b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "床臥之具病瘦醫藥。",
        "from": "T21n1331_004:0508b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不如有人受持是灌頂章句結願神呪經。",
        "from": "T21n1331_004:0508b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書持讀誦。",
        "from": "T21n1331_004:0508b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書著竹帛為他說者。",
        "from": "T21n1331_004:0508b11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則為具足供養於佛。",
        "from": "T21n1331_004:0508b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時天帝釋與無央數諸天俱來。",
        "from": "T21n1331_004:0508b12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各齎。",
        "from": "T21n1331_004:0508b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天華香供養散佛。",
        "from": "T21n1331_004:0508b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而白佛言我當將護持結願經者。",
        "from": "T21n1331_004:0508b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四天王及上諸天。",
        "from": "T21n1331_004:0508b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各齎華香以供養佛。",
        "from": "T21n1331_004:0508b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各白佛言。",
        "from": "T21n1331_004:0508b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當擁護善男子善女人。",
        "from": "T21n1331_004:0508b15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受此經典書持讀誦為他人說。",
        "from": "T21n1331_004:0508b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所在遊行周匝營。",
        "from": "T21n1331_004:0508b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護令無伺求得其便也。",
        "from": "T21n1331_004:0508b17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告阿難我今為汝及四眾弟子。",
        "from": "T21n1331_004:0508b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此灌頂章句百神王名護身結願大神呪經。",
        "from": "T21n1331_004:0508b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當受持度與男子女人等輩令其諷誦。",
        "from": "T21n1331_004:0508b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宣傳吾言示於未聞。",
        "from": "T21n1331_004:0508b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說如是天龍鬼神四部弟子。",
        "from": "T21n1331_004:0508b21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞經歡喜作禮奉行。",
        "from": "T21n1331_004:0508b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灌頂經卷第四",
        "from": "T21n1331_004:0508b23_100",
        "context_with_next_sentence": false
    }
]