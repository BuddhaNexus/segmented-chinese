[
    {
        "source_sentence": "佛說寶藏神大明曼拏羅儀軌經",
        "from": "T21n1283_002:0346a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卷下",
        "from": "T21n1283_002:0346a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試鴻臚少卿",
        "from": "T21n1283_002:0346a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳教大師臣法天奉　詔譯次下諸部真言。",
        "from": "T21n1283_002:0346a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請召發遣供獻加持並皆通用。",
        "from": "T21n1283_002:0346a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難哆鉢納摩二合引野娑嚩二合引賀引此是蓮花寶藏真言。",
        "from": "T21n1283_002:0346b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難哆商佉引野娑嚩二合引賀引此是硨磲寶藏真言。",
        "from": "T21n1283_002:0346b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "囉惹曩引婆引野馱曩引地鉢哆曳娑嚩二合引賀引此是囉惹哆曩婆夜叉王真言。",
        "from": "T21n1283_002:0346b06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩尼曩引婆引野引馱曩引地鉢哆曳娑嚩二合引賀引此是囉怛曩二合曩引婆夜叉王真言。",
        "from": "T21n1283_002:0346b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酥曩引婆引野馱曩引地鉢哆曳引娑嚩二合引賀此是酥曩引婆夜叉王真言。",
        "from": "T21n1283_002:0346b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "暗婆曩引婆引野馱曩引地鉢哆曳引娑嚩二合引賀此是暗婆曩引婆夜叉王真言。",
        "from": "T21n1283_002:0346b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儞囉曩引婆引野馱曩引地鉢哆曳引娑嚩二合引賀此是羅曩婆夜叉王真言。",
        "from": "T21n1283_002:0346b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吠室囉二合摩拏也二合野馱曩引地鉢哆曳引娑嚩二合引賀此是毘沙門夜叉王真言。",
        "from": "T21n1283_002:0346b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩賀引曩引婆引野馱曩引地鉢哆曳引娑嚩二合賀此是摩賀引曩引婆夜叉王真言。",
        "from": "T21n1283_002:0346b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賀囉惹敢二合婆迦吽頗吒娑嚩二合賀此是賀囉惹敢二合婆夜叉王真言。",
        "from": "T21n1283_002:0346b25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惹敢二合婆迦野羼捺囉二合野娑嚩二合賀此是惹敢二合婆迦夜叉王真言。",
        "from": "T21n1283_002:0346b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護努護努喃曩引酥細拏也二合野娑嚩二合賀此是酥細拏夜叉王真言。",
        "from": "T21n1283_002:0346c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賀囉賀囉贊捺囉二合細引曩引野娑嚩二合引賀此是贊捺囉二合細引曩夜叉王真言。",
        "from": "T21n1283_002:0346c03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惹婆曩引野娑嚩二合賀此是護摩真言。",
        "from": "T21n1283_002:0346c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作護摩。",
        "from": "T21n1283_002:0346c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用阿波摩陵誐濕柴。",
        "from": "T21n1283_002:0346c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以酥酪蜜搵此柴頭。",
        "from": "T21n1283_002:0346c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持戒一晝夜。",
        "from": "T21n1283_002:0346c07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作護摩八千。",
        "from": "T21n1283_002:0346c07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得大聰明。",
        "from": "T21n1283_002:0346c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令誦人日記一千頌永不忘失。",
        "from": "T21n1283_002:0346c08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若以阿里迦木根作寶藏神相。",
        "from": "T21n1283_002:0346c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可長六指已來。",
        "from": "T21n1283_002:0346c09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭戴天冠種種莊嚴。",
        "from": "T21n1283_002:0346c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以香花供養。",
        "from": "T21n1283_002:0346c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然用阿波摩陵誐濕柴。",
        "from": "T21n1283_002:0346c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於酥酪蜜中搵過。",
        "from": "T21n1283_002:0346c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作護摩一洛叉。",
        "from": "T21n1283_002:0346c11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日日得銀錢一百。",
        "from": "T21n1283_002:0346c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說寶藏神心真言。",
        "from": "T21n1283_002:0346c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨引囉怛曩二合怛囉二合夜引野曩謨引摩引尼婆捺囉二合引野摩賀引藥乞叉二合細引曩引鉢哆曳引酥引嚩引酥引尾訖里二合引尼野彌里致娑嚩二合引賀此寶藏神心真言。",
        "from": "T21n1283_002:0346c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能得最上成就。",
        "from": "T21n1283_002:0346c17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若入水至膝作水護摩誦此真言能獲財利又若以儞淫二合摩木。",
        "from": "T21n1283_002:0346c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可長四指令平正用。",
        "from": "T21n1283_002:0346c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持戒畫人畫寶賢相莊嚴具足。",
        "from": "T21n1283_002:0346c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以飲食等供獻已。",
        "from": "T21n1283_002:0346c20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日三時用焰。",
        "from": "T21n1283_002:0346c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多煙少之火。",
        "from": "T21n1283_002:0346c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使紫礦作護摩八千。",
        "from": "T21n1283_002:0346c21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能降伏大財主。",
        "from": "T21n1283_002:0346c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說別夜叉王心真言文句用圓。",
        "from": "T21n1283_002:0346c23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能令得最上成就。",
        "from": "T21n1283_002:0346c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "囉怛曩二合婆捺囉二合哆帝三摩野摩努瑟摩二合囉摩尼婆捺囉二合野婆誐嚩覩引夜引唧哆娑覩二合馱摩馱摩摩引唧囉摩賀引藥乞叉二合細引曩引必隣二合哆覩嚕覩嚕摩引尼婆捺囉二合覩嚕覩嚕摩引尼婆捺囉二合俱致俱致摩引尼婆捺囉二合覩致覩致摩引尼婆捺囉二合帝致帝致摩尼婆捺囉二合娑嚩二合引賀復有壇法不揀吉星曜日。",
        "from": "T21n1283_002:0347a03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不假持戒之者。",
        "from": "T21n1283_002:0347a03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但用阿里迦木或儞淫二合摩木及別樹木作寶賢相。",
        "from": "T21n1283_002:0347a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以香花供養。",
        "from": "T21n1283_002:0347a05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就日中用白芥子菜油。",
        "from": "T21n1283_002:0347a05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗在寶賢像身。",
        "from": "T21n1283_002:0347a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦前真言百千遍。",
        "from": "T21n1283_002:0347a06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日得八錢。",
        "from": "T21n1283_002:0347a06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得已須令使盡。",
        "from": "T21n1283_002:0347a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得留一錢日日如是。",
        "from": "T21n1283_002:0347a07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶藏神復說真言儀軌。",
        "from": "T21n1283_002:0347a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令持誦者先自乞食持齋。",
        "from": "T21n1283_002:0347a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦根本真言三洛叉。",
        "from": "T21n1283_002:0347a09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後於如來前設大供獻。",
        "from": "T21n1283_002:0347a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言八千能得多財。",
        "from": "T21n1283_002:0347a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次明水護摩每日以手掬水。",
        "from": "T21n1283_002:0347a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而却傾下水中。",
        "from": "T21n1283_002:0347a11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是至百掬或千掬。",
        "from": "T21n1283_002:0347a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對賢聖前而伸供獻。",
        "from": "T21n1283_002:0347a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是至六箇月能得大財。",
        "from": "T21n1283_002:0347a13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說入大曼拏羅諸部灌頂真言。",
        "from": "T21n1283_002:0347a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惹敢二合婆曩吒謨吒迦引野娑嚩二合引賀此是謨吒迦夜叉女真言。",
        "from": "T21n1283_002:0347a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "囉訖哆二合即以二合賀嚩二合娑嚩二合引賀此是囉訖哆𡁠嚩夜叉女真言。",
        "from": "T21n1283_002:0347a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩護引那囉引野娑嚩二合引賀此是摩護那羅夜叉女真言。",
        "from": "T21n1283_002:0347a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尾摩羅尾摩羅捺里舍二合儞阿引夜引呬娑嚩二合賀尾摩羅引尾摩羅尾舍引羅曩野儞引冒馱引野娑嚩二合引賀此是尾摩羅尾舍羅夜叉女真言。",
        "from": "T21n1283_002:0347a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "贊捺囉二合引輸摩帝贊捺里二合引贊捺囉二合摩帝娑嚩二合引賀此是贊捺囉輸摩帝夜叉女真言。",
        "from": "T21n1283_002:0347a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿嚩賀儞酥路左曩唵引贊捺囉二合鉢納彌二合引娑嚩二合引賀鉢納摩二合娑囉引酥訥嚕二合摩努引囉彌引娑嚩二合引賀酥部嚕二合嚩引護鉢囉二合怛也二合野娑嚩二合引賀嚩護嚩呬儞二合末隷伽吒具引致娑嚩二合引賀此是摩賀彌伽夜叉女等真言。",
        "from": "T21n1283_002:0347b04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酥枳引舍摩引隷枳引舍波里尼娑嚩二合引賀此是酥枳舍夜叉女真言。",
        "from": "T21n1283_002:0347b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚩曩摩引隷摩引隷儞娑嚩二合引賀此是摩曩摩羅夜叉女真言。",
        "from": "T21n1283_002:0347b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烏里底二合瑟吒二合布室左二合隷引娑嚩二合引賀此是布左隷夜叉女真言。",
        "from": "T21n1283_002:0347b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "訥嚕二合摩摩引隷迦引迦引隷賀隷訥嚕二合彌引枳隷枳隷娑嚩二合引賀迦摩摩引隷儞尾羅引悉儞娑嚩二合引賀迦摩摩隷薩里嚩二合引禰引嚩薩里嚩二合誐囉二合摩努引惹尾引婆誐嚩帝娑嚩二合賀摩努引惹嚩引迦引摩嚕閉引婆引尾儞娑嚩二合賀此是迦摩嚕閉等夜叉女真言。",
        "from": "T21n1283_002:0347b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酥嚕二合哆嚩引吉也二合緊迦里娑嚩二合賀此是酥嚕哆嚩引吉也二合夜叉女真言。",
        "from": "T21n1283_002:0347b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酥捺里舍二合儞引迦引彌儞娑嚩二合賀此是酥那里舍曩夜叉女真言。",
        "from": "T21n1283_002:0347b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩護引誐里二合引婆焰迦里娑嚩二合賀此是摩護仡囉夜叉女真言。",
        "from": "T21n1283_002:0347b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印捺囉二合引野禰引嚩引地鉢哆野娑嚩二合賀此是帝釋心真言。",
        "from": "T21n1283_002:0347b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿誐曩二合引曳引賀尾也二合地鉢多曳引娑嚩二合賀此是火天真言。",
        "from": "T21n1283_002:0347b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜摩引野必里二合引哆引地鉢哆曳引娑嚩二合引賀此是夜摩天心真言。",
        "from": "T21n1283_002:0347c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃引里怛也二合曳引洛乞叉二合娑引地鉢哆曳引娑嚩二合賀此是乃乙里帝心真言。",
        "from": "T21n1283_002:0347c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚩嚕拏也二合野惹羅引地鉢哆曳引娑嚩二合引賀此是水天心真言。",
        "from": "T21n1283_002:0347c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚩引野尾引按哆里乞叉二合引地鉢哆曳引娑嚩二合引賀此是風天心真言。",
        "from": "T21n1283_002:0347c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱尾引囉引野馱曩鉢哆曳引娑嚩二合引賀此是俱尾羅心真言。",
        "from": "T21n1283_002:0347c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伊舍曩引野馱曩引地鉢哆曳引娑嚩二合引賀此是伊舍曩天心真言。",
        "from": "T21n1283_002:0347c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惹里馱二合沒囉二合憾摩二合尼所二合娑嚩二合引賀此是大梵天心真言。",
        "from": "T21n1283_002:0347c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿冒引尾瑟拏二合吠引娑嚩二合引賀此是那羅延天心真言。",
        "from": "T21n1283_002:0347c21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿儞嚩里哆二合曩引野娑嚩二合引賀此是金剛心真言。",
        "from": "T21n1283_002:0347c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賀囉二合哆引娑曩引野娑嚩二合引賀此是槊心真言。",
        "from": "T21n1283_002:0347c25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩賀引寫引摩引野娑嚩二合引賀此是杖心真言。",
        "from": "T21n1283_002:0347c27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佉陵誐二合尾摩羅馱引里尼娑嚩二合引賀此是劍心真言。",
        "from": "T21n1283_002:0347c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚩嚕拏嚩摩引野娑嚩二合引賀此是羂索心真言。",
        "from": "T21n1283_002:0348a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鉢囉二合普敢二合惹曩野娑嚩二合引賀此是幢心真言。",
        "from": "T21n1283_002:0348a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩賀引燥引摩也二合囉娑摩曳娑嚩二合引賀此是輪心真言。",
        "from": "T21n1283_002:0348a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左引隷引閉孕二合誐隷引娑嚩二合引賀此是三股叉心真言。",
        "from": "T21n1283_002:0348a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并結界同用。",
        "from": "T21n1283_002:0348a08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娑囉捺引野娑嚩二合引賀此是弓箭真言。",
        "from": "T21n1283_002:0348a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引惹里馱二合惹羅嚩口囉二合半惹囉引野娑嚩二合賀此是金剛網真言。",
        "from": "T21n1283_002:0348a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是各各本尊真言。",
        "from": "T21n1283_002:0348a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一持誦加持賢瓶。",
        "from": "T21n1283_002:0348a14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有入曼拏羅弟子。",
        "from": "T21n1283_002:0348a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用此賢瓶而與灌頂。",
        "from": "T21n1283_002:0348a15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令得富貴吉祥。",
        "from": "T21n1283_002:0348a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若入大曼拏羅。",
        "from": "T21n1283_002:0348a16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "志心依法虔誠灌頂。",
        "from": "T21n1283_002:0348a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復能得見寶藏神。",
        "from": "T21n1283_002:0348a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三時教勅通達大印。",
        "from": "T21n1283_002:0348a17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復作護摩以二十種物作廣大供獻。",
        "from": "T21n1283_002:0348a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦聖天及大夜叉王本尊真言。",
        "from": "T21n1283_002:0348a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日三時隨力供養。",
        "from": "T21n1283_002:0348a19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得三昧真言最上成就為自擁護。",
        "from": "T21n1283_002:0348a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言我今復說所作精熟最上印法及真言句。",
        "from": "T21n1283_002:0348a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時大夜叉王等。",
        "from": "T21n1283_002:0348a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心生渴仰請佛宣說。",
        "from": "T21n1283_002:0348a22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊遍觀大會。",
        "from": "T21n1283_002:0348a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作師子吼告言。",
        "from": "T21n1283_002:0348a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等諦聽諦聽。",
        "from": "T21n1283_002:0348a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此寶藏神真言印法具大慈悲。",
        "from": "T21n1283_002:0348a24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難調伏者而能調伏。",
        "from": "T21n1283_002:0348a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與一切眾生成就一切勝利之事。",
        "from": "T21n1283_002:0348a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜叉王等右膝著地。",
        "from": "T21n1283_002:0348a26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌向佛默然而聽。",
        "from": "T21n1283_002:0348a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊。",
        "from": "T21n1283_002:0348a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如大師子自在無畏說真言印。",
        "from": "T21n1283_002:0348a28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手平掌相並。",
        "from": "T21n1283_002:0348a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指相交。",
        "from": "T21n1283_002:0348a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二無名指如針。",
        "from": "T21n1283_002:0348a29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈第三節。",
        "from": "T21n1283_002:0348b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指附無名指。",
        "from": "T21n1283_002:0348b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指順依頭指。",
        "from": "T21n1283_002:0348b01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二小指如針。",
        "from": "T21n1283_002:0348b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是寶藏神根本印。",
        "from": "T21n1283_002:0348b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此印結中間。",
        "from": "T21n1283_002:0348b02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得寶藏神現身。",
        "from": "T21n1283_002:0348b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀察誦人得施願成就。",
        "from": "T21n1283_002:0348b03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言。",
        "from": "T21n1283_002:0348b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨引囉怛曩二合怛囉二合夜野引曩謨摩引尼婆捺囉二合引野摩賀引藥乞叉二合細引曩引鉢哆曳引唵引惹敢二合婆羅惹隣捺囉二合引野娑嚩二合引賀此真言及印。",
        "from": "T21n1283_002:0348b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於自身上五處用之。",
        "from": "T21n1283_002:0348b09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作此法時彼持誦人。",
        "from": "T21n1283_002:0348b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如自為寶藏神。",
        "from": "T21n1283_002:0348b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次作左隣捺囉夜叉王印。",
        "from": "T21n1283_002:0348b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手合拳小指如針。",
        "from": "T21n1283_002:0348b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言。",
        "from": "T21n1283_002:0348b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引左隣引捺囉二合引野娑嚩二合引賀此真言印。",
        "from": "T21n1283_002:0348b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令夜叉王而作擁護。",
        "from": "T21n1283_002:0348b14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次作目契夜叉王印。",
        "from": "T21n1283_002:0348b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以手合拳小指入掌。",
        "from": "T21n1283_002:0348b15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無名指如針誦此真言。",
        "from": "T21n1283_002:0348b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引目契孕二合捺囉二合野娑嚩二合引賀此真言印。",
        "from": "T21n1283_002:0348b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能擁護誦人。",
        "from": "T21n1283_002:0348b18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次作一切夜叉女印。",
        "from": "T21n1283_002:0348b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以小指入掌。",
        "from": "T21n1283_002:0348b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無名指屈第三節。",
        "from": "T21n1283_002:0348b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言。",
        "from": "T21n1283_002:0348b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引惡儞此真言印亦能擁護。",
        "from": "T21n1283_002:0348b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "召請亦得用。",
        "from": "T21n1283_002:0348b22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次作曼拏羅牆印。",
        "from": "T21n1283_002:0348b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手合拳。",
        "from": "T21n1283_002:0348b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭指各各如針。",
        "from": "T21n1283_002:0348b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言。",
        "from": "T21n1283_002:0348b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引左隣捺囉二合滿馱禰苦𠷑吒次作曼拏羅上面網印。",
        "from": "T21n1283_002:0348b26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前印相却令頭指相合如針。",
        "from": "T21n1283_002:0348b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言。",
        "from": "T21n1283_002:0348b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引目契孕二合捺囉二合引野𠷑吒次作曼拏羅橛印。",
        "from": "T21n1283_002:0348b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手作拳。",
        "from": "T21n1283_002:0348b29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "垂手向地。",
        "from": "T21n1283_002:0348b29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言。",
        "from": "T21n1283_002:0348c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引賀里二合入次召請七眷屬印。",
        "from": "T21n1283_002:0348c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前根本印屈大指。",
        "from": "T21n1283_002:0348c03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言。",
        "from": "T21n1283_002:0348c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曀呬曀呬祖嚕祖嚕惹羅娑野曩必里二合夜引野娑嚩二合引賀如是儀則。",
        "from": "T21n1283_002:0348c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令七眷屬皆來集會。",
        "from": "T21n1283_002:0348c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次作獻閼伽水印。",
        "from": "T21n1283_002:0348c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手作掬。",
        "from": "T21n1283_002:0348c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指及頭指附著中指。",
        "from": "T21n1283_002:0348c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言。",
        "from": "T21n1283_002:0348c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引藥羼曩那引曳捺囉二合馱曩必里二合野鉢舍也二合鉢舍也二合引娑嚩二合引賀次作獻座印。",
        "from": "T21n1283_002:0348c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌舉手三雙指屈第三節。",
        "from": "T21n1283_002:0348c12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中指磔開大指各各相離。",
        "from": "T21n1283_002:0348c13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是寶藏神師子座印。",
        "from": "T21n1283_002:0348c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此印結中間。",
        "from": "T21n1283_002:0348c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜叉王并眷屬同坐師子座。",
        "from": "T21n1283_002:0348c14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切願誦此真言。",
        "from": "T21n1283_002:0348c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引惹敢二合婆羅惹隣捺囉二合伊那摩引娑曩摩里伽二合娑嚩二合引賀次結洗浴印。",
        "from": "T21n1283_002:0348c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請召寶藏神并眷屬洗浴。",
        "from": "T21n1283_002:0348c18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手作掬。",
        "from": "T21n1283_002:0348c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小指相交。",
        "from": "T21n1283_002:0348c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二無名指安入掌中。",
        "from": "T21n1283_002:0348c19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指如針。",
        "from": "T21n1283_002:0348c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭指附中指第三節。",
        "from": "T21n1283_002:0348c20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指亦附頭指。",
        "from": "T21n1283_002:0348c20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言。",
        "from": "T21n1283_002:0348c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引娑曩二合引曩必里二合野伽吒伽吒供婆惹羅娑嚩二合引賀次結獻衣印。",
        "from": "T21n1283_002:0348c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手相並如掌衣物。",
        "from": "T21n1283_002:0348c24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言。",
        "from": "T21n1283_002:0348c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃引里哆引野囉引叉娑引地鉢哆曳引娑嚩二合引賀次輪誦諸部真言發遣賢聖。",
        "from": "T21n1283_002:0348c28_11",
        "context_with_next_sentence": true
