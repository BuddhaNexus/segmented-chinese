[
    {
        "source_sentence": "佛說寶生陀羅尼經",
        "from": "T21n1412_001:0926c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試鴻臚少卿",
        "from": "T21n1412_001:0926c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳法大師臣施護奉　詔譯",
        "from": "T21n1412_001:0926c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨引囉怛曩二合囉濕彌二合贊捺囉二合鉢囉二合底曼尼哆尾儞焰二合帝引惹具世濕嚩二合囉引羅引惹野怛他引誐哆引野引囉賀二合帝引三藐三沒馱引野怛儞也二合他囉怛儞二合引囉怛儞二合引囉怛曩二合吉囉尼囉怛曩二合鉢囉二合底曼膩帝囉怛曩二合三婆尼引囉怛曩二合鉢囉二合鼻引囉怛努二合引訥誐二合帝引娑嚩二合引賀引若有眾生。",
        "from": "T21n1412_001:0926c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此如來陀羅尼名號受持供養。",
        "from": "T21n1412_001:0926c15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼人生生得轉輪位。",
        "from": "T21n1412_001:0926c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就梵行具大神通。",
        "from": "T21n1412_001:0926c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲十種陀羅尼。",
        "from": "T21n1412_001:0926c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復值恒河沙等諸佛如來而無虛妄。",
        "from": "T21n1412_001:0926c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經俱胝劫不入輪迴路。",
        "from": "T21n1412_001:0926c18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不斷菩提種不失菩提心。",
        "from": "T21n1412_001:0926c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永滅一切罪得報身如來。",
        "from": "T21n1412_001:0926c19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人持誦滿一七日。",
        "from": "T21n1412_001:0926c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人當得天眼清淨。",
        "from": "T21n1412_001:0926c20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人耳聞恒復憶念。",
        "from": "T21n1412_001:0926c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人決定當得菩提。",
        "from": "T21n1412_001:0926c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於過去世所作善根亦得現前。",
        "from": "T21n1412_001:0926c22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若傳一人所有無間罪業。",
        "from": "T21n1412_001:0926c22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉得除滅永斷輪迴。",
        "from": "T21n1412_001:0926c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人不被水火盜賊之所侵害。",
        "from": "T21n1412_001:0926c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而復諸根不缺眾病不生鬼魅不著。",
        "from": "T21n1412_001:0926c24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾所愛敬於當來世受持如來微妙之法供養諸佛。",
        "from": "T21n1412_001:0926c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人聞已心喜禮拜讚歎。",
        "from": "T21n1412_001:0926c26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人功德無量無邊。",
        "from": "T21n1412_001:0926c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於生生中口出妙香廣一由旬。",
        "from": "T21n1412_001:0926c27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身毛孔中恒有光明常自照曜。",
        "from": "T21n1412_001:0926c28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常作如來勝利之事如阿難陀。",
        "from": "T21n1412_001:0926c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具如是不可思議功德。",
        "from": "T21n1412_001:0926c29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說寶生陀羅尼經",
        "from": "T21n1412_001:0927a01_100",
        "context_with_next_sentence": false
    }
]