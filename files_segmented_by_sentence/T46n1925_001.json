[
    {
        "source_sentence": "法界次第初門總序法界次第初門總序",
        "from": "T46n1925_001:0664b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天台山修禪寺。沙門釋智顗。輒依經附論。撰",
        "from": "T46n1925_001:0664b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法界次第初門三百科。裁為七卷。流傳新學。",
        "from": "T46n1925_001:0664b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略為三意。",
        "from": "T46n1925_001:0664b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一為讀經尋論隨見法門。",
        "from": "T46n1925_001:0664b07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脫有迷於名數者。",
        "from": "T46n1925_001:0664b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二為未解聖教所制法門淺深之次第。",
        "from": "T46n1925_001:0664b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三為學三觀之者。",
        "from": "T46n1925_001:0664b09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當以此諸法名相義理。",
        "from": "T46n1925_001:0664b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一歷心而轉作。",
        "from": "T46n1925_001:0664b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則觀解無礙。",
        "from": "T46n1925_001:0664b10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觸境不迷。",
        "from": "T46n1925_001:0664b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於一念心中。",
        "from": "T46n1925_001:0664b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通達一切佛法者。",
        "from": "T46n1925_001:0664b11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則三觀自然了了分明也。",
        "from": "T46n1925_001:0664b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故出此三百科。",
        "from": "T46n1925_001:0664b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名教仍當。",
        "from": "T46n1925_001:0664b12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名下略辨體相。",
        "from": "T46n1925_001:0664b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始得三卷。",
        "from": "T46n1925_001:0664b13_11",
        "context_with_next_sentence": true
    },
