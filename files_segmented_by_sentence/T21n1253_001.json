[
    {
        "source_sentence": "大吉祥天女十二契一百八名無",
        "from": "T21n1253_001:0253b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "垢大乘經",
        "from": "T21n1253_001:0253b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T21n1253_001:0253b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空諡大鑒正號大廣智大興善寺三藏沙門不空奉　詔譯如是我聞。",
        "from": "T21n1253_001:0253b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時薄伽梵。",
        "from": "T21n1253_001:0253b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住安樂世界。",
        "from": "T21n1253_001:0253b11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大菩薩眾。",
        "from": "T21n1253_001:0253b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂觀自在菩薩。",
        "from": "T21n1253_001:0253b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得大勢菩薩。",
        "from": "T21n1253_001:0253b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除一切蓋障菩薩。",
        "from": "T21n1253_001:0253b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地藏菩薩。",
        "from": "T21n1253_001:0253b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普光菩薩。",
        "from": "T21n1253_001:0253b13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空藏菩薩。",
        "from": "T21n1253_001:0253b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛手菩薩除一切怖畏菩薩。",
        "from": "T21n1253_001:0253b14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持一切清淨吉祥菩薩。",
        "from": "T21n1253_001:0253b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持一切福相菩薩。",
        "from": "T21n1253_001:0253b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持日月三世菩薩。",
        "from": "T21n1253_001:0253b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊師利菩薩。",
        "from": "T21n1253_001:0253b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等菩薩摩訶薩而為上首。",
        "from": "T21n1253_001:0253b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時觀自在菩薩摩訶薩。",
        "from": "T21n1253_001:0253b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往詣世尊所頭面禮足退坐一面。",
        "from": "T21n1253_001:0253b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時大吉祥天女亦往詣佛所頭面禮足。",
        "from": "T21n1253_001:0253b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圍繞。",
        "from": "T21n1253_001:0253b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量百千匝及禮一切安樂世界所住菩薩退坐一面。",
        "from": "T21n1253_001:0253b20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊見吉祥天女。",
        "from": "T21n1253_001:0253b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有無量百千福莊嚴。",
        "from": "T21n1253_001:0253b21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱胝如來圍繞。",
        "from": "T21n1253_001:0253b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切釋梵護世讚揚稱歎。",
        "from": "T21n1253_001:0253b22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大梵音告觀自在菩薩摩訶薩言。",
        "from": "T21n1253_001:0253b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀自在菩薩若有國王王子比丘比丘尼優婆塞優婆夷婆羅門剎利毘舍首陀。",
        "from": "T21n1253_001:0253b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若受持大吉祥天女十二契一百八名無垢讚歎。",
        "from": "T21n1253_001:0253b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其王剎利國界所有眾生。",
        "from": "T21n1253_001:0253b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切怖畏逼惱並皆消除。",
        "from": "T21n1253_001:0253b27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切怨賊人非人怖亦不為害。",
        "from": "T21n1253_001:0253b28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切財穀皆悉豐饒。",
        "from": "T21n1253_001:0253b28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉祥天女於彼王剎利宅中常所居止。",
        "from": "T21n1253_001:0253b29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時彼菩薩摩訶薩說如是言。",
        "from": "T21n1253_001:0253c01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉世尊妙說此語。",
        "from": "T21n1253_001:0253c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有持吉祥天女名號。",
        "from": "T21n1253_001:0253c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼獲如是福利。",
        "from": "T21n1253_001:0253c02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時無畏觀自在菩薩摩訶薩。",
        "from": "T21n1253_001:0253c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即白佛言世尊。",
        "from": "T21n1253_001:0253c03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉祥天女曾於何處種植善根。",
        "from": "T21n1253_001:0253c04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言彼於恒河沙如來應供正遍知處。",
        "from": "T21n1253_001:0253c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種植善根。",
        "from": "T21n1253_001:0253c05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無畏觀自在菩薩我念過去世。",
        "from": "T21n1253_001:0253c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於寶生世界。",
        "from": "T21n1253_001:0253c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶花功德海吠琉璃金山金光明吉祥如來應供正遍知。",
        "from": "T21n1253_001:0253c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出興於世。",
        "from": "T21n1253_001:0253c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大吉祥天女於彼種植善根。",
        "from": "T21n1253_001:0253c08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及餘多如來所。",
        "from": "T21n1253_001:0253c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由稱如是如來名號。",
        "from": "T21n1253_001:0253c09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大吉祥天女作成就善根。",
        "from": "T21n1253_001:0253c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此諸如來常隨逐大吉祥天女。",
        "from": "T21n1253_001:0253c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能脫一切罪。",
        "from": "T21n1253_001:0253c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除滅一切煩惱。",
        "from": "T21n1253_001:0253c12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令一切身作無垢。",
        "from": "T21n1253_001:0253c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "召集增益一切財穀。",
        "from": "T21n1253_001:0253c13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除貧窮。",
        "from": "T21n1253_001:0253c13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能攝召一切天龍藥叉羅剎乾闥婆阿脩羅迦樓羅緊那羅摩睺羅伽。",
        "from": "T21n1253_001:0253c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能息一切逼惱諍訟鬪戰。",
        "from": "T21n1253_001:0253c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能成辦六波羅蜜所謂。",
        "from": "T21n1253_001:0253c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無吉祥密如來南無恒河一切津口吉慶吉祥如來南無栴檀花威星光吉祥如來南無普遍照曜勝鬪戰吉祥如來南無功德海照曜曼茶羅吉祥如來南無法神通幢進吉祥如來南無曜寂靜香照曜吉祥如來南無眾生意樂寂靜身吉祥如來南無願海吉祥如來南無妙遍稱讚名號吉祥如來南無不退輪寶處吉祥如來南無日輪照曜踴起吉祥如來南無無數精進妙住吉祥如來南無無量善住吉祥如來南無音聲支分吉祥如來南無般若燈無數光幢吉祥如來南無那羅延禁戒甲冑吉祥如來南無梵吉祥如來南無摩醯首羅吉祥如來南無日月吉祥如來南無甚深法光王吉祥如來南無虛空燈現喜吉祥如來南無日光幢吉祥如來南無香燈吉祥如來南無海藏生吉祥如來南無變化雲妙聲吉祥如來南無一切照曜嚴吉祥如來南無樹王增長吉祥如來南無寶焰山吉祥如來南無智焰海吉祥如來南無大願進吉祥如來南無大雲吉祥如來南無念幢王吉祥如來南無帝幢幡王吉祥如來南無鉤召一切財穀吉祥如來南無鉤召寂靜吉祥如來南無鉤召吉慶吉祥如來如是如來名號。",
        "from": "T21n1253_001:0254a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有恭敬受持讀誦者。",
        "from": "T21n1253_001:0254a25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼善男子善女人。",
        "from": "T21n1253_001:0254a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得發生甚多福聚。",
        "from": "T21n1253_001:0254a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來授記大吉祥天女。",
        "from": "T21n1253_001:0254a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當於吉祥寶莊嚴世界成等正覺。",
        "from": "T21n1253_001:0254a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號吉祥摩尼寶生如來應供正遍知。",
        "from": "T21n1253_001:0254a28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其世界種種天寶以為莊嚴。",
        "from": "T21n1253_001:0254a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彼世界。",
        "from": "T21n1253_001:0254a29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯此如來作光明。",
        "from": "T21n1253_001:0254b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼菩薩眾於彼佛世界中。",
        "from": "T21n1253_001:0254b01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然光明壽命無量。",
        "from": "T21n1253_001:0254b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從空演出佛法僧音聲。",
        "from": "T21n1253_001:0254b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有菩薩於彼佛世界生者。",
        "from": "T21n1253_001:0254b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切皆蓮華臺化生。",
        "from": "T21n1253_001:0254b03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何十二契一百八名無垢讚歎。",
        "from": "T21n1253_001:0254b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無畏觀自在汝今諦聽。",
        "from": "T21n1253_001:0254b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂。",
        "from": "T21n1253_001:0254b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來所灌頂一　一切如來母二　一切天母三　一切如來吉祥四　一切菩薩吉祥五　一切賢聖聲聞緣覺吉祥六　梵毘紐摩醯首羅吉祥七　一切天上首吉祥八　一切處到吉祥九　一切天龍藥叉羅剎乾闥婆阿脩羅迦樓羅緊那羅摩睺羅伽吉祥十　一切持金剛手持金剛吉祥十一　四王護世吉祥十二　八曜二十八宿吉祥十三　唵娑尾怛哩十四　馱怛哩摩多去十五　四明吉祥十六　吉祥鬼母十七　勝十八　最勝十九　恒河二十　一切津二十一　一切吉慶二十二　無垢吉祥二十三　一切除罪二十四　無逸二十五　月吉祥二十六　日吉祥二十七　一切曜吉祥二十八　乘師子二十九　百千俱胝頻婆羅蓮華莊嚴三十　蓮華三十一　大蓮華三十二　蓮華座三十三　蓮華藏三十四　持蓮華三十五　具蓮華三十六　無量寶光明三十七　施財三十八　白三十九　大白蓮華四十　白臂四十一　持一切吉慶四十二　莊嚴一切福身四十三　調柔者四十四　百千臂四十五　百千眼四十六　百千頭四十七　持種種間錯摩尼冠四十八　妙色四十九　種種色五十　名稱五十一　極名稱五十二　寂靜五十三　貳母多五十四　清淨髮五十五　月光五十六　日光五十七　作端嚴五十八　一切有情對面吉祥五十九　聖者六十　依花六十一　花自在六十二　一切須彌山王吉祥六十三　一切江河吉祥六十四　一切海水吉祥六十五　一切津口吉祥六十六　一切藥草樹財穀吉祥六十七　施金六十八　施飲食六十九　色清淨身七十　色者七十一　一切如來自在者七十二　一切天眾對面吉祥七十三　焰摩水天俱尾羅嚩平娑嚩上首吉祥七十四　與者七十五　食者七十六　威光七十七　具威光七十八　豐饒七十九　榮盛八十　增長八十一　高遷八十二　法吉祥八十三　依春八十四　俱牟陀藏八十五　慈悲者八十六　依丈夫身八十七　一切清淨吉慶手八十八　除一切不吉祥者八十九　鉤召一切福吉祥九十　一切地王吉祥九十一　一切持明吉祥九十二　一切鬼藥叉羅剎餓鬼毘舍遮鳩槃茶摩睺羅伽吉祥九十三　一切天宮諸天吉祥九十四　一切念誦護摩吉祥九十五　曜極喜九十六　福德遊戲九十七　一切仙清淨吉祥九十八　一切吉祥九十九　一切宮殿尊勝吉祥一百　一切緊那羅吉祥一百一　一切日勝吉祥一百二　無罪處流者一百三　音樂一百四　適悅者一百五　俱尾羅一百六　愛者一百七　法王吉祥一百八如上一百八名真言曰。",
        "from": "T21n1253_001:0254c18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵微路迦上耶多羅耶一慕者耶薩嚩耨契毘藥二合二薩嚩補尼耶二合三三婆去囉那摩上目企平俱嚕四娑嚩二合訶引唵凝上哦薩嚩底丁以反㗚他二合目企平娑嚩訶唵娑尾怛哩娑嚩訶薩嚩莽哦攞馱哩尼娑嚩訶遮咄吠二合那薩嚩諾朅沙二合怛囉二合󰋲囉二合訶哦拏地母㗚低二合三帶曳二合娑婆訶沒囉二合唅摩二合耶娑嚩訶尾瑟弩二合微開口呼娑嚩訶嚕㮈囉二合耶娑嚩訶尾濕嚩目佉耶娑嚩訶唵仡里二合儞仡里二合儞一薩嚩迦哩耶二僧娑達儞三悉儞悉儞四儞儞儞儞五阿洛乞灑弭四合咩那舍耶六阿嚩訶泥微七室哩二合微室囉末拏耶八娑嚩訶蘇靺㗚拏二合馱那馱儞耶二合羯哩瑟尼娑嚩訶薩嚩布尼耶二合羯哩瑟尼娑嚩訶室哩泥去嚩多羯哩瑟尼娑嚩訶薩嚩播波奢末尼娑嚩訶薩嚩引洛乞瑟彌四合鉢囉二合奢末尼娑嚩訶薩嚩怛儞󰋲多引毘色羯他曳娑嚩訶薩嚩泥靺多引鉢囉二合目佉室哩曳娑嚩訶阿欲重呼摩上囉靺哩拏二合迦羅曳娑嚩訶薩嚩波尾怛囉莽薩囉訶薩多曳娑嚩訶僧息之反平質嚩上呬儞曳二合娑嚩訶波上娜三步多娑嚩訶薩嚩吉哩爹一迦區開口重呼㗚娜尾娜尸尼上曳二合娑嚩訶無畏觀自在菩薩。",
        "from": "T21n1253_001:0255a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大吉祥真言及以一百八名號。",
        "from": "T21n1253_001:0255a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除一切煩惱。",
        "from": "T21n1253_001:0255a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能摧一切罪。",
        "from": "T21n1253_001:0255a17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能鉤召一切福。",
        "from": "T21n1253_001:0255a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除一切不祥。",
        "from": "T21n1253_001:0255a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能鉤召一切福德。",
        "from": "T21n1253_001:0255a18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人受持讀誦及諸如來名號者。",
        "from": "T21n1253_001:0255a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼當早起於一切佛。",
        "from": "T21n1253_001:0255a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒香及花供養。",
        "from": "T21n1253_001:0255a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為吉祥天女。",
        "from": "T21n1253_001:0255a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應燒檀香應讀此經。",
        "from": "T21n1253_001:0255a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人不久獲得一切吉祥一切安樂喜悅。",
        "from": "T21n1253_001:0255a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切天擁護一切事業悉得成就。",
        "from": "T21n1253_001:0255a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說是經已。",
        "from": "T21n1253_001:0255a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀自在菩薩摩訶薩及大吉祥天女。",
        "from": "T21n1253_001:0255a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切大眾聞佛所說。",
        "from": "T21n1253_001:0255a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆大歡喜信受奉行。",
        "from": "T21n1253_001:0255a25_2",
        "context_with_next_sentence": true
    },
