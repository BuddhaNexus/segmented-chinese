[
    {
        "source_sentence": "觀自在如意輪菩薩瑜伽法要",
        "from": "T20n1087_001:0211b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一卷",
        "from": "T20n1087_001:0211b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大唐南天竺國三藏金剛智譯",
        "from": "T20n1087_001:0211b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今順瑜伽　　金剛頂經說",
        "from": "T20n1087_001:0211b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩尼蓮花部　　如意念誦法",
        "from": "T20n1087_001:0211b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修此三昧故　　能如觀自在",
        "from": "T20n1087_001:0211b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先擇其弟子　　族姓敬法者",
        "from": "T20n1087_001:0211b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多人所敬愛　　智慧而勇進",
        "from": "T20n1087_001:0211b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "決定毘離耶　　覺慧常不捨",
        "from": "T20n1087_001:0211b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡孝於父母　　淨信於三寶",
        "from": "T20n1087_001:0211c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樂修菩薩行　　於四無量心",
        "from": "T20n1087_001:0211c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剎那無有間　　常樂大乘法",
        "from": "T20n1087_001:0211c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住於菩薩戒　　恭敬阿闍梨",
        "from": "T20n1087_001:0211c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸聖者　　成就堅固力",
        "from": "T20n1087_001:0211c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "丈夫之勇猛　　善通相應門",
        "from": "T20n1087_001:0211c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常樂寂靜行　　智慧無所畏",
        "from": "T20n1087_001:0211c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以戒常嚴身　　精修祕密乘",
        "from": "T20n1087_001:0211c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬依理趣道　　一心無所悋",
        "from": "T20n1087_001:0211c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常樂聞妙法　　曾入三昧耶",
        "from": "T20n1087_001:0211c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從師獲灌頂　　既蒙印可已",
        "from": "T20n1087_001:0211c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不久當成就　　弟子具此相",
        "from": "T20n1087_001:0211c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方可為傳授　　此即如意寶",
        "from": "T20n1087_001:0211c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能成諸事業　　如經說處所",
        "from": "T20n1087_001:0211c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "山間及流水　　清淨阿蘭若",
        "from": "T20n1087_001:0211c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨樂之㵎谷　　離諸危怖難",
        "from": "T20n1087_001:0211c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨力嚴供具　　行人面於西",
        "from": "T20n1087_001:0211c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漫提自在王　　次禮餘方佛",
        "from": "T20n1087_001:0211c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以五輪著地　　如教之敬禮",
        "from": "T20n1087_001:0211c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雙膝長跪已　　合掌虛心住",
        "from": "T20n1087_001:0211c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誠心盡陳說　　三業一切罪",
        "from": "T20n1087_001:0211c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我從過去世　　流轉於生死",
        "from": "T20n1087_001:0211c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今對大聖尊　　盡心而懺悔",
        "from": "T20n1087_001:0211c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如先佛所懺　　我今亦如是",
        "from": "T20n1087_001:0211c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願垂加持力　　眾生悉清淨",
        "from": "T20n1087_001:0211c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此大願故　　自他獲無垢",
        "from": "T20n1087_001:0211c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1087_001:0211c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　莎嚩皤嚩輸馱　薩婆達麼　莎嚩皤嚩輸度㟏行者次應隨喜一切諸佛菩薩所集福智。",
        "from": "T20n1087_001:0212a01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過現三世佛　　菩薩及眾聖",
        "from": "T20n1087_001:0212a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所集諸善根　　合掌盡隨喜",
        "from": "T20n1087_001:0212a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如我身所集　　歡喜無有異",
        "from": "T20n1087_001:0212a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應右膝著地芙蓉合掌置於頂上想禮一切如來及菩薩足。",
        "from": "T20n1087_001:0212a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1087_001:0212a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　鉢頭入麼二合微微吉反禮諸佛已全加半加或輪王加隨意而坐。",
        "from": "T20n1087_001:0212a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作此坐印已　　觀遍虛空佛",
        "from": "T20n1087_001:0212a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已身各於前　　住彼眾聖會",
        "from": "T20n1087_001:0212a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "止觀從膝上　　旋舞當心合",
        "from": "T20n1087_001:0212a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如蓮之未敷　　想禮於諸佛",
        "from": "T20n1087_001:0212a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結三昧印　　當心堅固縛",
        "from": "T20n1087_001:0212a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "檀慧禪智竪　　金剛蓮花印",
        "from": "T20n1087_001:0212a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通持蓮花者　　警覺眾聖已",
        "from": "T20n1087_001:0212a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由結此印故　　佛及善逝子",
        "from": "T20n1087_001:0212a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸大名稱者　　妙觀察攝受",
        "from": "T20n1087_001:0212a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憶昔本誓願　　對於遍照尊",
        "from": "T20n1087_001:0212a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不違教令故　　加持使圓滿",
        "from": "T20n1087_001:0212a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結一切諸佛如來安樂悅意歡喜三昧耶印。",
        "from": "T20n1087_001:0212a23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十度堅固縛　　忍願中交合",
        "from": "T20n1087_001:0212a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "檀慧與禪智　　各相合而竪",
        "from": "T20n1087_001:0212a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1087_001:0212a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由示此印故　　諸佛及菩薩",
        "from": "T20n1087_001:0212a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切執金剛　　皆悉妙歡喜",
        "from": "T20n1087_001:0212a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當開心戶　　入金剛智字",
        "from": "T20n1087_001:0212b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀於二乳上　　右怛囉左吒",
        "from": "T20n1087_001:0212b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如宮室戶扇　　殊勝金剛縛",
        "from": "T20n1087_001:0212b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三業同時發　　拍心開兩字",
        "from": "T20n1087_001:0212b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1087_001:0212b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無始熏種子　　所集之塵勞",
        "from": "T20n1087_001:0212b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今以召罪印　　集之欲摧碎",
        "from": "T20n1087_001:0212b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十度堅固縛　　忍願申如針",
        "from": "T20n1087_001:0212b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力屈如鉤　　心想召諸罪",
        "from": "T20n1087_001:0212b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想彼眾罪狀　　植髮裸黑形",
        "from": "T20n1087_001:0212b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "反印刺於心　　觸已誦密語",
        "from": "T20n1087_001:0212b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三業相應故　　能召諸罪積",
        "from": "T20n1087_001:0212b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此召集已　　方作摧碎法",
        "from": "T20n1087_001:0212b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1087_001:0212b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "召入於掌已　　施作摧破法",
        "from": "T20n1087_001:0212b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前印內相叉　　稱𤚥縛諸罪",
        "from": "T20n1087_001:0212b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忍願俱申直　　有怛𡀩吒字",
        "from": "T20n1087_001:0212b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想為金剛杵　　相拍如摧山",
        "from": "T20n1087_001:0212b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忿句及怒形　　能淨諸惡趣",
        "from": "T20n1087_001:0212b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦已忍願拍　　七三隨所宜",
        "from": "T20n1087_001:0212b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此相應門　　先佛方便故",
        "from": "T20n1087_001:0212b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三業所積罪　　無量極重障",
        "from": "T20n1087_001:0212c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作此摧滅已　　如火焚枯草",
        "from": "T20n1087_001:0212c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有情常愚迷　　不知此理趣",
        "from": "T20n1087_001:0212c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來大悲故　　開此祕妙門",
        "from": "T20n1087_001:0212c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當結入印　　內如來智字",
        "from": "T20n1087_001:0212c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二羽堅固縛　　禪智入於中",
        "from": "T20n1087_001:0212c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以進力二度　　相拄如環勢",
        "from": "T20n1087_001:0212c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀前八葉蓮　　其上置阿字",
        "from": "T20n1087_001:0212c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二點嚴飾故　　妙字方名惡",
        "from": "T20n1087_001:0212c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色白如珂雪　　流散千光明",
        "from": "T20n1087_001:0212c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想以進力支　　捻字安心內",
        "from": "T20n1087_001:0212c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三業齊運用　　誦此密言曰",
        "from": "T20n1087_001:0212c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既想入心中　　字相逾光耀",
        "from": "T20n1087_001:0212c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者應是觀　　不久悟寂靜",
        "from": "T20n1087_001:0212c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法本不生故　　三世諸如來",
        "from": "T20n1087_001:0212c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛身口意　　皆以妙方便",
        "from": "T20n1087_001:0212c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持在金剛拳　　以此闔心門",
        "from": "T20n1087_001:0212c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智字獲堅固　　便屈進力度",
        "from": "T20n1087_001:0212c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拄於禪智背　　以印觸胸已",
        "from": "T20n1087_001:0212c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者住等引　　二羽堅固縛",
        "from": "T20n1087_001:0212c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仰置於臍下　　禪智蓮葉形",
        "from": "T20n1087_001:0212c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此名三昧印　　誦此密言曰",
        "from": "T20n1087_001:0212c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出息及入息　　住阿那波那",
        "from": "T20n1087_001:0212c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想佛遍虛空　　彈指警覺我",
        "from": "T20n1087_001:0212c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛子汝云何　　成無上等覺",
        "from": "T20n1087_001:0213a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不知諸如來　　實相之妙法",
        "from": "T20n1087_001:0213a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既聞警覺已　　行者復白言",
        "from": "T20n1087_001:0213a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名真實　　願最勝尊說",
        "from": "T20n1087_001:0213a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛皆歡喜　　作如是勝言",
        "from": "T20n1087_001:0213a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉摩訶薩　　能作如是問",
        "from": "T20n1087_001:0213a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝想於心中　　所內惡字門",
        "from": "T20n1087_001:0213a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以字徹於心　　誦此密言曰",
        "from": "T20n1087_001:0213a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當默誦一遍　　便想為月輪",
        "from": "T20n1087_001:0213a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倍欲精進故　　復誦妙言曰",
        "from": "T20n1087_001:0213a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令心月輪　　圓滿甚清淨",
        "from": "T20n1087_001:0213a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中想妙蓮花　　上安寶金剛",
        "from": "T20n1087_001:0213a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1087_001:0213a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "引量同虛空　　周遍於三界",
        "from": "T20n1087_001:0213a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復誦此妙言　　金剛語離聲",
        "from": "T20n1087_001:0213a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此引妙蓮　　流放千光焰",
        "from": "T20n1087_001:0213a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一光明中　　無量佛剎土",
        "from": "T20n1087_001:0213a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剎中有妙蓮　　想持寶蓮者",
        "from": "T20n1087_001:0213a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持寶蓮勝幢　　幢中出妙聲",
        "from": "T20n1087_001:0213a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誰有薄福者　　當滿一切願",
        "from": "T20n1087_001:0213a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住是寂三昧　　為利諸有情",
        "from": "T20n1087_001:0213a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是菩薩類　　皆住於等引",
        "from": "T20n1087_001:0213a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從蓮花胎藏　　妙放千光明",
        "from": "T20n1087_001:0213a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆為利眾生　　檀波羅密等",
        "from": "T20n1087_001:0213b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍入諸三昧　　理趣善巧門",
        "from": "T20n1087_001:0213b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為慜念有情　　作無量方便",
        "from": "T20n1087_001:0213b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化身為種種　　從生及涅槃",
        "from": "T20n1087_001:0213b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉大妙法輪　　皆從意寶出",
        "from": "T20n1087_001:0213b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所說之妙法　　皆以輪成就",
        "from": "T20n1087_001:0213b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以輪為妙智　　能斷諸結使",
        "from": "T20n1087_001:0213b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶轉大法輪　　此為福智路",
        "from": "T20n1087_001:0213b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次皆正觀察　　漸斂其智蓮",
        "from": "T20n1087_001:0213b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1087_001:0213b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所在諸如來　　皆入為一體",
        "from": "T20n1087_001:0213b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如於明鏡　　能現於萬像",
        "from": "T20n1087_001:0213b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法界自性體　　住於金剛蓮",
        "from": "T20n1087_001:0213b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即變其寶蓮　　為真多菩薩",
        "from": "T20n1087_001:0213b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手持如意寶　　六臂身金色",
        "from": "T20n1087_001:0213b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆想於自身　　頂髻寶莊嚴",
        "from": "T20n1087_001:0213b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冠坐自在王　　住於說法相",
        "from": "T20n1087_001:0213b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一手思惟　　愍念有情故",
        "from": "T20n1087_001:0213b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二持意寶　　能滿一切願",
        "from": "T20n1087_001:0213b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三持念珠　　為度傍生苦",
        "from": "T20n1087_001:0213b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左按光明山　　成就無傾動",
        "from": "T20n1087_001:0213b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二持蓮手　　能淨諸非法",
        "from": "T20n1087_001:0213b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三手持輪　　能轉無上法",
        "from": "T20n1087_001:0213b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六臂廣博體　　能遊於六道",
        "from": "T20n1087_001:0213b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大悲方便　　斷諸有情苦",
        "from": "T20n1087_001:0213b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者如是觀　　坐於月輪中",
        "from": "T20n1087_001:0213b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身流千光明　　項背皆圓光",
        "from": "T20n1087_001:0213b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復想心月輪　　亦有寶蓮花",
        "from": "T20n1087_001:0213c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以是能堅固　　無動觀己身",
        "from": "T20n1087_001:0213c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為離諸妄想　　誦此密言曰",
        "from": "T20n1087_001:0213c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此法加持　　十度芙蓉合",
        "from": "T20n1087_001:0213c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力屈如寶　　印心額喉頂",
        "from": "T20n1087_001:0213c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吽字想於心　　怛囉安於額",
        "from": "T20n1087_001:0213c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "紇哩當喉上　　惡字置於頂",
        "from": "T20n1087_001:0213c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶此布想故　　此身如金剛",
        "from": "T20n1087_001:0213c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復誦此密言　　蓮花語為聲",
        "from": "T20n1087_001:0213c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應結灌頂　　智者合蓮掌",
        "from": "T20n1087_001:0213c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力如寶形　　檀惠開相近",
        "from": "T20n1087_001:0213c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置額誦密言　　心想佛灌頂",
        "from": "T20n1087_001:0213c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以此妙印　　二手分兩邊",
        "from": "T20n1087_001:0213c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如繫蓮花鬘　　徐徐前下散",
        "from": "T20n1087_001:0213c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想垂白帶勢　　誦此妙言曰",
        "from": "T20n1087_001:0213c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當結甲鎧　　二手蓮花拳",
        "from": "T20n1087_001:0213c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從心遶向背　　從背當臍繞",
        "from": "T20n1087_001:0213c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向腰及兩膝　　漸上繞頸後",
        "from": "T20n1087_001:0213c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從頸後當喉　　復於頸後繞",
        "from": "T20n1087_001:0213c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還來至額上　　却於頂後繞",
        "from": "T20n1087_001:0214a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徐從臍下散　　誦此祕密言",
        "from": "T20n1087_001:0214a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為喜諸佛故　　應拍蓮花印",
        "from": "T20n1087_001:0214a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手結蓮掌　　妙拍令歡喜",
        "from": "T20n1087_001:0214a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1087_001:0214a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想於己身前　　觀紇哩字門",
        "from": "T20n1087_001:0214a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變為蓮花王　　中有紇哩字",
        "from": "T20n1087_001:0214a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛囉安兩邊　　為金剛寶蓮",
        "from": "T20n1087_001:0214a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共變為所尊　　持真多妙寶",
        "from": "T20n1087_001:0214a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前已身觀　　今所觀亦然",
        "from": "T20n1087_001:0214a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為令體無二　　次作呼召法",
        "from": "T20n1087_001:0214a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十度未敷蓮　　進力如鉤勢",
        "from": "T20n1087_001:0214a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦此密語　　應為蓮花音",
        "from": "T20n1087_001:0214a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者既召已　　次當結索印",
        "from": "T20n1087_001:0214a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前合蓮掌　　進力拄如環",
        "from": "T20n1087_001:0214a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此名蓮花索　　能滿諸意願",
        "from": "T20n1087_001:0214a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應誦此密語　　召入於智身",
        "from": "T20n1087_001:0214a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既入於智身　　為令無傾動",
        "from": "T20n1087_001:0214a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復當結蓮鎖　　應作決定心",
        "from": "T20n1087_001:0214a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前合蓮掌　　進禪捻如環",
        "from": "T20n1087_001:0214a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "力智亦復然　　相結如鉤鎖",
        "from": "T20n1087_001:0214a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為令妙歡喜　　結蓮花鈴印",
        "from": "T20n1087_001:0214a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當以蓮花捧　　禪智入於中",
        "from": "T20n1087_001:0214a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力如環住　　誦此祕密語",
        "from": "T20n1087_001:0214b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　只惹　娜　鉢頭入麼二合尾捨耶吽次當誦蓮花百字密言捧閼伽器以欝金白檀龍腦香水捧而供養。",
        "from": "T20n1087_001:0214b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以內外供　　供養蓮花王",
        "from": "T20n1087_001:0214b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂內供養　　芙蓉掌當心",
        "from": "T20n1087_001:0214b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪智並申直　　名為蓮花喜",
        "from": "T20n1087_001:0214b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結花鬘印　　以此而供養",
        "from": "T20n1087_001:0214b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不易前喜印　　二手捧而前",
        "from": "T20n1087_001:0214b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想種種寶鬘　　遍滿虛空界",
        "from": "T20n1087_001:0214b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1087_001:0214b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應以歌印　　奉獻智蓮者",
        "from": "T20n1087_001:0214b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復以前妙印　　屈掌拄諸度",
        "from": "T20n1087_001:0214b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從臍漸至口　　散下如寫勢",
        "from": "T20n1087_001:0214b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想緊那羅音　　供養諸聖者",
        "from": "T20n1087_001:0214b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1087_001:0214b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應結舞印　　前印左右旋",
        "from": "T20n1087_001:0214c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合芙蓉妙掌　　安於頂上散",
        "from": "T20n1087_001:0214c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此四供養　　能獲大神通",
        "from": "T20n1087_001:0214c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1087_001:0214c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作此四供養　　能成最勝事",
        "from": "T20n1087_001:0214c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結外供養　　喜心而獻之",
        "from": "T20n1087_001:0214c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運心無邊界　　蓮花焚香法",
        "from": "T20n1087_001:0214c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛誠言說　　為利諸有情",
        "from": "T20n1087_001:0214c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮掌向下散　　猶如焚香勢",
        "from": "T20n1087_001:0214c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此祕密言　　想供養香雲",
        "from": "T20n1087_001:0214c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周遍虛空界　　供養諸聖眾",
        "from": "T20n1087_001:0214c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應結花印　　以三十二相",
        "from": "T20n1087_001:0214c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莊嚴諸如來　　觀妙色花雲",
        "from": "T20n1087_001:0214c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運心遍一切　　如前合蓮掌",
        "from": "T20n1087_001:0214c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上散如花勢　　供養諸如來",
        "from": "T20n1087_001:0214c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸善逝子　　想滿虛空界",
        "from": "T20n1087_001:0214c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "花雲妙芬馥　　劫樹極端嚴",
        "from": "T20n1087_001:0214c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此祕妙言　　三業齊運用",
        "from": "T20n1087_001:0214c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生無明覆　　離於智慧光",
        "from": "T20n1087_001:0214c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為彼淨除故　　應結智燈印",
        "from": "T20n1087_001:0214c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以前蓮花掌　　禪智竪相逼",
        "from": "T20n1087_001:0214c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心想摩尼燈　　遍照虛空界",
        "from": "T20n1087_001:0214c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量光所出　　誦此密言曰",
        "from": "T20n1087_001:0214c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智者次應結　　解脫塗香印",
        "from": "T20n1087_001:0215a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為淨眾生故　　獻此尸羅香",
        "from": "T20n1087_001:0215a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手散蓮掌　　當心塗香勢",
        "from": "T20n1087_001:0215a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十度成熏習　　香海遍虛空",
        "from": "T20n1087_001:0215a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獻佛及所尊　　誦此祕密語",
        "from": "T20n1087_001:0215a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內外供養已　　然後應順念",
        "from": "T20n1087_001:0215a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結祕根本印　　以對密言主",
        "from": "T20n1087_001:0215a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先誦根本言　　分明七遍已",
        "from": "T20n1087_001:0215a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平掌當於心　　忍願如蓮葉",
        "from": "T20n1087_001:0215a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力摩尼狀　　餘度盡如幢",
        "from": "T20n1087_001:0215a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦根本密言　　思滿有情願",
        "from": "T20n1087_001:0215a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1087_001:0215a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結心祕密　　依前根本印",
        "from": "T20n1087_001:0215a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戒方檀惠縛　　名為本心印",
        "from": "T20n1087_001:0215a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸意願　　應心之所念",
        "from": "T20n1087_001:0215a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由結此印故　　皆悉得成就",
        "from": "T20n1087_001:0215a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1087_001:0215a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結隨心印　　二手堅固縛",
        "from": "T20n1087_001:0215a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力摩尼形　　禪智並而申",
        "from": "T20n1087_001:0215a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戒方亦舒直　　檀惠相交竪",
        "from": "T20n1087_001:0215a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次想尊口中　　流出祕密言",
        "from": "T20n1087_001:0215b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分明成字道　　五色光照耀",
        "from": "T20n1087_001:0215b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "間錯殊勝色　　入於諭岐口",
        "from": "T20n1087_001:0215b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "列心月輪中　　瑩如紅頗犁",
        "from": "T20n1087_001:0215b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一諦思惟　　順理隨覺悟",
        "from": "T20n1087_001:0215b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住定而修習　　入於阿字門",
        "from": "T20n1087_001:0215b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即入輪字觀　　皆遍觀諸字",
        "from": "T20n1087_001:0215b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此名三昧念　　獲智及解脫",
        "from": "T20n1087_001:0215b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此相應故　　不久成種智",
        "from": "T20n1087_001:0215b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若當聲順念　　最勝妙奇特",
        "from": "T20n1087_001:0215b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住於本尊觀　　不應急躁心",
        "from": "T20n1087_001:0215b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不高亦不下　　不緩亦不急",
        "from": "T20n1087_001:0215b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智者離分別　　及諸妄想心",
        "from": "T20n1087_001:0215b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦洛叉遍　　所求皆悉地",
        "from": "T20n1087_001:0215b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手持念珠　　頗知與蓮子",
        "from": "T20n1087_001:0215b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "螺珠及餘寶　　無瑕光好者",
        "from": "T20n1087_001:0215b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當穿一百八　　一一誦七遍",
        "from": "T20n1087_001:0215b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心及心中心　　及毘俱多羅",
        "from": "T20n1087_001:0215b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作此法加持　　穿貫珠鬘已",
        "from": "T20n1087_001:0215b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當心一一度　　與莎訶齊聲",
        "from": "T20n1087_001:0215b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一千與百八　　隨力而念誦",
        "from": "T20n1087_001:0215b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四時或三時　　此法後夜勝",
        "from": "T20n1087_001:0215b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如意輪經中　　本教佛所說",
        "from": "T20n1087_001:0215b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若如是修習　　現世證初地",
        "from": "T20n1087_001:0215b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過此十六生　　成無上菩提",
        "from": "T20n1087_001:0215b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況世悉地　　現生不如意",
        "from": "T20n1087_001:0215b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨力念誦已　　重結三昧印",
        "from": "T20n1087_001:0215b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復為八供養　　發遣密言主",
        "from": "T20n1087_001:0215b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二羽堅固縛　　忍願蓮葉形",
        "from": "T20n1087_001:0215c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從心至面散　　頂上合花掌",
        "from": "T20n1087_001:0215c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想尊虛空中　　復道還本宮",
        "from": "T20n1087_001:0215c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1087_001:0215c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發遣聖者已　　自住本尊觀",
        "from": "T20n1087_001:0215c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於閑靜處　　轉讀摩訶衍",
        "from": "T20n1087_001:0215c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "楞伽與花嚴　　般若及理趣",
        "from": "T20n1087_001:0215c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等經教　　思惟而修習",
        "from": "T20n1087_001:0215c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦讀經典已　　自恣行住坐",
        "from": "T20n1087_001:0215c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至於寢息　　不間菩提心",
        "from": "T20n1087_001:0215c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不久當悉地　　金剛藏所說",
        "from": "T20n1087_001:0215c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大悲軌儀　　不擇日及宿",
        "from": "T20n1087_001:0215c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時食與澡浴　　若淨與不淨",
        "from": "T20n1087_001:0215c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常應不間斷　　遠離於散亂",
        "from": "T20n1087_001:0215c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不營諸世務　　念畢發誓願",
        "from": "T20n1087_001:0215c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結三昧耶印　　禮佛菩薩已",
        "from": "T20n1087_001:0215c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意而經行　　由是積福業",
        "from": "T20n1087_001:0215c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解脫本願力　　能令諸有情",
        "from": "T20n1087_001:0215c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速獲一切智　　此上四句願",
        "from": "T20n1087_001:0215c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛儀軌述　　為利眾生故",
        "from": "T20n1087_001:0215c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今同結集觀自在如意輪菩薩瑜伽法要",
        "from": "T20n1087_001:0215c23_100",
        "context_with_next_sentence": false
    }
]