    "X63n1250_004:0638a13_5": "展三拜。",
    "X63n1250_004:0638a13_6": "出堂。",
    "X63n1250_004:0638a14_7": "引全班。",
    "X63n1250_004:0638a14_8": "至住持前。",
    "X63n1250_004:0638a14_9": "行禮謝茶詞語。",
    "X63n1250_004:0638a14_10": "送出。",
    "X63n1250_004:0638a14_11": "復歸堂炷香。",
    "X63n1250_004:0638a15_12": "上下間問訊。",
    "X63n1250_004:0638a15_13": "収盞退座。",
    "X63n1250_004:0638a15_14": "並與結夏特為茶禮同。",
    "X63n1250_004:0638a15_15": "有榜當先安下處呈新命。",
    "X63n1250_004:0638a16_16": "入院日挂僧堂前。",
    "X63n1250_004:0638a16_17": "上下間茶湯。",
    "X63n1250_004:0638a17_18": "只炷香稟請。",
    "X63n1250_004:0638a17_19": "但請湯罷。",
    "X63n1250_004:0638a17_20": "覆就座藥石。",
    "X63n1250_004:0638a18_0": "狀式(當寺庫司比丘某。",
    "X63n1250_004:0638a18_1": "　右某啟取。",
    "X63n1250_004:0638a18_2": "今辰齋退。",
    "X63n1250_004:0638a18_3": "就雲堂點茶。",
    "X63n1250_004:0638a18_4": "用伸　陳賀之儀。",
    "X63n1250_004:0638a18_5": "伏望　尊慈。",
    "X63n1250_004:0638a18_6": "特埀　降重。",
    "X63n1250_004:0638a19_7": "謹狀　湯狀。",
    "X63n1250_004:0638a19_8": "只改湯字。",
    "X63n1250_004:0638a19_9": "並同　　　年　　月　　日。",
    "X63n1250_004:0638a19_10": "當寺庫司比丘某甲　狀)。",
    "X63n1250_004:0638a20_0": "(可漏)(端狀拜請　新命堂頭大和尚尊座前　當寺庫司比丘某　謹封)。",
    "X63n1250_004:0638a21_0": "柰　小參",
    "X63n1250_004:0638a22_0": "湯罷。",
    "X63n1250_004:0638a22_1": "侍者覆住持。",
    "X63n1250_004:0638a22_2": "令客頭報寮挂牌。",
    "X63n1250_004:0638a22_3": "具寫專使兩班耆舊。",
    "X63n1250_004:0638a23_4": "四寮。",
    "X63n1250_004:0638a23_5": "莊庫庵。",
    "X63n1250_004:0638a23_6": "塔暫到行堂。",
    "X63n1250_004:0638a23_7": "入院侍者禪客。",
    "X63n1250_004:0638a23_8": "參隨。",
    "X63n1250_004:0638a24_0": "或有宿山外客。",
    "X63n1250_004:0638a24_1": "逐一條列。",
    "X63n1250_004:0638a24_2": "預呈新命。",
    "X63n1250_004:0638a24_3": "復收。",
    "X63n1250_004:0638a24_4": "至敘謝。",
    "X63n1250_004:0638a24_5": "行者秉燭。",
    "X63n1250_004:0638b01_6": "侍者捧呈。",
    "X63n1250_004:0638b01_7": "詳申謝悃。",
    "X63n1250_004:0638b01_8": "官員諸山留宿。",
    "X63n1250_004:0638b01_9": "當設位。",
    "X63n1250_004:0638b02_0": "下座。",
    "X63n1250_004:0638b02_1": "方丈客頭喝請云。",
    "X63n1250_004:0638b02_2": "(方丈和尚。",
    "X63n1250_004:0638b02_3": "請諸山和尚。",
    "X63n1250_004:0638b02_4": "兩班西堂。",
    "X63n1250_004:0638b02_5": "大耆舊。",
    "X63n1250_004:0638b02_6": "蒙堂。",
    "X63n1250_004:0638b02_7": "侍者禪客。",
    "X63n1250_004:0638b03_0": "即今就寢堂献湯)知事送官客。",
    "X63n1250_004:0638b03_1": "歸客位。",
    "X63n1250_004:0638b03_2": "眾散而後。",
    "X63n1250_004:0638b03_3": "湯果。",
    "X63n1250_004:0638b03_4": "請專使參隨人光伴。",
    "X63n1250_004:0638b05_0": "菜　檀越陞座",
    "X63n1250_004:0638b06_0": "建寺捨田。",
    "X63n1250_004:0638b06_1": "大檀越。",
    "X63n1250_004:0638b06_2": "知事須隔宿覆方丈。",
    "X63n1250_004:0638b06_3": "次早侍者。",
    "X63n1250_004:0638b06_4": "令客頭挂牌報寮。",
    "X63n1250_004:0638b07_5": "庫司差人。",
    "X63n1250_004:0638b07_6": "嚴設祠堂。",
    "X63n1250_004:0638b07_7": "備供養。",
    "X63n1250_004:0638b07_8": "粥罷。",
    "X63n1250_004:0638b07_9": "特為上堂。",
    "X63n1250_004:0638b08_10": "下座。",
    "X63n1250_004:0638b08_11": "集眾詣祠堂。",
    "X63n1250_004:0638b08_12": "知事先到燒香。",
    "X63n1250_004:0638b08_13": "點茶湯。",
    "X63n1250_004:0638b08_14": "上供養。",
    "X63n1250_004:0638b09_15": "住持至。",
    "X63n1250_004:0638b09_16": "炷香歸位。",
    "X63n1250_004:0638b09_17": "維那舉經。",
    "X63n1250_004:0638b09_18": "回向。",
    "X63n1250_004:0638b09_19": "然隨方毗尼也。",
    "X63n1250_004:0638b11_0": "重　管待專使",
    "X63n1250_004:0638b12_0": "知事預稟住持。",
    "X63n1250_004:0638b12_1": "專使厚嚫。",
    "X63n1250_004:0638b12_2": "宣疏帖人嚫。",
    "X63n1250_004:0638b12_3": "衣鉢閤俻貼嚫。",
    "X63n1250_004:0638b13_4": "重輕合節。",
    "X63n1250_004:0638b13_5": "至日寢堂釘挂鋪設位次。",
    "X63n1250_004:0638b13_6": "請兩班耆舊光伴。",
    "X63n1250_004:0638b14_7": "中設專使特為位。",
    "X63n1250_004:0638b14_8": "請客侍者躬請專使。",
    "X63n1250_004:0638b14_9": "餘人則方丈客頭稟請。",
    "X63n1250_004:0638b15_10": "禮與前特為同。",
    "X63n1250_004:0638b16_0": "芥　留請兩班",
    "X63n1250_004:0638b17_0": "兩班俟管待專使畢。",
    "X63n1250_004:0638b17_1": "午後俱詣方丈。",
    "X63n1250_004:0638b17_2": "咨稟告退。",
    "X63n1250_004:0638b17_3": "(首座報集西序。",
    "X63n1250_004:0638b18_4": "都寺報集東序)方丈垂情。",
    "X63n1250_004:0638b18_5": "委曲送出。",
    "X63n1250_004:0638b18_6": "侍者令客頭。",
    "X63n1250_004:0638b18_7": "備柈袱爐燭湯具。",
    "X63n1250_004:0638b19_8": "住持同侍者。",
    "X63n1250_004:0638b19_9": "一一詣寮。",
    "X63n1250_004:0638b19_10": "點湯勉留。",
    "X63n1250_004:0638b19_11": "客頭先報寮。",
    "X63n1250_004:0638b20_12": "出門外右邊。",
    "X63n1250_004:0638b20_13": "迎住持入門。",
    "X63n1250_004:0638b20_14": "居主位分坐。",
    "X63n1250_004:0638b20_15": "侍者燒香行湯收盞。",
    "X63n1250_004:0638b21_16": "住持詳盡勸勉已。",
    "X63n1250_004:0638b21_17": "送出門。",
    "X63n1250_004:0638b21_18": "諸寮庫司同。",
    "X63n1250_004:0638b22_0": "若職滿俟久。",
    "X63n1250_004:0638b22_1": "亦當詣諸寮。",
    "X63n1250_004:0638b22_2": "點湯勉留。",
    "X63n1250_004:0638b22_3": "盡主禮也。",
    "X63n1250_004:0638b22_4": "候住持參訪罷。",
    "X63n1250_004:0638b23_5": "再上稟辭。",
    "X63n1250_004:0638b23_6": "次第進退。",
    "X63n1250_004:0638b24_0": "薑　參訪出入",
    "X63n1250_004:0638c01_0": "入院之先。",
    "X63n1250_004:0638c01_1": "未參府縣官屬。",
    "X63n1250_004:0638c01_2": "當即具門狀參見。",
    "X63n1250_004:0638c01_3": "次謁檀越寄居官貴諸山。",
    "X63n1250_004:0638c02_4": "亦用門狀。",
    "X63n1250_004:0638c02_5": "合訪及處在知事逐一稟覆。",
    "X63n1250_004:0638c03_6": "住持出門。",
    "X63n1250_004:0638c03_7": "令行者傳語。",
    "X63n1250_004:0638c03_8": "首座維那。",
    "X63n1250_004:0638c03_9": "諸頭首知事。",
    "X63n1250_004:0638c04_0": "當詣方丈。",
    "X63n1250_004:0638c04_1": "候送。",
    "X63n1250_004:0638c04_2": "庫司當探計住持歸期。",
    "X63n1250_004:0638c04_3": "令堂司挂牌報眾。",
    "X63n1250_004:0638c05_4": "俟入山。",
    "X63n1250_004:0638c05_5": "鳴堂前鐘。",
    "X63n1250_004:0638c05_6": "集眾門迎。",
    "X63n1250_004:0638c05_7": "住持出轎。",
    "X63n1250_004:0638c05_8": "問訊云。",
    "X63n1250_004:0638c06_0": "(勿勞大眾)即往佛殿土地堂燒香。",
    "X63n1250_004:0638c06_1": "首座領眾。",
    "X63n1250_004:0638c06_2": "至方丈。",
    "X63n1250_004:0638c06_3": "接已眾退。",
    "X63n1250_004:0638c07_4": "客頭揖請兩班耆舊獻湯。",
    "X63n1250_004:0638c07_5": "略敘話而退。",
    "X63n1250_004:0638c07_6": "方丈行者。",
    "X63n1250_004:0638c08_7": "插香禮拜。",
    "X63n1250_004:0638c08_8": "次參頭領眾禮拜。",
    "X63n1250_004:0638c08_9": "住持須巡寮報禮。",
    "X63n1250_004:0638c08_10": "城郭近便朝暮出入無時。",
    "X63n1250_004:0638c09_11": "不必講行。",
    "X63n1250_004:0638c09_12": "凡公出遠歸。",
    "X63n1250_004:0638c09_13": "首座領眾迎接。",
    "X63n1250_004:0638c10_14": "住持傳語。",
    "X63n1250_004:0638c10_15": "免到方丈。",
    "X63n1250_004:0638c10_16": "或密歸鉢位。",
    "X63n1250_004:0638c10_17": "巡堂一帀。",
    "X63n1250_004:0638c11_18": "而上方丈。",
    "X63n1250_004:0638c11_19": "或密回方丈。",
    "X63n1250_004:0638c11_20": "兩班(耆舊)皆當詣方丈問起居。",
    "X63n1250_004:0638c12_21": "無準和尚住徑山日。",
    "X63n1250_004:0638c12_22": "化緣多出入。",
    "X63n1250_004:0638c12_23": "每闞齋粥時。",
    "X63n1250_004:0638c12_24": "徑歸鉢位。",
    "X63n1250_004:0638c13_25": "伴眾食。",
    "X63n1250_004:0638c13_26": "挂鉢了。",
    "X63n1250_004:0638c13_27": "方丈客頭喝云。",
    "X63n1250_004:0638c13_28": "(方丈和尚巡堂人事)眾下地。",
    "X63n1250_004:0638c14_29": "住持燒香一炷。",
    "X63n1250_004:0638c14_30": "巡堂一帀。",
    "X63n1250_004:0638c14_31": "出堂喝云。",
    "X63n1250_004:0638c14_32": "(堂頭和尚傳語。",
    "X63n1250_004:0638c14_33": "不煩訪及)兩班耆舊。",
    "X63n1250_004:0638c15_34": "亦當詣方丈省問。",
    "X63n1250_004:0638c15_35": "當炷香獻茶。",
    "X63n1250_004:0638c15_36": "欵曲敘話而散。",
    "X63n1250_004:0638c17_0": "海　交割什物",
    "X63n1250_004:0638c18_0": "方丈請兩班大耆舊茶。",
    "X63n1250_004:0638c18_1": "詳說家務。",
    "X63n1250_004:0638c18_2": "次至庫司恭。",
    "X63n1250_004:0638c18_3": "眼同撿視御書寶器。",
    "X63n1250_004:0638c19_4": "砧基物件。",
    "X63n1250_004:0638c19_5": "先呈方丈。",
    "X63n1250_004:0638c19_6": "次呈頭首耆舊。",
    "X63n1250_004:0638c20_0": "逐一點對交割。",
    "X63n1250_004:0638c20_1": "計筭財糓。",
    "X63n1250_004:0638c20_2": "簿書分明。",
    "X63n1250_004:0638c20_3": "見管若干。",
    "X63n1250_004:0638c20_4": "具呈方丈。",
    "X63n1250_004:0638c21_5": "參照須知。",
    "X63n1250_004:0638c21_6": "諸莊佃戶換契。",
    "X63n1250_004:0638c21_7": "住持俻榜。",
    "X63n1250_004:0638c21_8": "諸莊曉示。",
    "X63n1250_004:0638c22_0": "諸方多於住持交換之際。",
    "X63n1250_004:0638c22_1": "執事作弊。",
    "X63n1250_004:0638c22_2": "苛取錢物。",
    "X63n1250_004:0638c22_3": "後患非輕。",
    "X63n1250_004:0638c23_4": "可不審諸。",
    "X63n1250_004:0638c23_5": "庫司此日俻點心。",
    "X63n1250_004:0638c24_0": "禪林備用卷之四"
}