[
    {
        "source_sentence": "禪波羅蜜序禪波羅蜜序",
        "from": "T46n1916_001:0475a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪波羅蜜者。輔行云。次第禪門目錄云。大師",
        "from": "T46n1916_001:0475a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於瓦官寺說也。大莊嚴寺。法慎私記。章安頂",
        "from": "T46n1916_001:0475a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪師。",
        "from": "T46n1916_001:0475a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "治定為十卷。",
        "from": "T46n1916_001:0475a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開十大章。",
        "from": "T46n1916_001:0475a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一大意。",
        "from": "T46n1916_001:0475a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二釋名。",
        "from": "T46n1916_001:0475a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三明門。",
        "from": "T46n1916_001:0475a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四詮次。",
        "from": "T46n1916_001:0475a21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五法心。",
        "from": "T46n1916_001:0475a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六方便。",
        "from": "T46n1916_001:0475a21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七修證。",
        "from": "T46n1916_001:0475a21_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八果報。",
        "from": "T46n1916_001:0475a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九起教。",
        "from": "T46n1916_001:0475a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十歸趣。",
        "from": "T46n1916_001:0475a22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但至修證。",
        "from": "T46n1916_001:0475a22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘三略無。",
        "from": "T46n1916_001:0475a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於修證中。",
        "from": "T46n1916_001:0475a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又開四別。",
        "from": "T46n1916_001:0475a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一世間禪。",
        "from": "T46n1916_001:0475a23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二亦世間亦出世間。",
        "from": "T46n1916_001:0475a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三出世間。",
        "from": "T46n1916_001:0475a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四非世間非出世間。",
        "from": "T46n1916_001:0475a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四中唯至第三出世。",
        "from": "T46n1916_001:0475a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復為二。",
        "from": "T46n1916_001:0475a25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一對治無漏。",
        "from": "T46n1916_001:0475a25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二緣理無漏。",
        "from": "T46n1916_001:0475a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但至對治。",
        "from": "T46n1916_001:0475a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又為九。",
        "from": "T46n1916_001:0475a26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂九想。",
        "from": "T46n1916_001:0475a26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八念十想。",
        "from": "T46n1916_001:0475a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "背捨勝處。",
        "from": "T46n1916_001:0475a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切處。",
        "from": "T46n1916_001:0475a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九次第定。",
        "from": "T46n1916_001:0475a27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奮迅。",
        "from": "T46n1916_001:0475a27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "超越。",
        "from": "T46n1916_001:0475a27_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然修證之相。",
        "from": "T46n1916_001:0475a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈可盡具。",
        "from": "T46n1916_001:0475a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳曰。",
        "from": "T46n1916_001:0475a28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大師甞在高座云。",
        "from": "T46n1916_001:0475a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若說次第禪門。",
        "from": "T46n1916_001:0475a29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年可一遍。",
        "from": "T46n1916_001:0475a29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若著章疏。",
        "from": "T46n1916_001:0475a29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可五十卷。",
        "from": "T46n1916_001:0475b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今刊預示大科。",
        "from": "T46n1916_001:0475b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庶學者不昧始末云。",
        "from": "T46n1916_001:0475b01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十大章初修禪波羅蜜大意第一卷上下二釋禪波羅蜜名三明禪波羅蜜門四辨禪波羅蜜詮次五簡禪波羅蜜法心六分別禪波羅蜜前方便二初外方便第二卷二內方便二初正明因止發內外善根第三卷上下二明驗惡根性第四卷七釋禪波羅蜜修證四初修證世間禪相三初四禪第五卷二四無量心第六卷三四無色定二修證亦世間亦出世間禪相三初六妙門第七卷二十六特勝三通明第八卷三修證出世間禪相二初對治無漏九觀壞法第九卷初九想二八念三十想觀不壞法第十卷四八背捨五八勝處六十一切處鍊七九次第定薰八師子奮迅三昧修九超越三昧二緣理無漏不說四修證非世間非出世間禪相八顯示禪波羅蜜果報九從禪波羅蜜起教十結會禪波羅蜜歸趣",
        "from": "T46n1916_001:0475c03_100",
        "context_with_next_sentence": false
    }
]