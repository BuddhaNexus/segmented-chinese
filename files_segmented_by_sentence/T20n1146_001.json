[
    {
        "source_sentence": "大虛空藏菩薩念誦法",
        "from": "T20n1146_001:0603a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T20n1146_001:0603a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空諡大鑒",
        "from": "T20n1146_001:0603a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正號大廣智大興善寺三藏沙門不空奉　詔譯我今依瑜伽金剛頂經。",
        "from": "T20n1146_001:0603a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說寶部虛空藏菩薩真言教法。",
        "from": "T20n1146_001:0603a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為愍念在家山家。",
        "from": "T20n1146_001:0603a15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薄福少德乏少資具者。",
        "from": "T20n1146_001:0603a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求世間出世間勝願多不遂意。",
        "from": "T20n1146_001:0603a16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若依此教法修行。",
        "from": "T20n1146_001:0603a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "業報等障皆悉消除。",
        "from": "T20n1146_001:0603a17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福德增長心神適悅。",
        "from": "T20n1146_001:0603a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨信大乘利樂有情。",
        "from": "T20n1146_001:0603a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心無退轉。",
        "from": "T20n1146_001:0603a18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世出世間所有財寶悉皆獲得。",
        "from": "T20n1146_001:0603a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切眾生能作利益。",
        "from": "T20n1146_001:0603a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一稱一念所得福聚尚猶虛空。",
        "from": "T20n1146_001:0603a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況作意如法修行。",
        "from": "T20n1146_001:0603a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所願必獲殊勝成就。",
        "from": "T20n1146_001:0603a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者先應入灌頂道場。",
        "from": "T20n1146_001:0603a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "親對師前受得儀軌。",
        "from": "T20n1146_001:0603a22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於山間靜處。",
        "from": "T20n1146_001:0603a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於寺舍隨所樂處。",
        "from": "T20n1146_001:0603a23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建立精室作一方壇。",
        "from": "T20n1146_001:0603a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨其大小。",
        "from": "T20n1146_001:0603a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以瞿摩夷塗地。",
        "from": "T20n1146_001:0603a24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作八曼茶羅。",
        "from": "T20n1146_001:0603a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周匝懸幡。",
        "from": "T20n1146_001:0603a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上安天蓋。",
        "from": "T20n1146_001:0603a25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於壇西面安虛空藏菩薩像。",
        "from": "T20n1146_001:0603a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持誦者壇東對像念誦。",
        "from": "T20n1146_001:0603a26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以種種時華散於壇上。",
        "from": "T20n1146_001:0603a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒香燈明飲食菓子。",
        "from": "T20n1146_001:0603a27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨力所辦以為供養。",
        "from": "T20n1146_001:0603a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每入道場。",
        "from": "T20n1146_001:0603a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對尊像前五體投地。",
        "from": "T20n1146_001:0603a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮一切如來及諸聖眾。",
        "from": "T20n1146_001:0603a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即懺悔隨喜勸請發願已。",
        "from": "T20n1146_001:0603b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後結跏趺坐。",
        "from": "T20n1146_001:0603b01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或半跏隨意而坐端身正念。",
        "from": "T20n1146_001:0603b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當以塗香用塗二手。",
        "from": "T20n1146_001:0603b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛心合掌如未敷蓮華。",
        "from": "T20n1146_001:0603b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦清淨真言三遍或七遍頂上散印。",
        "from": "T20n1146_001:0603b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則三業清淨以成勝義澡浴。",
        "from": "T20n1146_001:0603b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨三業真言曰。",
        "from": "T20n1146_001:0603b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵舜入第耨戍引馱曩引耶娑囉二合引賀引次結佛部心三昧耶印。",
        "from": "T20n1146_001:0603b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以止觀十度。",
        "from": "T20n1146_001:0603b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內相叉作拳。",
        "from": "T20n1146_001:0603b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以禪智並竪。",
        "from": "T20n1146_001:0603b08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結印成已。",
        "from": "T20n1146_001:0603b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀想諸佛遍滿虛空。",
        "from": "T20n1146_001:0603b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦佛部心真言三遍。",
        "from": "T20n1146_001:0603b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂上散印真言曰。",
        "from": "T20n1146_001:0603b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵爾曩爾迦半音娑嚩二合引賀引次結蓮華部心三昧耶印。",
        "from": "T20n1146_001:0603b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "準前佛部心印。",
        "from": "T20n1146_001:0603b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智度屈入掌。",
        "from": "T20n1146_001:0603b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直竪禪度。",
        "from": "T20n1146_001:0603b13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結此契已。",
        "from": "T20n1146_001:0603b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想於一切如來右邊。",
        "from": "T20n1146_001:0603b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有觀自在菩薩并諸眷屬。",
        "from": "T20n1146_001:0603b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印誦蓮華部心真言三遍。",
        "from": "T20n1146_001:0603b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂上右散印真言曰。",
        "from": "T20n1146_001:0603b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿去引嚧引力迦半音娑嚩二合引賀引次結金剛部心三昧耶印。",
        "from": "T20n1146_001:0603b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前佛部心印。",
        "from": "T20n1146_001:0603b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以禪度屈入掌。",
        "from": "T20n1146_001:0603b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直竪智度。",
        "from": "T20n1146_001:0603b19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想於一切如來左邊。",
        "from": "T20n1146_001:0603b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有金剛手菩薩并諸眷屬。",
        "from": "T20n1146_001:0603b20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦金剛部心真言三遍。",
        "from": "T20n1146_001:0603b21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂左散印真言曰。",
        "from": "T20n1146_001:0603b21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉二合地力二合迦半音娑嚩二合引賀次結被甲護身印。",
        "from": "T20n1146_001:0603b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以觀羽禪度橫於掌內。",
        "from": "T20n1146_001:0603b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以進忍戒檀四度。",
        "from": "T20n1146_001:0603b24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "握拳結此契成印身五處所謂印額右肩左肩心喉。",
        "from": "T20n1146_001:0603b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名五處護身真言曰。",
        "from": "T20n1146_001:0603b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵步引入嚩二合羅吽引由結此印加持五處。",
        "from": "T20n1146_001:0603b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成被金剛光焰堅固甲冑。",
        "from": "T20n1146_001:0603b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸魔不能障難。",
        "from": "T20n1146_001:0603b29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所持真言速得成就。",
        "from": "T20n1146_001:0603c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結請虛空藏菩薩印。",
        "from": "T20n1146_001:0603c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二羽金剛縛。",
        "from": "T20n1146_001:0603c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直竪忍願。",
        "from": "T20n1146_001:0603c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "反蹙如寶形。",
        "from": "T20n1146_001:0603c03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力各屈如鉤。",
        "from": "T20n1146_001:0603c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想於壇中有寶樓閣。",
        "from": "T20n1146_001:0603c04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於樓閣內有八葉開敷蓮華。",
        "from": "T20n1146_001:0603c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言四遍。",
        "from": "T20n1146_001:0603c05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以進力向身招之。",
        "from": "T20n1146_001:0603c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本尊并及眷屬皆來集會。",
        "from": "T20n1146_001:0603c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迎請真言曰。",
        "from": "T20n1146_001:0603c06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩嚩怛他去誐哆引毘囇引迦嚩日囉二合囉怛曩二合薩嚩引舍引跛哩布囉迦弱吽鑁斛引怛囕二合引次結軍吒利身印。",
        "from": "T20n1146_001:0603c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以檀慧右押左。",
        "from": "T20n1146_001:0603c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相叉入掌。",
        "from": "T20n1146_001:0603c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以戒方並押交上。",
        "from": "T20n1146_001:0603c11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以禪智並押戒方。",
        "from": "T20n1146_001:0603c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以忍願直竪頭相拄。",
        "from": "T20n1146_001:0603c12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力屈如鉤。",
        "from": "T20n1146_001:0603c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作三股杵形。",
        "from": "T20n1146_001:0603c13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦軍吒利真言。",
        "from": "T20n1146_001:0603c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨誦以印。",
        "from": "T20n1146_001:0603c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左旋三匝辟除一切諸魔。",
        "from": "T20n1146_001:0603c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右轉三匝便成結界真言曰。",
        "from": "T20n1146_001:0603c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨囉怛曩二合怛囉二合夜引耶娜莫室戰二合拏摩賀嚩日囉二合矩嚕二合引馱引耶唵戶魯戶魯底瑟姹二合底瑟姹二合滿馱滿馱賀曩賀曩阿密哩二合帝吽發吒娑嚩二合引賀引次獻遏伽水。",
        "from": "T20n1146_001:0604a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者常於壇上。",
        "from": "T20n1146_001:0604a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "近膝置二淨器。",
        "from": "T20n1146_001:0604a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿盛香水以為遏伽。",
        "from": "T20n1146_001:0604a05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初迎請時獻右邊者。",
        "from": "T20n1146_001:0604a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後奉送時獻左邊者。",
        "from": "T20n1146_001:0604a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每奉獻時二羽捧遏伽器。",
        "from": "T20n1146_001:0604a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當額奉獻。",
        "from": "T20n1146_001:0604a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦真言想浴本尊及諸聖眾真言曰。",
        "from": "T20n1146_001:0604a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉二合娜迦吽次結獻蓮華座印。",
        "from": "T20n1146_001:0604a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二羽虛心合掌。",
        "from": "T20n1146_001:0604a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以檀慧禪智各頭相著。",
        "from": "T20n1146_001:0604a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘中間六度微屈。",
        "from": "T20n1146_001:0604a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭相離猶如開敷蓮華葉形真言曰。",
        "from": "T20n1146_001:0604a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵迦麼攞娑嚩二合賀引由結此印真言加持。",
        "from": "T20n1146_001:0604a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切聖眾并及本尊。",
        "from": "T20n1146_001:0604a15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆得七寶蓮華為座。",
        "from": "T20n1146_001:0604a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結大虛空藏普通供養印。",
        "from": "T20n1146_001:0604a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二羽合掌。",
        "from": "T20n1146_001:0604a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以戒方二度外縛。",
        "from": "T20n1146_001:0604a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以進力反蹙如寶形。",
        "from": "T20n1146_001:0604a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結成契已。",
        "from": "T20n1146_001:0604a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言四遍。",
        "from": "T20n1146_001:0604a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普供養真言曰。",
        "from": "T20n1146_001:0604a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵誐誐曩三去婆去嚩引嚩日囉二合斛引想從印出生無量種種供養香花燈燭塗香飲食寶幢幡蓋。",
        "from": "T20n1146_001:0604a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於本尊及一切聖眾前。",
        "from": "T20n1146_001:0604a23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則成就真實廣大供養。",
        "from": "T20n1146_001:0604a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結羯磨印。",
        "from": "T20n1146_001:0604a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以止羽當心仰掌。",
        "from": "T20n1146_001:0604a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以智力相捻。",
        "from": "T20n1146_001:0604a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "反屈力度如寶形。",
        "from": "T20n1146_001:0604a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以觀羽仰掌。",
        "from": "T20n1146_001:0604a27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向前作施願勢結此契已作是思惟。",
        "from": "T20n1146_001:0604a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我身即同虛空藏菩薩即誦羯磨真言曰。",
        "from": "T20n1146_001:0604a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉二合囉怛弩二合引鼻聲憾由作此觀加持故。",
        "from": "T20n1146_001:0604b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者自身即等同本尊虛空藏菩薩。",
        "from": "T20n1146_001:0604b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結三昧耶印。",
        "from": "T20n1146_001:0604b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二羽金剛縛。",
        "from": "T20n1146_001:0604b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力反蹙如寶形。",
        "from": "T20n1146_001:0604b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪智並竪置於當心。",
        "from": "T20n1146_001:0604b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦三昧耶真言七遍真言曰。",
        "from": "T20n1146_001:0604b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉二合囉怛曩二合吽即取水精念珠安於掌中。",
        "from": "T20n1146_001:0604b08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌當心。",
        "from": "T20n1146_001:0604b08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦加持念珠真言三遍真言曰。",
        "from": "T20n1146_001:0604b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵尾嚧引左曩麼攞娑嚩二合賀引則捧珠安於頂上。",
        "from": "T20n1146_001:0604b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發是願言。",
        "from": "T20n1146_001:0604b11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方世界所有修真言行者。",
        "from": "T20n1146_001:0604b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼所受持一切真言。",
        "from": "T20n1146_001:0604b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願速成就。",
        "from": "T20n1146_001:0604b12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即止羽承珠。",
        "from": "T20n1146_001:0604b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀羽當心。",
        "from": "T20n1146_001:0604b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "移珠不緩不急。",
        "from": "T20n1146_001:0604b13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心離散亂。",
        "from": "T20n1146_001:0604b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或千或百限數畢已。",
        "from": "T20n1146_001:0604b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捧珠頂戴又發是願。",
        "from": "T20n1146_001:0604b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切有情所希望。",
        "from": "T20n1146_001:0604b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世出世間殊勝果報。",
        "from": "T20n1146_001:0604b15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以我念誦福力。",
        "from": "T20n1146_001:0604b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速令成就。",
        "from": "T20n1146_001:0604b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即安珠於本處。",
        "from": "T20n1146_001:0604b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復結本尊三昧耶印。",
        "from": "T20n1146_001:0604b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦三昧耶真言七遍。",
        "from": "T20n1146_001:0604b17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂上散印。",
        "from": "T20n1146_001:0604b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又結普供養印誦普通供養真言七遍。",
        "from": "T20n1146_001:0604b18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂上散印誦虛空藏菩薩讚歎曰。",
        "from": "T20n1146_001:0604b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚩日囉二合囉怛曩二合素上嚩日囉二合囉他二合一嚩日囉二合引迦引捨摩賀引麼柅二阿去引迦引捨󰋲婆去嚩日囉二合茶馱夜反三嚩日囉二合󰋲婆去曩謨引窣堵二合帝四誦讚歎已。",
        "from": "T20n1146_001:0604b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即取左邊閼伽。",
        "from": "T20n1146_001:0604b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當額奉獻。",
        "from": "T20n1146_001:0604b24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結前軍吒利印。",
        "from": "T20n1146_001:0604b25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左旋一匝解界。",
        "from": "T20n1146_001:0604b25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結三昧耶印。",
        "from": "T20n1146_001:0604b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉送一切聖眾。",
        "from": "T20n1146_001:0604b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉送真言曰。",
        "from": "T20n1146_001:0604b26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉二合囉怛曩二合穆舉印安頂上。",
        "from": "T20n1146_001:0604b28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言七遍。",
        "from": "T20n1146_001:0604b28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成奉送一切聖眾。",
        "from": "T20n1146_001:0604b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復結三部心三昧耶各誦三遍。",
        "from": "T20n1146_001:0604b29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結護身印。",
        "from": "T20n1146_001:0604c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前印於五處。",
        "from": "T20n1146_001:0604c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即禮佛發願隨意出道場。",
        "from": "T20n1146_001:0604c01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉讀大乘印佛印塔。",
        "from": "T20n1146_001:0604c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣行檀施常須饒益一切有情。",
        "from": "T20n1146_001:0604c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大虛空藏菩薩念誦法",
        "from": "T20n1146_001:0604c04_100",
        "context_with_next_sentence": false
    }
]