[
    {
        "source_sentence": "金剛頂勝初瑜伽普賢菩薩念誦",
        "from": "T20n1123_001:0528a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法一卷",
        "from": "T20n1123_001:0528a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿",
        "from": "T20n1123_001:0528a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命禮普賢　　法界真如體",
        "from": "T20n1123_001:0528a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今依大教　　金剛頂勝初",
        "from": "T20n1123_001:0528a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略述修行儀　　勝初金剛界",
        "from": "T20n1123_001:0528a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "海會諸聖眾　　垂慈見加護",
        "from": "T20n1123_001:0528a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利益修行者　　是故結集之",
        "from": "T20n1123_001:0528a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲求解脫　　彼於阿闍梨",
        "from": "T20n1123_001:0528a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求受於灌頂　　若得許可已",
        "from": "T20n1123_001:0528a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方依本教修　　揀擇得勝處",
        "from": "T20n1123_001:0528a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建立於輪壇　　即當想自身",
        "from": "T20n1123_001:0528a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同彼普賢體　　色白如珂雪",
        "from": "T20n1123_001:0528a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端坐入三昧　　舌上想五股",
        "from": "T20n1123_001:0528a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨妙金剛杵　　密誦此真言",
        "from": "T20n1123_001:0528a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同一法界淨　　即誦真言曰",
        "from": "T20n1123_001:0528a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由誦此真言　　身器皆清淨",
        "from": "T20n1123_001:0528a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即觀虛空佛　　遍滿如胡麻",
        "from": "T20n1123_001:0528b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則誦遍照明　　歷然見諸佛",
        "from": "T20n1123_001:0528b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應滿普賢行　　求成最正覺",
        "from": "T20n1123_001:0528b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身心不動搖　　定中禮諸佛",
        "from": "T20n1123_001:0528b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本尊大印成　　次結金剛掌",
        "from": "T20n1123_001:0528b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪合交初分　　密言如是稱",
        "from": "T20n1123_001:0528b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便為金剛縛　　其明如後陳",
        "from": "T20n1123_001:0528b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開縛摧拍心　　應誦密言曰",
        "from": "T20n1123_001:0528b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次前金剛縛　　禪智檀慧開",
        "from": "T20n1123_001:0528b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱遍入真言　　降臨每加護",
        "from": "T20n1123_001:0528b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不改次前契　　禪智進力加",
        "from": "T20n1123_001:0528b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名三昧拳　　真言如是誦",
        "from": "T20n1123_001:0528b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分彼拳作二　　左慢右安心",
        "from": "T20n1123_001:0528b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身語意金剛　　形體依初觀",
        "from": "T20n1123_001:0528b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同前縛為準　　智與進力門",
        "from": "T20n1123_001:0528b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪逼於其中　　祕密三昧契",
        "from": "T20n1123_001:0528b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印心額喉頂　　皆誦此真言",
        "from": "T20n1123_001:0528b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛縛又陳　　忍願成刀狀",
        "from": "T20n1123_001:0528b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力捻刀側　　依初第一文",
        "from": "T20n1123_001:0528b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪押於智端　　如結加趺勢",
        "from": "T20n1123_001:0528b30_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印頂心當想　　毘盧遮那佛",
        "from": "T20n1123_001:0528c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儼然鮮白耀　　誦此真言曰",
        "from": "T20n1123_001:0528c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當印其額　　應想阿閦鼙",
        "from": "T20n1123_001:0528c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色青處於前　　誦此密明句",
        "from": "T20n1123_001:0528c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次按於頂右　　其名寶生尊",
        "from": "T20n1123_001:0528c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黃色相端嚴　　真言如是誦",
        "from": "T20n1123_001:0528c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉置於頂後　　無量壽如來",
        "from": "T20n1123_001:0528c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色赤殊特儀　　稱此真言曰",
        "from": "T20n1123_001:0528c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復至於頂左　　不空成就尊",
        "from": "T20n1123_001:0528c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "綠色五佛周　　稱此祕明句",
        "from": "T20n1123_001:0528c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次作寶鬘印　　灌頂以嚴身",
        "from": "T20n1123_001:0528c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用二金剛拳　　額前遂縈遶",
        "from": "T20n1123_001:0528c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復分拳腦後　　如前又繫鬘",
        "from": "T20n1123_001:0528c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自檀慧徐開　　以羽兩傍下",
        "from": "T20n1123_001:0528c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若垂繒帶想　　誦次後真言",
        "from": "T20n1123_001:0528c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定慧羽皆舒　　俱拍契成就",
        "from": "T20n1123_001:0528c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能悅一切聖　　真言如是稱",
        "from": "T20n1123_001:0528c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛慢印明　　默心誦一遍",
        "from": "T20n1123_001:0529a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復具明如上　　金剛薩埵冠",
        "from": "T20n1123_001:0529a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以五佛色身　　安住相應印",
        "from": "T20n1123_001:0529a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍照薄伽梵　　契住如來拳",
        "from": "T20n1123_001:0529a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次陳阿閦鼙　　定羽持衣角",
        "from": "T20n1123_001:0529a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成拳按心上　　慧觸地如儀",
        "from": "T20n1123_001:0529a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施願寶生尊　　智掌仰當乳",
        "from": "T20n1123_001:0529a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量光勝印　　定拳慢執蓮",
        "from": "T20n1123_001:0529a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧拳似敷華　　又如無動佛",
        "from": "T20n1123_001:0529a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智羽三幡相　　如拔濟有情",
        "from": "T20n1123_001:0529a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "揚掌於乳傍　　不空成就印",
        "from": "T20n1123_001:0529a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又作金剛縛　　開掌禪智合",
        "from": "T20n1123_001:0529a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "檀慧直如峯　　忍願入於掌",
        "from": "T20n1123_001:0529a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相合如箭狀　　印心額及喉",
        "from": "T20n1123_001:0529a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於頂亦如之　　皆用心密語",
        "from": "T20n1123_001:0529a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便捧其香水　　稱後閼伽明",
        "from": "T20n1123_001:0529a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "近額奉獻之　　如儀浴眾聖",
        "from": "T20n1123_001:0529a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依前觀滿月　　皓白現壇中",
        "from": "T20n1123_001:0529a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大聖處於間　　契住金剛慢",
        "from": "T20n1123_001:0529a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復陳四尊位　　眼箭在其前",
        "from": "T20n1123_001:0529a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色赤衣服然　　冠鬘以嚴飾",
        "from": "T20n1123_001:0529a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二羽彎弓矢　　瞻矚薩埵儀",
        "from": "T20n1123_001:0529a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "計里計羅尊　　色白居其右",
        "from": "T20n1123_001:0529a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛拳二羽　　交抱三昧耶",
        "from": "T20n1123_001:0529b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於後愛為名　　形服皆青色",
        "from": "T20n1123_001:0529b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二拳竪慧臂　　肘以定羽承",
        "from": "T20n1123_001:0529b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共執摩竭幢　　於左名為慢",
        "from": "T20n1123_001:0529b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色黃拳在胯　　向左小低頭",
        "from": "T20n1123_001:0529b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羯摩印真言　　加持諦安立",
        "from": "T20n1123_001:0529b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大聖金剛慢　　應誦心密言",
        "from": "T20n1123_001:0529b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸尊次復陳　　自眼箭為始",
        "from": "T20n1123_001:0529b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二拳各堅固　　彎弓放箭儀",
        "from": "T20n1123_001:0529b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密契相已成　　祕明如後誦",
        "from": "T20n1123_001:0529b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "準前拳二羽　　交臂抱於胸",
        "from": "T20n1123_001:0529b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "計里計羅尊　　當誦此後句",
        "from": "T20n1123_001:0529b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又改次前印　　慧臂直如幢",
        "from": "T20n1123_001:0529b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定拳承肘間　　誦明名愛契",
        "from": "T20n1123_001:0529b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二拳各居𦜮　　以頭向左傾",
        "from": "T20n1123_001:0529b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慢契相遂成　　真言如後誦",
        "from": "T20n1123_001:0529b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次陳內供養　　初起東南隅",
        "from": "T20n1123_001:0529b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捧華形服白　　金剛妙適悅",
        "from": "T20n1123_001:0529b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛適悅性　　色黑執香爐",
        "from": "T20n1123_001:0529b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛眼獻燈　　色赤嚴飾爾",
        "from": "T20n1123_001:0529b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "末為塗香位　　金剛大吉祥",
        "from": "T20n1123_001:0529b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形質皆以黃　　次陳其契相",
        "from": "T20n1123_001:0529b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱先二拳舞　　如儀遂結成",
        "from": "T20n1123_001:0529c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二金剛拳　　相並上擲散",
        "from": "T20n1123_001:0529c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想妙華供養　　真言如後稱",
        "from": "T20n1123_001:0529c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並拳乃下擲　　念焚香雲海",
        "from": "T20n1123_001:0529c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普遍於一切　　誦如後真言",
        "from": "T20n1123_001:0529c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二拳禪智合　　如燈應運想",
        "from": "T20n1123_001:0529c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣施為佛事　　密言如是稱",
        "from": "T20n1123_001:0529c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並覆其二拳　　依胸兩向散",
        "from": "T20n1123_001:0529c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若妙塗香勢　　當誦此真言",
        "from": "T20n1123_001:0529c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外供養諸尊　　四隅又存想",
        "from": "T20n1123_001:0529c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東南名嬉戲　　二拳以當心",
        "from": "T20n1123_001:0529c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "笑處於西南　　二羽口傍散",
        "from": "T20n1123_001:0529c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歌居於西北　　彈執其箜篌",
        "from": "T20n1123_001:0529c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東北舞為名　　如儀旋轉勢",
        "from": "T20n1123_001:0529c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形服皆金色　　真言契又陳",
        "from": "T20n1123_001:0529c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覆並於二拳　　繞心應右轉",
        "from": "T20n1123_001:0529c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名嬉戲印　　其名如後稱",
        "from": "T20n1123_001:0529c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前印口傍　　自檀慧徐散",
        "from": "T20n1123_001:0529c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "揚掌極舒臂　　含笑誦真言",
        "from": "T20n1123_001:0529c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定臂如箜篌　　改進力微屈",
        "from": "T20n1123_001:0529c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧羽彈弦勢　　為歌誦其明",
        "from": "T20n1123_001:0529c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二拳舞於心　　頂上合便散",
        "from": "T20n1123_001:0530a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其名舞契相　　真言句遂陳",
        "from": "T20n1123_001:0530a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四承旨居門　　形儀復當演",
        "from": "T20n1123_001:0530a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初持鉤青色　　嚴麗處於間",
        "from": "T20n1123_001:0530a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南携索皆黃　　西執鎖尚赤",
        "from": "T20n1123_001:0530a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其北名為磬　　具綠色冠鬘",
        "from": "T20n1123_001:0530a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祕契及真言　　復次今當設",
        "from": "T20n1123_001:0530a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二拳背應逼　　檀慧反相鉤",
        "from": "T20n1123_001:0530a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力皆極舒　　又稍屈進度",
        "from": "T20n1123_001:0530a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微招是鉤契　　其明如後稱",
        "from": "T20n1123_001:0530a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前印進力交　　反以頭相挂",
        "from": "T20n1123_001:0530a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其中如環索　　稱誦後真言",
        "from": "T20n1123_001:0530a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "改進力相鉤　　開拳背交臂",
        "from": "T20n1123_001:0530a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂名鎖契成　　密言如是稱",
        "from": "T20n1123_001:0530a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如鎖背相著　　動搖磬明曰",
        "from": "T20n1123_001:0530a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大聖所嚴飾　　華座及衣服",
        "from": "T20n1123_001:0530a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并餘見前尊　　其色隨身相",
        "from": "T20n1123_001:0530a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殊形具眾德　　首戴五佛冠",
        "from": "T20n1123_001:0530a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將建曼荼羅　　諸位先存想",
        "from": "T20n1123_001:0530a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月輪圓明現　　其中觀念之",
        "from": "T20n1123_001:0530a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "半跏面本尊　　適悅目瞻仰",
        "from": "T20n1123_001:0530a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又宣三昧印　　本尊前所陳",
        "from": "T20n1123_001:0530a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼契及真言　　如儀勿差謬",
        "from": "T20n1123_001:0530a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘尊次當設　　亦眼箭為初",
        "from": "T20n1123_001:0530b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准彼所護身　　大樂隨心印",
        "from": "T20n1123_001:0530b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "極屈其進力　　初分背相著",
        "from": "T20n1123_001:0530b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪智並押之　　契相當成就",
        "from": "T20n1123_001:0530b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以禪押於智　　深交印乃成",
        "from": "T20n1123_001:0530b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名計里計羅　　次陳摩竭相",
        "from": "T20n1123_001:0530b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進鉤於願度　　力握其忍端",
        "from": "T20n1123_001:0530b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戒方舒成針　　檀慧合而直",
        "from": "T20n1123_001:0530b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪智自相並　　各押進力傍",
        "from": "T20n1123_001:0530b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛契相已成　　不改次前印",
        "from": "T20n1123_001:0530b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從外觸其股　　先右左亦然",
        "from": "T20n1123_001:0530b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內供養又明　　側捩金剛掌",
        "from": "T20n1123_001:0530b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上擲為華印　　下散成燒香",
        "from": "T20n1123_001:0530b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪智逼為燈　　塗香依胸啟",
        "from": "T20n1123_001:0530b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嬉戲旋轉禮　　笑契近口傍",
        "from": "T20n1123_001:0530b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自檀慧徐開　　又結其歌印",
        "from": "T20n1123_001:0530b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "改力度微屈　　進虛撥於間",
        "from": "T20n1123_001:0530b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定羽佉吒迦　　慧作三幡相",
        "from": "T20n1123_001:0530b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當心乃旋舞　　八供養已周",
        "from": "T20n1123_001:0530b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結金剛縛　　改進力微屈",
        "from": "T20n1123_001:0530b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徐招是鉤契　　從縛索當生",
        "from": "T20n1123_001:0530b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪度智力間　　印成又為鎖",
        "from": "T20n1123_001:0530b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "環進禪智力　　捻已便相鉤",
        "from": "T20n1123_001:0530b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又作堅固縛　　禪智戒方下",
        "from": "T20n1123_001:0530b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當胸遂搖動　　為磬四攝成",
        "from": "T20n1123_001:0530b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三昧耶真言　　同前羯磨句",
        "from": "T20n1123_001:0530b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛薩埵位　　及行者所居",
        "from": "T20n1123_001:0530b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆有十六尊　　圍遶端嚴住",
        "from": "T20n1123_001:0530b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又誦次所陳　　最勝真實讚",
        "from": "T20n1123_001:0530b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能纔稱念故　　速令悉地圓",
        "from": "T20n1123_001:0530c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦讚聲畢已　　觀念本所尊",
        "from": "T20n1123_001:0530c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身心不散亂　　捧戴於珠鬘",
        "from": "T20n1123_001:0530c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當心以加持　　而誦真言曰",
        "from": "T20n1123_001:0530c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次誦本所尊　　持珠住等引",
        "from": "T20n1123_001:0530c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不極動舌端　　脣齒二俱合",
        "from": "T20n1123_001:0530c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛語離聲　　分明觀相好",
        "from": "T20n1123_001:0530c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四時不令間　　百千已為限",
        "from": "T20n1123_001:0530c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或復過於是　　真言如是稱",
        "from": "T20n1123_001:0530c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦分限畢　　捧珠發大願",
        "from": "T20n1123_001:0530c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結根本印　　誦本明七遍",
        "from": "T20n1123_001:0530c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復修八供養　　以妙聲誦讚",
        "from": "T20n1123_001:0530c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捧獻閼伽水　　解界及諸印",
        "from": "T20n1123_001:0530c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結三昧拳　　一誦而掣開",
        "from": "T20n1123_001:0530c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結羯摩拏　　三誦三開手",
        "from": "T20n1123_001:0530c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼彼所生印　　一一自當解",
        "from": "T20n1123_001:0530c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結奉送印　　二羽金剛縛",
        "from": "T20n1123_001:0530c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忍願竪如針　　誦已而上擲",
        "from": "T20n1123_001:0530c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當結寶印　　二羽金剛縛",
        "from": "T20n1123_001:0531a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力如寶形　　禪智亦復爾",
        "from": "T20n1123_001:0531a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印相從心起　　置於灌頂處",
        "from": "T20n1123_001:0531a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分手如繫鬘　　亦成甲冑印",
        "from": "T20n1123_001:0531a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持被甲已　　齊掌而三拍",
        "from": "T20n1123_001:0531a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令聖眾歡喜　　以此真言印",
        "from": "T20n1123_001:0531a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解縛得歡喜　　而誦真言曰",
        "from": "T20n1123_001:0531a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉送聖眾已　　自作加持竟",
        "from": "T20n1123_001:0531a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便出於道場　　任意自經行",
        "from": "T20n1123_001:0531a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉讀大乘典　　調息自身心",
        "from": "T20n1123_001:0531a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心常想本尊　　仁者應遵奉",
        "from": "T20n1123_001:0531a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛頂勝初瑜伽普賢菩薩念誦法一卷",
        "from": "T20n1123_001:0531a20_100",
        "context_with_next_sentence": false
    }
]