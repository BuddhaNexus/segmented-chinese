[
    {
        "source_sentence": "大毘盧遮那略要速疾門五支念",
        "from": "T18n0858_001:0177b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦法",
        "from": "T18n0858_001:0177b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三金剛真言誦三遍：",
        "from": "T18n0858_001:0177b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吽引吽引吽引三昧耶真言誦三遍：",
        "from": "T18n0858_001:0177b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱南引阿參迷怛里參迷三摩曳娑縛二合賀法界生真言誦三遍：",
        "from": "T18n0858_001:0177b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱南引達磨馱暏娑縛婆縛句唅引轉大法輪真言誦三遍：",
        "from": "T18n0858_001:0177b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多縛日羅二合赦嚩日囉二合怛募句唅引金剛護身真言誦三遍：",
        "from": "T18n0858_001:0177b17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多縛日羅二合赦縛日羅二合迦縛制吽毘盧舍那根本真言誦七遍：",
        "from": "T18n0858_001:0177b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱南引阿上聲呼之娑嚩二合賀引不動尊真言誦七遍：",
        "from": "T18n0858_001:0177b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多縛日羅二合赦戰拏摩訶嚕殺拏娑泮吒耶吽怛羅吒含𤚥普供養真言誦三遍：",
        "from": "T18n0858_001:0177b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱南薩縛他欠蘊那蘖帝娑破囉𠰘𤚥引誐誐曩釰娑縛賀燒香真言誦三遍：",
        "from": "T18n0858_001:0177b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱南引達磨馱都拏蘖帝娑縛賀獻閼伽真言誦三遍：",
        "from": "T18n0858_001:0177c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱南誐誐曩三磨三磨娑縛二合賀引烏蒭澁摩金剛真言誦五遍：",
        "from": "T18n0858_001:0177c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵俱嚕馱喃吽惹慈覺錄云大毘盧遮那略要速疾門五支念誦法一卷仁運睿。",
        "from": "T18n0858_001:0177c08_7",
        "context_with_next_sentence": true
    }
]