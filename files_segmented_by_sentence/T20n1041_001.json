[
    {
        "source_sentence": "觀自在菩薩心真言一印念誦法",
        "from": "T20n1041_001:0032a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千手千眼軌出",
        "from": "T20n1041_001:0032a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大興善寺三藏沙門大廣智",
        "from": "T20n1041_001:0032a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空奉　詔譯夫修行者欲念誦時。",
        "from": "T20n1041_001:0032a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先應誦清淨真言七遍。",
        "from": "T20n1041_001:0032a26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1041_001:0032a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵􄙦󰡊詩聿反􇧼弟􄖭耨􄙣戌引􇧹馱􄖪曩引􄗱野􅁥娑嚩二合引􄚩賀引由誦此真言。",
        "from": "T20n1041_001:0032b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則獲得三業清淨。",
        "from": "T20n1041_001:0032b03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則成以功德法水灑浴身心。",
        "from": "T20n1041_001:0032b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則結觀自在菩薩印。",
        "from": "T20n1041_001:0032b04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二羽相叉作拳。",
        "from": "T20n1041_001:0032b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以禪度出外直竪即成。",
        "from": "T20n1041_001:0032b05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦心真言七遍。",
        "from": "T20n1041_001:0032b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1041_001:0032b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵􆿆阿去引􄙆嚧引􄙌力􈅝迦半音􅁥婆嚩二合引􄚩賀引由結此印及誦真言。",
        "from": "T20n1041_001:0032b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成三昧耶。",
        "from": "T20n1041_001:0032b11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又結前印誦真言三遍。",
        "from": "T20n1041_001:0032b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印左繞三匝。",
        "from": "T20n1041_001:0032b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則成辟除一切諸魔障者。",
        "from": "T20n1041_001:0032b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又結前印誦真言三遍。",
        "from": "T20n1041_001:0032b13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印右轉三匝則成結界。",
        "from": "T20n1041_001:0032b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則想身前八葉蓮華。",
        "from": "T20n1041_001:0032b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又結前印誦真言七遍。",
        "from": "T20n1041_001:0032b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以禪度向身招之。",
        "from": "T20n1041_001:0032b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則觀自在菩薩。",
        "from": "T20n1041_001:0032b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并蓮華部一切聖眾。",
        "from": "T20n1041_001:0032b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆來集會。",
        "from": "T20n1041_001:0032b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則以二手捧閼伽香水器。",
        "from": "T20n1041_001:0032b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當額奉獻誦真言七遍。",
        "from": "T20n1041_001:0032b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想浴聖眾。",
        "from": "T20n1041_001:0032b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又結前印誦真言七遍。",
        "from": "T20n1041_001:0032b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則成獻蓮華之座。",
        "from": "T20n1041_001:0032b20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又結前印誦真言三遍。",
        "from": "T20n1041_001:0032b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則成塗香供養。",
        "from": "T20n1041_001:0032b21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又結前印誦真言三遍。",
        "from": "T20n1041_001:0032b22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則成華鬘供養。",
        "from": "T20n1041_001:0032b22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又結前印誦真言三遍。",
        "from": "T20n1041_001:0032b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則成種種天妙飲食供養。",
        "from": "T20n1041_001:0032b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又結前印誦真言三遍。",
        "from": "T20n1041_001:0032b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則成燈燭供養。",
        "from": "T20n1041_001:0032b25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又結前印當心誦真言七遍。",
        "from": "T20n1041_001:0032b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則想自身坐於八葉蓮華臺中。",
        "from": "T20n1041_001:0032b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相好威光具足圓滿等同觀自在菩薩。",
        "from": "T20n1041_001:0032b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於蓮華八葉。",
        "from": "T20n1041_001:0032b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每一一葉上。",
        "from": "T20n1041_001:0032b28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各有一如來入定。",
        "from": "T20n1041_001:0032b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面向觀自在菩薩。",
        "from": "T20n1041_001:0032b29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者以如是觀念極令分明。",
        "from": "T20n1041_001:0032c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉印於頂上散。",
        "from": "T20n1041_001:0032c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則取念珠蟠於掌中合掌當心。",
        "from": "T20n1041_001:0032c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦加持念珠真言七遍。",
        "from": "T20n1041_001:0032c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1041_001:0032c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵􄙝尾􄙒嚕引􄑼左􄖩曩引􄗥麼鼻聲􄙊攞􅁥娑嚩二合引􄚩賀引則二手當心。",
        "from": "T20n1041_001:0032c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各聚五指如未敷蓮華形。",
        "from": "T20n1041_001:0032c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當心誦真言。",
        "from": "T20n1041_001:0032c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不緩不急歷分明不應出聲。",
        "from": "T20n1041_001:0032c09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寂靜念誦每與真言後字聲齊移一珠。",
        "from": "T20n1041_001:0032c10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於心真言句中去初後字。",
        "from": "T20n1041_001:0032c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦之時勿令散亂。",
        "from": "T20n1041_001:0032c11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身心凝寂更不異緣。",
        "from": "T20n1041_001:0032c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯觀本尊專注念誦。",
        "from": "T20n1041_001:0032c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則觀想身前八葉蓮華上。",
        "from": "T20n1041_001:0032c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有觀自在菩薩相好圓滿處月輪中。",
        "from": "T20n1041_001:0032c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復觀自身同彼觀自在菩薩等無差別。",
        "from": "T20n1041_001:0032c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦數畢。",
        "from": "T20n1041_001:0032c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捧珠頂戴起慈悲心發大誓願。",
        "from": "T20n1041_001:0032c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切有情所希望。",
        "from": "T20n1041_001:0032c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間殊勝上願。",
        "from": "T20n1041_001:0032c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速得成就則安珠本處。",
        "from": "T20n1041_001:0032c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又結前印當心誦真言七遍。",
        "from": "T20n1041_001:0032c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成三昧耶。",
        "from": "T20n1041_001:0032c18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復以印加持自身五處。",
        "from": "T20n1041_001:0032c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂額上右肩左肩心上及喉是名五處。",
        "from": "T20n1041_001:0032c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各誦真言一遍。",
        "from": "T20n1041_001:0032c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持頂上散印。",
        "from": "T20n1041_001:0032c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又結前印如前獻五種供養。",
        "from": "T20n1041_001:0032c22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各誦真言一遍則成廣大供養。",
        "from": "T20n1041_001:0033a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則以二手捧閼伽水。",
        "from": "T20n1041_001:0033a01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當額奉獻誦真言一遍。",
        "from": "T20n1041_001:0033a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又結前印左轉一匝則成解界。",
        "from": "T20n1041_001:0033a03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又結前印以禪度向外擲之誦真言三遍。",
        "from": "T20n1041_001:0033a04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉印安頂則成奉送一切聖眾。",
        "from": "T20n1041_001:0033a05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又結前印加持。",
        "from": "T20n1041_001:0033a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自身五處。",
        "from": "T20n1041_001:0033a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各誦真言一遍。",
        "from": "T20n1041_001:0033a06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則成被甲。",
        "from": "T20n1041_001:0033a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂上散印。",
        "from": "T20n1041_001:0033a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行者如是依此祕密法要。",
        "from": "T20n1041_001:0033a08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精誠念誦作意修持。",
        "from": "T20n1041_001:0033a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切所為速得成就。",
        "from": "T20n1041_001:0033a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世出世間諸願圓滿。",
        "from": "T20n1041_001:0033a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "業障消除三昧現前。",
        "from": "T20n1041_001:0033a10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不轉此身騰空自在隨意。",
        "from": "T20n1041_001:0033a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即往十方淨土歷事諸佛。",
        "from": "T20n1041_001:0033a11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速成無上菩提。",
        "from": "T20n1041_001:0033a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀自在菩薩心真言一印念誦法八家祕錄云。",
        "from": "T20n1041_001:0033a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀自在菩薩心真言一印念誦法仁不空此中仁者圓仁。",
        "from": "T20n1041_001:0033a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即慈覺大師之諱也。",
        "from": "T20n1041_001:0033a16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此本則慈覺請來也。",
        "from": "T20n1041_001:0033a17_6",
        "context_with_next_sentence": true
    }
]