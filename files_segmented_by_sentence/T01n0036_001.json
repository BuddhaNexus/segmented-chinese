[
    {
        "source_sentence": "佛說本相猗致經",
        "from": "T01n0036_001:0819c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後漢安息國三藏安世高譯",
        "from": "T01n0036_001:0819c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞如是：一時，佛在舍衛國祇樹給孤獨園。佛",
        "from": "T01n0036_001:0819c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便告比丘：",
        "from": "T01n0036_001:0819c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「本有愛不見不了，今見有從有愛，設是本有愛無有今為有，今見分明。",
        "from": "T01n0036_001:0819c24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從是本因緣，令致有愛，有愛，比丘！",
        "from": "T01n0036_001:0819c25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從致有本，不為無有本。",
        "from": "T01n0036_001:0819c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0819c26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從有愛致本？",
        "from": "T01n0036_001:0819c26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂為癡。",
        "from": "T01n0036_001:0819c26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「癡亦，比丘！",
        "from": "T01n0036_001:0819c27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有從致本，不為無有本。",
        "from": "T01n0036_001:0819c27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0819c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "癡有本從致？",
        "from": "T01n0036_001:0819c28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂為五蓋。",
        "from": "T01n0036_001:0819c28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「五蓋，比丘！",
        "from": "T01n0036_001:0819c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦從有本致，不為無有本。",
        "from": "T01n0036_001:0819c29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0820a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五蓋從有致？",
        "from": "T01n0036_001:0820a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂為三惡行。",
        "from": "T01n0036_001:0820a01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「三惡行，比丘！",
        "from": "T01n0036_001:0820a01_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有本從致，不為無有本。",
        "from": "T01n0036_001:0820a02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0820a02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三惡行本從致？",
        "from": "T01n0036_001:0820a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂為不攝根。",
        "from": "T01n0036_001:0820a03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「不攝根，比丘！",
        "from": "T01n0036_001:0820a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有本從致，不為無有本。",
        "from": "T01n0036_001:0820a04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0820a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不攝根從致？",
        "from": "T01n0036_001:0820a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂非本念故。",
        "from": "T01n0036_001:0820a05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「非本念，比丘！",
        "from": "T01n0036_001:0820a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有本從致，不為無有本。",
        "from": "T01n0036_001:0820a06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0820a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非本念從致？",
        "from": "T01n0036_001:0820a07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂不信故。",
        "from": "T01n0036_001:0820a07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「不信，比丘！",
        "from": "T01n0036_001:0820a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有本從致，不為無有本。",
        "from": "T01n0036_001:0820a08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0820a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不信本從致？",
        "from": "T01n0036_001:0820a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂惡非法聞故。",
        "from": "T01n0036_001:0820a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「非法聞，比丘！",
        "from": "T01n0036_001:0820a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有本從致，不為無有本。",
        "from": "T01n0036_001:0820a10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0820a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非法聞本從致？",
        "from": "T01n0036_001:0820a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂非賢者人事。",
        "from": "T01n0036_001:0820a11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「非賢者亦有本從致，不為無有本。",
        "from": "T01n0036_001:0820a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0820a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非賢者從致？",
        "from": "T01n0036_001:0820a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂非賢者人共會樂。",
        "from": "T01n0036_001:0820a13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「如是，比丘，已不賢者聚會滿，令不賢者事滿；",
        "from": "T01n0036_001:0820a14_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已不賢者事滿，令非法滿；",
        "from": "T01n0036_001:0820a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已非法滿，令不信滿；",
        "from": "T01n0036_001:0820a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已不信滿，令非本念滿；",
        "from": "T01n0036_001:0820a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已非本念滿，令不攝根滿；",
        "from": "T01n0036_001:0820a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已不攝根滿，令三惡行犯法滿；",
        "from": "T01n0036_001:0820a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已三惡行犯法滿，令五蓋滿；",
        "from": "T01n0036_001:0820a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已五蓋滿，令癡滿；",
        "from": "T01n0036_001:0820a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已癡滿，令有世間愛滿。",
        "from": "T01n0036_001:0820a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是愛樂滿，稍轉稍轉猗增有。",
        "from": "T01n0036_001:0820a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「度世智慧解脫亦有本，不為無有本。",
        "from": "T01n0036_001:0820a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為度世智慧解脫本？",
        "from": "T01n0036_001:0820a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂七覺意為本。",
        "from": "T01n0036_001:0820a22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「七覺意從有本，不為無有本。",
        "from": "T01n0036_001:0820a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為七覺意從有本？",
        "from": "T01n0036_001:0820a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂為四意止。",
        "from": "T01n0036_001:0820a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「四意止從有本，不為無有本。",
        "from": "T01n0036_001:0820a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0820a25_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四意止從有本？",
        "from": "T01n0036_001:0820a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂三清淨行。",
        "from": "T01n0036_001:0820a26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「三清淨，比丘！",
        "from": "T01n0036_001:0820a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有本從行，不為無有本。",
        "from": "T01n0036_001:0820a27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0820a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三清淨有本？",
        "from": "T01n0036_001:0820a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂為守攝根。",
        "from": "T01n0036_001:0820a28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「守攝根亦有本，不為無有本。",
        "from": "T01n0036_001:0820a29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0820a29_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "守攝根有本？",
        "from": "T01n0036_001:0820b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂為本念故。",
        "from": "T01n0036_001:0820b01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「本念，比丘！",
        "from": "T01n0036_001:0820b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有本，不為無有本。",
        "from": "T01n0036_001:0820b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0820b02_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本念本？",
        "from": "T01n0036_001:0820b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂為信本。",
        "from": "T01n0036_001:0820b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「信本，比丘！",
        "from": "T01n0036_001:0820b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有本，不為無有本。",
        "from": "T01n0036_001:0820b04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0820b04_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信本有本？",
        "from": "T01n0036_001:0820b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂聞法經本。",
        "from": "T01n0036_001:0820b05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「聞法經亦有本，不為無有本。",
        "from": "T01n0036_001:0820b06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為聞法經本？",
        "from": "T01n0036_001:0820b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂事賢者本。",
        "from": "T01n0036_001:0820b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「事賢者亦有本，不為無有本。",
        "from": "T01n0036_001:0820b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為事賢者有本？",
        "from": "T01n0036_001:0820b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂賢者聚本。",
        "from": "T01n0036_001:0820b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「如是，比丘！",
        "from": "T01n0036_001:0820b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聚賢者能得事賢者；",
        "from": "T01n0036_001:0820b10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已事賢者，便聞法言；",
        "from": "T01n0036_001:0820b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已聞法言，便致信本；",
        "from": "T01n0036_001:0820b11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已致信本，便得念本；",
        "from": "T01n0036_001:0820b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已得念本，便攝守根；",
        "from": "T01n0036_001:0820b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已攝守根，便得三清淨；",
        "from": "T01n0036_001:0820b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已有三清淨，便得四意止；",
        "from": "T01n0036_001:0820b13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已得四意止本，便有七覺意；",
        "from": "T01n0036_001:0820b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已有七覺意，便有無為解脫得度世。",
        "from": "T01n0036_001:0820b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是解脫度世，轉轉本，令得度世。",
        "from": "T01n0036_001:0820b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」佛說如是，弟子受行。",
        "from": "T01n0036_001:0820b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說本相猗致經",
        "from": "T01n0036_001:0820b17_100",
        "context_with_next_sentence": false
    }
]