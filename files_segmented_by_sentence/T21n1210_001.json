[
    {
        "source_sentence": "降三世忿怒明王念誦儀軌",
        "from": "T21n1210_001:0041a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "特進試鴻臚鄉大興",
        "from": "T21n1210_001:0041a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善寺三藏沙門大廣智",
        "from": "T21n1210_001:0041a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空奉　詔譯爾時諸佛菩薩一切賢聖現世間。",
        "from": "T21n1210_001:0041a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利益一切有情。",
        "from": "T21n1210_001:0041a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時為非順正法相。",
        "from": "T21n1210_001:0041a25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "損壞國土眾生魔等。",
        "from": "T21n1210_001:0041a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿閦普賢大弘誓甚深故示現忿怒形相。",
        "from": "T21n1210_001:0041a26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言。",
        "from": "T21n1210_001:0041a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我在真言。",
        "from": "T21n1210_001:0041a27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於世間甚所希有。",
        "from": "T21n1210_001:0041a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸如來被聽宣說真言。",
        "from": "T21n1210_001:0041a28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等當守護諸佛教法饒益一切有情。",
        "from": "T21n1210_001:0041a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛告言。",
        "from": "T21n1210_001:0041a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉忿怒王。",
        "from": "T21n1210_001:0041b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝聽說降伏真言。",
        "from": "T21n1210_001:0041b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則時示現四面八臂極惡大忿怒形相。",
        "from": "T21n1210_001:0041b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說真言句頌。",
        "from": "T21n1210_001:0041b02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1210_001:0041b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵蘇蘇吽反婆儞蘇婆吽短呼下同蘖哩二合訶拏二合蘖哩二合訶拏二合吽蘖哩二合訶拏二合波耶吽阿那野斛引婆誐𭊃嚩日羅二合吽𠰢吒爾時普賢大士等。",
        "from": "T21n1210_001:0041b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此真言時。",
        "from": "T21n1210_001:0041b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三千大千世界六種震動。",
        "from": "T21n1210_001:0041b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有天魔鬼界。",
        "from": "T21n1210_001:0041b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於眾生生損害者。",
        "from": "T21n1210_001:0041b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆悉恐怖非得安樂。",
        "from": "T21n1210_001:0041b09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各走集白忿怒尊言。",
        "from": "T21n1210_001:0041b09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願愍念令我無恐懼。",
        "from": "T21n1210_001:0041b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時忿怒王。",
        "from": "T21n1210_001:0041b10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆令得安穩。",
        "from": "T21n1210_001:0041b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則尚右足下踏大自在魔王。",
        "from": "T21n1210_001:0041b11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左足下踏魔婦人。",
        "from": "T21n1210_001:0041b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於國土有情。",
        "from": "T21n1210_001:0041b12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆令得快樂。",
        "from": "T21n1210_001:0041b12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言一遍。",
        "from": "T21n1210_001:0041b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則無量無邊魔界。",
        "from": "T21n1210_001:0041b13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各苦惱熱病故。",
        "from": "T21n1210_001:0041b13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於行者生障礙者無得便者。",
        "from": "T21n1210_001:0041b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變成行者僕從乃到妨難。",
        "from": "T21n1210_001:0041b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者　宜須護三業內外。",
        "from": "T21n1210_001:0041b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此亦可辟除結界印身五處。",
        "from": "T21n1210_001:0041b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作契儀則。",
        "from": "T21n1210_001:0041b16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可見金剛頂瑜伽教中。",
        "from": "T21n1210_001:0041b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更不翻譯。",
        "from": "T21n1210_001:0041b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復不須作諸契法印。",
        "from": "T21n1210_001:0041b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但威驗皆在此真言。",
        "from": "T21n1210_001:0041b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦無有別呪。",
        "from": "T21n1210_001:0041b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲修此真言者面東。",
        "from": "T21n1210_001:0041b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "像前忿怒坐。",
        "from": "T21n1210_001:0041b19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想我身則為不異本尊威怒王。",
        "from": "T21n1210_001:0041b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則淨三業。",
        "from": "T21n1210_001:0041b20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂吽字三遍。",
        "from": "T21n1210_001:0041b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦安法界。",
        "from": "T21n1210_001:0041b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則二手堅固縛捧額上。",
        "from": "T21n1210_001:0041b21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左右三轉亦成辟除結界。",
        "from": "T21n1210_001:0041b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次至心合掌。",
        "from": "T21n1210_001:0041b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮拜忿怒尊諸聖眾。",
        "from": "T21n1210_001:0041b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲降伏一切惡人。",
        "from": "T21n1210_001:0041b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作三角爐。",
        "from": "T21n1210_001:0041b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壇向南。",
        "from": "T21n1210_001:0041b24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取惡木一遍一燒。",
        "from": "T21n1210_001:0041b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿三百二十四遍。",
        "from": "T21n1210_001:0041b25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡人自降伏生歸依。",
        "from": "T21n1210_001:0041b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又欲令惡人病疾者。",
        "from": "T21n1210_001:0041b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作調伏壇誦根本真言。",
        "from": "T21n1210_001:0041b27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一千八十遍。",
        "from": "T21n1210_001:0041b28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒赤黑二種芥子。",
        "from": "T21n1210_001:0041b28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一千八十粒一遍一燒。",
        "from": "T21n1210_001:0041b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言句中誦姓名。",
        "from": "T21n1210_001:0041b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則洗長病而無死滅。",
        "from": "T21n1210_001:0041c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲除病者。",
        "from": "T21n1210_001:0041c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作息災壇向東。",
        "from": "T21n1210_001:0041c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒白米一千粒一呪一燒真言句中。",
        "from": "T21n1210_001:0041c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安除病宇則息病。",
        "from": "T21n1210_001:0041c03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸伏佛法。",
        "from": "T21n1210_001:0041c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有異心如此枝木。",
        "from": "T21n1210_001:0041c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲頓滅惡人。",
        "from": "T21n1210_001:0041c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爐中安惡人姓名形。",
        "from": "T21n1210_001:0041c05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以惡心誦呪一百八遍。",
        "from": "T21n1210_001:0041c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以砂打惡人燒之。",
        "from": "T21n1210_001:0041c06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則死。",
        "from": "T21n1210_001:0041c06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲急甦。",
        "from": "T21n1210_001:0041c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對忿怒王。",
        "from": "T21n1210_001:0041c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以慈悲心誦一百八遍。",
        "from": "T21n1210_001:0041c07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不過時甦復無損壞。",
        "from": "T21n1210_001:0041c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲勝軍陣者。",
        "from": "T21n1210_001:0041c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取香花木一百八枚。",
        "from": "T21n1210_001:0041c09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言燒之。",
        "from": "T21n1210_001:0041c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即雖魔敵軍即時得勝。",
        "from": "T21n1210_001:0041c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦須五穀五香。",
        "from": "T21n1210_001:0041c10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無者隨在。",
        "from": "T21n1210_001:0041c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫惱不得。",
        "from": "T21n1210_001:0041c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲入山林不遭惡禽獸毒虫者。",
        "from": "T21n1210_001:0041c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手掌內把隨節之石呪二七遍。",
        "from": "T21n1210_001:0041c13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "打投行道前。",
        "from": "T21n1210_001:0041c13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖禽獸毒虫皆降伏。",
        "from": "T21n1210_001:0041c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敢無有害損心。",
        "from": "T21n1210_001:0041c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲被敬愛國王大臣者。",
        "from": "T21n1210_001:0041c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作敬愛壇爐燒菊根一百八枚。",
        "from": "T21n1210_001:0041c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一呪一燒一則敬愛自來。",
        "from": "T21n1210_001:0041c16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲濟助重病人者。",
        "from": "T21n1210_001:0042a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對像前呪香水一百八遍。",
        "from": "T21n1210_001:0042a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "急撮入病者腹懷內。",
        "from": "T21n1210_001:0042a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則醒起。",
        "from": "T21n1210_001:0042a02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲留癲㾠病者。",
        "from": "T21n1210_001:0042a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取龍水則呪一百八遍。",
        "from": "T21n1210_001:0042a03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洗二眼口舌等。",
        "from": "T21n1210_001:0042a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然消滅無有他病。",
        "from": "T21n1210_001:0042a04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲縛男女令問顯一辭。",
        "from": "T21n1210_001:0042a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取柘榴木一丈二尺加持一萬遍。",
        "from": "T21n1210_001:0042a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後別處。",
        "from": "T21n1210_001:0042a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以枝一呪一打。",
        "from": "T21n1210_001:0042a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "打人二七處。",
        "from": "T21n1210_001:0042a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即萬事隨問皆顯語。",
        "from": "T21n1210_001:0042a07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即想意計被解縛。",
        "from": "T21n1210_001:0042a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若行者修此法時。",
        "from": "T21n1210_001:0042a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七七日間。",
        "from": "T21n1210_001:0042a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須斷五穀味鹽一心無餘境滿真言十萬遍。",
        "from": "T21n1210_001:0042a10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時本尊現形。",
        "from": "T21n1210_001:0042a10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作大惡怖魔體。",
        "from": "T21n1210_001:0042a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "試謙行者心。",
        "from": "T21n1210_001:0042a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時慎勿生散發則法成就。",
        "from": "T21n1210_001:0042a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時忿怒王說此真言時。",
        "from": "T21n1210_001:0042a13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切天魔惡鬼等。",
        "from": "T21n1210_001:0042a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆大降伏。",
        "from": "T21n1210_001:0042a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而信受歸依佛道。",
        "from": "T21n1210_001:0042a14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各作將護者行者。",
        "from": "T21n1210_001:0042a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次解結界。",
        "from": "T21n1210_001:0042a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮佛出堂場。",
        "from": "T21n1210_001:0042a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "任意讀般若等之經。",
        "from": "T21n1210_001:0042a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降三世忿怒明王念誦儀軌",
        "from": "T21n1210_001:0042a18_100",
        "context_with_next_sentence": false
    }
]