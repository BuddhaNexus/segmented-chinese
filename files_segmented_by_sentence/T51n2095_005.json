[
    {
        "source_sentence": "廬山記卷第五",
        "from": "T51n2095_005:1048a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "7 古碑目古碑目第七",
        "from": "T51n2095_005:1048a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "序廬山自晉宋齊梁陳隋唐。至本朝。幾八百年。",
        "from": "T51n2095_005:1048a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其間廢興盛衰皆有記述。",
        "from": "T51n2095_005:1048a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歲月浸久往往亡失。",
        "from": "T51n2095_005:1048a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若殷仲堪作遠公碑。",
        "from": "T51n2095_005:1048a07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天祐間猶見於貫休之詩。",
        "from": "T51n2095_005:1048a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今無復遺漫矣。",
        "from": "T51n2095_005:1048a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今錄太平東林已下五寺觀五代以前人所作碑誌爵里歲月之日。",
        "from": "T51n2095_005:1048a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡四十一。",
        "from": "T51n2095_005:1048a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辭多不載。",
        "from": "T51n2095_005:1048a10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "太平觀太平觀使者靈廟碑使者靈廟碑荊州黃梅縣東山衡門之下草茅臣李泚撰。",
        "from": "T51n2095_005:1048a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開元二十年歲次壬申正月乙巳朔二十五日己巳樹。",
        "from": "T51n2095_005:1048a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使者靈驗記使者靈驗記宣義郎行彭澤縣尉潘觀篹。",
        "from": "T51n2095_005:1048a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開元二十年壬申三月八日辛亥立。",
        "from": "T51n2095_005:1048a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "治平三年重立。",
        "from": "T51n2095_005:1048a18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "張靈官記張靈官記御史大夫徐鉉撰　右內史舍人集賢殿學士徐鍇書并篆額。",
        "from": "T51n2095_005:1048a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歲次癸酉上元日。",
        "from": "T51n2095_005:1048a21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "胡則傳胡則傳太常丞集賢校理通判江州軍州事章岷撰并書載。",
        "from": "T51n2095_005:1048a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開寶八年胡則守江州事。",
        "from": "T51n2095_005:1048a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "太一觀太一觀真人廟記真人廟記昇元六年歲在壬寅七月六日。",
        "from": "T51n2095_005:1048a27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應運匡國佐聖功臣寧國軍節度宣州營田觀察。",
        "from": "T51n2095_005:1048a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處置等使特進檢校大尉兼中書令持節宣州諸軍事宣州刺史上柱國食邑一萬戶。",
        "from": "T51n2095_005:1048b01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "韓王知證記登仕郎守宣州司戶參軍掌表奏試祕書省校書郎賜緋魚袋孟拱辰書并額。",
        "from": "T51n2095_005:1048b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又一本。",
        "from": "T51n2095_005:1048b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "保大十二年歲在乙亥十一月。",
        "from": "T51n2095_005:1048b06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南岳朱陵道士倪少通撰　道士鍾德載書篆。",
        "from": "T51n2095_005:1048b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東林寺東林寺慧遠法師碑銘慧遠法師碑銘謝靈運撰張野序無立名年月宋佛馱跋陀羅禪師碑宋佛馱跋陀羅禪師碑唐開元十七年歲在己巳七月己丑朔十五日癸卯建兀兀禪師兀兀禪師中書舍人趙郡李訥文　金紫光祿大夫行同州刺史上柱國范陽縣開國男張廷珪書江州刺史河東柳貞望樹唐永泰丙午歲顏真卿題碑側凡一百一十六字東林寺碑并序東林寺碑并序前陳州刺史江夏李邕撰并書。",
        "from": "T51n2095_005:1048b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開元十九年七月十五日建　洪州刺史裴休題云。",
        "from": "T51n2095_005:1048b22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覽北海之詞翰想風采。",
        "from": "T51n2095_005:1048b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "碑陰記碑陰記會昌三年。",
        "from": "T51n2095_005:1048b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "江州刺史張又新撰。",
        "from": "T51n2095_005:1048b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大中十年四月三十日。",
        "from": "T51n2095_005:1048b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "裴光遠篆額。",
        "from": "T51n2095_005:1048b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東林寺遠法師影堂碑東林寺遠法師影堂碑并序隴西李演述　江州錄事參軍王遹篆額。",
        "from": "T51n2095_005:1048b28_16",
        "context_with_next_sentence": true
    },
    {
