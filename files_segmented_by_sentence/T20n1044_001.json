[
    {
        "source_sentence": "佛說六字呪王經",
        "from": "T20n1044_001:0038a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "失譯人名今附東晉錄",
        "from": "T20n1044_001:0038a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。一時佛在舍衛國祇陀林中。爾時",
        "from": "T20n1044_001:0038a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有一外道栴陀羅女。",
        "from": "T20n1044_001:0038a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "厭惑尊者阿難即時如來見阿難恍惚。",
        "from": "T20n1044_001:0038a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為說六字呪王經。",
        "from": "T20n1044_001:0038a29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先佛所說我今亦說。",
        "from": "T20n1044_001:0038b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T20n1044_001:0038b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安陀隸　般陀隸　迦羅知　趐由隸　帝闍婆帝　頻頭婆帝　陀頭隸　陀帝隸　陀究摩帝　修摩帝　安陀邏般陀邏　檀陀羅　提兜羅　阿那延陀慢陀婆帝　阿那阿那夜　摩頭摩婆帝迦羅吒　趐由邏　浮登伽彌帝　闍婆帝　頻頭摩帝　阿羅婆　伽婆帝毘吒毘提膩　至吒毘提膩　悉波呵若有為某甲作惡呪詛。",
        "from": "T20n1044_001:0038b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作已若當作。",
        "from": "T20n1044_001:0038b09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若天若龍。",
        "from": "T20n1044_001:0038b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若夜叉若羅剎。",
        "from": "T20n1044_001:0038b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若餓鬼若鳩槃荼。",
        "from": "T20n1044_001:0038b10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若富多那若毘舍闍。",
        "from": "T20n1044_001:0038b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若阿婆摩羅若吉遮。",
        "from": "T20n1044_001:0038b11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若佉歐陀若毘陀羅。",
        "from": "T20n1044_001:0038b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若半毘陀羅若半毘陀羅女。",
        "from": "T20n1044_001:0038b12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若沙門若婆羅門。",
        "from": "T20n1044_001:0038b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若剎利。",
        "from": "T20n1044_001:0038b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若毘舍若首陀。",
        "from": "T20n1044_001:0038b13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若摩登伽若摩登伽女。",
        "from": "T20n1044_001:0038b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若栴陀羅若栴陀羅女。",
        "from": "T20n1044_001:0038b14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若奴若婢。",
        "from": "T20n1044_001:0038b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若男若女。",
        "from": "T20n1044_001:0038b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若外道出家男女。",
        "from": "T20n1044_001:0038b15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等種種悉能呪詛厭蠱。",
        "from": "T20n1044_001:0038b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今以此六字呪王經。",
        "from": "T20n1044_001:0038b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若上向作上向滅之。",
        "from": "T20n1044_001:0038b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若下向作下向滅之。",
        "from": "T20n1044_001:0038b17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若地上作地上滅之。",
        "from": "T20n1044_001:0038b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若壁著作壁著滅之。",
        "from": "T20n1044_001:0038b18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若柱邊作柱邊滅之。",
        "from": "T20n1044_001:0038b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若城內作城內滅之。",
        "from": "T20n1044_001:0038b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若城門中作城門中滅之。",
        "from": "T20n1044_001:0038b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若宮內作宮內滅之。",
        "from": "T20n1044_001:0038b20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若宮門中作宮門中滅之。",
        "from": "T20n1044_001:0038b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若道中作道中滅之。",
        "from": "T20n1044_001:0038b21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若四徼道中作四徼道中滅之。",
        "from": "T20n1044_001:0038b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若婆羅門作悉皆滅之。",
        "from": "T20n1044_001:0038b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若水中作水中滅之。",
        "from": "T20n1044_001:0038b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若水邊作水邊滅之。",
        "from": "T20n1044_001:0038b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若河邊作河邊滅之。",
        "from": "T20n1044_001:0038b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若家中作家中滅之。",
        "from": "T20n1044_001:0038b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若樹邊作樹邊滅之。",
        "from": "T20n1044_001:0038b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若樹根下作樹根下滅之。",
        "from": "T20n1044_001:0038b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若火中作火中滅之。",
        "from": "T20n1044_001:0038b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若火爐中作火爐中滅之。",
        "from": "T20n1044_001:0038b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若井竈邊作井竈邊滅之。",
        "from": "T20n1044_001:0038b27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若碓磨邊作碓磨邊滅之。",
        "from": "T20n1044_001:0038b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若塚邊作塚邊滅之。",
        "from": "T20n1044_001:0038b28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若床敷間作床敷間滅之。",
        "from": "T20n1044_001:0038b29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若衣被服飾中作悉皆滅之。",
        "from": "T20n1044_001:0038c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若夜作夜滅之。",
        "from": "T20n1044_001:0038c01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若晝作晝滅之。",
        "from": "T20n1044_001:0038c01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若晝夜常作悉皆滅之。",
        "from": "T20n1044_001:0038c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此呪能斷絕帝釋呪道。",
        "from": "T20n1044_001:0038c02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能斷絕梵天呪道。",
        "from": "T20n1044_001:0038c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能斷絕四天呪道。",
        "from": "T20n1044_001:0038c03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及與一切所有邪鬼呪道方術悉皆能斷。",
        "from": "T20n1044_001:0038c04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有為某甲作厭蠱呪詛。",
        "from": "T20n1044_001:0038c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及與毒藥悉皆滅之。",
        "from": "T20n1044_001:0038c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若佉歐陀若毘陀羅。",
        "from": "T20n1044_001:0038c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若吉遮若富多那所為悉皆滅之。",
        "from": "T20n1044_001:0038c06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾惡都盡眾善補處。",
        "from": "T20n1044_001:0038c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願此呪常吉。",
        "from": "T20n1044_001:0038c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此真實章句。",
        "from": "T20n1044_001:0038c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使某甲晝安夜安晝夜常安得壽百歲得見百秋。",
        "from": "T20n1044_001:0038c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故世間最上無過於佛。",
        "from": "T20n1044_001:0038c09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人天所敬眾鬼敬奉。",
        "from": "T20n1044_001:0038c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有行惡呪道。",
        "from": "T20n1044_001:0038c10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作已若當作若中間作。",
        "from": "T20n1044_001:0038c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若成就若未成就悉皆滅之。",
        "from": "T20n1044_001:0038c11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及一切眾毒能害人者。",
        "from": "T20n1044_001:0038c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此六字呪王經皆消滅之。",
        "from": "T20n1044_001:0038c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T20n1044_001:0038c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佉智　佉注　佉毘智　緘壽緘壽　多智　婆智若為某甲作惡呪詛。",
        "from": "T20n1044_001:0038c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作已若當作。",
        "from": "T20n1044_001:0038c16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若天若龍。",
        "from": "T20n1044_001:0038c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若夜叉若羅剎。",
        "from": "T20n1044_001:0038c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及與一切能作惡呪詛者悉皆滅之。",
        "from": "T20n1044_001:0038c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若佉歐陀羅若毘陀羅。",
        "from": "T20n1044_001:0038c18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若富多那所為悉皆滅之。",
        "from": "T20n1044_001:0038c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾惡都盡眾善補處。",
        "from": "T20n1044_001:0038c19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願此呪常吉。",
        "from": "T20n1044_001:0038c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此真實章句使某甲晝安夜安晝夜常安得壽百歲得見百秋。",
        "from": "T20n1044_001:0038c21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故世間上法無過佛法。",
        "from": "T20n1044_001:0038c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人天所敬眾鬼奉受離欲無著。",
        "from": "T20n1044_001:0038c22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T20n1044_001:0038c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佉智　佉注　佉毘智　緘壽緘壽　多智　婆智何以故僧是世間最上福田。",
        "from": "T20n1044_001:0038c26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人天所敬眾鬼守護。",
        "from": "T20n1044_001:0038c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T20n1044_001:0038c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佉智　佉注　佉毘智　緘壽緘壽　多智　婆智　興帝　阿禰帝　阿周帝若有為某甲作惡呪詛。",
        "from": "T20n1044_001:0039a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若和合毒藥皆悉滅之。",
        "from": "T20n1044_001:0039a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T20n1044_001:0039a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩休婆　烏摩帝　烏摩陀　蓰陀禰　佉歐陀　波提蓰陀禰　頻頭摩提　至帝至多　波提蓰陀禰　阿渒栴陀渒　呵呵遮利　呵呵那彌　呵呵浮摩提　呵呵浮陀尼劬沙　呵呵尼呵陀　佉歐陀　尼毘沙　佉歐陀　伊禰彌禰　陀陀弊莎訶若有人讀持此六字大呪王經。",
        "from": "T20n1044_001:0039a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "假使呪枯樹可得還生枝葉。",
        "from": "T20n1044_001:0039a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況人身。",
        "from": "T20n1044_001:0039a10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使某甲得壽百歲得見百秋。",
        "from": "T20n1044_001:0039a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛所說阿難所傳。",
        "from": "T20n1044_001:0039a11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人讀誦通利。",
        "from": "T20n1044_001:0039a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆自護眾惡不著身。",
        "from": "T20n1044_001:0039a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若呪他者能除彼患。",
        "from": "T20n1044_001:0039a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦者斷五辛至心鮮潔。",
        "from": "T20n1044_001:0039a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後乃能行之南無觀世音菩薩摩訶薩。",
        "from": "T20n1044_001:0039a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮彼大士然後說此神呪。",
        "from": "T20n1044_001:0039a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願此神呪常吉。",
        "from": "T20n1044_001:0039a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀世音照我身觀我身憐愍我故。",
        "from": "T20n1044_001:0039a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T20n1044_001:0039a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烏呵尼　須呵尼　阿陀尼呵陀闍毘尼　耽毘尼　安陀隸　般陀隸　斯畀提　般陀邏　波斯尼　薩陸頭率　吒婆囉豆率　吒質耽　闍婆夜　耽呵夜　須呵夜　尼婆羅夜　耶婆某羅提　那某遮提　盧婁至吒　般陀死　盧婁盧　漏至吒　般陀死　悉波呵　和𨢘利知　利收利　摩登耆　栴陀利羅叉羅叉羅耶　悉波呵此呪護一切厄。",
        "from": "T20n1044_001:0039a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鮮淨無垢解脫。",
        "from": "T20n1044_001:0039a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光明觀照縛一切賊。",
        "from": "T20n1044_001:0039a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迷荒一切惡護身縛賊。",
        "from": "T20n1044_001:0039a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虎狼師子狂象及犲悉皆被縛。",
        "from": "T20n1044_001:0039a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我某甲及共行同伴。",
        "from": "T20n1044_001:0039a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切呪力所向之處願當平吉。",
        "from": "T20n1044_001:0039a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無觀世音菩薩。",
        "from": "T20n1044_001:0039a28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願此呪大吉。",
        "from": "T20n1044_001:0039a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用此呪法當持之日。",
        "from": "T20n1044_001:0039a29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不食五辛淨潔洗浴。",
        "from": "T20n1044_001:0039b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得行婬不得飲酒噉肉。",
        "from": "T20n1044_001:0039b01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以白疊綖手捉誦呪七遍一遍一結。",
        "from": "T20n1044_001:0039b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有官事被言。",
        "from": "T20n1044_001:0039b02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若鬪諍若呪詛。",
        "from": "T20n1044_001:0039b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及一切惡皆能滅之。",
        "from": "T20n1044_001:0039b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪訖此綖繫著其人衣帶頭過事主自解之。",
        "from": "T20n1044_001:0039b04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無過去未來現在三世諸佛。",
        "from": "T20n1044_001:0039b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無文殊師利普賢彌勒一切諸大菩薩。",
        "from": "T20n1044_001:0039b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸依如是等菩薩摩訶薩眾。",
        "from": "T20n1044_001:0039b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後說此大神陀羅尼呪。",
        "from": "T20n1044_001:0039b07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T20n1044_001:0039b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "𨢘豆摩帝　耶舍婆帝　婆羅沙天婆帝　烏受婆帝　欝多羅尼　阿叉夜婆帝　阿㝹羶帝目　闍婆羅婆目帝　波羅婆只豆悉波帝迦羅尼伽帝　波毘耶舍首羅婆隸陀提㝹提　散陀波提欝多羅尼　舍陀目佉　舍陀波羅毘舍　羶陀迦梨舍　婆薩那婆伽帝　究舍羅婆娑尼　阿㝹散帝　毘散帝　知帝阿知帝　湯那伽帝　阿湯那伽帝　阿婆娑帝　呵舍波羅禰　婆羅禰目隸　牟羅牟羅婆陀禰　舍尼波羅陸　須利婆帝　殊帝波羅陸　須毘摩隸　菴摩隸此呪除一切闇。",
        "from": "T20n1044_001:0039b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來所持諸天敬愛。",
        "from": "T20n1044_001:0039b20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾神擁護梵天所解。",
        "from": "T20n1044_001:0039b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝釋敬奉護世守護。",
        "from": "T20n1044_001:0039b21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "決了眾聖諸仙受持。",
        "from": "T20n1044_001:0039b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和合眾性解脫諸縛。",
        "from": "T20n1044_001:0039b22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸天及人。",
        "from": "T20n1044_001:0039b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷諸煩惱降諸魔怨。",
        "from": "T20n1044_001:0039b23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏諸外道摧伏眾論。",
        "from": "T20n1044_001:0039b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸憍慢過諸法師。",
        "from": "T20n1044_001:0039b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不捨大眾悅諸學法。",
        "from": "T20n1044_001:0039b24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "守護法藏利益三寶。",
        "from": "T20n1044_001:0039b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈哀一切眾生之類。",
        "from": "T20n1044_001:0039b25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莊嚴眾義有如是利。",
        "from": "T20n1044_001:0039b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名無盡藏陀羅尼非護能護。",
        "from": "T20n1044_001:0039b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有讀誦者。",
        "from": "T20n1044_001:0039b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得三十三無隸畏難。",
        "from": "T20n1044_001:0039b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用此呪法晨起澡手漱口。",
        "from": "T20n1044_001:0039b28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒香禮拜三遍。",
        "from": "T20n1044_001:0039b28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讀誦所行之處言語談說。",
        "from": "T20n1044_001:0039b29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切向處無常不勝眾人。",
        "from": "T20n1044_001:0039c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貴敬見者歡喜怨家降伏。",
        "from": "T20n1044_001:0039c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有如是利益。",
        "from": "T20n1044_001:0039c01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說六字呪王經",
        "from": "T20n1044_001:0039c02_100",
        "context_with_next_sentence": false
    }
]