[
    {
        "source_sentence": "佛說洛叉陀羅尼經",
        "from": "T21n1390_001:0907b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試光祿卿",
        "from": "T21n1390_001:0907b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明教大師臣法賢奉　詔譯",
        "from": "T21n1390_001:0907b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告阿難言。",
        "from": "T21n1390_001:0907b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當諦聽今我與汝。",
        "from": "T21n1390_001:0907b23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宣說洛叉陀羅尼。",
        "from": "T21n1390_001:0907b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是陀羅尼難得值遇。",
        "from": "T21n1390_001:0907b24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由如諸佛。",
        "from": "T21n1390_001:0907b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出於世間。",
        "from": "T21n1390_001:0907b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難若有眾生。",
        "from": "T21n1390_001:0907b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得此陀羅尼能受持者。",
        "from": "T21n1390_001:0907b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人所獲功德如持洛叉佛無異。",
        "from": "T21n1390_001:0907b26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能與眾生成大福聚。",
        "from": "T21n1390_001:0907b27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能滅眾生無量重罪。",
        "from": "T21n1390_001:0907b27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊即說洛叉陀羅尼曰。",
        "from": "T21n1390_001:0907b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那莫三滿多沒馱引曩引一唵引那謨婆誐嚩帝引二尾布羅左那曩剛引左努得叱二合鉢多二合三鉢囉二合婆引薩計覩母哩儞二合四怛他引誐多引野五阿囉曷引帝引三藐訖三二合沒馱引野六怛𭔞他引七冒地冒地八薩哩嚩二合怛他引誐多虞引左哩引九達囉達囉十曷囉曷囉十一鉢囉二合曷囉鉢囉二合曷囉十二摩賀引冒地唧多達哩引十三祖魯祖魯十四母魯母魯十五設多薩賀薩囉二合十六囉濕弭二合散祖引儞帝引十七薩哩嚩二合怛他引誐多婆引始帝十八虞尼引虞拏嚩底十九沒馱虞拏引嚩婆引細二十蜜里蜜里二十一薩哩嚩二合怛他引誐多阿地瑟致二合帝引二十二那婆薩多二合隷二十三設摩設摩二十四鉢囉二合設摩鉢囉二合設摩二十五薩哩嚩二合播波鉢囉二合設摩儞引二十六薩哩嚩二合播波尾輸達儞引二十七虎魯虎魯二十八冒地摩引勒誐二合三鉢囉二合悉體二合帝引二十九薩哩嚩二合怛他引誐多鉢囉二合底瑟致二合帝引三十尾󰡊提引娑嚩二合引賀引三十一唵引薩哩嚩二合怛他引誐多三十二咩嚩魯引吉帝引三十三惹野惹野娑嚩二合引賀引三十四唵引虎嚕虎嚕三十五惹野目契引娑嚩二合引賀引三十六爾時阿難聞佛說是陀羅尼已。",
        "from": "T21n1390_001:0907c22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡喜信受作禮而退。",
        "from": "T21n1390_001:0907c23_3",
        "context_with_next_sentence": true
    },
