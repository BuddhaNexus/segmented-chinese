[
    {
        "source_sentence": "佛說摩利支天經",
        "from": "T21n1255b_001:0260b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐三藏沙門大廣智",
        "from": "T21n1255b_001:0260b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空奉　詔譯",
        "from": "T21n1255b_001:0260b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T21n1255b_001:0260b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時薄伽梵。",
        "from": "T21n1255b_001:0260b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在室羅筏城逝多林給孤獨園。",
        "from": "T21n1255b_001:0260b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告諸苾芻有天女名摩利支。",
        "from": "T21n1255b_001:0260b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大神通自在之力。",
        "from": "T21n1255b_001:0260b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常行日月天前。",
        "from": "T21n1255b_001:0260b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日天月天不能見彼。",
        "from": "T21n1255b_001:0260b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼能見日。",
        "from": "T21n1255b_001:0260b09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能見無人能知。",
        "from": "T21n1255b_001:0260b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能捉無人能縛。",
        "from": "T21n1255b_001:0260b10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能害無人能欺誑。",
        "from": "T21n1255b_001:0260b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能債其財物。",
        "from": "T21n1255b_001:0260b11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能責罰。",
        "from": "T21n1255b_001:0260b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為怨家能得其便。",
        "from": "T21n1255b_001:0260b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告諸苾芻。",
        "from": "T21n1255b_001:0260b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有知彼摩利支天名常憶念者。",
        "from": "T21n1255b_001:0260b13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼人亦不可見亦不可知。",
        "from": "T21n1255b_001:0260b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不可捉亦不可縛。",
        "from": "T21n1255b_001:0260b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不可害亦不可欺誑。",
        "from": "T21n1255b_001:0260b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不為人債其財物。",
        "from": "T21n1255b_001:0260b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不為人之所責罰。",
        "from": "T21n1255b_001:0260b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不為怨家能得其便。",
        "from": "T21n1255b_001:0260b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人。",
        "from": "T21n1255b_001:0260b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知彼摩利支天名求加護者。",
        "from": "T21n1255b_001:0260b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應作是言。",
        "from": "T21n1255b_001:0260b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我某甲知摩利支天母有大神力。",
        "from": "T21n1255b_001:0260b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今歸命願護我身。",
        "from": "T21n1255b_001:0260b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能見我。",
        "from": "T21n1255b_001:0260b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能知我。",
        "from": "T21n1255b_001:0260b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能捉我。",
        "from": "T21n1255b_001:0260b20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能縛我。",
        "from": "T21n1255b_001:0260b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能害我。",
        "from": "T21n1255b_001:0260b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能欺誑我。",
        "from": "T21n1255b_001:0260b21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能債我財物。",
        "from": "T21n1255b_001:0260b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能責罰我。",
        "from": "T21n1255b_001:0260b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不為怨家能得其便。",
        "from": "T21n1255b_001:0260b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊說陀羅尼曰。",
        "from": "T21n1255b_001:0260b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨引囉怛曩二合怛囉二合夜野一怛儞也二合他去聲引二遏迦麼枲鼻聲已下同三沫迦麼枲四阿上聲度引麼枲五紫精以切引鉢囉麼枲六摩訶紫鉢囉麼枲七頞怛馱引曩麼枲八麼鼻聲引哩引紫野麼枲九曩謨引娑覩二合帝十囉轉舌乞灑二合囉乞灑二合𤚥引十一薩嚩薩怛嚩二合引難上聲左十二薩嚩怛囉二合十三薩嚩婆去聲喻引鉢捺囉二合吠引毘藥二合娑嚩二合引賀引十四心真言曰。",
        "from": "T21n1255b_001:0260c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜莫三去聲滿多沒馱引南一唵引二摩鼻聲引哩引唧引娑嚩二合賀引三爾時世尊說此陀羅尼已。",
        "from": "T21n1255b_001:0260c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告諸苾芻。",
        "from": "T21n1255b_001:0260c07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有受持此經法者。",
        "from": "T21n1255b_001:0260c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應作是願。",
        "from": "T21n1255b_001:0260c08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王難中護我賊難中護我。",
        "from": "T21n1255b_001:0260c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行路難中護我於失道曠野中護我。",
        "from": "T21n1255b_001:0260c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水火難中護我刀兵軍陣難中護我。",
        "from": "T21n1255b_001:0260c10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼神難中護我毒藥難中護我。",
        "from": "T21n1255b_001:0260c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡獸難中護我毒蟲難中護我。",
        "from": "T21n1255b_001:0260c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切怨家惡人難中護我。",
        "from": "T21n1255b_001:0260c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛實語護我法實語護我僧實語護我。",
        "from": "T21n1255b_001:0260c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天實語護我仙人實語護我。",
        "from": "T21n1255b_001:0260c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切處一切時。",
        "from": "T21n1255b_001:0260c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願常護我弟子某甲莎嚩二合引賀引。",
        "from": "T21n1255b_001:0260c15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告諸苾芻若善男子善女人苾芻苾芻尼。",
        "from": "T21n1255b_001:0260c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鄔波索迦鄔波斯迦。",
        "from": "T21n1255b_001:0260c17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國王大臣一切人等有諸難時。",
        "from": "T21n1255b_001:0260c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但當至心誦此摩利支陀羅尼。",
        "from": "T21n1255b_001:0260c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不待加功隨誦隨成。",
        "from": "T21n1255b_001:0260c19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離諸難除不至心。",
        "from": "T21n1255b_001:0260c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持誦之時并結本印。",
        "from": "T21n1255b_001:0260c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以香塗手先結三部心印。",
        "from": "T21n1255b_001:0260c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉為拳並竪二大拇指。",
        "from": "T21n1255b_001:0260c21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是一切如來心印真言曰。",
        "from": "T21n1255b_001:0260c22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一爾惹以切曩爾迦半音二次結蓮部心印。",
        "from": "T21n1255b_001:0260c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前印以左大拇指屈入掌。",
        "from": "T21n1255b_001:0260c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直竪右大拇指真言曰。",
        "from": "T21n1255b_001:0260c25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一阿去聲引嚕引力迦半音二次結金剛部心印。",
        "from": "T21n1255b_001:0260c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前印以右大拇指屈入掌直竪左大拇指真言曰。",
        "from": "T21n1255b_001:0260c28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一嚩日囉二合地力二合迦半音二次結護身如來拳印。",
        "from": "T21n1255b_001:0261a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手屈大拇指橫於掌中。",
        "from": "T21n1255b_001:0261a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便以四指握大拇指為拳。",
        "from": "T21n1255b_001:0261a02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此拳印加持自身五處。",
        "from": "T21n1255b_001:0261a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先額右肩左肩心喉每處各誦真言一遍真言曰。",
        "from": "T21n1255b_001:0261a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一僕引入嚩二合羅吽引二次結摩利支菩薩根本印。",
        "from": "T21n1255b_001:0261a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二小指二無名指。",
        "from": "T21n1255b_001:0261a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右押左內相叉。",
        "from": "T21n1255b_001:0261a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直竪二頭指相捻。",
        "from": "T21n1255b_001:0261a07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二中指各繳頭指背。",
        "from": "T21n1255b_001:0261a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向前頭相拄。",
        "from": "T21n1255b_001:0261a08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指並竪即成。",
        "from": "T21n1255b_001:0261a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結印當心。",
        "from": "T21n1255b_001:0261a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦前摩利支身陀羅尼及心各七遍。",
        "from": "T21n1255b_001:0261a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每遍屈二大拇指招之。",
        "from": "T21n1255b_001:0261a10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名迎請印。",
        "from": "T21n1255b_001:0261a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼以此印加持身五處。",
        "from": "T21n1255b_001:0261a11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂上散印。",
        "from": "T21n1255b_001:0261a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結大三昧耶印。",
        "from": "T21n1255b_001:0261a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辟除結界。",
        "from": "T21n1255b_001:0261a13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手大指捻小指甲上。",
        "from": "T21n1255b_001:0261a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘三指直竪如三股杵形。",
        "from": "T21n1255b_001:0261a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手作金剛拳按於心上。",
        "from": "T21n1255b_001:0261a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨誦真言以右手印於頂上。",
        "from": "T21n1255b_001:0261a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左轉三匝辟除一切作障難者。",
        "from": "T21n1255b_001:0261a16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便右旋三匝并揮上下。",
        "from": "T21n1255b_001:0261a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成結十方界。",
        "from": "T21n1255b_001:0261a17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切天龍人非人等不能附近真言曰。",
        "from": "T21n1255b_001:0261a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一商迦㘑輕舌二摩訶引三去聲麼鼻聲琰娑嚩賀次結摩利支安怛袒那印此言隱形以左手虛掌作拳。",
        "from": "T21n1255b_001:0261a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指微捻頭指甲如環。",
        "from": "T21n1255b_001:0261a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已下三指握拳令密。",
        "from": "T21n1255b_001:0261a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又令掌中作孔。",
        "from": "T21n1255b_001:0261a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安自心前。",
        "from": "T21n1255b_001:0261a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想自身入此印孔中藏。",
        "from": "T21n1255b_001:0261a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手平掌。",
        "from": "T21n1255b_001:0261a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋摩此印便蓋孔上。",
        "from": "T21n1255b_001:0261a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想此印即是摩利支天菩薩身。",
        "from": "T21n1255b_001:0261a25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我自身隱藏於摩利支天菩薩心中。",
        "from": "T21n1255b_001:0261a26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心專注不間斷。",
        "from": "T21n1255b_001:0261a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦前根本及心真言。",
        "from": "T21n1255b_001:0261a27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不限遍數但虔誠至心。",
        "from": "T21n1255b_001:0261a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必獲菩薩威神加護。",
        "from": "T21n1255b_001:0261a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切怨家惡人悉不能見。",
        "from": "T21n1255b_001:0261a29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切災難皆得解脫。",
        "from": "T21n1255b_001:0261a29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲供養摩利支菩薩者。",
        "from": "T21n1255b_001:0261b01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應用金或銀或赤銅。",
        "from": "T21n1255b_001:0261b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或白檀香木或紫檀木等。",
        "from": "T21n1255b_001:0261b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刻作摩利支菩薩像。",
        "from": "T21n1255b_001:0261b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如天女形可長半寸。",
        "from": "T21n1255b_001:0261b03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或一寸二寸已下。",
        "from": "T21n1255b_001:0261b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於蓮花上或立或坐。",
        "from": "T21n1255b_001:0261b04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭冠瓔珞種種莊嚴極令端正。",
        "from": "T21n1255b_001:0261b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手把天扇。",
        "from": "T21n1255b_001:0261b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其扇如維摩詰前天女扇。",
        "from": "T21n1255b_001:0261b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手垂下揚掌向外。",
        "from": "T21n1255b_001:0261b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "展五指作與願勢。",
        "from": "T21n1255b_001:0261b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有二天女各執白拂侍立左右。",
        "from": "T21n1255b_001:0261b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作此像成。",
        "from": "T21n1255b_001:0261b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戴於頂上或戴臂上或置衣中。",
        "from": "T21n1255b_001:0261b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以菩薩威神之力不逢災難。",
        "from": "T21n1255b_001:0261b09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於怨家處決定得勝。",
        "from": "T21n1255b_001:0261b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼神惡人無得便。",
        "from": "T21n1255b_001:0261b10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲成驗願見摩利支天真身求勝願者。",
        "from": "T21n1255b_001:0261b11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此陀羅尼滿十萬遍。",
        "from": "T21n1255b_001:0261b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依法建立曼荼羅。",
        "from": "T21n1255b_001:0261b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫摩利支菩薩像。",
        "from": "T21n1255b_001:0261b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安置壇中種種供養。",
        "from": "T21n1255b_001:0261b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并作護摩火壇。",
        "from": "T21n1255b_001:0261b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩利支天女必現其身。",
        "from": "T21n1255b_001:0261b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求勝願決定成就除不至心。",
        "from": "T21n1255b_001:0261b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告諸苾芻。",
        "from": "T21n1255b_001:0261b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為當來惡世苦難恐怖有情。",
        "from": "T21n1255b_001:0261b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略說摩利支天法。",
        "from": "T21n1255b_001:0261b16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此菩薩有大悲願。",
        "from": "T21n1255b_001:0261b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常於苦難恐怖之處。",
        "from": "T21n1255b_001:0261b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護諸有情不令天龍鬼神人及非人怨家惡獸所能為害。",
        "from": "T21n1255b_001:0261b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當受持廣宣流布饒益。",
        "from": "T21n1255b_001:0261b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有情諸苾芻等及天龍。",
        "from": "T21n1255b_001:0261b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八部一切大眾聞佛所說。",
        "from": "T21n1255b_001:0261b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆大歡喜信受奉行。",
        "from": "T21n1255b_001:0261b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說摩利支天經",
        "from": "T21n1255b_001:0261b22_100",
        "context_with_next_sentence": false
    }
]