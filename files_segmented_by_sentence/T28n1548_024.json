    "T28n1548_024:0679a06_2": "除餘法。",
    "T28n1548_024:0679a06_3": "除何等法。",
    "T28n1548_024:0679a07_4": "除非空定相應法。",
    "T28n1548_024:0679a07_5": "空定謂無緣法身分。",
    "T28n1548_024:0679a07_6": "意識非聖分。",
    "T28n1548_024:0679a08_7": "無相無願相應分。",
    "T28n1548_024:0679a08_8": "無相定幾法相應。",
    "T28n1548_024:0679a09_9": "問何等法。",
    "T28n1548_024:0679a09_10": "問無相定相應法。",
    "T28n1548_024:0679a09_11": "除餘法除何等法。",
    "T28n1548_024:0679a10_12": "除非無相定相應法。",
    "T28n1548_024:0679a10_13": "無相定。",
    "T28n1548_024:0679a10_14": "謂無緣法身分。",
    "T28n1548_024:0679a11_15": "意識非聖分。",
    "T28n1548_024:0679a11_16": "聖空無願相應分。",
    "T28n1548_024:0679a11_17": "無願定幾法相應。",
    "T28n1548_024:0679a12_18": "問何等法。",
    "T28n1548_024:0679a12_19": "問無願定相應法。",
    "T28n1548_024:0679a12_20": "除餘法。",
    "T28n1548_024:0679a13_21": "除何等法。",
    "T28n1548_024:0679a13_22": "除非無願定相應法。",
    "T28n1548_024:0679a13_23": "無願定謂無緣法身分。",
    "T28n1548_024:0679a14_24": "意識非聖分。",
    "T28n1548_024:0679a14_25": "聖空無相相應分。",
    "T28n1548_024:0679a15_26": "信根幾法相應。",
    "T28n1548_024:0679a15_27": "問何等法。",
    "T28n1548_024:0679a15_28": "問信根相應法。",
    "T28n1548_024:0679a16_0": "除餘法。",
    "T28n1548_024:0679a16_1": "除何等法。",
    "T28n1548_024:0679a16_2": "除非信根相應法。",
    "T28n1548_024:0679a16_3": "信根。",
    "T28n1548_024:0679a16_4": "謂無緣法身分。",
    "T28n1548_024:0679a17_5": "意識非聖分。",
    "T28n1548_024:0679a17_6": "進根念根定根慧根亦如是。",
    "T28n1548_024:0679a19_0": "舍利弗阿毘曇論卷第二十四"
}