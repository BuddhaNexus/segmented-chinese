[
    {
        "source_sentence": "佛說無能勝大明王陀羅尼經",
        "from": "T21n1233_001:0171b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天中印度摩伽陀國那爛陀寺",
        "from": "T21n1233_001:0171b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏傳教大師賜紫沙門臣法天",
        "from": "T21n1233_001:0171b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉　詔譯爾時世尊。",
        "from": "T21n1233_001:0171b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告舍利子阿難陀等。",
        "from": "T21n1233_001:0171b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今宣說無能勝大明王陀羅尼。",
        "from": "T21n1233_001:0171b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為彼人天作利樂故。",
        "from": "T21n1233_001:0171b09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有吠多拏及步多等。",
        "from": "T21n1233_001:0171b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此大明王起根本業作諸障難。",
        "from": "T21n1233_001:0171b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至藥叉羅剎娑。",
        "from": "T21n1233_001:0171b11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起暴惡業欲破此明王者無能破壞。",
        "from": "T21n1233_001:0171b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1233_001:0171b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞野二合他引一賀娑儞引二尾賀娑儞引三護魯祖魯魯四娑嚩二合引賀引五若有聰慧之人。",
        "from": "T21n1233_001:0171b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此明王心不疑慮尊重受持者。",
        "from": "T21n1233_001:0171b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼人於囉惹門或在師子中。",
        "from": "T21n1233_001:0171b16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在道路時或非道路。",
        "from": "T21n1233_001:0171b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心常憶念。",
        "from": "T21n1233_001:0171b17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至魔王處及遇彼冤家。",
        "from": "T21n1233_001:0171b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此明王常自擁護無諸恐怖。",
        "from": "T21n1233_001:0171b18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1233_001:0171b18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞野二合他引一度弭引二度度弭引三度麼引嚩諦四娑嚩二合引賀引五若有女人。",
        "from": "T21n1233_001:0171b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "專注受持。",
        "from": "T21n1233_001:0171b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復能書寫戴於頂上者。",
        "from": "T21n1233_001:0171b21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼人所有財物。",
        "from": "T21n1233_001:0171b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒常豐盈無有耗散。",
        "from": "T21n1233_001:0171b22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眷屬皆得安樂。",
        "from": "T21n1233_001:0171b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1233_001:0171b23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞野二合他引一惹嚩惹嚩二惹嚩儞引三娑嚩二合引賀引四若有女人身懷其孕。",
        "from": "T21n1233_001:0171b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "久久不生受諸苦惱。",
        "from": "T21n1233_001:0171b26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持此明皆得易生。",
        "from": "T21n1233_001:0171b27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1233_001:0171b27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞野二合他引一迦捨迦捨二尾迦捨三摩賀引迦捨四娑嚩二合引賀引五若或有人於軍陣中互相殺時。",
        "from": "T21n1233_001:0171c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此無能勝而為擁護咸得安住。",
        "from": "T21n1233_001:0171c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1233_001:0171c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞野二合他引一吒吒跓摘故反二致跓准上切三娑嚩二合引賀引四若或有人於迷醉時。",
        "from": "T21n1233_001:0171c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或睡夢中乃至行住之時。",
        "from": "T21n1233_001:0171c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼步多眾無能嬈亂恒得安樂。",
        "from": "T21n1233_001:0171c06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復有人以慇重心恒常受持諷誦宣說此無能勝。",
        "from": "T21n1233_001:0171c07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於晝夜中而為擁護。",
        "from": "T21n1233_001:0171c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心聽聞。",
        "from": "T21n1233_001:0171c08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1233_001:0171c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞野二合他引一賀囉二賀囉囉三娑嚩二合引賀引四我今宣說彼惡魔羅恃其力勢。",
        "from": "T21n1233_001:0171c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現種種色相而來相持。",
        "from": "T21n1233_001:0171c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩見已止住其軍。",
        "from": "T21n1233_001:0171c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同彼兄弟而說此言。",
        "from": "T21n1233_001:0171c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我千俱胝劫為求菩提。",
        "from": "T21n1233_001:0171c13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨自身命及以男女。",
        "from": "T21n1233_001:0171c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至財寶而無悋惜。",
        "from": "T21n1233_001:0171c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發大誓願修此精進難行苦行。",
        "from": "T21n1233_001:0171c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為有情故起利他行。",
        "from": "T21n1233_001:0171c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈心悲愍覬得圓滿。",
        "from": "T21n1233_001:0171c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼惡魔羅見此事已。",
        "from": "T21n1233_001:0171c16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來為我障。",
        "from": "T21n1233_001:0171c16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩是時憶念此無能勝大明王陀羅尼。",
        "from": "T21n1233_001:0171c17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以彼明力令彼魔羅及大軍眾。",
        "from": "T21n1233_001:0171c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆退散。",
        "from": "T21n1233_001:0171c18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1233_001:0171c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞野二合他引一伊底弭底二底羅弭羅三弭底弭隷引儞四娑嚩二合引賀引五於是明王現女人相。",
        "from": "T21n1233_001:0171c22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來菩薩前合掌頂上。",
        "from": "T21n1233_001:0171c22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而說頌曰。",
        "from": "T21n1233_001:0171c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大智慧宣說　　如大光普照",
        "from": "T21n1233_001:0171c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而我於今日　　降伏魔羅軍",
        "from": "T21n1233_001:0171c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如汝過去求　　無上正等覺",
        "from": "T21n1233_001:0171c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃得大無畏　　魔羅軍降伏",
        "from": "T21n1233_001:0171c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今以彼明力。",
        "from": "T21n1233_001:0171c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破壞魔羅及大軍眾并諸眷屬。",
        "from": "T21n1233_001:0171c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1233_001:0171c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞野二合他引一迦嚩置引二迦嚩吒三謨隷引四曩俱羅五俱賀羅六麼引隷引儞七娑嚩二合引賀引八爾時世尊而說頌曰。",
        "from": "T21n1233_001:0172a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來正等覺　　令一切世間",
        "from": "T21n1233_001:0172a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得無上果者　　毒藥不能害",
        "from": "T21n1233_001:0172a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "器杖不能傷　　火亦不能燒",
        "from": "T21n1233_001:0172a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至於眠睡中　　常不見惡夢",
        "from": "T21n1233_001:0172a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若為王繫縛　　及在賊手中",
        "from": "T21n1233_001:0172a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或山崖巇嶮　　乃至於江河",
        "from": "T21n1233_001:0172a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切恐怖難　　悉皆得解脫",
        "from": "T21n1233_001:0172a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾罪及苦惱　　解脫而安隱",
        "from": "T21n1233_001:0172a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明力及慧力　　而為大威德",
        "from": "T21n1233_001:0172a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡所作事業　　能速得成就",
        "from": "T21n1233_001:0172a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1233_001:0172a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞野二合他引一素㘑引二素素㘑引三嚩囉四嚩嚩囉五摩曩婆嚩引六娑摩囉引七護隷引八娑嚩二合引賀引九摩隷引十娑嚩二合引賀引十一佛言我先求菩提時。",
        "from": "T21n1233_001:0172a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作六牙白象時。",
        "from": "T21n1233_001:0172a19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為彼獵師而作遮護。",
        "from": "T21n1233_001:0172a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斯若真實願諸有情。",
        "from": "T21n1233_001:0172a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切苦惱皆得除盡。",
        "from": "T21n1233_001:0172a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我於過去之世為海中龜時。",
        "from": "T21n1233_001:0172a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身備割截心無嗔恨。",
        "from": "T21n1233_001:0172a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斯若真實願諸有情。",
        "from": "T21n1233_001:0172a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有疾病皆悉除滅。",
        "from": "T21n1233_001:0172a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1233_001:0172a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞野二合他引一呬隷弭隸儞二度嚕二合鑁跛能去四娑嚩二合引賀引五佛言我過去世求菩提時。",
        "from": "T21n1233_001:0172a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施於有情種種安樂。",
        "from": "T21n1233_001:0172a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我昔為雉被獵師所獲所要施與。",
        "from": "T21n1233_001:0172a27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起慈心觀不生嗔恨。",
        "from": "T21n1233_001:0172a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1233_001:0172a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞野二合他引一鼻哩二度度度儞三摩賀引曩諦引四娑嚩二合引賀引五度度度儞引六呬致弭致七鼻致哩八娑嚩二合引賀引九佛言我為忍辱仙人時。",
        "from": "T21n1233_001:0172b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修忍辱波羅蜜多觀。",
        "from": "T21n1233_001:0172b03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心不煩惱。",
        "from": "T21n1233_001:0172b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀察一切有情由如赤子。",
        "from": "T21n1233_001:0172b04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我身出血而變成乳。",
        "from": "T21n1233_001:0172b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為求菩提一切施時。",
        "from": "T21n1233_001:0172b05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼婆羅門來於我前。",
        "from": "T21n1233_001:0172b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而乞其頭以頭施之乃以髮索繫縛於樹。",
        "from": "T21n1233_001:0172b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我於是時起慈悲心不生嗔恨。",
        "from": "T21n1233_001:0172b07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀婆羅門由如赤子。",
        "from": "T21n1233_001:0172b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至妻妾皆悉施與我不求五慾樂不求王自在不求天富貴。",
        "from": "T21n1233_001:0172b09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真實為彼降伏魔羅及以眷屬。",
        "from": "T21n1233_001:0172b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至調伏一切冤家得最上位。",
        "from": "T21n1233_001:0172b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為破彼故說大明大力。",
        "from": "T21n1233_001:0172b11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而能出生過去行菩提行。",
        "from": "T21n1233_001:0172b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為降伏魔羅我今復說即說呪曰。",
        "from": "T21n1233_001:0172b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也二合他去聲引一企企𩕳二尾麼那𩕳三尾度度度寧上四麼賀引曩底丁曳反囉拏二合嚩底五麼賀引嚩引囉他二合嚩底六醯醯醯七弭弭禮八始始抳九始抳始抳十悉哩抳十一弭哩抳十二麼賀引弭哩抳十三禮禮𩕳十四弭里抳十五抳娑嚩二合羅跛二合嚩引儞𩕳十六底底𩕳十七蜜底哩抳十八薩轉舌嚩迦引麼引囉他二合娑引上達𩕳十九設咄嚕二合達哩灑二合抳二十設嚩達㘑二十一麼賀引達㘑二十二儞泥翳反跛多二合達㘑二十三乞芻二合嚕引上跛謎二十四娑賀娑囉二合馱引㘑二十五尾麼禮二十六烏仡囉二合底丁曳反乞瑟膩三合二十七麼賀哆閉二十八設咄嚕二合𭊥引佩孕迦引哩二十九布爾仁除反哆上底哩二合數路去引計數三十儞泥逸反底焰二合路去引計數尾戍嚕二合哆上引三十一悉馱引佐底哩二合數三十三路去引計瑟嚩二合𩕳泥逸反爾哆上三十四沒囉二合憾麼二合難拏數二合跛謎三十五悉弟設訖底二合三十六塞建二合馱寫也二合尾戍嚕二合帝三十七我為汝說。",
        "from": "T21n1233_001:0172c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼大明力威德精進。",
        "from": "T21n1233_001:0172c05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天與阿蘇羅兩陣相持時。",
        "from": "T21n1233_001:0172c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天憶念此無能勝彼自降伏。",
        "from": "T21n1233_001:0172c06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝天精進與一切阿蘇羅。",
        "from": "T21n1233_001:0172c07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相持汝天壽命千年。",
        "from": "T21n1233_001:0172c07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伺其方便彼無能得。",
        "from": "T21n1233_001:0172c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無能勝真實力。",
        "from": "T21n1233_001:0172c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就往於梵天。",
        "from": "T21n1233_001:0172c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是恒常作種種事業而無能勝彼梵天威德。",
        "from": "T21n1233_001:0172c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而供養故諸佛所說。",
        "from": "T21n1233_001:0172c10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1233_001:0172c10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尾捨引禮一尾補禮二底丁曳反乞瑟膩三合蘇底准上切乞瑟膩三合三阿仡𩕳二合娑上引儞哩二合勢四阿謨引去契阿鉢囉二合底賀帝五謎佉嚩引喻娑麼引怛麼二合計六麼賀引鉢囉二合陛七尾儞喻二合𩕳陛八尾儞喻二合哆上囉九𩕳陛乞灑二合抳十阿仡𩕳二合鉢囉二合陛十一鉢囉二合入嚩二合栗帝十二嚩引喻𮧬無鉢反怛參二合去鉢囉二合麼囉那二合寧上十三入嚩二合栗哆上十四入嚩二合栗哆嚩引喻𮧬准上切怛參二合去十五鉢囉二合麼囉那二合寧上十六入嚩二合栗哆上引仡𩕳二合十七尾瑟拏二合十八尾嚩囉嚩㘑十九阿曩禮阿謨引去契二十齲㘑婆訖怛二合哆哩乞始二合二十一畢佐蘖瑳麼引曩引野二十二吠半底部上哆上嚕挽無漢反底二十三禰嚩引素囉二十四藥乞叉二合囉引乞叉二合娑二十五畢捨引佐引麼賀引疙哩二十六娑引誐囉引抳二十七鉢囉嚩二合哆上拶闌上底二十八劍半底二十九乞芻二合盤上底三十󰡊嚕二合怛嚩二合引𩕳那上引能上三十一麼賀引末㘄上三十二設訖帝二合三十三囉設訖帝二合囉乞始二合抳三十四阿儞也二合部上哆上喃去引三十五麼那部上哆上喃去引三十六怛囉二合引娑怛囉二合娑哆二合喃上引三十八設囉𭊥禰尾娑引攞佐引哩抳四十賀哩氷上誐攞引乞始二合四十一囉訖哆二合乞始二合四十二囉訖哆二合󰹺無鉢反娑怛囉三合四十三馱引去哩抳四十四麼賀引㜸囉二合󰾰四十五嚩囉禰泥翳反跛哆二合四十六帝孺設訖底二合塞建二合馱四十七嚩路野謎四十八尾曩引尸迦引謎四十九婆嚩設咄嚕二合𭊥娑嚩二合引賀引五十賀哩賀哩五十一氷誐攞引乞始二合五十二野娑尾儞喻二合哆上野五十三鉢囉二合尾儞喻二合婆曳五十四度麼麼引里寧上曳五十五吠濕嚩二合引曩囉達囉引曳五十六阿爾仁際反哆野布哩五十七禰嚩那引上曩嚩僧去㜸囉二合賀𭊥野五十八唧致唧致五十九哩致𩕳致六十枳里底里唧𩕳六十一醯醯醯醯醯醯醯醯六十二度嚕尾度嚕六十三唧哩𩕳致計六十四娑囉尾娑上囉六十五跛囉嚩攞六十六惹惹惹惹六十七部染仁昝反鉢囉二合娑引哩野二合左上娑嚩二合引賀引六十八佛言我於後夜時日初出時。",
        "from": "T21n1233_001:0173a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為於世間作最上修行圓滿。",
        "from": "T21n1233_001:0173a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成佛事業獲得菩提。",
        "from": "T21n1233_001:0173a22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降伏魔軍轉於法輪。",
        "from": "T21n1233_001:0173a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有千俱胝淨居天眾來。",
        "from": "T21n1233_001:0173a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於我前發甚深意。",
        "from": "T21n1233_001:0173a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以柔軟音請轉法輪。",
        "from": "T21n1233_001:0173a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告舍利子言。",
        "from": "T21n1233_001:0173a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諦聽舍利子。",
        "from": "T21n1233_001:0173a25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此無能勝明王利益安樂一切有情。",
        "from": "T21n1233_001:0173a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利子昔過去之世。",
        "from": "T21n1233_001:0173a26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天與阿蘇羅兩相鬪戰。",
        "from": "T21n1233_001:0173b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時天眾退散阿蘇羅得勝。",
        "from": "T21n1233_001:0173b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時帝釋天主。",
        "from": "T21n1233_001:0173b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以此無能勝陀羅尼。",
        "from": "T21n1233_001:0173b02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書於金葉安旗幟頭。",
        "from": "T21n1233_001:0173b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復與相戰。",
        "from": "T21n1233_001:0173b03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時天眾得勝。",
        "from": "T21n1233_001:0173b03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿蘇羅軍尋即退散。",
        "from": "T21n1233_001:0173b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1233_001:0173b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛上儞也二合他去引一沒素謎二嚩素迦囉帝三曩引虞上那哩佐四迦播哩抳五娑嚩二合引賀引六佛告舍利子。",
        "from": "T21n1233_001:0173b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我不見有諸天及魔羅等。",
        "from": "T21n1233_001:0173b08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至人間沙門婆羅門。",
        "from": "T21n1233_001:0173b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并阿蘇羅一切鬼魅。",
        "from": "T21n1233_001:0173b09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿跛薩麼囉乾闥婆。",
        "from": "T21n1233_001:0173b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥叉囉乞叉蘖路荼緊那羅。",
        "from": "T21n1233_001:0173b10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尾那夜迦布旦曩供盤拏。",
        "from": "T21n1233_001:0173b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羯吒布多曩食生者等。",
        "from": "T21n1233_001:0173b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此無能勝大明王無敢違犯。",
        "from": "T21n1233_001:0173b12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是大明王陀羅尼。",
        "from": "T21n1233_001:0173b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不見不聞者。",
        "from": "T21n1233_001:0173b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃是過去見在業所障故。",
        "from": "T21n1233_001:0173b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊說是陀羅尼一切成就已。",
        "from": "T21n1233_001:0173b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時阿難陀舍利子天人阿蘇羅等。",
        "from": "T21n1233_001:0173b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心受持歡喜奉行。",
        "from": "T21n1233_001:0173b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說無能勝大明王陀羅尼經",
        "from": "T21n1233_001:0173b18_100",
        "context_with_next_sentence": false
    }
]