[
    {
        "source_sentence": "文殊師利發願經",
        "from": "T10n0296_001:0878c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東晉天竺三藏佛陀跋陀羅譯",
        "from": "T10n0296_001:0878c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身口意清淨，　　除滅諸垢穢，",
        "from": "T10n0296_001:0878c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心恭敬禮，　　十方三世佛。",
        "from": "T10n0296_001:0878c26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普賢願力故，　　悉覩見諸佛，",
        "from": "T10n0296_001:0878c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一如來所，　　一切剎塵禮。",
        "from": "T10n0296_001:0878c28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一微塵中，　　見一切諸佛，",
        "from": "T10n0296_001:0878c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩眾圍繞，　　法界塵亦然。",
        "from": "T10n0296_001:0879a01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以眾妙音聲，　　宣揚諸最勝，",
        "from": "T10n0296_001:0879a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量功德海，　　不可得窮盡。",
        "from": "T10n0296_001:0879a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以普賢行力，　　無上眾供具，",
        "from": "T10n0296_001:0879a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養於十方，　　三世一切佛。",
        "from": "T10n0296_001:0879a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以妙香花鬘，　　種種諸伎樂，",
        "from": "T10n0296_001:0879a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切妙莊嚴，　　普供養諸佛。",
        "from": "T10n0296_001:0879a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我以貪恚癡，　　造一切惡行，",
        "from": "T10n0296_001:0879a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身口意不善，　　悔過悉除滅。",
        "from": "T10n0296_001:0879a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生福，　　諸聲聞緣覺，",
        "from": "T10n0296_001:0879a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩及諸佛，　　功德悉隨喜。",
        "from": "T10n0296_001:0879a11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方一切佛，　　初成等正覺，",
        "from": "T10n0296_001:0879a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我令悉勸請，　　轉無上法輪。",
        "from": "T10n0296_001:0879a13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "示現涅槃者，　　合掌恭敬請，",
        "from": "T10n0296_001:0879a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住一切塵劫，　　安樂諸群生。",
        "from": "T10n0296_001:0879a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我所集功德，　　迴向施眾生，",
        "from": "T10n0296_001:0879a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "究竟菩薩行，　　逮無上菩提。",
        "from": "T10n0296_001:0879a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉供養過去，　　現在十方佛，",
        "from": "T10n0296_001:0879a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願未來世尊，　　速成菩提道。",
        "from": "T10n0296_001:0879a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普莊嚴十方，　　一切諸佛剎，",
        "from": "T10n0296_001:0879a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來坐道場，　　菩薩眾充滿。",
        "from": "T10n0296_001:0879a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令十方眾生，　　除滅諸煩惱，",
        "from": "T10n0296_001:0879a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深解真實義，　　常得安樂住。",
        "from": "T10n0296_001:0879a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我修菩薩行，　　成就宿命智，",
        "from": "T10n0296_001:0879a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除滅一切障，　　永盡無有餘。",
        "from": "T10n0296_001:0879a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉遠離生死，　　諸魔煩惱業，",
        "from": "T10n0296_001:0879a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶日處虛空，　　蓮花不著水。",
        "from": "T10n0296_001:0879a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍行遊十方，　　教化諸群生，",
        "from": "T10n0296_001:0879a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除滅惡道苦，　　具足菩薩行。",
        "from": "T10n0296_001:0879a29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖隨順世間，　　不捨菩薩道，",
        "from": "T10n0296_001:0879b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡未來際劫，　　具修普賢行。",
        "from": "T10n0296_001:0879b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有同行者，　　願常集一處，",
        "from": "T10n0296_001:0879b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身口意善業，　　皆悉令同等。",
        "from": "T10n0296_001:0879b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遇善知識，　　開示普賢行，",
        "from": "T10n0296_001:0879b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此菩薩所，　　親近常不離。",
        "from": "T10n0296_001:0879b06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常見一切佛，　　菩薩眾圍繞，",
        "from": "T10n0296_001:0879b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡未來際劫，　　悉恭敬供養。",
        "from": "T10n0296_001:0879b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "守護諸佛法，　　讚歎菩薩行，",
        "from": "T10n0296_001:0879b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡未來劫修，　　究竟普賢道。",
        "from": "T10n0296_001:0879b10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖在生死中，　　具無盡功德，",
        "from": "T10n0296_001:0879b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智慧巧方便，　　諸三昧解脫。",
        "from": "T10n0296_001:0879b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一微塵中，　　見不思議剎，",
        "from": "T10n0296_001:0879b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一一剎中，　　見不思議佛。",
        "from": "T10n0296_001:0879b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見如是十方，　　一切世界海，",
        "from": "T10n0296_001:0879b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一世界海，　　悉見諸佛海。",
        "from": "T10n0296_001:0879b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一言音中，　　具一切妙音，",
        "from": "T10n0296_001:0879b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一妙音中，　　具足最勝音。",
        "from": "T10n0296_001:0879b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甚深智慧力，　　入無盡妙音，",
        "from": "T10n0296_001:0879b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉三世諸佛，　　清淨正法輪。",
        "from": "T10n0296_001:0879b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切未來劫，　　悉能作一念，",
        "from": "T10n0296_001:0879b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三世一切劫，　　悉為一念際。",
        "from": "T10n0296_001:0879b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一念中悉見，　　三世諸如來，",
        "from": "T10n0296_001:0879b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦普分別知，　　解脫及境界。",
        "from": "T10n0296_001:0879b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一微塵中，　　出三世淨剎，",
        "from": "T10n0296_001:0879b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切十方塵，　　莊嚴剎亦然。",
        "from": "T10n0296_001:0879b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉見未來佛，　　成道轉法輪，",
        "from": "T10n0296_001:0879b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "究竟佛事已，　　示現入涅槃。",
        "from": "T10n0296_001:0879b28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神力遍遊行，　　大乘力普門，",
        "from": "T10n0296_001:0879b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈力覆一切，　　行力功德滿。",
        "from": "T10n0296_001:0879c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功德力清淨，　　智慧力無礙，",
        "from": "T10n0296_001:0879c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三昧方便力，　　逮得菩提力。",
        "from": "T10n0296_001:0879c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清淨善業力，　　除滅煩惱力，",
        "from": "T10n0296_001:0879c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壞散諸魔力，　　具普賢行力。",
        "from": "T10n0296_001:0879c05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚴淨佛剎海，　　度脫眾生海，",
        "from": "T10n0296_001:0879c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分別諸業海，　　窮盡智慧海。",
        "from": "T10n0296_001:0879c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清淨諸行海，　　滿足諸願海，",
        "from": "T10n0296_001:0879c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉見諸佛海，　　我於劫海行。",
        "from": "T10n0296_001:0879c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三世諸佛行，　　及無量大願，",
        "from": "T10n0296_001:0879c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我皆悉具足，　　普賢行成佛。",
        "from": "T10n0296_001:0879c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普賢菩薩名，　　諸佛第一子，",
        "from": "T10n0296_001:0879c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我善根迴向，　　願悉與彼同。",
        "from": "T10n0296_001:0879c13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身口意清淨，　　自在莊嚴剎，",
        "from": "T10n0296_001:0879c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逮成等正覺，　　皆悉同普賢。",
        "from": "T10n0296_001:0879c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如文殊師利，　　普賢菩薩行，",
        "from": "T10n0296_001:0879c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我所有善根，　　迴向亦如是。",
        "from": "T10n0296_001:0879c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三世諸如來，　　所歎迴向道，",
        "from": "T10n0296_001:0879c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我迴向善根，　　成滿普賢行。",
        "from": "T10n0296_001:0879c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願我命終時，　　除滅諸障礙，",
        "from": "T10n0296_001:0879c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面見阿彌陀，　　往生安樂國。",
        "from": "T10n0296_001:0879c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生彼佛國已，　　成滿諸大願，",
        "from": "T10n0296_001:0879c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿彌陀如來，　　現前授我記。",
        "from": "T10n0296_001:0879c23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚴淨普賢行，　　滿足文殊願，",
        "from": "T10n0296_001:0879c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡未來際劫，　　究竟菩薩行。",
        "from": "T10n0296_001:0879c25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊師利發願經",
        "from": "T10n0296_001:0879c26_100",
        "context_with_next_sentence": false
    }
]