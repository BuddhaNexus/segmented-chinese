[
    {
        "source_sentence": "訶利帝母真言經",
        "from": "T21n1261_001:0289b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T21n1261_001:0289b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空諡大鑒",
        "from": "T21n1261_001:0289b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正號大廣智大興善寺三藏沙門不空奉　詔譯爾時訶利帝藥叉女。",
        "from": "T21n1261_001:0289b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在佛眾會從座而起。",
        "from": "T21n1261_001:0289b25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五體投地禮佛雙足。",
        "from": "T21n1261_001:0289b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而白佛言。",
        "from": "T21n1261_001:0289b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我有心真言。",
        "from": "T21n1261_001:0289b26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由如真多摩尼寶能滿一切意願。",
        "from": "T21n1261_001:0289b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為利益安樂閻浮提諸善男子及善女人。",
        "from": "T21n1261_001:0289b28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊哀愍聽許。",
        "from": "T21n1261_001:0289b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今說之。",
        "from": "T21n1261_001:0289b29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1261_001:0289c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵弩弩麼引里迦引呬帝娑嚩二合引訶引若有女人不宜男女。",
        "from": "T21n1261_001:0289c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或在胎中墮落斷緒不收。",
        "from": "T21n1261_001:0289c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆由四大不不能調適。",
        "from": "T21n1261_001:0289c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或被鬼神作諸障難。",
        "from": "T21n1261_001:0289c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或是宿業因緣不宜男女。",
        "from": "T21n1261_001:0289c05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應取白㲲或一肘或一搩手或長五寸隨意大小。",
        "from": "T21n1261_001:0289c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫訶利帝母。",
        "from": "T21n1261_001:0289c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作天女形純金色。",
        "from": "T21n1261_001:0289c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身著天衣頭冠瓔珞。",
        "from": "T21n1261_001:0289c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐宣臺上垂下兩足。",
        "from": "T21n1261_001:0289c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於垂足兩邊畫二孩子。",
        "from": "T21n1261_001:0289c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傍宣臺立。",
        "from": "T21n1261_001:0289c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於二膝上各坐一孩子。",
        "from": "T21n1261_001:0289c09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左手懷中抱一孩子。",
        "from": "T21n1261_001:0289c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於右手中持吉祥菓。",
        "from": "T21n1261_001:0289c10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫師應受八戒。",
        "from": "T21n1261_001:0289c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其彩色中不用皮膠。",
        "from": "T21n1261_001:0289c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫像成已。",
        "from": "T21n1261_001:0289c11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨治一室嚴儀塗拭。",
        "from": "T21n1261_001:0289c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以香塗作一方壇。",
        "from": "T21n1261_001:0289c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置像壇中。",
        "from": "T21n1261_001:0289c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以種種華散於壇上。",
        "from": "T21n1261_001:0289c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復以甘脆飲食乳糜酪飯及諸菓子閼伽香水。",
        "from": "T21n1261_001:0289c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒沈水香而用供養。",
        "from": "T21n1261_001:0289c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "像面向西持誦者面東。",
        "from": "T21n1261_001:0289c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對像念誦每日三時。",
        "from": "T21n1261_001:0289c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時別誦一千遍。",
        "from": "T21n1261_001:0289c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取月生五日起首。",
        "from": "T21n1261_001:0289c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先誦十萬遍。",
        "from": "T21n1261_001:0289c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後對像前念誦。",
        "from": "T21n1261_001:0289c17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求一切事皆悉圓滿。",
        "from": "T21n1261_001:0289c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法女人欲得男女者。",
        "from": "T21n1261_001:0289c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月經後澡浴。",
        "from": "T21n1261_001:0289c19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取黃乳牛母子同色者。",
        "from": "T21n1261_001:0289c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "搆乳一升置銀器中。",
        "from": "T21n1261_001:0289c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手無名指攪乳。",
        "from": "T21n1261_001:0289c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言加持一千八十遍。",
        "from": "T21n1261_001:0289c21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後取服。",
        "from": "T21n1261_001:0289c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至七日內則得有胎。",
        "from": "T21n1261_001:0289c22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法欲令他人歡喜敬愛者。",
        "from": "T21n1261_001:0289c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或飲食菓子或華或香。",
        "from": "T21n1261_001:0290a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持一百八遍。",
        "from": "T21n1261_001:0290a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於真言句中加彼人名。",
        "from": "T21n1261_001:0290a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將與彼人則得歡喜愛敬。",
        "from": "T21n1261_001:0290a02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若有人作留難口舌者。",
        "from": "T21n1261_001:0290a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫彼人形左脚踏之。",
        "from": "T21n1261_001:0290a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言一千八十遍則得無難。",
        "from": "T21n1261_001:0290a04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法欲令一切人歡喜者。",
        "from": "T21n1261_001:0290a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取牛黃末置銀器中。",
        "from": "T21n1261_001:0290a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前以無名指攪誦真言加持一百八遍。",
        "from": "T21n1261_001:0290a06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "點於額上一切人見皆歡喜順伏。",
        "from": "T21n1261_001:0290a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若得新衣服。",
        "from": "T21n1261_001:0290a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對像前加持一百八遍。",
        "from": "T21n1261_001:0290a08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後取著一切人見亦皆歡喜敬愛。",
        "from": "T21n1261_001:0290a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若有惡夢。",
        "from": "T21n1261_001:0290a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言一百八遍。",
        "from": "T21n1261_001:0290a10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則得惡夢消除。",
        "from": "T21n1261_001:0290a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若月蝕時。",
        "from": "T21n1261_001:0290a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取酥五兩置金器中。",
        "from": "T21n1261_001:0290a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以金筯攪。",
        "from": "T21n1261_001:0290a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無令間斷念誦加持。",
        "from": "T21n1261_001:0290a13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至月却得圓滿為限。",
        "from": "T21n1261_001:0290a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後取一分供養訶利帝母。",
        "from": "T21n1261_001:0290a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘者漸喫。",
        "from": "T21n1261_001:0290a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即有胎孕所生男女聰慧福德　又法取牛膝根作齒木。",
        "from": "T21n1261_001:0290a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持一百八遍嚼及揩齒。",
        "from": "T21n1261_001:0290a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出言詞令人樂聞。",
        "from": "T21n1261_001:0290a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "意欲所求他人之事皆得隨意成就　又法欲得壽命長遠者。",
        "from": "T21n1261_001:0290a18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取骨屢草嫩苗。",
        "from": "T21n1261_001:0290a18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "搵蘇蜜酪護摩七夜。",
        "from": "T21n1261_001:0290a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜別誦真言一千八遍。",
        "from": "T21n1261_001:0290a19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一遍一擲火中則長壽。",
        "from": "T21n1261_001:0290a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法欲得所為所作隨意成就者。",
        "from": "T21n1261_001:0290a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每月月生五日二十日。",
        "from": "T21n1261_001:0290a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗拭道場散華。",
        "from": "T21n1261_001:0290a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種飲食乳糜酪飯。",
        "from": "T21n1261_001:0290a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸菓子閼伽香水。",
        "from": "T21n1261_001:0290a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒沈香薰陸以用供養。",
        "from": "T21n1261_001:0290a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先供養一切如來文殊師利菩薩普賢等一切菩薩。",
        "from": "T21n1261_001:0290a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後供養我訶利帝母。",
        "from": "T21n1261_001:0290a25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故。",
        "from": "T21n1261_001:0290a25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我本藥叉女。",
        "from": "T21n1261_001:0290a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來授與我三歸五戒菩提心律儀戒。",
        "from": "T21n1261_001:0290a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對諸十地菩薩故。",
        "from": "T21n1261_001:0290a27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是先供養諸佛菩薩。",
        "from": "T21n1261_001:0290a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養已。",
        "from": "T21n1261_001:0290a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對此像前誦一萬遍。",
        "from": "T21n1261_001:0290a28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求事業皆得滿足。",
        "from": "T21n1261_001:0290a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "訶利帝母念誦法。",
        "from": "T21n1261_001:0290b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛子心真言曰。",
        "from": "T21n1261_001:0290b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵知上尾知上𩕳娑嚩二合引訶印合掌屈二大拇指入掌中。",
        "from": "T21n1261_001:0290b04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦心真言供養訖。",
        "from": "T21n1261_001:0290b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於頂上放二大拇指即散去。",
        "from": "T21n1261_001:0290b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請召發遣皆放此印。",
        "from": "T21n1261_001:0290b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以白檀香。",
        "from": "T21n1261_001:0290b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "橫量六指作童子形。",
        "from": "T21n1261_001:0290b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足兩手各把菓子與人。",
        "from": "T21n1261_001:0290b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭上作三髻角子。",
        "from": "T21n1261_001:0290b07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於閑淨處安置。",
        "from": "T21n1261_001:0290b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言萬遍。",
        "from": "T21n1261_001:0290b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即現身問言。",
        "from": "T21n1261_001:0290b08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喚我何事。",
        "from": "T21n1261_001:0290b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨心答之。",
        "from": "T21n1261_001:0290b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取黃牛肉方寸。",
        "from": "T21n1261_001:0290b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一加持一燒日三時。",
        "from": "T21n1261_001:0290b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿四日得大自在隨意。",
        "from": "T21n1261_001:0290b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法牛肉安悉香為丸。",
        "from": "T21n1261_001:0290b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月八日三時念誦。",
        "from": "T21n1261_001:0290b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒上香一八遍。",
        "from": "T21n1261_001:0290b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至十日已。",
        "from": "T21n1261_001:0290b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜半現大光相得大神力。",
        "from": "T21n1261_001:0290b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安悉香為丸一加持一燒日三時。",
        "from": "T21n1261_001:0290b13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時別一千八遍五日調伏一切人。",
        "from": "T21n1261_001:0290b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法百草花一一加持散身上。",
        "from": "T21n1261_001:0290b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得千人衣食。",
        "from": "T21n1261_001:0290b15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法日誦真言燒蘇合香七日。",
        "from": "T21n1261_001:0290b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見地下金藏。",
        "from": "T21n1261_001:0290b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以種種花菓飲食壇中供養。",
        "from": "T21n1261_001:0290b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日日如是得一切財寶。",
        "from": "T21n1261_001:0290b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法安悉和乳粥。",
        "from": "T21n1261_001:0290b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一加持一燒七日。",
        "from": "T21n1261_001:0290b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日三時時別一千八遍。",
        "from": "T21n1261_001:0290b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現身共語供意問。",
        "from": "T21n1261_001:0290b20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法被禁閉。",
        "from": "T21n1261_001:0290b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦一萬遍即解脫。",
        "from": "T21n1261_001:0290b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "治病加持菴羅菓芽乳中漬燒加持。",
        "from": "T21n1261_001:0290b22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除一切病。",
        "from": "T21n1261_001:0290b22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "訶利帝母經",
        "from": "T21n1261_001:0290b23_100",
        "context_with_next_sentence": false
    }
]