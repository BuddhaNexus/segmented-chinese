    },
    {
        "source_sentence": "復次小兒於初生後。",
        "from": "T21n1330_001:0492b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三日第三月第三年得患者。",
        "from": "T21n1330_001:0492b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是哩嚩帝母鬼所執。",
        "from": "T21n1330_001:0492b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其小兒忽然驚悸。",
        "from": "T21n1330_001:0492b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "叫呼啼哭身體疼痛。",
        "from": "T21n1330_001:0492b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寒熱無恒頭面顫動。",
        "from": "T21n1330_001:0492b14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顧視自身漸漸羸弱。",
        "from": "T21n1330_001:0492b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不思飲食以至枯瘦者。",
        "from": "T21n1330_001:0492b15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持明人當用種種上味肉食及生肉生魚酒等。",
        "from": "T21n1330_001:0492b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設紅色憧八座燃燈八盞。",
        "from": "T21n1330_001:0492b17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復用尾螺樹葉安息香蛇皮蒜猫兒糞白芥子等。",
        "from": "T21n1330_001:0492b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用酥同和燒薰小兒。",
        "from": "T21n1330_001:0492b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及同前五藥煎以水。",
        "from": "T21n1330_001:0492b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沐浴小兒即誦大明。",
        "from": "T21n1330_001:0492b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持如上曼拏羅中祭食及種種物。",
        "from": "T21n1330_001:0492b20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說大明曰。",
        "from": "T21n1330_001:0492b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引曩謨囉引嚩拏引野一捺舍嚩那曩二贊捺囉二合賀引娑引野三鉢囉二合入嚩二合㘑多賀娑多二合引野四賀曩賀曩五那賀那賀六摩哩那二合野摩哩那二合野七努瑟吒二合屹囉二合㰠八謨吒野謨吒野娑嚩二合引賀引九如是誦大明加持已出於城外。",
        "from": "T21n1330_001:0492b27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以戌時面向北。",
        "from": "T21n1330_001:0492b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祭哩嚩帝母鬼。",
        "from": "T21n1330_001:0492b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有祭食種種物等隨處棄之。",
        "from": "T21n1330_001:0492b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所患小兒速得除瘥。",
        "from": "T21n1330_001:0492b29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次小兒於初生後。",
        "from": "T21n1330_001:0492c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四日第四月第四年得患者。",
        "from": "T21n1330_001:0492c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是目佉曼尼迦母鬼所執。",
        "from": "T21n1330_001:0492c02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其小兒先患寒熱。",
        "from": "T21n1330_001:0492c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欬瘶吐逆身顫垂頭。",
        "from": "T21n1330_001:0492c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "啼哭手搔兩目顧視人面。",
        "from": "T21n1330_001:0492c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不思飲食饒大小便者。",
        "from": "T21n1330_001:0492c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持明人當於河兩岸取土。",
        "from": "T21n1330_001:0492c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作病小兒像。",
        "from": "T21n1330_001:0492c05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面西安曼拏羅內。",
        "from": "T21n1330_001:0492c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復設種種香華。",
        "from": "T21n1330_001:0492c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及生熟肉食并酒果等。",
