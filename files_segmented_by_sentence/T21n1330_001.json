[
    {
        "source_sentence": "囉嚩拏說救療小兒疾病經",
        "from": "T21n1330_001:0491c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試光祿卿",
        "from": "T21n1330_001:0491c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明教大師臣法賢奉　詔譯",
        "from": "T21n1330_001:0491c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時囉嚩拏。",
        "from": "T21n1330_001:0491c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀於世間一切小兒。",
        "from": "T21n1330_001:0491c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從其初生至十二歲。",
        "from": "T21n1330_001:0491c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並在幼稚癡騃之位。",
        "from": "T21n1330_001:0491c13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神氣未足鬼魅得便。",
        "from": "T21n1330_001:0491c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有十二曜母鬼遊行世間。",
        "from": "T21n1330_001:0491c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於晝夜分常伺其便。",
        "from": "T21n1330_001:0491c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或因眠睡或獨行坐。",
        "from": "T21n1330_001:0491c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此之際現作種種差異之相。",
        "from": "T21n1330_001:0491c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "驚怖小兒令其失常。",
        "from": "T21n1330_001:0491c16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "噏取精氣。",
        "from": "T21n1330_001:0491c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因成疾病遂至殤夭。",
        "from": "T21n1330_001:0491c17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我見是事深所哀愍。",
        "from": "T21n1330_001:0491c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今為說十二曜母鬼。",
        "from": "T21n1330_001:0491c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執魅小兒年月時分所患疾狀。",
        "from": "T21n1330_001:0491c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及說大明救療之法。",
        "from": "T21n1330_001:0491c19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至作法出生祭祀儀則。",
        "from": "T21n1330_001:0491c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人聞我所說。",
        "from": "T21n1330_001:0491c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有疾患者時持明人。",
        "from": "T21n1330_001:0491c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依於我法而作救療發至誠者。",
        "from": "T21n1330_001:0491c21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定獲輕𭙬安樂吉祥。",
        "from": "T21n1330_001:0491c22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二曜母鬼名者所謂摩怛哩難那一蘇難那二哩嚩帝三目佉曼尼迦四尾拏隷五設俱儞六布多曩七輸瑟迦八阿哩也迦九染婆迦十必隷氷砌迦十一塞健馱十二。",
        "from": "T21n1330_001:0491c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等十二曜母鬼。",
        "from": "T21n1330_001:0491c27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執魅小兒為求祭祀。",
        "from": "T21n1330_001:0491c27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今各各說其執魅相狀。",
        "from": "T21n1330_001:0491c28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復小兒於初生日初生月初生年被執魅者。",
        "from": "T21n1330_001:0491c29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是摩怛哩難那母鬼所執。",
        "from": "T21n1330_001:0492a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其小兒先患寒熱。",
        "from": "T21n1330_001:0492a01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身體瘦弱漸漸乾枯。",
        "from": "T21n1330_001:0492a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心神荒亂身常顫掉。",
        "from": "T21n1330_001:0492a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "啼哭不食者。",
        "from": "T21n1330_001:0492a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持明人於二河岸取土。",
        "from": "T21n1330_001:0492a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作患小兒像。",
        "from": "T21n1330_001:0492a03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於四方曼拏羅內。",
        "from": "T21n1330_001:0492a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面西安小兒像。",
        "from": "T21n1330_001:0492a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復於曼拏羅設種種香華。",
        "from": "T21n1330_001:0492a04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及白色飲食乃至酒肉等。",
        "from": "T21n1330_001:0492a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復設七座幢燃七盞燈。",
        "from": "T21n1330_001:0492a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復用白芥子野狐糞猫兒糞安悉香蛇皮。",
        "from": "T21n1330_001:0492a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以如是等藥用黃牛酥。",
        "from": "T21n1330_001:0492a07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同和為香燒薰小兒。",
        "from": "T21n1330_001:0492a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復用𦱔麻油麻荊子。",
        "from": "T21n1330_001:0492a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或用葉及蓽茇羅樹葉嚩囉迦藥。",
        "from": "T21n1330_001:0492a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是五藥以水煎之沐浴小兒。",
        "from": "T21n1330_001:0492a09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦大明。",
        "from": "T21n1330_001:0492a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持如上曼拏羅中祭食及種種物。",
        "from": "T21n1330_001:0492a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦大明曰。",
        "from": "T21n1330_001:0492a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引曩謨囉引嚩拏引野一怛賴二合路枳也二合尾捺囉二合引鉢拏引野二賀曩嚩日哩二合引拏三沒囉二合憾摩二合難儞曩四摩引哩迦二合嚕閉拏五摩引怛哩二合難那引六嚩囉嚩囉七輸瑟迦二合輸瑟迦二合八捫左捫左九俱摩引囉崗娑嚩二合引賀引十如是誦大明加持已出於城外。",
        "from": "T21n1330_001:0492a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以日中時面東。",
        "from": "T21n1330_001:0492a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祭摩怛哩難那母鬼。",
        "from": "T21n1330_001:0492a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有祭食香華等物隨處棄擲。",
        "from": "T21n1330_001:0492a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所患小兒速得除瘥。",
        "from": "T21n1330_001:0492a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次小兒於初生後。",
        "from": "T21n1330_001:0492a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二日第二月第二年得患者。",
        "from": "T21n1330_001:0492a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是蘇難那母鬼所執。",
        "from": "T21n1330_001:0492a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其小兒先患寒熱作荒亂相。",
        "from": "T21n1330_001:0492a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合眼不食手足畜搦腹內疼痛吐逆喘息者。",
        "from": "T21n1330_001:0492a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持明人當用米粉一斗。",
        "from": "T21n1330_001:0492a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作病小兒像面西。",
        "from": "T21n1330_001:0492a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安曼拏羅中。",
        "from": "T21n1330_001:0492a25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復於曼拏羅設種種上妙香華飲食及酒肉等。",
        "from": "T21n1330_001:0492a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復用白色幢四座燃燈四盞。",
        "from": "T21n1330_001:0492a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復用安悉香蒜蛇皮白芥子猫兒糞酥。",
        "from": "T21n1330_001:0492a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同和為香燒薰小兒。",
        "from": "T21n1330_001:0492a28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又同前用五藥水沐浴小兒。",
        "from": "T21n1330_001:0492a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦大明。",
        "from": "T21n1330_001:0492a29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持如上曼拏羅中祭食及種種物即說大明曰。",
        "from": "T21n1330_001:0492b01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引曩謨囉引嚩拏引野一怛賴二合路枳也二合尾捺囉二合引波拏引野二贊捺囉二合賀引娑馱哩尼三入嚩二合㘑多賀娑多二合引野四賀曩賀曩五那賀那賀六怛哩二合輸隷曩努瑟吒二合屹囉二合賀引七儞訖哩二合多野儞訖哩多野八祖沙野祖沙野九賀曩賀曩十蘇難那捫左捫左十一俱摩引囉崗娑嚩二合引賀引十二如是誦大明加持已出於城外。",
        "from": "T21n1330_001:0492b09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以戌時面西。",
        "from": "T21n1330_001:0492b09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祭蘇難那母鬼。",
        "from": "T21n1330_001:0492b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有祭食香華等物隨處棄之。",
        "from": "T21n1330_001:0492b11_1",
