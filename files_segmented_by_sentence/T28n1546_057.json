[
    {
        "source_sentence": "阿毘曇毘婆沙論卷第五十七",
        "from": "T28n1546_057:0399b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦旃延子造　五百羅漢釋",
        "from": "T28n1546_057:0399b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北涼天竺沙門浮陀跋摩",
        "from": "T28n1546_057:0399b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共道泰等譯4 相應品1智揵度相應品第四之一七人：",
        "from": "T28n1546_057:0399b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅信、堅法、信解脫、見到、身證、慧解脫、俱解脫人。",
        "from": "T28n1546_057:0399b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八智、三三昧、三根、七覺支、八道支。",
        "from": "T28n1546_057:0399b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅信人於此八智，幾成就、幾不成就？",
        "from": "T28n1546_057:0399b13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至俱解脫人幾成就、幾不成就？",
        "from": "T28n1546_057:0399b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如此章及解章義，此中廣說優波提舍。",
        "from": "T28n1546_057:0399b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰：",
        "from": "T28n1546_057:0399b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何故此中及定揵度依七人而作論，使揵度中依五人而作論？",
        "from": "T28n1546_057:0399b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣說如使揵度不善品中。",
        "from": "T28n1546_057:0399b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅信人於此八智，幾成就、幾不成就？",
        "from": "T28n1546_057:0399b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰：",
        "from": "T28n1546_057:0399b18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何故作此論？",
        "from": "T28n1546_057:0399b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0399b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲止言無成就者意，亦明實有成就故，而作此論。",
        "from": "T28n1546_057:0399b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次欲以七人作章、八智三三昧等立門故，而作此論。",
        "from": "T28n1546_057:0399b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅信人於此八智，幾成就、幾不成就？",
        "from": "T28n1546_057:0399b22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0399b22_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或一二三四五六七八。",
        "from": "T28n1546_057:0399b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦法忍無知他心智一，有他心智二；",
        "from": "T28n1546_057:0399b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦法智無他心智三，有他心智四；",
        "from": "T28n1546_057:0399b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中增見、增慧、增通、增智、增名。",
        "from": "T28n1546_057:0399b25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦比忍無他心智三，有他心智四；",
        "from": "T28n1546_057:0399b26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦比智無他心智五，有他心智六，乃至道比忍無他心智七，有他心智八。",
        "from": "T28n1546_057:0399b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如堅信，堅法亦如是。",
        "from": "T28n1546_057:0399b28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何？",
        "from": "T28n1546_057:0399b28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此二人地等、所依身等、道等、離欲等、定等，唯根有差別，堅信是鈍根，堅法是利根。",
        "from": "T28n1546_057:0399c01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信解脫人於此八智，幾成就、幾不成就？",
        "from": "T28n1546_057:0399c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0399c02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無他心智七，有他心智八。",
        "from": "T28n1546_057:0399c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如信解脫，見到亦如是。",
        "from": "T28n1546_057:0399c03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何？",
        "from": "T28n1546_057:0399c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此二人地等，廣說如上。",
        "from": "T28n1546_057:0399c04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身證人於此八智盡成就。",
        "from": "T28n1546_057:0399c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如身證人，慧解脫、俱解脫人亦如是。",
        "from": "T28n1546_057:0399c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅信人於此八智，幾過去成就、幾未來成就、幾現在成就？",
        "from": "T28n1546_057:0399c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰：",
        "from": "T28n1546_057:0399c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何故作此論？",
        "from": "T28n1546_057:0399c07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0399c07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為止言無過去未來者意，亦明過去未來是實有法故，而作此論。",
        "from": "T28n1546_057:0399c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次先總明智成就不成就，今欲分別世中成就不成就故，而作此論。",
        "from": "T28n1546_057:0399c10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅信人於此八智，幾過去成就、幾未來成就、幾現在成就？",
        "from": "T28n1546_057:0399c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0399c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦法忍無他心智一，過去未來成就。",
        "from": "T28n1546_057:0399c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有他心智二，過去、未來成就。",
        "from": "T28n1546_057:0399c13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有現在，現在是忍故。",
        "from": "T28n1546_057:0399c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦法智，無他心智一，過去成就謂等智，三未來成就謂法智、等智、苦智，二現在成就謂法智、苦智。",
        "from": "T28n1546_057:0399c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有他心智，二過去成就，四未來成就，增他心智二現在成就，如先說。",
        "from": "T28n1546_057:0399c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦比忍，無他心智三，過去未來成就。",
        "from": "T28n1546_057:0399c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有他心智，四增他心智。",
        "from": "T28n1546_057:0399c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現在無，是忍故。",
        "from": "T28n1546_057:0399c19_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦比智，無他心智，三過去成就，四未來成就，增比智二現在成就，謂苦智、比智。",
        "from": "T28n1546_057:0399c21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有他心智，四過去成就，五未來成就，二現在成就，如先說。",
        "from": "T28n1546_057:0399c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至道比忍現在前，無他心智，七過去未來成就，除他心智。",
        "from": "T28n1546_057:0399c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有他心智八，增他心智。",
        "from": "T28n1546_057:0399c24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現在無，是忍故。",
        "from": "T28n1546_057:0399c25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如堅信，堅法亦如是。",
        "from": "T28n1546_057:0399c25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何？",
        "from": "T28n1546_057:0399c25_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此二人地等，廣說如上。",
        "from": "T28n1546_057:0399c26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信解脫人於此八智，幾過去成就、幾未來成就、幾現在成就？",
        "from": "T28n1546_057:0399c27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣說如經本。",
        "from": "T28n1546_057:0399c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅信人，法智現在前時，幾智現在前？",
        "from": "T28n1546_057:0399c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0399c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二。",
        "from": "T28n1546_057:0399c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法智苦智二，法智集智二，法智滅智二，法智道智二。",
        "from": "T28n1546_057:0400a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體性是一，以事故異，對治欲界故是法智，以行故是苦集滅道智。",
        "from": "T28n1546_057:0400a02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比智現在前時，幾智現在前？",
        "from": "T28n1546_057:0400a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0400a03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二。",
        "from": "T28n1546_057:0400a03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比智苦智二，比智集智二，比智滅智二。",
        "from": "T28n1546_057:0400a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體性是一，以事故異，對治色無色界故是比智，以行故是苦集滅智。",
        "from": "T28n1546_057:0400a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦智現在前時，幾智現在前？",
        "from": "T28n1546_057:0400a06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0400a06_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二。",
        "from": "T28n1546_057:0400a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦智法智二，苦智比智二。",
        "from": "T28n1546_057:0400a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體性是一，以事故異，以行故是苦智，以對治故是法智。",
        "from": "T28n1546_057:0400a08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比智集智滅智說亦如是。",
        "from": "T28n1546_057:0400a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道智現在前時，幾智現在前？",
        "from": "T28n1546_057:0400a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0400a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二。",
        "from": "T28n1546_057:0400a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道智法智二。",
        "from": "T28n1546_057:0400a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體性是一，以事故異，以行故是道智，以對治故是法智。",
        "from": "T28n1546_057:0400a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如堅信，堅法亦如是。",
        "from": "T28n1546_057:0400a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信解脫人，法智現在前時，幾智現在前？",
        "from": "T28n1546_057:0400a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0400a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或二或三。",
        "from": "T28n1546_057:0400a13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法智苦智二，法智集智二，法智滅智二。",
        "from": "T28n1546_057:0400a14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法智道智，無他心智二、有他心智三。",
        "from": "T28n1546_057:0400a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體性是一，以事故異，對治故是法智，以行故是苦集滅道智，以方便故是他心智。",
        "from": "T28n1546_057:0400a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如法智，比智亦如是。",
        "from": "T28n1546_057:0400a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他心智現在前時，幾智現在前？",
        "from": "T28n1546_057:0400a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0400a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或二或三。",
        "from": "T28n1546_057:0400a18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他心智等智二，他心智道智三。",
        "from": "T28n1546_057:0400a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體性是一，以事故異，方便故是他心智，對治故是法智比智，以自體故是等智，以行故是道智。",
        "from": "T28n1546_057:0400a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "等智現在前時，幾智現在前？",
        "from": "T28n1546_057:0400a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0400a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或一或二。",
        "from": "T28n1546_057:0400a22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無他心智一，有他心智二。",
        "from": "T28n1546_057:0400a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體性是一，以事故異，以自體故是等智，以方便故是他心智，苦智集滅智如先說。",
        "from": "T28n1546_057:0400a24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道智現在前時，幾智現在前？",
        "from": "T28n1546_057:0400a25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0400a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或二或三。",
        "from": "T28n1546_057:0400a25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道智無他心智二，有他心智三。",
        "from": "T28n1546_057:0400a26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體性是一，以事故異，以行故是道智，以對治故是法智比智，以方便故是他心智。",
        "from": "T28n1546_057:0400a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如信解脫，見到、身證亦如是。",
        "from": "T28n1546_057:0400a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧解脫人，法智現在前時，幾智現在前？",
        "from": "T28n1546_057:0400a29_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0400b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或二或三。",
        "from": "T28n1546_057:0400b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法智苦智非盡智無生智二，若是盡智無生智三。",
        "from": "T28n1546_057:0400b02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體性是一，以事故異，對治故是法智，以行故是苦智，以所作竟故是盡智，以從因生故是無生智，集智滅智說亦如是。",
        "from": "T28n1546_057:0400b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法智道智非盡智無生智他心智二，若是盡智無生智他心智三。",
        "from": "T28n1546_057:0400b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體性是一，以事故異，以對治故是法智，以行故是道智，以所作竟故是盡智，從因生故是無生智，以方便故是他心智。",
        "from": "T28n1546_057:0400b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比智亦如是。",
        "from": "T28n1546_057:0400b09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他心智等智如先說。",
        "from": "T28n1546_057:0400b09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦智現在前時，幾智現在前？",
        "from": "T28n1546_057:0400b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0400b10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或二或三。",
        "from": "T28n1546_057:0400b10_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦智法智非盡智無生智二，若是盡智無生智三。",
        "from": "T28n1546_057:0400b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦智比智非盡智無生智二，若是盡智無生智三。",
        "from": "T28n1546_057:0400b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體性是一，以事故異，以行故是苦智，以對治故是法智，以所作竟故是盡智，從因生故是無生智。",
        "from": "T28n1546_057:0400b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如苦智，集滅智亦如是。",
        "from": "T28n1546_057:0400b15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道智現在前時，幾智現在前？",
        "from": "T28n1546_057:0400b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0400b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或二或三。",
        "from": "T28n1546_057:0400b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道智法智非盡智無生智他心智二，若是盡智無生智他心智三。",
        "from": "T28n1546_057:0400b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道智比智非盡智無生智他心智二，若是盡智無生智他心智三。",
        "from": "T28n1546_057:0400b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體性是一，以事故異，廣說如上。",
        "from": "T28n1546_057:0400b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如慧解脫，俱解脫亦如是。",
        "from": "T28n1546_057:0400b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰：",
        "from": "T28n1546_057:0400b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何故不說盡智無生智攝他心智耶？",
        "from": "T28n1546_057:0400b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0400b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他心智以對治故是法智比智，以方便故是他心智。",
        "from": "T28n1546_057:0400b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他心智但緣他心，盡智無生智緣自身他身及非身法。",
        "from": "T28n1546_057:0400b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次他心智緣現在，彼二智緣三世及無為。",
        "from": "T28n1546_057:0400b25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次他心智緣心心數法，彼二智緣四陰五陰及無為法。",
        "from": "T28n1546_057:0400b26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次他心智是見，彼二智非見。",
        "from": "T28n1546_057:0400b27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅信人，於此三三昧，幾成就、幾不成就？",
        "from": "T28n1546_057:0400b28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0400b28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅法忍未生成就二謂空無願，滅法忍生成就三，增無相。",
        "from": "T28n1546_057:0400b29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如堅信人，堅法人亦如是。",
        "from": "T28n1546_057:0400c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信解脫人盡成就。",
        "from": "T28n1546_057:0400c01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如信解脫，見到、身證、慧解脫、俱解脫亦如是。",
        "from": "T28n1546_057:0400c02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅信人，於此三三昧，幾過去成就、幾未來成就、幾現在成就？",
        "from": "T28n1546_057:0400c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0400c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若依空三昧得正決定者，苦法忍無過去，二未來成就謂空無願，一現在成就謂空，餘廣說如經本。",
        "from": "T28n1546_057:0400c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依無願三昧得正決定，說亦如是。",
        "from": "T28n1546_057:0400c07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰：",
        "from": "T28n1546_057:0400c07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等人依空三昧得正決定、何等人依無願耶？",
        "from": "T28n1546_057:0400c08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0400c08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有見行者、或有愛行者，若見行者，依空三昧得正決定。",
        "from": "T28n1546_057:0400c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若愛行者，依無願。",
        "from": "T28n1546_057:0400c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯除菩薩是愛行人，依空三昧得正決定。",
        "from": "T28n1546_057:0400c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見行者有二種，一著我見、二著我所見。",
        "from": "T28n1546_057:0400c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛行者亦有二種，一行我慢、二多󰝬墮。",
        "from": "T28n1546_057:0400c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著我見者行無我行，著我所見者行空行，行我慢者行無常行，多󰝬墮者行苦行。",
        "from": "T28n1546_057:0400c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次若利根者，依空三昧得正決定。",
        "from": "T28n1546_057:0400c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若鈍根者，依無願。",
        "from": "T28n1546_057:0400c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如利鈍，乃至聞即能解，廣分別亦如是。",
        "from": "T28n1546_057:0400c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若依無願得正決定者，或依無願無相離三界欲，此身中不離起空三昧現在前。",
        "from": "T28n1546_057:0400c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅信人，空三昧現在前時，幾智現在前？",
        "from": "T28n1546_057:0400c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0400c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或二或無。",
        "from": "T28n1546_057:0400c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦智法智二，苦智比智二，忍中無。",
        "from": "T28n1546_057:0400c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘廣說如經本。",
        "from": "T28n1546_057:0400c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至如慧解脫，俱解脫亦如是。",
        "from": "T28n1546_057:0400c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰：",
        "from": "T28n1546_057:0400c22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何故盡智無生智不與空三昧相應耶？",
        "from": "T28n1546_057:0400c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0400c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所行異故。",
        "from": "T28n1546_057:0400c23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若行是空三昧，所行非盡智無生智；",
        "from": "T28n1546_057:0400c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若行是盡智無生智，所行非空三昧。",
        "from": "T28n1546_057:0400c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次空三昧與見相應，彼二智性非見。",
        "from": "T28n1546_057:0400c26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次空三昧體是第一義，所行亦是第一義；",
        "from": "T28n1546_057:0400c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼二智體雖是第一義，而所行是世諦。",
        "from": "T28n1546_057:0400c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅信人，未知欲知根現在前時，幾智現在前？",
        "from": "T28n1546_057:0400c29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0401a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或二或無。",
        "from": "T28n1546_057:0401a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦智法智二，苦智比智二，乃至道智法智二，忍中無。",
        "from": "T28n1546_057:0401a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如未知欲知根，七覺、八道支亦如是。",
        "from": "T28n1546_057:0401a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如堅信，堅法亦如是。",
        "from": "T28n1546_057:0401a03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘廣說如經本。",
        "from": "T28n1546_057:0401a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若如此所說，則明慧解脫阿羅漢能起根本禪現在前。",
        "from": "T28n1546_057:0401a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰：",
        "from": "T28n1546_057:0401a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若慧解脫阿羅漢能起根本禪現在前者，佛經云何通？",
        "from": "T28n1546_057:0401a06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如說：",
        "from": "T28n1546_057:0401a06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘇尸摩問諸比丘：",
        "from": "T28n1546_057:0401a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何起諸禪現在前？",
        "from": "T28n1546_057:0401a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸比丘答蘇尸摩：",
        "from": "T28n1546_057:0401a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知我等是慧解脫人。",
        "from": "T28n1546_057:0401a08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0401a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧解脫有二種，一是少分、二是滿分。",
        "from": "T28n1546_057:0401a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少分慧解脫者能起一禪二禪三禪現在前，滿分者乃至不能起一禪現在前。",
        "from": "T28n1546_057:0401a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中說少分慧解脫，經中說滿分慧解脫，是故二俱善通。",
        "from": "T28n1546_057:0401a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若法與法智相應，亦與比智相應耶？",
        "from": "T28n1546_057:0401a13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸法以三事故共合，或以攝故合、或以相應故合、或以攝以相應故合。",
        "from": "T28n1546_057:0401a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以攝合者，如智於智。",
        "from": "T28n1546_057:0401a15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以相應合者，如智於定。",
        "from": "T28n1546_057:0401a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以攝以相應合者，如智於根、覺支、道支。",
        "from": "T28n1546_057:0401a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若法與法智相應，亦與比智相應耶？",
        "from": "T28n1546_057:0401a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0401a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若法與法智相應，不與比智相應。",
        "from": "T28n1546_057:0401a18_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何？",
        "from": "T28n1546_057:0401a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非一心故。",
        "from": "T28n1546_057:0401a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有法智，則無比智而與他心智相應耶？",
        "from": "T28n1546_057:0401a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0401a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或法與法智相應，不與他心智相應，乃至廣作四句。",
        "from": "T28n1546_057:0401a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與法智相應不與他心智相應者，他心智所不攝法智相應法。",
        "from": "T28n1546_057:0401a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼是何耶？",
        "from": "T28n1546_057:0401a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0401a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦集滅法智他心智所不攝道法智相應法。",
        "from": "T28n1546_057:0401a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與他心智相應非法智者，法智所不攝他心智相應法。",
        "from": "T28n1546_057:0401a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼是何耶。",
        "from": "T28n1546_057:0401a25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T28n1546_057:0401a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比智他心智世俗他心智。",
        "from": "T28n1546_057:0401a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與法智相應亦與他心智相應者，法智所攝他心智相應法。",
        "from": "T28n1546_057:0401a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼是何耶？",
        "from": "T28n1546_057:0401a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0401a28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法智他心智相應法，謂九大地、十善大地及以心覺觀隨地。",
        "from": "T28n1546_057:0401a29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不與法智相應亦不與他心智相應者，法智他心智。",
        "from": "T28n1546_057:0401b01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何？",
        "from": "T28n1546_057:0401b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自體不應自體，廣說如上，及法智他心智所不攝不相應法。",
        "from": "T28n1546_057:0401b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不攝者，除自體；",
        "from": "T28n1546_057:0401b03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不相應者，除相應。",
        "from": "T28n1546_057:0401b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼是何耶？",
        "from": "T28n1546_057:0401b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0401b04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦集滅比智相應聚，他心智所不攝道比智相應聚，諸忍相應他心智不相應諸餘有漏心、心數法、色、無為、心不相應行，無緣故不與相應。",
        "from": "T28n1546_057:0401b07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如他心智，苦集滅道智正見亦如是。",
        "from": "T28n1546_057:0401b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若法與法智相應，不與等智相應。",
        "from": "T28n1546_057:0401b09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何？",
        "from": "T28n1546_057:0401b09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法智相應聚異、等智相應聚異。",
        "from": "T28n1546_057:0401b10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與空三昧相應耶？",
        "from": "T28n1546_057:0401b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法智與三三昧相應，空三昧與二智相應，謂苦法智、苦比智，亦與二忍相應，謂苦法忍、苦比忍，是故得作大四句。",
        "from": "T28n1546_057:0401b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與法智相應非空三昧者，空三昧應於法智。",
        "from": "T28n1546_057:0401b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法智俱聚中空三昧體與法智相應，不與空三昧相應。",
        "from": "T28n1546_057:0401b15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何？",
        "from": "T28n1546_057:0401b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自體不應自體，如先說，及空三昧不相應法智相應法。",
        "from": "T28n1546_057:0401b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼是何耶？",
        "from": "T28n1546_057:0401b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0401b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無願無相相應聚與法智相應者。",
        "from": "T28n1546_057:0401b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與空三昧相應非法智者，法智應於空三昧。",
        "from": "T28n1546_057:0401b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空三昧俱聚中法智體與空三昧相應非法智。",
        "from": "T28n1546_057:0401b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何？",
        "from": "T28n1546_057:0401b20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自體不應自體，如先說，及法智不相應空三昧相應法。",
        "from": "T28n1546_057:0401b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼是何耶？",
        "from": "T28n1546_057:0401b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0401b22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦比智俱聚中苦忍俱聚空三昧相應法。",
        "from": "T28n1546_057:0401b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與法智相應亦與空三昧相應者，除空三昧應於法智，除法智應於空三昧。",
        "from": "T28n1546_057:0401b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法智空三昧俱聚中名除自體，餘心數法。",
        "from": "T28n1546_057:0401b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼是何耶？",
        "from": "T28n1546_057:0401b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0401b26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂八大地、十善大地及心覺觀隨地。",
        "from": "T28n1546_057:0401b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不與法智相應亦不與空三昧相應者，空三昧不與法智相應者與苦比智苦忍相應者。",
        "from": "T28n1546_057:0401b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空三昧自體不與法智相應，以是他聚故。",
        "from": "T28n1546_057:0401c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不與空三昧相應，自體不應自體，如先說。",
        "from": "T28n1546_057:0401c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法智不與空三昧相應者與無願無相三昧相應者，法智自體不與空三昧相應，以是他聚故。",
        "from": "T28n1546_057:0401c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不與法智相應，自體不應自體，如先說。",
        "from": "T28n1546_057:0401c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸餘法智空三昧不相應法。",
        "from": "T28n1546_057:0401c05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼是何耶？",
        "from": "T28n1546_057:0401c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0401c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法智不相應無願無相俱聚是也，及有漏心、心數法、色、無為、心不相應行。",
        "from": "T28n1546_057:0401c07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等法作第四句。",
        "from": "T28n1546_057:0401c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如法智於空三昧，法智於無願三昧、無相三昧、喜覺支、正覺亦如是。",
        "from": "T28n1546_057:0401c09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若法與法智相應，亦與未知欲知根相應耶？",
        "from": "T28n1546_057:0401c10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至廣作四句。",
        "from": "T28n1546_057:0401c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與法智相應不與未知欲知根相應者，未知欲知根所不攝法智相應法。",
        "from": "T28n1546_057:0401c12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼是何耶？",
        "from": "T28n1546_057:0401c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0401c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知根知已根所攝法智相應法不與未知欲知根相應。",
        "from": "T28n1546_057:0401c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何？",
        "from": "T28n1546_057:0401c14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以是他聚故。",
        "from": "T28n1546_057:0401c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與未知欲知根相應非法智者，未知欲知根所攝法智，未知欲知根俱聚中法智體與未知欲知根相應非法智，自體不應自體，如先說。",
        "from": "T28n1546_057:0401c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及法智不攝不相應未知欲知根相應法。",
        "from": "T28n1546_057:0401c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼是何耶？",
        "from": "T28n1546_057:0401c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比智俱聚忍俱聚中未知欲知根相應法，是名與未知欲知根相應非法智相應。",
        "from": "T28n1546_057:0401c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何？",
        "from": "T28n1546_057:0401c21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是他聚故。",
        "from": "T28n1546_057:0401c21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與法智相應亦與未知欲知根相應者，未知欲知根所攝法智相應法。",
        "from": "T28n1546_057:0401c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼是何耶？",
        "from": "T28n1546_057:0401c23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0401c23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八根及彼相應非根心數法。",
        "from": "T28n1546_057:0401c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不與法智相應亦不與未知欲知根相應者，未知欲知根所不攝法智。",
        "from": "T28n1546_057:0401c25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知根知已根俱聚中法智體不與法智相應，自體不應自體，廣說如上。",
        "from": "T28n1546_057:0401c27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不與未知欲知根相應。",
        "from": "T28n1546_057:0401c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何？",
        "from": "T28n1546_057:0401c28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以他聚故。",
        "from": "T28n1546_057:0401c28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及法智未知欲知根所不攝不相應心心數法。",
        "from": "T28n1546_057:0401c29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼是何耶？",
        "from": "T28n1546_057:0401c29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0401c29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法智不攝不相應知根知已根俱聚，及有漏心、心數法、色、無為、心不相應行。",
        "from": "T28n1546_057:0402a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等法作第四句。",
        "from": "T28n1546_057:0402a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如法智於未知欲知根，法智於知根、知已根亦如是。",
        "from": "T28n1546_057:0402a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若法與法智相應，亦與念覺支相應耶？",
        "from": "T28n1546_057:0402a05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至廣作四句。",
        "from": "T28n1546_057:0402a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與法智相應不與念覺支相應者，念覺支應於法智，法智俱聚中念覺支體與法智相應，不與念覺支相應。",
        "from": "T28n1546_057:0402a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何？",
        "from": "T28n1546_057:0402a08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自體不應自體，如先說。",
        "from": "T28n1546_057:0402a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與念覺支相應不與法智相應者，法智應於念覺支，念覺支俱聚中法智體與念覺支相應，不與法智相應。",
        "from": "T28n1546_057:0402a11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何？",
        "from": "T28n1546_057:0402a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自體不應自體，如先說。",
        "from": "T28n1546_057:0402a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及法智不相應念覺支相應法。",
        "from": "T28n1546_057:0402a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼是何耶？",
        "from": "T28n1546_057:0402a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0402a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忍俱聚中、比智俱聚中念覺支相應法。",
        "from": "T28n1546_057:0402a14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與法智相應亦與念覺支相應者，除念覺支應於法智，以多故除，餘念覺支法智覺支聚中，各除自體，餘心心數法與二相應。",
        "from": "T28n1546_057:0402a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼是何耶？",
        "from": "T28n1546_057:0402a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0402a17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂八大地、十善大地及心覺觀隨地。",
        "from": "T28n1546_057:0402a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不與法智相應亦不與念覺支相應者，法智不相應念覺支。",
        "from": "T28n1546_057:0402a19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼是何耶？",
        "from": "T28n1546_057:0402a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0402a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忍俱聚中、比智俱聚中念覺支體不與法智相應，是他聚故，自體不應自體，如先說。",
        "from": "T28n1546_057:0402a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切無漏心，更無餘。",
        "from": "T28n1546_057:0402a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有餘有漏心、心數法、色、無為、心不相應行作第四句。",
        "from": "T28n1546_057:0402a23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如法智於念覺支，法智於精進覺枝、猗覺枝、定覺枝、捨覺支、正方便、正念、正定亦如是。",
        "from": "T28n1546_057:0402a25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若法與法智相應，亦與擇法覺支相應耶？",
        "from": "T28n1546_057:0402a26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0402a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若法與法智相應，亦與擇法覺枝相應。",
        "from": "T28n1546_057:0402a27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頗與擇法覺支相應非法智耶？",
        "from": "T28n1546_057:0402a28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0402a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有。",
        "from": "T28n1546_057:0402a28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法智所不攝擇法覺支相應法。",
        "from": "T28n1546_057:0402a29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼是何耶？",
        "from": "T28n1546_057:0402a29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T28n1546_057:0402a29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忍俱聚中、比智俱聚中擇法覺支相應。",
        "from": "T28n1546_057:0402b01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如法智，比智說亦如是。",
        "from": "T28n1546_057:0402b02_6",
        "context_with_next_sentence": true
    },
