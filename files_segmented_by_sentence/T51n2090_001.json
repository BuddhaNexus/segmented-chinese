[
    {
        "source_sentence": "釋迦牟尼如來像法滅盡之記",
        "from": "T51n2090_001:0996a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國大德三藏法師沙門法成譯",
        "from": "T51n2090_001:0996a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有于闐國來。六代王已過至第七代王。名日",
        "from": "T51n2090_001:0996a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘左耶訖多。",
        "from": "T51n2090_001:0996a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "治國之時。",
        "from": "T51n2090_001:0996a07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼國有寺。",
        "from": "T51n2090_001:0996a07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名薩迦般羅訶郍。",
        "from": "T51n2090_001:0996a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "去此寺不遠有一山谷。",
        "from": "T51n2090_001:0996a08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名娑迦耶幾郍。",
        "from": "T51n2090_001:0996a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼山谷中有一羅漢。",
        "from": "T51n2090_001:0996a09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼有苾芻弟子。",
        "from": "T51n2090_001:0996a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從其師所。",
        "from": "T51n2090_001:0996a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以學律儀。",
        "from": "T51n2090_001:0996a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後見月藏菩薩所問經。",
        "from": "T51n2090_001:0996a10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及聖教已。",
        "from": "T51n2090_001:0996a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即問尊師羅漢曰。",
        "from": "T51n2090_001:0996a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛滅度後。",
        "from": "T51n2090_001:0996a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "于闐疎勒及與安息。",
        "from": "T51n2090_001:0996a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來像法窣覩波等。",
        "from": "T51n2090_001:0996a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾時住世。",
        "from": "T51n2090_001:0996a12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誰當毀滅。",
        "from": "T51n2090_001:0996a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "究竟至其何所。",
        "from": "T51n2090_001:0996a13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願為解說。",
        "from": "T51n2090_001:0996a13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時羅漢即□□日□□□□□□□□□釋迦牟尼如來滅後。",
        "from": "T51n2090_001:0996a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼法影像及以塔廟。",
        "from": "T51n2090_001:0996a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二千年在世。",
        "from": "T51n2090_001:0996a15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後滅沒。",
        "from": "T51n2090_001:0996a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "于闐等此三之國。",
        "from": "T51n2090_001:0996a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漢與赤面．蘇毘．突厥。",
        "from": "T51n2090_001:0996a16_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迴鶻等賊。",
        "from": "T51n2090_001:0996a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "動其干戈而來侵損。",
        "from": "T51n2090_001:0996a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故佛法以漸衰微。",
        "from": "T51n2090_001:0996a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毀滅塔寺。",
        "from": "T51n2090_001:0996a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾僧資具亦皆斷絕。",
        "from": "T51n2090_001:0996a18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此三國中。",
        "from": "T51n2090_001:0996a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安息疎勒不行法賊被侵。",
        "from": "T51n2090_001:0996a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故擾塔寺。",
        "from": "T51n2090_001:0996a19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多分焚燒。",
        "from": "T51n2090_001:0996a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毀滅皆悉。",
        "from": "T51n2090_001:0996a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空棄諸寺。",
        "from": "T51n2090_001:0996a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾僧多分移從于闐國。",
        "from": "T51n2090_001:0996a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "于闐塔寺。",
        "from": "T51n2090_001:0996a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五百菩薩常護持。",
        "from": "T51n2090_001:0996a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故二百五十以出家儀。",
        "from": "T51n2090_001:0996a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二百五十在於俗徒。",
        "from": "T51n2090_001:0996a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受生護持。",
        "from": "T51n2090_001:0996a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牛頭山寺。",
        "from": "T51n2090_001:0996a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢劫一千五佛常當履踐。",
        "from": "T51n2090_001:0996a23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為宮殿。",
        "from": "T51n2090_001:0996a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸賢聖威德慈悲加持于闐塔寺妙法。",
        "from": "T51n2090_001:0996a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故行法人多於餘國。",
        "from": "T51n2090_001:0996a25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "久住於世。",
        "from": "T51n2090_001:0996a25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時諸國王等。",
        "from": "T51n2090_001:0996a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲取於于闐國故鬪諍紛紜。",
        "from": "T51n2090_001:0996a26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何王得者。",
        "from": "T51n2090_001:0996a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即與于闐作大施主。",
        "from": "T51n2090_001:0996a27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不毀不滅。",
        "from": "T51n2090_001:0996a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常當供養。",
        "from": "T51n2090_001:0996a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當爾之時。",
        "from": "T51n2090_001:0996a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "赤面國王有大威勢。",
        "from": "T51n2090_001:0996a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多侵餘國。",
        "from": "T51n2090_001:0996a28_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為自境。",
        "from": "T51n2090_001:0996a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時有一菩薩。",
        "from": "T51n2090_001:0996a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於赤面國受生。",
        "from": "T51n2090_001:0996a29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為王於自國內廣行妙法。",
        "from": "T51n2090_001:0996b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從於他國。",
        "from": "T51n2090_001:0996b01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請其法師及經論。",
        "from": "T51n2090_001:0996b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "赤面國中。",
        "from": "T51n2090_001:0996b02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建立精舍。",
        "from": "T51n2090_001:0996b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造窣堵波。",
        "from": "T51n2090_001:0996b02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "度□□眾。",
        "from": "T51n2090_001:0996b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國王大臣并諸國人。",
        "from": "T51n2090_001:0996b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣行正法。",
        "from": "T51n2090_001:0996b03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時于闐屬彼赤面王。",
        "from": "T51n2090_001:0996b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故廣行正法。",
        "from": "T51n2090_001:0996b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建立塔寺。",
        "from": "T51n2090_001:0996b04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置其三寶人戶田園。",
        "from": "T51n2090_001:0996b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "興大供養。",
        "from": "T51n2090_001:0996b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "赤面國王。",
        "from": "T51n2090_001:0996b05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七代已來以行妙。",
        "from": "T51n2090_001:0996b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此七代王。",
        "from": "T51n2090_001:0996b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於餘國中所有三寶及塔寺處。",
        "from": "T51n2090_001:0996b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不起惡心。",
        "from": "T51n2090_001:0996b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不損害。",
        "from": "T51n2090_001:0996b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時于闐眾僧。",
        "from": "T51n2090_001:0996b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如月藏菩薩受記經中所說。",
        "from": "T51n2090_001:0996b08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多分信心漸薄。",
        "from": "T51n2090_001:0996b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不於戒法。",
        "from": "T51n2090_001:0996b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求世利譽。",
        "from": "T51n2090_001:0996b09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入於王臣謀密之事。",
        "from": "T51n2090_001:0996b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令其正法漸漸衰秏。",
        "from": "T51n2090_001:0996b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時王臣及以子孫。",
        "from": "T51n2090_001:0996b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "退失淨信。",
        "from": "T51n2090_001:0996b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常住所有一切諸物。",
        "from": "T51n2090_001:0996b11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苾芻秏己用。",
        "from": "T51n2090_001:0996b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故于闐塔寺正法常住所屬諸物。",
        "from": "T51n2090_001:0996b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不同往日。",
        "from": "T51n2090_001:0996b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漸當衰秏。",
        "from": "T51n2090_001:0996b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國王大臣。",
        "from": "T51n2090_001:0996b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於出家眾不生歡喜。",
        "from": "T51n2090_001:0996b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後於異時有一菩薩。",
        "from": "T51n2090_001:0996b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為赤面國第七代王。",
        "from": "T51n2090_001:0996b14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼王納漢菩薩公主以為妃。",
        "from": "T51n2090_001:0996b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "后將六百侍從至赤面國。",
        "from": "T51n2090_001:0996b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時彼公主極信佛法。",
        "from": "T51n2090_001:0996b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大具福德。",
        "from": "T51n2090_001:0996b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "赤面國王亦大淨信。",
        "from": "T51n2090_001:0996b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過於先代。",
        "from": "T51n2090_001:0996b17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣興正法。",
        "from": "T51n2090_001:0996b17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是之時。",
        "from": "T51n2090_001:0996b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "于闐國王年少不行佛之正法。",
        "from": "T51n2090_001:0996b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先諸苾芻曰。",
        "from": "T51n2090_001:0996b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不然歸俗。",
        "from": "T51n2090_001:0996b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不然隨意。",
        "from": "T51n2090_001:0996b19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出向他處。",
        "from": "T51n2090_001:0996b19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以逼逐故。",
        "from": "T51n2090_001:0996b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾僧集拶摩寺。",
        "from": "T51n2090_001:0996b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "評議是事。",
        "from": "T51n2090_001:0996b20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時寺內眾前。",
        "from": "T51n2090_001:0996b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忽現七分金寶之食。",
        "from": "T51n2090_001:0996b21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五千苾芻王斷其食窘急之者。",
        "from": "T51n2090_001:0996b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "易彼金食為三月糧。",
        "from": "T51n2090_001:0996b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當爾之時。",
        "from": "T51n2090_001:0996b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有歸俗。",
        "from": "T51n2090_001:0996b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有不歸俗。",
        "from": "T51n2090_001:0996b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往他方者。",
        "from": "T51n2090_001:0996b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別離父母親戚眷屬及本生國。",
        "from": "T51n2090_001:0996b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "極生憂惱。",
        "from": "T51n2090_001:0996b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放奔大哭。",
        "from": "T51n2090_001:0996b24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧徒眾會。",
        "from": "T51n2090_001:0996b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進路往至牟吽寺中。",
        "from": "T51n2090_001:0996b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼處有一大窣堵波。",
        "from": "T51n2090_001:0996b25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大淨信□伯神王。",
        "from": "T51n2090_001:0996b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開折彼塔。",
        "from": "T51n2090_001:0996b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出一金捥盛滿珎珠。",
        "from": "T51n2090_001:0996b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以施眾僧。",
        "from": "T51n2090_001:0996b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復更有余淨信施主。",
        "from": "T51n2090_001:0996b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辦眾資具。",
        "from": "T51n2090_001:0996b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以施其僧。",
        "from": "T51n2090_001:0996b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是次第眾僧漸次至奴盧川。",
        "from": "T51n2090_001:0996b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以彼珎珠為半月糧。",
        "from": "T51n2090_001:0996b29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時眾僧至奴盧川。",
        "from": "T51n2090_001:0996b29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在宋多紇恭娘寺。",
        "from": "T51n2090_001:0996c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時多聞天王及功德天變為夫妻。",
        "from": "T51n2090_001:0996c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住在彼處。",
        "from": "T51n2090_001:0996c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬白僧言。",
        "from": "T51n2090_001:0996c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等作福。",
        "from": "T51n2090_001:0996c02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願垂納受。",
        "from": "T51n2090_001:0996c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "半月已來設大施會。",
        "from": "T51n2090_001:0996c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恭養僧眾。",
        "from": "T51n2090_001:0996c03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功德天女出一衣袖金寶之錢。",
        "from": "T51n2090_001:0996c04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以施眾僧。",
        "from": "T51n2090_001:0996c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時僧眾俱從赤面大番之國行至破山。",
        "from": "T51n2090_001:0996c05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遇守關人留難不放。",
        "from": "T51n2090_001:0996c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而作是言。",
        "from": "T51n2090_001:0996c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘處有路。",
        "from": "T51n2090_001:0996c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一任當往。",
        "from": "T51n2090_001:0996c06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時多聞天王自變其身為一白犛牛有其脊瘡并有棬紖。",
        "from": "T51n2090_001:0996c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾僧見之互相謂曰。",
        "from": "T51n2090_001:0996c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此脊瘡犛牛是人之畜。",
        "from": "T51n2090_001:0996c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此往何處。",
        "from": "T51n2090_001:0996c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨後當行。",
        "from": "T51n2090_001:0996c09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時彼犛牛以引直路於四五間一切眾僧至赤面國薩毘之境。",
        "from": "T51n2090_001:0996c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時薩毘軍當道節度多聞眾僧從西來至。",
        "from": "T51n2090_001:0996c11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "走白赤面王知。",
        "from": "T51n2090_001:0996c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時彼王夫人聞多眾僧失土波迸。",
        "from": "T51n2090_001:0996c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白其王曰。",
        "from": "T51n2090_001:0996c13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我辦畜乘及以資具。",
        "from": "T51n2090_001:0996c13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願請眾僧。",
        "from": "T51n2090_001:0996c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來至赤面國。",
        "from": "T51n2090_001:0996c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王亦許之。",
        "from": "T51n2090_001:0996c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以辦畜乘。",
        "from": "T51n2090_001:0996c14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便迎眾僧。",
        "from": "T51n2090_001:0996c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至赤面國。",
        "from": "T51n2090_001:0996c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時赤面國王公主侍從供養禮拜。",
        "from": "T51n2090_001:0996c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問眾僧中善解三藏諸大德曰。",
        "from": "T51n2090_001:0996c16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除汝更有波迸者不。",
        "from": "T51n2090_001:0996c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸三藏大德謹對之曰。",
        "from": "T51n2090_001:0996c17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安息．疎勒．勃律．加悉蜜國。",
        "from": "T51n2090_001:0996c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有如是眾多僧眾。",
        "from": "T51n2090_001:0996c18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "波迸流移現在彼處。",
        "from": "T51n2090_001:0996c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即便發使迎請眾僧至赤面國。",
        "from": "T51n2090_001:0996c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時赤面國置七所寺。",
        "from": "T51n2090_001:0996c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辦諸供具。",
        "from": "T51n2090_001:0996c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常住人戶倍勝往曰。",
        "from": "T51n2090_001:0996c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安置眾僧住在七寺。",
        "from": "T51n2090_001:0996c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後經三四年。",
        "from": "T51n2090_001:0996c21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公主心上有惡瘡出。",
        "from": "T51n2090_001:0996c22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "病苦之時。",
        "from": "T51n2090_001:0996c22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公主白王。",
        "from": "T51n2090_001:0996c22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妾因此疾終不得免。",
        "from": "T51n2090_001:0996c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有僮僕及以財物。",
        "from": "T51n2090_001:0996c23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願施三寶。",
        "from": "T51n2090_001:0996c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王亦許之。",
        "from": "T51n2090_001:0996c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六百侍從悉放出家。",
        "from": "T51n2090_001:0996c24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後公主命將終盡。",
        "from": "T51n2090_001:0996c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公主終後。",
        "from": "T51n2090_001:0996c25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "赤面國王境界之內。",
        "from": "T51n2090_001:0996c25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豆瘡病起。",
        "from": "T51n2090_001:0996c26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大臣百官并諸子孫而死者眾。",
        "from": "T51n2090_001:0996c26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時彼群臣而集會之白其王曰。",
        "from": "T51n2090_001:0996c27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王國界內。",
        "from": "T51n2090_001:0996c27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先無如是瘡苦病惱。",
        "from": "T51n2090_001:0996c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今諸波迸戎夷僧眾來到此處。",
        "from": "T51n2090_001:0996c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公主崩逝。",
        "from": "T51n2090_001:0996c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大臣百官多有死者。",
        "from": "T51n2090_001:0996c29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故此諸出家之眾。",
        "from": "T51n2090_001:0997a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不留王界。",
        "from": "T51n2090_001:0997a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "理合驅出。",
        "from": "T51n2090_001:0997a01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王先之日。",
        "from": "T51n2090_001:0997a01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合驅不驅。",
        "from": "T51n2090_001:0997a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "委細詳之。",
        "from": "T51n2090_001:0997a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "群臣同心。",
        "from": "T51n2090_001:0997a02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樂驅眾僧。",
        "from": "T51n2090_001:0997a02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以白王知。",
        "from": "T51n2090_001:0997a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲驅眾僧出於王界。",
        "from": "T51n2090_001:0997a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時赤面國王境內舊住苾芻瞋恚而言。",
        "from": "T51n2090_001:0997a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若以驅逐此諸僧。",
        "from": "T51n2090_001:0997a04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等亦皆不住於此。",
        "from": "T51n2090_001:0997a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸臣恚言。",
        "from": "T51n2090_001:0997a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等亦當隨意而去。",
        "from": "T51n2090_001:0997a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公主來至赤面國後。",
        "from": "T51n2090_001:0997a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漢王興崇道士法。",
        "from": "T51n2090_001:0997a06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故一切漢僧悉皆來至赤面國界。",
        "from": "T51n2090_001:0997a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是之時。",
        "from": "T51n2090_001:0997a07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼界眾僧。",
        "from": "T51n2090_001:0997a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "赤面國內所有舍利聖教經論供養諸具常住財物。",
        "from": "T51n2090_001:0997a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡皆齎持。",
        "from": "T51n2090_001:0997a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往至於西大乾陀羅國。",
        "from": "T51n2090_001:0997a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當爾之時。",
        "from": "T51n2090_001:0997a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漢與赤面婆羅行國于闐國等直至恒河。",
        "from": "T51n2090_001:0997a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "像法滅沒更無有餘。",
        "from": "T51n2090_001:0997a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒河彼岸俱閃彌國像法三月住世。",
        "from": "T51n2090_001:0997a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最後滅盡。",
        "from": "T51n2090_001:0997a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切僧眾至乾陀羅界伊羅葉龍王所住海岸。",
        "from": "T51n2090_001:0997a13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三寶威力。",
        "from": "T51n2090_001:0997a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼海騰波。",
        "from": "T51n2090_001:0997a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍王思惟。",
        "from": "T51n2090_001:0997a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我所住處何故騰波。",
        "from": "T51n2090_001:0997a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以天眼觀知釋迦牟尼如來聖教像法近滅末後見僧之期。",
        "from": "T51n2090_001:0997a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自變其身為一老人。",
        "from": "T51n2090_001:0997a16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從海而出。",
        "from": "T51n2090_001:0997a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮彼眾僧。",
        "from": "T51n2090_001:0997a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以問之曰。",
        "from": "T51n2090_001:0997a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是眾僧從於何來。",
        "from": "T51n2090_001:0997a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往至何所。",
        "from": "T51n2090_001:0997a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾僧答曰。",
        "from": "T51n2090_001:0997a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等本居赤面國界。",
        "from": "T51n2090_001:0997a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施主無信。",
        "from": "T51n2090_001:0997a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毀滅常住及精舍。",
        "from": "T51n2090_001:0997a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故我等往至大乾陀羅國。",
        "from": "T51n2090_001:0997a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍王問曰。",
        "from": "T51n2090_001:0997a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾僧極廣。",
        "from": "T51n2090_001:0997a20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有何糧食。",
        "from": "T51n2090_001:0997a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大乾陀羅國遶海而去四十日餘。",
        "from": "T51n2090_001:0997a21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始達彼處。",
        "from": "T51n2090_001:0997a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今現眾僧唯有二十日糧。",
        "from": "T51n2090_001:0997a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何達彼。",
        "from": "T51n2090_001:0997a22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾僧聞已。",
        "from": "T51n2090_001:0997a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆大啼哭。",
        "from": "T51n2090_001:0997a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍王見已。",
        "from": "T51n2090_001:0997a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "極生憂惱。",
        "from": "T51n2090_001:0997a23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "語僧眾曰。",
        "from": "T51n2090_001:0997a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此有直路。",
        "from": "T51n2090_001:0997a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有其蛇橋。",
        "from": "T51n2090_001:0997a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能上往者速達彼處。",
        "from": "T51n2090_001:0997a24_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是語訖。",
        "from": "T51n2090_001:0997a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "老人不見。",
        "from": "T51n2090_001:0997a25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時彼龍王現一登山之路。",
        "from": "T51n2090_001:0997a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自變其身為一大蛇。",
        "from": "T51n2090_001:0997a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "海上作橋。",
        "from": "T51n2090_001:0997a26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是思惟。",
        "from": "T51n2090_001:0997a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今若度此諸僧眾。",
        "from": "T51n2090_001:0997a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先作惡業。",
        "from": "T51n2090_001:0997a27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今受傍生之身。",
        "from": "T51n2090_001:0997a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦可得脫。",
        "from": "T51n2090_001:0997a28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發是願矣。",
        "from": "T51n2090_001:0997a28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便置蛇橋十五日餘。",
        "from": "T51n2090_001:0997a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾多人畜於上而過。",
        "from": "T51n2090_001:0997a29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蛇橋之上人畜過時。",
        "from": "T51n2090_001:0997a29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有墮水而命終者。",
        "from": "T51n2090_001:0997b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是人畜往來去故。",
        "from": "T51n2090_001:0997b01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其蛇脊背破裂毀壞。",
        "from": "T51n2090_001:0997b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "血流而雨。",
        "from": "T51n2090_001:0997b02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "海變為血。",
        "from": "T51n2090_001:0997b02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾多僧眾於蛇橋上而往過。",
        "from": "T51n2090_001:0997b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時末後餘殘赤面留難而不放過。",
        "from": "T51n2090_001:0997b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後大蛇墮於海中而命終已。",
        "from": "T51n2090_001:0997b04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得生兜率陀天。",
        "from": "T51n2090_001:0997b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "海亦枯竭。",
        "from": "T51n2090_001:0997b05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時僧眾到乾陀羅國。",
        "from": "T51n2090_001:0997b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼國之王具辦供具。",
        "from": "T51n2090_001:0997b06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二年供養各令安樂。",
        "from": "T51n2090_001:0997b07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經二年已。",
        "from": "T51n2090_001:0997b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼王捨壽。",
        "from": "T51n2090_001:0997b07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王有二子。",
        "from": "T51n2090_001:0997b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一信佛法。",
        "from": "T51n2090_001:0997b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一行外道法。",
        "from": "T51n2090_001:0997b08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後爭王位。",
        "from": "T51n2090_001:0997b08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其時僧眾為信法王子作用儻。",
        "from": "T51n2090_001:0997b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故彼得勝已而紹王位。",
        "from": "T51n2090_001:0997b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼王統治之時。",
        "from": "T51n2090_001:0997b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加僧俸祿資緣具足。",
        "from": "T51n2090_001:0997b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經半年已。",
        "from": "T51n2090_001:0997b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有一苾芻。",
        "from": "T51n2090_001:0997b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殺却彼王。",
        "from": "T51n2090_001:0997b11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自紹王位。",
        "from": "T51n2090_001:0997b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乾陀羅國一切人眾。",
        "from": "T51n2090_001:0997b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時而反殺苾芻王。",
        "from": "T51n2090_001:0997b12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸餘僧眾盡皆驅逐。",
        "from": "T51n2090_001:0997b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出王界外。",
        "from": "T51n2090_001:0997b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故乾陀羅國像法亦滅。",
        "from": "T51n2090_001:0997b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切僧眾波迸流離。",
        "from": "T51n2090_001:0997b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向在餘國。",
        "from": "T51n2090_001:0997b14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是之時。",
        "from": "T51n2090_001:0997b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方國王．北方國王．葉婆那王等祿福勝前。",
        "from": "T51n2090_001:0997b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此三國王會盟一家。",
        "from": "T51n2090_001:0997b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時彼三王化治西方及北方等。",
        "from": "T51n2090_001:0997b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時彼三王各將十萬兵攻俱閃彌國。",
        "from": "T51n2090_001:0997b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王以滅三十萬兵及其王等一不餘殘。",
        "from": "T51n2090_001:0997b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時俱閃彌王為欲懺除殺眾兵罪。",
        "from": "T51n2090_001:0997b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "召請閻浮界內一切眾僧。",
        "from": "T51n2090_001:0997b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至俱閃彌國。",
        "from": "T51n2090_001:0997b20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後因眾像自內鬪諍。",
        "from": "T51n2090_001:0997b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂互相殺一無餘殘。",
        "from": "T51n2090_001:0997b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故閻浮界佛之像法從茲滅盡。",
        "from": "T51n2090_001:0997b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一廣明如月藏菩薩受記經說。",
        "from": "T51n2090_001:0997b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋迦如來般涅槃後經五十七俱胝六兆年歲。",
        "from": "T51n2090_001:0997b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈氏世尊現娑婆界。",
        "from": "T51n2090_001:0997b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化治有情。",
        "from": "T51n2090_001:0997b24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋迦牟尼如來像法滅盡因緣一卷",
        "from": "T51n2090_001:0997b25_100",
        "context_with_next_sentence": false
    }
]