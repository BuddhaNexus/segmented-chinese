[
    {
        "source_sentence": "阿吒薄𤘽付囑呪",
        "from": "T21n1240_001:0202b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無阿吒薄𤘽摩俱吒耶　摩訶藥叉那耶",
        "from": "T21n1240_001:0202b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　摩醯首羅耶泮毘摩質多羅耶泮　那吒鳩",
        "from": "T21n1240_001:0202b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伐羅泮呵呵呵呵泮泮泮唅引　曳吒曳吒曳吒曳吒　尼藍婆耶乾馱婆耶毘舍闍耶莎訶南無薄伽梵跋折羅軍荼利耶泮　跋折唎薄悉陀耶泮　薄伽跋帝阿吒婆拘耶泮　毘沙門那泮波波吒吒耶耶泮泮　因陀羅藥叉賴耶泮毘盧釋迦耶泮　毘盧博芻藥叉那泮婆羅摩泮速速去去馺婆訶　摩訶藥叉馺婆訶伽伽伽伽羅羅泮泮泮泮　摩尼跋陀羅泮那羅延都泮　摩訶迦羅泮阿耨陀耶泮甌婆難陀泮蘇耆羅耶泮　南單那泮黑布單那泮闍摩羅闍耶泮　目真隣耶泮蘇盧鳩槃茶唅引泮善女功德耶泮降怨泮泮　泮泮藥叉藥叉速速速速馺婆呵摩訶藥叉藥叉速速速速馺婆呵阿阿阿阿阿阿鉢鉢遮遮勅勅勅勅跋跋羅羅跋折羅荷長引吒吒吒吒曳泮吒馺婆呵吞攝馺婆訶阿吒阿吒阿呵阿呵呵呵唅引泮斫迦羅護帝跋折羅力力力力拔拔拔拔急急急急頻陀攝持師子王吼吼吼吼呵呵呵呵泮吒阿吒薄𤘽曳莎呵此呪急惡。",
        "from": "T21n1240_001:0202c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不清淨請不輒誦。",
        "from": "T21n1240_001:0202c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有欲心。",
        "from": "T21n1240_001:0202c03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我遣鬼王把金輪破汝頭。",
        "from": "T21n1240_001:0202c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法誦舍利。",
        "from": "T21n1240_001:0202c04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸法從緣起。",
        "from": "T21n1240_001:0202c04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來說是因。",
        "from": "T21n1240_001:0202c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼法因緣盡。",
        "from": "T21n1240_001:0202c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是大沙門說。",
        "from": "T21n1240_001:0202c05_17",
        "context_with_next_sentence": true
    }
