[
    {
        "source_sentence": "金剛壽命陀羅尼經",
        "from": "T20n1134B_001:0577b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "特進試鴻臚卿大辨正",
        "from": "T20n1134B_001:0577b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大廣智三藏奉　詔譯",
        "from": "T20n1134B_001:0577b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時毘盧遮那如來。",
        "from": "T20n1134B_001:0577b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於色界頂第四禪成等正覺。",
        "from": "T20n1134B_001:0577b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即下須彌山頂。",
        "from": "T20n1134B_001:0577b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於金剛寶樓閣。",
        "from": "T20n1134B_001:0577b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡虛空遍法界一切如來皆悉雲集。",
        "from": "T20n1134B_001:0577b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後圍繞異口同音。",
        "from": "T20n1134B_001:0577b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊轉妙法輪甚深祕密。",
        "from": "T20n1134B_001:0577b22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂金剛界輪。",
        "from": "T20n1134B_001:0577b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降三世教令輪。",
        "from": "T20n1134B_001:0577b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍調伏法輪。",
        "from": "T20n1134B_001:0577b23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切義成就輪。",
        "from": "T20n1134B_001:0577b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是四輪從毘盧遮那心出。",
        "from": "T20n1134B_001:0577b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一輪皆有三十七聖者。",
        "from": "T20n1134B_001:0577b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一真言一一三摩地。",
        "from": "T20n1134B_001:0577b25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一契印威儀軌持。",
        "from": "T20n1134B_001:0577b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大悲願力。",
        "from": "T20n1134B_001:0577b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於雜染佛剎。",
        "from": "T20n1134B_001:0577b26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於淨妙世界。",
        "from": "T20n1134B_001:0577b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或隱或顯。",
        "from": "T20n1134B_001:0577b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉輪利樂度諸眾生各各不同。",
        "from": "T20n1134B_001:0577b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘盧遮那佛。",
        "from": "T20n1134B_001:0577b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受諸如來請已。",
        "from": "T20n1134B_001:0577b28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲轉法輪。",
        "from": "T20n1134B_001:0577b28_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時即入三摩地。",
        "from": "T20n1134B_001:0577b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀見摩醯首羅大自在天。",
        "from": "T20n1134B_001:0577b29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剛強難化執持邪見。",
        "from": "T20n1134B_001:0577c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非我寂靜大悲之身堪任調伏。",
        "from": "T20n1134B_001:0577c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是世尊入悲怒三摩地。",
        "from": "T20n1134B_001:0577c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從胸臆間出現五峯金剛大菩提心。",
        "from": "T20n1134B_001:0577c03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流出四面八臂威德熾盛奇特難覩降三世金剛菩薩身。",
        "from": "T20n1134B_001:0577c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍禮毘盧遮那如來及一切諸佛。",
        "from": "T20n1134B_001:0577c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊示教於我。",
        "from": "T20n1134B_001:0577c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何所作為。",
        "from": "T20n1134B_001:0577c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告降三世菩薩。",
        "from": "T20n1134B_001:0577c06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今降伏彼難調伏諸大天等。",
        "from": "T20n1134B_001:0577c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令悉歸依諸佛法僧發菩提心。",
        "from": "T20n1134B_001:0577c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尋即降伏一切諸天。",
        "from": "T20n1134B_001:0577c08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡皆歸依佛法僧眾。",
        "from": "T20n1134B_001:0577c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯大自在天。",
        "from": "T20n1134B_001:0577c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "特大威德而生拒敵。",
        "from": "T20n1134B_001:0577c09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降三世菩薩種種苦治乃至於死左踏大天右踏天后於是毘盧遮那如來。",
        "from": "T20n1134B_001:0577c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入大悲愍三摩地已。",
        "from": "T20n1134B_001:0577c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說金剛壽命陀羅尼。",
        "from": "T20n1134B_001:0577c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復入金剛壽命三昧耶。",
        "from": "T20n1134B_001:0577c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及結密印加持摩醯首羅天王。",
        "from": "T20n1134B_001:0577c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還復得甦重增壽命。",
        "from": "T20n1134B_001:0577c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸依諸佛發菩提心。",
        "from": "T20n1134B_001:0577c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灌頂受記證得八地位。",
        "from": "T20n1134B_001:0577c14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛壽命真言。",
        "from": "T20n1134B_001:0577c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵􄙖嚩􄦽日囉二合􄗵喻􄙵曬􅁥薩嚩二合􄚩訶引佛告執金剛菩薩。",
        "from": "T20n1134B_001:0577c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人。",
        "from": "T20n1134B_001:0577c18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持念誦每日三時時別千遍。",
        "from": "T20n1134B_001:0577c19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過現所有惡業因緣短命夭壽。",
        "from": "T20n1134B_001:0577c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由持此真言故信心清淨。",
        "from": "T20n1134B_001:0577c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "業障消滅更增壽命。",
        "from": "T20n1134B_001:0577c21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有修習三摩地者。",
        "from": "T20n1134B_001:0577c21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現生不轉父母生身。",
        "from": "T20n1134B_001:0577c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲五神通陵空自在。",
        "from": "T20n1134B_001:0577c22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當誦護身被甲真言曰。",
        "from": "T20n1134B_001:0577c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵􈂰砧讁簪反􄙖嚩􄦽日囉二合􈃷欲次當觀􄗼欲字。",
        "from": "T20n1134B_001:0577c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光明晃曜遍照。",
        "from": "T20n1134B_001:0577c26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成大金剛壽命菩薩。",
        "from": "T20n1134B_001:0577c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以四字明引請菩薩。",
        "from": "T20n1134B_001:0577c27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降入壇中受諸供養。",
        "from": "T20n1134B_001:0577c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四字密言曰。",
        "from": "T20n1134B_001:0577c28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄓁惹􆿁吽􄙠鑁􈁟斛爾時大眾聞佛所說信受奉行。",
        "from": "T20n1134B_001:0578a01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛壽命陀羅尼經延寶四年五月。",
        "from": "T20n1134B_001:0578a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寫栂尾藏本。",
        "from": "T20n1134B_001:0578a03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "律師淨嚴天明元辛丑年寫宇治慧心院本。",
        "from": "T20n1134B_001:0578a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈忍今年得件本令寫之。",
        "from": "T20n1134B_001:0578a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "挍勘以印版。",
        "from": "T20n1134B_001:0578a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓋此經者。",
        "from": "T20n1134B_001:0578a05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前經之略出。",
        "from": "T20n1134B_001:0578a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為不堪者唯示眼精。",
        "from": "T20n1134B_001:0578a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非是別本也。",
        "from": "T20n1134B_001:0578a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一挍加筆畢　　癸亥四月六日　慈順",
        "from": "T20n1134B_001:0578a08_0",
        "context_with_next_sentence": true
    }
]