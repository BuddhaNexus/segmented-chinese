[
    {
        "source_sentence": "諸經要略文",
        "from": "T85n2821_001:1204c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊婆須蜜經說。若為亡人設齋眾僧行食遍。",
        "from": "T85n2821_001:1204c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即為亡人下食。若不作者亡人不得食喫。",
        "from": "T85n2821_001:1204c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地獄寶即經說。",
        "from": "T85n2821_001:1204c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "齋家行食未遍出眾生食者。",
        "from": "T85n2821_001:1204c24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入截舌地獄。",
        "from": "T85n2821_001:1204c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "罪福決定經云。",
        "from": "T85n2821_001:1204c25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伽葉論說。",
        "from": "T85n2821_001:1204c25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩願一處者。",
        "from": "T85n2821_001:1204c26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "死作兩頭蛇遞相教示。",
        "from": "T85n2821_001:1204c26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大乘莊嚴論說。",
        "from": "T85n2821_001:1204c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "齋家佛盤上一顆一粒不許俗人。",
        "from": "T85n2821_001:1204c27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食食者得餓鬼報。",
        "from": "T85n2821_001:1204c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦云。",
        "from": "T85n2821_001:1204c28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通與虫鳥喫。",
        "from": "T85n2821_001:1204c28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入佛境界經說。",
        "from": "T85n2821_001:1204c28_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人食聖僧槃上食者。",
        "from": "T85n2821_001:1204c29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得無量罪。",
        "from": "T85n2821_001:1204c29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦云。",
        "from": "T85n2821_001:1204c29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通與守護伽藍人及與虫鳥喫。",
        "from": "T85n2821_001:1205a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "決定毘尼經說。",
        "from": "T85n2821_001:1205a01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應是齋家不問大小供養。",
        "from": "T85n2821_001:1205a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛盤上食不得食。",
        "from": "T85n2821_001:1205a02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上座應如法處分。",
        "from": "T85n2821_001:1205a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不爾者得無量罪。",
        "from": "T85n2821_001:1205a03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T85n2821_001:1205a03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "購贖得否准罪福決定經。",
        "from": "T85n2821_001:1205a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛且不求科故無人堪。",
        "from": "T85n2821_001:1205a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上座教云。",
        "from": "T85n2821_001:1205a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獻佛及僧行之則得無犯。",
        "from": "T85n2821_001:1205a05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不爾者千劫萬劫墮大地獄。",
        "from": "T85n2821_001:1205a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師若不教亦招前殃。",
        "from": "T85n2821_001:1205a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縱得人身九百萬歲在下賤中。",
        "from": "T85n2821_001:1205a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生曰。",
        "from": "T85n2821_001:1205a07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藏經云。",
        "from": "T85n2821_001:1205a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寧可一日然九百頭牛莫說三寶師僧長短。",
        "from": "T85n2821_001:1205a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其罪甚。",
        "from": "T85n2821_001:1205a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶集經不空過。",
        "from": "T85n2821_001:1205a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略說七。",
        "from": "T85n2821_001:1205a09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善提分亦名七寶泉。",
        "from": "T85n2821_001:1205a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名七聖財。",
        "from": "T85n2821_001:1205a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蛤聽法得生天緣。",
        "from": "T85n2821_001:1205a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出善見律毘婆抄要略。",
        "from": "T85n2821_001:1205a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔佛在世到膽婆國迦羅池邊。",
        "from": "T85n2821_001:1205a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為眾說法。",
        "from": "T85n2821_001:1205a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼池中有一蛤。",
        "from": "T85n2821_001:1205a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛池邊說法之聲。",
        "from": "T85n2821_001:1205a13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即從池出入草根下聽佛說法。",
        "from": "T85n2821_001:1205a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時有一人持杖放牛見佛在座為眾說法。",
        "from": "T85n2821_001:1205a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即往佛所欲聽聞故以杖刺地。",
        "from": "T85n2821_001:1205a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誤著蛤頭。",
        "from": "T85n2821_001:1205a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即命終生忉利天。",
        "from": "T85n2821_001:1205a16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以福報故宮殿縱廣十二由旬。",
        "from": "T85n2821_001:1205a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與諸天女娛樂受樂。",
        "from": "T85n2821_001:1205a17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生天已為聽法故得須陀洹果。",
        "from": "T85n2821_001:1205a18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳥聞比丘誦經得生天緣。",
        "from": "T85n2821_001:1205a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出賢愚經　昔佛在世時有比丘林中誦經。",
        "from": "T85n2821_001:1205a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "音聲雅好。",
        "from": "T85n2821_001:1205a20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有一鳥聞法。",
        "from": "T85n2821_001:1205a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在樹而聽。",
        "from": "T85n2821_001:1205a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時有獵師所射。",
        "from": "T85n2821_001:1205a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命終生忉利天。",
        "from": "T85n2821_001:1205a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面貌端政無有倫比。",
        "from": "T85n2821_001:1205a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自識宿命。",
        "from": "T85n2821_001:1205a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知因比丘見經聽法得生此中。",
        "from": "T85n2821_001:1205a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即持天花到比丘所。",
        "from": "T85n2821_001:1205a23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮敬問訊。",
        "from": "T85n2821_001:1205a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以天花香供養比丘。",
        "from": "T85n2821_001:1205a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比丘且問知其委由。",
        "from": "T85n2821_001:1205a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即命令座為其說法得須陀洹果。",
        "from": "T85n2821_001:1205a25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既得果已還歸天上。",
        "from": "T85n2821_001:1205a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禽獸聽法尚獲宿報無量無邊。",
        "from": "T85n2821_001:1205a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈況於人信心聽法寧無善根。",
        "from": "T85n2821_001:1205a27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鸚鵡聞說四諦得生天緣。",
        "from": "T85n2821_001:1205a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出賢愚經。",
        "from": "T85n2821_001:1205a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔佛在世時。",
        "from": "T85n2821_001:1205a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍衛國中須長者信敬佛法。",
        "from": "T85n2821_001:1205a29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為僧壇越眾僧所須一切供給。",
        "from": "T85n2821_001:1205b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須達家中有二鸚鵡。",
        "from": "T85n2821_001:1205b01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名律捉。",
        "from": "T85n2821_001:1205b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二名賒律提。",
        "from": "T85n2821_001:1205b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稟性黠慧解人言語。",
        "from": "T85n2821_001:1205b02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比丘見來先報家內令出丞迎。",
        "from": "T85n2821_001:1205b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難後時到長者家見鳥聰明。",
        "from": "T85n2821_001:1205b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為說四諦苦集滅道。",
        "from": "T85n2821_001:1205b04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "門前有樹。",
        "from": "T85n2821_001:1205b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二鳥聞法飛向樹上。",
        "from": "T85n2821_001:1205b05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡喜誦持夜在樹宿野貍所食。",
        "from": "T85n2821_001:1205b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣此善根生四王天忉利天等。",
        "from": "T85n2821_001:1205b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是七返受天已畢。",
        "from": "T85n2821_001:1205b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來生人中出家修道得壁支佛果。",
        "from": "T85n2821_001:1205b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名曇摩。",
        "from": "T85n2821_001:1205b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二名修曇摩。",
        "from": "T85n2821_001:1205b08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謹檢大小乘經。",
        "from": "T85n2821_001:1205b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食胡䕑菜得惡趣報。",
        "from": "T85n2821_001:1205b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大順經云。",
        "from": "T85n2821_001:1205b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食此菜者所生之處不得聞經佛法。",
        "from": "T85n2821_001:1205b10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華報經云。",
        "from": "T85n2821_001:1205b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食此菜者生生之處不覩諸佛被八增疾。",
        "from": "T85n2821_001:1205b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天畔經云。",
        "from": "T85n2821_001:1205b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誤食此菜不生悔。",
        "from": "T85n2821_001:1205b12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長劫處阿鼻地獄無有出期。",
        "from": "T85n2821_001:1205b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若故食者不通懺悔。",
        "from": "T85n2821_001:1205b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大集經云。",
        "from": "T85n2821_001:1205b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此菜者是天魔波旬變身作千年媚狐尿處因生此菜。",
        "from": "T85n2821_001:1205b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食者能滅道。",
        "from": "T85n2821_001:1205b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設今生不諸經典來生決定。",
        "from": "T85n2821_001:1205b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能厭聖教作外道一闡提。",
        "from": "T85n2821_001:1205b16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間一切罪極不過五逆盜常住物。",
        "from": "T85n2821_001:1205b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此不通懺悔。",
        "from": "T85n2821_001:1205b17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若對喫胡䕑人五逆盜常住僧物人吾即能救。",
        "from": "T85n2821_001:1205b18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喫胡䕑菜人自業深重厚吾不能救也。",
        "from": "T85n2821_001:1205b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩戒經中蘭葱是也。",
        "from": "T85n2821_001:1205b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食者結業罪。",
        "from": "T85n2821_001:1205b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍樹論云。",
        "from": "T85n2821_001:1205b20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人食胡䕑入佛塔中墮大地獄。",
        "from": "T85n2821_001:1205b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足十劫受大極苦花報。",
        "from": "T85n2821_001:1205b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作穢糞中經無量劫。",
        "from": "T85n2821_001:1205b22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五明論云。",
        "from": "T85n2821_001:1205b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食此菜者令人散亂失於正定。",
        "from": "T85n2821_001:1205b23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼神得便多非命所墮無間獄。",
        "from": "T85n2821_001:1205b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊者婆須蜜識其弟子婆眉多言。",
        "from": "T85n2821_001:1205b25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝宜護正法道引群生。",
        "from": "T85n2821_001:1205b25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便佛種不斷乃真出家。",
        "from": "T85n2821_001:1205b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當勤精進。",
        "from": "T85n2821_001:1205b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將來無食胡䕑五辛酒肉。",
        "from": "T85n2821_001:1205b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則十重戒自然清淨現世證果。",
        "from": "T85n2821_001:1205b27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人食胡䕑菜者。",
        "from": "T85n2821_001:1205b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縱齋戒具足死墮守廁中。",
        "from": "T85n2821_001:1205b28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神中功令斷之。",
        "from": "T85n2821_001:1205b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勸僧以俗守巖壇　　莫喫胡䕑觸佛顏",
        "from": "T85n2821_001:1205c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薩埵投岸由自辨　　菜中間擇有何難",
        "from": "T85n2821_001:1205c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修四正懃。",
        "from": "T85n2821_001:1205c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一惡法未生方便令不生。",
        "from": "T85n2821_001:1205c03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二惡法已生方便令除斷。",
        "from": "T85n2821_001:1205c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三善法未生方便令生。",
        "from": "T85n2821_001:1205c04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四善法已生方便令增長。",
        "from": "T85n2821_001:1205c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡從心出還從心滅。",
        "from": "T85n2821_001:1205c05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善從心生還從心長。",
        "from": "T85n2821_001:1205c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當懃心觀察此四種法。",
        "from": "T85n2821_001:1205c06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修四無量心。",
        "from": "T85n2821_001:1205c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一修慈心。",
        "from": "T85n2821_001:1205c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念一切眾生欲與其樂。",
        "from": "T85n2821_001:1205c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思惟作無量衣食等物施與眾生怨親平等。",
        "from": "T85n2821_001:1205c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆以土樂是名修慈。",
        "from": "T85n2821_001:1205c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二修悲心。",
        "from": "T85n2821_001:1205c09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念一切眾生欲拔其苦。",
        "from": "T85n2821_001:1205c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此名修悲心。",
        "from": "T85n2821_001:1205c10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三修喜心。",
        "from": "T85n2821_001:1205c10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念一切眾生。",
        "from": "T85n2821_001:1205c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若修善者我悉讚善。",
        "from": "T85n2821_001:1205c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若造惡者勸令斷惡。",
        "from": "T85n2821_001:1205c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當遇惡揚善。",
        "from": "T85n2821_001:1205c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四修捨心。",
        "from": "T85n2821_001:1205c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切眾生皆悉平等不見怨親等。",
        "from": "T85n2821_001:1205c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同空法也。",
        "from": "T85n2821_001:1205c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修六妙行。",
        "from": "T85n2821_001:1205c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一識不與色觀繫心不染著眼妙行明者青黃朱二耳不著聲。",
        "from": "T85n2821_001:1205c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三鼻不染香。",
        "from": "T85n2821_001:1205c15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四舌不含味。",
        "from": "T85n2821_001:1205c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五身不著細滑等。",
        "from": "T85n2821_001:1205c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六意不思惟惡法。",
        "from": "T85n2821_001:1205c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四恩者。",
        "from": "T85n2821_001:1205c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一父母恩。",
        "from": "T85n2821_001:1205c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能生己身得有今日。",
        "from": "T85n2821_001:1205c17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二師僧恩。",
        "from": "T85n2821_001:1205c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生我法身以法施我。",
        "from": "T85n2821_001:1205c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三國王恩。",
        "from": "T85n2821_001:1205c18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救我課沒水土草木常以施我。",
        "from": "T85n2821_001:1205c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四檀越恩。",
        "from": "T85n2821_001:1205c19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供給衣食濟我身命。",
        "from": "T85n2821_001:1205c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四輩檀越。",
        "from": "T85n2821_001:1205c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一天輩天厨甘露獻出家。",
        "from": "T85n2821_001:1205c21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二龍輩牛鹿獻乳猨猴。",
        "from": "T85n2821_001:1205c22_6",
        "context_with_next_sentence": true
    }
]