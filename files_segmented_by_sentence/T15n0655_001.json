[
    {
        "source_sentence": "佛說勝義空經",
        "from": "T15n0655_001:0806c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝奉大夫試光祿卿",
        "from": "T15n0655_001:0806c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳法大師賜紫沙門臣施護等奉　詔譯",
        "from": "T15n0655_001:0806c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛世尊一時在俱盧數國，與苾芻眾俱。",
        "from": "T15n0655_001:0806c27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告諸苾芻言：",
        "from": "T15n0655_001:0806c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「諸有法門，我如前說；",
        "from": "T15n0655_001:0806c28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有正法，名勝義空，汝等諦聽，極善作意，今為汝說。",
        "from": "T15n0655_001:0807a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「諸苾芻！",
        "from": "T15n0655_001:0807a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中云何名勝義空？",
        "from": "T15n0655_001:0807a01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂：",
        "from": "T15n0655_001:0807a01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼生時而無少法有所從來；",
        "from": "T15n0655_001:0807a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又眼滅時亦無少法離散可去。",
        "from": "T15n0655_001:0807a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「諸苾芻！",
        "from": "T15n0655_001:0807a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其眼無實，離於實法。",
        "from": "T15n0655_001:0807a03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以要而言：",
        "from": "T15n0655_001:0807a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有業有報，作者不可得，此蘊既終，復他蘊攝；",
        "from": "T15n0655_001:0807a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別法合集，因緣所生。",
        "from": "T15n0655_001:0807a05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耳、鼻、舌、身、意亦復如是。",
        "from": "T15n0655_001:0807a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼意生時，而無少法有所從來；",
        "from": "T15n0655_001:0807a06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又意滅時，亦無少法離散可去。",
        "from": "T15n0655_001:0807a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「諸苾芻！",
        "from": "T15n0655_001:0807a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼意無實，離於實法，亦如前說，別法合集。",
        "from": "T15n0655_001:0807a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此合集法，無實可得，因緣所生。",
        "from": "T15n0655_001:0807a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此緣生者，所謂：",
        "from": "T15n0655_001:0807a09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無明緣行、行緣識、識緣名色、名色緣六處、六處緣觸、觸緣受、受緣愛、愛緣取、取緣有、有緣生、生緣老死憂悲苦惱，如是即一大苦蘊生。",
        "from": "T15n0655_001:0807a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此所生法，無實可得，生已即滅。",
        "from": "T15n0655_001:0807a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由是無明滅即行滅、行滅即識滅、識滅即名色滅、名色滅即六處滅、六處滅即觸滅、觸滅即受滅、受滅即愛滅、愛滅即取滅、取滅即有滅、有滅即生滅、生滅即老死憂悲苦惱滅，如是即一大苦蘊滅。",
        "from": "T15n0655_001:0807a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等所說，是為勝義空。",
        "from": "T15n0655_001:0807a18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「諸苾芻！",
        "from": "T15n0655_001:0807a18_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸有法門，如前已說，今此正法，汝等當學。",
        "from": "T15n0655_001:0807a19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」佛說勝義空經",
        "from": "T15n0655_001:0807a20_100",
        "context_with_next_sentence": false
    }
]