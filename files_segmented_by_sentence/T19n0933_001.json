[
    {
        "source_sentence": "九品往生阿彌陀三摩地集陀",
        "from": "T19n0933_001:0079b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅尼經",
        "from": "T19n0933_001:0079b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "特進試鴻臚卿大興善寺三藏",
        "from": "T19n0933_001:0079b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙門大廣智不空奉　詔譯爾時毘盧遮那如來。",
        "from": "T19n0933_001:0079b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於大三摩地門大精舍。",
        "from": "T19n0933_001:0079b26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大比丘眾八萬九千人俱。",
        "from": "T19n0933_001:0079b27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆是大阿羅漢。",
        "from": "T19n0933_001:0079b27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧善具足所作皆辦。",
        "from": "T19n0933_001:0079b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其名曰神力智辨觀世音菩薩得大勢菩薩。",
        "from": "T19n0933_001:0079b29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神通自在王菩薩淨光無垢陀羅尼菩薩。",
        "from": "T19n0933_001:0079c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大力普聞菩薩大莊嚴力菩薩。",
        "from": "T19n0933_001:0079c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量光菩薩慧善慧普光王菩薩。",
        "from": "T19n0933_001:0079c02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等大菩薩聲聞大眾。",
        "from": "T19n0933_001:0079c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往詣佛所白言世尊。",
        "from": "T19n0933_001:0079c03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量壽國在九品淨識三摩地。",
        "from": "T19n0933_001:0079c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是即諸佛境界如來所居。",
        "from": "T19n0933_001:0079c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三世諸佛從是成正覺。",
        "from": "T19n0933_001:0079c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足三明增長福慧。",
        "from": "T19n0933_001:0079c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其九品境界。",
        "from": "T19n0933_001:0079c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上品上生真色地。",
        "from": "T19n0933_001:0079c06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上品中生無垢地。",
        "from": "T19n0933_001:0079c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上品下生離垢地。",
        "from": "T19n0933_001:0079c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中品上生善覺地。",
        "from": "T19n0933_001:0079c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中品中生明力地中品下生無漏地。",
        "from": "T19n0933_001:0079c08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下品上生真覺地。",
        "from": "T19n0933_001:0079c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下品中生賢覺地。",
        "from": "T19n0933_001:0079c09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下品下生樂門地。",
        "from": "T19n0933_001:0079c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名曰九品淨識真如境。",
        "from": "T19n0933_001:0079c10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是內坐十二大曼陀羅大圓鏡智寶像。",
        "from": "T19n0933_001:0079c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其名曰一切三達無量光佛。",
        "from": "T19n0933_001:0079c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍覺三明無邊光佛。",
        "from": "T19n0933_001:0079c12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智道三明無礙光佛。",
        "from": "T19n0933_001:0079c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六真理智三明無對光佛。",
        "from": "T19n0933_001:0079c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色善三明光炎王光佛。",
        "from": "T19n0933_001:0079c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一覺三明清淨光佛。",
        "from": "T19n0933_001:0079c14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普門三明歡喜光佛。",
        "from": "T19n0933_001:0079c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入慧三明智慧光佛。",
        "from": "T19n0933_001:0079c15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光色三明不斷光佛。",
        "from": "T19n0933_001:0079c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明達三明難思光佛。",
        "from": "T19n0933_001:0079c16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五德三明無稱光佛。",
        "from": "T19n0933_001:0079c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智力三明超日月光佛。",
        "from": "T19n0933_001:0079c17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是諸佛如來是真色具足。",
        "from": "T19n0933_001:0079c18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切三世如來悲相所依。",
        "from": "T19n0933_001:0079c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生欲往生如是九品淨土。",
        "from": "T19n0933_001:0079c19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉視十二圓妙。",
        "from": "T19n0933_001:0079c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日夜三時稱如是九品淨土名。",
        "from": "T19n0933_001:0079c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讚十二光佛號。",
        "from": "T19n0933_001:0079c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即永出三界火宅。",
        "from": "T19n0933_001:0079c21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定生真如。",
        "from": "T19n0933_001:0079c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離有漏永入無漏。",
        "from": "T19n0933_001:0079c22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲入如是三摩地境。",
        "from": "T19n0933_001:0079c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足佛慧淨心潔身。",
        "from": "T19n0933_001:0079c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀念大三摩耶實相真言。",
        "from": "T19n0933_001:0079c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T19n0933_001:0079c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿密㗚哆帝濟伽藍吽善男子如是真言梵呪。",
        "from": "T19n0933_001:0079c26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此坐一切十方三世諸佛如來究竟理智。",
        "from": "T19n0933_001:0079c27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二空願根本。",
        "from": "T19n0933_001:0079c27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若我弟子欲利樂三界饒益人天。",
        "from": "T19n0933_001:0079c28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書寫此經受持讀誦者增長福樂。",
        "from": "T19n0933_001:0079c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增益智慧辨才增長壽命色力。",
        "from": "T19n0933_001:0080a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "消除業障消滅萬病。",
        "from": "T19n0933_001:0080a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增長悲愛無不具足色善。",
        "from": "T19n0933_001:0080a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況至心書寫讀誦者。",
        "from": "T19n0933_001:0080a02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "度三世無生變三界火宅。",
        "from": "T19n0933_001:0080a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所誦得往生極樂界會九品淨土。",
        "from": "T19n0933_001:0080a03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時大眾聞佛所說。",
        "from": "T19n0933_001:0080a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆大歡喜信受奉行。",
        "from": "T19n0933_001:0080a04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九品往生阿彌陀經建久四年四月八日。",
        "from": "T19n0933_001:0080a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於栂尾山。",
        "from": "T19n0933_001:0080a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以勸修寺慈尊院阿闍梨御房御本書寫校訖　寬超之本　享和改元秋八月。",
        "from": "T19n0933_001:0080a08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請求智山慈順僧正御本令寫。",
        "from": "T19n0933_001:0080a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便校以印版。",
        "from": "T19n0933_001:0080a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瑜師阿彌陀大心抄云。",
        "from": "T19n0933_001:0080a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大心呪。",
        "from": "T19n0933_001:0080a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出無量壽軌並九品往生經。",
        "from": "T19n0933_001:0080a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又云。",
        "from": "T19n0933_001:0080a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "軌說大心呪勝能云。",
        "from": "T19n0933_001:0080a11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至九品往生經說同之。",
        "from": "T19n0933_001:0080a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住心論愚草云。",
        "from": "T19n0933_001:0080a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九品往生阿彌陀經云。",
        "from": "T19n0933_001:0080a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是阿彌陀如來根本大呪真言。",
        "from": "T19n0933_001:0080a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纔誦一遍者。",
        "from": "T19n0933_001:0080a14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則身中十惡五無間罪。",
        "from": "T19n0933_001:0080a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切業障皆悉消滅云云。",
        "from": "T19n0933_001:0080a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然此本唯有小呪。",
        "from": "T19n0933_001:0080a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無大呪說。",
        "from": "T19n0933_001:0080a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定知脫大呪文。",
        "from": "T19n0933_001:0080a16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有文中不隱暢。",
        "from": "T19n0933_001:0080a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讀者應知。",
        "from": "T19n0933_001:0080a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更待全本。",
        "from": "T19n0933_001:0080a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　　　　豐山長谷抖擻　快道　誌",
        "from": "T19n0933_001:0080a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一挍加筆畢　　　　　　　　　　慈順",
        "from": "T19n0933_001:0080a19_0",
        "context_with_next_sentence": true
    }
]