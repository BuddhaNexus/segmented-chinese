[
    {
        "source_sentence": "阿吒薄俱元帥大將上佛陀羅尼",
        "from": "T21n1239_003:0199a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經修行儀軌卷下",
        "from": "T21n1239_003:0199a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐大善無畏內道場祕譯",
        "from": "T21n1239_003:0199a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命十方等正覺　　最勝妙法菩薩眾",
        "from": "T21n1239_003:0199a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我身口意清淨業　　至心傾誠合掌禮",
        "from": "T21n1239_003:0199a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願諸佛法僧眾　　慈悲哀愍攝護我",
        "from": "T21n1239_003:0199a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮元帥大將軍　　此是毘盧遮那身",
        "from": "T21n1239_003:0199a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及以釋迦諸佛變　　觀音無盡無邊化",
        "from": "T21n1239_003:0199a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或現菩薩喜悅形　　兩臂與諸願印相",
        "from": "T21n1239_003:0199a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或示金剛忿怒身　　四手執器降魔相",
        "from": "T21n1239_003:0199a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元帥本體是如來　　為救世間現種種",
        "from": "T21n1239_003:0199a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四面八臂堅利牙　　執持器杖隨類護",
        "from": "T21n1239_003:0199a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有國王歸元帥　　即領一切將軍眾",
        "from": "T21n1239_003:0199a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "守護彼王國境內　　消伏隣王怨敵事",
        "from": "T21n1239_003:0199a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摧滅逆臣自調伏　　國內無諸疾疫苦",
        "from": "T21n1239_003:0199a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切世間有情類　　念持神呪稱名號",
        "from": "T21n1239_003:0199b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨除內外所生障　　必與世間出世願",
        "from": "T21n1239_003:0199b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲息災者即息災　　求榮愛者施榮愛",
        "from": "T21n1239_003:0199b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降伏惡等一切業　　一一無不成就者",
        "from": "T21n1239_003:0199b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若聞阿吒薄俱名　　口裏一誦讚歎者",
        "from": "T21n1239_003:0199b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常當擁護無諸難　　世間寶貨悉圓備",
        "from": "T21n1239_003:0199b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿吒薄俱領眷屬　　摧碎金剛青面王",
        "from": "T21n1239_003:0199b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘇悉地王軍吒利　　密迹力士摩登伽",
        "from": "T21n1239_003:0199b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵釋四王自在天　　二十八部藥叉王",
        "from": "T21n1239_003:0199b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四十二部羅剎娑　　犍闥龍王鳩槃荼",
        "from": "T21n1239_003:0199b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天魔鬼神併驅使　　閻浮眾生亦如是",
        "from": "T21n1239_003:0199b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能歸依者得勝利　　生謗逆者被重禍",
        "from": "T21n1239_003:0199b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故一切世間人　　悉應歸依大元帥",
        "from": "T21n1239_003:0199b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結請十方諸佛印。",
        "from": "T21n1239_003:0199b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "反叉左右無名指於掌中。",
        "from": "T21n1239_003:0199b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直竪二中指頭相拄。",
        "from": "T21n1239_003:0199b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二食指押中指上節背。",
        "from": "T21n1239_003:0199b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並竪二大指押捻中節側。",
        "from": "T21n1239_003:0199b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭指來去七遍。",
        "from": "T21n1239_003:0199b17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即一切佛來呪曰。",
        "from": "T21n1239_003:0199b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵一􄒶闍􄗱耶二余何反􄙖婆􄚮醯三􅁥莎􄚩訶四誦七遍次結觀世音等諸大菩薩印。",
        "from": "T21n1239_003:0199b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩腕相付。",
        "from": "T21n1239_003:0199b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指並著。",
        "from": "T21n1239_003:0199b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二小指亦然。",
        "from": "T21n1239_003:0199b22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘指散小曲竪。",
        "from": "T21n1239_003:0199b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如析蓮華。",
        "from": "T21n1239_003:0199b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭指上來下去呪曰。",
        "from": "T21n1239_003:0199b23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵一􄙘毘􄙤是􄕙底二􄙘毘􄑘伽􄙔藍三􄗬迷􄕝帝􄙂盧􄑀迦四􄙘毘􄒶闍􄗷曳五􅁥莎􄚩訶六次結十四部金剛印。",
        "from": "T21n1239_003:0199b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩腕側竪。",
        "from": "T21n1239_003:0199c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並二大指各捻二小指甲上。",
        "from": "T21n1239_003:0199c01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二無名指側竪相拄頭。",
        "from": "T21n1239_003:0199c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指各直竪。",
        "from": "T21n1239_003:0199c02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端向前不相拄。",
        "from": "T21n1239_003:0199c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指來去呪曰。",
        "from": "T21n1239_003:0199c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵一􄙖跋􄦽折羅二􄑼旃􆩀抳􄚮醯三􅁥莎􄚩訶次結三十三天神印。",
        "from": "T21n1239_003:0199c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指稍去中指第二節半分許。",
        "from": "T21n1239_003:0199c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指來去呪曰。",
        "from": "T21n1239_003:0199c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵一􄙢鑠􄩣覩嚕二􄪼跛羅􄗥魔􄕻陀􄖫儞􄗷曳三􅁥莎􄚩訶次結二十八部星宿天印。",
        "from": "T21n1239_003:0199c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左手中指無名指並仰。",
        "from": "T21n1239_003:0199c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右中指在左無名指下文上。",
        "from": "T21n1239_003:0199c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右無名指亦覆左中指下文上。",
        "from": "T21n1239_003:0199c15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總相綟如索。",
        "from": "T21n1239_003:0199c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小指頭指大指各竪相拄。",
        "from": "T21n1239_003:0199c16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指來去呪曰。",
        "from": "T21n1239_003:0199c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵一􄖩曩􄚴乞叉􄩞多羅二􄕵提􄙖婆􄕗多􄗷曳三􅁥莎􄚩訶次結東方提頭賴吒天王領乾闥婆軍眾印。",
        "from": "T21n1239_003:0199c22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "側左手中指已下三指把拳。",
        "from": "T21n1239_003:0199c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又屈頭指少曲。",
        "from": "T21n1239_003:0199c23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中指下節文。",
        "from": "T21n1239_003:0199c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指來去呪曰。",
        "from": "T21n1239_003:0199c24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵一􈁋陀羅五􄕽帝􄖵波􄕗多􄗷曳六􅁥莎􄚩訶次結南方毘樓勒叉天王領鳩槃荼眾印。",
        "from": "T21n1239_003:0200a01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "側左腕。",
        "from": "T21n1239_003:0200a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手腕側著左腕根上。",
        "from": "T21n1239_003:0200a02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二掌相背。",
        "from": "T21n1239_003:0200a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二無名指相鉤。",
        "from": "T21n1239_003:0200a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "申之如索。",
        "from": "T21n1239_003:0200a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二小指二大指各少曲。",
        "from": "T21n1239_003:0200a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭指來去呪曰。",
        "from": "T21n1239_003:0200a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵一􄕽毘􄙂嚕􄗁陀􄑀迦􄗱耶二􄖵波􄕗多􄗷曳三􅁥莎􄚩訶次結西方毘樓博叉天王領龍軍眾印。",
        "from": "T21n1239_003:0200a07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手側腕。",
        "from": "T21n1239_003:0200a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手腕著於左手腕根上。",
        "from": "T21n1239_003:0200a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手中指已下三指。",
        "from": "T21n1239_003:0200a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈在掌中作拳。",
        "from": "T21n1239_003:0200a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二大指各押上。",
        "from": "T21n1239_003:0200a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指交如索申之。",
        "from": "T21n1239_003:0200a10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指來去呪曰。",
        "from": "T21n1239_003:0200a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵一􄙘嚊􄙃嚧􄖶博􄚴叉二􄑄鳩􄖵槃􆧽荼三􆿅阿􄕽地􄖵波􄕗多􄗷曳四􅁥莎􄚩訶五次結北方毘沙門天王領藥叉軍眾印。",
        "from": "T21n1239_003:0200a16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手側腕。",
        "from": "T21n1239_003:0200a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦屈頭指以下四指。",
        "from": "T21n1239_003:0200a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在掌作拳。",
        "from": "T21n1239_003:0200a17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈大指押頭指上。",
        "from": "T21n1239_003:0200a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手腕亦然。",
        "from": "T21n1239_003:0200a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "側之作拳。",
        "from": "T21n1239_003:0200a18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指直申向上。",
        "from": "T21n1239_003:0200a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手拳累著左手拳上。",
        "from": "T21n1239_003:0200a19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右大指來去呪曰。",
        "from": "T21n1239_003:0200a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵一􄙝毘􄙢沙􈂱門􄗱耶二􆿅阿􄕽地􄖵波􄕗多􄗷曳三􅁥莎􄚩訶四次結四天王所領四方鬼神藥叉羅剎八部鬼兵印。",
        "from": "T21n1239_003:0200a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "反叉頭指中指入掌。",
        "from": "T21n1239_003:0200a27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無名指直竪。",
        "from": "T21n1239_003:0200a27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小指頭相叉入掌合腕。",
        "from": "T21n1239_003:0200a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指來去呪曰。",
        "from": "T21n1239_003:0200a28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵一􄒶瞻􆫌皤􄗽羅二􄒶謝􄙐連􈁋達羅􄗱耶三􅁥莎􄚩呵四次結八部龍王軍眾印。",
        "from": "T21n1239_003:0200b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右肘頭在左肘內。",
        "from": "T21n1239_003:0200b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復以右手後四指少曲。",
        "from": "T21n1239_003:0200b03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大指傅頭指少曲之。",
        "from": "T21n1239_003:0200b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手反曲之。",
        "from": "T21n1239_003:0200b04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向右手亦如之。",
        "from": "T21n1239_003:0200b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "狀如蛇口用之。",
        "from": "T21n1239_003:0200b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手各四指來去呪曰。",
        "from": "T21n1239_003:0200b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵一􇇳漚􄖵波􄖩難􈁋陀二􅁥莎􄚩訶次結百千天龍興雲致雨龍王印。",
        "from": "T21n1239_003:0200b09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手臂腕如前不改。",
        "from": "T21n1239_003:0200b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但以左右手小指無名指。",
        "from": "T21n1239_003:0200b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "反相叉入掌中右押左。",
        "from": "T21n1239_003:0200b11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指直竪頭相拄。",
        "from": "T21n1239_003:0200b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二食指拄中指背上節。",
        "from": "T21n1239_003:0200b12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指少曲。",
        "from": "T21n1239_003:0200b12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各付二食指內中節。",
        "from": "T21n1239_003:0200b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭指來去呪曰。",
        "from": "T21n1239_003:0200b13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵一􄗞步􄑚耆􄗽羅二􅁥莎􄚩訶三次結日天子軍眾印。",
        "from": "T21n1239_003:0200b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先背二中指無名指小指。",
        "from": "T21n1239_003:0200b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掌內相拄皆使齊。",
        "from": "T21n1239_003:0200b17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其二頭指直竪相拄。",
        "from": "T21n1239_003:0200b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指捻頭指根第一節。",
        "from": "T21n1239_003:0200b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指來去呪曰。",
        "from": "T21n1239_003:0200b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵一􄗽喝囉􅊻濕迷二􄗥摩􄙌利􄖫儞三􅁥莎􄚩訶四次結月天子眷屬印。",
        "from": "T21n1239_003:0200b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二小指無名指掌內相叉。",
        "from": "T21n1239_003:0200b23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右押左挺在掌中。",
        "from": "T21n1239_003:0200b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其中指直頭相糾頭指。",
        "from": "T21n1239_003:0200b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使中指第三節屈。",
        "from": "T21n1239_003:0200b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怒其大指。",
        "from": "T21n1239_003:0200b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各押無名指頭。",
        "from": "T21n1239_003:0200b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指來去呪曰。",
        "from": "T21n1239_003:0200b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵一次結一切火天魔兵令伏印。",
        "from": "T21n1239_003:0200b29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪右手三指。",
        "from": "T21n1239_003:0200c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈大指捻無名指下文。",
        "from": "T21n1239_003:0200c01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其頭指稍屈頭。",
        "from": "T21n1239_003:0200c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與中指第一節齊。",
        "from": "T21n1239_003:0200c02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭指來去呪曰。",
        "from": "T21n1239_003:0200c02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵一􇩹車四􅁥莎􄚩訶五次結閻羅五道大將軍牛頭兵眾印。",
        "from": "T21n1239_003:0200c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "側左手腕握。",
        "from": "T21n1239_003:0200c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四指仍稍出頭指三分許。",
        "from": "T21n1239_003:0200c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指來去呪曰。",
        "from": "T21n1239_003:0200c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵一􄗻閻􄗥魔􄗽羅􄒶闍二􇇳嗚􄥟揭羅􄙘嚊􅝩利耶三􆿅阿􄑘揭􇩹車四􅁥莎􄚩訶五次結阿修羅王鬪戰軍眾印。",
        "from": "T21n1239_003:0200c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手中指以下三指等屈。",
        "from": "T21n1239_003:0200c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭指去掌一分許。",
        "from": "T21n1239_003:0200c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指頭亦稍曲。",
        "from": "T21n1239_003:0200c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共東方天王印同令相應呪曰。",
        "from": "T21n1239_003:0200c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵一􄙘毘􄗥摩􄑾質􄩞多羅二􆿅阿􈁃蘇􄗾羅三􄕽地􄖵波􄕗多􄗷曳四􅁥莎􄚩訶五次結羅剎娑王軍兵鬪戰者急來印。",
        "from": "T21n1239_003:0200c22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手大指。",
        "from": "T21n1239_003:0200c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捻小指無名指甲上。",
        "from": "T21n1239_003:0200c23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中指及頭指申之。",
        "from": "T21n1239_003:0200c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭指來去呪曰。",
        "from": "T21n1239_003:0200c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵一􄑌揭􅖱伽二􆿅阿􄕽地􄖵波􄕗多􄗷曳三􅁥莎􄚩訶四次結五方大力藥叉王軍眾印。",
        "from": "T21n1239_003:0200c27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左手四指向下鉤右手。",
        "from": "T21n1239_003:0200c28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其二大指直申怒之。",
        "from": "T21n1239_003:0200c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指來去呪曰。",
        "from": "T21n1239_003:0200c29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵一􄑄拘􄙘嚊􄗽羅二􅁥莎􄚩訶三次結阿利陀遮文荼印。",
        "from": "T21n1239_003:0201a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指中指無名指相絞。",
        "from": "T21n1239_003:0201a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以小指相叉。",
        "from": "T21n1239_003:0201a04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大指向上相叉。",
        "from": "T21n1239_003:0201a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指來去呪曰。",
        "from": "T21n1239_003:0201a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵一􈂪胡􄙂嚧􇥗胡嚧二􄑼遮􈆿文􄙝荼三徒皆反􅁥莎􄚩訶四次結毘那夜迦鬼神王印。",
        "from": "T21n1239_003:0201a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先竪三指。",
        "from": "T21n1239_003:0201a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二無名指相叉垂下向掌背。",
        "from": "T21n1239_003:0201a09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指直竪附頭指側。",
        "from": "T21n1239_003:0201a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指來去呪曰。",
        "from": "T21n1239_003:0201a10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵一􄖵薄􄑀迦􄗽羅􄙦準􆧽荼二􆿅阿􄕽地􄖵波􄕗多􄗷曳三􅁥莎􄚩訶四次結摩醯首羅天王二十八部印。",
        "from": "T21n1239_003:0201a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小指無名指向內相鉤。",
        "from": "T21n1239_003:0201a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以中指竪相叉。",
        "from": "T21n1239_003:0201a16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以二頭指各竪。",
        "from": "T21n1239_003:0201a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "附中指側甲下。",
        "from": "T21n1239_003:0201a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指亦竪附近。",
        "from": "T21n1239_003:0201a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭指來去呪曰。",
        "from": "T21n1239_003:0201a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵一􄗥摩􄚮醯􄙦首􄗽羅􄗱耶二􅁥莎􄚩訶次結阿吒薄拘元帥大怒使金剛二十八部神降伏惡鬼印。",
        "from": "T21n1239_003:0201a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迎坐以左右手。",
        "from": "T21n1239_003:0201a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "急作拳內怒。",
        "from": "T21n1239_003:0201a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三叩齒即齧下脣。",
        "from": "T21n1239_003:0201a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以下二手拳捺著二腿上。",
        "from": "T21n1239_003:0201a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大將身印。",
        "from": "T21n1239_003:0201a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡鬼神自打自縛自求死乞命。",
        "from": "T21n1239_003:0201a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結阿吒薄拘大將大悅會天龍鬼神自護身印。",
        "from": "T21n1239_003:0201a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左右二大指。",
        "from": "T21n1239_003:0201a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捻二無名指中節內左右。",
        "from": "T21n1239_003:0201a28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百事不畏不被刀兵鬼神傷。",
        "from": "T21n1239_003:0201a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結阿吒婆拘著衣甲印。",
        "from": "T21n1239_003:0201b01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左手執刀拄之。",
        "from": "T21n1239_003:0201b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手空繞頭三周匝。",
        "from": "T21n1239_003:0201b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攬衣袖作懸牙勢。",
        "from": "T21n1239_003:0201b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼見悉著衣甲。",
        "from": "T21n1239_003:0201b03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦大呪二十一遍。",
        "from": "T21n1239_003:0201b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結阿吒婆拘千輻輪降鬼印。",
        "from": "T21n1239_003:0201b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手大母指。",
        "from": "T21n1239_003:0201b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "押中指無名指中節背上。",
        "from": "T21n1239_003:0201b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "狀如跋折羅印形。",
        "from": "T21n1239_003:0201b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結阿吒婆拘立身印。",
        "from": "T21n1239_003:0201b08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正立翹右脚。",
        "from": "T21n1239_003:0201b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左手頭指以下四指。",
        "from": "T21n1239_003:0201b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向前托跨。",
        "from": "T21n1239_003:0201b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手大母指。",
        "from": "T21n1239_003:0201b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "押中指以下三指背上中節。",
        "from": "T21n1239_003:0201b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以頭指向前申之。",
        "from": "T21n1239_003:0201b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "指一切病即差。",
        "from": "T21n1239_003:0201b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "指一切鬼魅自伏。",
        "from": "T21n1239_003:0201b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "指江海面自即竭。",
        "from": "T21n1239_003:0201b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結阿吒婆拘刀印。",
        "from": "T21n1239_003:0201b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取石榴枝長三尺二寸呪三遍。",
        "from": "T21n1239_003:0201b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手把一頭左手三摩之。",
        "from": "T21n1239_003:0201b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼見是刀。",
        "from": "T21n1239_003:0201b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左手托跨。",
        "from": "T21n1239_003:0201b15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手如金剛刺跋折羅。",
        "from": "T21n1239_003:0201b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結阿吒婆拘弓箭印。",
        "from": "T21n1239_003:0201b17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左手大指。",
        "from": "T21n1239_003:0201b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "押頭指無名指小指甲上。",
        "from": "T21n1239_003:0201b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手無名指小指屈在掌中。",
        "from": "T21n1239_003:0201b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指押頭指中指甲。",
        "from": "T21n1239_003:0201b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於左手中指側。",
        "from": "T21n1239_003:0201b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "申放右手大母指。",
        "from": "T21n1239_003:0201b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彈指之。",
        "from": "T21n1239_003:0201b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已上七種法契。",
        "from": "T21n1239_003:0201b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以用小心呪呪之。",
        "from": "T21n1239_003:0201b22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即小心呪曰。",
        "from": "T21n1239_003:0201b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄙢柘􄛀頡􄗫迷􄙢柘􄛀頡􄗫迷五􄩞怛羅􄙺散􈂰淡六􆿉嗚􄗻鹽􄙘毘七􅁥莎􄚩訶八次結阿吒婆拘降伏大魔鬼神印。",
        "from": "T21n1239_003:0201c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懸脚坐。",
        "from": "T21n1239_003:0201c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左右二手急作拳。",
        "from": "T21n1239_003:0201c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捺左右二腿上。",
        "from": "T21n1239_003:0201c02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "急以上脣齧下脣。",
        "from": "T21n1239_003:0201c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "裂眶看。",
        "from": "T21n1239_003:0201c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是大降伏印。",
        "from": "T21n1239_003:0201c03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結縛一切鬼神印。",
        "from": "T21n1239_003:0201c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左右手二小指反相叉。",
        "from": "T21n1239_003:0201c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大指押二小指甲上。",
        "from": "T21n1239_003:0201c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中指無名指竪。",
        "from": "T21n1239_003:0201c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭指亦然。",
        "from": "T21n1239_003:0201c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結勅鬼神印。",
        "from": "T21n1239_003:0201c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若勅鬼神。",
        "from": "T21n1239_003:0201c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即偏坐。",
        "from": "T21n1239_003:0201c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左手托跨。",
        "from": "T21n1239_003:0201c08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手以大指。",
        "from": "T21n1239_003:0201c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "押小指無名指甲上。",
        "from": "T21n1239_003:0201c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直竪頭指中指。",
        "from": "T21n1239_003:0201c09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結火輪印。",
        "from": "T21n1239_003:0201c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若伏大力惡鬼作火輪印。",
        "from": "T21n1239_003:0201c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左右二無名指。",
        "from": "T21n1239_003:0201c11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈在掌中令頭齊。",
        "from": "T21n1239_003:0201c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘皆相拄頭。",
        "from": "T21n1239_003:0201c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稍虛掌即是。",
        "from": "T21n1239_003:0201c12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結投魅鬼印。",
        "from": "T21n1239_003:0201c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中指以下三指。",
        "from": "T21n1239_003:0201c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆相叉掌背後。",
        "from": "T21n1239_003:0201c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指相著令齊。",
        "from": "T21n1239_003:0201c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指橫捻二頭指中節文。",
        "from": "T21n1239_003:0201c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三呼三吸即縛來已上五種法契用大呪。",
        "from": "T21n1239_003:0201c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次四天結界呪。",
        "from": "T21n1239_003:0201c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵􄗙婆􄕝諦􄗱耶􄙖盤􄾫陀􄙖盤􄾫陀􄚲唅􆿁吽􄗁􈁆泮八部都呪􄖩曩􄗭謨􈎭勃􇧺陀􄗱耶􄖩曩􄗭謨􄕻達􈊪摩􄗱耶􄖩曩􄗭謨􈂱僧􄑥伽􄗱耶􄖩曩􄗭謨􈎭佛􄙀利􄗱藥􄚴叉􄕝帝􈎭佛􄙀利􄗱藥若追八部神。",
        "from": "T21n1239_003:0202a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉前八部印。",
        "from": "T21n1239_003:0202a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦前呪二十一遍其神立至。",
        "from": "T21n1239_003:0202a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊願傳流布遍於閻浮。",
        "from": "T21n1239_003:0202a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令一切眾生受持。",
        "from": "T21n1239_003:0202a05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為作大救護。",
        "from": "T21n1239_003:0202a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛告一切大會。",
        "from": "T21n1239_003:0202a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此呪誦之入官不瞋。",
        "from": "T21n1239_003:0202a07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入水不溺入火不焚。",
        "from": "T21n1239_003:0202a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入軍不畏刀兵。",
        "from": "T21n1239_003:0202a08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國王貴人見之下位承迎。",
        "from": "T21n1239_003:0202a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "追般若不空羂索使者神鬼印。",
        "from": "T21n1239_003:0202a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左右手。",
        "from": "T21n1239_003:0202a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌當心虛八指。",
        "from": "T21n1239_003:0202a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具作供養印。",
        "from": "T21n1239_003:0202a11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後即大指押頭指中指。",
        "from": "T21n1239_003:0202a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈在左右掌中。",
        "from": "T21n1239_003:0202a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無名小指合著。",
        "from": "T21n1239_003:0202a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如佛頂形。",
        "from": "T21n1239_003:0202a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次作般若降伏天鬼神印。",
        "from": "T21n1239_003:0202a14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左右二手合腕。",
        "from": "T21n1239_003:0202a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指博近頭指側。",
        "from": "T21n1239_003:0202a15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手中指竪申之。",
        "from": "T21n1239_003:0202a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指無名指小指。",
        "from": "T21n1239_003:0202a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈節曲相拄頭中央虛掌。",
        "from": "T21n1239_003:0202a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大將使神散禁法先持呪有驗然後行之。",
        "from": "T21n1239_003:0202a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不爾不成。",
        "from": "T21n1239_003:0202a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平旦於神前安一盆井華水一爐香。",
        "from": "T21n1239_003:0202a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪師整衣服當床中迎坐勿笑。",
        "from": "T21n1239_003:0202a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手作大怒印。",
        "from": "T21n1239_003:0202a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但有鬼病人來勿多語。",
        "from": "T21n1239_003:0202a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口云急急以瞋怒罵之。",
        "from": "T21n1239_003:0202a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若似怖也即告之。",
        "from": "T21n1239_003:0202a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神眾更待何時急縛將來。",
        "from": "T21n1239_003:0202a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聲大。",
        "from": "T21n1239_003:0202a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "官使人應聲即縛竟。",
        "from": "T21n1239_003:0202a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲打欲禁之。",
        "from": "T21n1239_003:0202a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但以隨時一言更勿多語。",
        "from": "T21n1239_003:0202a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大將處法不誦呪。",
        "from": "T21n1239_003:0202a25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲有人持我呪者。",
        "from": "T21n1239_003:0202a25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平等有賢聖之人乃可持。",
        "from": "T21n1239_003:0202a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不爾自損身。",
        "from": "T21n1239_003:0202a26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略說已前行用不少。",
        "from": "T21n1239_003:0202a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若廣說窮劫不盡。",
        "from": "T21n1239_003:0202a27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　淨嚴四十九載",
        "from": "T21n1239_003:0202b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本再挍了　　尊教",
        "from": "T21n1239_003:0202b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶永五十一月十五日一挍了　　性寂",
        "from": "T21n1239_003:0202b04_0",
        "context_with_next_sentence": true
    }
]