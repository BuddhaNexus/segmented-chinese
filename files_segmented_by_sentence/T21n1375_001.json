[
    {
        "source_sentence": "佛說莊嚴王陀羅尼呪經一卷",
        "from": "T21n1375_001:0894c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏法師義淨奉　制譯",
        "from": "T21n1375_001:0894c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。一時薄伽梵。在布怛洛迦山。為諸",
        "from": "T21n1375_001:0894c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人天一切大眾演說法要。",
        "from": "T21n1375_001:0894c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍觀十方以妙音聲。",
        "from": "T21n1375_001:0894c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告觀自在菩薩妙吉祥菩薩言。",
        "from": "T21n1375_001:0894c21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子有經名一切如來所護觀察眾生示現佛剎莊嚴王陀羅尼。",
        "from": "T21n1375_001:0894c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我昔初發心時。",
        "from": "T21n1375_001:0894c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於花光顯現如來所。",
        "from": "T21n1375_001:0894c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與九十俱胝眾生。",
        "from": "T21n1375_001:0894c24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一聞此法皆得如來智慧之分咸蒙授記。",
        "from": "T21n1375_001:0894c25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸佛剎當得作佛。",
        "from": "T21n1375_001:0894c25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦於中蒙佛授記。",
        "from": "T21n1375_001:0894c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子我以佛眼。",
        "from": "T21n1375_001:0894c26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀見過去三萬如來。",
        "from": "T21n1375_001:0894c27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸眾生說此法要。",
        "from": "T21n1375_001:0894c27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝觀自在及妙吉祥。",
        "from": "T21n1375_001:0894c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於說法時皆為導首。",
        "from": "T21n1375_001:0894c28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊說是語已。",
        "from": "T21n1375_001:0894c29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現前大眾菩薩聲聞諸來聽者。",
        "from": "T21n1375_001:0895a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸發信心供養彼佛。",
        "from": "T21n1375_001:0895a01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "于時無量百千俱胝那庾多眾生皆蒙授記。",
        "from": "T21n1375_001:0895a02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦得菩薩勝妙等持。",
        "from": "T21n1375_001:0895a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "病苦悉除蓋纏消滅。",
        "from": "T21n1375_001:0895a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所願滿足善根成就。",
        "from": "T21n1375_001:0895a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顏容端正財寶豐盈。",
        "from": "T21n1375_001:0895a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國王大臣諸眾生類見者歡喜。",
        "from": "T21n1375_001:0895a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念慧神通無不圓備。",
        "from": "T21n1375_001:0895a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於佛法僧得不壞信。",
        "from": "T21n1375_001:0895a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽命色力有大威勢。",
        "from": "T21n1375_001:0895a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無愛別離怨憎會苦。",
        "from": "T21n1375_001:0895a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人等。",
        "from": "T21n1375_001:0895a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能於此經五輪著地生希有心。",
        "from": "T21n1375_001:0895a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以諸花香瓔鬘幡蓋恭敬供養。",
        "from": "T21n1375_001:0895a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸依讚歎讀誦受持。",
        "from": "T21n1375_001:0895a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若自書寫若教人書。",
        "from": "T21n1375_001:0895a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量功德皆悉成就。",
        "from": "T21n1375_001:0895a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見受持者生信敬心。",
        "from": "T21n1375_001:0895a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦於現身眾德具足。",
        "from": "T21n1375_001:0895a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆得菩薩勝妙等持。",
        "from": "T21n1375_001:0895a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口中恒出栴檀香氣。",
        "from": "T21n1375_001:0895a13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其目皎淨如青蓮葉。",
        "from": "T21n1375_001:0895a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於晝夜中見佛形像及大菩薩。",
        "from": "T21n1375_001:0895a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有五無間等業障之罪。",
        "from": "T21n1375_001:0895a15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆消滅諸天衛護。",
        "from": "T21n1375_001:0895a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命終之時得見諸佛菩薩心不錯亂。",
        "from": "T21n1375_001:0895a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必得往生極樂國土。",
        "from": "T21n1375_001:0895a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我與斯人授記作佛。",
        "from": "T21n1375_001:0895a17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼見我身恭敬供養。",
        "from": "T21n1375_001:0895a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於妙菩提心不疑惑。",
        "from": "T21n1375_001:0895a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人於此法門。",
        "from": "T21n1375_001:0895a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讀誦受持供養恭敬。",
        "from": "T21n1375_001:0895a19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若自書寫若教人書。",
        "from": "T21n1375_001:0895a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及有得聞此經名者。",
        "from": "T21n1375_001:0895a20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知此人即是菩薩應修供養。",
        "from": "T21n1375_001:0895a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復此人由先惡業。",
        "from": "T21n1375_001:0895a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "財命色力內外所資悉皆短乏。",
        "from": "T21n1375_001:0895a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求不遂親愛別離。",
        "from": "T21n1375_001:0895a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國土荒殘王賊衰難。",
        "from": "T21n1375_001:0895a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此經力現身輕受。",
        "from": "T21n1375_001:0895a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或暫頭痛或得惡衣惡食。",
        "from": "T21n1375_001:0895a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或遭罵詈及餘毀辱。",
        "from": "T21n1375_001:0895a25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往諸業障即自消除。",
        "from": "T21n1375_001:0895a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子彼持經之人應生此念。",
        "from": "T21n1375_001:0895a26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我昔流轉生死海中。",
        "from": "T21n1375_001:0895a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸有情造作眾惡。",
        "from": "T21n1375_001:0895a27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不善之業日夜增長。",
        "from": "T21n1375_001:0895a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今時覺了不敢覆藏。",
        "from": "T21n1375_001:0895a28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發露罪源誠心懺悔。",
        "from": "T21n1375_001:0895a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於佛法僧起不壞信。",
        "from": "T21n1375_001:0895a29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復彼人。",
        "from": "T21n1375_001:0895b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曾於佛法僧寶獨覺聲聞父母師長作諸惡業。",
        "from": "T21n1375_001:0895b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣食果報皆當散盡。",
        "from": "T21n1375_001:0895b02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由經力故此業咸除衣食充足。",
        "from": "T21n1375_001:0895b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復彼人有捺洛迦受苦之業。",
        "from": "T21n1375_001:0895b04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛別離苦生盲之苦。",
        "from": "T21n1375_001:0895b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無根二根異熟之業亦皆消滅。",
        "from": "T21n1375_001:0895b05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復由先嫉妬瞋恚惡業之力。",
        "from": "T21n1375_001:0895b06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當墮琰摩王界傍生餓鬼諸惡趣中。",
        "from": "T21n1375_001:0895b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由經力故所有惡業無不消滅。",
        "from": "T21n1375_001:0895b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切福德悉皆增長。",
        "from": "T21n1375_001:0895b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子若於方處有此法門。",
        "from": "T21n1375_001:0895b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼諸人等常為此經之所擁護。",
        "from": "T21n1375_001:0895b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功德具足所求圓滿。",
        "from": "T21n1375_001:0895b10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒受富樂惡業消除。",
        "from": "T21n1375_001:0895b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦無橫死及諸惡夢。",
        "from": "T21n1375_001:0895b11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有病苦鬪戰常勝壽命延長。",
        "from": "T21n1375_001:0895b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色力具足有大威勢。",
        "from": "T21n1375_001:0895b12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切世間人及天龍諸鬼神等皆隨意轉。",
        "from": "T21n1375_001:0895b13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不遭惡毒水火災橫。",
        "from": "T21n1375_001:0895b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至由持神呪力故。",
        "from": "T21n1375_001:0895b14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常蒙諸佛之所授記。",
        "from": "T21n1375_001:0895b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人。",
        "from": "T21n1375_001:0895b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得聞如是神呪受持讀誦。",
        "from": "T21n1375_001:0895b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若自書若教人書恭敬供養。",
        "from": "T21n1375_001:0895b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼善男子善女人。",
        "from": "T21n1375_001:0895b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆獲如前所說功德。",
        "from": "T21n1375_001:0895b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊說此語已。",
        "from": "T21n1375_001:0895b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大地忽然六種震動。",
        "from": "T21n1375_001:0895b19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此諸大眾以妙花香。",
        "from": "T21n1375_001:0895b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗香鬘蓋勝妙衣服。",
        "from": "T21n1375_001:0895b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉上世尊同聲唱言。",
        "from": "T21n1375_001:0895b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉我等云何。",
        "from": "T21n1375_001:0895b21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得聞如來所說神呪。",
        "from": "T21n1375_001:0895b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊即說呪曰。",
        "from": "T21n1375_001:0895b22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南謨薩婆呾他揭多喃一但姪他二勃睇三蘇勃睇四盧迦毘盧計五盧迦伐底羯囒帝六薩埵阿伐盧羯泥去七薩婆呾他揭多阿提瑟恥帝八薩婆阿奢鉢唎晡唎泥去九聚丁達囇十捺羅提婆布侍帝十一呾他揭多慎若那達睇十二呾他揭多阿提瑟侘泥娜十三薩婆盧迦速企婆跋覩十四薩婆羯磨鉢唎釵耶野十五麼麼阿木羯寫自稱已名十六薩婆薩埵難者十七曷洛叉婆跋覩十八呾他揭多阿提瑟侘泥娜十九莎訶二十但是口邊作字者皆可彈舌道之善男子此之神呪。",
        "from": "T21n1375_001:0895c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃是三萬如來之所宣說共所加護。",
        "from": "T21n1375_001:0895c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今亦復說此神呪。",
        "from": "T21n1375_001:0895c05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲利益一切有情。",
        "from": "T21n1375_001:0895c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令得安樂常作擁護。",
        "from": "T21n1375_001:0895c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除其病苦生諸佛國。",
        "from": "T21n1375_001:0895c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人作如是念。",
        "from": "T21n1375_001:0895c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如佛所說神呪功德。",
        "from": "T21n1375_001:0895c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今云何而能成就。",
        "from": "T21n1375_001:0895c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於晨朝早起清淨。",
        "from": "T21n1375_001:0895c09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切有情興大慈悲發憐愍心。",
        "from": "T21n1375_001:0895c10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除諸嫉妬憍慢瞋恚。",
        "from": "T21n1375_001:0895c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恭敬一心於佛像前。",
        "from": "T21n1375_001:0895c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香花飲食廣興供養。",
        "from": "T21n1375_001:0895c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮十方佛心祈所願。",
        "from": "T21n1375_001:0895c12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此神呪滿一百八遍。",
        "from": "T21n1375_001:0895c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一一遍各呪一花。",
        "from": "T21n1375_001:0895c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以一一花供養於佛。",
        "from": "T21n1375_001:0895c13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此人即便隨所願求。",
        "from": "T21n1375_001:0895c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆如意。",
        "from": "T21n1375_001:0895c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦於夢中得見諸佛。",
        "from": "T21n1375_001:0895c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨命終時覩佛菩薩。",
        "from": "T21n1375_001:0895c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨身之後必得往生極樂世界。",
        "from": "T21n1375_001:0895c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽命色力悉皆具足。",
        "from": "T21n1375_001:0895c16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切怨讎並生歡喜。",
        "from": "T21n1375_001:0895c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此呪時六萬有情得無生法忍。",
        "from": "T21n1375_001:0895c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永除諸障所願滿足。",
        "from": "T21n1375_001:0895c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊說此經已。",
        "from": "T21n1375_001:0895c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀自在菩薩妙吉祥菩薩。",
        "from": "T21n1375_001:0895c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切大眾聞佛所說歡喜奉行。",
        "from": "T21n1375_001:0895c20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說莊嚴王陀羅尼呪經",
        "from": "T21n1375_001:0895c21_100",
        "context_with_next_sentence": false
    }
]