[
    {
        "source_sentence": "馬鳴菩薩大神力無比驗法念誦",
        "from": "T20n1166_001:0674c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "軌儀",
        "from": "T20n1166_001:0674c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛智奉　詔譯",
        "from": "T20n1166_001:0674c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩日釋迦如來菩提樹下坐大眾集會中。",
        "from": "T20n1166_001:0674c13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時馬鳴菩薩前白佛言。",
        "from": "T20n1166_001:0674c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲說我莊嚴佛法。",
        "from": "T20n1166_001:0674c14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及為成像末貧窮下賤裸形眾生有情衣服。",
        "from": "T20n1166_001:0674c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歷無數劫行菩薩行。",
        "from": "T20n1166_001:0674c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所得大神呪有大威德。",
        "from": "T20n1166_001:0674c16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色力無比。",
        "from": "T20n1166_001:0674c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能於世間行殊勝事。",
        "from": "T20n1166_001:0674c17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無能礙罣。",
        "from": "T20n1166_001:0674c17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時即佛微㗛告馬鳴菩薩言。",
        "from": "T20n1166_001:0674c18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉大丈夫。",
        "from": "T20n1166_001:0674c18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝速可說可說。",
        "from": "T20n1166_001:0674c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯然世尊愍念我聽說。",
        "from": "T20n1166_001:0674c19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌歡喜從口中放大光明。",
        "from": "T20n1166_001:0674c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說光中神呪曰。",
        "from": "T20n1166_001:0674c20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三曼多沒他那迦迦馬鳴吃哩吒囉佉吒囉佉莎縛訶若欲受持此法。",
        "from": "T20n1166_001:0674c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先從師盡財利布施供給。",
        "from": "T20n1166_001:0674c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受習印契真言。",
        "from": "T20n1166_001:0674c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則畫作菩薩像。",
        "from": "T20n1166_001:0674c24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色相白肉色而合掌坐白蓮華。",
        "from": "T20n1166_001:0674c25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘白馬著白色衣。",
        "from": "T20n1166_001:0674c25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以瓔珞莊嚴身。",
        "from": "T20n1166_001:0674c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "首戴花冠垂右足。",
        "from": "T20n1166_001:0674c26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫像了泥壇。",
        "from": "T20n1166_001:0674c26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始自三月三日。",
        "from": "T20n1166_001:0674c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東面像向西。",
        "from": "T20n1166_001:0674c27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以種種廣大供具蘇乳蘇燈供養。",
        "from": "T20n1166_001:0674c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十八日乃至百日。",
        "from": "T20n1166_001:0674c28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須清淨潔淨。",
        "from": "T20n1166_001:0674c28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比丘及傳法利生之師恩不已失時。",
        "from": "T20n1166_001:0674c29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我常檀那家及國土放大光明。",
        "from": "T20n1166_001:0675a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就錦繡羅綿財絹縺緻之類。",
        "from": "T20n1166_001:0675a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金寶積如山岳。",
        "from": "T20n1166_001:0675a02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間受用咨矣。",
        "from": "T20n1166_001:0675a02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有一國一人。",
        "from": "T20n1166_001:0675a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行我本誓故。",
        "from": "T20n1166_001:0675a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普天率土民家皆悉如前所說。",
        "from": "T20n1166_001:0675a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "財利莊嚴具成就。",
        "from": "T20n1166_001:0675a04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一人莫乏有患。",
        "from": "T20n1166_001:0675a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我本願皆悉為莊嚴財寶。",
        "from": "T20n1166_001:0675a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於若法至疑惑之心。",
        "from": "T20n1166_001:0675a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發起不信之念。",
        "from": "T20n1166_001:0675a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不成就一世之願故。",
        "from": "T20n1166_001:0675a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後世之果不定。",
        "from": "T20n1166_001:0675a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "好好以堅固之情歸於法。",
        "from": "T20n1166_001:0675a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非忌傳授之德。",
        "from": "T20n1166_001:0675a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我必日日時時加護念。",
        "from": "T20n1166_001:0675a08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓滿世間第一妙樂之財利。",
        "from": "T20n1166_001:0675a09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如向所說係念本誓。",
        "from": "T20n1166_001:0675a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向寶壇前。",
        "from": "T20n1166_001:0675a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作契印念誦。",
        "from": "T20n1166_001:0675a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈左中指無名指。",
        "from": "T20n1166_001:0675a10_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大母指押彼二指甲。",
        "from": "T20n1166_001:0675a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舒頭指及小指安心上。",
        "from": "T20n1166_001:0675a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手把數珠念真言。",
        "from": "T20n1166_001:0675a12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此名玉環成就一切所願印。",
        "from": "T20n1166_001:0675a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此契印末代愚學之妄不可記於文字。",
        "from": "T20n1166_001:0675a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬人中撰十人。",
        "from": "T20n1166_001:0675a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十人之中撰一人。",
        "from": "T20n1166_001:0675a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "許可矣。",
        "from": "T20n1166_001:0675a14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就蠶養儀軌",
        "from": "T20n1166_001:0675a15_100",
        "context_with_next_sentence": false
    }
]