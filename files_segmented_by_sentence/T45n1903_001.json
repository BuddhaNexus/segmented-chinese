[
    {
        "source_sentence": "說罪要行法",
        "from": "T45n1903_001:0903c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐三藏法師義淨撰",
        "from": "T45n1903_001:0903c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每於半月月盡憶所犯罪。準法而說。",
        "from": "T45n1903_001:0903c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或故妄語　或飲酒　或非時食等　或請香不淨洗手而食每於旦朝或復餘時。",
        "from": "T45n1903_001:0903c26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不觀水而飲用。",
        "from": "T45n1903_001:0903c26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不如法放生故。",
        "from": "T45n1903_001:0903c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷眾生命。",
        "from": "T45n1903_001:0903c27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自壞生地。",
        "from": "T45n1903_001:0903c27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或教他壞。",
        "from": "T45n1903_001:0903c27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不作知淨語。",
        "from": "T45n1903_001:0903c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於五生種不以火等作淨而便食。",
        "from": "T45n1903_001:0903c28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用鉢椀不淨洗而食。",
        "from": "T45n1903_001:0903c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "銅椀匙筯不以灰揩而食。",
        "from": "T45n1903_001:0903c29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡是銅器皆以灰揩方淨。",
        "from": "T45n1903_001:0904a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若以澡豆水洗不得成淨。",
        "from": "T45n1903_001:0904a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或飲用殘宿惡觸瓶水。",
        "from": "T45n1903_001:0904a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及殘宿惡觸刀子割餅等而食。",
        "from": "T45n1903_001:0904a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非時飲不淨茶湯酥蜜等水。",
        "from": "T45n1903_001:0904a04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食五正食已。",
        "from": "T45n1903_001:0904a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨威儀竟。",
        "from": "T45n1903_001:0904a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吞咽餘津。",
        "from": "T45n1903_001:0904a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自受捉金銀錢寶。",
        "from": "T45n1903_001:0904a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及使人受捉。",
        "from": "T45n1903_001:0904a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不作知淨語。",
        "from": "T45n1903_001:0904a06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡觸火不持心。",
        "from": "T45n1903_001:0904a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒香等觸火不持心。",
        "from": "T45n1903_001:0904a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非時入聚落不白苾芻。",
        "from": "T45n1903_001:0904a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與未受具人同室宿過二夜。",
        "from": "T45n1903_001:0904a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與未受具人同誦。",
        "from": "T45n1903_001:0904a10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及同聲唱佛。",
        "from": "T45n1903_001:0904a10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此等波逸底迦罪。",
        "from": "T45n1903_001:0904a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "據數犯者言之。",
        "from": "T45n1903_001:0904a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘皆准此罪應三說。",
        "from": "T45n1903_001:0904a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又每旦及大食後不嚼木。",
        "from": "T45n1903_001:0904a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或向塔嚼齒木等。",
        "from": "T45n1903_001:0904a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用訖不洗而棄。",
        "from": "T45n1903_001:0904a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在僧淨地中洟唾。",
        "from": "T45n1903_001:0904a13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或棄蚤虱等不依處所。",
        "from": "T45n1903_001:0904a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或食時飲噉作聲。",
        "from": "T45n1903_001:0904a14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或食時含食語話。",
        "from": "T45n1903_001:0904a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或齧半食。",
        "from": "T45n1903_001:0904a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與未受大戒人同床席坐臥。",
        "from": "T45n1903_001:0904a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或立小便。",
        "from": "T45n1903_001:0904a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或大小便不洗淨。",
        "from": "T45n1903_001:0904a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大小便時嗽口吐水。",
        "from": "T45n1903_001:0904a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及洟唾棄齒木皆不彈指謦欬。",
        "from": "T45n1903_001:0904a18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "污手捉飲食器。",
        "from": "T45n1903_001:0904a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不淨洗手𠻳口飲水等。",
        "from": "T45n1903_001:0904a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非時食蜜。",
        "from": "T45n1903_001:0904a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不以水滴作淨。",
        "from": "T45n1903_001:0904a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或觀男子及以女人。",
        "from": "T45n1903_001:0904a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不善持心而生欲想。",
        "from": "T45n1903_001:0904a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或自觸身起愛染心。",
        "from": "T45n1903_001:0904a21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或不繫想光明縱心眠睡。",
        "from": "T45n1903_001:0904a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在燈燭光下眠臥。",
        "from": "T45n1903_001:0904a22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖有開緣若觸火等不持心。",
        "from": "T45n1903_001:0904a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於三寶師僧父母所。",
        "from": "T45n1903_001:0904a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起不尊重心。",
        "from": "T45n1903_001:0904a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及生瞋忿而不忍受。",
        "from": "T45n1903_001:0904a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅燈火不持心。",
        "from": "T45n1903_001:0904a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此等皆是惡作罪。",
        "from": "T45n1903_001:0904a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且隨要言之。",
        "from": "T45n1903_001:0904a25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若更有餘者隨所憶罪。",
        "from": "T45n1903_001:0904a26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此等皆須對人一說而悔。",
        "from": "T45n1903_001:0904a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有責心者。",
        "from": "T45n1903_001:0904a27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何責心。",
        "from": "T45n1903_001:0904a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡出家者。",
        "from": "T45n1903_001:0904a27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於不謹慎心中違律教時。",
        "from": "T45n1903_001:0904a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即須自責心云。",
        "from": "T45n1903_001:0904a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此事是我所不應作。",
        "from": "T45n1903_001:0904a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我從今已去更不如是。",
        "from": "T45n1903_001:0904a29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若常能如此自剋責時。",
        "from": "T45n1903_001:0904b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然不虧諸戒。",
        "from": "T45n1903_001:0904b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須知佛教意在如此。",
        "from": "T45n1903_001:0904b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又凡出家受十戒及大戒已去。",
        "from": "T45n1903_001:0904b03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一事皆白親教師或軌範師。",
        "from": "T45n1903_001:0904b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯除五事不須言白。",
        "from": "T45n1903_001:0904b04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為五。",
        "from": "T45n1903_001:0904b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂大小便利。",
        "from": "T45n1903_001:0904b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲淨水。",
        "from": "T45n1903_001:0904b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚼齒木。",
        "from": "T45n1903_001:0904b05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同一界內齊四十九尋內。",
        "from": "T45n1903_001:0904b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮佛繞塔。",
        "from": "T45n1903_001:0904b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘皆須白。",
        "from": "T45n1903_001:0904b06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不白師者。",
        "from": "T45n1903_001:0904b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一皆得惡作罪。",
        "from": "T45n1903_001:0904b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡至褒灑陀日。",
        "from": "T45n1903_001:0904b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應對不同犯清淨苾芻。",
        "from": "T45n1903_001:0904b08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨其大小而為敬儀。",
        "from": "T45n1903_001:0904b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蹲踞合掌憶其罪名。",
        "from": "T45n1903_001:0904b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說。",
        "from": "T45n1903_001:0904b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具壽存念。",
        "from": "T45n1903_001:0904b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我苾芻某甲有故妄語等隨其犯者稱之。",
        "from": "T45n1903_001:0904b10_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "犯眾多根本波逸底迦罪。",
        "from": "T45n1903_001:0904b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及眾多方便惡作罪。",
        "from": "T45n1903_001:0904b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或不嚼齒木等隨有稱之。",
        "from": "T45n1903_001:0904b12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "犯眾多根本惡作罪。",
        "from": "T45n1903_001:0904b12_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及眾多方便惡作罪。",
        "from": "T45n1903_001:0904b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此等諸罪。",
        "from": "T45n1903_001:0904b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各有不敬聖教波逸底迦罪。",
        "from": "T45n1903_001:0904b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及此方便惡作罪。",
        "from": "T45n1903_001:0904b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并不向人發露各有覆藏罪。",
        "from": "T45n1903_001:0904b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此所犯罪。",
        "from": "T45n1903_001:0904b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今於具壽前從清淨來。",
        "from": "T45n1903_001:0904b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並皆發露不作覆藏。",
        "from": "T45n1903_001:0904b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由發露已便得安樂三說已。",
        "from": "T45n1903_001:0904b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼應問言。",
        "from": "T45n1903_001:0904b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝見罪不。",
        "from": "T45n1903_001:0904b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答言。",
        "from": "T45n1903_001:0904b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我見。",
        "from": "T45n1903_001:0904b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又問。",
        "from": "T45n1903_001:0904b18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將來諸戒能善護不。",
        "from": "T45n1903_001:0904b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答言。",
        "from": "T45n1903_001:0904b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能護。",
        "from": "T45n1903_001:0904b19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所對苾芻應云奧箄迦梵語此義翻為方便由作此事是解脫之方便也。",
        "from": "T45n1903_001:0904b20_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其說罪者答言。",
        "from": "T45n1903_001:0904b20_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娑度此云善哉。",
        "from": "T45n1903_001:0904b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若犯僧伽伐尸沙罪者。",
        "from": "T45n1903_001:0904b21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但且對人發露。",
        "from": "T45n1903_001:0904b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後別行法。",
        "from": "T45n1903_001:0904b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若犯窣吐羅。",
        "from": "T45n1903_001:0904b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及犯捨墮罪。",
        "from": "T45n1903_001:0904b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准法除之。",
        "from": "T45n1903_001:0904b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每食罷發願法每食罷發願法准如律教。",
        "from": "T45n1903_001:0904b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若苾芻食了之時。",
        "from": "T45n1903_001:0904b25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若須誦持欹拏伽他。",
        "from": "T45n1903_001:0904b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂施頌也。",
        "from": "T45n1903_001:0904b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨於靜處或坐或立。",
        "from": "T45n1903_001:0904b26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或可經行。",
        "from": "T45n1903_001:0904b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先誦小經一紙半紙。",
        "from": "T45n1903_001:0904b27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次誦伽他云。",
        "from": "T45n1903_001:0904b27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飯食已訖。",
        "from": "T45n1903_001:0904b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當願眾生。",
        "from": "T45n1903_001:0904b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "德行充盈成十種力。",
        "from": "T45n1903_001:0904b28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所為布施者。",
        "from": "T45n1903_001:0904b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必獲其義利。",
        "from": "T45n1903_001:0904b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若為樂故施。",
        "from": "T45n1903_001:0904b29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後必得安樂。",
        "from": "T45n1903_001:0904b29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸以寺舍房宇。",
        "from": "T45n1903_001:0904c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "布施眾僧。",
        "from": "T45n1903_001:0904c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造寺之主。",
        "from": "T45n1903_001:0904c01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及護寺天神。",
        "from": "T45n1903_001:0904c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國主百僚。",
        "from": "T45n1903_001:0904c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師僧父母因緣眷屬。",
        "from": "T45n1903_001:0904c02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及一切眾生。",
        "from": "T45n1903_001:0904c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若先亡者。",
        "from": "T45n1903_001:0904c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願生西方見阿彌陀佛。",
        "from": "T45n1903_001:0904c03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或生覩史多天見慈氏尊者。",
        "from": "T45n1903_001:0904c04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脫屣塵勞悟無生忍。",
        "from": "T45n1903_001:0904c04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其現存者。",
        "from": "T45n1903_001:0904c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願無病長壽福智莊嚴。",
        "from": "T45n1903_001:0904c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願我自身無諸障惱。",
        "from": "T45n1903_001:0904c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常持淨戒不犯尸羅。",
        "from": "T45n1903_001:0904c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於小罪中心生大懼。",
        "from": "T45n1903_001:0904c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於所犯罪悉皆發露。",
        "from": "T45n1903_001:0904c07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡未來際不失信心。",
        "from": "T45n1903_001:0904c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逢善知識。",
        "from": "T45n1903_001:0904c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願生中國離於八難。",
        "from": "T45n1903_001:0904c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常存正見至求解脫。",
        "from": "T45n1903_001:0904c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒與眾生作不請之友。",
        "from": "T45n1903_001:0904c09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以此福普施一切。",
        "from": "T45n1903_001:0904c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同出有流證無上果。",
        "from": "T45n1903_001:0904c10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於每日不作如是念誦發願者。",
        "from": "T45n1903_001:0904c11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是懈怠人不銷信施洗淨法。",
        "from": "T45n1903_001:0904c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其大小便室。",
        "from": "T45n1903_001:0904c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必須別處。",
        "from": "T45n1903_001:0904c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各安門扇。",
        "from": "T45n1903_001:0904c13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并置橫串。",
        "from": "T45n1903_001:0904c13_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡入小行室及上廁時法。",
        "from": "T45n1903_001:0904c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合持瓶勿口含水用為洗淨。",
        "from": "T45n1903_001:0904c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謦欬彈指或再或三方始前進。",
        "from": "T45n1903_001:0904c15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦既入已却串門扇。",
        "from": "T45n1903_001:0904c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持土兩塊或灰一抄。",
        "from": "T45n1903_001:0904c16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "半用洗身半洗左手。",
        "from": "T45n1903_001:0904c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "筒槽帛拂皆非本儀。",
        "from": "T45n1903_001:0904c17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既出廁外。",
        "from": "T45n1903_001:0904c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其瓶可置三叉木上注水向身。",
        "from": "T45n1903_001:0904c18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或安於脾令水斜出。",
        "from": "T45n1903_001:0904c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無瓶時盆子盛水權充亦得。",
        "from": "T45n1903_001:0904c19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以土十五塊。",
        "from": "T45n1903_001:0904c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安在右邊地上或塼版上。",
        "from": "T45n1903_001:0904c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七塊偏洗左手。",
        "from": "T45n1903_001:0904c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七塊兩手俱洗。",
        "from": "T45n1903_001:0904c21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘有一在。",
        "from": "T45n1903_001:0904c21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將洗瓶器。",
        "from": "T45n1903_001:0904c21_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其塼版上即須淨洗。",
        "from": "T45n1903_001:0904c22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後安置觸瓶。",
        "from": "T45n1903_001:0904c22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取淨瓶水漱口三度。",
        "from": "T45n1903_001:0904c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方合淨儀。",
        "from": "T45n1903_001:0904c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始得受禮禮他。",
        "from": "T45n1903_001:0904c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虔敬三寶坐諸床席讀誦尊經。",
        "from": "T45n1903_001:0904c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不如是依律行者。",
        "from": "T45n1903_001:0904c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡有所為咸招惡作之罪。",
        "from": "T45n1903_001:0904c25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或在行路及乘船時。",
        "from": "T45n1903_001:0904c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "任量事斟酌。",
        "from": "T45n1903_001:0904c26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "希諸行者。",
        "from": "T45n1903_001:0904c26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知是聖教勿輕灰覆。",
        "from": "T45n1903_001:0904c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終墮泥犁。",
        "from": "T45n1903_001:0904c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉持獲福。",
        "from": "T45n1903_001:0904c27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輕法罪生。",
        "from": "T45n1903_001:0904c27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此既常事。",
        "from": "T45n1903_001:0904c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告示分明。",
        "from": "T45n1903_001:0904c28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說罪要行法",
        "from": "T45n1903_001:0904c29_100",
        "context_with_next_sentence": false
    }
]