[
    {
        "source_sentence": "大唐內典錄卷第七",
        "from": "T55n2149_007:0296a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "京師西明寺釋氏撰",
        "from": "T55n2149_007:0296a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歷代小乘藏經翻本單重傳譯有無錄歷代小乘藏經翻本單重傳譯有無錄第",
        "from": "T55n2149_007:0296a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三序序曰。",
        "from": "T55n2149_007:0296a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所云小乘藏者。",
        "from": "T55n2149_007:0296a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂諸佛隨緣赴機之漸教也。",
        "from": "T55n2149_007:0296a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "良由智識褊隘固執鏗然。",
        "from": "T55n2149_007:0296a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空有分壃心塵別境。",
        "from": "T55n2149_007:0296a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "封守界繫位列因緣。",
        "from": "T55n2149_007:0296a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "排倒我之本基。",
        "from": "T55n2149_007:0296a24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "折流轉之纏結。",
        "from": "T55n2149_007:0296a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憚佛道長遠居止化城。",
        "from": "T55n2149_007:0296a25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恥聲聞從師栖形空土。",
        "from": "T55n2149_007:0296a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斯等之經名二乘道也。",
        "from": "T55n2149_007:0296a26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "討論教主曲引釋迦。",
        "from": "T55n2149_007:0296a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "託八相而垂光。",
        "from": "T55n2149_007:0296a27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寄三界而稱號。",
        "from": "T55n2149_007:0296a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "胎誕右脇。",
        "from": "T55n2149_007:0296a28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "引同類而攝生。",
        "from": "T55n2149_007:0296a28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨位若遣。",
        "from": "T55n2149_007:0296a28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "接染愛之迷客。",
        "from": "T55n2149_007:0296a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四十九載。",
        "from": "T55n2149_007:0296a29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三輪現於人天。",
        "from": "T55n2149_007:0296a29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方八十年。",
        "from": "T55n2149_007:0296b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四諦揚於生趣。",
        "from": "T55n2149_007:0296b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斯道被俗開誘寔繁。",
        "from": "T55n2149_007:0296b01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非佛本懷乘機權設。",
        "from": "T55n2149_007:0296b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故經云。",
        "from": "T55n2149_007:0296b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方佛土惟有一乘。",
        "from": "T55n2149_007:0296b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨宜方便故說三教。",
        "from": "T55n2149_007:0296b03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而登機之士依教策修。",
        "from": "T55n2149_007:0296b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷戒見之牽連。",
        "from": "T55n2149_007:0296b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傾分段之生死。",
        "from": "T55n2149_007:0296b04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鈍根證此。",
        "from": "T55n2149_007:0296b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂窮蓋廕之源。",
        "from": "T55n2149_007:0296b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利智澄慮。",
        "from": "T55n2149_007:0296b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沈疑而在空性。",
        "from": "T55n2149_007:0296b05_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以五部異執分計而討其迷十八本二尋根而知理一。",
        "from": "T55n2149_007:0296b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故知兔馬涉水。",
        "from": "T55n2149_007:0296b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未盡香象之能。",
        "from": "T55n2149_007:0296b07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羊鹿載駝。",
        "from": "T55n2149_007:0296b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈等大牛之力。",
        "from": "T55n2149_007:0296b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以大乘義本。",
        "from": "T55n2149_007:0296b08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性空極於教宗。",
        "from": "T55n2149_007:0296b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小乘理淺。",
        "from": "T55n2149_007:0296b09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生滅會於真解。",
        "from": "T55n2149_007:0296b09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故佛性論云。",
        "from": "T55n2149_007:0296b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二乘之人觀同生滅以為真如。",
        "from": "T55n2149_007:0296b10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "據斯以言。",
        "from": "T55n2149_007:0296b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯小大之衢術也。",
        "from": "T55n2149_007:0296b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至於經部所攝必祖四含。",
        "from": "T55n2149_007:0296b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨機說導更開雜藏。",
        "from": "T55n2149_007:0296b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戒律流被非經所收。",
        "from": "T55n2149_007:0296b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "議論披解最為繁廣。",
        "from": "T55n2149_007:0296b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且夫大聖施化本遣惑纏。",
        "from": "T55n2149_007:0296b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除病稱藥不拘名體。",
        "from": "T55n2149_007:0296b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故初說四諦。",
        "from": "T55n2149_007:0296b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八萬諸天而發大心。",
        "from": "T55n2149_007:0296b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後說六度。",
        "from": "T55n2149_007:0296b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "億量比丘悟於四果。",
        "from": "T55n2149_007:0296b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自餘凡淺執教守株互相指斥。",
        "from": "T55n2149_007:0296b16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "全乖本意。",
        "from": "T55n2149_007:0296b16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "述客舊之二醫。",
        "from": "T55n2149_007:0296b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明于極教。",
        "from": "T55n2149_007:0296b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毀本師之兩足。",
        "from": "T55n2149_007:0296b17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著在雜詞。",
        "from": "T55n2149_007:0296b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小無述大之言。",
        "from": "T55n2149_007:0296b18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自局心計。",
        "from": "T55n2149_007:0296b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大有含小之致。",
        "from": "T55n2149_007:0296b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通明蔽開。",
        "from": "T55n2149_007:0296b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是知迦葉興悲於敗種。",
        "from": "T55n2149_007:0296b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逼引同徒。",
        "from": "T55n2149_007:0296b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身子悔吝於法性。",
        "from": "T55n2149_007:0296b20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟迷斯反。",
        "from": "T55n2149_007:0296b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸餘故習沿革卒難。",
        "from": "T55n2149_007:0296b21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終待會機異名施化。",
        "from": "T55n2149_007:0296b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今所集經。",
        "from": "T55n2149_007:0296b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始於仙苑終盡金河。",
        "from": "T55n2149_007:0296b22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所說半教號聲聞藏。",
        "from": "T55n2149_007:0296b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳度東漸年代可知。",
        "from": "T55n2149_007:0296b23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總有二百七十二部。",
        "from": "T55n2149_007:0296b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一千四百九十四卷。",
        "from": "T55n2149_007:0296b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用為小乘藏錄。",
        "from": "T55n2149_007:0296b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘有賢聖傳集。",
        "from": "T55n2149_007:0296b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將二百卷文兼小大理雜聖凡。",
        "from": "T55n2149_007:0296b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不在二藏所收。",
        "from": "T55n2149_007:0296b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自依別錄所顯。",
        "from": "T55n2149_007:0296b26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至於單重翻本傳譯存亡。",
        "from": "T55n2149_007:0296b27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無勞別歷通入三藏。",
        "from": "T55n2149_007:0296b27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庶得披覽之者。",
        "from": "T55n2149_007:0296b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以類相從即用大觀釋然易辯云爾。",
        "from": "T55n2149_007:0296b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小乘經五百四十四卷小乘律二百七十四卷小乘論六百七十六卷西梵賢聖集傳此通大乘合一百八十四卷都合四件凡三百一十九部一千六百七十八卷二萬四百二十四紙。",
        "from": "T55n2149_007:0296c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小乘經單重翻本并譯有無錄小乘經單重翻本并譯有無錄合二百四部五百四十四卷七千六百七十四紙正法念處經七十紙一千一百九十九紙後魏菩提留支於洛都譯中阿含經六十卷一千一百四十七紙東晉僧伽提婆於楊都譯增一阿含經五十卷七百九十五紙東晉前秦建元年曇摩難提於長安譯雜阿含經五十卷一千三十紙宋求那跋陀羅於楊都譯長阿含經二十二卷四百二十六紙東晉後秦弘始年佛陀耶舍於常安譯已前四經。",
        "from": "T55n2149_007:0296c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小乘大宗四含為本。",
        "from": "T55n2149_007:0296c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "支流分散故有多部。",
        "from": "T55n2149_007:0296c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今總舉本經如上。",
        "from": "T55n2149_007:0296c20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自餘別品殊譯。",
        "from": "T55n2149_007:0296c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "濟俗引生不無弘利。",
        "from": "T55n2149_007:0296c21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故復因仍相從敘列。",
        "from": "T55n2149_007:0296c22_1",
        "context_with_next_sentence": true
    },
