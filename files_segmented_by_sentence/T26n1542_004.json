[
    {
        "source_sentence": "2阿毘達磨品類足論卷第四",
        "from": "T26n1542_004:0704b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊者世友造",
        "from": "T26n1542_004:0704b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏法師玄奘奉　詔譯",
        "from": "T26n1542_004:0704b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辯隨眠品第五之二有唯二十法，謂唯見苦所斷法、唯見集所斷法、唯見滅所斷法、惟見道所斷法、唯修所斷法。",
        "from": "T26n1542_004:0704b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如不定繫，欲界繫、色界繫、無色界繫亦爾。",
        "from": "T26n1542_004:0704b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於惟見苦所斷法，幾隨眠隨增？",
        "from": "T26n1542_004:0704b10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答：",
        "from": "T26n1542_004:0704b10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟見苦所斷非遍行隨眠。",
        "from": "T26n1542_004:0704b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於惟見集所斷法，幾隨眠隨增？",
        "from": "T26n1542_004:0704b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答：",
        "from": "T26n1542_004:0704b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟見集所斷非遍行隨眠。",
        "from": "T26n1542_004:0704b12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於惟見滅所斷法，幾隨眠隨增？",
        "from": "T26n1542_004:0704b13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答：",
        "from": "T26n1542_004:0704b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟見滅所斷一切隨眠。",
        "from": "T26n1542_004:0704b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於惟見道所斷法，幾隨眠隨增？",
        "from": "T26n1542_004:0704b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答：",
