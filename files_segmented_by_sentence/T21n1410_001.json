[
    {
        "source_sentence": "佛說勝幡瓔珞陀羅尼經",
        "from": "T21n1410_001:0925c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試鴻臚少卿",
        "from": "T21n1410_001:0925c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳法大師臣施護奉　詔譯",
        "from": "T21n1410_001:0925c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T21n1410_001:0925c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在喜樂山頂。",
        "from": "T21n1410_001:0925c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天宮不遠仙人住處與大比丘眾千二百五十人俱。",
        "from": "T21n1410_001:0925c08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有菩薩摩訶薩。",
        "from": "T21n1410_001:0925c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊師利童子及賢護菩薩等。",
        "from": "T21n1410_001:0925c09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十六大士皆來集會。",
        "from": "T21n1410_001:0925c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊三昧正受。",
        "from": "T21n1410_001:0925c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀彼天上人間。",
        "from": "T21n1410_001:0925c11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而欲說法平等施與。",
        "from": "T21n1410_001:0925c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當使有情得聞法已。",
        "from": "T21n1410_001:0925c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱讚殊勝咸令歡喜。",
        "from": "T21n1410_001:0925c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時大梵天王及天龍夜叉乾闥婆阿修羅迦樓羅緊曩羅摩睺羅伽人非人等亦來集會。",
        "from": "T21n1410_001:0925c15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時大梵天王前詣佛所。",
        "from": "T21n1410_001:0925c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稽首佛足合掌恭肅而白佛言世尊。",
        "from": "T21n1410_001:0925c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切有情造種種罪業。",
        "from": "T21n1410_001:0925c17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身壞命終墮於惡趣。",
        "from": "T21n1410_001:0925c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或墮焰魔界或墮餓鬼界或墮畜生界。",
        "from": "T21n1410_001:0925c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既墮如是諸惡趣已受種種苦。",
        "from": "T21n1410_001:0925c19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及彼色等輪迴亦然。",
        "from": "T21n1410_001:0925c20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊大慈大悲無量善巧願施方便。",
        "from": "T21n1410_001:0925c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破壞如是一切有情。",
        "from": "T21n1410_001:0925c21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "罪業果報使得解脫。",
        "from": "T21n1410_001:0925c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時觀世音菩薩摩訶薩。",
        "from": "T21n1410_001:0925c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與無數持明天前後圍繞亦來集會。",
        "from": "T21n1410_001:0925c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至佛所已五輪著地。",
        "from": "T21n1410_001:0925c24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮佛雙足合掌肅恭。",
        "from": "T21n1410_001:0925c25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而白佛言世尊。",
        "from": "T21n1410_001:0925c25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大威德者十力具足。",
        "from": "T21n1410_001:0925c26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願方便施諸善巧。",
        "from": "T21n1410_001:0925c26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救拔一切輪迴有情令得出離。",
        "from": "T21n1410_001:0925c27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言善哉善男子汝等諦聽。",
        "from": "T21n1410_001:0925c28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今為汝等及未來世一切有情。",
        "from": "T21n1410_001:0925c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宣說最上勝幡明王。",
        "from": "T21n1410_001:0925c29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有得聞此名能稱念者。",
        "from": "T21n1410_001:0926a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當得無量無邊功德果報。",
        "from": "T21n1410_001:0926a01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說陀羅尼曰。",
        "from": "T21n1410_001:0926a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也二合他引迦囉迦囉枳哩枳哩俱嚕俱嚕娑囉娑囉悉哩悉哩酥嚕酥嚕薩哩嚩二合沒馱引嚩路二合枳帝引嚩囉嚩囉達哩摩二合目契引左囉左囉僧伽引地瑟恥二合帝引婆囉婆囉阿儞迦沒馱俱引胝婆引史帝引剎拏引剎拏引薩哩嚩二合羯哩麼二合嚩囉拏儞薩哩嚩二合播引波儞薩哩嚩二合耨佉引儞何波曩野部哩二合吒部哩二合吒度哩二合吒度哩二合吒怛吒怛吒薩普二合吒野薩哩嚩二合羯哩麼二合引嚩囉拏引播引野訥哩誐二合底儞薩哩嚩二合薩怛嚩二合引喃引左娑嚩二合訶善男子此勝旛瓔珞陀羅尼。",
        "from": "T21n1410_001:0926a14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是彼恒河沙等諸佛同共宣說。",
        "from": "T21n1410_001:0926a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為汝等今復宣說。",
        "from": "T21n1410_001:0926a15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子此陀羅尼希有難遇。",
        "from": "T21n1410_001:0926a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬彼如來出興於世難得值遇。",
        "from": "T21n1410_001:0926a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此陀羅尼得值遇者倍復甚難。",
        "from": "T21n1410_001:0926a17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人。",
        "from": "T21n1410_001:0926a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得聞是勝幡瓔珞陀羅尼名字。",
        "from": "T21n1410_001:0926a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信心受持恒常讀誦者。",
        "from": "T21n1410_001:0926a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人所有五逆重罪悉皆滅盡。",
        "from": "T21n1410_001:0926a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得見果報獲大富貴於當來世得生上族。",
        "from": "T21n1410_001:0926a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復善男子善女人。",
        "from": "T21n1410_001:0926a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此陀羅尼殊勝功德。",
        "from": "T21n1410_001:0926a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣宣流布為人解說。",
        "from": "T21n1410_001:0926a22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或勸人書寫受持讀誦。",
        "from": "T21n1410_001:0926a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我即遙知。",
        "from": "T21n1410_001:0926a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至毘婆尸佛。",
        "from": "T21n1410_001:0926a23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尸棄佛。",
        "from": "T21n1410_001:0926a23_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘舍浮佛。",
        "from": "T21n1410_001:0926a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拘留孫佛。",
        "from": "T21n1410_001:0926a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羯諾迦牟尼佛。",
        "from": "T21n1410_001:0926a24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉佛。",
        "from": "T21n1410_001:0926a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是過去恒河沙等如來應正等覺皆悉聞知。",
        "from": "T21n1410_001:0926a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人功德無量無邊。",
        "from": "T21n1410_001:0926a26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時觀世音菩薩。",
        "from": "T21n1410_001:0926a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙吉祥菩薩。",
        "from": "T21n1410_001:0926a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢護菩薩等。",
        "from": "T21n1410_001:0926a27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及大比丘眾乃至一切天。",
        "from": "T21n1410_001:0926a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及天人阿修羅乾闥婆人非人等。",
        "from": "T21n1410_001:0926a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛所說皆大歡喜信受奉行。",
        "from": "T21n1410_001:0926b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說勝幡瓔珞陀羅尼經",
        "from": "T21n1410_001:0926b02_100",
        "context_with_next_sentence": false
    }
]