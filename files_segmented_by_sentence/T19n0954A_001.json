[
    {
        "source_sentence": "一字頂輪王念誦儀軌",
        "from": "T19n0954A_001:0307c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿",
        "from": "T19n0954A_001:0307c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "肅國公食邑三千戶賜紫贈司",
        "from": "T19n0954A_001:0307c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空諡大鑒正號大廣智大興善寺三藏沙門不空奉　詔譯我今依忉利天宮會釋迦牟尼如來所說無比力超勝世間出世間真言上上一切佛頂主宰一字頂輪王念誦儀則。",
        "from": "T19n0954A_001:0307c11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行者先當入此頂輪王大曼荼羅。",
        "from": "T19n0954A_001:0307c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得阿闍梨灌頂印可。",
        "from": "T19n0954A_001:0307c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方受此法。",
        "from": "T19n0954A_001:0307c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須善明解。",
        "from": "T19n0954A_001:0307c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後於清淨處安本尊像。",
        "from": "T19n0954A_001:0307c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面西稽首禮。",
        "from": "T19n0954A_001:0307c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受三歸捨身說罪受戒發菩提心隨喜勸請發願迴向已。",
        "from": "T19n0954A_001:0307c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應結佛部三昧耶印。",
        "from": "T19n0954A_001:0307c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手內相叉。",
        "from": "T19n0954A_001:0307c16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雙並。",
        "from": "T19n0954A_001:0307c16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二大拇指。",
        "from": "T19n0954A_001:0307c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成。",
        "from": "T19n0954A_001:0307c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名一切如來心印真言曰。",
        "from": "T19n0954A_001:0307c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵邇那邇入聲呼次結蓮華部三昧耶印。",
        "from": "T19n0954A_001:0307c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前佛部心印。",
        "from": "T19n0954A_001:0307c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈左大拇指入掌。",
        "from": "T19n0954A_001:0307c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右大指准前直竪即成。",
        "from": "T19n0954A_001:0307c21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名蓮華部心印真言曰。",
        "from": "T19n0954A_001:0307c22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿嚧力次結金剛部三昧耶印。",
        "from": "T19n0954A_001:0307c24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前佛部心印。",
        "from": "T19n0954A_001:0307c24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈右大指入掌。",
        "from": "T19n0954A_001:0307c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直竪左大拇指即成。",
        "from": "T19n0954A_001:0307c25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名金剛部心印真言曰。",
        "from": "T19n0954A_001:0307c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉二合地力二合次結甲冑印。",
        "from": "T19n0954A_001:0307c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手內相叉。",
        "from": "T19n0954A_001:0307c28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二中指各屈上節如劍形。",
        "from": "T19n0954A_001:0307c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二頭指各拄中指背。",
        "from": "T19n0954A_001:0307c29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印加持額右肩左肩心喉五處一遍真言曰。",
        "from": "T19n0954A_001:0308a01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵斫羯囉二合靺㗚底鉢囉二合賖弭多囉捺囉二合引囉捺囉二合引婆去娑摩二合車盧瑟尼二合沙𡀔乞灑二合𡀔乞灑二合𤚥吽發吒半音娑嚩二合引訶引次結佛眼印。",
        "from": "T19n0954A_001:0308a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手合掌。",
        "from": "T19n0954A_001:0308a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指各拄中指背。",
        "from": "T19n0954A_001:0308a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並屈二大拇指入掌。",
        "from": "T19n0954A_001:0308a07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印真言加持五處真言曰。",
        "from": "T19n0954A_001:0308a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱引南引一唵嚕嚕薩普二合嚕入嚩二合羅底瑟咤二合悉馱盧者寧薩嚩囉他二合娑引達泥娑嚩二合引訶次結大海印。",
        "from": "T19n0954A_001:0308a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手內相叉。",
        "from": "T19n0954A_001:0308a12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌。",
        "from": "T19n0954A_001:0308a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擘開二大指。",
        "from": "T19n0954A_001:0308a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印右旋三匝。",
        "from": "T19n0954A_001:0308a13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想成大海水真言曰。",
        "from": "T19n0954A_001:0308a13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵微摩嚧娜地娑嚩二合引訶引次於大海中想須彌盧山四寶所成。",
        "from": "T19n0954A_001:0308a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手內相叉。",
        "from": "T19n0954A_001:0308a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "急握作拳。",
        "from": "T19n0954A_001:0308a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合腕並竪。",
        "from": "T19n0954A_001:0308a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成真言曰。",
        "from": "T19n0954A_001:0308a16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿者羅吽次於須彌盧山上。",
        "from": "T19n0954A_001:0308a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想七寶樓閣。",
        "from": "T19n0954A_001:0308a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結加持寶樓閣印。",
        "from": "T19n0954A_001:0308a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手金剛合掌。",
        "from": "T19n0954A_001:0308a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左右十指各交初分即成真言曰。",
        "from": "T19n0954A_001:0308a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱引南引唵薩嚩他欠嗢娜蘖二合帝薩頗二合囉呬引𤚥誐誐曩劍平聲呼娑嚩二合引訶引次結佛頂輪王印。",
        "from": "T19n0954A_001:0308a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉作拳。",
        "from": "T19n0954A_001:0308a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二中指屈上節如劍形。",
        "from": "T19n0954A_001:0308a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並竪二大指。",
        "from": "T19n0954A_001:0308a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指捻二大指頭上。",
        "from": "T19n0954A_001:0308a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成。",
        "from": "T19n0954A_001:0308a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印五處加護真言曰。",
        "from": "T19n0954A_001:0308a26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱引南步嚕唵三合次結網橛印。",
        "from": "T19n0954A_001:0308a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0308a28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指上節。",
        "from": "T19n0954A_001:0308a28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "背不相著。",
        "from": "T19n0954A_001:0308b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大拇指各壓上下。",
        "from": "T19n0954A_001:0308b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "揮轉即成結上下界真言曰。",
        "from": "T19n0954A_001:0308b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱引南引阿鉢囉二合底賀多舍娑娜南唵微枳囉拏微特防二合娑尼迦比羅貳嚩里尼怛囉二合娑耶嚩日囉二合吠賖薩帝呶引囉特嚩二合能上瑟吒囉二合𡀔吃沙二合𤚥發次結牆印。",
        "from": "T19n0954A_001:0308b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0308b08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指兩節相逼。",
        "from": "T19n0954A_001:0308b08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平竪二大拇指。",
        "from": "T19n0954A_001:0308b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "附二頭指。",
        "from": "T19n0954A_001:0308b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋三匝即成金剛牆界真言曰。",
        "from": "T19n0954A_001:0308b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "噁引莫壑歸命同前次結車輅印。",
        "from": "T19n0954A_001:0308b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉仰掌。",
        "from": "T19n0954A_001:0308b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "申二頭指令甲側相拄。",
        "from": "T19n0954A_001:0308b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二大指各柱頭指根下。",
        "from": "T19n0954A_001:0308b13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想於他方世界。",
        "from": "T19n0954A_001:0308b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉迎本尊真言曰。",
        "from": "T19n0954A_001:0308b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵覩嚕覩嚕吽次結迎車輅印。",
        "from": "T19n0954A_001:0308b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前車輅印。",
        "from": "T19n0954A_001:0308b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大指各撥中指頭。",
        "from": "T19n0954A_001:0308b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向身三招真言曰。",
        "from": "T19n0954A_001:0308b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨悉底哩也四合地尾二合迦南呾他蘖多南唵嚩日𠻴二合儗妍以反儞也二合羯哩沙二合耶娑嚩二合引訶若奉送除羯哩沙二合耶迦尾薩若耶次結迎請印。",
        "from": "T19n0954A_001:0308b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0308b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈右頭指於中指後。",
        "from": "T19n0954A_001:0308b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向前三招真言曰。",
        "from": "T19n0954A_001:0308b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨婆誐嚩帝阿鉢囉二合底賀覩瑟尼二合沙耶翳呬翳呬婆誐吻達麼囉惹鉢囉二合底掣䲭奚反南遏鉗巘談補澀奔補甘反二合度奔上末隣儞半者滿遮避路吃沙二合阿鉢囉二合底賀多麼囉鉢囉二合羯囉二合麼耶娑嚩二合引訶引次結一切辦事佛頂印。",
        "from": "T19n0954A_001:0308c01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手內相叉。",
        "from": "T19n0954A_001:0308c01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二中指屈上節如劍形。",
        "from": "T19n0954A_001:0308c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸供養物及浴水洗淨土等並用此真言。",
        "from": "T19n0954A_001:0308c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持辟除去垢結界皆用此真言。",
        "from": "T19n0954A_001:0308c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左旋辟除右旋結界真言曰。",
        "from": "T19n0954A_001:0308c04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱南一唵吒嚕唵三合滿馱娑嚩二合引訶引次重結前網橛印。",
        "from": "T19n0954A_001:0308c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一用結上方界。",
        "from": "T19n0954A_001:0308c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結阿娑莽倪倪枳反尼印。",
        "from": "T19n0954A_001:0308c08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "側二手左掩右。",
        "from": "T19n0954A_001:0308c08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二大指即成。",
        "from": "T19n0954A_001:0308c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右轉一匝即成密縫真言曰。",
        "from": "T19n0954A_001:0308c09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿娑莽倪尼吽發次結獻閼伽印。",
        "from": "T19n0954A_001:0308c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0308c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指各附中指。",
        "from": "T19n0954A_001:0308c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二大指。",
        "from": "T19n0954A_001:0308c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各附頭指根側真言曰。",
        "from": "T19n0954A_001:0308c12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱南唵閼伽囉訶閼伽必哩二合野鉢囉二合底掣䲭奚反娜末鉗娑嚩二合引訶引次重結根本印。",
        "from": "T19n0954A_001:0309a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結獻師子座印。",
        "from": "T19n0954A_001:0309a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0309a02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指於二大指甲側。",
        "from": "T19n0954A_001:0309a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0954A_001:0309a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿者囉尾囉耶娑嚩二合引訶引次結塗香印。",
        "from": "T19n0954A_001:0309a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0309a05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈右頭指倚於左中指下節真言曰。",
        "from": "T19n0954A_001:0309a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多　沒馱南唵怛𡃤二合盧枳也二合巘馱蘖帝吽吽發發娑嚩二合引訶次結獻花印。",
        "from": "T19n0954A_001:0309a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前塗香印。",
        "from": "T19n0954A_001:0309a09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "改左頭指。",
        "from": "T19n0954A_001:0309a09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倚左中指下節真言曰。",
        "from": "T19n0954A_001:0309a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱南引唵薩嚩盧迦補澁波二合步多耶吽吽發發娑嚩二合引訶引次結燒香印。",
        "from": "T19n0954A_001:0309a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0309a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指各倚於中指中節真言曰。",
        "from": "T19n0954A_001:0309a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱南唵尾囉蘖跢微蘖多度跛耶吽吽發發娑嚩二合引訶引次結獻食印。",
        "from": "T19n0954A_001:0309a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0309a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指上節。",
        "from": "T19n0954A_001:0309a17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各附於大指側真言曰。",
        "from": "T19n0954A_001:0309a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱南唵薩嚩盧迦麼里必哩二合夜引吽吽發發娑嚩二合引訶引次結獻燈明印。",
        "from": "T19n0954A_001:0309a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0309a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指兩節。",
        "from": "T19n0954A_001:0309a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令不相著。",
        "from": "T19n0954A_001:0309a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指各附於頭指上真言曰。",
        "from": "T19n0954A_001:0309a22_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱南唵薩嚩盧迦珊捺囉二合捨那耶吽吽發發娑嚩二合訶引次結普供養加持印　二手虛心合掌上頂各兩節相交真言曰。",
        "from": "T19n0954A_001:0309a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱南冒地薩怛嚩二合南唵薩嚩怛囉二合僧俱蘇弭多避枳惹囉始寧曩謨薩覩二合諦娑嚩二合訶引次結遍照佛頂印。",
        "from": "T19n0954A_001:0309b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉。",
        "from": "T19n0954A_001:0309b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合為拳。",
        "from": "T19n0954A_001:0309b01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令二中指節微起。",
        "from": "T19n0954A_001:0309b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0954A_001:0309b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱南噁引莫㟏次結白傘蓋佛頂印。",
        "from": "T19n0954A_001:0309b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大指。",
        "from": "T19n0954A_001:0309b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各捻二無名指甲上側。",
        "from": "T19n0954A_001:0309b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相合二頭指屈如蓋形。",
        "from": "T19n0954A_001:0309b05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指微屈相合。",
        "from": "T19n0954A_001:0309b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二小指各竪相合真言曰。",
        "from": "T19n0954A_001:0309b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱南阿鉢囉二合底賀多舍娑那南唵麼麼麼吽匿儞翼反次結光聚佛頂印。",
        "from": "T19n0954A_001:0309b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前白傘蓋印。",
        "from": "T19n0954A_001:0309b09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坼開二頭指即成真言曰。",
        "from": "T19n0954A_001:0309b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱南阿鉢囉二合底賀多舍娑娜南唵怛他󰋲都瑟尼二合娑阿耶嚩盧枳多慕㗚馱帝儒囉始吽入嚩二合攞入嚩二合攞馱迦馱迦娜囉娜囉微娜囉微娜囉嗔那嗔那頻那頻那吽吽發發娑嚩二合引訶引次結高佛頂印。",
        "from": "T19n0954A_001:0309b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前白傘蓋印。",
        "from": "T19n0954A_001:0309b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指。",
        "from": "T19n0954A_001:0309b17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各拄中指中節背。",
        "from": "T19n0954A_001:0309b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0954A_001:0309b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱南阿鉢囉二合底賀多舍娑娜南唵引儞𭅤儞𭅤卑瑜二合娜誐都瑟尼二合沙吽吽發發娑嚩二合訶引次結勝佛頂印。",
        "from": "T19n0954A_001:0309b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准高佛頂印。",
        "from": "T19n0954A_001:0309b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "移二頭指。",
        "from": "T19n0954A_001:0309b22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向上兩󰎣麥許真言曰。",
        "from": "T19n0954A_001:0309b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱南阿鉢囉二合底賀多舍娑那南唵入嚩二合羅惹諭瑟尼二合沙吽發娑嚩二合引訶引次結摧毀佛頂尾枳囉拏印。",
        "from": "T19n0954A_001:0309b27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉作拳。",
        "from": "T19n0954A_001:0309b27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二中指屈節。",
        "from": "T19n0954A_001:0309b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右中指拄左中指面。",
        "from": "T19n0954A_001:0309b28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令出半節許真言曰。",
        "from": "T19n0954A_001:0309b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱南阿鉢囉二合底賀多舍娑娜南唵尾枳囉拏度那度那犢引次結摧碎佛頂印。",
        "from": "T19n0954A_001:0309c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前改左中指。",
        "from": "T19n0954A_001:0309c03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拄右中指面。",
        "from": "T19n0954A_001:0309c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦出半節許真言曰。",
        "from": "T19n0954A_001:0309c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱南阿鉢囉二合底賀多舍娑那喃唵阿鉢囉二合底賀都瑟尼二合沙耶薩嚩尾伽曩二合尾特望二合娑那迦囉耶怛嚧二合吒耶娑嚩二合訶引次結輪王佛頂心印。",
        "from": "T19n0954A_001:0309c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0309c09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指各拄中指上節真言曰。",
        "from": "T19n0954A_001:0309c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱南阿鉢囉二合底賀多舍娑娜南唵怛他󰋲都瑟尼二合沙阿那嚩盧枳多沒馱尼斫羯羅二合靺㗚二合底吽入嚩二合羅入嚩二合羅馱迦馱迦度那微度那怛囉二合娑耶麼囉逾瑳囉耶賀那賀那伴惹伴惹暗惡屩鉢龍二合企尼君吒哩尼阿鉢囉二合爾多薩怛羅二合馱哩尼吽發娑嚩二合訶次結心中心印。",
        "from": "T19n0954A_001:0310a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0310a03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指各加於二中指上節上真言曰。",
        "from": "T19n0954A_001:0310a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱南阿鉢囉二合底賀多舍娑那南唵阿鉢囉二合爾多特他翼反普通諸佛頂印二手虛心金剛合掌。",
        "from": "T19n0954A_001:0310a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如花在掌中。",
        "from": "T19n0954A_001:0310a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行者若急遽不能遍結諸佛頂印。",
        "from": "T19n0954A_001:0310a08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但結此印誦諸佛頂真言。",
        "from": "T19n0954A_001:0310a09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結頂印。",
        "from": "T19n0954A_001:0310a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0310a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈右頭指。",
        "from": "T19n0954A_001:0310a10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪於右中指後。",
        "from": "T19n0954A_001:0310a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令不相著真言曰。",
        "from": "T19n0954A_001:0310a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱南阿鉢囉三合底賀多舍娑那南唵斫羯羅二合靺㗚二合底唵吽次結頭印。",
        "from": "T19n0954A_001:0310a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0310a14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開二頭指各直竪於中指後。",
        "from": "T19n0954A_001:0310a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令不相著微屈真言曰歸命同上。",
        "from": "T19n0954A_001:0310a15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵斫羯囉二合靺㗚二合底吽發娑嚩二合引訶引次又結根本印。",
        "from": "T19n0954A_001:0310a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結大三昧耶印。",
        "from": "T19n0954A_001:0310a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加護本尊二手內相叉。",
        "from": "T19n0954A_001:0310a19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二中指。",
        "from": "T19n0954A_001:0310a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指中指後。",
        "from": "T19n0954A_001:0310a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如鉤相去一󰎣麥許。",
        "from": "T19n0954A_001:0310a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指各附頭指根下。",
        "from": "T19n0954A_001:0310a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋三匝真言曰。",
        "from": "T19n0954A_001:0310a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵商羯哩摩訶三昧延娑嚩二合訶引次誦一百八名讚讚歎。",
        "from": "T19n0954A_001:0310a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲念誦。",
        "from": "T19n0954A_001:0310a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以五支成本尊。",
        "from": "T19n0954A_001:0310a25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或五相成本尊瑜伽。",
        "from": "T19n0954A_001:0310a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於三處頂舌心也想一字頂輪。",
        "from": "T19n0954A_001:0310a26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成本尊坐八葉蓮華。",
        "from": "T19n0954A_001:0310a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一一葉上想七寶。",
        "from": "T19n0954A_001:0310a27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯當前蓮華葉上想佛眼尊。",
        "from": "T19n0954A_001:0310a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應持珠此依菩提道場所說經。",
        "from": "T19n0954A_001:0310a28_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次持珠合掌捧珠。",
        "from": "T19n0954A_001:0310a29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦淨珠真言七遍真言曰。",
        "from": "T19n0954A_001:0310a29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿娜步二合低尾惹曳而奚反悉地悉馱囉梯娑嚩二合引訶次結持珠印。",
        "from": "T19n0954A_001:0310b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手各以大指。",
        "from": "T19n0954A_001:0310b03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捻無名指甲上。",
        "from": "T19n0954A_001:0310b03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直竪二中指二小指。",
        "from": "T19n0954A_001:0310b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指於中指後。",
        "from": "T19n0954A_001:0310b04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令不相著如毘盧遮那經半金剛杵印。",
        "from": "T19n0954A_001:0310b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0954A_001:0310b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應淨其心　　如法而念誦",
        "from": "T19n0954A_001:0310b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持珠令當心　　繫心於鼻端",
        "from": "T19n0954A_001:0310b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "字句分明呼　　不緩亦不急",
        "from": "T19n0954A_001:0310b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不頻伸噦嗌　　咳嗽與唾涕",
        "from": "T19n0954A_001:0310b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "染等心相應　　及心緣苦受",
        "from": "T19n0954A_001:0310b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等過患　　皆不得成就",
        "from": "T19n0954A_001:0310b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當念誦時。",
        "from": "T19n0954A_001:0310b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身心不得疲怠。",
        "from": "T19n0954A_001:0310b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若勞倦即應結五供養印。",
        "from": "T19n0954A_001:0310b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦讚歎獻閼伽念。",
        "from": "T19n0954A_001:0310b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦畢持珠頂上。",
        "from": "T19n0954A_001:0310b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結前密縫印左轉一匝。",
        "from": "T19n0954A_001:0310b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成解界。",
        "from": "T19n0954A_001:0310b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結奉送印。",
        "from": "T19n0954A_001:0310b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0310b18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左頭指外擲。",
        "from": "T19n0954A_001:0310b18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦迎請真言除翳呬翳呬。",
        "from": "T19n0954A_001:0310b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加蘖車蘖車句。",
        "from": "T19n0954A_001:0310b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成奉送。",
        "from": "T19n0954A_001:0310b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應復結牆及網橛等印。",
        "from": "T19n0954A_001:0310b21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加護處上下及所成就物。",
        "from": "T19n0954A_001:0310b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結計里枳里印。",
        "from": "T19n0954A_001:0310b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左大指壓左小指甲上。",
        "from": "T19n0954A_001:0310b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘三指頭拆開。",
        "from": "T19n0954A_001:0310b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直竪如三股杵形。",
        "from": "T19n0954A_001:0310b24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋轉三匝成結界真言曰。",
        "from": "T19n0954A_001:0310b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵枳里枳里嚩日囉二合吽發次結軍吒利印。",
        "from": "T19n0954A_001:0310b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二小指於掌中交。",
        "from": "T19n0954A_001:0310b27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二無名指小指上。",
        "from": "T19n0954A_001:0310b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大指壓二無名指上。",
        "from": "T19n0954A_001:0310b28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二中指相合。",
        "from": "T19n0954A_001:0310b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指於中指後令一麥許不相著。",
        "from": "T19n0954A_001:0310c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋三匝即成結界真言曰。",
        "from": "T19n0954A_001:0310c01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從一字真言　　至於十五字",
        "from": "T19n0954A_001:0310c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每計於字數　　十字一洛叉",
        "from": "T19n0954A_001:0310c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至於三字　　應誦三洛叉",
        "from": "T19n0954A_001:0310c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應作先事法　　三十字已上",
        "from": "T19n0954A_001:0310c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應誦一萬遍一字頂輪王念誦儀軌一卷",
        "from": "T19n0954A_001:0310c13_100",
        "context_with_next_sentence": false
    }
]