[
    {
        "source_sentence": "北方毘沙門天王隨軍護法儀軌",
        "from": "T21n1247_001:0224c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "特進試鴻臚卿大興善寺三藏",
        "from": "T21n1247_001:0224c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙門大廣智不空奉　詔譯",
        "from": "T21n1247_001:0224c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時那吒太子。",
        "from": "T21n1247_001:0224c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手捧戟。",
        "from": "T21n1247_001:0224c12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以惡眼見四方白佛言。",
        "from": "T21n1247_001:0224c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我是北方天王吠室羅摩那羅闍第三王子其第二之孫。",
        "from": "T21n1247_001:0224c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我祖父天王。",
        "from": "T21n1247_001:0224c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及我那吒同共每日三度。",
        "from": "T21n1247_001:0224c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言。",
        "from": "T21n1247_001:0224c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我護持佛法。",
        "from": "T21n1247_001:0224c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲攝縛惡人或起不善之心。",
        "from": "T21n1247_001:0224c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我晝夜守護國王大臣及百官僚。",
        "from": "T21n1247_001:0224c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相與殺害打陵。",
        "from": "T21n1247_001:0224c17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是之輩者。",
        "from": "T21n1247_001:0224c17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等那吒以金剛杖刺其眼及其心。",
        "from": "T21n1247_001:0225a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若為比丘比丘尼優婆塞優婆夷起不善心及殺害心者。",
        "from": "T21n1247_001:0225a02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦以金剛棒打其頭。",
        "from": "T21n1247_001:0225a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時毘沙門孫那吒。",
        "from": "T21n1247_001:0225a03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊。",
        "from": "T21n1247_001:0225a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為未來諸不善眾生。",
        "from": "T21n1247_001:0225a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降伏攝縛皆悉滅散故。",
        "from": "T21n1247_001:0225a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦護持國界故。",
        "from": "T21n1247_001:0225a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說自心暴惡真言。",
        "from": "T21n1247_001:0225a05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊聽許。",
        "from": "T21n1247_001:0225a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我說佛言善哉善哉那吒天王。",
        "from": "T21n1247_001:0225a06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝為降伏一切國王大臣百寮殺淩者。",
        "from": "T21n1247_001:0225a07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦法佛相違者。",
        "from": "T21n1247_001:0225a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為降伏故恣汝意說真言曰。",
        "from": "T21n1247_001:0225a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵地舍那吠室羅二合摩拏野摩賀羅惹野藥迦灑二合地婆哆那謨婆誐縛帝摩多羅跋馱儞娑嚩二合賀引若行者受持此呪者。",
        "from": "T21n1247_001:0225a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先須書像。",
        "from": "T21n1247_001:0225a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彩色中並不得和膠。",
        "from": "T21n1247_001:0225a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於白㲲上畫一毘沙門神其孫那吒天神七寶莊嚴。",
        "from": "T21n1247_001:0225a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手令執口齒。",
        "from": "T21n1247_001:0225a14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手詫腰上令執三戟矟。",
        "from": "T21n1247_001:0225a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其神足下作一藥叉女住趺坐。",
        "from": "T21n1247_001:0225a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並作青黑色少赤加。",
        "from": "T21n1247_001:0225a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦此呪時。",
        "from": "T21n1247_001:0225a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就好地勿使有穢惡。",
        "from": "T21n1247_001:0225a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種花燒香供養。",
        "from": "T21n1247_001:0225a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者上下衣服並須一清。",
        "from": "T21n1247_001:0225a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一廁行時當護身。",
        "from": "T21n1247_001:0225a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黑月十五夜起首。",
        "from": "T21n1247_001:0225a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對像前誦呪滿三十萬遍訖。",
        "from": "T21n1247_001:0225a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後取香泥供養尊像。",
        "from": "T21n1247_001:0225a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若相違叛逆國王大臣百寮。",
        "from": "T21n1247_001:0225a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有不善之心起者。",
        "from": "T21n1247_001:0225a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以松葉護摩。",
        "from": "T21n1247_001:0225a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七日之內。",
        "from": "T21n1247_001:0225a21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚩日羅底瑟騣說云以金剛槌打其頭及心文。",
        "from": "T21n1247_001:0225a22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人比丘比丘尼犯者。",
        "from": "T21n1247_001:0225a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以苦練木護摩。",
        "from": "T21n1247_001:0225a23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七日內殄滅。",
        "from": "T21n1247_001:0225a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法犯國王人有。",
        "from": "T21n1247_001:0225a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以佉陀羅木上設都嚧名字書。",
        "from": "T21n1247_001:0225a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三角火中數數令出入。",
        "from": "T21n1247_001:0225a26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口誦上呪心想設都嚧。",
        "from": "T21n1247_001:0225a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降伏。",
        "from": "T21n1247_001:0225a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法富單那。",
        "from": "T21n1247_001:0225a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及一切鬼王來伺其便者。",
        "from": "T21n1247_001:0225a28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以金剛木護摩。",
        "from": "T21n1247_001:0225a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不過七日殄滅。",
        "from": "T21n1247_001:0225a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又不善怨賊來淩殺時。",
        "from": "T21n1247_001:0225b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以心印誦上呪一七遍自殄滅不敢來。",
        "from": "T21n1247_001:0225b01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法欲降伏怨人暴惡之人。",
        "from": "T21n1247_001:0225b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以苦練木護摩。",
        "from": "T21n1247_001:0225b02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又苦木之汁煮出。",
        "from": "T21n1247_001:0225b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黃土和作設都嚧形。",
        "from": "T21n1247_001:0225b03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "胸上書姓名七設都嚧。",
        "from": "T21n1247_001:0225b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七日之中投火中。",
        "from": "T21n1247_001:0225b04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數數誦呪作印無疑滅。",
        "from": "T21n1247_001:0225b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法四輩人。",
        "from": "T21n1247_001:0225b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於七珍起不善之心。",
        "from": "T21n1247_001:0225b06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或不撰父母兄弟姊妹六親眷屬。",
        "from": "T21n1247_001:0225b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依法柏木護摩。",
        "from": "T21n1247_001:0225b07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切珍財自來。",
        "from": "T21n1247_001:0225b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法妻子奴婢車乘。",
        "from": "T21n1247_001:0225b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及六畜生依法柏木護摩。",
        "from": "T21n1247_001:0225b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切六畜自來。",
        "from": "T21n1247_001:0225b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又欲勝一切諸人及惡怨人。",
        "from": "T21n1247_001:0225b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以松木作三戟矟。",
        "from": "T21n1247_001:0225b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數數打壇底。",
        "from": "T21n1247_001:0225b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其壇底設都嚧之形置三。",
        "from": "T21n1247_001:0225b12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數數打數數絕。",
        "from": "T21n1247_001:0225b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法阿修羅窟門。",
        "from": "T21n1247_001:0225b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當以三戟矟打。",
        "from": "T21n1247_001:0225b14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當自口開。",
        "from": "T21n1247_001:0225b14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法以胡粉和塭護摩。",
        "from": "T21n1247_001:0225b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "領天供四萬人。",
        "from": "T21n1247_001:0225b15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必來自國兵敵。",
        "from": "T21n1247_001:0225b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及他國兵。",
        "from": "T21n1247_001:0225b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時殄滅。",
        "from": "T21n1247_001:0225b16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心印者。",
        "from": "T21n1247_001:0225b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內縛。",
        "from": "T21n1247_001:0225b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二小二空出來去善惡一切用。",
        "from": "T21n1247_001:0225b17_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔五國大亂。",
        "from": "T21n1247_001:0225b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有八箇月經月行多法遂無法驗。",
        "from": "T21n1247_001:0225b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行此法降伏五國五萬軍自平安故。",
        "from": "T21n1247_001:0225b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名隨軍護法。",
        "from": "T21n1247_001:0225b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘沙門天隨軍護法儀軌",
        "from": "T21n1247_001:0225b21_100",
        "context_with_next_sentence": false
    }
]