[
    {
        "source_sentence": "大聖天歡喜雙身毘那夜迦法",
        "from": "T21n1266_001:0296a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一卷",
        "from": "T21n1266_001:0296a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T21n1266_001:0296a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空諡大鑑正號大廣智大興善寺三藏沙門不空奉　詔譯出陀羅尼集經第十一諸天下卷身呪曩謨尾那翼迦二合瀉賀悉底二合母佉瀉怛儞野二合佗唵娜去聲下同翼迦二合娜翼迦二合尾娜翼迦二合尾娜翼迦二合怛囉二合簸哩二合怛囉二合翼迦二合餉引佉賀悉底二合餉引佉迦只多扇上底迦囉二合娑嚩二合引賀引心呪唵儗上哩虐心中心呪唵虐虐吽娑嚩二合引賀若欲作此天法。",
        "from": "T21n1266_001:0296a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先須畫像。",
        "from": "T21n1266_001:0296a22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或用白鑞銅木等及花木。",
        "from": "T21n1266_001:0296a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若鑄若刻並得。",
        "from": "T21n1266_001:0296a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其像形。",
        "from": "T21n1266_001:0296a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫婦二身令相抱立。",
        "from": "T21n1266_001:0296a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各長五寸。",
        "from": "T21n1266_001:0296a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或七寸皆得。",
        "from": "T21n1266_001:0296a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並作象頭人身。",
        "from": "T21n1266_001:0296a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造像不得違價。",
        "from": "T21n1266_001:0296a25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其像成已。",
        "from": "T21n1266_001:0296a25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當以白月一日。",
        "from": "T21n1266_001:0296a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於淨室內。",
        "from": "T21n1266_001:0296a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用淨牛糞摩作圓壇。",
        "from": "T21n1266_001:0296a26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意大小。",
        "from": "T21n1266_001:0296a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當取一升清細麻油。",
        "from": "T21n1266_001:0296a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以淨銅器盛之。",
        "from": "T21n1266_001:0296a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用上呪文呪油一百八遍。",
        "from": "T21n1266_001:0296a28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即暖其油。",
        "from": "T21n1266_001:0296a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後將像放著油中。",
        "from": "T21n1266_001:0296a29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安置壇內。",
        "from": "T21n1266_001:0296a29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用淨銅匙銅杓等率油。",
        "from": "T21n1266_001:0296b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灌其二像頂身一百八遍。",
        "from": "T21n1266_001:0296b01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日之中七遍灌之。",
        "from": "T21n1266_001:0296b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平曉四遍日午三遍共成七遍。",
        "from": "T21n1266_001:0296b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已後每日更呪舊油以灌其像。",
        "from": "T21n1266_001:0296b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是作法乃至七日。",
        "from": "T21n1266_001:0296b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨心所願即得稱意。",
        "from": "T21n1266_001:0296b04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正灌油之時數數發願。",
        "from": "T21n1266_001:0296b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復用酥蜜和󰉓作團。",
        "from": "T21n1266_001:0296b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及蘿蔔根并一盞酒。",
        "from": "T21n1266_001:0296b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及歡喜團時新花菓等。",
        "from": "T21n1266_001:0296b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是日別取新者供養。",
        "from": "T21n1266_001:0296b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切善事隨意成就。",
        "from": "T21n1266_001:0296b07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切災禍悉皆銷滅。",
        "from": "T21n1266_001:0296b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其所獻食必須自食始得氣力。",
        "from": "T21n1266_001:0296b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請召印。",
        "from": "T21n1266_001:0296b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二小指二無名指。",
        "from": "T21n1266_001:0296b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相鉤向內。",
        "from": "T21n1266_001:0296b10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以二中指竪相叉。",
        "from": "T21n1266_001:0296b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以二頭指。",
        "from": "T21n1266_001:0296b11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各竪附中指。",
        "from": "T21n1266_001:0296b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大指亦竪。",
        "from": "T21n1266_001:0296b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "附近頭指側。",
        "from": "T21n1266_001:0296b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指來去。",
        "from": "T21n1266_001:0296b12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1266_001:0296b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵簸迦囉二合主拏禰去嚩哆野二合誦七遍。",
        "from": "T21n1266_001:0296b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讚歎呪唵誐娜簸波上下同底扇上聲呼底娑嚩二合引悉底摩賀誐娜簸底娑嚩二合引賀引送呪唵簸迦囉二合主拏禰去嚩哆野二合娑嚩二合引賀引誦七遍。",
        "from": "T21n1266_001:0296b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪水護身呪唵吉里吉里跋折囉二合引吽泮吒半音若欲作法時。",
        "from": "T21n1266_001:0296b25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以上呪呪水七遍。",
        "from": "T21n1266_001:0296b25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用洗口。",
        "from": "T21n1266_001:0296b25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并更取水呪七遍。",
        "from": "T21n1266_001:0296b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用楊柳散灑身上即為護身。",
        "from": "T21n1266_001:0296b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後入持誦室中作法之所。",
        "from": "T21n1266_001:0296b27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洗浴呪唵阿拏婆折唎二合婆跛隷拏訶以上呪呪水七遍當用洗浴。",
        "from": "T21n1266_001:0296c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "調和毘那夜迦法調和毘那夜迦法請一切天作帝殊羅施印。",
        "from": "T21n1266_001:0296c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二小指無名指。",
        "from": "T21n1266_001:0296c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相叉於掌內。",
        "from": "T21n1266_001:0296c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二中指指頭相捻。",
        "from": "T21n1266_001:0296c04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二頭指各加中指背第一節下半分。",
        "from": "T21n1266_001:0296c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指。",
        "from": "T21n1266_001:0296c05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來去呪曰。",
        "from": "T21n1266_001:0296c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一鑠都嚕二波羅摩馱儞曳三莎訶誦七遍。",
        "from": "T21n1266_001:0297a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但是誦呪人夢中驚怖。",
        "from": "T21n1266_001:0297a02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見諸畜生惡境界等。",
        "from": "T21n1266_001:0297a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知是毘那夜迦王瞋。",
        "from": "T21n1266_001:0297a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夢中覺已即慚愧乞莫瞋。",
        "from": "T21n1266_001:0297a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明日自有飲食勞謝。",
        "from": "T21n1266_001:0297a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以水摩地作二肘水圓壇。",
        "from": "T21n1266_001:0297a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如槃大亦得。",
        "from": "T21n1266_001:0297a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即取𩟘餅五顆蘿蔔根三顆火燒熟。",
        "from": "T21n1266_001:0297a06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有花著花并燒白膠薰陸等香安於壇中。",
        "from": "T21n1266_001:0297a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪人壇西面向東坐。",
        "from": "T21n1266_001:0297a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦大自在天呪一百八遍已。",
        "from": "T21n1266_001:0297a08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口云慚愧好去。",
        "from": "T21n1266_001:0297a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是語已。",
        "from": "T21n1266_001:0297a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壇中雜物盤盛。",
        "from": "T21n1266_001:0297a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出門向西棄却。",
        "from": "T21n1266_001:0297a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西北亦得。",
        "from": "T21n1266_001:0297a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口云薩婆藥叉囉闍阿藹捨訶娑鉢闍伽車。",
        "from": "T21n1266_001:0297a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是語已棄却即歸。",
        "from": "T21n1266_001:0297a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大自在天呪曰。",
        "from": "T21n1266_001:0297a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一毘跢囉薩尼二波囉末唎達尼三瞋達尼瞋達尼四頻達尼頻達尼五莎訶誦呪一百八遍。",
        "from": "T21n1266_001:0297a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即心歡喜非但夢中。",
        "from": "T21n1266_001:0297a15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但覺有魔事即作前法定好。",
        "from": "T21n1266_001:0297a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造像法造像法其像形端立。",
        "from": "T21n1266_001:0297a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "象頭人身。",
        "from": "T21n1266_001:0297a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左牙出右牙折。",
        "from": "T21n1266_001:0297a18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面少向左。",
        "from": "T21n1266_001:0297a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其鼻向外瘻。",
        "from": "T21n1266_001:0297a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有六臂。",
        "from": "T21n1266_001:0297a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其左上手把刀。",
        "from": "T21n1266_001:0297a19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次手把果盤。",
        "from": "T21n1266_001:0297a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下手把輪。",
        "from": "T21n1266_001:0297a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右上手把棒。",
        "from": "T21n1266_001:0297a20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次手把索。",
        "from": "T21n1266_001:0297a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下手把牙。",
        "from": "T21n1266_001:0297a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造此像不得違價。",
        "from": "T21n1266_001:0297a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "足數付之。",
        "from": "T21n1266_001:0297a21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若鑄若刻若畫並得。",
        "from": "T21n1266_001:0297a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右此大聖大祕要法。",
        "from": "T21n1266_001:0297a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人間希有。",
        "from": "T21n1266_001:0297a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實勿傳之宜誡慎也。",
        "from": "T21n1266_001:0297a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大聖天歡喜雙身毘那夜迦法一卷",
        "from": "T21n1266_001:0297a25_100",
        "context_with_next_sentence": false
    }
]