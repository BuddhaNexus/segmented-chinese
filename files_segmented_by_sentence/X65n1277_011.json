{
    "X65n1277_011:0041c16_0": "高峰龍泉院因師集賢語錄卷之十一",
    "X65n1277_011:0041c18_0": "總願碎語門",
    "X65n1277_011:0041c19_0": "總願全段",
    "X65n1277_011:0041c20_0": "上祝　當今皇帝道踰堯舜之君。",
    "X65n1277_011:0041c20_1": "在位大臣德邁皐夔之列。",
    "X65n1277_011:0041c21_2": "霧掃胡塵而干戈盡偃。",
    "X65n1277_011:0041c21_3": "月沉邊燧而文軌皆同。",
    "X65n1277_011:0041c22_4": "有民皆樂於春臺。",
    "X65n1277_011:0041c22_5": "無俗不躋於壽域。",
    "X65n1277_011:0041c22_6": "次異鄉閭寧。",
    "X65n1277_011:0041c23_0": "謐家室康安。",
    "X65n1277_011:0041c23_1": "免逢山水之蒙。",
    "X65n1277_011:0041c23_2": "常遇地天之泰。",
    "X65n1277_011:0041c23_3": "鑿井耕田而各安生業。",
    "X65n1277_011:0041c24_4": "含哺鼓腹而咸樂天真。",
    "X65n1277_011:0041c24_5": "商歌廛而物阜財豐。",
    "X65n1277_011:0042a01_6": "士修學而功成名遂。",
    "X65n1277_011:0042a01_7": "蠶桑春富家家繅白雪之絲。",
    "X65n1277_011:0042a02_8": "禾稼秋成處處刈黃雲之穗。",
    "X65n1277_011:0042a02_9": "夜絕覆梁之輩。",
    "X65n1277_011:0042a02_10": "時聞擊壤之童。",
    "X65n1277_011:0042a03_11": "牛羊茁壯滿千山。",
    "X65n1277_011:0042a03_12": "雞犬相聞達四境。",
    "X65n1277_011:0042a03_13": "獄囚永息。",
    "X65n1277_011:0042a04_14": "疫癘潛消。",
    "X65n1277_011:0042a04_15": "圓滿一切願心。",
    "X65n1277_011:0042a04_16": "成就無邊福利。",
    "X65n1277_011:0042a05_0": "為官者",
    "X65n1277_011:0042a06_0": "居官無恙。",
    "X65n1277_011:0042a06_1": "理事如神。",
    "X65n1277_011:0042a06_2": "人思召伯之甘棠。",
    "X65n1277_011:0042a06_3": "世競郭公之竹馬。",
    "X65n1277_011:0042a07_4": "早錫千鍾之祿。",
    "X65n1277_011:0042a07_5": "榮膺一尺之書。",
    "X65n1277_011:0042a07_6": "享台筭以綿長。",
    "X65n1277_011:0042a08_0": "履仕途而遷擢。",
    "X65n1277_011:0042a09_0": "修文者",
    "X65n1277_011:0042a10_0": "舒卷韓窻之編簡。",
    "X65n1277_011:0042a10_1": "富饒鄴架之詩書。",
    "X65n1277_011:0042a10_2": "禹門三級願先登。",
    "X65n1277_011:0042a11_3": "郄桂一枝祈早折。",
    "X65n1277_011:0042a11_4": "茂延百福。",
    "X65n1277_011:0042a11_5": "永殄千災。",
    "X65n1277_011:0042a12_0": "習武者",
    "X65n1277_011:0042a13_0": "六篇明豹路。",
    "X65n1277_011:0042a13_1": "三尺撫龍泉。",
    "X65n1277_011:0042a13_2": "孫吳之事業益精。",
    "X65n1277_011:0042a13_3": "衛霍之勳名早遂。",
    "X65n1277_011:0042a14_4": "彎弓馳射功収百步之穿楊。",
    "X65n1277_011:0042a14_5": "衣錦榮歸堂慶双親之戲綵。",
    "X65n1277_011:0042a16_0": "治家者",
    "X65n1277_011:0042a17_0": "理家有道。",
    "X65n1277_011:0042a17_1": "妙筭無遺。",
    "X65n1277_011:0042a17_2": "產業山崇還如王愷。",
    "X65n1277_011:0042a17_3": "貨財泉溢有若陶朱椿老。",
    "X65n1277_011:0042a18_4": "桂芳益顯竇家之陰德。",
    "X65n1277_011:0042a18_5": "蘭森玉立茂揚謝砌之休聲。",
    "X65n1277_011:0042a20_0": "事公者",
    "X65n1277_011:0042a21_0": "為吏事官多享祿。",
    "X65n1277_011:0042a21_1": "招財遇貴得安身。",
    "X65n1277_011:0042a21_2": "結案疑而無錯無差。",
    "X65n1277_011:0042a22_3": "覆官員而允順允聽。",
    "X65n1277_011:0042a22_4": "常願保全之美。",
    "X65n1277_011:0042a22_5": "永無過失之虞。",
    "X65n1277_011:0042a23_6": "家道榮華。",
    "X65n1277_011:0042a23_7": "公庭清吉。",
    "X65n1277_011:0042a24_0": "務農者",
    "X65n1277_011:0042b01_0": "水耨敢辭頻。",
    "X65n1277_011:0042b01_1": "火耕當作苦。",
    "X65n1277_011:0042b01_2": "春帶一犂江上雨。",
    "X65n1277_011:0042b01_3": "秋収千畒隴頭雲。",
    "X65n1277_011:0042b02_4": "大田常獲豐登。",
    "X65n1277_011:0042b02_5": "高廩益充委積。",
    "X65n1277_011:0042b02_6": "犬無吠夜。",
    "X65n1277_011:0042b03_0": "吏不登門。",
    "X65n1277_011:0042b04_0": "養蚕者",
    "X65n1277_011:0042b05_0": "帝釋密扶。",
    "X65n1277_011:0042b05_1": "馬鳴默佑。",
    "X65n1277_011:0042b05_2": "採空五畒無餘葉。",
    "X65n1277_011:0042b05_3": "繰就千機有細絲。",
    "X65n1277_011:0042b06_4": "縑帛豐盈。",
    "X65n1277_011:0042b06_5": "貨財富衍。",
    "X65n1277_011:0042b07_0": "工匠者",
    "X65n1277_011:0042b08_0": "性天曉了。",
    "X65n1277_011:0042b08_1": "心匠玲瓏。",
    "X65n1277_011:0042b08_2": "胸中自有規摹。",
    "X65n1277_011:0042b08_3": "手內多施巧妙。",
    "X65n1277_011:0042b09_0": "動止常居吉地。",
    "X65n1277_011:0042b09_1": "工夫每遇貴人。",
    "X65n1277_011:0042b09_2": "倍収日至之財。",
    "X65n1277_011:0042b09_3": "行見天書之詔。",
    "X65n1277_011:0042b11_0": "商賈者",
    "X65n1277_011:0042b12_0": "貿遷如意。",
    "X65n1277_011:0042b12_1": "出塗収鉅萬之財。",
    "X65n1277_011:0042b12_2": "斡運隨心。",
    "X65n1277_011:0042b12_3": "藏市獲倍蓰之利。",
    "X65n1277_011:0042b13_4": "錢流地上。",
    "X65n1277_011:0042b13_5": "貨衍日中。",
    "X65n1277_011:0042b13_6": "世業增崇。",
    "X65n1277_011:0042b13_7": "家聲赫奕。",
    "X65n1277_011:0042b14_0": "攻醫者",
    "X65n1277_011:0042b15_0": "傳不傳之秘方。",
    "X65n1277_011:0042b15_1": "治難治之篤疾。",
    "X65n1277_011:0042b15_2": "活人之功為甚愽。",
    "X65n1277_011:0042b15_3": "濟世之道願益行。",
    "X65n1277_011:0042b16_4": "福德藏王常密扶於動履。",
    "X65n1277_011:0042b16_5": "安樂菩薩更茂介於禎祥。",
    "X65n1277_011:0042b18_0": "尅擇者",
    "X65n1277_011:0042b19_0": "水法深明宗妙。",
    "X65n1277_011:0042b19_1": "山形洞曉玄機。",
    "X65n1277_011:0042b19_2": "胷包造化之工。",
    "X65n1277_011:0042b19_3": "數衍星辰之妙。",
    "X65n1277_011:0042b20_4": "常遇貴人持掣。",
    "X65n1277_011:0042b20_5": "頻蒙富室招延。",
    "X65n1277_011:0042b20_6": "錢財衣食如願如心。",
    "X65n1277_011:0042b21_7": "富貴榮華從謀從望。",
    "X65n1277_011:0042b22_0": "漁釣者",
    "X65n1277_011:0042b23_0": "數尺絲綸長在手。",
    "X65n1277_011:0042b23_1": "一簑煙雨任平生。",
    "X65n1277_011:0042b23_2": "江魚美可求。",
    "X65n1277_011:0042b23_3": "湘水清堪汲。",
    "X65n1277_011:0042b24_4": "壽比岡陵之久。",
    "X65n1277_011:0042b24_5": "福如江海之深。",
    "X65n1277_011:0042c01_0": "開酒肆者",
    "X65n1277_011:0042c02_0": "馬相如之滌器。",
    "X65n1277_011:0042c02_1": "卓文君之當壚。",
    "X65n1277_011:0042c02_2": "挑錢之客常來。",
    "X65n1277_011:0042c02_3": "盜飲之人已去。",
    "X65n1277_011:0042c03_4": "息収萬倍。",
    "X65n1277_011:0042c03_5": "願應千般。",
    "X65n1277_011:0042c04_0": "開店肆者",
    "X65n1277_011:0042c05_0": "百神擁護。",
    "X65n1277_011:0042c05_1": "萬聖維持。",
    "X65n1277_011:0042c05_2": "店舍興隆長似孟甞門下。",
    "X65n1277_011:0042c05_3": "資財進入常如王愷藏中。",
    "X65n1277_011:0042c06_4": "官非口舌逐時消。",
    "X65n1277_011:0042c06_5": "福祿吉祥隨日至。",
    "X65n1277_011:0042c08_0": "為屠戶者",
    "X65n1277_011:0042c09_0": "割肉固殊於方朔。",
    "X65n1277_011:0042c09_1": "操刀濫學於庖丁。",
    "X65n1277_011:0042c09_2": "殺生之罪業冰消。",
    "X65n1277_011:0042c10_3": "潤屋之貨財山積。",
    "X65n1277_011:0042c11_0": "為愽弈者",
    "X65n1277_011:0042c12_0": "妙筭隨心。",
    "X65n1277_011:0042c12_1": "取財遂意。",
    "X65n1277_011:0042c12_2": "但要見機喝采。",
    "X65n1277_011:0042c12_3": "終須得手盈錢。",
    "X65n1277_011:0042c13_0": "從教一擲之時。",
    "X65n1277_011:0042c13_1": "自獲百萬之利。",
    "X65n1277_011:0042c13_2": "官非不惹。",
    "X65n1277_011:0042c13_3": "盜賊無侵。",
    "X65n1277_011:0042c14_0": "行藏消不測之災。",
    "X65n1277_011:0042c14_1": "動止納新來之福。",
    "X65n1277_011:0042c15_0": "為蒿工者",
    "X65n1277_011:0042c16_0": "運篙持漿永無波浪之憂。",
    "X65n1277_011:0042c16_1": "遇險臨灘常有龍神之護。",
    "X65n1277_011:0042c17_0": "凡來往滄波之上。",
    "X65n1277_011:0042c17_1": "如安行平陸之中。",
    "X65n1277_011:0042c17_2": "事事亨通。",
    "X65n1277_011:0042c17_3": "頭頭利濟。",
    "X65n1277_011:0042c19_0": "為僧行者",
    "X65n1277_011:0042c20_0": "天龍外護。",
    "X65n1277_011:0042c20_1": "梵行內清。",
    "X65n1277_011:0042c20_2": "行住坐臥而百福資扶。",
    "X65n1277_011:0042c20_3": "春夏秋冬而千災殄滅。",
    "X65n1277_011:0042c22_0": "為道士者",
    "X65n1277_011:0042c23_0": "星冠离洞府。",
    "X65n1277_011:0042c23_1": "霞帔下煙霄。",
    "X65n1277_011:0042c23_2": "六匣風雷藏寶劒。",
    "X65n1277_011:0042c23_3": "一壺天地養靈丹。",
    "X65n1277_011:0042c24_4": "茨福駢臻。",
    "X65n1277_011:0042c24_5": "椿齡有永。",
    "X65n1277_011:0043a01_0": "為尼姑者",
    "X65n1277_011:0043a02_0": "眉攢柳綠栖身眾妙之門。",
    "X65n1277_011:0043a02_1": "臉映桃紅勵志自然之教。",
    "X65n1277_011:0043a03_0": "無為洞達。",
    "X65n1277_011:0043a03_1": "大道勤行。",
    "X65n1277_011:0043a03_2": "弘持清淨之規。",
    "X65n1277_011:0043a03_3": "延續神仙之壽。",
    "X65n1277_011:0043a04_0": "為師巫者",
    "X65n1277_011:0043a05_0": "救四百四病之苦。",
    "X65n1277_011:0043a05_1": "有百發百中之功。",
    "X65n1277_011:0043a05_2": "靈符鎻斷鬼門關。",
    "X65n1277_011:0043a06_3": "巾斗飜開人世界。",
    "X65n1277_011:0043a06_4": "三元尊聖時時大降吉祥。",
    "X65n1277_011:0043a06_5": "諸部吏兵日日勤加護衛。",
    "X65n1277_011:0043a08_0": "相士論命者",
    "X65n1277_011:0043a09_0": "術妙高古。",
    "X65n1277_011:0043a09_1": "藝精動人。",
    "X65n1277_011:0043a09_2": "論吉凶則究五星五行之奧旨。",
    "X65n1277_011:0043a10_0": "觀貴賤則有百發百中之奇功。",
    "X65n1277_011:0043a10_1": "何須西蜀問君平。",
    "X65n1277_011:0043a10_2": "不待江南尋郭璞。",
    "X65n1277_011:0043a11_3": "笑傲壺中之日月。",
    "X65n1277_011:0043a11_4": "優淤物外之煙霞。",
    "X65n1277_011:0043a12_0": "牙儈者",
    "X65n1277_011:0043a13_0": "持心正直。",
    "X65n1277_011:0043a13_1": "秤物公平。",
    "X65n1277_011:0043a13_2": "會四遠之經商。",
    "X65n1277_011:0043a13_3": "合兩家之交易。",
    "X65n1277_011:0043a14_0": "居多財貨只憑辯口之懸河。",
    "X65n1277_011:0043a14_1": "輻湊舟車常見滿門而成市。",
    "X65n1277_011:0043a16_0": "娼妓者",
    "X65n1277_011:0043a17_0": "雲水之生涯日進。",
    "X65n1277_011:0043a17_1": "煙花之活計風流。",
    "X65n1277_011:0043a17_2": "朝陪四海豪奢。",
    "X65n1277_011:0043a18_0": "暮伴五陵年少。",
    "X65n1277_011:0043a18_1": "綺羅叢裏潛消口舌官非。",
    "X65n1277_011:0043a18_2": "絃管聲中常獲金珠璧玉。",
    "X65n1277_011:0043a21_0": "高峰龍泉院因師集賢語錄卷之十一"
}