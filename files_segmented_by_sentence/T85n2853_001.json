[
    {
        "source_sentence": "布薩文等",
        "from": "T85n2853_001:1301b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛現興善權化物。群生瞻奉鄙惑消亡。雖",
        "from": "T85n2853_001:1301b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法闡三乘戒□□□□□□鈍開合有差。凡",
        "from": "T85n2853_001:1301b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所見聞俱蒙勝益。",
        "from": "T85n2853_001:1301b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此會事也時我□□□□爰及教授闍梨為勸道之主也。",
        "from": "T85n2853_001:1301b12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將使真風廣扇佛日重明□□□謝之端土庶展聽聞之福。",
        "from": "T85n2853_001:1301b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "于時開寶地竪金幢。",
        "from": "T85n2853_001:1301b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香煙與瑞□□□□清梵共笙歌而合響籌稱解脫頂戴受持戒號防非深□□□菩提藏種霑甘露。",
        "from": "T85n2853_001:1301b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以抽芳覺樹祥花。",
        "from": "T85n2853_001:1301b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結香園之味果。",
        "from": "T85n2853_001:1301b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以斯一一□□□□先用莊嚴。",
        "from": "T85n2853_001:1301b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵釋諸王龍天八部。",
        "from": "T85n2853_001:1301b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願身光增益聖力冥□□念含生匡茲教法。",
        "from": "T85n2853_001:1301b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使陰陽應序。",
        "from": "T85n2853_001:1301b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風雨聲和。",
        "from": "T85n2853_001:1301b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稼穡豐登。",
        "from": "T85n2853_001:1301b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人民樂業。",
        "from": "T85n2853_001:1301b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又□勝福莊嚴聖神贊。",
        "from": "T85n2853_001:1301b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願壽齊聖石。",
        "from": "T85n2853_001:1301b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命等靈椿。",
        "from": "T85n2853_001:1301b21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "官寮善披無疆尊宿。",
        "from": "T85n2853_001:1301b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福資有識。",
        "from": "T85n2853_001:1301b22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後兵刀永絕。",
        "from": "T85n2853_001:1301b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教跡流通。",
        "from": "T85n2853_001:1301b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡厥含情俱登覺道。",
        "from": "T85n2853_001:1301b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "布薩文布薩文夫法王應現威振大千。",
        "from": "T85n2853_001:1301b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法教興崇弘通是務。",
        "from": "T85n2853_001:1301b25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "況宣傳戒藏。",
        "from": "T85n2853_001:1301b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每月二時精守不逾福資家國。",
        "from": "T85n2853_001:1301b26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是幢鍾召眾奏梵延僧。",
        "from": "T85n2853_001:1301b27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香騰五雲幡暉眾綵。",
        "from": "T85n2853_001:1301b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總斯多善無限良緣即用莊嚴。",
        "from": "T85n2853_001:1301b28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上界天仙龍神八部。",
        "from": "T85n2853_001:1301b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願威靈潛衛聖德冥加。",
        "from": "T85n2853_001:1301b29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使日月貞明。",
        "from": "T85n2853_001:1301c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陰陽克序。",
        "from": "T85n2853_001:1301c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和風應節。",
        "from": "T85n2853_001:1301c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甘雨順時。",
        "from": "T85n2853_001:1301c01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四人有樂於安邊。",
        "from": "T85n2853_001:1301c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬里無虞於永歲。",
        "from": "T85n2853_001:1301c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即願法永扇釋教弘敷。",
        "from": "T85n2853_001:1301c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切含靈俱登覺道。",
        "from": "T85n2853_001:1301c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "布薩文布薩文夫竊見流沙一方緇徒累百。",
        "from": "T85n2853_001:1301c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其能秉惠炬建法幢。",
        "from": "T85n2853_001:1301c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弘志教於即時。",
        "from": "T85n2853_001:1301c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪津梁於來世者。",
        "from": "T85n2853_001:1301c06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈非我教授之謂歟。",
        "from": "T85n2853_001:1301c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故能使二部律儀策懃而不倦三　□躅相繼而無窮。",
        "from": "T85n2853_001:1301c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "布薩之法洗滌於煩籠。",
        "from": "T85n2853_001:1301c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住持之功繼明於動植。",
        "from": "T85n2853_001:1301c09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願以斯自□無疆福因先用莊嚴。",
        "from": "T85n2853_001:1301c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵釋四王龍天八部。",
        "from": "T85n2853_001:1301c10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即願福德逾增威光轉盛。",
        "from": "T85n2853_001:1301c11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "消除疫癘利樂生靈。",
        "from": "T85n2853_001:1301c11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三邊無變怪之憂。",
        "from": "T85n2853_001:1301c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百穀有豐登之樂。",
        "from": "T85n2853_001:1301c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又用功德奉資聖神贊普。",
        "from": "T85n2853_001:1301c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願明齊舜宇靡叶堯湯。",
        "from": "T85n2853_001:1301c13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "補恩惠於八方。",
        "from": "T85n2853_001:1301c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "親鐱黎於一子。",
        "from": "T85n2853_001:1301c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次用功德莊嚴我節兒上論。",
        "from": "T85n2853_001:1301c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願榮高往歲慶益今辰。",
        "from": "T85n2853_001:1301c15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此用莊嚴都督社公。",
        "from": "T85n2853_001:1301c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟福逐年長壽逾金石。",
        "from": "T85n2853_001:1301c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後散霑法界普及有情。",
        "from": "T85n2853_001:1301c17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "罷四季文罷四季文夫佛日舒光無災不遣。",
        "from": "T85n2853_001:1301c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "般若流演河福不臻。",
        "from": "T85n2853_001:1301c19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今者啟八龍之寶藏。",
        "from": "T85n2853_001:1301c20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開五印之真文。",
        "from": "T85n2853_001:1301c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "會二百之梵輪。",
        "from": "T85n2853_001:1301c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於三旬而轉誦者。",
        "from": "T85n2853_001:1301c21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則我當今聖神贊保。",
        "from": "T85n2853_001:1301c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願功德之端敘矣。",
        "from": "T85n2853_001:1301c22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏惟聖神贊普道邁義軒。",
        "from": "T85n2853_001:1301c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功超堯舜握圖御歷秉錄匡時八表廓清廣弘。",
        "from": "T85n2853_001:1301c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十善家以別崇妙福特竪芳。",
        "from": "T85n2853_001:1301c24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因建四季道場。",
        "from": "T85n2853_001:1301c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "希萬機永古。",
        "from": "T85n2853_001:1301c25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由是照自舟闕遠今燉煌每歲修崇恒為常戒。",
        "from": "T85n2853_001:1301c26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今茲會者則春季之終畢矣。",
        "from": "T85n2853_001:1301c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是日也無田初綠桃苑讒紅。",
        "from": "T85n2853_001:1301c27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玉綵向終金言告罷。",
        "from": "T85n2853_001:1301c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相此殊勝無疆福。",
        "from": "T85n2853_001:1301c28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因先用莊嚴。",
        "from": "T85n2853_001:1301c28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫應化無方神用不倦。",
        "from": "T85n2853_001:1301c29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恩霑動植。",
        "from": "T85n2853_001:1301c29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福洽生靈。",
        "from": "T85n2853_001:1301c29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天中之天獨擅其務。",
        "from": "T85n2853_001:1302a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至於妙事豈足繁詞。",
        "from": "T85n2853_001:1302a01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此瑞之興抑有由矣。",
        "from": "T85n2853_001:1302a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今則三春中律四序初分。",
        "from": "T85n2853_001:1302a02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "絮拆南枝。",
        "from": "T85n2853_001:1302a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "氷開北岸。",
        "from": "T85n2853_001:1302a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣法王之化跡冀彌千殃。",
        "from": "T85n2853_001:1302a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "揚大聖之辭榮悕臻萬善。",
        "from": "T85n2853_001:1302a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是不局月殿。",
        "from": "T85n2853_001:1302a04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜擊霜鍾受集緇徒。",
        "from": "T85n2853_001:1302a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "競持幡蓋列四門之勝會旋一郡之都城。",
        "from": "T85n2853_001:1302a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "像設金容。",
        "from": "T85n2853_001:1302a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雲非鷲嶺。",
        "from": "T85n2853_001:1302a06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眉開毫月。",
        "from": "T85n2853_001:1302a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "花步蓮宮。",
        "from": "T85n2853_001:1302a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傾市傾城搖山蕩谷。",
        "from": "T85n2853_001:1302a07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦維厭慾豈用年哉。",
        "from": "T85n2853_001:1302a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所冀四王護世百福。",
        "from": "T85n2853_001:1302a08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "潛加攙搶掃於天門。",
        "from": "T85n2853_001:1302a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疫癘藏於地戶。",
        "from": "T85n2853_001:1302a09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慶雲有世善色。",
        "from": "T85n2853_001:1302a09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凝空倒載干戈。",
        "from": "T85n2853_001:1302a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修文偃武。",
        "from": "T85n2853_001:1302a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總斯功德迴奉龍天八部。",
        "from": "T85n2853_001:1302a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願威光恒赫神力無涯。",
        "from": "T85n2853_001:1302a11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "災害不生禍亂不作。",
        "from": "T85n2853_001:1302a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又持勝福上資聖神贊普。",
        "from": "T85n2853_001:1302a12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願萬國納貢四海來庭。",
        "from": "T85n2853_001:1302a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶歷恒昌。",
        "from": "T85n2853_001:1302a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金石比壽。",
        "from": "T85n2853_001:1302a13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又持勝福莊嚴節兒都督。",
        "from": "T85n2853_001:1302a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願壽命逾遠祿極萬鐘部落。",
        "from": "T85n2853_001:1302a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使官遼門傳九戟。",
        "from": "T85n2853_001:1302a15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後散霑法界普洎有情。",
        "from": "T85n2853_001:1302a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賴此勝因咸登樂果。",
        "from": "T85n2853_001:1302a16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行城文行城文我法王之利見也大矣哉。",
        "from": "T85n2853_001:1302a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故降神兜率現影王城。",
        "from": "T85n2853_001:1302a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀妙色有苦於癰瘡。",
        "from": "T85n2853_001:1302a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "骸寶住乃踰於宮闕。",
        "from": "T85n2853_001:1302a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "御四魔而登正覺。",
        "from": "T85n2853_001:1302a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居三界而獨稱尊。",
        "from": "T85n2853_001:1302a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神化難量叵能談也。",
        "from": "T85n2853_001:1302a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今者春陽今月地拆萠芽。",
        "from": "T85n2853_001:1302a21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳥嚮含春風搖。",
        "from": "T85n2853_001:1302a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "抑於是豁開奈苑洞啟蓮宮。",
        "from": "T85n2853_001:1302a22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金相煥爛於四衢。",
        "from": "T85n2853_001:1302a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "銀毫暉舒於八極。",
        "from": "T85n2853_001:1302a23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隱隱振振如旋白飯之城。",
        "from": "T85n2853_001:1302a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "巍巍峨峨似繞迦維之闕。",
        "from": "T85n2853_001:1302a24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊卑務集大小雲奔。",
        "from": "T85n2853_001:1302a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "笙歌競奏而啾留法曲。",
        "from": "T85n2853_001:1302a25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爭陳而槽揆所冀百福被矣。",
        "from": "T85n2853_001:1302a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千障雲祛覩勝相兮獲因。",
        "from": "T85n2853_001:1302a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞻妙色兮生福。",
        "from": "T85n2853_001:1302a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總斯多善。",
        "from": "T85n2853_001:1302a27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫限良緣。",
        "from": "T85n2853_001:1302a27_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先用莊嚴。",
        "from": "T85n2853_001:1302a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋梵四王龍天八部。",
        "from": "T85n2853_001:1302a28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願增威力益神通。",
        "from": "T85n2853_001:1302a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "併妖氛驅疫癘。",
        "from": "T85n2853_001:1302a29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次持勝福奉用莊嚴。",
        "from": "T85n2853_001:1302a29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當今聖神贊普。",
        "from": "T85n2853_001:1302b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願壽永固等乾坤六幾賓四海。",
        "from": "T85n2853_001:1302b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏次用莊嚴節兒尚論麥及都督杜公為雲為雨濟枯涸。",
        "from": "T85n2853_001:1302b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於明朝部落使諸官建忠貞於聖伐。",
        "from": "T85n2853_001:1302b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後上空窮空界傍枯十方。",
        "from": "T85n2853_001:1302b04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賴此勝因成正覺道。",
        "from": "T85n2853_001:1302b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行城文行城文應化無窮作用不倦。",
        "from": "T85n2853_001:1302b07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恩霑動植福洽生靈。",
        "from": "T85n2853_001:1302b07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天中之天獨擅其勢。",
        "from": "T85n2853_001:1302b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至於妙事豈足繁詞。",
        "from": "T85n2853_001:1302b08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此會之瑞抑有由矣。",
        "from": "T85n2853_001:1302b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今則四序將盡。",
        "from": "T85n2853_001:1302b09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三春肇來。",
        "from": "T85n2853_001:1302b09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "送故納新。",
        "from": "T85n2853_001:1302b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除災建福。",
        "from": "T85n2853_001:1302b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冀清封壘保艾蒸黎。",
        "from": "T85n2853_001:1302b10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是月殿不局霜鍾。",
        "from": "T85n2853_001:1302b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜擊爰集緇侶。",
        "from": "T85n2853_001:1302b11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉索幡花出佛像。",
        "from": "T85n2853_001:1302b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於四門遶重城而一匝儼然相好鷲嶺雲非煥爛。",
        "from": "T85n2853_001:1302b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毫光蓮峯降步傾城傾市蕩谷搖山舍衛踰城豈用年矣。",
        "from": "T85n2853_001:1302b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即冀四王護世百福潛加攙槍掃於天門。",
        "from": "T85n2853_001:1302b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疫癘藏於地戶。",
        "from": "T85n2853_001:1302b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庶雲布族喜色凝空。",
        "from": "T85n2853_001:1302b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倒載干戈修文偃武。",
        "from": "T85n2853_001:1302b16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總斯功德迴施龍天八部。",
        "from": "T85n2853_001:1302b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願威光恒赫神力無涯。",
        "from": "T85n2853_001:1302b17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "災害不生禍亂不作。",
        "from": "T85n2853_001:1302b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又持景福上資聖神贊普。",
        "from": "T85n2853_001:1302b18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願萬國納貢四海來庭。",
        "from": "T85n2853_001:1302b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶曆恒昌金石比壽。",
        "from": "T85n2853_001:1302b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皇太子殿下游雷遠震少海。",
        "from": "T85n2853_001:1302b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長清夫人蘭桂永芳。",
        "from": "T85n2853_001:1302b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妃嬪神花獻頌。",
        "from": "T85n2853_001:1302b21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又持勝福總用莊嚴我都督杜公祿極萬鍾。",
        "from": "T85n2853_001:1302b22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後。",
        "from": "T85n2853_001:1302b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行軍轉經文行軍轉經文夫諸佛興悲無緣普備有情。",
        "from": "T85n2853_001:1302b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見異感迹緣老故使歸向者福逐。",
        "from": "T85n2853_001:1302b25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願生輕毀者禍隨心起。",
        "from": "T85n2853_001:1302b25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則知禍福自致非聖愛憎者與然。",
        "from": "T85n2853_001:1302b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此會轉經意者。",
        "from": "T85n2853_001:1302b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則我東軍國相論掣脯敬為西征。",
        "from": "T85n2853_001:1302b27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將仕保願功德之建修也。",
        "from": "T85n2853_001:1302b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏惟相公天降英靈地資秀氣。",
        "from": "T85n2853_001:1302b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "岳山作鎮謀略坐籌。",
        "from": "T85n2853_001:1302b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每見北勇興師頻犯邊境。",
        "from": "T85n2853_001:1302c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "抄劫人畜暴枆田苗。",
        "from": "T85n2853_001:1302c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使人色不安峯飈數舉。",
        "from": "T85n2853_001:1302c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我國相慢然忿起。",
        "from": "T85n2853_001:1302c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怒髮衝冠。",
        "from": "T85n2853_001:1302c02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂擇良才主兵西討。",
        "from": "T85n2853_001:1302c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖料謀指掌。",
        "from": "T85n2853_001:1302c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百無一遺。",
        "from": "T85n2853_001:1302c03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然必賴福資保其清吉。",
        "from": "T85n2853_001:1302c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是以遠啟三危之侶遙祈八藏之文。",
        "from": "T85n2853_001:1302c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冀仕馬平安永寧家國。",
        "from": "T85n2853_001:1302c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故使虔虔一志諷誦金剛。",
        "from": "T85n2853_001:1302c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "濟濟僧尼宣揚般若。",
        "from": "T85n2853_001:1302c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想此殊勝夫何以加。",
        "from": "T85n2853_001:1302c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先用莊嚴護世四王龍神八部。",
        "from": "T85n2853_001:1302c07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願使威光盛福力增。",
        "from": "T85n2853_001:1302c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使西陳齊威北戎伏欵。",
        "from": "T85n2853_001:1302c08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又用莊嚴行軍將相。",
        "from": "T85n2853_001:1302c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願才智日新福同山積。",
        "from": "T85n2853_001:1302c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽命遐遠鎮坐臺階。",
        "from": "T85n2853_001:1302c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸將仕等三寶撫護萬善莊嚴。",
        "from": "T85n2853_001:1302c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後。",
        "from": "T85n2853_001:1302c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉經文轉經文我法王之利見也難可詳焉。",
        "from": "T85n2853_001:1302c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其有歸依者果無不剋矣。",
        "from": "T85n2853_001:1302c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然今啟龍藏宅一心擊洪鍾邀二眾者。",
        "from": "T85n2853_001:1302c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其誰施之。",
        "from": "T85n2853_001:1302c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則我國相論掣脯敬為西征將仕保願功德之所建矣。",
        "from": "T85n2853_001:1302c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏惟相公乃何岳降靈神威動物咸恩。",
        "from": "T85n2853_001:1302c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出塞曰　撫俗安邊。",
        "from": "T85n2853_001:1302c17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一昨春初扶陽作𮓃摽棹人畜。",
        "from": "T85n2853_001:1302c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由是大舉軍師併除兇醜。",
        "from": "T85n2853_001:1302c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖兵強仕勇。",
        "from": "T85n2853_001:1302c19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然福乃禍師。",
        "from": "T85n2853_001:1302c19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是以遠杖流沙精祈轉念。",
        "from": "T85n2853_001:1302c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今者能事遐列勝福斯圓。",
        "from": "T85n2853_001:1302c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總用莊嚴行軍將相即體。",
        "from": "T85n2853_001:1302c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願使諸佛護念使無傷損之憂。",
        "from": "T85n2853_001:1302c22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八部潛加願起降和之意。",
        "from": "T85n2853_001:1302c22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後人馬咸吉仕卒保康。",
        "from": "T85n2853_001:1302c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各守垂永除征戰。",
        "from": "T85n2853_001:1302c23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後散霑法界普及有情。",
        "from": "T85n2853_001:1302c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賴此方因咸登覺道。",
        "from": "T85n2853_001:1302c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置傘文置傘文夫除災靜難者莫善於佛頂密言。",
        "from": "T85n2853_001:1302c27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "集福延休者事資於行城念誦。",
        "from": "T85n2853_001:1302c28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今者春陽今月寒色猶威。",
        "from": "T85n2853_001:1302c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請二部之僧尼建白幢於五所者。",
        "from": "T85n2853_001:1302c29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其誰施之。",
        "from": "T85n2853_001:1303a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時則有節兒都督為合邑黎元報願功德之所建矣。",
        "from": "T85n2853_001:1303a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏惟節兒都督公平育物罄節安邊。",
        "from": "T85n2853_001:1303a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恐瘳疾流行災殃條起。",
        "from": "T85n2853_001:1303a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是以預修弘願建竪良因。",
        "from": "T85n2853_001:1303a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行城將殄於妖氛。",
        "from": "T85n2853_001:1303a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪幢用臻乎福利。",
        "from": "T85n2853_001:1303a04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今既能事備勝願享。",
        "from": "T85n2853_001:1303a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福長空量難比。",
        "from": "T85n2853_001:1303a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以茲勝利先用莊嚴梵釋四王龍神八部。",
        "from": "T85n2853_001:1303a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置傘文置傘文夫覩相興善者無出於應化之身。",
        "from": "T85n2853_001:1303a08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "穰災怯禍者莫過乎佛頂心呪。",
        "from": "T85n2853_001:1303a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然無身之身故現身而齊難。",
        "from": "T85n2853_001:1303a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無說而說說心呪而持危。",
        "from": "T85n2853_001:1303a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盛事之興莫大於茲矣。",
        "from": "T85n2853_001:1303a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今者燉煌之府內竪白法之勝幢。",
        "from": "T85n2853_001:1303a11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設佛頂於四門。",
        "from": "T85n2853_001:1303a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使黑業之殄掃厥。",
        "from": "T85n2853_001:1303a12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此會其誰施之。",
        "from": "T85n2853_001:1303a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時則有二節兒岳牧杜公等。",
        "from": "T85n2853_001:1303a13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為城惶報安之所建也。",
        "from": "T85n2853_001:1303a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯節兒都督以慮燉煌西極境接北胡。",
        "from": "T85n2853_001:1303a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "躍馬控弦𡨥盜無准。",
        "from": "T85n2853_001:1303a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恐󱫝踐稼穡百減衣食之源。",
        "from": "T85n2853_001:1303a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九農匪登使萬人壞罄懸之念。",
        "from": "T85n2853_001:1303a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以互相設計務在安人。",
        "from": "T85n2853_001:1303a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若論護國匡邦無過。",
        "from": "T85n2853_001:1303a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建斯幢傘。",
        "from": "T85n2853_001:1303a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即冀除災殃於不毛之地。",
        "from": "T85n2853_001:1303a18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "併疫癘於無何有之鄉。",
        "from": "T85n2853_001:1303a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五穀無霜雹之災。",
        "from": "T85n2853_001:1303a19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬品登人壽之城。",
        "from": "T85n2853_001:1303a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先資是福奉用莊嚴聖神讚普。",
        "from": "T85n2853_001:1303a20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願寶位永固金石齊年。",
        "from": "T85n2853_001:1303a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四海澄清萬方朝貢。",
        "from": "T85n2853_001:1303a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦持此善莊嚴節兒都督為霜為雨濟枯旱。",
        "from": "T85n2853_001:1303a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於明朝部落使諸官建中貞於聖代。",
        "from": "T85n2853_001:1303a23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又持是善亦用莊嚴。",
        "from": "T85n2853_001:1303a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二教授闍梨。",
        "from": "T85n2853_001:1303a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願極拔殊苦超出輪迴。",
        "from": "T85n2853_001:1303a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽等寒松福如春草。",
        "from": "T85n2853_001:1303a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後薄霑動植遍及無疆。",
        "from": "T85n2853_001:1303a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賴此勝因登正覺道。",
        "from": "T85n2853_001:1303a26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置傘文置傘文夫延祥展慶必賴於勝幢。",
        "from": "T85n2853_001:1303a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掃蘖除災要資於兒力。",
        "from": "T85n2853_001:1303a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故使善住聞其增壽。",
        "from": "T85n2853_001:1303a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慶喜剋獲本心。",
        "from": "T85n2853_001:1303a29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魑魅畏之逃刑。",
        "from": "T85n2853_001:1303b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天魔怖而求救。",
        "from": "T85n2853_001:1303b01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大哉神兒。",
        "from": "T85n2853_001:1303b01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無得而稱者歟。",
        "from": "T85n2853_001:1303b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今屬和風動物蟄戶。",
        "from": "T85n2853_001:1303b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將開幡蓋俳佪緇倫肅穆者何。",
        "from": "T85n2853_001:1303b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂耶時則有我節兒。",
        "from": "T85n2853_001:1303b03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尚論及都督杜公等。",
        "from": "T85n2853_001:1303b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並乃養人如子憂國同家。",
        "from": "T85n2853_001:1303b04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恐妖氛肆惡於城中。",
        "from": "T85n2853_001:1303b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "品物屢遭於逆厄。",
        "from": "T85n2853_001:1303b05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是以三陽令月啟三福於釋尊。",
        "from": "T85n2853_001:1303b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四季初辰竪四門之利。",
        "from": "T85n2853_001:1303b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總斯殊妙最上福田。",
        "from": "T85n2853_001:1303b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡用莊嚴梵釋四王龍神八部。",
        "from": "T85n2853_001:1303b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願威光盛福力增。",
        "from": "T85n2853_001:1303b08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "育黎無護軍國。",
        "from": "T85n2853_001:1303b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我聖神贊普。",
        "from": "T85n2853_001:1303b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願聖躬堅遠。",
        "from": "T85n2853_001:1303b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日往月來。",
        "from": "T85n2853_001:1303b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶住恒昌。",
        "from": "T85n2853_001:1303b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天長地久。",
        "from": "T85n2853_001:1303b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "節兒都督松皇比壽。",
        "from": "T85n2853_001:1303b10_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福慶相資。",
        "from": "T85n2853_001:1303b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "部落使諸官等。",
        "from": "T85n2853_001:1303b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願助理平和。",
        "from": "T85n2853_001:1303b11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟清惟直。",
        "from": "T85n2853_001:1303b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後四時順五穀登。",
        "from": "T85n2853_001:1303b12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百殃除萬祥集。",
        "from": "T85n2853_001:1303b12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "般若神兒諸佛所師。",
        "from": "T85n2853_001:1303b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大眾□城一切普誦。",
        "from": "T85n2853_001:1303b13_17",
        "context_with_next_sentence": true
    }
]