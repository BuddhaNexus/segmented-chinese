[
    {
        "source_sentence": "王梵志詩集　　前序□□□□上",
        "from": "T85n2863_001:1323a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但以佛教道法無我苦空。知先薄之福緣。悉",
        "from": "T85n2863_001:1323a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後之因果。撰修勸善誡勗非違。目錄雖則數",
        "from": "T85n2863_001:1323a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "條。",
        "from": "T85n2863_001:1323a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "制詩三百餘首。",
        "from": "T85n2863_001:1323a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且言時事不浪虛。",
        "from": "T85n2863_001:1323a20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "談王梵志之貴文。",
        "from": "T85n2863_001:1323a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "習丁郭之要義。",
        "from": "T85n2863_001:1323a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不受經典皆陳俗語。",
        "from": "T85n2863_001:1323a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非但智士迴意實易。",
        "from": "T85n2863_001:1323a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愚夫改容。",
        "from": "T85n2863_001:1323a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠近傳聞勸徵令善。",
        "from": "T85n2863_001:1323a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貪婪之史□□侵漁。",
        "from": "T85n2863_001:1323a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尸祿之官自當廉謹。",
        "from": "T85n2863_001:1323a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各雖愚昧情極愴然。",
        "from": "T85n2863_001:1323a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一遍略尋三思無忘。",
        "from": "T85n2863_001:1323a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縱大德講說。",
        "from": "T85n2863_001:1323a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不及讀此善文。",
        "from": "T85n2863_001:1323a25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逆子定省翻成甚孝。",
        "from": "T85n2863_001:1323a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嬾婦晨夕事姑嫜。",
        "from": "T85n2863_001:1323a26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "査郎𨉱子生慚愧。",
        "from": "T85n2863_001:1323a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸州遊客憶家鄉。",
        "from": "T85n2863_001:1323a27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慵夫夜起。",
        "from": "T85n2863_001:1323a27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嬾婦徹明。",
        "from": "T85n2863_001:1323a27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對絹筐悉皆咸臻。",
        "from": "T85n2863_001:1323a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知罪福懃耕懇苦足口糧。",
        "from": "T85n2863_001:1323a28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一志五情不改易。",
        "from": "T85n2863_001:1323a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東州西郡並稱揚。",
        "from": "T85n2863_001:1323a29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但令讀此篇章熟。",
        "from": "T85n2863_001:1323b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頑愚暗憃悉賢良。",
        "from": "T85n2863_001:1323b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遙看世間人。",
        "from": "T85n2863_001:1323b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "村坊安社邑。",
        "from": "T85n2863_001:1323b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一家有死生。",
        "from": "T85n2863_001:1323b02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合村相就泣。",
        "from": "T85n2863_001:1323b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "張口哭他屍。",
        "from": "T85n2863_001:1323b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不知身去急。",
        "from": "T85n2863_001:1323b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本是長眠鬼。",
        "from": "T85n2863_001:1323b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "暫來地上立。",
        "from": "T85n2863_001:1323b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲似養兒氈。",
        "from": "T85n2863_001:1323b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迴乾且就濕。",
        "from": "T85n2863_001:1323b04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前死深理却。",
        "from": "T85n2863_001:1323b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後死續即入。",
        "from": "T85n2863_001:1323b05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾富有錢時。",
        "from": "T85n2863_001:1323b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婦兒看我好。",
        "from": "T85n2863_001:1323b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾若脫衣裳。",
        "from": "T85n2863_001:1323b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與吾疊袍襖。",
        "from": "T85n2863_001:1323b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾出經求去。",
        "from": "T85n2863_001:1323b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "送吾即上道。",
        "from": "T85n2863_001:1323b07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將□入舍來。",
        "from": "T85n2863_001:1323b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見吾滿面咲。",
        "from": "T85n2863_001:1323b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遶吾白鴿旋。",
        "from": "T85n2863_001:1323b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怜似鸚鵡鳥。",
        "from": "T85n2863_001:1323b08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "邂逅暫時貧。",
        "from": "T85n2863_001:1323b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "看吾即貌哨。",
        "from": "T85n2863_001:1323b09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人有七貧時。",
        "from": "T85n2863_001:1323b09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七富四相報。",
        "from": "T85n2863_001:1323b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從財不顧人。",
        "from": "T85n2863_001:1323b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且看來時道。",
        "from": "T85n2863_001:1323b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "家口總死盡。",
        "from": "T85n2863_001:1323b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾死無親表。",
        "from": "T85n2863_001:1323b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "急首買資產。",
        "from": "T85n2863_001:1323b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與設逆修齋。",
        "from": "T85n2863_001:1323b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "託生得好處。",
        "from": "T85n2863_001:1323b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身死雇人埋。",
        "from": "T85n2863_001:1323b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "錢財隣保出。",
        "from": "T85n2863_001:1323b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "任儞自相差。",
        "from": "T85n2863_001:1323b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身如圈裏羊。",
        "from": "T85n2863_001:1323b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命報怜相當。",
        "from": "T85n2863_001:1323b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羊即披毛走。",
        "from": "T85n2863_001:1323b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人著好衣裳。",
        "from": "T85n2863_001:1323b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脫衣赤體立。",
        "from": "T85n2863_001:1323b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則假不如羊。",
        "from": "T85n2863_001:1323b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羊即日日死。",
        "from": "T85n2863_001:1323b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人還日日亡。",
        "from": "T85n2863_001:1323b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從頭捉將去。",
        "from": "T85n2863_001:1323b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還同肥好羊。",
        "from": "T85n2863_001:1323b16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羊即辛苦死。",
        "from": "T85n2863_001:1323b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人去無破破傷。",
        "from": "T85n2863_001:1323b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命絕逐他走。",
        "from": "T85n2863_001:1323b17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魂魄曆他鄉。",
        "from": "T85n2863_001:1323b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有錢多造福。",
        "from": "T85n2863_001:1323b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喫著好衣裳。",
        "from": "T85n2863_001:1323b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愚人廣造罪。",
        "from": "T85n2863_001:1323b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "志者好思量。",
        "from": "T85n2863_001:1323b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可咲世間人。",
        "from": "T85n2863_001:1323b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "癡多黠者少。",
        "from": "T85n2863_001:1323b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不愁死路長。",
        "from": "T85n2863_001:1323b20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貪著苦煩惱。",
        "from": "T85n2863_001:1323b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜眠遊鬼界。",
        "from": "T85n2863_001:1323b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天曉歸人道。",
        "from": "T85n2863_001:1323b21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忽起相羅拽啾唧索祖調。",
        "from": "T85n2863_001:1323b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貧苦無處得。",
        "from": "T85n2863_001:1323b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相接彼鞭拷。",
        "from": "T85n2863_001:1323b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生時有苦痛。",
        "from": "T85n2863_001:1323b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不如早死好。",
        "from": "T85n2863_001:1323b23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他家咲吾貧吾貧。",
        "from": "T85n2863_001:1323b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾貧極快樂。",
        "from": "T85n2863_001:1323b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無牛亦無馬。",
        "from": "T85n2863_001:1323b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不愁賤抄掠。",
        "from": "T85n2863_001:1323b25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儞富戶役高。",
        "from": "T85n2863_001:1323b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羞利並用却。",
        "from": "T85n2863_001:1323b25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾無呼喚處。",
        "from": "T85n2863_001:1323b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飽喫長展脚。",
        "from": "T85n2863_001:1323b26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儞富披錦袍。",
        "from": "T85n2863_001:1323b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尋常被纏縛。",
        "from": "T85n2863_001:1323b27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "窮苦無煩惱。",
        "from": "T85n2863_001:1323b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "草衣隨體著。",
        "from": "T85n2863_001:1323b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大有愚癡君。",
        "from": "T85n2863_001:1323b28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獨身無兒子。",
        "from": "T85n2863_001:1323b28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣貪多覓財。",
        "from": "T85n2863_001:1323b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "養奴多□婢。",
        "from": "T85n2863_001:1323b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伺命門前喚。",
        "from": "T85n2863_001:1323b29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不客別隣里。",
        "from": "T85n2863_001:1323b29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "死得四片板。",
        "from": "T85n2863_001:1323c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一條黃衾□。",
        "from": "T85n2863_001:1323c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "錢財奴婢用。",
        "from": "T85n2863_001:1323c01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "任將別經紀。",
        "from": "T85n2863_001:1323c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有錢不解用。",
        "from": "T85n2863_001:1323c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空手入都等。",
        "from": "T85n2863_001:1323c02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汎□□□道。",
        "from": "T85n2863_001:1323c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "負持愚癡鬼。",
        "from": "T85n2863_001:1323c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "荒忙身卒死。",
        "from": "T85n2863_001:1323c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即遍伺命使。",
        "from": "T85n2863_001:1323c03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "反縛棒□□。",
        "from": "T85n2863_001:1323c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□渡奈河水。",
        "from": "T85n2863_001:1323c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倒󱭊至廳前。",
        "from": "T85n2863_001:1323c04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "枷棒遍身起。",
        "from": "T85n2863_001:1323c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "死經一七日。",
        "from": "T85n2863_001:1323c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刑受罪鬼牛。",
        "from": "T85n2863_001:1323c05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭錢叉□□卒把乃□。",
        "from": "T85n2863_001:1323c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "碓擣磑磨身。",
        "from": "T85n2863_001:1323c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覆生還覆死。",
        "from": "T85n2863_001:1323c06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "橑亂失精□。",
        "from": "T85n2863_001:1323c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無由見家裏。",
        "from": "T85n2863_001:1323c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妻是兒被他人□後翁使。",
        "from": "T85n2863_001:1323c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奴事新郎君。",
        "from": "T85n2863_001:1323c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婢逐度娘子。",
        "from": "T85n2863_001:1323c08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "駟馬被金鞍。",
        "from": "T85n2863_001:1323c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鏤鐙銀鞦轡。",
        "from": "T85n2863_001:1323c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "角弓無主張。",
        "from": "T85n2863_001:1323c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶劍□著地。",
        "from": "T85n2863_001:1323c09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設却百日□。",
        "from": "T85n2863_001:1323c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "渾家忘却儞。",
        "from": "T85n2863_001:1323c10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "錢財他人用。",
        "from": "T85n2863_001:1323c10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "古來尋常事。",
        "from": "T85n2863_001:1323c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前人多貯積。",
        "from": "T85n2863_001:1323c11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後人無慚愧。",
        "from": "T85n2863_001:1323c11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是守財奴。",
        "from": "T85n2863_001:1323c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不兌貧窮死。",
        "from": "T85n2863_001:1323c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫婦相對坐。",
        "from": "T85n2863_001:1323c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千年亦不足。",
        "from": "T85n2863_001:1323c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一箇病著床。",
        "from": "T85n2863_001:1323c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遙看手不觸心報到頭來。",
        "from": "T85n2863_001:1323c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徒費將錢上。",
        "from": "T85n2863_001:1323c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶物積如山。",
        "from": "T85n2863_001:1323c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "死得一棺木。",
        "from": "T85n2863_001:1323c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空手把兩拳。",
        "from": "T85n2863_001:1323c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口裏徒含玉。",
        "from": "T85n2863_001:1323c15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永離臺上鏡。",
        "from": "T85n2863_001:1323c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無心開衣眼鏡塵塵滿中。",
        "from": "T85n2863_001:1323c16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剪刀生衣□。",
        "from": "T85n2863_001:1323c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平坐歌舞處。",
        "from": "T85n2863_001:1323c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無由更習曲。",
        "from": "T85n2863_001:1323c17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "琵琶絕咲聲。",
        "from": "T85n2863_001:1323c17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "琴絃斷不續。",
        "from": "T85n2863_001:1323c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "花悵後人眼。",
        "from": "T85n2863_001:1323c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前人自薄福。",
        "from": "T85n2863_001:1323c18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生座七寶堂。",
        "from": "T85n2863_001:1323c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "死入土角觸。",
        "from": "T85n2863_001:1323c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喪車相勾牽。",
        "from": "T85n2863_001:1323c19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼□還相哭。",
        "from": "T85n2863_001:1323c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日埋幾千般。",
        "from": "T85n2863_001:1323c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光影急迅速。",
        "from": "T85n2863_001:1323c20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "富者辨棺木。",
        "from": "T85n2863_001:1323c20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貧窮席裏角。",
        "from": "T85n2863_001:1323c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相共唱奈何。",
        "from": "T85n2863_001:1323c21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "送著空塚□。",
        "from": "T85n2863_001:1323c21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千休即萬休。",
        "from": "T85n2863_001:1323c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永別生平樂。",
        "from": "T85n2863_001:1323c22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "志者入西方。",
        "from": "T85n2863_001:1323c22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愚人墮地獄。",
        "from": "T85n2863_001:1323c22_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□頭入苦海。",
        "from": "T85n2863_001:1323c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冥冥不省覺。",
        "from": "T85n2863_001:1323c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擎頭鄉里行。",
        "from": "T85n2863_001:1323c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "事當逞靴襖。",
        "from": "T85n2863_001:1323c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有錢但著用。",
        "from": "T85n2863_001:1323c24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫作千年調。",
        "from": "T85n2863_001:1323c24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百歲乃有一人。",
        "from": "T85n2863_001:1323c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得七十者稀。",
        "from": "T85n2863_001:1323c25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "張眼看他死。",
        "from": "T85n2863_001:1323c25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能自覺知。",
        "from": "T85n2863_001:1323c25_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "癡皮裹膿血。",
        "from": "T85n2863_001:1323c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頑骨強相隨。",
        "from": "T85n2863_001:1323c26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩脚行衣架步步阿鼻。",
        "from": "T85n2863_001:1323c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雙盲不識鬼。",
        "from": "T85n2863_001:1323c27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伺命急來追。",
        "from": "T85n2863_001:1323c27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "赤繩串著項。",
        "from": "T85n2863_001:1323c27_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "反縛棒背皮。",
        "from": "T85n2863_001:1323c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "露頭赤脚走。",
        "from": "T85n2863_001:1323c28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身上無衣被。",
        "from": "T85n2863_001:1323c28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獨自心中驟。",
        "from": "T85n2863_001:1323c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四面被兵圍。",
        "from": "T85n2863_001:1323c29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向前十道稅。",
        "from": "T85n2863_001:1323c29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "背後鐵鎚鎚。",
        "from": "T85n2863_001:1324a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伺命張弓射。",
        "from": "T85n2863_001:1324a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦痛劇刀錐。",
        "from": "T85n2863_001:1324a01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使者門前喚忙怕不容遲。",
        "from": "T85n2863_001:1324a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "裸體逐他走。",
        "from": "T85n2863_001:1324a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "渾舍共號悲。",
        "from": "T85n2863_001:1324a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宅舍無身護。",
        "from": "T85n2863_001:1324a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妻子被人欺。",
        "from": "T85n2863_001:1324a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "錢財不關已。",
        "from": "T85n2863_001:1324a03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莊收永長離。",
        "from": "T85n2863_001:1324a03_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三魂無倚住。",
        "from": "T85n2863_001:1324a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七魄散頭飛。",
        "from": "T85n2863_001:1324a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汎淪三惡道。",
        "from": "T85n2863_001:1324a04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "家內無人知。",
        "from": "T85n2863_001:1324a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有衣不能著。",
        "from": "T85n2863_001:1324a05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有馬不能騎。",
        "from": "T85n2863_001:1324a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有奴不能使。",
        "from": "T85n2863_001:1324a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有婢不相隨。",
        "from": "T85n2863_001:1324a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有食不能喫。",
        "from": "T85n2863_001:1324a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向前恒受飢。",
        "from": "T85n2863_001:1324a06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冥冥地獄苦。",
        "from": "T85n2863_001:1324a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難見出頭時。",
        "from": "T85n2863_001:1324a07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□逐次第去。",
        "from": "T85n2863_001:1324a07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "却活知有誰。",
        "from": "T85n2863_001:1324a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善勸諸貴等。",
        "from": "T85n2863_001:1324a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□□□□□□造橋樑運度身得過福至生西方□□□□□□□□知厭足身是有限。",
        "from": "T85n2863_001:1324a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身程期太劇。",
        "from": "T85n2863_001:1324a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但縱□□□□□□□□轉燭□人連腦癡買錦妻裴束無心□□□□□□□□誤。",
        "from": "T85n2863_001:1324a13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "只得暫時勞。",
        "from": "T85n2863_001:1324a13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曠身入苦毒。",
        "from": "T85n2863_001:1324a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傍看□□□□□□□□造宅舍。",
        "from": "T85n2863_001:1324a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懝作萬年期人人歲乃有一。",
        "from": "T85n2863_001:1324a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縱令長□□□期却半欲似。",
        "from": "T85n2863_001:1324a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流星光暫時　中途少□□□□□於兒無問男夫及女婦不得驚忙審三界□□□黑月月增長害身肥日日造罪不知足。",
        "from": "T85n2863_001:1324a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大雲寺學仕郎鄧慶長",
        "from": "T85n2863_001:1324a20_100",
        "context_with_next_sentence": false
    }
]