[
    {
        "source_sentence": "佛說無量功德陀羅尼經",
        "from": "T19n0934_001:0080a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試光",
        "from": "T19n0934_001:0080a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祿卿明教大師臣法賢奉　詔譯",
        "from": "T19n0934_001:0080a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告阿難言。",
        "from": "T19n0934_001:0080a27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當諦聽我今為汝及末法眾生。",
        "from": "T19n0934_001:0080a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宣說無量功德陀羅尼。",
        "from": "T19n0934_001:0080a28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當憶念勿得忘失。",
        "from": "T19n0934_001:0080a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使於當來濁惡世中。",
        "from": "T19n0934_001:0080a29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與諸眾生作大善利。",
        "from": "T19n0934_001:0080b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難若有眾生得聞此陀羅尼。",
        "from": "T19n0934_001:0080b01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每日晨朝誦二十一遍。",
        "from": "T19n0934_001:0080b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人於千劫中所積惡業悉皆消滅。",
        "from": "T19n0934_001:0080b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見身獲得安隱快樂。",
        "from": "T19n0934_001:0080b03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人志心持誦一洛叉數。",
        "from": "T19n0934_001:0080b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人當來得見慈氏菩薩。",
        "from": "T19n0934_001:0080b04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若持誦二洛叉數。",
        "from": "T19n0934_001:0080b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當來得見觀自在菩薩。",
        "from": "T19n0934_001:0080b05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若持誦三洛叉數。",
        "from": "T19n0934_001:0080b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當來得見無量壽佛。",
        "from": "T19n0934_001:0080b06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說陀羅尼曰。",
        "from": "T19n0934_001:0080b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那謨引囉怛那二合怛囉二合夜引野一那莫阿引哩也二合阿彌多引婆引野二怛他引誐多引野三阿囉曷二合帝引三藐訖三二合沒馱引野四怛𭔞他引五阿彌帝引六阿彌覩引訥婆二合味引七阿彌多三婆味八阿彌多尾訖蘭二合引帝引九阿彌多尾訖蘭二合引多誐引彌儞十誐誐那計引哩底二合羯哩引十一薩哩嚩二合訖梨二合舍十二剎煬羯哩二合曳引娑嚩二合引賀引十三爾時阿難。",
        "from": "T19n0934_001:0080b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得聞如來說是無量功德陀羅尼已。",
        "from": "T19n0934_001:0080b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡喜信受禮佛而退。",
        "from": "T19n0934_001:0080b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說無量功德陀羅尼經",
        "from": "T19n0934_001:0080b19_100",
        "context_with_next_sentence": false
    }
]