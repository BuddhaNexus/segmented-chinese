[
    {
        "source_sentence": "佛說滅除五逆罪大陀羅尼經",
        "from": "T21n1399_001:0915c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試光祿卿",
        "from": "T21n1399_001:0915c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明教大師臣法賢奉　詔譯",
        "from": "T21n1399_001:0915c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告阿難言。",
        "from": "T21n1399_001:0915c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大陀羅尼具大威力功德無量。",
        "from": "T21n1399_001:0915c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能滅眾生五逆重罪。",
        "from": "T21n1399_001:0915c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人聞是陀羅尼。",
        "from": "T21n1399_001:0915c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發至誠心盡此身命。",
        "from": "T21n1399_001:0915c18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常能頂戴受持讀誦。",
        "from": "T21n1399_001:0915c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人所獲功德。",
        "from": "T21n1399_001:0915c19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如持千佛無異。",
        "from": "T21n1399_001:0915c19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說陀羅尼曰。",
        "from": "T21n1399_001:0915c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那謨引囉怛那二合怛囉二合夜引野一那莫阿引哩也二合引嚩路吉帝引說囉引野二󰂛提薩埵引野三摩賀引薩埵引野四摩賀引哥引嚕尼哥引野引五怛𭔞他引六唵引󰡊提引七尾󰡊提引八蘇尾󰡊提引九嗤尼去聲下同鉢哩嗤尼十補瑟閉二合蘇補瑟閉二合引十一惹路賀囉尼十二訶囉訶囉十三薩哩嚩二合阿嚩囉拏引儞十四鉢左鉢左十五薩哩嚩二合引播引野薩他二合引那引儞引十六鉢訥彌二合引鉢訥摩二合引叱十七鉢訥摩二合尾舍引隷引十八癹隷引癹癹癹十九訶訶訶訶訶二十阿倪也二合引隷引阿倪也二合引隷引二十一悉馱惹隷引娑嚩二合引賀引二十二爾時阿難。",
        "from": "T21n1399_001:0916a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得聞如來宣說滅除五逆罪業陀羅尼已。",
        "from": "T21n1399_001:0916a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡喜踊躍禮佛而退。",
        "from": "T21n1399_001:0916a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說滅除五逆罪大陀羅尼經",
        "from": "T21n1399_001:0916a06_100",
        "context_with_next_sentence": false
    }
]