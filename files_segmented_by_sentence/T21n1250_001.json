[
    {
        "source_sentence": "北方毘沙門多聞寶藏天王神妙",
        "from": "T21n1250_001:0230a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陀羅尼別行儀軌",
        "from": "T21n1250_001:0230a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "特進試鴻臚卿大興善寺三藏沙門",
        "from": "T21n1250_001:0230a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大廣智不空奉　詔譯爾時毘沙門天王告一切眾生言。",
        "from": "T21n1250_001:0230a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子汝於南閻浮提五濁惡世中。",
        "from": "T21n1250_001:0230a25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或出家比丘僧比丘尼。",
        "from": "T21n1250_001:0230a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在家優婆塞優婆夷等。",
        "from": "T21n1250_001:0230a26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同在法中。",
        "from": "T21n1250_001:0230a26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我領諸天善神擁護是人。",
        "from": "T21n1250_001:0230a27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若聞我名者有知我者。",
        "from": "T21n1250_001:0230a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當於是人左右。",
        "from": "T21n1250_001:0230a28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如護眼精。",
        "from": "T21n1250_001:0230a28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如護己命無令諸惡者惱亂。",
        "from": "T21n1250_001:0230a29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有一切男子女人。",
        "from": "T21n1250_001:0230a29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以王事我。",
        "from": "T21n1250_001:0230b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我以王事之。",
        "from": "T21n1250_001:0230b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以一切貴勝事我。",
        "from": "T21n1250_001:0230b01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我以一切貴勝事之。",
        "from": "T21n1250_001:0230b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若以富樂音聲事我我以一切富樂音聲事之。",
        "from": "T21n1250_001:0230b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人能受持誦陀羅尼及結印法。",
        "from": "T21n1250_001:0230b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有行用供養我形像者。",
        "from": "T21n1250_001:0230b04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我一切隨於汝所求。",
        "from": "T21n1250_001:0230b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應汝之願令汝滿足。",
        "from": "T21n1250_001:0230b05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有出家人受持者。",
        "from": "T21n1250_001:0230b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為童子助之。",
        "from": "T21n1250_001:0230b06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有國王君臣等受持供養我者。",
        "from": "T21n1250_001:0230b07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當富貴兵甲王位恒願應之誓無相背。",
        "from": "T21n1250_001:0230b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子是真言法。",
        "from": "T21n1250_001:0230b08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我於佛前奉上世尊。",
        "from": "T21n1250_001:0230b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護持末法比丘佛弟子及諸眾生等。",
        "from": "T21n1250_001:0230b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有於我世尊末法弟子起惡心相向者。",
        "from": "T21n1250_001:0230b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我自領多兵眾鬼神藥叉乾闥婆等。",
        "from": "T21n1250_001:0230b11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摧碎惡人猶如微塵。",
        "from": "T21n1250_001:0230b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子若有於我法中常供養依法受持。",
        "from": "T21n1250_001:0230b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨汝所須所願與汝。",
        "from": "T21n1250_001:0230b13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人能受持我真言法者。",
        "from": "T21n1250_001:0230b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當先作四肘壇。",
        "from": "T21n1250_001:0230b14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於淨室中掘去惡土。",
        "from": "T21n1250_001:0230b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深一尺。",
        "from": "T21n1250_001:0230b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠處取淨黃土。",
        "from": "T21n1250_001:0230b15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "填築令平正。",
        "from": "T21n1250_001:0230b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勒成階道。",
        "from": "T21n1250_001:0230b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高一磔手。",
        "from": "T21n1250_001:0230b16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上層方廣三尺。",
        "from": "T21n1250_001:0230b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高一磔手。",
        "from": "T21n1250_001:0230b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開四門。",
        "from": "T21n1250_001:0230b17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用水瓶盛香水。",
        "from": "T21n1250_001:0230b17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "箭四隻刀四口鏡四面。",
        "from": "T21n1250_001:0230b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養飲食用十二盤。",
        "from": "T21n1250_001:0230b18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煎餅乳酪酥燈四盞散華。",
        "from": "T21n1250_001:0230b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "春夏採雜華。",
        "from": "T21n1250_001:0230b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "秋冬散綵華。",
        "from": "T21n1250_001:0230b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其壇下層畫山石。",
        "from": "T21n1250_001:0230b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上層畫大蓮華。",
        "from": "T21n1250_001:0230b20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四角畫海水。",
        "from": "T21n1250_001:0230b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安像中心蓮華上。",
        "from": "T21n1250_001:0230b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上懸幡十六口。",
        "from": "T21n1250_001:0230b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆作神形像。",
        "from": "T21n1250_001:0230b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其像面南。",
        "from": "T21n1250_001:0230b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持法人面向北對像坐念誦。",
        "from": "T21n1250_001:0230b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作壇真言曰。",
        "from": "T21n1250_001:0230b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴唵􄖩難􆩮馱􄖩􆩮難馱􄕯娜􄓜智􄕯􄓜娜智􄖩難􆩮馱􄙖婆􈁖里入􅁥薩嚩二合引􄚩賀引若有受持真言法。",
        "from": "T21n1250_001:0230b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作壇時。",
        "from": "T21n1250_001:0230b29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先誦此真言一百八遍。",
        "from": "T21n1250_001:0230c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持地及土石處。",
        "from": "T21n1250_001:0230c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有一切蟲蟻諸類。",
        "from": "T21n1250_001:0230c01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆移避不在此住。",
        "from": "T21n1250_001:0230c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其地即得清淨。",
        "from": "T21n1250_001:0230c02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堪置壇場。",
        "from": "T21n1250_001:0230c02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結壇時。",
        "from": "T21n1250_001:0230c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚴加持結界。",
        "from": "T21n1250_001:0230c03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結地界契右手中指於左頭指期間向外背出頭。",
        "from": "T21n1250_001:0230c05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右無名指於左小指期間亦爾。",
        "from": "T21n1250_001:0230c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左中指內向入右頭指期間向內出頭。",
        "from": "T21n1250_001:0230c07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左無名指於右小指期間亦爾。",
        "from": "T21n1250_001:0230c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩小指頭指竪合。",
        "from": "T21n1250_001:0230c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指亦合頭向下。",
        "from": "T21n1250_001:0230c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指合頭著拄地翼兩臂肘。",
        "from": "T21n1250_001:0230c09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1250_001:0230c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴唵􄑂吉􄙌里􄑂􄙌吉里􄙖嚩􄦽日囉􄙖嚩􄦿日哩􄗝部􈃖律􄙖滿􄾫馱􄙖􄾫滿馱􆿁𤙖􄗁發􈁆吒此印能除地中一切惡類。",
        "from": "T21n1250_001:0230c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下至金剛際。",
        "from": "T21n1250_001:0230c15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除一切惡鬼神。",
        "from": "T21n1250_001:0230c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須加持七遍拄天地上下。",
        "from": "T21n1250_001:0230c16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能為害。",
        "from": "T21n1250_001:0230c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得清淨福。",
        "from": "T21n1250_001:0230c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結四方界契。",
        "from": "T21n1250_001:0230c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "準前地界契。",
        "from": "T21n1250_001:0230c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯改開二大指。",
        "from": "T21n1250_001:0230c19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相去二寸。",
        "from": "T21n1250_001:0230c19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "指頭向身直竪。",
        "from": "T21n1250_001:0230c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怒指向右轉。",
        "from": "T21n1250_001:0230c20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1250_001:0230c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴唵􄙺薩􄗽羅􄙺􄗽薩羅􄙖嚩􄦽日囉二合􄪼鉢羅二合􄑀迦􄗽羅􆿁𤙖􄗁泮􈁆吒此契能除四方魔鬼神等。",
        "from": "T21n1250_001:0230c25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作法會壇場處作印契。",
        "from": "T21n1250_001:0230c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言七遍。",
        "from": "T21n1250_001:0230c26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向四方隨日轉。",
        "from": "T21n1250_001:0230c26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切藥叉鬼退散。",
        "from": "T21n1250_001:0230c27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得法成就。",
        "from": "T21n1250_001:0230c27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結虛空界契。",
        "from": "T21n1250_001:0230c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "準前地界契。",
        "from": "T21n1250_001:0230c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯改二大指。",
        "from": "T21n1250_001:0230c29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各附著頭指側上。",
        "from": "T21n1250_001:0230c29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以指向於頭上右旋三匝。",
        "from": "T21n1250_001:0231a01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1250_001:0231a01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴唵􄙘尾􆷹薩普二合􄗽囉􄩶捺落二合􄚴乞灑二合􄙖嚩􄦽日囉二合􈇀半􄒶惹􄗽囉􆿁𤙖􄗁泮􈁆吒此印契能除空中上至於有頂諸魔鬼神。",
        "from": "T21n1250_001:0231a06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不敢惡念。",
        "from": "T21n1250_001:0231a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印契向頭上右旋三匝。",
        "from": "T21n1250_001:0231a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言加持。",
        "from": "T21n1250_001:0231a07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飛行藥叉退散。",
        "from": "T21n1250_001:0231a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能作大道場終身成事法者。",
        "from": "T21n1250_001:0231a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擇地未規時。",
        "from": "T21n1250_001:0231a09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言一遍。",
        "from": "T21n1250_001:0231a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結界地平正堅實。",
        "from": "T21n1250_001:0231a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香華供養。",
        "from": "T21n1250_001:0231a10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又誦真言一遍結界地。",
        "from": "T21n1250_001:0231a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若竪立拄額規郭。",
        "from": "T21n1250_001:0231a11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懸幢鈴珮等少分華復供養。",
        "from": "T21n1250_001:0231a12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一遍誦明結界。",
        "from": "T21n1250_001:0231a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "布置彩色座。",
        "from": "T21n1250_001:0231a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開諸位地。",
        "from": "T21n1250_001:0231a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又誦真言一遍結界。",
        "from": "T21n1250_001:0231a13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將燈安置壇上。",
        "from": "T21n1250_001:0231a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又誦真言一遍結界。",
        "from": "T21n1250_001:0231a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水罐華香飯食酥蜜胡麻󰡊粳米等供養物盡入道場。",
        "from": "T21n1250_001:0231a15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安置訖更誦真言一遍結界。",
        "from": "T21n1250_001:0231a16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請佛般若菩薩金剛天等。",
        "from": "T21n1250_001:0231a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未請前。",
        "from": "T21n1250_001:0231a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復作誦真言一遍結界。",
        "from": "T21n1250_001:0231a17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一結界左手把跋折羅。",
        "from": "T21n1250_001:0231a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降伏一切。",
        "from": "T21n1250_001:0231a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手把香爐。",
        "from": "T21n1250_001:0231a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口請一切聖眾天王。",
        "from": "T21n1250_001:0231a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右繞三匝作結界。",
        "from": "T21n1250_001:0231a19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想心標萬里千里百里一由旬等。",
        "from": "T21n1250_001:0231a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆心標想。",
        "from": "T21n1250_001:0231a20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口云各四近處三里結界。",
        "from": "T21n1250_001:0231a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即供養動止威儀具諸法相。",
        "from": "T21n1250_001:0231a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切賢聖天王歡喜。",
        "from": "T21n1250_001:0231a22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作本契印本真言法。",
        "from": "T21n1250_001:0231a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆迎請結界。",
        "from": "T21n1250_001:0231a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出入似國王動止威儀無異。",
        "from": "T21n1250_001:0231a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結香華契。",
        "from": "T21n1250_001:0231a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二無名指二小指交叉。",
        "from": "T21n1250_001:0231a26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右壓左在掌中。",
        "from": "T21n1250_001:0231a26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仍屈向腕。",
        "from": "T21n1250_001:0231a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即舒中指頭斜相跓。",
        "from": "T21n1250_001:0231a27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二頭指各捻中指第三節。",
        "from": "T21n1250_001:0231a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大指各附二食指側合腕。",
        "from": "T21n1250_001:0231a28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1250_001:0231a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴唵􄗥摩􄖪拏阿􅚼羅他􄖵鉢􄙀哩􄪼布􄗽囉􄑁迦引􄗱也􅁥薩嚩􄚩賀在處結壇及不結壇。",
        "from": "T21n1250_001:0231b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空對像處。",
        "from": "T21n1250_001:0231b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有壇處。",
        "from": "T21n1250_001:0231b05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作真言契印法。",
        "from": "T21n1250_001:0231b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持香華。",
        "from": "T21n1250_001:0231b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入道場供養。",
        "from": "T21n1250_001:0231b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無道場空對尊像空作契供養亦得。",
        "from": "T21n1250_001:0231b07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當有壇香華一也。",
        "from": "T21n1250_001:0231b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切天王善神金剛等歡喜。",
        "from": "T21n1250_001:0231b08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結飲食契。",
        "from": "T21n1250_001:0231b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "準前香華印。",
        "from": "T21n1250_001:0231b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯改二小指二無名指。",
        "from": "T21n1250_001:0231b10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拄在掌中間。",
        "from": "T21n1250_001:0231b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1250_001:0231b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄖩曩􄗭謨引􄒶惹岐􄕝帝引􄗙婆􄖵跛􄙺薩􄗽羅􄕝帝引􄕗跢􄓚吒􄗽羅􄓚吒􄖩娜􄗽攞􄗻延􄗱也􄒶惹婆􅁥薩嚩􄚩賀此契印及真言。",
        "from": "T21n1250_001:0231b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若擬獻飲食供養者。",
        "from": "T21n1250_001:0231b16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一真言加持食盤。",
        "from": "T21n1250_001:0231b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一真言加持了。",
        "from": "T21n1250_001:0231b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後入道場。",
        "from": "T21n1250_001:0231b17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壇處安置供養。",
        "from": "T21n1250_001:0231b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無飲食。",
        "from": "T21n1250_001:0231b18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但對天王像前。",
        "from": "T21n1250_001:0231b18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每朝空結契印及誦真言供養亦得福利。",
        "from": "T21n1250_001:0231b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天王歡喜。",
        "from": "T21n1250_001:0231b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結燈契。",
        "from": "T21n1250_001:0231b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手小指無名指屈在掌中。",
        "from": "T21n1250_001:0231b22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直竪中指。",
        "from": "T21n1250_001:0231b22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其頭指亦屈向掌中。",
        "from": "T21n1250_001:0231b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指屈上節。",
        "from": "T21n1250_001:0231b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "押頭指上節。",
        "from": "T21n1250_001:0231b23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指頭側捻中指中節上。",
        "from": "T21n1250_001:0231b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1250_001:0231b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􈁴唵􄗙婆􄖻閉􄕙底􄙐隷􅁥薩嚩􄚩賀若有道場。",
        "from": "T21n1250_001:0231b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須加持燈用契真言方乃入壇內供養。",
        "from": "T21n1250_001:0231b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如無壇。",
        "from": "T21n1250_001:0231b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空淨室置天王像。",
        "from": "T21n1250_001:0231b28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦須加持燈契誦真言供養。",
        "from": "T21n1250_001:0231b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神力光明照曜去處無障。",
        "from": "T21n1250_001:0231b29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天王歡喜。",
        "from": "T21n1250_001:0231c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四天王結界契二手小指屈入掌中。",
        "from": "T21n1250_001:0231c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右押左。",
        "from": "T21n1250_001:0231c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二中指無名指相離一寸。",
        "from": "T21n1250_001:0231c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指拄二大指。",
        "from": "T21n1250_001:0231c04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並屈入掌中。",
        "from": "T21n1250_001:0231c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1250_001:0231c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄚩賀此契再結界。",
        "from": "T21n1250_001:0231c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印上下各三遍。",
        "from": "T21n1250_001:0231c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸惡魔悉遠散。",
        "from": "T21n1250_001:0231c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然舉契四方東西南北周而復始作契誦真言加持。",
        "from": "T21n1250_001:0231c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魔障皆走不敢近於道場。",
        "from": "T21n1250_001:0231c12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "再三再四結界如法。",
        "from": "T21n1250_001:0231c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方乃啟請天王。",
        "from": "T21n1250_001:0231c13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得成就力。",
        "from": "T21n1250_001:0231c13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得無畏。",
        "from": "T21n1250_001:0231c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結召請契。",
        "from": "T21n1250_001:0231c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手前把左手腕。",
        "from": "T21n1250_001:0231c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於頂上左手四指來去。",
        "from": "T21n1250_001:0231c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1250_001:0231c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄖩曩􄗭謨引􄙝吠􈃡室羅二合􄗥末􄕋拏􄣢薩也􄗥麼􄕾地以􄩶瑟捺羅􄣢薩也􄙼悉􄕣他􄗙婆􄙖嚩􄕛覩􄖵婆􄕗哆􅁥薩嚩􄚩賀此契及誦真言七遍。",
        "from": "T21n1250_001:0231c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天王眷屬降臨壇會。",
        "from": "T21n1250_001:0231c24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口云。",
        "from": "T21n1250_001:0231c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天王慈悲就座受小供養。",
        "from": "T21n1250_001:0231c25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所屈請諸徒侶各著座位受我供養。",
        "from": "T21n1250_001:0231c26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即䠒跪手執香爐。",
        "from": "T21n1250_001:0231c26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對天王前。",
        "from": "T21n1250_001:0231c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至心奉請諸神眷屬夜叉羅剎諸佛菩薩金剛力士等。",
        "from": "T21n1250_001:0231c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降臨道場。",
        "from": "T21n1250_001:0231c28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為我證明。",
        "from": "T21n1250_001:0231c28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方毘沙門天王降臨道場。",
        "from": "T21n1250_001:0231c29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願得成就法力神通。",
        "from": "T21n1250_001:0231c29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得大自在。",
        "from": "T21n1250_001:0232a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所願隨意。",
        "from": "T21n1250_001:0232a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "感天動地契合君臣。",
        "from": "T21n1250_001:0232a01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兵甲相扶人民愛念。",
        "from": "T21n1250_001:0232a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所為事皆稱意。",
        "from": "T21n1250_001:0232a02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有神驗不虛。",
        "from": "T21n1250_001:0232a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天王現處變相。",
        "from": "T21n1250_001:0232a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令我知驗。",
        "from": "T21n1250_001:0232a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天王大身陀羅尼曰。",
        "from": "T21n1250_001:0232a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄖩曩􄗭謨引􄗽囉􅑈怛曩二合􄩞怛囉二合􄗲夜引􄗱也􄖩曩􄗭謨􈂽失戰二合􆧽荼􄙖嚩􄦽日囉二合􄖵播􄕌拏引􄗷曳􄗥摩􄚩賀引􄗱藥􄚴叉􄚢細引􄖩曩􆻤鉢跢二合引􄗷曳􄖩曩􄗭謨引􆿅阿􄕣他􄑈劫􄙃路引􄗞步引􄕗跢􄗾羅引􄗱也一􈋮地入􄙢捨􄗱也二􄙝吠􈃡室囉􄗥摩􄕋拏􄙖嚩􄑼左􄖪曩引􄣢薩也三􄗥摩􄚩賀引􄗽囉􄒷惹引􄣢薩也四􄗱夜􄚵乞叉引􄕽地波二合􆻤哆􄣢薩也五􄙖嚩􄗥摩􄕗多􄣢薩也六􈁨薩覩二合􄕗哆􄣢薩也七􄗙婆􄙢捨􄖩那八􄪼鉢囉􄙖嚩􄚩賀􄕯娜􄣢薩也九􄚮呬􄗥摩􄕯馱􄓚吒十􄓚吒􄕍尼􄪼鉢囉􄙮沙􄗲夜引􄗧弭十一􄕗怛􇩊儞也二合􄕤他十二􄑄俱􄙪曬􄗧弭􆥧俱􄙪曬􄗧弭􆥧俱􄙢沙十三􄙘尾􈃡捨囉􄗥摩􄕋娜􄣢薩也十四􄗥摩􄚩賀􄗽羅􄒶惹引十五􆿏翳􄙠畔􄕻馱􄑀迦十六􄖯寧􄩞哆羅􄗥摩􄚴蹉􄕛覩十七􅁥薩嚩二合引􄚩賀引十八若有受持者。",
        "from": "T21n1250_001:0232b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清淨念誦三十萬遍。",
        "from": "T21n1250_001:0232b02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天王加被中所作事業起心動念降伏。",
        "from": "T21n1250_001:0232b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至一切榮祿官職我隨汝心願。",
        "from": "T21n1250_001:0232b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人於月一日十五日起首。",
        "from": "T21n1250_001:0232b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦三十萬遍。",
        "from": "T21n1250_001:0232b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作壇者有音樂。",
        "from": "T21n1250_001:0232b05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但有少微塵心依隨。",
        "from": "T21n1250_001:0232b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我常擁護。",
        "from": "T21n1250_001:0232b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如護眼精。",
        "from": "T21n1250_001:0232b06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如護身命。",
        "from": "T21n1250_001:0232b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若口含香念誦心自他知。",
        "from": "T21n1250_001:0232b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若常持誦我真言不絕。",
        "from": "T21n1250_001:0232b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我隨是人坐臥。",
        "from": "T21n1250_001:0232b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常隨是人出入往來。",
        "from": "T21n1250_001:0232b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我隨是人意願滿足。",
        "from": "T21n1250_001:0232b09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若每日誦真言一百八遍。",
        "from": "T21n1250_001:0232b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅去身中一百八劫罪。",
        "from": "T21n1250_001:0232b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦持我真言滿一萬遍。",
        "from": "T21n1250_001:0232b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能滅八萬四千十惡五逆重罪。",
        "from": "T21n1250_001:0232b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破塔破戒不孝之罪悉皆消滅。",
        "from": "T21n1250_001:0232b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若對天王面。",
        "from": "T21n1250_001:0232b12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦持十萬遍身真言。",
        "from": "T21n1250_001:0232b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨事亦須小小稱意。",
        "from": "T21n1250_001:0232b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若日日供養我形像者。",
        "from": "T21n1250_001:0232b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃與我作意音樂乳粥供養者。",
        "from": "T21n1250_001:0232b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當金甲之身示現令願滿足。",
        "from": "T21n1250_001:0232b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若和合諸香。",
        "from": "T21n1250_001:0232b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取牛黃雄黃各等分。",
        "from": "T21n1250_001:0232b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持安我像足下。",
        "from": "T21n1250_001:0232b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我與香法成就隨汝所願塗自額。",
        "from": "T21n1250_001:0232b17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所向處見人國王王子大臣官長諸貴人等。",
        "from": "T21n1250_001:0232b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并一切所見者歡喜。",
        "from": "T21n1250_001:0232b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兵士愛念。",
        "from": "T21n1250_001:0232b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能每日日中時。",
        "from": "T21n1250_001:0232b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面向北對像前。",
        "from": "T21n1250_001:0232b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦我心真言滿三十萬遍。",
        "from": "T21n1250_001:0232b20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我示現大將身。",
        "from": "T21n1250_001:0232b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手持戟劍現前。",
        "from": "T21n1250_001:0232b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我於夢中現身。",
        "from": "T21n1250_001:0232b21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於遠山頂示汝令見。",
        "from": "T21n1250_001:0232b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿於諸願。",
        "from": "T21n1250_001:0232b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如不見吾身。",
        "from": "T21n1250_001:0232b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦隨汝願。",
        "from": "T21n1250_001:0232b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應念加被。",
        "from": "T21n1250_001:0232b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方天王心真言曰。",
        "from": "T21n1250_001:0232b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄖩曩􄗭謨引􄗽囉􅑈怛曩􄩞怛羅􄗲夜引􄗱也􄖩曩􄗭謨引􈂽失戰二合􆧽荼􄙖嚩􄦽日囉二合􄖵播􄕌拏引􄗷曳引􄗥摩􄚩賀􄗱藥􄚴乞叉二合􄚢細􄖩曩􄖵鉢􄕘多􄗷曳􄖩曩􄗭謨引􆿅阿􄕣他􄑈骨􄙂嚕􄗱藥􄚶室􄕋拏􄣢薩也􄗥摩􄚩賀引􄗾囉引􄒶惹􄗥摩􄗽羅􄗥𤚥􆧽拏􄙊藍􄪼鉢羅􄙘尾􄙣沙􄗧弭􄙺薩􅞣嚩􄙺薩􄽙怛嚩􄚪呬􄕙底􆵋瑟吒二合􈁀南􈁴唵􄗥摩􄕍尼􄗙跋􈆦捺羅二合􄗱也􅁥娑嚩􄚩賀􄖹布􄙂嚕􄕋拏􄗙跋􈆦捺羅二合􄗱也􅁥娑嚩􄚩賀􄗥摩􄖪拏􄗽㗚􄕤詑􄗱也􅁥娑嚩􄚩賀􄙼悉􈒻定􄑀迦􄗾羅􄗱也􅁥娑嚩􄚩賀􄙝吠􈃡室羅􄗥末􄕋拏􄗱也􅁥娑嚩􄚩賀􄕻難上􄖩曩􄕰馱􄗱也􅁥娑嚩􄚩賀若有受持念誦我心真言作諸供養者。",
        "from": "T21n1250_001:0232c16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯我身心意在汝身上。",
        "from": "T21n1250_001:0232c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛念汝無量。",
        "from": "T21n1250_001:0232c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人求財求官。",
        "from": "T21n1250_001:0232c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求一切外諸心事。",
        "from": "T21n1250_001:0232c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自令汝願滿足。",
        "from": "T21n1250_001:0232c18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不令惡人害汝。",
        "from": "T21n1250_001:0232c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令汝等受持我法者。",
        "from": "T21n1250_001:0232c19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得富貴安樂。",
        "from": "T21n1250_001:0232c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端正男女奴婢車乘五穀豆麥田莊妻妾。",
        "from": "T21n1250_001:0232c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必決定與汝等。",
        "from": "T21n1250_001:0232c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就一切樂具。",
        "from": "T21n1250_001:0232c21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨汝意願滿足。",
        "from": "T21n1250_001:0232c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至命終身往所生處。",
        "from": "T21n1250_001:0232c22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨汝意願所生不相違背。",
        "from": "T21n1250_001:0232c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不實者。",
        "from": "T21n1250_001:0232c23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我不名為護法天王。",
        "from": "T21n1250_001:0232c23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我必為汝墮阿鼻地獄。",
        "from": "T21n1250_001:0232c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等受持讀誦者。",
        "from": "T21n1250_001:0232c24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於我法中生少疑心誹謗輕罵者。",
        "from": "T21n1250_001:0232c25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是皆汝等地獄苦業因緣未盡。",
        "from": "T21n1250_001:0232c26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即知此人無有出期。",
        "from": "T21n1250_001:0232c26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等人民憶持者。",
        "from": "T21n1250_001:0232c27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香華油酥燈乃作水壇安置像於中。",
        "from": "T21n1250_001:0232c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種草菓子飲食煎餅乳酪等。",
        "from": "T21n1250_001:0232c28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "粥酥蜜盤如法供養。",
        "from": "T21n1250_001:0232c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作音樂歌唄。",
        "from": "T21n1250_001:0232c29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵音讚歎。",
        "from": "T21n1250_001:0232c29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是日日每月一日十五日正月七日。",
        "from": "T21n1250_001:0233a01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對像前念誦行道。",
        "from": "T21n1250_001:0233a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我示現身。",
        "from": "T21n1250_001:0233a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於壇中施諸寶物與汝滿足。",
        "from": "T21n1250_001:0233a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或隨汝心行動。",
        "from": "T21n1250_001:0233a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所向去處東西南北四維上下。",
        "from": "T21n1250_001:0233a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切去處乃至夜夢中。",
        "from": "T21n1250_001:0233a04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆施與汝等。",
        "from": "T21n1250_001:0233a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令人無畏入陣刀戟相害無能知者。",
        "from": "T21n1250_001:0233a05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但汝心存念我名及真言句。",
        "from": "T21n1250_001:0233a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手執契印。",
        "from": "T21n1250_001:0233a06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所擬方面彼所有惡敵兵賊。",
        "from": "T21n1250_001:0233a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉自被縛。",
        "from": "T21n1250_001:0233a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自散自顛自滅自狂。",
        "from": "T21n1250_001:0233a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無能害我法者。",
        "from": "T21n1250_001:0233a08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有持我心真言。",
        "from": "T21n1250_001:0233a08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和合香用含口見貴人。",
        "from": "T21n1250_001:0233a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取麝香欝金香龍腦香丁香等和合。",
        "from": "T21n1250_001:0233a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨多少和合。",
        "from": "T21n1250_001:0233a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用金銀合子盛。",
        "from": "T21n1250_001:0233a10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如無金銀合子。",
        "from": "T21n1250_001:0233a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用白琉璃合子。",
        "from": "T21n1250_001:0233a11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如無琉璃合子。",
        "from": "T21n1250_001:0233a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用瓷合子盛和合香也。",
        "from": "T21n1250_001:0233a12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方毘沙門多聞寶藏天王神妙陀羅尼別行儀軌延寶二年。",
        "from": "T21n1250_001:0233a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使他謄之。",
        "from": "T21n1250_001:0233a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今元祿六年六月二十五日。",
