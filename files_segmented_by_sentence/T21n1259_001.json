[
    {
        "source_sentence": "摩利支天一印法",
        "from": "T21n1259_001:0285c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有二印。一者身印。二者隱形印也。先作身印",
        "from": "T21n1259_001:0285c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當心誦真言七遍。加持身五處。謂五處者。一",
        "from": "T21n1259_001:0285c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心二額三左肩四右肩五頂是也。",
        "from": "T21n1259_001:0285c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身印相虛心合拳。",
        "from": "T21n1259_001:0285c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二中指指端屈纏著二頭指端。",
        "from": "T21n1259_001:0285c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是大金剛輪印也真言曰。",
        "from": "T21n1259_001:0285c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵引􄗦摩􄙌利􄙰支􅁥娑嚩􄚩訶次作隱形印當心念誦一百八遍。",
        "from": "T21n1259_001:0285c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依此印真言加持力故。",
        "from": "T21n1259_001:0285c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切天魔惡鬼不善邪魔外道不得其便。",
        "from": "T21n1259_001:0285c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦無見其行者形體。",
        "from": "T21n1259_001:0285c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖求覓空盡邪力。",
        "from": "T21n1259_001:0285c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得持明行者形體。",
        "from": "T21n1259_001:0285c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故每七種所行時。",
        "from": "T21n1259_001:0285c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印真言可加持。",
        "from": "T21n1259_001:0285c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七種所行時者。",
        "from": "T21n1259_001:0285c16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一睡眠時。",
        "from": "T21n1259_001:0285c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二覺悟時。",
        "from": "T21n1259_001:0285c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三沐浴時。",
        "from": "T21n1259_001:0285c17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四遠行時。",
        "from": "T21n1259_001:0285c17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五逢客時。",
        "from": "T21n1259_001:0285c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六飲食時。",
        "from": "T21n1259_001:0285c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七行廁時。",
        "from": "T21n1259_001:0285c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以身印加持五處。",
        "from": "T21n1259_001:0285c18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以身印想隱入金剛堅固智拳城中。",
        "from": "T21n1259_001:0285c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其隱形印。",
        "from": "T21n1259_001:0285c19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手作虛掌。",
        "from": "T21n1259_001:0285c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拳之舒右掌五輪。",
        "from": "T21n1259_001:0285c20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覆其左虛掌拳上。",
        "from": "T21n1259_001:0285c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不令間隙。",
        "from": "T21n1259_001:0285c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其心身。",
        "from": "T21n1259_001:0285c21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1259_001:0285c21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵引􆿅阿􄕱爾􄟘底也引􄗦摩􄙌利􄙰支􅁥娑嚩􄚩賀摩利支天一印法",
        "from": "T21n1259_001:0285c24_100",
        "context_with_next_sentence": false
    }
]