[
    {
        "source_sentence": "慈仁問八十種好經",
        "from": "T85n2867_001:1327a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈仁問佛。何因緣揚八十種好。佛言。一者世",
        "from": "T85n2867_001:1327a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世等意向人故。得足下平蹈地文現。二者人",
        "from": "T85n2867_001:1327a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "之所憙輒與之令安穩故。",
        "from": "T85n2867_001:1327a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得足槦好□□□表識。",
        "from": "T85n2867_001:1327a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者和合人見人輒歡喜故。",
        "from": "T85n2867_001:1327a28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "足下綺□人受經莫不開解。",
        "from": "T85n2867_001:1327a29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者丘墻不平生荊棘鋤治故。",
        "from": "T85n2867_001:1327b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "足下潤澤光耀行常安穩。",
        "from": "T85n2867_001:1327b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者未嘗恚恨□人亦不鬪亂人故。",
        "from": "T85n2867_001:1327b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "足肌細軟好安□□步無極。",
        "from": "T85n2867_001:1327b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六者見深坑泥水之處輒為施橋梁故。",
        "from": "T85n2867_001:1327b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所蹈平安□好。",
        "from": "T85n2867_001:1327b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七者持數千萬種物羅列道中用布施。",
        "from": "T85n2867_001:1327b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "足下輪輻成行說經□□□時。",
        "from": "T85n2867_001:1327b05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八者在所輒益天下人壽命故。",
        "from": "T85n2867_001:1327b06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輪輻相當自致大尊。",
        "from": "T85n2867_001:1327b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九者在所布施深經自教亦教人故。",
        "from": "T85n2867_001:1327b07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "足輪盡深。",
        "from": "T85n2867_001:1327b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十者在所柔仁見人如□□故。",
        "from": "T85n2867_001:1327b08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "足下文不受塵柔軟。",
        "from": "T85n2867_001:1327b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十一者不數人惡亦不令人恚恨故。",
        "from": "T85n2867_001:1327b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "足下盡直以善意向十方。",
        "from": "T85n2867_001:1327b10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二者在所次第教人不懈怠故。",
        "from": "T85n2867_001:1327b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "足指節盡正直。",
        "from": "T85n2867_001:1327b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十三者稱誦佛功德故。",
        "from": "T85n2867_001:1327b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "足指□洞好。",
        "from": "T85n2867_001:1327b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十四者於官道邊種樹施井浴池故名聞。",
        "from": "T85n2867_001:1327b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十五者解人疑難無所愛惜故。",
        "from": "T85n2867_001:1327b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爪赤銅色。",
        "from": "T85n2867_001:1327b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十六者作事常竟之人有愛重不別離之故。",
        "from": "T85n2867_001:1327b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "足指□□□生種姓家得佛無有能度者。",
        "from": "T85n2867_001:1327b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十七者持戒不動故。",
        "from": "T85n2867_001:1327b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "足指間慢人所作悉知。",
        "from": "T85n2867_001:1327b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十八者持戒不願生天上故。",
        "from": "T85n2867_001:1327b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "足指齊。",
        "from": "T85n2867_001:1327b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十九者在種姓□□□易人作惡故。",
        "from": "T85n2867_001:1327b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "裸平落知人意。",
        "from": "T85n2867_001:1327b19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十者不貪所問即為解之故。",
        "from": "T85n2867_001:1327b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得無上智。",
        "from": "T85n2867_001:1327b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十一者自守精進故。",
        "from": "T85n2867_001:1327b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "裸澤潤光好人有惡意悉知之。",
        "from": "T85n2867_001:1327b21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十二者□□□至誠故。",
        "from": "T85n2867_001:1327b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "裸□不桎知人心中所念。",
        "from": "T85n2867_001:1327b22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十三者□經悉欲令人在前知佛說經皆樂問之。",
        "from": "T85n2867_001:1327b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得身力無能折碎者。",
        "from": "T85n2867_001:1327b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十四者布施人經故。",
        "from": "T85n2867_001:1327b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身□具說經諦無謬誤。",
        "from": "T85n2867_001:1327b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十五者人說經終不可止授人經皆具足故。",
        "from": "T85n2867_001:1327b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身體𪪠博成滿好說經𧑮動之類皆聞知之。",
        "from": "T85n2867_001:1327b27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十六等有疑惑者以佛三藏為解之故。",
        "from": "T85n2867_001:1327b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勝正貴好。",
        "from": "T85n2867_001:1327b28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十七者不毀傷人身意亦不形咲。",
        "from": "T85n2867_001:1327b29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故得八十種好人見之莫不歡喜。",
        "from": "T85n2867_001:1327c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十八者見女人有病以智慧意治。",
        "from": "T85n2867_001:1327c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故身端正尊無比。",
        "from": "T85n2867_001:1327c02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十九者浴洗人身治去三□故。",
        "from": "T85n2867_001:1327c03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身淨潔光照十方。",
        "from": "T85n2867_001:1327c03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十者見□貢高不敬者輒敬練止之故。",
        "from": "T85n2867_001:1327c04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身軟澤。",
        "from": "T85n2867_001:1327c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十一者不為媒灼之事亦不謀議人故。",
        "from": "T85n2867_001:1327c05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面無□理得無不畏。",
        "from": "T85n2867_001:1327c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十二者見疾病者與藥令健故不愁憂。",
        "from": "T85n2867_001:1327c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十三者見人不端政者不形咲但成人之美故。",
        "from": "T85n2867_001:1327c08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成佛身潤澤淨潔。",
        "from": "T85n2867_001:1327c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十四者勸樂作福為善故。",
        "from": "T85n2867_001:1327c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身□□在惡處度脫之。",
        "from": "T85n2867_001:1327c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十五者見疲極飢渴者勞問案摩之故。",
        "from": "T85n2867_001:1327c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "背平好亦愈人愛欲病。",
        "from": "T85n2867_001:1327c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十六者見人作善歡喜高大之故。",
        "from": "T85n2867_001:1327c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩䝱骨上下平悉知人意。",
        "from": "T85n2867_001:1327c12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十七者隨人所憙與戒令忍辱制意故。",
        "from": "T85n2867_001:1327c13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩䝱洞好為人□經隨次第。",
        "from": "T85n2867_001:1327c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十八者見女人產乳難以慈心助之令早順生故。",
        "from": "T85n2867_001:1327c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "齊深好度脫人愛恩之根。",
        "from": "T85n2867_001:1327c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十九者不恚瞋所作不謬誤不恭敬者教之故。",
        "from": "T85n2867_001:1327c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "齊員隆起。",
        "from": "T85n2867_001:1327c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四十者以師所戒自教亦教人故。",
        "from": "T85n2867_001:1327c18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "齊理隨順施。",
        "from": "T85n2867_001:1327c18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四十一者自不婬妷亦心他人故。",
        "from": "T85n2867_001:1327c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從匃奴下乘條除腹中無惡露。",
        "from": "T85n2867_001:1327c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四十二者人意喜樂時不敗亂安穩之不豫凡人事故。",
        "from": "T85n2867_001:1327c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文相成身如天剛鐵無能壞者。",
        "from": "T85n2867_001:1327c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四十三者慈心念天下人意等故。",
        "from": "T85n2867_001:1327c22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩䝱隆滿。",
        "from": "T85n2867_001:1327c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四十四者所念弘廣教人為善隆滿故。",
        "from": "T85n2867_001:1327c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩肩廣大所知彌深。",
        "from": "T85n2867_001:1327c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四十五者智慧勇健持心不動故。",
        "from": "T85n2867_001:1327c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩臂洞長指頭有萬字。",
        "from": "T85n2867_001:1327c25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四十六者不說欺慢婬婬之事故。",
        "from": "T85n2867_001:1327c26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唇口如真丹無臭處惡露。",
        "from": "T85n2867_001:1327c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四十七者不止人識心無瞋恚諭諂之態故。",
        "from": "T85n2867_001:1327c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口不大不小適得其中如含㗛。",
        "from": "T85n2867_001:1327c28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四十八者以五香之味與人五□藥故舌無垢。",
        "from": "T85n2867_001:1327c29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四十九者不形咲人不罵人故。",
        "from": "T85n2867_001:1328a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人□其語莫不悅喜開解者。",
        "from": "T85n2867_001:1328a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五十者持戒行觀不說女人故。",
        "from": "T85n2867_001:1328a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牙員好堅緻以點力破壞愛欲。",
        "from": "T85n2867_001:1328a03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五十一者分別□要解人疑難故。",
        "from": "T85n2867_001:1328a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牙堅利愛欲破散。",
        "from": "T85n2867_001:1328a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五十二者人問一事為解若干事故。",
        "from": "T85n2867_001:1328a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牙濟方利隨次外道。",
        "from": "T85n2867_001:1328a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五十三者雜香澤和與人□著鼻中故。",
        "from": "T85n2867_001:1328a06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□殊好未嘗聞臭但聞香。",
        "from": "T85n2867_001:1328a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五十四者見聚糞掃除棄之故。",
        "from": "T85n2867_001:1328a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光明淨潔以好眼視人。",
        "from": "T85n2867_001:1328a08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五十五者世世□佛像故。",
        "from": "T85n2867_001:1328a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目長好經莫不開解者。",
        "from": "T85n2867_001:1328a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五十六者世世以淨心瞻視人故。",
        "from": "T85n2867_001:1328a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目如含㗛所視莫不見。",
        "from": "T85n2867_001:1328a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五十七者世世見人富貴心不貪樂故。",
        "from": "T85n2867_001:1328a11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目不大不小察然得其中未嘗𥈇。",
        "from": "T85n2867_001:1328a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五十八者世世以善意安穩心故。",
        "from": "T85n2867_001:1328a13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目無痑埋視天上天下。",
        "from": "T85n2867_001:1328a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五十九者世世不能識故。",
        "from": "T85n2867_001:1328a14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明目不形咲人亦不睇□人婦女故。",
        "from": "T85n2867_001:1328a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眉目明好在所為人中師。",
        "from": "T85n2867_001:1328a15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六十者世世自念後世亦念人善故。",
        "from": "T85n2867_001:1328a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眉好如半月悉見十方之事。",
        "from": "T85n2867_001:1328a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六十一者世世不瞋恚變色故眉光澤。",
        "from": "T85n2867_001:1328a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六十二者世世見怨家親屬心正等故眉柔潤。",
        "from": "T85n2867_001:1328a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六十三者世世憙事明師欲解後妙深經故。",
        "from": "T85n2867_001:1328a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眉紺青色。",
        "from": "T85n2867_001:1328a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六十四者世世見人說經不求短以語人故。",
        "from": "T85n2867_001:1328a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耳洞聽聞天上。",
        "from": "T85n2867_001:1328a21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六十五者世世不家人耳不據人耳及手足故。",
        "from": "T85n2867_001:1328a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耳不大小亦得其中潤澤好不聽聲。",
        "from": "T85n2867_001:1328a23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六十六者世世見行道自守者不敢驚恐之故。",
        "from": "T85n2867_001:1328a24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識事不忘。",
        "from": "T85n2867_001:1328a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六十七者世世所聞經法不增減其義故。",
        "from": "T85n2867_001:1328a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "額長六寸威神生。",
        "from": "T85n2867_001:1328a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六十八者世世教授以次第故。",
        "from": "T85n2867_001:1328a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髮隨次澤好不受塵垢。",
        "from": "T85n2867_001:1328a27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六十九者世世與人語常和顏悅色不罵詈惡口故。",
        "from": "T85n2867_001:1328a28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髮細好無比。",
        "from": "T85n2867_001:1328a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七十者世世恭敬師及明人持戒者以醍醐麻󰪎及所著衣物施與人故。",
        "from": "T85n2867_001:1328b01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髮潤澤姝好。",
        "from": "T85n2867_001:1328b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七十一者世世以諸功德教授人故。",
        "from": "T85n2867_001:1328b02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髮均遍無間缺人見其髮莫不喜者。",
        "from": "T85n2867_001:1328b03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七十二者世世見人是非不傳說其善惡故。",
        "from": "T85n2867_001:1328b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□污塵不著入亦不見其惡。",
        "from": "T85n2867_001:1328b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七十三者世世不滅佛前及人燈火故。",
        "from": "T85n2867_001:1328b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塵垢不著其髮生光耀。",
        "from": "T85n2867_001:1328b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七十四者世世以好意與人冠㥽貂蟬覆髮巾席故。",
        "from": "T85n2867_001:1328b07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髮文聚生威神□見無厭極。",
        "from": "T85n2867_001:1328b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七十五者布髮令父母師蹈之故。",
        "from": "T85n2867_001:1328b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髮文相見知十方人所念。",
        "from": "T85n2867_001:1328b09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七十六者世世欲得珍寶物者輒善與□故。",
        "from": "T85n2867_001:1328b10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髮遍等如盡所願悉得。",
        "from": "T85n2867_001:1328b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七十七者世世欲使人以戒不從索物常願與善師明經人共從事故。",
        "from": "T85n2867_001:1328b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髮文相生其間如瓶威業聲聞十方。",
        "from": "T85n2867_001:1328b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七十八者世世教同學明經者故。",
        "from": "T85n2867_001:1328b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髮齊等意定。",
        "from": "T85n2867_001:1328b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七十九者世世所學作輒而竟之故。",
        "from": "T85n2867_001:1328b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功德成就。",
        "from": "T85n2867_001:1328b15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八十者世世常饒益人使安穩故。",
        "from": "T85n2867_001:1328b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈心所說曠盪潤懿無有窮極。",
        "from": "T85n2867_001:1328b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世世菩薩所行如是。",
        "from": "T85n2867_001:1328b17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈仁問八十種好經",
        "from": "T85n2867_001:1328b18_100",
        "context_with_next_sentence": false
    }
]