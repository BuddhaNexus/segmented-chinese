[
    {
        "source_sentence": "宗󰤗僧正於唐國師所口受",
        "from": "T20n1156B_001:0650c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "1 大隨求根本印大隨求根本印第一",
        "from": "T20n1156B_001:0650c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉。二中指合竪。二頭指於中指後",
        "from": "T20n1156B_001:0650c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微屈如鉤。",
        "from": "T20n1156B_001:0650c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二小指二大指合竪微屈即成。",
        "from": "T20n1156B_001:0650c25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵云嚩日羅二合　唐云五股金剛杵2 一切如來心真言一切如來心真言第二二手左覆右仰背相著。",
        "from": "T20n1156B_001:0650c28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中指互相背鉤竪如斧形即成。",
        "from": "T20n1156B_001:0650c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵云跛羅戌　唐云鉞斧安云二手背立中指相鉤3 一切如來心印真言一切如來心印真言第三二手內相叉二中指頭相跓屈令圓即成。",
        "from": "T20n1156B_001:0651a03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵云跛羅播捨　唐云索4 一切如來金剛被甲真言印一切如來金剛被甲真言印第四二手合掌。",
        "from": "T20n1156B_001:0651a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指屈中節令平偃。",
        "from": "T20n1156B_001:0651a06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭相跓即成。",
        "from": "T20n1156B_001:0651a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵云渴誐　唐云劍5 一切如來灌頂真言印一切如來灌頂真言印第五歟二手外相叉。",
        "from": "T20n1156B_001:0651a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二無名指合竪。",
        "from": "T20n1156B_001:0651a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二小指竪交即成。",
        "from": "T20n1156B_001:0651a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵云斫羯羅引　唐云輪6 一切如來結界真言印一切如來結界真言印第六歟二大指捻二小指甲上。",
        "from": "T20n1156B_001:0651a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘指竪合如三戟叉形即成。",
        "from": "T20n1156B_001:0651a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵云底哩戌羅　唐云三股叉7 一切如來心中心真言印一切如來心中心真言印第七歟二手外相叉。",
        "from": "T20n1156B_001:0651a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指相跓蹴如寶形。",
        "from": "T20n1156B_001:0651a18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指並竪即成。",
        "from": "T20n1156B_001:0651a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵云進跢引麼尼　唐云寶8 一切如來隨心真言印一切如來隨心真言印第八歟以左手仰於當心展五指。",
        "from": "T20n1156B_001:0651a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手覆於左上相合令平即成。",
        "from": "T20n1156B_001:0651a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵云摩訶尾儞也二合馱羅抳異　唐云大明總持慈覺大師大隨求印慈覺大師大隨求印內縛五股印是也。",
        "from": "T20n1156B_001:0651a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊勝佛頂印尊勝佛頂印虛心合掌。",
        "from": "T20n1156B_001:0651a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二風甲相對。",
        "from": "T20n1156B_001:0651a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二空捻二風側如彈指勢也本軌文。",
        "from": "T20n1156B_001:0651b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊根本印文殊根本印虛心合掌。",
        "from": "T20n1156B_001:0651b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二火覆。",
        "from": "T20n1156B_001:0651b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捻二水而頭相拄屈二風橫捻二空上　又說內縛二地合竪屈二風橫二空上。",
        "from": "T20n1156B_001:0651b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿足句印滿足句印二地合針。",
        "from": "T20n1156B_001:0651b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二水內鉤。",
        "from": "T20n1156B_001:0651b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二火如寶。",
        "from": "T20n1156B_001:0651b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二風屈捻二空上。",
        "from": "T20n1156B_001:0651b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1156B_001:0651b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿尾羅吽欠　貞觀十九年三月二十二日傳",
        "from": "T20n1156B_001:0651b10_100",
        "context_with_next_sentence": false
    }
]