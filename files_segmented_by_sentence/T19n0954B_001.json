[
    {
        "source_sentence": "一字頂輪王念誦儀軌依忉利天宮所說經譯",
        "from": "T19n0954B_001:0310c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今說無比力超勝世間出世間真言上上。",
        "from": "T19n0954B_001:0310c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切佛頂王佛頂輪王念誦儀則。行者先當",
        "from": "T19n0954B_001:0310c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清淨。",
        "from": "T19n0954B_001:0311a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於淨處安本尊像。",
        "from": "T19n0954B_001:0311a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面西稽首禮。",
        "from": "T19n0954B_001:0311a01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受三歸捨身說罪受戒。",
        "from": "T19n0954B_001:0311a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發菩提心隨喜勸請發願廻向已。",
        "from": "T19n0954B_001:0311a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應結佛部三昧耶印。",
        "from": "T19n0954B_001:0311a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四頂互內交。",
        "from": "T19n0954B_001:0311a03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二輪並合竪。",
        "from": "T19n0954B_001:0311a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前附於蓋。",
        "from": "T19n0954B_001:0311a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名一切如來心印真言曰。",
        "from": "T19n0954B_001:0311a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一邇那匿而翼切二次結蓮華三昧耶印。",
        "from": "T19n0954B_001:0311a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前印。",
        "from": "T19n0954B_001:0311a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左輪屈入掌中。",
        "from": "T19n0954B_001:0311a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右輪如前竪。",
        "from": "T19n0954B_001:0311a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名蓮華部心印真言曰。",
        "from": "T19n0954B_001:0311a08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一阿嚕力二次結金剛部三昧耶印。",
        "from": "T19n0954B_001:0311a10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前蓮華部心印。",
        "from": "T19n0954B_001:0311a10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右輪屈入掌中。",
        "from": "T19n0954B_001:0311a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左輪依前竪。",
        "from": "T19n0954B_001:0311a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名金剛部心印真言曰。",
        "from": "T19n0954B_001:0311a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一嚩日囉二合地力二合二次結甲冑印准根本印。",
        "from": "T19n0954B_001:0311a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋拄二光。",
        "from": "T19n0954B_001:0311a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "背印額右肩左肩心喉五處。",
        "from": "T19n0954B_001:0311a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0954B_001:0311a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一斫羯囉二合靺㗚底二合二鉢囉二合賒弭多三囉捺囉二合引囉捺囉二合引四婆去聲娑摩二合車盧瑟尼二合沙五囉乞沙二合囉乞沙二合𤚥六吽癹娑嚩二合合訶引七次結佛眼印。",
        "from": "T19n0954B_001:0311a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手合掌。",
        "from": "T19n0954B_001:0311a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋各拄光背。",
        "from": "T19n0954B_001:0311a20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二輪屈入掌。",
        "from": "T19n0954B_001:0311a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印五處真言曰。",
        "from": "T19n0954B_001:0311a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南一唵引二嚕嚕薩怖二合嚕三入嚩二合攞底瑟姹二合四悉馱盧者寧五薩嚩喇他二合娑達尼娑嚩二合引訶引六次結大海印。",
        "from": "T19n0954B_001:0311a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉。",
        "from": "T19n0954B_001:0311a25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仰掌擘開二輪。",
        "from": "T19n0954B_001:0311a25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋三匝想大海真言曰。",
        "from": "T19n0954B_001:0311a26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一微摩盧娜地娑嚩二合引訶引二次於大海中想須彌盧山。",
        "from": "T19n0954B_001:0311a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四寶所成。",
        "from": "T19n0954B_001:0311a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉急握豎拳真言曰。",
        "from": "T19n0954B_001:0311a29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一阿者攞吽二次於須彌盧山上想寶樓閣。",
        "from": "T19n0954B_001:0311b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持寶樓閣印。",
        "from": "T19n0954B_001:0311b02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印二手金剛掌真言曰。",
        "from": "T19n0954B_001:0311b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南引一唵薩嚩他欠二鄔娜哦二合帝薩頗二合囉呬引𤚥三哦哦那檢娑嚩二合訶引四次結佛頂輪王印。",
        "from": "T19n0954B_001:0311b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二掌內相叉作拳。",
        "from": "T19n0954B_001:0311b07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二光屈上節如劍。",
        "from": "T19n0954B_001:0311b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並竪二輪屈二蓋兩節。",
        "from": "T19n0954B_001:0311b08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相拄於二輪上。",
        "from": "T19n0954B_001:0311b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印五處加護真言曰。",
        "from": "T19n0954B_001:0311b09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南一步嚕唵三合二次結網橛印。",
        "from": "T19n0954B_001:0311b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0311b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋上節。",
        "from": "T19n0954B_001:0311b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "背不相著。",
        "from": "T19n0954B_001:0311b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二輪各壓。",
        "from": "T19n0954B_001:0311b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上下揮轉。",
        "from": "T19n0954B_001:0311b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成結上下界真言曰。",
        "from": "T19n0954B_001:0311b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南一阿鉢囉二合底呵多舍娑娜南二唵引三微枳囉拏微特防去聲二合娑尼四迦比羅貳縛里尼五怛囉二合娑耶六嚩日囉二合吠賒薩帝呶引囉特縛二合七能上聲瑟吒囉三合囉乞沙二合𤚥癹八次結牆印。",
        "from": "T19n0954B_001:0311b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0311b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋兩節相逼。",
        "from": "T19n0954B_001:0311b19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平竪二輪附二蓋。",
        "from": "T19n0954B_001:0311b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋三匝即成。",
        "from": "T19n0954B_001:0311b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛牆界真言曰歸命同前。",
        "from": "T19n0954B_001:0311b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "噁引一莫壑二次結車輅印。",
        "from": "T19n0954B_001:0311b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉。",
        "from": "T19n0954B_001:0311b23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仰掌申二蓋。",
        "from": "T19n0954B_001:0311b23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令甲側相拄。",
        "from": "T19n0954B_001:0311b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二輪拄蓋根下。",
        "from": "T19n0954B_001:0311b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想於他方世界奉迎本尊。",
        "from": "T19n0954B_001:0311b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0954B_001:0311b25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一覩嚕覩嚕吽二次結迎輅印。",
        "from": "T19n0954B_001:0311b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准車輅印。",
        "from": "T19n0954B_001:0311b27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二輪撥光頭。",
        "from": "T19n0954B_001:0311b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向身三招。",
        "from": "T19n0954B_001:0311b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0954B_001:0311b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發遣除羯哩灑二合耶加尾薩哩惹耶娜謨薩底哩野三合地尾二合迦南一怛他孽哆南二唵引三嚩日啷二合儗愚以切儞也二合羯哩灑耶娑嚩二合引訶引四次結迎請印。",
        "from": "T19n0954B_001:0311c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0311c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈右蓋於光後。",
        "from": "T19n0954B_001:0311c04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向前三招。",
        "from": "T19n0954B_001:0311c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0954B_001:0311c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨婆誐嚩帝一阿鉢囉二合底呵覩瑟尼二合沙耶二翳呬翳二合娑誐刎三達麼囉惹平聲鉢囉二合底車鴟奚切南四遏鉗巘談補瑟貶逋諳切度貶同上末隣儞半者五滿遮避洛乞叉二合六阿鉢囉二合底呵多七麼囉鉢囉引羯囉二合麼耶娑嚩二合引訶引八次結一切辨事佛頂印。",
        "from": "T19n0954B_001:0311c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉。",
        "from": "T19n0954B_001:0311c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二光如幢。",
        "from": "T19n0954B_001:0311c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸供養物及浴水洗淨土等。",
        "from": "T19n0954B_001:0311c13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並用此真言。",
        "from": "T19n0954B_001:0311c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持辟除去垢結界皆用此真言。",
        "from": "T19n0954B_001:0311c14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左旋辟除右旋結界真言曰。",
        "from": "T19n0954B_001:0311c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三滿多勃馱南一唵引二吒嚕嚕二合三滿馱娑嚩二合引訶引四次重結前網橛印。",
        "from": "T19n0954B_001:0311c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結上方界。",
        "from": "T19n0954B_001:0311c18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結阿娑莽倪移枳切尼印。",
        "from": "T19n0954B_001:0311c19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "側二手。",
        "from": "T19n0954B_001:0311c19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左掩右。",
        "from": "T19n0954B_001:0311c19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅二輪右轉一匝。",
        "from": "T19n0954B_001:0311c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成密縫真言曰。",
        "from": "T19n0954B_001:0311c20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一阿娑莽倪尼吽癹二次結獻閼伽印。",
        "from": "T19n0954B_001:0311c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0311c22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋相拄。",
        "from": "T19n0954B_001:0311c22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "附二光。",
        "from": "T19n0954B_001:0311c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二輪各附蓋根側真言曰。",
        "from": "T19n0954B_001:0311c23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南一唵引二閼伽囉訶三閼伽必哩二合野四鉢囉二合底車鴟奚切娜末鉗娑嚩二合引訶引五次重結根本印。",
        "from": "T19n0954B_001:0311c27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結獻師子座印。",
        "from": "T19n0954B_001:0311c28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0311c28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋於二輪甲側。",
        "from": "T19n0954B_001:0311c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0954B_001:0311c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一阿者囉尾囉耶娑嚩二合引訶引二次結塗香印。",
        "from": "T19n0954B_001:0312a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0312a02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈右蓋。",
        "from": "T19n0954B_001:0312a02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倚於右光下節真言曰。",
        "from": "T19n0954B_001:0312a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南一唵引二怛嚟二合盧枳也二合獻馱孽帝三吽吽癹癹娑嚩二合引訶引四次結獻華印。",
        "from": "T19n0954B_001:0312a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前塗香印。",
        "from": "T19n0954B_001:0312a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "改左蓋。",
        "from": "T19n0954B_001:0312a06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倚右光下節真言曰。",
        "from": "T19n0954B_001:0312a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南一唵引二薩嚩盧迦補瑟波二合步哆耶三吽吽癹癹娑嚩二合引訶引四次結燒香印。",
        "from": "T19n0954B_001:0312a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0312a10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋。",
        "from": "T19n0954B_001:0312a10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各倚於光下節真言曰。",
        "from": "T19n0954B_001:0312a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南一唵引二尾囉孽哆微哦哆度麼耶三吽吽癹癹娑嚩二合引訶引四次結獻食印。",
        "from": "T19n0954B_001:0312a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0312a14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋上節。",
        "from": "T19n0954B_001:0312a14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各附於輪側真言曰。",
        "from": "T19n0954B_001:0312a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南一唵引二薩嚩盧迦麼里必哩二合夜耶三吽吽癹癹娑嚩二合引訶引四次結獻燈明印。",
        "from": "T19n0954B_001:0312a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0312a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋兩節令不相著二輪各附於蓋上真言曰。",
        "from": "T19n0954B_001:0312a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南一唵引二薩嚩盧迦珊捺囉二合捨娜耶三吽吽癹癹娑嚩二合引訶引四次結普供養加持印。",
        "from": "T19n0954B_001:0312a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手虛心合掌。",
        "from": "T19n0954B_001:0312a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五頂各兩節相交真言曰。",
        "from": "T19n0954B_001:0312a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨薩嚩勃馱冒地薩怛嚩二合南一唵引二薩嚩怛囉二合僧俱蘇弭多三避吉惹囉始那慕薩低娑嚩二合引訶引四次結遍照佛頂印。",
        "from": "T19n0954B_001:0312a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉合為拳。",
        "from": "T19n0954B_001:0312a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令二光節微起真言曰。",
        "from": "T19n0954B_001:0312a28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南一噁引二莫含呼頷切三次結白傘蓋佛頂印。",
        "from": "T19n0954B_001:0312b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二輪各捻二高甲上。",
        "from": "T19n0954B_001:0312b01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "側相合。",
        "from": "T19n0954B_001:0312b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二蓋屈如蓋形。",
        "from": "T19n0954B_001:0312b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二光微屈相合。",
        "from": "T19n0954B_001:0312b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二勝各竪相合真言曰。",
        "from": "T19n0954B_001:0312b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南一阿鉢囉二合底呵多舍娑娜南二唵引三麼麼麼吽匿儞翼切四次結光聚佛頂印。",
        "from": "T19n0954B_001:0312b06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前白傘蓋印。",
        "from": "T19n0954B_001:0312b06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坼開二蓋真言曰。",
        "from": "T19n0954B_001:0312b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南一阿鉢囉二合底呵多舍娑娜南二唵引三怛他蘖都瑟尼二合沙四阿娜嚩盧枳多慕㗚馱二合五帝儒囉始吽六入嚩二合攞入嚩二合攞七馱迦馱迦八娜囉微娜囉九瞋那牝那吽吽癹癹娑嚩二合引訶引十次結高佛頂印。",
        "from": "T19n0954B_001:0312b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前白傘蓋印。",
        "from": "T19n0954B_001:0312b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋拄二光中節背真言曰。",
        "from": "T19n0954B_001:0312b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南一阿鉢囉二合底呵多舍娑娜南二唵引三儞𭅤儞卑渝二合娜哦都瑟尼二合沙四吽吽癹癹娑嚩二合引訶引五次結勝佛頂印。",
        "from": "T19n0954B_001:0312b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准高佛頂印。",
        "from": "T19n0954B_001:0312b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "移二蓋向上。",
        "from": "T19n0954B_001:0312b18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩穬麥許真言曰。",
        "from": "T19n0954B_001:0312b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南一阿鉢囉二合底呵多舍娑娜南二唵引三入嚩二合羅惹踰瑟尼二合沙四吽癹娑嚩二合引訶引五次結摧毀佛頂尾枳囉拏印。",
        "from": "T19n0954B_001:0312b23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉作拳。",
        "from": "T19n0954B_001:0312b23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二光屈上節。",
        "from": "T19n0954B_001:0312b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右光拄左光面。",
        "from": "T19n0954B_001:0312b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令出半節許真言曰。",
        "from": "T19n0954B_001:0312b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南一阿鉢囉二合底呵多舍娑娜南二唵引三尾枳囉那四度那度那犢引五次結摧碎佛頂印。",
        "from": "T19n0954B_001:0312b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並准前。",
        "from": "T19n0954B_001:0312b28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "改左光拄右光面。",
        "from": "T19n0954B_001:0312b28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦出半節許。",
        "from": "T19n0954B_001:0312b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0954B_001:0312b29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南一阿鉢囉二合底呵多舍娑娜南二唵引二阿鉢囉二合底呵都瑟尼二合沙耶四薩嚩尾伽那二合尾持望二合娑那迦囉耶五怛嚧二合吒耶娑嚩二合引訶引六次結輪王佛頂心印。",
        "from": "T19n0954B_001:0312c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0312c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋拄二光上節節上真言曰。",
        "from": "T19n0954B_001:0312c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南一阿鉢囉二合底呵多舍娑娜南二唵引三怛他薩那瑟尼二合沙那嚩盧枳多四沒馱尼斫羯囉二合靺㗚底二合五吽入嚩二合羅入嚩二合羅六馱迦馱迦七度那微度那八怛囉二合娑耶麼囉逾瑳囉耶九呵那呵那十伴惹伴惹十一暗音𡳯鉢嚨二合十二企尼君吒哩尼十三阿鉢囉爾多薩怛囉馱哩尼十四吽癹娑嚩二合引訶引十五次結心中印。",
        "from": "T19n0954B_001:0312c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0312c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋加於二光上節上真言曰。",
        "from": "T19n0954B_001:0312c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南一阿鉢囉一合底呵多舍娑娜南二唵引三阿鉢囉引爾多特地翼切四普通諸佛頂印。",
        "from": "T19n0954B_001:0312c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手虛心金剛合掌。",
        "from": "T19n0954B_001:0312c19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如華在掌中。",
        "from": "T19n0954B_001:0312c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者若怱遽不能遍結諸佛頂印。",
        "from": "T19n0954B_001:0312c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但結此印誦諸佛頂真言。",
        "from": "T19n0954B_001:0312c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結頂印。",
        "from": "T19n0954B_001:0312c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0312c21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈右蓋竪。",
        "from": "T19n0954B_001:0312c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於右光後。",
        "from": "T19n0954B_001:0312c22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令不相著真言曰。",
        "from": "T19n0954B_001:0312c22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南一阿鉢囉二合底呵多舍娑娜南二唵引三斫羯囉二合靺㗚底二合唵吽四次結頭印。",
        "from": "T19n0954B_001:0312c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0312c25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開二蓋各直竪於光後。",
        "from": "T19n0954B_001:0312c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令不相著微屈真言曰歸命同前。",
        "from": "T19n0954B_001:0312c26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一斫羯囉二合靺㗚底二合二吽癹娑嚩二合引訶引三次又結根本印。",
        "from": "T19n0954B_001:0312c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結大三昧耶印。",
        "from": "T19n0954B_001:0313a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加護本尊。",
        "from": "T19n0954B_001:0313a01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉。",
        "from": "T19n0954B_001:0313a01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二光。",
        "from": "T19n0954B_001:0313a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋於光後如鉤。",
        "from": "T19n0954B_001:0313a02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相去一穬麥許。",
        "from": "T19n0954B_001:0313a02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二輪各附蓋根下。",
        "from": "T19n0954B_001:0313a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋三匝真言曰。",
        "from": "T19n0954B_001:0313a03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一商羯哩二摩訶三昧鹽娑嚩二合引訶引三次誦一百八名讚歎。",
        "from": "T19n0954B_001:0313a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲念誦。",
        "from": "T19n0954B_001:0313a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以五支成本尊。",
        "from": "T19n0954B_001:0313a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或五相成本尊瑜伽。",
        "from": "T19n0954B_001:0313a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於處處頂舌心也想一字頂輪。",
        "from": "T19n0954B_001:0313a07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成本尊坐八葉蓮華。",
        "from": "T19n0954B_001:0313a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一一葉上想七寶。",
        "from": "T19n0954B_001:0313a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前蓮華葉上想佛眼尊。",
        "from": "T19n0954B_001:0313a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應持珠此依菩提場所說經。",
        "from": "T19n0954B_001:0313a09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次持珠合掌。",
        "from": "T19n0954B_001:0313a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捧珠誦清珠真言七遍真言曰。",
        "from": "T19n0954B_001:0313a10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一阿娜步二合低尾惹曳而矣切二悉地悉馱囉梯娑嚩二合引訶引三次結持珠印。",
        "from": "T19n0954B_001:0313a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手各以輪捻高甲。",
        "from": "T19n0954B_001:0313a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直竪二光二勝。",
        "from": "T19n0954B_001:0313a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋於光後。",
        "from": "T19n0954B_001:0313a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令不相著如毘盧遮那經半金剛杵印真言曰。",
        "from": "T19n0954B_001:0313a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應淨其心　　如法而念誦",
        "from": "T19n0954B_001:0313a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持珠令當心　　繫心於鼻端",
        "from": "T19n0954B_001:0313a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "字句分明呼　　不緩亦不急",
        "from": "T19n0954B_001:0313a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不頻伸噦嗌　　咳嗽與唾涕",
        "from": "T19n0954B_001:0313a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "染等心相應　　及心緣苦受",
        "from": "T19n0954B_001:0313a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等過患　　皆不得成就",
        "from": "T19n0954B_001:0313a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當念誦時。",
        "from": "T19n0954B_001:0313a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身心不得疲怠。",
        "from": "T19n0954B_001:0313a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若勞倦即應結五供養印。",
        "from": "T19n0954B_001:0313a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦讚歎獻閼伽。",
        "from": "T19n0954B_001:0313a25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦畢持珠頂上。",
        "from": "T19n0954B_001:0313a25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結前密縫印。",
        "from": "T19n0954B_001:0313a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左轉一匝即成解界。",
        "from": "T19n0954B_001:0313a26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結奉送印。",
        "from": "T19n0954B_001:0313a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0313a27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左蓋外擲。",
        "from": "T19n0954B_001:0313a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦迎請真言。",
        "from": "T19n0954B_001:0313a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除翳呬翳呬。",
        "from": "T19n0954B_001:0313a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加孽車孽車句。",
        "from": "T19n0954B_001:0313a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成奉送。",
        "from": "T19n0954B_001:0313a28_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應復結牆及網橛等印。",
        "from": "T19n0954B_001:0313a29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加護處上下及所成就物。",
        "from": "T19n0954B_001:0313b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結計里枳里印。",
        "from": "T19n0954B_001:0313b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左輪壓勝甲上。",
        "from": "T19n0954B_001:0313b01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘三指頂坼開直竪。",
        "from": "T19n0954B_001:0313b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如三股杵形。",
        "from": "T19n0954B_001:0313b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋轉三匝成結界真言曰。",
        "from": "T19n0954B_001:0313b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一枳里枳里嚩日囉二合吽癹二次結軍吒利印。",
        "from": "T19n0954B_001:0313b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二勝於掌中交。",
        "from": "T19n0954B_001:0313b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二高壓。",
        "from": "T19n0954B_001:0313b05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二輪壓高上。",
        "from": "T19n0954B_001:0313b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二光相合。",
        "from": "T19n0954B_001:0313b06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋於光後。",
        "from": "T19n0954B_001:0313b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令一麥許不相著。",
        "from": "T19n0954B_001:0313b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋三匝即成結界真言曰。",
        "from": "T19n0954B_001:0313b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨囉怛那二合怛囉二合夜耶一娜謨室戰二合拏嚩日囉二合波拏曳二摩訶藥乞叉二合細那波多曳三娜謨室戰二合拏嚩日囉二合句𡀔馱耶四唵引五虎嚕虎嚕六底瑟姹二合底瑟姹二合七滿馱滿馱八呵那呵那九阿蜜哩二合帝吽癹娑嚩二合引訶引十從一字真言。",
        "from": "T19n0954B_001:0313b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至於十五字。",
        "from": "T19n0954B_001:0313b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每計於字數。",
        "from": "T19n0954B_001:0313b15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一字一洛叉。",
        "from": "T19n0954B_001:0313b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至三十字。",
        "from": "T19n0954B_001:0313b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應誦三十洛叉。",
        "from": "T19n0954B_001:0313b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應作先事法。",
        "from": "T19n0954B_001:0313b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十字已上。",
        "from": "T19n0954B_001:0313b17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應誦一萬遍。",
        "from": "T19n0954B_001:0313b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一字頂輪王念誦儀軌",
        "from": "T19n0954B_001:0313b18_100",
        "context_with_next_sentence": false
    }
]