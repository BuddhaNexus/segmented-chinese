[
    {
        "source_sentence": "佛說天公經",
        "from": "T85n2876_001:1361a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鎘銅灌作柱。象□作屋椽。虎珀作屋𣖷。真珠",
        "from": "T85n2876_001:1361a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作羅網。星宿作鵲頭。日月作繚牕。可得萬",
        "from": "T85n2876_001:1361a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬九千年。",
        "from": "T85n2876_001:1361a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上有一小劫。",
        "from": "T85n2876_001:1361a13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下有一由旬。",
        "from": "T85n2876_001:1361a13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經雖小大有威神。",
        "from": "T85n2876_001:1361a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦等法華。",
        "from": "T85n2876_001:1361a14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦等涅槃。",
        "from": "T85n2876_001:1361a14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦朦雜經。",
        "from": "T85n2876_001:1361a14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如東海。",
        "from": "T85n2876_001:1361a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喻如太山。",
        "from": "T85n2876_001:1361a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入海取寶。",
        "from": "T85n2876_001:1361a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨其多少亦得重來。",
        "from": "T85n2876_001:1361a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入山取材。",
        "from": "T85n2876_001:1361a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "任材長短亦得重來。",
        "from": "T85n2876_001:1361a16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誰能抄此經。",
        "from": "T85n2876_001:1361a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手中螺文成。",
        "from": "T85n2876_001:1361a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誰能看此經。",
        "from": "T85n2876_001:1361a17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼中重光生。",
        "from": "T85n2876_001:1361a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誰能誦此經。",
        "from": "T85n2876_001:1361a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歷劫大聰明。",
        "from": "T85n2876_001:1361a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀世音造金。",
        "from": "T85n2876_001:1361a18_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大罪得除。",
        "from": "T85n2876_001:1361a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小罪得滅。",
        "from": "T85n2876_001:1361a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲向刀山。",
        "from": "T85n2876_001:1361a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀山摧折。",
        "from": "T85n2876_001:1361a19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲向劍樹。",
        "from": "T85n2876_001:1361a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劍樹自缺。",
        "from": "T85n2876_001:1361a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲向鑪炭。",
        "from": "T85n2876_001:1361a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鑪炭自滅。",
        "from": "T85n2876_001:1361a20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲向鑵湯。",
        "from": "T85n2876_001:1361a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鑵湯自冷。",
        "from": "T85n2876_001:1361a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倉倉靈音聲朗朗。",
        "from": "T85n2876_001:1361a21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "講堂中央瑠璃作地。",
        "from": "T85n2876_001:1361a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白銀為譬。",
        "from": "T85n2876_001:1361a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前登金床後仚王凡手把金經。",
        "from": "T85n2876_001:1361a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至佛說言。",
        "from": "T85n2876_001:1361a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憶佛如憶食。",
        "from": "T85n2876_001:1361a23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念佛如念命。",
        "from": "T85n2876_001:1361a23_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日誦五遍。",
        "from": "T85n2876_001:1361a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除罪一千五百萬劫。",
        "from": "T85n2876_001:1361a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆釋成佛道。",
        "from": "T85n2876_001:1361a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說天公經一卷",
        "from": "T85n2876_001:1361a26_100",
        "context_with_next_sentence": false
    }
]