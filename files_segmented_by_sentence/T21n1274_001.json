[
    {
        "source_sentence": "大聖歡喜雙身毘那夜迦天形像",
        "from": "T21n1274_001:0323b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "品儀軌一卷",
        "from": "T21n1274_001:0323b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憬瑟撰集",
        "from": "T21n1274_001:0323b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今於含光口傳。",
        "from": "T21n1274_001:0323b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "制此祕密儀軌。",
        "from": "T21n1274_001:0323b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其雙身天王形像。",
        "from": "T21n1274_001:0323b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫婦二天令相抱立。",
        "from": "T21n1274_001:0323b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其長七寸或五寸作之。",
        "from": "T21n1274_001:0323b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二天俱象頭人身。",
        "from": "T21n1274_001:0323b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但男天面繫女天右肩。",
        "from": "T21n1274_001:0323b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而令現女天背。",
        "from": "T21n1274_001:0323b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦女天面繫男天右肩而令視男天背。",
        "from": "T21n1274_001:0323b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "足踵皆俱露現。",
        "from": "T21n1274_001:0323b11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手足柔軟猶如壯肥端正女人。",
        "from": "T21n1274_001:0323b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "男天頭無華鬘。",
        "from": "T21n1274_001:0323b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "肩係赤色袈裟。",
        "from": "T21n1274_001:0323b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "女天頭有華鬘。",
        "from": "T21n1274_001:0323b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而不著袈裟。",
        "from": "T21n1274_001:0323b13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手足有瓔珞環。",
        "from": "T21n1274_001:0323b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦用其兩足蹈男天足端。",
        "from": "T21n1274_001:0323b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此二天俱白肉色著赤色裙。",
        "from": "T21n1274_001:0323b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各以二手互抱腰上。",
        "from": "T21n1274_001:0323b15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其右手覆左手背。",
        "from": "T21n1274_001:0323b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二天右手中指端。",
        "from": "T21n1274_001:0323b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令至左手中指中節背上。",
        "from": "T21n1274_001:0323b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此相抱像表六處之愛。",
        "from": "T21n1274_001:0323b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六處之愛者。",
        "from": "T21n1274_001:0323b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者以鼻各觸愛背。",
        "from": "T21n1274_001:0323b18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者以臆合愛。",
        "from": "T21n1274_001:0323b18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者以手抱愛腰。",
        "from": "T21n1274_001:0323b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者以腹合愛。",
        "from": "T21n1274_001:0323b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者以二足蹈愛。",
        "from": "T21n1274_001:0323b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六者著赤色裙。",
        "from": "T21n1274_001:0323b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是偏表敬愛故。",
        "from": "T21n1274_001:0323b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有行者造供六愛敬像。",
        "from": "T21n1274_001:0323b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此人必得國王大臣后妃婇女及以一切諸人敬愛。",
        "from": "T21n1274_001:0323b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發勇猛信心。",
        "from": "T21n1274_001:0323b22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫生一念疑滯。",
        "from": "T21n1274_001:0323b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往往證驗甚以多多　若欲修調和法除障難者。",
        "from": "T21n1274_001:0323b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應造供六臂天像。",
        "from": "T21n1274_001:0323b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其形象頭人身具足六臂。",
        "from": "T21n1274_001:0323b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六臂者。",
        "from": "T21n1274_001:0323b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂左上手把刀。",
        "from": "T21n1274_001:0323b25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次手把果盤。",
        "from": "T21n1274_001:0323b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下手把輪。",
        "from": "T21n1274_001:0323b26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右上手把棒。",
        "from": "T21n1274_001:0323b26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次手把跛折羅。",
        "from": "T21n1274_001:0323b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下手把索。",
        "from": "T21n1274_001:0323b27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有四臂像。",
        "from": "T21n1274_001:0323b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其形象頭人身具足四臂。",
        "from": "T21n1274_001:0323b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂右第一手執鉞斧。",
        "from": "T21n1274_001:0323b28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二手把歡喜團盤。",
        "from": "T21n1274_001:0323b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左第一手把牙或執杵第二手執寶棒。",
        "from": "T21n1274_001:0323c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復依不空奮怒王觀自在菩薩經。",
        "from": "T21n1274_001:0323c01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明四臂天相者。",
        "from": "T21n1274_001:0323c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此毘那夜迦金剛。",
        "from": "T21n1274_001:0323c02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其形象頭人身具足四臂。",
        "from": "T21n1274_001:0323c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左一手執金剛杵。",
        "from": "T21n1274_001:0323c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次手持鉞斧。",
        "from": "T21n1274_001:0323c03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右一手執羂索。",
        "from": "T21n1274_001:0323c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次手持三叉戟。",
        "from": "T21n1274_001:0323c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此形像品儀軌。",
        "from": "T21n1274_001:0323c04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能祕擇器授。",
        "from": "T21n1274_001:0323c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大聖天王。",
        "from": "T21n1274_001:0323c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大自在天變化自在身故。",
        "from": "T21n1274_001:0323c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祕法云。",
        "from": "T21n1274_001:0323c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六通自在故名聖天。",
        "from": "T21n1274_001:0323c06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智慧自在故名大自在天。",
        "from": "T21n1274_001:0323c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就敬愛故名雙身毘那夜迦王。",
        "from": "T21n1274_001:0323c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就五穀故名六臂天。",
        "from": "T21n1274_001:0323c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又如經所說。",
        "from": "T21n1274_001:0323c08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魔醯首羅智自在。",
        "from": "T21n1274_001:0323c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大海龍王降雨時。",
        "from": "T21n1274_001:0323c09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉能分別數其渧。",
        "from": "T21n1274_001:0323c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一念中皆明了。",
        "from": "T21n1274_001:0323c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依此聖說以智慧自在故。",
        "from": "T21n1274_001:0323c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以名大自在天　復次明四方六臂毘那夜迦形像者。",
        "from": "T21n1274_001:0323c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東方摧碎天亦名無臺大將。",
        "from": "T21n1274_001:0323c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其形如天人。",
        "from": "T21n1274_001:0323c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天冠之上安象頭。",
        "from": "T21n1274_001:0323c13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手執傘蓋。",
        "from": "T21n1274_001:0323c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持劍。",
        "from": "T21n1274_001:0323c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南方飲食天亦名嚴髻大將。",
        "from": "T21n1274_001:0323c14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其形如天人。",
        "from": "T21n1274_001:0323c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天冠之上安象頭。",
        "from": "T21n1274_001:0323c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手執索。",
        "from": "T21n1274_001:0323c15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手執華鬘。",
        "from": "T21n1274_001:0323c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方衣服天亦名頂行大將。",
        "from": "T21n1274_001:0323c16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其形如天人。",
        "from": "T21n1274_001:0323c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冠之上安龍頭。",
        "from": "T21n1274_001:0323c17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手執弓。",
        "from": "T21n1274_001:0323c17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手把箭。",
        "from": "T21n1274_001:0323c17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方象頭天亦名金色伽那鉢底。",
        "from": "T21n1274_001:0323c18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其形象頭人身。",
        "from": "T21n1274_001:0323c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持白瑠璃珠。",
        "from": "T21n1274_001:0323c19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手執寶棒。",
        "from": "T21n1274_001:0323c19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或具足六臂如前所明　復六面六臂像如經所說。",
        "from": "T21n1274_001:0323c20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此法祕之滿賢右手執寶棒。",
        "from": "T21n1274_001:0323c21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛子左手把三股杵。",
        "from": "T21n1274_001:0323c21_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雙身毘那夜迦天形像品儀軌享和二年壬戌夏五月二日。",
        "from": "T21n1274_001:0323c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求此本於智積院慈順僧正所。",
        "from": "T21n1274_001:0323c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而挍合點國字命工上木。",
        "from": "T21n1274_001:0323c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豐山輪下僧快道誌。",
        "from": "T21n1274_001:0323c25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一挍了　　　　　　　　　　慈　順",
        "from": "T21n1274_001:0323c26_0",
        "context_with_next_sentence": true
    }
]