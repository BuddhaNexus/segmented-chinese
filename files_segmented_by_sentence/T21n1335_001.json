[
    {
        "source_sentence": "大吉義神呪經卷第一",
        "from": "T21n1335_001:0568a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元魏昭玄統沙門釋曇曜譯",
        "from": "T21n1335_001:0568a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無諸佛眾生真濟於一切法得自在者。七",
        "from": "T21n1335_001:0568a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛真濟毘婆尸。",
        "from": "T21n1335_001:0568a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尸棄。",
        "from": "T21n1335_001:0568a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比𮚒婆阜。",
        "from": "T21n1335_001:0568a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "訖囉迦孫陀。",
        "from": "T21n1335_001:0568a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦那迦牟尼。",
        "from": "T21n1335_001:0568a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉。",
        "from": "T21n1335_001:0568a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋師子兩足之尊有大名稱。",
        "from": "T21n1335_001:0568a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彌勒在兜率天上與大眾圍繞。",
        "from": "T21n1335_001:0568a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我至心念過去一切諸佛。",
        "from": "T21n1335_001:0568a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未來一切諸佛。",
        "from": "T21n1335_001:0568a10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現在一切諸佛無上法王。",
        "from": "T21n1335_001:0568a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是一切三世諸佛。",
        "from": "T21n1335_001:0568a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我皆歸命我悉歸依。",
        "from": "T21n1335_001:0568a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸依於法而此法者一切所敬。",
        "from": "T21n1335_001:0568a12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸依僧眾。",
        "from": "T21n1335_001:0568a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此眾僧者名勝福田。",
        "from": "T21n1335_001:0568a13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若少布施獲大果報。",
        "from": "T21n1335_001:0568a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸如來及諸聲聞今皆歸依。",
        "from": "T21n1335_001:0568a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復歸命住色究竟摩醯首羅。",
        "from": "T21n1335_001:0568a15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命首陀會天諸阿那含天。",
        "from": "T21n1335_001:0568a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命梵天王世間之祖。",
        "from": "T21n1335_001:0568a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命他化自在天王。",
        "from": "T21n1335_001:0568a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命化樂天王。",
        "from": "T21n1335_001:0568a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命兜率陀天王。",
        "from": "T21n1335_001:0568a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命須耶門天王。",
        "from": "T21n1335_001:0568a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命天帝釋。",
        "from": "T21n1335_001:0568a18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命四方護世大王。",
        "from": "T21n1335_001:0568a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命諸依地者一切鬼神。",
        "from": "T21n1335_001:0568a19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命一切龍乾闥婆。",
        "from": "T21n1335_001:0568a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲界中諸王因陀樓。",
        "from": "T21n1335_001:0568a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "炎羅婆樓㝹那素摩波羅墮闍仙波闍波提。",
        "from": "T21n1335_001:0568a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如此等今皆歸命。",
        "from": "T21n1335_001:0568a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二仙那囉達鉢婆力。",
        "from": "T21n1335_001:0568a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今說一切義吉成呪。",
        "from": "T21n1335_001:0568a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結甘露不死界。",
        "from": "T21n1335_001:0568a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能作五百事。",
        "from": "T21n1335_001:0568a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能使一切所為事吉。",
        "from": "T21n1335_001:0568a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能斷一切蠱道惡呪。",
        "from": "T21n1335_001:0568a24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能擁護一切世間。",
        "from": "T21n1335_001:0568a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令一切諸惡鬼神皆令退散。",
        "from": "T21n1335_001:0568a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切侵害人者盡能除滅。",
        "from": "T21n1335_001:0568a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令一切所作悉皆吉善。",
        "from": "T21n1335_001:0568a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能使惡者懷於怖懼。",
        "from": "T21n1335_001:0568a27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸欲誦呪者當誠心。",
        "from": "T21n1335_001:0568a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以妙香華鬘塗香末香寶蓋幢幡。",
        "from": "T21n1335_001:0568a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作倡妓樂微妙之音。",
        "from": "T21n1335_001:0568a29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養於彼滅結世尊如來。",
        "from": "T21n1335_001:0568b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清淨斷諸煩惱。",
        "from": "T21n1335_001:0568b01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "𣫍心在定不曾馳散。",
        "from": "T21n1335_001:0568b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行四弘誓救護一切。",
        "from": "T21n1335_001:0568b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出妙梵音雷聲遠震。",
        "from": "T21n1335_001:0568b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如迦陵頻伽人所樂聞。",
        "from": "T21n1335_001:0568b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願受我最上供養。",
        "from": "T21n1335_001:0568b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無過去未來現在一切諸佛。",
        "from": "T21n1335_001:0568b04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無毘婆尸佛。",
        "from": "T21n1335_001:0568b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尸棄佛。",
        "from": "T21n1335_001:0568b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘舍婆佛。",
        "from": "T21n1335_001:0568b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦羅那迦孫陀佛。",
        "from": "T21n1335_001:0568b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦那含牟尼佛。",
        "from": "T21n1335_001:0568b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉佛釋迦師子。",
        "from": "T21n1335_001:0568b06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彌勒上首十方世界如恒沙數。",
        "from": "T21n1335_001:0568b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多陀阿伽度阿羅呵三藐三佛陀。",
        "from": "T21n1335_001:0568b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸眾生作大救護。",
        "from": "T21n1335_001:0568b08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有災患能使善吉。",
        "from": "T21n1335_001:0568b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "審如是者我今至誠運香供養。",
        "from": "T21n1335_001:0568b09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願此香氣至諸佛所。",
        "from": "T21n1335_001:0568b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1335_001:0568b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "達遲　陀遲　阿遲　阿遲　寐遲　阿遲　那遲　彌遲說是呪已燒諸名香蘇合香等。",
        "from": "T21n1335_001:0568b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養三世一切諸佛人中師子。",
        "from": "T21n1335_001:0568b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常當憶念福祐於我。",
        "from": "T21n1335_001:0568b14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次至心燒眾名香薩闍賴闍香等。",
        "from": "T21n1335_001:0568b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運心供養摩醯首羅天王。",
        "from": "T21n1335_001:0568b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願此香氣至彼天宮。",
        "from": "T21n1335_001:0568b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1335_001:0568b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那那　那那　那那　囉囉　囉囉　囉囉　呵梨　阿梨　帝𠼝　俟羅帝　𠼝帝　斯帝復次誠心燒眾名香逼佉香等。",
        "from": "T21n1335_001:0568b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運心供養欲界魔王有大勢力欲界之主。",
        "from": "T21n1335_001:0568b22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願此香氣達彼王所。",
        "from": "T21n1335_001:0568b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1335_001:0568b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豆豆　豆豆　豆豆　豆尼　豆尼　豆尼　披那　訶𠯋　呵尼　陀遲　摩尼　𠼝帝復次誠心燒眾名香弗迦香等運心供養化樂天王并化樂諸天。",
        "from": "T21n1335_001:0568b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願此香氣達彼王所。",
        "from": "T21n1335_001:0568b28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不憶念我當頭破作七分。",
        "from": "T21n1335_001:0568b29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1335_001:0568b29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尼梨　尼黎　尼𥟦　寐黎　寐黎　寐黎　脂隷　脂隷　彌脂隷　彌脂隷復次誠心燒眾名香。",
        "from": "T21n1335_001:0568c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運心供養他化天王。",
        "from": "T21n1335_001:0568c03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願此香氣達彼王所。",
        "from": "T21n1335_001:0568c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當憶念我即說呪曰。",
        "from": "T21n1335_001:0568c04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦隷　婆隷　那隷　呵隷　毘那隷　毘舍隷　訶羅彌帝莎呵復次我今誠心燒眾名香。",
        "from": "T21n1335_001:0568c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運心供養兜率陀天王。",
        "from": "T21n1335_001:0568c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願此香氣達彼王所。",
        "from": "T21n1335_001:0568c08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當受我供。",
        "from": "T21n1335_001:0568c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1335_001:0568c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩私　摩私　摩私　摩摩私　娑羅摩私　摩那私　呵羅尼　呵富呵　波羅尼　賀梨　寐帝　乾途　婆賀羅　莎呵復次我今燒眾名香。",
        "from": "T21n1335_001:0568c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運心供養須耶門天王輔相將從。",
        "from": "T21n1335_001:0568c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願此香氣達彼王所。",
        "from": "T21n1335_001:0568c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1335_001:0568c14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佉咃　佉尼咃　佉稚毘尼　薩旨遲　薩持遲地茶　婆婆利　乾頭　婆賀羅　莎呵復次燒眾名香。",
        "from": "T21n1335_001:0568c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運心供養帝釋三十三天王。",
        "from": "T21n1335_001:0568c18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願此香氣達彼王所。",
        "from": "T21n1335_001:0568c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1335_001:0568c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賀羅　賀羅　摩黎尼　遮羅私利　毘羅私利復次燒眾名香。",
        "from": "T21n1335_001:0568c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運心供養東方提頭賴吒天王輔相眷屬。",
        "from": "T21n1335_001:0568c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願此香氣達彼王所。",
        "from": "T21n1335_001:0568c23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1335_001:0568c23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿夷　婆夷　那稚夷　那耶尼　那那那　波帝　尼波帝　波羅尼　莎呵復次燒眾名香。",
        "from": "T21n1335_001:0568c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運心供養南方毘留勒叉天王輔相眷屬。",
        "from": "T21n1335_001:0568c27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願此香氣達彼王所。",
        "from": "T21n1335_001:0568c27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1335_001:0568c27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘利私　毘利私　旨利私　寐利私　寐尼利私復次燒眾名香。",
        "from": "T21n1335_001:0569a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運心供養西方毘留博又天王輔相眷屬。",
        "from": "T21n1335_001:0569a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願此香氣達彼王所。",
        "from": "T21n1335_001:0569a02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1335_001:0569a02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豆嚁　鼻豆嚁　鼻𠯋　𠯋寐　呵𠯋寐寐　寐利　至寐利　至寐利帝　呵羅帝　莎呵復次燒眾名香。",
        "from": "T21n1335_001:0569a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運心供養北方毘沙門天王輔相眷屬。",
        "from": "T21n1335_001:0569a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願此香氣達彼王所。",
        "from": "T21n1335_001:0569a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1335_001:0569a07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佉稚　佉稚　佉佉稚　佉佉稚　摩稚　摩稚私　摩茶　呵梨膩復次燒眾名香。",
        "from": "T21n1335_001:0569a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運心供養大海渚中楞伽城內羅剎之主毘沙拏。",
        "from": "T21n1335_001:0569a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願此香氣達於彼所。",
        "from": "T21n1335_001:0569a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1335_001:0569a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遮利　遮利　周利　周利　帝利　帝利　婆羅尼利　婆羅呵帝尼　呵羅蜜帝　莎呵復次燒眾名香。",
        "from": "T21n1335_001:0569a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運心供養毘摩質多羅阿修羅王居在海邊。",
        "from": "T21n1335_001:0569a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願此香氣達彼王所。",
        "from": "T21n1335_001:0569a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1335_001:0569a16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伊支　毘支　毘遮支　遮吒隷　摩吒隷摩摩吒　阿羅蜜帝　莎呵復次燒眾名香。",
        "from": "T21n1335_001:0569a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運心供養伽羅龍王居在海中。",
        "from": "T21n1335_001:0569a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願此香氣達龍王所。",
        "from": "T21n1335_001:0569a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1335_001:0569a20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦　迦　迦利　迦羅尼利　迦羅婆梨尼　毘黎私利　呵羅蜜帝　莎婆呵復次燒眾名香。",
        "from": "T21n1335_001:0569a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運心供養地神。",
        "from": "T21n1335_001:0569a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願此香氣達於彼所。",
        "from": "T21n1335_001:0569a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1335_001:0569a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘陳地𠼝　毘婆陳提𠼝　遮隣提梨　舍囉摩𠼝遲　摩摩囉囉遲　阿羅彌帝　莎呵若不信我呪。",
        "from": "T21n1335_001:0569a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心裂作七分血從面門出。",
        "from": "T21n1335_001:0569a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不聽佛所說善法。",
        "from": "T21n1335_001:0569a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終無吉利為病所殺。",
        "from": "T21n1335_001:0569a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身壞命終當墮地獄天阿修羅共相鬪戰。",
        "from": "T21n1335_001:0569a29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時帝釋軍敗退還。",
        "from": "T21n1335_001:0569b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既入城已即作是念。",
        "from": "T21n1335_001:0569b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今當詣婆伽婆所求索擁護。",
        "from": "T21n1335_001:0569b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於天人中佛為最勝。",
        "from": "T21n1335_001:0569b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈悲救濟一切眾生。",
        "from": "T21n1335_001:0569b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不害一切常為擁護。",
        "from": "T21n1335_001:0569b03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊在憂波難陀山常所樂處。",
        "from": "T21n1335_001:0569b04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大比丘眾五百人俱。",
        "from": "T21n1335_001:0569b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩千那由他。",
        "from": "T21n1335_001:0569b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月十五日布薩之時。",
        "from": "T21n1335_001:0569b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時兩足尊人天中上。",
        "from": "T21n1335_001:0569b06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四眾圍繞如世明燈。",
        "from": "T21n1335_001:0569b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋提桓因與其將從百千那由他三十三天。",
        "from": "T21n1335_001:0569b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來詣佛所頂禮佛足却坐一面一心合掌。",
        "from": "T21n1335_001:0569b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而說偈曰。",
        "from": "T21n1335_001:0569b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大聖世雄天人尊　　常以慈心愍一切",
        "from": "T21n1335_001:0569b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉一切智導師　　世間黑闇作照明",
        "from": "T21n1335_001:0569b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸天所不達　　唯有瞿曇獨知之",
        "from": "T21n1335_001:0569b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今我歸命大慈悲　　唯願為我作擁護",
        "from": "T21n1335_001:0569b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊聞此偈已告帝釋言。",
        "from": "T21n1335_001:0569b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天帝我佛法中有結呪界法。",
        "from": "T21n1335_001:0569b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能為人天作大擁護。",
        "from": "T21n1335_001:0569b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有聞是結呪界法。",
        "from": "T21n1335_001:0569b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若自持若教人持。",
        "from": "T21n1335_001:0569b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至心讀誦如說修行。",
        "from": "T21n1335_001:0569b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以呪力故刀不能傷。",
        "from": "T21n1335_001:0569b17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒火不害能却怨敵百由旬內無諸災患。",
        "from": "T21n1335_001:0569b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天阿修羅一切鬼神。",
        "from": "T21n1335_001:0569b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無能越此呪界而作衰害。",
        "from": "T21n1335_001:0569b19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故天帝應當受持是結呪法。",
        "from": "T21n1335_001:0569b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至心讀誦不得忘失。",
        "from": "T21n1335_001:0569b20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1335_001:0569b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闍隷　闍梨尼　摩闍隷　摩呵闍隷　摩何闍羅婆帝　阿闍利　阿者利　闍羅者利　闍𠯋隷　摩隷　𠯋摩摩隷　闍囉帝　遮囉婆囉帝　者羅帝　者囉闍囉帝　那囉帝　婆羅闍隷　伊𠼝伊囉婆帝　寐𠼝　寐羅婆帝　合鞞唎合鞞𠼝　毘唎　合子　翅由唎　摩呵翅由囉　婆帝　摩佉唎　摩呵　摩佉唎　呵羅私唎　婆闍梨　婆伽踰唎　鍮囉婆利　婆囉婆私唎　毘私唎　私利　私唎　私唎膩以此實呪擁護帝主眾患都除。",
        "from": "T21n1335_001:0569c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今慈彼提頭賴吒龍王。",
        "from": "T21n1335_001:0569c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禋囉婆尼龍王。",
        "from": "T21n1335_001:0569c04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘留博叉黑瞿曇龍王。",
        "from": "T21n1335_001:0569c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如意珠龍王。",
        "from": "T21n1335_001:0569c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婆修翅龍王檀茶波陀龍王。",
        "from": "T21n1335_001:0569c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿賢龍王。",
        "from": "T21n1335_001:0569c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難陀龍王。",
        "from": "T21n1335_001:0569c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名色威力身體俱大住須彌山。",
        "from": "T21n1335_001:0569c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天阿修羅鬪時。",
        "from": "T21n1335_001:0569c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常佐帝釋而為擁護。",
        "from": "T21n1335_001:0569c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿那婆達多龍王。",
        "from": "T21n1335_001:0569c08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿樓泥龍王。",
        "from": "T21n1335_001:0569c08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "般闍摩翅龍王。",
        "from": "T21n1335_001:0569c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得叉迦龍王。",
        "from": "T21n1335_001:0569c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難提那龍王。",
        "from": "T21n1335_001:0569c09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婆蓃翅龍王。",
        "from": "T21n1335_001:0569c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿婆囉闍等龍王。",
        "from": "T21n1335_001:0569c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大自在意龍王。",
        "from": "T21n1335_001:0569c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自在意龍王。",
        "from": "T21n1335_001:0569c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那茶黔龍王。",
        "from": "T21n1335_001:0569c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿波羅羅龍王。",
        "from": "T21n1335_001:0569c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "等沙彌勒迦龍王。",
        "from": "T21n1335_001:0569c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酪首龍王。",
        "from": "T21n1335_001:0569c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有如意龍王。",
        "from": "T21n1335_001:0569c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分陀利龍王。",
        "from": "T21n1335_001:0569c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方主龍王。",
        "from": "T21n1335_001:0569c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦駒吒翅龍王。",
        "from": "T21n1335_001:0569c13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "𭌘婆羅龍王。",
        "from": "T21n1335_001:0569c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿尸婆多羅龍王。",
        "from": "T21n1335_001:0569c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是龍王等我皆生慈心。",
        "from": "T21n1335_001:0569c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婆翅羅龍王。",
        "from": "T21n1335_001:0569c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "𭌘毘羅龍王。",
        "from": "T21n1335_001:0569c15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毛針龍王。",
        "from": "T21n1335_001:0569c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憂囉伽羯那龍王。",
        "from": "T21n1335_001:0569c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牛王龍王。",
        "from": "T21n1335_001:0569c16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等龍王我亦皆生慈心。",
        "from": "T21n1335_001:0569c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奮耳龍王。",
        "from": "T21n1335_001:0569c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "車龍王。",
        "from": "T21n1335_001:0569c17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聚積龍王。",
        "from": "T21n1335_001:0569c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善歡喜龍王。",
        "from": "T21n1335_001:0569c18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "犢子龍王。",
        "from": "T21n1335_001:0569c18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伊羅鉢龍王。",
        "from": "T21n1335_001:0569c18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禋濫拔唎龍王。",
        "from": "T21n1335_001:0569c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是龍王我皆生慈心。",
        "from": "T21n1335_001:0569c19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是一頭二頭龍等。",
        "from": "T21n1335_001:0569c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我於彼所都生慈心。",
        "from": "T21n1335_001:0569c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切世界有足無足二足四足多足。",
        "from": "T21n1335_001:0569c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆於彼所生於慈心。",
        "from": "T21n1335_001:0569c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾無足二足四足多足諸龍夜叉。",
        "from": "T21n1335_001:0569c22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸依地動以不動并根生者。",
        "from": "T21n1335_001:0569c23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆莫害我。",
        "from": "T21n1335_001:0569c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以諸佛力。",
        "from": "T21n1335_001:0569c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及正法威勢阿羅漢。",
        "from": "T21n1335_001:0569c24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使我吉安。",
        "from": "T21n1335_001:0569c24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1335_001:0569c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "都企　目企摩企　摩呵摩企　郁佉摩佉婆囉帝　婆囉婆帝　地唎　地囉　婆帝　守婆地唎　守婆那𠼝　那囉婆唎那囉地毘提利叔呵般茶唎　迦茶翅　迦茶脾　迦囉茶脾翅由唎　收囉婆豉　收羅婆豉　婆那豉　婆囉婆那豉　阿施　阿賒施　阿那囉脾　婆囉婆囉泥　者羅　者梨尼　毘者羅涅唎　婆陀地　阿婆陀地　伊吒陀地　阿吒吒唎　阿那吒囉哋　吒䶩　伊吒䶩　者䶩　毘茶脾　毘囉婆囉陀　摩呵囉涅唎　婆囉陀唎　波羅陀唎波伽陀利　跛那跛那　尼吒唎尼寐說是大呪王時。",
        "from": "T21n1335_001:0570a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而此大地周遍俱時六返震動。",
        "from": "T21n1335_001:0570a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切鬼神阿修羅等。",
        "from": "T21n1335_001:0570a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆大驚怖自相謂言。",
        "from": "T21n1335_001:0570a10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怪哉瞿曇大名稱者。",
        "from": "T21n1335_001:0570a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸天眾說是神呪。",
        "from": "T21n1335_001:0570a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而使諸天得於自在。",
        "from": "T21n1335_001:0570a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛告釋提桓因言。",
        "from": "T21n1335_001:0570a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "假使呪師誦持是呪。",
        "from": "T21n1335_001:0570a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結於呪界遍滿三界。",
        "from": "T21n1335_001:0570a13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "動不動等無能越者。",
        "from": "T21n1335_001:0570a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四道天王有大威力。",
        "from": "T21n1335_001:0570a14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名曰摩醯首羅。",
        "from": "T21n1335_001:0570a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處在阿那含聖果。",
        "from": "T21n1335_001:0570a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有不順越此呪界。",
        "from": "T21n1335_001:0570a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有是處。",
        "from": "T21n1335_001:0570a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大梵天王有大威德。",
        "from": "T21n1335_001:0570a16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千世界主有自在力。",
        "from": "T21n1335_001:0570a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能不順越此呪界亦無是處。",
        "from": "T21n1335_001:0570a17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魔王波旬欲界之主。",
        "from": "T21n1335_001:0570a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲越呪界亦無是處。",
        "from": "T21n1335_001:0570a18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他化自在天王化樂天王。",
        "from": "T21n1335_001:0570a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲越此呪界亦無是處。",
        "from": "T21n1335_001:0570a19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧兜率天王炎摩天王。",
        "from": "T21n1335_001:0570a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲越此呪界亦無是處。",
        "from": "T21n1335_001:0570a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝釋千眼舍脂之夫。",
        "from": "T21n1335_001:0570a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲越此呪界亦無是處。",
        "from": "T21n1335_001:0570a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護世四天王駒毘羅提頭賴吒。",
        "from": "T21n1335_001:0570a22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘留博叉毘留勒等。",
        "from": "T21n1335_001:0570a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲越此呪界亦無是處。",
        "from": "T21n1335_001:0570a23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有四王威力熾盛。",
        "from": "T21n1335_001:0570a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳩那羅伽蘭茶毘勒駒搖訶竺。",
        "from": "T21n1335_001:0570a24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲越此呪界亦無是處。",
        "from": "T21n1335_001:0570a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有四王於欲界中最得自在。",
        "from": "T21n1335_001:0570a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝釋閻羅王拔留那兌羅闍波闍波提。",
        "from": "T21n1335_001:0570a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲過此呪界亦無是處。",
        "from": "T21n1335_001:0570a27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有四王有大威力。",
        "from": "T21n1335_001:0570a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "䉛尼毘呵呪帝婆留闍提呵娑帝奢。",
        "from": "T21n1335_001:0570a28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲過此呪界亦無是處。",
        "from": "T21n1335_001:0570a29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有八龍王住於大海有大威德。",
        "from": "T21n1335_001:0570b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿那婆達多龍王。",
        "from": "T21n1335_001:0570b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拔留那龍王。",
        "from": "T21n1335_001:0570b01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘留勒龍王。",
        "from": "T21n1335_001:0570b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修鉢囉龍王。",
        "from": "T21n1335_001:0570b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "㨖絺都龍王。",
        "from": "T21n1335_001:0570b02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難陀龍王。",
        "from": "T21n1335_001:0570b02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "優波難陀龍王。",
        "from": "T21n1335_001:0570b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娑伽羅龍王等。",
        "from": "T21n1335_001:0570b03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大神變。",
        "from": "T21n1335_001:0570b03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等龍王欲越此呪界亦無是處。",
        "from": "T21n1335_001:0570b04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有八阿修羅王。",
        "from": "T21n1335_001:0570b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘摩質多囉阿修羅王。",
        "from": "T21n1335_001:0570b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修質多羅阿修羅王。",
        "from": "T21n1335_001:0570b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "邏花阿修羅王。",
        "from": "T21n1335_001:0570b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苫波唎阿修羅王。",
        "from": "T21n1335_001:0570b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鉢羅度阿修羅王。",
        "from": "T21n1335_001:0570b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "茂至連達囉那阿修羅王。",
        "from": "T21n1335_001:0570b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纏豆嚧那曜阿修羅王等。",
        "from": "T21n1335_001:0570b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能不順此呪欲過此呪界亦無是處。",
        "from": "T21n1335_001:0570b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有乾闥婆王。",
        "from": "T21n1335_001:0570b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "質多羅斯那那厨羅闍捺闍捺沙婆毘毘尸婆塞多羅尸騫遲提婆婆屯頭摩啅囉般者尸企乾闥婆鉢浮嘍守梵達囉迦摩勢絺栴檀那提乾闥婆等有大神力。",
        "from": "T21n1335_001:0570b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顏貌端正兼有名聞。",
        "from": "T21n1335_001:0570b13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲越呪界亦無是處。",
        "from": "T21n1335_001:0570b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告天帝此呪何故有是威力。",
        "from": "T21n1335_001:0570b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此呪乃是八十四億那由他百千恒河沙數諸佛神口之所共說。",
        "from": "T21n1335_001:0570b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今現在亦復宣說如是呪王。",
        "from": "T21n1335_001:0570b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復告天帝諦聽諦聽善思念之。",
        "from": "T21n1335_001:0570b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此呪之力能為眾生作大擁護。",
        "from": "T21n1335_001:0570b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遮諸鬼神不令嬈近。",
        "from": "T21n1335_001:0570b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以擁護。",
        "from": "T21n1335_001:0570b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此諸人道者人道之中。",
        "from": "T21n1335_001:0570b20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能為生天作其種子。",
        "from": "T21n1335_001:0570b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能出一切諸天勝報。",
        "from": "T21n1335_001:0570b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此處出佛此處出生聲聞弟子。",
        "from": "T21n1335_001:0570b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此處出生諸大德仙。",
        "from": "T21n1335_001:0570b22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此處出於沙門及婆羅門。",
        "from": "T21n1335_001:0570b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此處能出鬼神。",
        "from": "T21n1335_001:0570b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有惱亂於眾生者。",
        "from": "T21n1335_001:0570b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能遮惡鬼不令加害。",
        "from": "T21n1335_001:0570b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故出此呪。",
        "from": "T21n1335_001:0570b24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1335_001:0570b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿囉池　毘囉池　婆囉池　婆那池　婆囉毘囉毘　曇脾　曇脾一智　毘吒智毘囉吒智　首慧首慧　首呵曼茶唎　迦羅池　翅由唎　緊頭摩帝　陀囉　陀唎　陀囉囉遲　陀陀婆毘　達陀曷帝　囉娑阿竭囉婆帝　頻頭伽唎　囉多那闍梨說此呪時大地大海悉皆六返震動。",
        "from": "T21n1335_001:0570c03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸鬼神等咸發大取唱曰苦哉。",
        "from": "T21n1335_001:0570c04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸羅剎等亦復如是。",
        "from": "T21n1335_001:0570c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共相謂言今者說呪。",
        "from": "T21n1335_001:0570c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等大苦欲無生路。",
        "from": "T21n1335_001:0570c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是猛呪諸佛所說。",
        "from": "T21n1335_001:0570c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊復告天帝言。",
        "from": "T21n1335_001:0570c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今應當護此呪界。",
        "from": "T21n1335_001:0570c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由我護念此呪緣故。",
        "from": "T21n1335_001:0570c07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切人天無能越者。",
        "from": "T21n1335_001:0570c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復告四道面天王有大名稱。",
        "from": "T21n1335_001:0570c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住淨居天阿那含處。",
        "from": "T21n1335_001:0570c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名摩醯首羅。",
        "from": "T21n1335_001:0570c09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及其眷屬輔相大臣。",
        "from": "T21n1335_001:0570c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等宜應擁護此呪。",
        "from": "T21n1335_001:0570c10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五阿那含天神通威德。",
        "from": "T21n1335_001:0570c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色身名稱悉皆具足。",
        "from": "T21n1335_001:0570c11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等眷屬亦應擁護如是明呪。",
        "from": "T21n1335_001:0570c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時大梵天王從蓮花生娑婆界主。",
        "from": "T21n1335_001:0570c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眷屬輔相。",
        "from": "T21n1335_001:0570c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等亦當擁護此呪。",
        "from": "T21n1335_001:0570c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四禪諸天乃至梵身。",
        "from": "T21n1335_001:0570c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神通色貌名聞具足。",
        "from": "T21n1335_001:0570c14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及其眷屬輔相大臣。",
        "from": "T21n1335_001:0570c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦應擁護如是神呪。",
        "from": "T21n1335_001:0570c15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲界之主魔王波旬眷屬輔相。",
        "from": "T21n1335_001:0570c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應護斯呪。",
        "from": "T21n1335_001:0570c16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他化自在天王及其眷屬輔相大臣應護此呪。",
        "from": "T21n1335_001:0570c17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化樂天王及其眷屬輔相大臣應護斯呪。",
        "from": "T21n1335_001:0570c18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "炎魔天王眷屬輔相應護此呪。",
        "from": "T21n1335_001:0570c19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兜率天王眷屬輔相應護此呪。",
        "from": "T21n1335_001:0570c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝釋千眼舍脂之夫眷屬輔相應護斯呪。",
        "from": "T21n1335_001:0570c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大吉義神呪經卷第一",
        "from": "T21n1335_001:0570c22_100",
        "context_with_next_sentence": false
    }
]