[
    {
        "source_sentence": "瑜伽金剛頂經釋字母品一卷",
        "from": "T18n0880_001:0338b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T18n0880_001:0338b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空諡大鑒",
        "from": "T18n0880_001:0338b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正號大廣智大興善寺三藏沙門不空奉　詔譯􆿅阿上字門，一切法本不生故。",
        "from": "T18n0880_001:0338b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􆿆阿引去字門，一切法寂靜故。",
        "from": "T18n0880_001:0338b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􆿇伊上字門，一切法根不可得故。",
        "from": "T18n0880_001:0338b21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􆿈伊引去字門，一切法災禍不可得故。",
        "from": "T18n0880_001:0338b22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇇳塢字門，一切法譬喻不可得故。",
        "from": "T18n0880_001:0338b23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􆿊污引字門，一切法損減不可得故。",
        "from": "T18n0880_001:0338b24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􆿋哩字門，一切法神通不可得故。",
        "from": "T18n0880_001:0338b25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􆿌哩引字門，一切法類例不可得故。",
        "from": "T18n0880_001:0338b26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􆿍󰳆字門，一切法染不可得故。",
        "from": "T18n0880_001:0338b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􆿎𡃖字門，一切法沈沒不可得故。",
        "from": "T18n0880_001:0338b28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􆿏曀字門，一切法求不可得故。",
        "from": "T18n0880_001:0338b29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􆿐愛字門，一切法自在不可得故。",
        "from": "T18n0880_001:0338c01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􆿑污字門，一切法瀑流不可得故。",
        "from": "T18n0880_001:0338c02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􆿒奧字門，一切法化生不可得故。",
        "from": "T18n0880_001:0338c03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􆿓暗字門，一切法邊際不可得故。",
        "from": "T18n0880_001:0338c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􆿔惡字門，一切法遠離不可得故。",
        "from": "T18n0880_001:0338c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄑀迦上字門，一切法離作業故。",
        "from": "T18n0880_001:0338c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄑌佉上字門，一切法等虛空不可得故。",
        "from": "T18n0880_001:0338c07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄑘誐上字門，一切法行不可得故。",
        "from": "T18n0880_001:0338c08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄑤伽去引字門，一切法一合不可得故。",
        "from": "T18n0880_001:0338c09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄑰仰鼻呼字門，一切法支分不可得故。",
        "from": "T18n0880_001:0338c10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄑼左字門，一切法離一切遷變故。",
        "from": "T18n0880_001:0338c11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄒪磋上字門，一切法影像不可得故。",
        "from": "T18n0880_001:0338c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄒶惹字門，一切法生不可得故。",
        "from": "T18n0880_001:0338c13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄓂酇去字門，一切法戰敵不可得故。",
        "from": "T18n0880_001:0338c14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄓎穰上字門，一切法智不可得故。",
        "from": "T18n0880_001:0338c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄓚吒上字門，一切法慢不可得故。",
        "from": "T18n0880_001:0338c16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄓦咤上字門，一切法長養不可得故。",
        "from": "T18n0880_001:0338c17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄓲拏上字門，一切法怨敵不可得故。",
        "from": "T18n0880_001:0338c18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄓾荼去字門，一切法執持不可得故。",
        "from": "T18n0880_001:0338c19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄕋拏尼爽反鼻呼字門，一切法諍不可得故。",
        "from": "T18n0880_001:0338c20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄕗多上字門，一切法如如不可得故。",
        "from": "T18n0880_001:0338c21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄕣他上字門，一切法住處不可得故。",
        "from": "T18n0880_001:0338c22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄕯娜字門，一切法施不可得故。",
        "from": "T18n0880_001:0338c23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄕻馱去字門，一切法界不可得故。",
        "from": "T18n0880_001:0338c24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄖩曩字門，一切法名不可得故。",
        "from": "T18n0880_001:0339a01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄖵跛字門，一切法第一義諦不可得故。",
        "from": "T18n0880_001:0339a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄗁頗字門，一切法不堅，如聚沫故。",
        "from": "T18n0880_001:0339a03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄗍麼字門，一切法縛不可得故。",
        "from": "T18n0880_001:0339a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄗙婆去重字門，一切法有不可得故。",
        "from": "T18n0880_001:0339a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄗥莽字門，一切法吾我不可得故。",
        "from": "T18n0880_001:0339a06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄗱野字門，一切法乘不可得故。",
        "from": "T18n0880_001:0339a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄗽囉字門，一切法離諸塵染故。",
        "from": "T18n0880_001:0339a08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄙊邏字門，一切法相不可得故。",
        "from": "T18n0880_001:0339a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄙖嚩字門，一切法語言道斷故。",
        "from": "T18n0880_001:0339a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄙢捨字門，一切法本性寂故。",
        "from": "T18n0880_001:0339a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄙮灑字門，一切法性鈍故。",
        "from": "T18n0880_001:0339a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄙺娑上字門，一切法一切諦不可得故。",
        "from": "T18n0880_001:0339a13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄚨賀字門，一切法因不可得故。",
        "from": "T18n0880_001:0339a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄚴乞灑二合字門，一切法盡不可得故。",
        "from": "T18n0880_001:0339a15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瑜伽金剛頂經釋字母品一卷",
        "from": "T18n0880_001:0339a16_100",
        "context_with_next_sentence": false
    }
]