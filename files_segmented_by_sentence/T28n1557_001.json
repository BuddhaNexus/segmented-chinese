{
    "T28n1557_001:0998a09_0": "阿毘曇五法行經",
    "T28n1557_001:0998a11_0": "後漢安息三藏安世高譯",
    "T28n1557_001:0998a12_0": "苦法黠可苦法黠。",
    "T28n1557_001:0998a12_1": "習法黠可習法黠。",
    "T28n1557_001:0998a12_2": "盡法黠可盡法黠。",
    "T28n1557_001:0998a13_3": "道法黠可道法黠。",
    "T28n1557_001:0998a13_4": "苦法者。",
    "T28n1557_001:0998a13_5": "謂形體萬物皆當衰老死亡。",
    "T28n1557_001:0998a14_6": "是為苦。",
    "T28n1557_001:0998a14_7": "癡人謂可常保持。",
    "T28n1557_001:0998a15_8": "是為樂黠。",
    "T28n1557_001:0998a15_9": "可知是為苦。",
    "T28n1557_001:0998a15_10": "便不復向生死。",
    "T28n1557_001:0998a15_11": "是為苦法黠。",
    "T28n1557_001:0998a16_12": "可。",
    "T28n1557_001:0998a17_0": "習法者。",
    "T28n1557_001:0998a17_1": "謂習欲習得習婬習怒習癡習好習美。",
    "T28n1557_001:0998a18_2": "黠可者如是為習。",
    "T28n1557_001:0998a18_3": "從習得盡便不欲。",
    "T28n1557_001:0998a18_4": "是為習法黠。",
    "T28n1557_001:0998a19_5": "可盡法者。",
    "T28n1557_001:0998a19_6": "謂人物會當消散滅盡。",
    "T28n1557_001:0998a19_7": "便得亦不喜。",
    "T28n1557_001:0998a20_8": "失亦不憂。",
    "T28n1557_001:0998a20_9": "是為盡法黠。",
    "T28n1557_001:0998a20_10": "可道法者。",
    "T28n1557_001:0998a21_0": "行道得道。",
    "T28n1557_001:0998a21_1": "作善上天。",
    "T28n1557_001:0998a21_2": "作惡入惡。",
    "T28n1557_001:0998a21_3": "道黠可者知去惡就善。",
    "T28n1557_001:0998a22_4": "是為道法黠。",
    "T28n1557_001:0998a22_5": "可黠者。",
    "T28n1557_001:0998a22_6": "覺可者。",
    "T28n1557_001:0998a22_7": "知。",
    "T28n1557_001:0998a23_0": "本不知是為苦。",
    "T28n1557_001:0998a23_1": "苦為一意知為苦。",
    "T28n1557_001:0998a23_2": "是為二意。",
    "T28n1557_001:0998a24_0": "習為一意知為習。",
    "T28n1557_001:0998a24_1": "是為二意。",
    "T28n1557_001:0998a24_2": "盡為一意知為盡。",
    "T28n1557_001:0998a25_3": "是為二意。",
    "T28n1557_001:0998a25_4": "道為一意知為道。",
    "T28n1557_001:0998a25_5": "是為二意。",
    "T28n1557_001:0998a25_6": "此八意在外。",
    "T28n1557_001:0998a27_0": "非常苦空非身。",
    "T28n1557_001:0998a27_1": "本習生因緣盡。",
    "T28n1557_001:0998a27_2": "止如意要道處受觀。",
    "T28n1557_001:0998a29_0": "盡。",
    "T28n1557_001:0998a29_1": "苦空非身何緣得盡。",
    "T28n1557_001:0998a29_2": "盡從苦來。",
    "T28n1557_001:0998a29_3": "從苦得盡。",
    "T28n1557_001:0998b01_4": "因盡便得空。",
    "T28n1557_001:0998b01_5": "得空便知非我身。",
    "T28n1557_001:0998b01_6": "是四意為隨苦諦。",
    "T28n1557_001:0998b03_0": "何等為苦。",
    "T28n1557_001:0998b03_1": "一切在生死皆為苦。",
    "T28n1557_001:0998b03_2": "會欲亦不欲會欲。",
    "T28n1557_001:0998b04_3": "謂人諸所欲得亦不欲。",
    "T28n1557_001:0998b04_4": "謂人意諸所不欲。",
    "T28n1557_001:0998b05_5": "是皆為苦。",
    "T28n1557_001:0998b05_6": "貪從習出。",
    "T28n1557_001:0998b05_7": "隨非常意求滅苦從習得。",
    "T28n1557_001:0998b07_0": "何等為法。",
    "T28n1557_001:0998b07_1": "謂因緣作是得是是為法。",
    "T28n1557_001:0998b07_2": "當為識已識為却意當為斷。",
    "T28n1557_001:0998b08_3": "從四諦中苦諦。",
    "T28n1557_001:0998b08_4": "習諦為證有。",
    "T28n1557_001:0998b09_5": "道見苦知從習起。",
    "T28n1557_001:0998b09_6": "見習知苦見盡諦知非常。",
    "T28n1557_001:0998b11_0": "何等為可。",
    "T28n1557_001:0998b11_1": "謂憙道不忘道。",
    "T28n1557_001:0998b11_2": "常求道以道為可。",
    "T28n1557_001:0998b12_0": "何等為黠。",
    "T28n1557_001:0998b12_1": "常問道為黠。",
    "T28n1557_001:0998b12_2": "已問能受能行是為黠。",
    "T28n1557_001:0998b13_3": "習亦如是。",
    "T28n1557_001:0998b13_4": "盡滅亦如是。",
    "T28n1557_001:0998b13_5": "道亦如是。",
    "T28n1557_001:0998b14_0": "苦為罪法為行結。",
    "T28n1557_001:0998b14_1": "黠為三十七品經。",
    "T28n1557_001:0998b14_2": "可為行行者為行道。",
    "T28n1557_001:0998b15_3": "如是為習。",
    "T28n1557_001:0998b15_4": "如是為盡。",
    "T28n1557_001:0998b15_5": "如是為道。",
    "T28n1557_001:0998b16_6": "皆為增上。",
    "T28n1557_001:0998b17_0": "第一為苦。",
    "T28n1557_001:0998b17_1": "何等為苦。",
    "T28n1557_001:0998b17_2": "一切惡不可意為苦。",
    "T28n1557_001:0998b17_3": "已識苦不欲者。",
    "T28n1557_001:0998b18_4": "便行道不離為可。",
    "T28n1557_001:0998b18_5": "苦生有本。",
    "T28n1557_001:0998b18_6": "苦為何等。",
    "T28n1557_001:0998b19_7": "本從萬物。",
    "T28n1557_001:0998b19_8": "萬物無有亦不盡。",
    "T28n1557_001:0998b19_9": "已不盡人亦不憂。",
    "T28n1557_001:0998b20_10": "已不憂人亦無有苦。",
    "T28n1557_001:0998b21_0": "第二為習。",
    "T28n1557_001:0998b21_1": "何等為習。",
    "T28n1557_001:0998b21_2": "意隨愛為習。",
    "T28n1557_001:0998b21_3": "斷愛無有習。",
    "T28n1557_001:0998b22_4": "持何等行為斷愛。",
    "T28n1557_001:0998b22_5": "萬物皆從因緣生。",
    "T28n1557_001:0998b23_6": "斷因緣不復生。",
    "T28n1557_001:0998b23_7": "當那得斷因緣。",
    "T28n1557_001:0998b23_8": "持意念道。",
    "T28n1557_001:0998b24_0": "已持意念道。",
    "T28n1557_001:0998b24_1": "意不得兩念。",
    "T28n1557_001:0998b24_2": "便在道是。",
    "T28n1557_001:0998b24_3": "為習。",
    "T28n1557_001:0998b25_0": "第三為盡。",
    "T28n1557_001:0998b25_1": "苦法為萬物。",
    "T28n1557_001:0998b25_2": "以敗便得憂。",
    "T28n1557_001:0998b25_3": "已得憂便老。",
    "T28n1557_001:0998b26_4": "已老便得病瘦死。",
    "T28n1557_001:0998b26_5": "是名為外盡苦法。",
    "T28n1557_001:0998b26_6": "何以故。",
    "T28n1557_001:0998b27_7": "為外盡苦法。",
    "T28n1557_001:0998b27_8": "為自罪未除。",
    "T28n1557_001:0998b27_9": "何以故。",
    "T28n1557_001:0998b27_10": "為自罪未除。",
    "T28n1557_001:0998b28_11": "為生死未滅。",
    "T28n1557_001:0998b28_12": "何以故。",
    "T28n1557_001:0998b28_13": "生死未滅為非一意。",
    "T28n1557_001:0998b29_14": "何以故。",
    "T28n1557_001:0998b29_15": "非一意為不墮禪棄故。",
    "T28n1557_001:0998b29_16": "何以故。",
    "T28n1557_001:0998c01_0": "不墮禪棄不受行如佛語。",
    "T28n1557_001:0998c01_1": "是名為外盡。",
    "T28n1557_001:0998c01_2": "內盡為何等。",
    "T28n1557_001:0998c02_3": "意墮守。",
    "T28n1557_001:0998c02_4": "已墮守餘意不得生。",
    "T28n1557_001:0998c02_5": "已餘意不得生便滅結。",
    "T28n1557_001:0998c03_6": "已滅結便罪盡。",
    "T28n1557_001:0998c03_7": "已罪盡便盡無有。",
    "T28n1557_001:0998c04_8": "是名為內盡。",
    "T28n1557_001:0998c05_0": "第四為道。",
    "T28n1557_001:0998c05_1": "何等為道。",
    "T28n1557_001:0998c05_2": "苦可意道名為八種。",
    "T28n1557_001:0998c05_3": "何等為八種。",
    "T28n1557_001:0998c06_4": "如是安般守意說八行意。",
    "T28n1557_001:0998c06_5": "不墮生死但有墮道。",
    "T28n1557_001:0998c07_6": "已墮道便斷上頭三事。",
    "T28n1557_001:0998c07_7": "何等三事。",
    "T28n1557_001:0998c08_8": "苦習盡。",
    "T28n1557_001:0998c08_9": "已斷苦習盡便定。",
    "T28n1557_001:0998c08_10": "已定所向便得道。",
    "T28n1557_001:0998c09_11": "何等為得道。",
    "T28n1557_001:0998c09_12": "已苦滅不復生是為得。",
    "T28n1557_001:0998c09_13": "有五法行。",
    "T28n1557_001:0998c10_14": "何等五。",
    "T28n1557_001:0998c10_15": "一者色。",
    "T28n1557_001:0998c10_16": "二者意。",
    "T28n1557_001:0998c10_17": "三者所念。",
    "T28n1557_001:0998c10_18": "四者別離意行。",
    "T28n1557_001:0998c11_19": "五者無為。",
    "T28n1557_001:0998c12_0": "色為何等。",
    "T28n1557_001:0998c12_1": "所色一切在四行。",
    "T28n1557_001:0998c12_2": "亦從四行所。",
    "T28n1557_001:0998c12_3": "四行為何等。",
    "T28n1557_001:0998c13_4": "地種水種火種風種。",
    "T28n1557_001:0998c13_5": "亦從四行因。",
    "T28n1557_001:0998c14_6": "所色為何等。",
    "T28n1557_001:0998c14_7": "眼根耳根鼻根舌根身根。",
    "T28n1557_001:0998c14_8": "色聲香味細滑。",
    "T28n1557_001:0998c15_9": "亦一處不更。",
    "T28n1557_001:0998c16_0": "意為何等。",
    "T28n1557_001:0998c16_1": "所意心識。",
    "T28n1557_001:0998c16_2": "是為何等。",
    "T28n1557_001:0998c16_3": "六識身。",
    "T28n1557_001:0998c16_4": "六識為何等。",
    "T28n1557_001:0998c17_5": "眼識耳識鼻識舌識身識心識。",
    "T28n1557_001:0998c18_0": "所念法為何等。",
    "T28n1557_001:0998c18_1": "若所念法意共俱。",
    "T28n1557_001:0998c18_2": "是為何等。",
    "T28n1557_001:0998c19_0": "痛想行痒念欲。",
    "T28n1557_001:0998c19_1": "是意定黠。",
    "T28n1557_001:0998c19_2": "信進計念貪不貪善本惡本不分別本。",
    "T28n1557_001:0998c20_3": "一切結縛便勞。",
    "T28n1557_001:0998c20_4": "從起所黠所見所要亦所有。",
    "T28n1557_001:0998c21_5": "如是法意共俱。",
    "T28n1557_001:0998c21_6": "是名為意所念法。",
    "T28n1557_001:0998c23_0": "別離意行為何等。",
    "T28n1557_001:0998c23_1": "所別離意不共。",
    "T28n1557_001:0998c23_2": "是為何等。",
    "T28n1557_001:0998c24_0": "得不思想政盡政不思想念想。",
    "T28n1557_001:0998c24_1": "下輩苦得處得種得。",
    "T28n1557_001:0998c25_2": "入生老止非常名字絕具如應。",
    "T28n1557_001:0998c25_3": "亦餘如是法分別意行。",
    "T28n1557_001:0998c26_4": "是名分別意行。",
    "T28n1557_001:0998c27_0": "無為何等。",
    "T28n1557_001:0998c27_1": "空滅未離滅不須受。",
    "T28n1557_001:0998c28_0": "地種為何等堅者。",
    "T28n1557_001:0998c28_1": "水種何等濕者。",
    "T28n1557_001:0998c28_2": "火種何等熱者。",
    "T28n1557_001:0998c29_3": "風種何等起者。",
    "T28n1557_001:0998c29_4": "眼根何等。",
    "T28n1557_001:0998c29_5": "眼識相著可色。",
    "T28n1557_001:0999a01_6": "耳根何等。",
    "T28n1557_001:0999a01_7": "耳識相著可色。",
    "T28n1557_001:0999a01_8": "鼻根何等。",
    "T28n1557_001:0999a01_9": "鼻識相著可色。",
    "T28n1557_001:0999a02_10": "舌根何等。",
    "T28n1557_001:0999a02_11": "舌識相著可色。",
    "T28n1557_001:0999a02_12": "身根何等。",
    "T28n1557_001:0999a03_13": "身根相著可色。",
    "T28n1557_001:0999a03_14": "色為何等。",
    "T28n1557_001:0999a03_15": "若色端正不端正等色。",
    "T28n1557_001:0999a04_16": "俱中央色想。",
    "T28n1557_001:0999a04_17": "像上頭一識更。",
    "T28n1557_001:0999a04_18": "眼識色更為心識更。",
    "T28n1557_001:0999a05_19": "是色兩識。",
    "T28n1557_001:0999a05_20": "更知何等。",
    "T28n1557_001:0999a05_21": "兩眼識心識。",
    "T28n1557_001:0999a06_22": "聲為何等。",
    "T28n1557_001:0999a06_23": "從受行出聲。",
    "T28n1557_001:0999a06_24": "亦不從受行出聲。",
    "T28n1557_001:0999a07_25": "從受行本聲。",
    "T28n1557_001:0999a07_26": "亦不從受行本受聲。",
    "T28n1557_001:0999a07_27": "若上頭一識更知耳識已。",
    "T28n1557_001:0999a08_28": "更心識便知是聲。",
    "T28n1557_001:0999a08_29": "兩識更知耳識心識。",
    "T28n1557_001:0999a09_30": "香為何等。",
    "T28n1557_001:0999a09_31": "若根香若莖香若花香若實香。",
    "T28n1557_001:0999a10_32": "香香臭香等香所香。",
    "T28n1557_001:0999a10_33": "是名為香。",
    "T28n1557_001:0999a11_0": "若上頭一識。",
    "T28n1557_001:0999a11_1": "一識更知鼻識已。",
    "T28n1557_001:0999a11_2": "更心識便如是香。",
    "T28n1557_001:0999a12_3": "兩識更知鼻識心識。",
    "T28n1557_001:0999a12_4": "味為何等。",
    "T28n1557_001:0999a12_5": "若酢味甜味鹽味苦味醎味辛味澁味。",
    "T28n1557_001:0999a13_6": "亦所噉覺味。",
    "T28n1557_001:0999a14_0": "若上頭一識知舌識。",
    "T28n1557_001:0999a14_1": "舌識已更心識便知是味。",
    "T28n1557_001:0999a15_2": "兩識更知舌識心識。",
    "T28n1557_001:0999a15_3": "細滑更為何等。",
    "T28n1557_001:0999a15_4": "若滑若麁若輕若重若寒若熱若飢若渴。",
    "T28n1557_001:0999a16_5": "為上頭一識知身識身。",
    "T28n1557_001:0999a17_6": "識已更心識便知。",
    "T28n1557_001:0999a17_7": "是一處樂為兩識更知身識心識。",
    "T28n1557_001:0999a18_8": "心識一處不更。",
    "T28n1557_001:0999a18_9": "色為何等。",
    "T28n1557_001:0999a19_10": "若色法識想著。",
    "T28n1557_001:0999a19_11": "是為何等。",
    "T28n1557_001:0999a19_12": "若身善者不善者不更。",
    "T28n1557_001:0999a20_13": "若常一識知心識。",
    "T28n1557_001:0999a21_0": "眼識為何等。",
    "T28n1557_001:0999a21_1": "眼相依色因知。",
    "T28n1557_001:0999a21_2": "耳識為何等。",
    "T28n1557_001:0999a22_0": "故耳根相依聲因知。",
    "T28n1557_001:0999a22_1": "鼻為何等。",
    "T28n1557_001:0999a22_2": "鼻根相依香因知。",
    "T28n1557_001:0999a23_3": "舌識為何等。",
    "T28n1557_001:0999a23_4": "舌根相依味因知。",
    "T28n1557_001:0999a23_5": "身識為何等。",
    "T28n1557_001:0999a24_6": "身根相依樂著因知。",
    "T28n1557_001:0999a24_7": "心識為何等。",
    "T28n1557_001:0999a24_8": "心根相依法因知。",
    "T28n1557_001:0999a25_9": "痛為何等。",
    "T28n1557_001:0999a25_10": "為樂是亦為三輩少多無有量。",
    "T28n1557_001:0999a26_11": "想為何等。",
    "T28n1557_001:0999a26_12": "所對行為何等。",
    "T28n1557_001:0999a26_13": "所作是亦為三輩。",
    "T28n1557_001:0999a27_14": "善惡不分別福殃度。",
    "T28n1557_001:0999a27_15": "願樂為何等。",
    "T28n1557_001:0999a28_16": "三會是亦為三輩。",
    "T28n1557_001:0999a28_17": "善樂惡樂亦不善亦不惡樂意念。",
    "T28n1557_001:0999a29_18": "何等為意念。",
    "T28n1557_001:0999a29_19": "是亦為三輩。",
    "T28n1557_001:0999a29_20": "善惡不分別。",
    "T28n1557_001:0999b01_21": "欲為何等。",
    "T28n1557_001:0999b01_22": "欲作是何等。",
    "T28n1557_001:0999b01_23": "意可意為何等。",
    "T28n1557_001:0999b02_24": "念思惟何等。",
    "T28n1557_001:0999b02_25": "為一意。",
    "T28n1557_001:0999b02_26": "黠為何等。",
    "T28n1557_001:0999b02_27": "為觀法。",
    "T28n1557_001:0999b03_28": "信為何等。",
    "T28n1557_001:0999b03_29": "可意。",
    "T28n1557_001:0999b03_30": "進為何等。",
    "T28n1557_001:0999b03_31": "觀念。",
    "T28n1557_001:0999b03_32": "計為何等。",
    "T28n1557_001:0999b04_33": "所念使求增望念願。",
    "T28n1557_001:0999b04_34": "願是名為計。",
    "T28n1557_001:0999b04_35": "分別念為何等。",
    "T28n1557_001:0999b05_36": "所觀觀隨不絕相隨。",
    "T28n1557_001:0999b05_37": "是名為念。",
    "T28n1557_001:0999b06_38": "計念為何等。",
    "T28n1557_001:0999b06_39": "異意大為計。",
    "T28n1557_001:0999b06_40": "意微為念。",
    "T28n1557_001:0999b06_41": "計念是為異。",
    "T28n1557_001:0999b07_42": "貪為何等。",
    "T28n1557_001:0999b07_43": "不隨善法不信至誠不行不應行。",
    "T28n1557_001:0999b08_44": "是名為貪。",
    "T28n1557_001:0999b08_45": "不貪為何等。",
    "T28n1557_001:0999b08_46": "隨善法信至誠行應行。",
    "T28n1557_001:0999b09_47": "是名為不貪。",
    "T28n1557_001:0999b09_48": "善本何等。",
    "T28n1557_001:0999b09_49": "有三善本。",
    "T28n1557_001:0999b10_50": "無有貪善本。",
    "T28n1557_001:0999b10_51": "無有瞋恚善本。",
    "T28n1557_001:0999b10_52": "無有愚癡善本。",
    "T28n1557_001:0999b11_53": "是名為善本。",
    "T28n1557_001:0999b11_54": "不善本為何等。",
    "T28n1557_001:0999b11_55": "不善本有三。",
    "T28n1557_001:0999b12_56": "貪為不善本。",
    "T28n1557_001:0999b12_57": "瞋恚為不善本。",
    "T28n1557_001:0999b12_58": "愚癡為不善本。",
    "T28n1557_001:0999b13_59": "是名為不善本。",
    "T28n1557_001:0999b13_60": "不分別本為何等有五不分別。",
    "T28n1557_001:0999b14_61": "愛不分別。",
    "T28n1557_001:0999b14_62": "憍慢不分別。",
    "T28n1557_001:0999b14_63": "癡不分別。",
    "T28n1557_001:0999b14_64": "疑不分別。",
    "T28n1557_001:0999b15_65": "行是名為不分別本。",
    "T28n1557_001:0999b16_0": "結為何等。",
    "T28n1557_001:0999b16_1": "有九結。",
    "T28n1557_001:0999b16_2": "一為持念結。",
    "T28n1557_001:0999b16_3": "二為憎結。",
    "T28n1557_001:0999b17_0": "三為憍慢結。",
    "T28n1557_001:0999b17_1": "四為癡結。",
    "T28n1557_001:0999b17_2": "五為邪結。",
    "T28n1557_001:0999b17_3": "六為失願結。",
    "T28n1557_001:0999b18_4": "七為疑結。",
    "T28n1557_001:0999b18_5": "八為嫉結。",
    "T28n1557_001:0999b18_6": "九為慳結。",
    "T28n1557_001:0999b19_0": "持念結為何等。",
    "T28n1557_001:0999b19_1": "三界中貪。",
    "T28n1557_001:0999b19_2": "增結為何等。",
    "T28n1557_001:0999b19_3": "為人間不可息。",
    "T28n1557_001:0999b20_4": "憍慢結為何等。",
    "T28n1557_001:0999b20_5": "憍慢結名為七輩。",
    "T28n1557_001:0999b21_6": "何等七。",
    "T28n1557_001:0999b21_7": "一為憍慢。",
    "T28n1557_001:0999b21_8": "二為憍。",
    "T28n1557_001:0999b21_9": "三為自慢。",
    "T28n1557_001:0999b21_10": "四為自計慢。",
    "T28n1557_001:0999b22_11": "五為欺慢。",
    "T28n1557_001:0999b22_12": "六為不如慢。",
    "T28n1557_001:0999b22_13": "七為邪慢。",
    "T28n1557_001:0999b23_0": "憍慢為何等。",
    "T28n1557_001:0999b23_1": "不如者我為勝如者等。",
    "T28n1557_001:0999b23_2": "從是憍慢自計意起。",
    "T28n1557_001:0999b24_3": "意識合意。",
    "T28n1557_001:0999b24_4": "是名為憍慢。",
    "T28n1557_001:0999b24_5": "慢為何等輩中勝勝中等。",
    "T28n1557_001:0999b25_6": "從是所慢亦自計自計勝者勝。",
    "T28n1557_001:0999b26_7": "是名為自慢。",
    "T28n1557_001:0999b26_8": "亦說者憍慢慢當為一切合會。",
    "T28n1557_001:0999b27_9": "是名為自慢。",
    "T28n1557_001:0999b27_10": "自計慢為何等。",
    "T28n1557_001:0999b27_11": "為五陰自身是我身計自念。",
    "T28n1557_001:0999b28_12": "從是慢慢自知意生意起合意。",
    "T28n1557_001:0999b29_13": "是名為自計慢。",
    "T28n1557_001:0999b29_14": "欺慢為何等。",
    "T28n1557_001:0999b29_15": "未得計得未知計知未盡計盡。",
    "T28n1557_001:0999c01_16": "從是憍慢自計意觀意起合意。",
    "T28n1557_001:0999c02_17": "是名欺慢。",
    "T28n1557_001:0999c02_18": "不如慢為何等遠不如自計少不如。",
    "T28n1557_001:0999c03_19": "若豪若業。",
    "T28n1557_001:0999c03_20": "若業若何。",
    "T28n1557_001:0999c03_21": "若罪若病不及十倍百倍。",
    "T28n1557_001:0999c04_22": "自計如是為不如。",
    "T28n1557_001:0999c04_23": "從是憍慢自計自見意生意起合念。",
    "T28n1557_001:0999c05_24": "自為是名為不如慢。",
    "T28n1557_001:0999c06_25": "邪慢為何等。",
    "T28n1557_001:0999c06_26": "不賢者自計賢者。",
    "T28n1557_001:0999c06_27": "從是有憍慢自念自計。",
    "T28n1557_001:0999c07_28": "意生意起合念。",
    "T28n1557_001:0999c07_29": "是名為邪慢。",
    "T28n1557_001:0999c08_0": "是為七慢。",
    "T28n1557_001:0999c08_1": "慢名為憍慢結。",
    "T28n1557_001:0999c09_0": "癡結為何等。",
    "T28n1557_001:0999c09_1": "三界中所有癡。",
    "T28n1557_001:0999c09_2": "是名為癡結。",
    "T28n1557_001:0999c10_0": "邪結為何等。",
    "T28n1557_001:0999c10_1": "邪結有三輩名為邪結。",
    "T28n1557_001:0999c10_2": "一為身邪。",
    "T28n1557_001:0999c11_3": "二為邊邪。",
    "T28n1557_001:0999c11_4": "三為邪邪。",
    "T28n1557_001:0999c11_5": "身邪為何等。",
    "T28n1557_001:0999c11_6": "是身是我身。",
    "T28n1557_001:0999c12_7": "是名為身邪。",
    "T28n1557_001:0999c12_8": "邊邪為何等。",
    "T28n1557_001:0999c12_9": "一者斷滅。",
    "T28n1557_001:0999c12_10": "二者常在。",
    "T28n1557_001:0999c13_11": "是名為邊邪。",
    "T28n1557_001:0999c13_12": "邪邪為何等。",
    "T28n1557_001:0999c13_13": "邪邪為諍本壞福。",
    "T28n1557_001:0999c14_14": "是為三邪。",
    "T28n1557_001:0999c15_0": "失願結為何等。",
    "T28n1557_001:0999c15_1": "失本不受功挍恩。",
    "T28n1557_001:0999c15_2": "是名失願結。",
    "T28n1557_001:0999c16_3": "是兩失名為失願結。",
    "T28n1557_001:0999c16_4": "盜結為何等。",
    "T28n1557_001:0999c16_5": "兩盜名盜結。",
    "T28n1557_001:0999c17_6": "一為受盜。",
    "T28n1557_001:0999c17_7": "二為戒盜。",
    "T28n1557_001:0999c17_8": "受盜為何等。",
    "T28n1557_001:0999c17_9": "為五陰念尊大最無有極。",
    "T28n1557_001:0999c18_10": "從是所欲所意。",
    "T28n1557_001:0999c18_11": "所可所用。",
    "T28n1557_001:0999c19_12": "是名為盜結。",
    "T28n1557_001:0999c19_13": "盜戒為何等。",
    "T28n1557_001:0999c19_14": "從是淨從是離從是解從是要出用。",
    "T28n1557_001:0999c20_15": "是故所人所意所可所願。",
    "T28n1557_001:0999c21_16": "是名為盜戒。",
    "T28n1557_001:0999c21_17": "是兩盜名為盜結。",
    "T28n1557_001:0999c22_0": "疑結為何等。",
    "T28n1557_001:0999c22_1": "為疑四諦。",
    "T28n1557_001:0999c22_2": "是名為疑結。",
    "T28n1557_001:0999c22_3": "嫉結為何等。",
    "T28n1557_001:0999c23_4": "亂意為嫉結。",
    "T28n1557_001:0999c24_0": "慳結為何等。",
    "T28n1557_001:0999c24_1": "不能制意。",
    "T28n1557_001:0999c24_2": "是名為慳結。",
    "T28n1557_001:0999c24_3": "故一切結。",
    "T28n1557_001:0999c26_0": "縛者為何等。",
    "T28n1557_001:0999c26_1": "所結者名為縛故說縛。",
    "T28n1557_001:0999c27_0": "使者為何等。",
    "T28n1557_001:0999c27_1": "使者為七。",
    "T28n1557_001:0999c27_2": "何等為七。",
    "T28n1557_001:0999c27_3": "一為欲使。",
    "T28n1557_001:0999c28_4": "二為不可使。",
    "T28n1557_001:0999c28_5": "三為欲世間使。",
    "T28n1557_001:0999c28_6": "四為憍慢使。",
    "T28n1557_001:0999c29_0": "五為癡使。",
    "T28n1557_001:0999c29_1": "六為邪使。",
    "T28n1557_001:0999c29_2": "七為疑使。",
    "T28n1557_001:1000a01_0": "欲使為何等。",
    "T28n1557_001:1000a01_1": "欲使名為五使。",
    "T28n1557_001:1000a01_2": "何等為五者欲。",
    "T28n1557_001:1000a02_3": "從苦見斷欲。",
    "T28n1557_001:1000a02_4": "著欲從習見斷欲。",
    "T28n1557_001:1000a02_5": "著欲從盡見斷欲。",
    "T28n1557_001:1000a03_6": "著欲從道見斷欲。",
    "T28n1557_001:1000a03_7": "著欲從思惟見斷欲。",
    "T28n1557_001:1000a04_8": "是名為五使。",
    "T28n1557_001:1000a04_9": "名為欲使。",
    "T28n1557_001:1000a05_0": "不可使名為何等。",
    "T28n1557_001:1000a05_1": "五使名為不可使。",
    "T28n1557_001:1000a05_2": "何等為五。",
    "T28n1557_001:1000a06_3": "從苦見不可斷。",
    "T28n1557_001:1000a06_4": "從習見不可斷。",
    "T28n1557_001:1000a06_5": "從盡見不可斷。",
    "T28n1557_001:1000a07_6": "從道見不可斷。",
    "T28n1557_001:1000a07_7": "從思惟見不可斷。",
    "T28n1557_001:1000a07_8": "是五使名為不可使。",
    "T28n1557_001:1000a08_9": "世間欲可使為何等。",
    "T28n1557_001:1000a08_10": "十使名為世間可欲使。",
    "T28n1557_001:1000a09_11": "何等為十。",
    "T28n1557_001:1000a09_12": "著色見苦斷欲。",
    "T28n1557_001:1000a09_13": "著色見習斷欲。",
    "T28n1557_001:1000a10_14": "著色見盡斷欲。",
    "T28n1557_001:1000a10_15": "著色見行道斷欲。",
    "T28n1557_001:1000a11_16": "從色因著思惟斷欲。",
    "T28n1557_001:1000a11_17": "從無有色因著見苦。",
    "T28n1557_001:1000a12_0": "斷欲。",
    "T28n1557_001:1000a12_1": "從無有色因著見習斷欲。",
    "T28n1557_001:1000a12_2": "從無有色因著見盡斷欲。",
    "T28n1557_001:1000a13_3": "從無有色因著見道斷欲。",
    "T28n1557_001:1000a13_4": "從無有色因著思惟斷欲。",
    "T28n1557_001:1000a14_5": "是十使名為欲世間使。",
    "T28n1557_001:1000a15_0": "憍慢使為何等。",
    "T28n1557_001:1000a15_1": "憍慢使有十五。",
    "T28n1557_001:1000a15_2": "何等為十五。",
    "T28n1557_001:1000a16_0": "著欲見苦憍慢斷。",
    "T28n1557_001:1000a16_1": "著欲見習憍慢斷。",
    "T28n1557_001:1000a16_2": "著欲見盡憍慢斷。",
    "T28n1557_001:1000a17_3": "著欲見道憍慢斷。",
    "T28n1557_001:1000a17_4": "著欲思惟見憍慢斷著色見苦憍慢斷。",
    "T28n1557_001:1000a18_5": "著色見習憍慢斷。",
    "T28n1557_001:1000a18_6": "著色見盡憍慢斷。",
    "T28n1557_001:1000a19_7": "著色見道憍慢斷。",
    "T28n1557_001:1000a19_8": "著色思惟憍慢斷。",
    "T28n1557_001:1000a20_9": "著無有色見苦憍慢斷。",
    "T28n1557_001:1000a20_10": "著無有色見習憍慢斷。",
    "T28n1557_001:1000a21_11": "著無有色見盡憍慢斷。",
    "T28n1557_001:1000a21_12": "著無有色見道憍慢斷。",
    "T28n1557_001:1000a22_13": "著無有色思惟憍慢斷。",
    "T28n1557_001:1000a22_14": "是十五使名為憍慢使。",
    "T28n1557_001:1000a24_0": "癡使為何等。",
    "T28n1557_001:1000a24_1": "十五使名為癡使。",
    "T28n1557_001:1000a24_2": "何等為十五。",
    "T28n1557_001:1000a25_0": "著欲見苦癡斷。",
    "T28n1557_001:1000a25_1": "著欲見習癡斷。",
    "T28n1557_001:1000a25_2": "著欲見盡癡斷。",
    "T28n1557_001:1000a26_3": "著欲見道癡斷。",
    "T28n1557_001:1000a26_4": "著欲思惟癡斷。",
    "T28n1557_001:1000a26_5": "著色見苦癡斷。",
    "T28n1557_001:1000a27_6": "著色見習癡斷。",
    "T28n1557_001:1000a27_7": "著色見盡癡斷。",
    "T28n1557_001:1000a27_8": "著色見道癡斷。",
    "T28n1557_001:1000a28_9": "著色思惟癡斷。",
    "T28n1557_001:1000a28_10": "著無有色見苦癡斷。",
    "T28n1557_001:1000a29_11": "著無有色見習癡斷。",
    "T28n1557_001:1000a29_12": "著無有色見盡癡斷。",
    "T28n1557_001:1000b01_0": "著無有色見道癡斷。",
    "T28n1557_001:1000b01_1": "著無有色思惟癡斷。",
    "T28n1557_001:1000b01_2": "是十五使名為癡使。",
    "T28n1557_001:1000b03_0": "邪使為何等。",
    "T28n1557_001:1000b03_1": "三十六使為邪使。",
    "T28n1557_001:1000b03_2": "何等為三十六。",
    "T28n1557_001:1000b04_3": "十二使著欲。",
    "T28n1557_001:1000b04_4": "十二使著色。",
    "T28n1557_001:1000b04_5": "十二使著無有色。",
    "T28n1557_001:1000b05_6": "是名為三十六使。",
    "T28n1557_001:1000b05_7": "十二使著欲為何等。",
    "T28n1557_001:1000b05_8": "著欲見苦斷身邪。",
    "T28n1557_001:1000b06_9": "著欲見苦斷要邪。",
    "T28n1557_001:1000b06_10": "著欲見苦斷邪邪。",
    "T28n1557_001:1000b07_11": "著欲見習斷邪邪。",
    "T28n1557_001:1000b07_12": "著欲見盡斷邪邪。",
    "T28n1557_001:1000b08_0": "著欲見道斷邪邪。",
    "T28n1557_001:1000b08_1": "著欲見苦斷見盜。",
    "T28n1557_001:1000b08_2": "著欲見習斷見盜。",
    "T28n1557_001:1000b09_3": "著欲見盡斷見盜。",
    "T28n1557_001:1000b09_4": "著欲見道斷見盜。",
    "T28n1557_001:1000b10_5": "著欲見苦斷戒盜。",
    "T28n1557_001:1000b10_6": "著欲見道斷戒盜。",
    "T28n1557_001:1000b10_7": "是名為十二使著欲使。",
    "T28n1557_001:1000b12_0": "十二使著色為何等。",
    "T28n1557_001:1000b12_1": "著色見苦斷身邪。",
    "T28n1557_001:1000b12_2": "著色見苦斷要邪。",
    "T28n1557_001:1000b13_3": "著色見苦斷邪邪。",
    "T28n1557_001:1000b13_4": "著色見習斷邪邪。",
    "T28n1557_001:1000b14_5": "著色見盡斷邪邪。",
    "T28n1557_001:1000b14_6": "著色見道斷邪邪。",
    "T28n1557_001:1000b14_7": "著色見苦斷見盜。",
    "T28n1557_001:1000b15_8": "著色見習斷見盜。",
    "T28n1557_001:1000b15_9": "著色見盡斷見盜。",
    "T28n1557_001:1000b16_10": "著色見道斷見盜。",
    "T28n1557_001:1000b16_11": "著色見苦斷戒盜。",
    "T28n1557_001:1000b17_0": "著色見道斷戒盜。",
    "T28n1557_001:1000b17_1": "是名為十二使。",
    "T28n1557_001:1000b17_2": "著色使。",
    "T28n1557_001:1000b18_0": "十二使。",
    "T28n1557_001:1000b18_1": "著無有色為何等。",
    "T28n1557_001:1000b18_2": "著無有色見苦斷身邪。",
    "T28n1557_001:1000b19_3": "著無有色見苦斷要邪。",
    "T28n1557_001:1000b19_4": "著無有色見苦斷邪邪。",
    "T28n1557_001:1000b20_5": "著無有色見習斷邪邪。",
    "T28n1557_001:1000b20_6": "著無有色見習斷邪邪。",
    "T28n1557_001:1000b21_7": "著無有色見道斷邪邪。",
    "T28n1557_001:1000b21_8": "著無有色見苦斷見盜。",
    "T28n1557_001:1000b22_9": "著無有色見習斷見盜。",
    "T28n1557_001:1000b22_10": "著無有色見盡斷見盜。",
    "T28n1557_001:1000b23_11": "著無有色見道斷見盜。",
    "T28n1557_001:1000b23_12": "著無有色見苦斷戒盜。",
    "T28n1557_001:1000b24_13": "著無有色見道斷戒盜。",
    "T28n1557_001:1000b25_0": "是名為十二使著無有色使。",
    "T28n1557_001:1000b25_1": "是為三十六使。",
    "T28n1557_001:1000b26_0": "名為三十六邪使。",
    "T28n1557_001:1000b27_0": "疑使為何等。",
    "T28n1557_001:1000b27_1": "十二使名為疑使。",
    "T28n1557_001:1000b27_2": "何等十二。",
    "T28n1557_001:1000b27_3": "著欲見苦斷疑。",
    "T28n1557_001:1000b28_4": "著欲見習斷疑。",
    "T28n1557_001:1000b28_5": "著欲見盡斷疑。",
    "T28n1557_001:1000b29_0": "著欲見道斷疑。",
    "T28n1557_001:1000b29_1": "著色見苦斷疑。",
    "T28n1557_001:1000b29_2": "著色見習斷疑。",
    "T28n1557_001:1000c01_3": "著色見盡斷疑。",
    "T28n1557_001:1000c01_4": "著色見道斷疑。",
    "T28n1557_001:1000c01_5": "著無有色見苦斷疑。",
    "T28n1557_001:1000c02_6": "著無有色見習斷疑。",
    "T28n1557_001:1000c02_7": "著無有色見盡斷疑。",
    "T28n1557_001:1000c03_8": "著無有色見道斷疑。",
    "T28n1557_001:1000c03_9": "是名為十二疑。",
    "T28n1557_001:1000c04_10": "使是為塵是為塵腦。",
    "T28n1557_001:1000c04_11": "有時塵無有腦者。",
    "T28n1557_001:1000c05_0": "除塵所餘亂意念法。",
    "T28n1557_001:1000c05_1": "是為腦非塵。",
    "T28n1557_001:1000c05_2": "從起為八。",
    "T28n1557_001:1000c06_3": "一為睡。",
    "T28n1557_001:1000c06_4": "二為瞑。",
    "T28n1557_001:1000c06_5": "三為樂。",
    "T28n1557_001:1000c06_6": "四為疑。",
    "T28n1557_001:1000c06_7": "五為猗。",
    "T28n1557_001:1000c07_8": "六為恣態。",
    "T28n1557_001:1000c07_9": "七為不愧。",
    "T28n1557_001:1000c07_10": "八為不慚。",
    "T28n1557_001:1000c07_11": "是故說從起八。",
    "T28n1557_001:1000c09_0": "所黠為何等。",
    "T28n1557_001:1000c09_1": "十黠。",
    "T28n1557_001:1000c09_2": "何等為十。",
    "T28n1557_001:1000c09_3": "一為法黠。",
    "T28n1557_001:1000c09_4": "二為比黠。",
    "T28n1557_001:1000c10_5": "三為知人心黠。",
    "T28n1557_001:1000c10_6": "四為巧黠。",
    "T28n1557_001:1000c10_7": "五為苦黠。",
    "T28n1557_001:1000c11_8": "六為習黠。",
    "T28n1557_001:1000c11_9": "七為滅黠。",
    "T28n1557_001:1000c11_10": "八為道黠。",
    "T28n1557_001:1000c11_11": "九為盡黠。",
    "T28n1557_001:1000c12_12": "十為無為黠。",
    "T28n1557_001:1000c13_0": "法黠為何等。",
    "T28n1557_001:1000c13_1": "在生死欲所無有結黠。",
    "T28n1557_001:1000c13_2": "在生死欲本所無有結黠在。",
    "T28n1557_001:1000c14_3": "生死欲滅無有結黠。",
    "T28n1557_001:1000c14_4": "在生死欲壞道行無有結黠。",
    "T28n1557_001:1000c15_5": "亦在法黠亦在法地所無有結黠。",
    "T28n1557_001:1000c16_6": "是名為法黠。",
    "T28n1557_001:1000c17_0": "比黠為何等。",
    "T28n1557_001:1000c17_1": "在色無有色行無有結黠。",
    "T28n1557_001:1000c17_2": "在色無有色本無有結黠。",
    "T28n1557_001:1000c18_3": "在色無有色行滅無有結黠。",
    "T28n1557_001:1000c19_4": "在色無有色行斷為道無有結黠。",
    "T28n1557_001:1000c19_5": "亦在比黠亦在比地無有結黠。",
    "T28n1557_001:1000c20_6": "是名為比黠。",
    "T28n1557_001:1000c21_0": "知人心黠為何等。",
    "T28n1557_001:1000c21_1": "所黠行所黠福所黠合。",
    "T28n1557_001:1000c21_2": "已得不舍。",
    "T28n1557_001:1000c22_3": "常在前常念不忘。",
    "T28n1557_001:1000c22_4": "為人故為他眾故恩行。",
    "T28n1557_001:1000c23_5": "是故意念知。",
    "T28n1557_001:1000c23_6": "是名為知人心黠。",
    "T28n1557_001:1000c24_0": "巧黠為何等。",
    "T28n1557_001:1000c24_1": "世間所行黠。",
    "T28n1557_001:1000c24_2": "是名巧黠。",
    "T28n1557_001:1000c24_3": "苦黠為何等。",
    "T28n1557_001:1000c25_4": "受五陰非常苦空非身念所無有結黠。",
    "T28n1557_001:1000c26_5": "是名為苦黠。",
    "T28n1557_001:1000c27_0": "習黠為何等。",
    "T28n1557_001:1000c27_1": "世間本亦本集生因緣思念無有結黠。",
    "T28n1557_001:1000c28_2": "是名為習黠。",
    "T28n1557_001:1000c29_0": "滅黠為何等。",
    "T28n1557_001:1000c29_1": "滅滅為黠。",
    "T28n1557_001:1000c29_2": "最要念不結黠。",
    "T28n1557_001:1000c29_3": "是名為滅黠。",
    "T28n1557_001:1001a02_0": "道黠為何等。",
    "T28n1557_001:1001a02_1": "道為道如應受觀者。",
    "T28n1557_001:1001a02_2": "欲出念無有結黠。",
    "T28n1557_001:1001a03_3": "是名為道黠。",
    "T28n1557_001:1001a04_0": "盡黠為何等。",
    "T28n1557_001:1001a04_1": "已識苦已舍習盡已有證。",
    "T28n1557_001:1001a04_2": "道已行從是黠見知意得應。",
    "T28n1557_001:1001a05_3": "是名為盡黠。",
    "T28n1557_001:1001a06_0": "無為黠為何等。",
    "T28n1557_001:1001a06_1": "苦已更不復更。",
    "T28n1557_001:1001a06_2": "習已畢不復畢。",
    "T28n1557_001:1001a07_3": "盡已有證不復用證。",
    "T28n1557_001:1001a07_4": "道已行不復行。",
    "T28n1557_001:1001a07_5": "從是所黠所見所知所意得。",
    "T28n1557_001:1001a08_6": "是名為無為黠。",
    "T28n1557_001:1001a08_7": "故說所黠。",
    "T28n1557_001:1001a10_0": "所有見為何等。",
    "T28n1557_001:1001a10_1": "所有黠見為見。",
    "T28n1557_001:1001a10_2": "有時見非黠。",
    "T28n1557_001:1001a11_3": "為何等。",
    "T28n1557_001:1001a11_4": "八更者可。",
    "T28n1557_001:1001a11_5": "八更者可為何等。",
    "T28n1557_001:1001a11_6": "苦法黠可苦譬黠可。",
    "T28n1557_001:1001a12_7": "習法黠可習譬黠可。",
    "T28n1557_001:1001a12_8": "盡法黠可盡譬黠可。",
    "T28n1557_001:1001a13_9": "道法黠可道譬黠可。",
    "T28n1557_001:1001a13_10": "故說所見。",
    "T28n1557_001:1001a14_11": "若得是為黠。",
    "T28n1557_001:1001a14_12": "不有時得。",
    "T28n1557_001:1001a14_13": "非黠八更可如上說。",
    "T28n1557_001:1001a15_14": "故說所更。",
    "T28n1557_001:1001a16_0": "德為何等。",
    "T28n1557_001:1001a16_1": "得法為德。",
    "T28n1557_001:1001a16_2": "無有思想思惟為何等。",
    "T28n1557_001:1001a17_3": "天上一處名為一切淨。",
    "T28n1557_001:1001a17_4": "在有無有欲前。",
    "T28n1557_001:1001a17_5": "有思想出所意念法滅不隨。",
    "T28n1557_001:1001a18_6": "是名為不思想思惟。",
    "T28n1557_001:1001a19_7": "滅思惟為何等。",
    "T28n1557_001:1001a19_8": "二十六天上名為不欲。",
    "T28n1557_001:1001a20_0": "中得道者。",
    "T28n1557_001:1001a20_1": "上頭行要出所意念法滅倒。",
    "T28n1557_001:1001a20_2": "是名為滅思惟不思想為何等。",
    "T28n1557_001:1001a21_3": "無有思想人化生天上。",
    "T28n1557_001:1001a22_4": "上頭意亦墮天上時意除是中間。",
    "T28n1557_001:1001a22_5": "乃從是若意念法滅倒。",
    "T28n1557_001:1001a23_6": "是名為不思想。",
    "T28n1557_001:1001a24_0": "念根為何等。",
    "T28n1557_001:1001a24_1": "三界中命會為何等。",
    "T28n1557_001:1001a24_2": "人同居得。",
    "T28n1557_001:1001a25_3": "處為何等。",
    "T28n1557_001:1001a25_4": "同郡縣。",
    "T28n1557_001:1001a25_5": "種得為何等。",
    "T28n1557_001:1001a25_6": "為五陰。",
    "T28n1557_001:1001a26_7": "入得為何等。",
    "T28n1557_001:1001a26_8": "所內外得入。",
    "T28n1557_001:1001a26_9": "是名為入得。",
    "T28n1557_001:1001a27_0": "生為何等。",
    "T28n1557_001:1001a27_1": "得陰。",
    "T28n1557_001:1001a27_2": "老為何等。",
    "T28n1557_001:1001a27_3": "陰熟。",
    "T28n1557_001:1001a27_4": "止行何等。",
    "T28n1557_001:1001a28_5": "宿命行來望。",
    "T28n1557_001:1001a28_6": "非常為何等。",
    "T28n1557_001:1001a28_7": "已生復亡。",
    "T28n1557_001:1001a28_8": "名字為何等。",
    "T28n1557_001:1001a29_9": "知分別。",
    "T28n1557_001:1001a29_10": "絕為何等。",
    "T28n1557_001:1001a29_11": "字為具。",
    "T28n1557_001:1001a29_12": "政用為何等。",
    "T28n1557_001:1001b01_13": "字會。",
    "T28n1557_001:1001b02_0": "空為何等。",
    "T28n1557_001:1001b02_1": "虛空無所有無所著無所色。",
    "T28n1557_001:1001b02_2": "是名為空。",
    "T28n1557_001:1001b03_3": "盡尚未離為何等。",
    "T28n1557_001:1001b03_4": "已盡不復更不復著。",
    "T28n1557_001:1001b04_5": "盡為何等。",
    "T28n1557_001:1001b04_6": "度世無為。",
    "T28n1557_001:1001b04_7": "是名為五法五行行說具。",
    "T28n1557_001:1001b06_0": "阿毘曇五法行經"
}