[
    {
        "source_sentence": "寶藏天女陀羅尼法",
        "from": "T21n1282_001:0342c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時三十三天主摩醯首羅天王。共諸天女",
        "from": "T21n1282_001:0342c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾讚寶藏天王名吒羅佉。有大威德色力無",
        "from": "T21n1282_001:0342c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比能於世間行殊勝事。",
        "from": "T21n1282_001:0342c22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迴天動地傾山覆海。",
        "from": "T21n1282_001:0342c22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兵戈勝負志意而成。",
        "from": "T21n1282_001:0342c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "財帛金寶積如山岳。",
        "from": "T21n1282_001:0342c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間受用。",
        "from": "T21n1282_001:0342c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1282_001:0342c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵吒羅佉吒羅佉毘多羅儞莎嚩訶若欲受持此法。",
        "from": "T21n1282_001:0342c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先畫一天女形。",
        "from": "T21n1282_001:0342c26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "泥檀法五月五日前七日。",
        "from": "T21n1282_001:0342c27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喫希粥長齋。",
        "from": "T21n1282_001:0342c27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "意欲去處須淨潔也。",
        "from": "T21n1282_001:0342c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡行此法天女不喚自來。",
        "from": "T21n1282_001:0342c28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然須思存用表至誠。",
        "from": "T21n1282_001:0342c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此法必須五月五日夜受持即果成就。",
        "from": "T21n1282_001:0342c29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五月五日夜。",
        "from": "T21n1282_001:0343a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "注想思存換換誦呪一千八遍。",
        "from": "T21n1282_001:0343a01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得急。",
        "from": "T21n1282_001:0343a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至三更即心昏似睡。",
        "from": "T21n1282_001:0343a02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天女使諸女來令相試弄。",
        "from": "T21n1282_001:0343a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必莫共語。",
        "from": "T21n1282_001:0343a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至第二遍。",
        "from": "T21n1282_001:0343a03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其天女吒羅佉自來相見。",
        "from": "T21n1282_001:0343a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其作法人不得赴。",
        "from": "T21n1282_001:0343a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便䠒跪合掌頂禮兩拜。",
        "from": "T21n1282_001:0343a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即相問之有何所須。",
        "from": "T21n1282_001:0343a05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其作法人隨口答之。",
        "from": "T21n1282_001:0343a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乞好藥長生不死通靈。",
        "from": "T21n1282_001:0343a06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間萬事之情。",
        "from": "T21n1282_001:0343a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終身奉事。",
        "from": "T21n1282_001:0343a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天女意生欲為夫妻。",
        "from": "T21n1282_001:0343a07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天女聞已甚大歡喜。",
        "from": "T21n1282_001:0343a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當夜便與交諸願如意。",
        "from": "T21n1282_001:0343a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲作姊妹。",
        "from": "T21n1282_001:0343a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於事必須分明勿生狐疑。",
        "from": "T21n1282_001:0343a09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其壇法二肘半量用牛糞泥地。",
        "from": "T21n1282_001:0343a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更以香泥塗地。",
        "from": "T21n1282_001:0343a10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五色彩畫又五色綵各二尺一寸。",
        "from": "T21n1282_001:0343a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在壇內安天女座。",
        "from": "T21n1282_001:0343a11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酥密乳等菓子各一盤。",
        "from": "T21n1282_001:0343a12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五色花餅各五枚。",
        "from": "T21n1282_001:0343a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已雜栗麥燒花隨意供養。",
        "from": "T21n1282_001:0343a13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其畫像法天女身長二尺五寸。",
        "from": "T21n1282_001:0343a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭作花冠所點花極妙端正。",
        "from": "T21n1282_001:0343a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身著紫袍金帶烏靴。",
        "from": "T21n1282_001:0343a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手把蓮花。",
        "from": "T21n1282_001:0343a15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手把如意寶珠。",
        "from": "T21n1282_001:0343a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其天女端正光明。",
        "from": "T21n1282_001:0343a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間畫師無能畫者。",
        "from": "T21n1282_001:0343a16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必須好手不得論價。",
        "from": "T21n1282_001:0343a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應時通靈心神自在。",
        "from": "T21n1282_001:0343a17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "財帛盈溢官位高遷。",
        "from": "T21n1282_001:0343a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中一切禍福了達。",
        "from": "T21n1282_001:0343a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貴賤男女皆悉歡喜。",
        "from": "T21n1282_001:0343a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡有所動一切鬼神皆來迎待如敬天官。",
        "from": "T21n1282_001:0343a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悚懼瞻仰便得無礙。",
        "from": "T21n1282_001:0343a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至世間好妙藝隨意而成。",
        "from": "T21n1282_001:0343a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有惡來相害者。",
        "from": "T21n1282_001:0343a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便呼賊各皆得自縛自打。",
        "from": "T21n1282_001:0343a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言放即放世間走失死亡如驗。",
        "from": "T21n1282_001:0343a22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目前萬病舉意持之皆差。",
        "from": "T21n1282_001:0343a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又八真言各須清淨沐浴著新淨衣。",
        "from": "T21n1282_001:0343a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於佛前形像前各誦三七遍。",
        "from": "T21n1282_001:0343a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後作法。",
        "from": "T21n1282_001:0343a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一真言唵興婆羅帝吒急急娑嚩二合賀第二真言唵拔羅帝吒唎摩那羅娑嚩賀第三真言唵勒叉那帝吒娑吒娑嚩二合賀第四真言唵婆婆隷若帝吒娑嚩二合賀第五真言唵勃惹羅帝吒娑嚩二合賀第六真言唵藥叉藥叉底那婆娑嚩二合賀第七真言唵遮帝毘折曳吽泮吒娑嚩二合賀第八真言唵折勒那耶利婆帝吒娑嚩二合賀以中指無名指屈。",
        "from": "T21n1282_001:0343b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大母指押二指甲。",
        "from": "T21n1282_001:0343b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舒頭指及小指安心上。",
        "from": "T21n1282_001:0343b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手把數珠念真言。",
        "from": "T21n1282_001:0343b14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此名玉環印。",
        "from": "T21n1282_001:0343b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶藏天女法竟",
        "from": "T21n1282_001:0343b16_100",
        "context_with_next_sentence": false
    }
]