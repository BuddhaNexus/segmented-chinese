[
    {
        "source_sentence": "千眼千臂觀世音菩薩陀羅尼神",
        "from": "T20n1057b_002:0094a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪經卷下",
        "from": "T20n1057b_002:0094a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐總持寺三藏沙門智通譯",
        "from": "T20n1057b_002:0094a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時觀世音菩薩。",
        "from": "T20n1057b_002:0094a29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是呪時三千大千世界。",
        "from": "T20n1057b_002:0094a29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至非想非非想天六反震動。",
        "from": "T20n1057b_002:0094b01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色究竟天摩醯首羅戰慄不安皆大恐懼。",
        "from": "T20n1057b_002:0094b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切惡鬼皆大叫喚受大苦惱。",
        "from": "T20n1057b_002:0094b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東西散走莫知所趣。",
        "from": "T20n1057b_002:0094b03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時化身語諸大眾及諸惡鬼神等。",
        "from": "T20n1057b_002:0094b04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不隨順我呪者。",
        "from": "T20n1057b_002:0094b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "違逆者頭破粉碎。",
        "from": "T20n1057b_002:0094b05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此呪能摧碎諸山乾竭大海。",
        "from": "T20n1057b_002:0094b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此呪能摧伏阿脩羅軍護諸國土。",
        "from": "T20n1057b_002:0094b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此呪能摧伏一切諸惡鬼神。",
        "from": "T20n1057b_002:0094b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸病一切惡毒。",
        "from": "T20n1057b_002:0094b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切惡人。",
        "from": "T20n1057b_002:0094b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此呪能摧破三十三天皆令降伏。",
        "from": "T20n1057b_002:0094b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子有能誦持此呪者。",
        "from": "T20n1057b_002:0094b09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人威力說不可盡。",
        "from": "T20n1057b_002:0094b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此呪能令誦持之者豪貴自在。",
        "from": "T20n1057b_002:0094b10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能令國王終身愛念。",
        "from": "T20n1057b_002:0094b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱意所求悉皆滿足。",
        "from": "T20n1057b_002:0094b11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲降伏魔怨者。",
        "from": "T20n1057b_002:0094b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當燒求羅香誦我身呪二十一遍。",
        "from": "T20n1057b_002:0094b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲令一切人愛己者。",
        "from": "T20n1057b_002:0094b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪楊枝二十一遍。",
        "from": "T20n1057b_002:0094b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口中嚼之即得愛敬。",
        "from": "T20n1057b_002:0094b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲令自身辯才智慧者。",
        "from": "T20n1057b_002:0094b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪菖蒲一千八遍。",
        "from": "T20n1057b_002:0094b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗其心上即得辯才無礙。",
        "from": "T20n1057b_002:0094b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作姥陀羅尼心印。",
        "from": "T20n1057b_002:0094b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "13 辯才無礙印咒辯才無礙印呪第十三以兩手相背合掌。",
        "from": "T20n1057b_002:0094b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大母指向前舒。",
        "from": "T20n1057b_002:0094b18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此印能自護護他。",
        "from": "T20n1057b_002:0094b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當須結界隨所遊方。",
        "from": "T20n1057b_002:0094b19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或呪淨水或呪淨灰各呪七遍。",
        "from": "T20n1057b_002:0094b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所在住處以手掬水掬灰先灑自身。",
        "from": "T20n1057b_002:0094b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後向於四方四角如法散灑。",
        "from": "T20n1057b_002:0094b21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人。",
        "from": "T20n1057b_002:0094b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被諸惡鬼眾邪魍魎之所惑亂者。",
        "from": "T20n1057b_002:0094b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取石榴枝及柳枝。",
        "from": "T20n1057b_002:0094b23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陰誦此呪輕打病人無病不差。",
        "from": "T20n1057b_002:0094b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T20n1057b_002:0094b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無薩婆佛陀達摩僧祇比耶　南無阿利耶婆盧吉低攝伐羅𡃂　菩提薩多跛寫南無跋折羅波尼寫菩提薩多波寫　跢姪他　徒比徒比迦耶　徒比婆羅闍婆羅尼馺皤訶此呪印能降伏諸邪見外道。",
        "from": "T20n1057b_002:0094c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人。",
        "from": "T20n1057b_002:0094c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於晨朝時日三時。",
        "from": "T20n1057b_002:0094c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各誦一遍者。",
        "from": "T20n1057b_002:0094c02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即與種種供養十億諸佛無有異也。",
        "from": "T20n1057b_002:0094c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永不受女身。",
        "from": "T20n1057b_002:0094c03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命終之後永離三塗。",
        "from": "T20n1057b_002:0094c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得往生阿彌陀佛國。",
        "from": "T20n1057b_002:0094c04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來授手摩頂。",
        "from": "T20n1057b_002:0094c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝莫怖懼來生我國。",
        "from": "T20n1057b_002:0094c05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現身不被橫死。",
        "from": "T20n1057b_002:0094c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為鬼神之所得便。",
        "from": "T20n1057b_002:0094c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "14 破三千大三世界滅罪印破三千大三世界滅罪印第十四起立以左手向前展臂。",
        "from": "T20n1057b_002:0094c08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五指向前散托竪五指。",
        "from": "T20n1057b_002:0094c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以右手大母指屈在掌中。",
        "from": "T20n1057b_002:0094c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以四指把拳當右耳上當。",
        "from": "T20n1057b_002:0094c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦身呪頭指來去。",
        "from": "T20n1057b_002:0094c10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此印日別須三時一時誦七遍。",
        "from": "T20n1057b_002:0094c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能滅五逆四重罪。",
        "from": "T20n1057b_002:0094c11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切眾生生慈慈心。",
        "from": "T20n1057b_002:0094c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即能燒一切罪根。",
        "from": "T20n1057b_002:0094c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此身滅後即為值佛。",
        "from": "T20n1057b_002:0094c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彼佛土得作轉輪聖王。",
        "from": "T20n1057b_002:0094c13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復得陀羅尼名曰無盡藏。",
        "from": "T20n1057b_002:0094c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復得三昧名智等。",
        "from": "T20n1057b_002:0094c14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復得身中二十八種相。",
        "from": "T20n1057b_002:0094c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現現身不患眼舌耳鼻等病。",
        "from": "T20n1057b_002:0094c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至身中一切疾病悉能消滅。",
        "from": "T20n1057b_002:0094c16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有先業罪者亦得消滅。",
        "from": "T20n1057b_002:0094c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見天旱時。",
        "from": "T20n1057b_002:0094c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取烏麻子和毘麻子脂。",
        "from": "T20n1057b_002:0094c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作丸呪一百八遍。",
        "from": "T20n1057b_002:0094c18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擲著湫水中即得雨。",
        "from": "T20n1057b_002:0094c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若雨過多。",
        "from": "T20n1057b_002:0094c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取稻穀炒作華。",
        "from": "T20n1057b_002:0094c19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取蔓菁子脂和作丸。",
        "from": "T20n1057b_002:0094c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪之一百八遍。",
        "from": "T20n1057b_002:0094c20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擲著湫水中雨即止也。",
        "from": "T20n1057b_002:0094c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "15 降伏三千大千世界魔怨印降伏三千大千世界魔怨印第十五以五指相叉。",
        "from": "T20n1057b_002:0094c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左壓右急把拳。",
        "from": "T20n1057b_002:0094c23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當頂上著。",
        "from": "T20n1057b_002:0094c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦身呪即降伏。",
        "from": "T20n1057b_002:0095a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作此法。",
        "from": "T20n1057b_002:0095a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向舍利塔前二十九日夜。",
        "from": "T20n1057b_002:0095a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取白檀香作末。",
        "from": "T20n1057b_002:0095a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗地作曼荼羅。",
        "from": "T20n1057b_002:0095a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其中散種種華。",
        "from": "T20n1057b_002:0095a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "澡浴清淨著新淨衣。",
        "from": "T20n1057b_002:0095a03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手把香鑪燒沈水香。",
        "from": "T20n1057b_002:0095a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面向東坐呪一千八遍。",
        "from": "T20n1057b_002:0095a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是最初功能。",
        "from": "T20n1057b_002:0095a04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又取芥子烏麻。",
        "from": "T20n1057b_002:0095a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著一處擣為末。",
        "from": "T20n1057b_002:0095a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以三指撮取少許。",
        "from": "T20n1057b_002:0095a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪之一遍擲著火中。",
        "from": "T20n1057b_002:0095a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是七日日別誦一百八遍。",
        "from": "T20n1057b_002:0095a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後所作皆悉成就。",
        "from": "T20n1057b_002:0095a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "16 廣大無畏印廣大無畏印第十六起立並足。",
        "from": "T20n1057b_002:0095a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以右手仰垂左肘膝頭。",
        "from": "T20n1057b_002:0095a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手亦如之。",
        "from": "T20n1057b_002:0095a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於舍利像前。",
        "from": "T20n1057b_002:0095a10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦身呪一百八遍。",
        "from": "T20n1057b_002:0095a10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得無畏施於眾生。",
        "from": "T20n1057b_002:0095a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又取槐香白芥子菖蒲捨多婆利外國藥名以此等物內火中燒之。",
        "from": "T20n1057b_002:0095a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒火之時應於佛前或在淨處。",
        "from": "T20n1057b_002:0095a13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦呪三十二遍。",
        "from": "T20n1057b_002:0095a13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以香華供養。",
        "from": "T20n1057b_002:0095a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪法悉皆成就。",
        "from": "T20n1057b_002:0095a14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所為之者皆悉剋果。",
        "from": "T20n1057b_002:0095a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若餘呪無驗。",
        "from": "T20n1057b_002:0095a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此呪呪之亦皆成就。",
        "from": "T20n1057b_002:0095a15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲乞夢。",
        "from": "T20n1057b_002:0095a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此呪并作印眠。",
        "from": "T20n1057b_002:0095a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即有夢隨所欲見皆得見之。",
        "from": "T20n1057b_002:0095a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人無福所向不諧者。",
        "from": "T20n1057b_002:0095a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日誦三遍呪滿七日。",
        "from": "T20n1057b_002:0095a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸有所求一切皆得。",
        "from": "T20n1057b_002:0095a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時菩薩在娑竭羅龍宮海會說法。",
        "from": "T20n1057b_002:0095a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見諸龍眾受大苦惱。",
        "from": "T20n1057b_002:0095a19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愍諸龍眾為度苦惱眾生。",
        "from": "T20n1057b_002:0095a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉得離苦無諸怨害。",
        "from": "T20n1057b_002:0095a20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時龍女獻一寶珠。",
        "from": "T20n1057b_002:0095a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "價直娑婆世界。",
        "from": "T20n1057b_002:0095a21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為求法故吾為廣說離諸苦難。",
        "from": "T20n1057b_002:0095a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "17 水精菩薩護持千眼印咒水精菩薩護持千眼印呪第十七爾時水精菩薩。",
        "from": "T20n1057b_002:0095a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲利益護持此呪。",
        "from": "T20n1057b_002:0095a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而說呪曰。",
        "from": "T20n1057b_002:0095a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘摩隸　摩訶毘摩隸　郁呵隸　摩訶郁呵隷　休摩隷　摩訶休摩隷　薩訶隷　止隷睇　馺婆訶若有善男子善女人。",
        "from": "T20n1057b_002:0095a29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在所遊方受持此千眼千臂菩薩法者。",
        "from": "T20n1057b_002:0095b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當常隨衛護。",
        "from": "T20n1057b_002:0095b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至諸魔眷屬無惱亂者。",
        "from": "T20n1057b_002:0095b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人急難他國相侵盜賊逆亂。",
        "from": "T20n1057b_002:0095b02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當取五色縷。",
        "from": "T20n1057b_002:0095b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結誦呪二十一遍。",
        "from": "T20n1057b_002:0095b03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一遍一結繫於左臂。",
        "from": "T20n1057b_002:0095b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以左手無名指中指頭指。",
        "from": "T20n1057b_002:0095b04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "把拳大母指壓上。",
        "from": "T20n1057b_002:0095b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "展小母指。",
        "from": "T20n1057b_002:0095b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "指所至賊方誦呪七遍。",
        "from": "T20n1057b_002:0095b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆退散不能為害。",
        "from": "T20n1057b_002:0095b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時觀世音菩薩在雪山中說法。",
        "from": "T20n1057b_002:0095b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀見夜叉羅剎國中人民。",
        "from": "T20n1057b_002:0095b07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯食眾生血肉無有善心。",
        "from": "T20n1057b_002:0095b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩為欲利益方便教化。",
        "from": "T20n1057b_002:0095b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生以神通力尋至彼國。",
        "from": "T20n1057b_002:0095b09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現千眼千臂降魔身。",
        "from": "T20n1057b_002:0095b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說成就姥陀羅尼法。",
        "from": "T20n1057b_002:0095b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時羅剎國王。",
        "from": "T20n1057b_002:0095b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來至我所求哀頂禮。",
        "from": "T20n1057b_002:0095b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我以成就印印之。",
        "from": "T20n1057b_002:0095b11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得成無上道。",
        "from": "T20n1057b_002:0095b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "18 成就印成就印第十八起立並足。",
        "from": "T20n1057b_002:0095b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌當心。",
        "from": "T20n1057b_002:0095b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以小指相叉左壓右。",
        "from": "T20n1057b_002:0095b14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦身呪二十一遍。",
        "from": "T20n1057b_002:0095b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種皆得成就。",
        "from": "T20n1057b_002:0095b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若救六道苦難眾生。",
        "from": "T20n1057b_002:0095b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用輪印。",
        "from": "T20n1057b_002:0095b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以十指頭各相拄。",
        "from": "T20n1057b_002:0095b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開腕掌中使開。",
        "from": "T20n1057b_002:0095b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其指間各相去一寸許。",
        "from": "T20n1057b_002:0095b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是菩薩在六道循環度諸苦難。",
        "from": "T20n1057b_002:0095b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此印輪迴悉得離苦。",
        "from": "T20n1057b_002:0095b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此印法拔吒那羅延長年師。",
        "from": "T20n1057b_002:0095b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纔翻便即歸國。",
        "from": "T20n1057b_002:0095b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并將所翻之本。",
        "from": "T20n1057b_002:0095b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智通畢竟尋逐不得。",
        "from": "T20n1057b_002:0095b20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遇於一僧邊得梵本。",
        "from": "T20n1057b_002:0095b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譯出在外無本。",
        "from": "T20n1057b_002:0095b21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "19 成等正覺印成等正覺印第十九結跏趺坐。",
        "from": "T20n1057b_002:0095b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以左手舒五指。",
        "from": "T20n1057b_002:0095b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仰掌在左膝上。",
        "from": "T20n1057b_002:0095b23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以右手舒五指。",
        "from": "T20n1057b_002:0095b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覆手捺右膝上。",
        "from": "T20n1057b_002:0095b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此與滅盡印法同。",
        "from": "T20n1057b_002:0095b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過去未來現在諸佛。",
        "from": "T20n1057b_002:0095b25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆同此印得佛菩提。",
        "from": "T20n1057b_002:0095b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此印能除一切業障。",
        "from": "T20n1057b_002:0095b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若坐禪諸法不現前者。",
        "from": "T20n1057b_002:0095b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當七日七夜於阿練若處。",
        "from": "T20n1057b_002:0095b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此陀羅尼并此印法。",
        "from": "T20n1057b_002:0095b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至心念佛晝夜六時懺悔即得諸法現前。",
        "from": "T20n1057b_002:0095b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及所得福無量無邊不可稱計。",
        "from": "T20n1057b_002:0095b29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "20 呼召三十三天印呼召三十三天印第二十先以左手四指把拳。",
        "from": "T20n1057b_002:0095c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以右手握左手大母指。",
        "from": "T20n1057b_002:0095c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦如把拳。",
        "from": "T20n1057b_002:0095c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令左手大母指。",
        "from": "T20n1057b_002:0095c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在右手虎口中出頭。",
        "from": "T20n1057b_002:0095c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手頭指來去。",
        "from": "T20n1057b_002:0095c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T20n1057b_002:0095c04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一俱智俱智二俱耶利三遮利遮利四遮犁隷五蘇婆訶此陀羅尼印呪不可思議。",
        "from": "T20n1057b_002:0095c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人臨欲眠時。",
        "from": "T20n1057b_002:0095c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此呪一百八遍。",
        "from": "T20n1057b_002:0095c08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心中所願於夢寐中悉得知見。",
        "from": "T20n1057b_002:0095c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能日日誦此呪者。",
        "from": "T20n1057b_002:0095c09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能滅一切罪不失菩提心。",
        "from": "T20n1057b_002:0095c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人昏夜寐夢。",
        "from": "T20n1057b_002:0095c10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漸漸增廣皆得吉祥。",
        "from": "T20n1057b_002:0095c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至夢見如來在菩提樹下授記成道。",
        "from": "T20n1057b_002:0095c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至釋梵諸天常來侍衛。",
        "from": "T20n1057b_002:0095c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "21 呼召天龍八部鬼神印呼召天龍八部鬼神印第二十一起立並足。",
        "from": "T20n1057b_002:0095c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以左手大母指屈在掌中。",
        "from": "T20n1057b_002:0095c14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四指把拳當心上著。",
        "from": "T20n1057b_002:0095c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以右手亦如之。",
        "from": "T20n1057b_002:0095c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手在右耳邊。",
        "from": "T20n1057b_002:0095c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以頭指來去。",
        "from": "T20n1057b_002:0095c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T20n1057b_002:0095c16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無尼健陀上聲一南無阿利闍波陀去聲二馺婆訶三南無阿利闍羅馺婆訶四堙醯夷醯五馺婆訶此呪印若善男子善女人受持讀誦者。",
        "from": "T20n1057b_002:0095c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知七世宿命。",
        "from": "T20n1057b_002:0095c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒蛇不敢螫。",
        "from": "T20n1057b_002:0095c21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒藥自然除。",
        "from": "T20n1057b_002:0095c21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀不能害王亦不能瞋。",
        "from": "T20n1057b_002:0095c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永劫不受地獄苦。",
        "from": "T20n1057b_002:0095c22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦此呪時。",
        "from": "T20n1057b_002:0095c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十八部鬼神來詣。",
        "from": "T20n1057b_002:0095c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦呪人邊坐聽誦呪。",
        "from": "T20n1057b_002:0095c23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人。",
        "from": "T20n1057b_002:0095c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為鬼魅所著。",
        "from": "T20n1057b_002:0095c24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以白縷為呪索。",
        "from": "T20n1057b_002:0095c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一遍一結如是四十九結。",
        "from": "T20n1057b_002:0095c25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫其咽下其病即除。",
        "from": "T20n1057b_002:0095c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若國內災疫流行國人死亡多者。",
        "from": "T20n1057b_002:0095c26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當取國王園池中蓮華一百八莖。",
        "from": "T20n1057b_002:0095c27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一華各呪一遍擲著火中。",
        "from": "T20n1057b_002:0095c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒令蕩盡災疫即除。",
        "from": "T20n1057b_002:0095c28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "22 解脫印解脫印第二十二結跏趺坐。",
        "from": "T20n1057b_002:0096a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以左手中指。",
        "from": "T20n1057b_002:0096a01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大母指頭相捻。",
        "from": "T20n1057b_002:0096a01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仰掌向上。",
        "from": "T20n1057b_002:0096a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘三指散展置於左膝上。",
        "from": "T20n1057b_002:0096a02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以右手亦如之。",
        "from": "T20n1057b_002:0096a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覆手置於右膝上。",
        "from": "T20n1057b_002:0096a03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦身呪二十一遍。",
        "from": "T20n1057b_002:0096a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所願悉皆滿足諸有苦惱悉皆解脫。",
        "from": "T20n1057b_002:0096a04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人。",
        "from": "T20n1057b_002:0096a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具造十惡五逆等罪。",
        "from": "T20n1057b_002:0096a05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如閻浮提履地變為微塵。",
        "from": "T20n1057b_002:0096a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一微塵成於一劫。",
        "from": "T20n1057b_002:0096a06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人造若干等罪。",
        "from": "T20n1057b_002:0096a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應墮地獄歷劫受苦永無出期。",
        "from": "T20n1057b_002:0096a07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是善男子善女人。",
        "from": "T20n1057b_002:0096a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能於舍利及佛像前。",
        "from": "T20n1057b_002:0096a08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白月十五日一日一夜不食。",
        "from": "T20n1057b_002:0096a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結印誦呪滿一百八遍。",
        "from": "T20n1057b_002:0096a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如上諸罪苦惱悉皆消滅。",
        "from": "T20n1057b_002:0096a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不滅者無有是處。",
        "from": "T20n1057b_002:0096a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此印智通本上先無。",
        "from": "T20n1057b_002:0096a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智通於涼州。",
        "from": "T20n1057b_002:0096a11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逢一婆羅門僧有此梵本。",
        "from": "T20n1057b_002:0096a12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遇會勘之更有此印。",
        "from": "T20n1057b_002:0096a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自得受持大有功効不可思議。",
        "from": "T20n1057b_002:0096a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "23 自在神足印自在神足印第二十三起立先以左手握右脚大母指如把拳。",
        "from": "T20n1057b_002:0096a15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以右手握左手腕背上。",
        "from": "T20n1057b_002:0096a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦身呪七遍。",
        "from": "T20n1057b_002:0096a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲進千里不以為難。",
        "from": "T20n1057b_002:0096a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦呪之時勿令聲出。",
        "from": "T20n1057b_002:0096a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "24 神變自在印神變自在印第二十四先以左手大母指捻小指甲上。",
        "from": "T20n1057b_002:0096a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以右手亦如之。",
        "from": "T20n1057b_002:0096a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘三指各散竪。",
        "from": "T20n1057b_002:0096a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合腕相著置於頂上。",
        "from": "T20n1057b_002:0096a20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦身呪二十一遍。",
        "from": "T20n1057b_002:0096a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆得遊行自在。",
        "from": "T20n1057b_002:0096a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔有罽賓國僧闍提。",
        "from": "T20n1057b_002:0096a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於北天竺求得此梵本。",
        "from": "T20n1057b_002:0096a22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未曾翻譯。",
        "from": "T20n1057b_002:0096a22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自得受持威力廣大不敢流傳。",
        "from": "T20n1057b_002:0096a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智通於此僧弟婆伽伽邊得本。",
        "from": "T20n1057b_002:0096a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依法受持功効不少。",
        "from": "T20n1057b_002:0096a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯不流行於世。",
        "from": "T20n1057b_002:0096a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此本絕無。",
        "from": "T20n1057b_002:0096a25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後同學得者願同功力。",
        "from": "T20n1057b_002:0096a25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "25 王心印咒王心印呪第二十五兩手合掌虛掌內合腕。",
        "from": "T20n1057b_002:0096a27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指來去。",
        "from": "T20n1057b_002:0096a27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T20n1057b_002:0096a27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一阿嚕力帝囇路迦夜去聲毘社時賀切耶二薩婆鑠覩嚧二合鉢囉麼馱那三迦囉耶𤙖泮莎訶爾時觀世音菩薩。",
        "from": "T20n1057b_002:0096b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告諸大眾言。",
        "from": "T20n1057b_002:0096b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若佛滅後於末法時五百年中。",
        "from": "T20n1057b_002:0096b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "閻浮提人忽眼見。",
        "from": "T20n1057b_002:0096b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及耳聞我此法印陀羅尼法門者。",
        "from": "T20n1057b_002:0096b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知此人過去已曾供養無量諸佛。",
        "from": "T20n1057b_002:0096b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今得值遇得我此法印陀羅尼。",
        "from": "T20n1057b_002:0096b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "抄寫受持讀誦如法修行者。",
        "from": "T20n1057b_002:0096b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得初地速成佛果。",
        "from": "T20n1057b_002:0096b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得阿耨多羅三藐三菩提。",
        "from": "T20n1057b_002:0096b07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時觀世音菩薩聞佛說已歡喜信受作禮而去。",
        "from": "T20n1057b_002:0096b08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千眼千臂觀世音菩薩陀羅尼神呪經卷下",
        "from": "T20n1057b_002:0096b09_100",
        "context_with_next_sentence": false
    }
]