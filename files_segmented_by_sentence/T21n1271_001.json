[
    {
        "source_sentence": "摩訶毘盧遮那如來定惠均等入",
        "from": "T21n1271_001:0305c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三昧耶身雙身大聖歡喜天菩薩",
        "from": "T21n1271_001:0305c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行祕密法儀軌",
        "from": "T21n1271_001:0305c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鴻盧卿大廣智不空三藏奉　勅譯若有佛弟子。",
        "from": "T21n1271_001:0305c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比丘比丘尼優婆塞優婆夷四種弟子。",
        "from": "T21n1271_001:0305c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人等。",
        "from": "T21n1271_001:0305c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲得修行此聖天菩薩法。",
        "from": "T21n1271_001:0305c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令成就所求所願隨心如意者。",
        "from": "T21n1271_001:0305c11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先須盡珍寶入勞傳法師並阿闍梨。",
        "from": "T21n1271_001:0305c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為令得歡喜心。",
        "from": "T21n1271_001:0305c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百味飲食。",
        "from": "T21n1271_001:0305c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "資具資財。",
        "from": "T21n1271_001:0305c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣服座具。",
        "from": "T21n1271_001:0305c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨身之資具。",
        "from": "T21n1271_001:0305c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養勞令得歡喜心後。",
        "from": "T21n1271_001:0305c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其微細祕密甚悉旨宗趣。",
        "from": "T21n1271_001:0305c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問求具習覺。",
        "from": "T21n1271_001:0305c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應修行祕密法。",
        "from": "T21n1271_001:0305c15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要道成就法奇捨身求道。",
        "from": "T21n1271_001:0305c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡珍寶入勞。",
        "from": "T21n1271_001:0305c16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂資身衣服也。",
        "from": "T21n1271_001:0305c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "續命飲食。",
        "from": "T21n1271_001:0305c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以自身可被著衣服之料。",
        "from": "T21n1271_001:0305c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自可食飲食之料。",
        "from": "T21n1271_001:0305c18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養傳法師知法阿闍梨。",
        "from": "T21n1271_001:0305c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養此盡珍寶入勞。",
        "from": "T21n1271_001:0305c19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂先須以火一升胡麻油溫。",
        "from": "T21n1271_001:0305c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "炭瓫火網白銅多羅加持。",
        "from": "T21n1271_001:0305c20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大真言百八返。",
        "from": "T21n1271_001:0305c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且溫且加持。",
        "from": "T21n1271_001:0305c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但印三股鉾也。",
        "from": "T21n1271_001:0305c21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "根本大真言曰　身呪。",
        "from": "T21n1271_001:0305c22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨尾那翼迦二合瀉賀悉底二合母佉瀉怛儞也二合引他唵娜去聲下同翼迦二合娜翼迦二合尾娜翼迦二合尾娜翼迦二合怛囉二合翼迦二合簸哩二合怛囉二合翼迦二合餉引佉賀悉底二合餉引生反佉迦只多扇上底迦囉二合娑嚩二合引賀引灌沐日中三返。",
        "from": "T21n1271_001:0305c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平日旦四返。",
        "from": "T21n1271_001:0305c28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灌沐限七日為期。",
        "from": "T21n1271_001:0305c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乞得成就悉地。",
        "from": "T21n1271_001:0305c29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以少心呪加持供物。",
        "from": "T21n1271_001:0305c29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其供物數蘿福根歡喜團餅時菓子等。",
        "from": "T21n1271_001:0306a01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯現身賢聖供養奉。",
        "from": "T21n1271_001:0306a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七日之內必驗得。",
        "from": "T21n1271_001:0306a02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少心呪曰。",
        "from": "T21n1271_001:0306a02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵只里虐娑婆賀引二合每供養奉畢。",
        "from": "T21n1271_001:0306a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦讚歎頌曰。",
        "from": "T21n1271_001:0306a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵誐娜婆底洲底娑婆悉底摩訶誐馱轉底洲底迦羅耶娑婆賀引二合成就所求所願如意圓滿具足故。",
        "from": "T21n1271_001:0306a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可令傳法阿闍梨歡喜。",
        "from": "T21n1271_001:0306a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後一七日可修行。",
        "from": "T21n1271_001:0306a08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得四種成就悉地。",
        "from": "T21n1271_001:0306a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "息增愛調。",
        "from": "T21n1271_001:0306a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此謂四種成就悉地法也。",
        "from": "T21n1271_001:0306a09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘那夜經說。",
        "from": "T21n1271_001:0306a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂此聖天菩薩供養奉。",
        "from": "T21n1271_001:0306a10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有三種果報。",
        "from": "T21n1271_001:0306a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上品供養奉。",
        "from": "T21n1271_001:0306a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令得國王威德。",
        "from": "T21n1271_001:0306a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中品供養奉者。",
        "from": "T21n1271_001:0306a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令具七寶富饒令得下品供養奉。",
        "from": "T21n1271_001:0306a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣食不乏飽滿具足自然也。",
        "from": "T21n1271_001:0306a13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上品供養奉者。",
        "from": "T21n1271_001:0306a13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令得傳法人歡喜之心。",
        "from": "T21n1271_001:0306a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祕密甚深□旨被教授。",
        "from": "T21n1271_001:0306a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如法供養奉。",
        "from": "T21n1271_001:0306a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中品供養奉者儀軌可見不悟具次第要道略供養奉。",
        "from": "T21n1271_001:0306a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下品供養奉者。",
        "from": "T21n1271_001:0306a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不灌沐油。",
        "from": "T21n1271_001:0306a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "只別殿令坐。",
        "from": "T21n1271_001:0306a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所得每初分供養奉。",
        "from": "T21n1271_001:0306a17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此名為下品。",
        "from": "T21n1271_001:0306a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "像末佛弟子。",
        "from": "T21n1271_001:0306a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自之心懷慳貪我慢愛惜之心。",
        "from": "T21n1271_001:0306a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不成得傳法知道阿闍梨得歡喜悅賀之心。",
        "from": "T21n1271_001:0306a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "委曲要深趣不問悟。",
        "from": "T21n1271_001:0306a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦自得疎略供養修行。",
        "from": "T21n1271_001:0306a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誹謗深祕勝道。",
        "from": "T21n1271_001:0306a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "招取貧窮無福種子。",
        "from": "T21n1271_001:0306a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天台智者阿闍梨止觀心要記云。",
        "from": "T21n1271_001:0306a22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂有福無智。",
        "from": "T21n1271_001:0306a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此愚人不殊龍身鬼云云有智無福。",
        "from": "T21n1271_001:0306a23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此橫惑者。",
        "from": "T21n1271_001:0306a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非賢聖之心。",
        "from": "T21n1271_001:0306a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間甚自無福能無勝者。",
        "from": "T21n1271_001:0306a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世中操者福德自莊嚴無過哉。",
        "from": "T21n1271_001:0306a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂所以此聖天菩薩供養奉　修行時。",
        "from": "T21n1271_001:0306a26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必令圓滿福智也。",
        "from": "T21n1271_001:0306a26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定者福也。",
        "from": "T21n1271_001:0306a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惠均等三摩身者。",
        "from": "T21n1271_001:0306a27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂此法初終有兩時。",
        "from": "T21n1271_001:0306a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者平旦。",
        "from": "T21n1271_001:0306a28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者日中。",
        "from": "T21n1271_001:0306a28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此兩時此平旦增益時也。",
        "from": "T21n1271_001:0306a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日中吉祥時也。",
        "from": "T21n1271_001:0306a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以此聖天供養奉人者。",
        "from": "T21n1271_001:0306b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福智圓滿具足耳。",
        "from": "T21n1271_001:0306b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦毘尼宗有□所權教人者。",
        "from": "T21n1271_001:0306b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百世無平等云云此法以酒供養附子。",
        "from": "T21n1271_001:0306b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此毒物服醫師得之除病安身藥。",
        "from": "T21n1271_001:0306b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和合人合得悅賀。",
        "from": "T21n1271_001:0306b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酒此歡喜水妙藥也。",
        "from": "T21n1271_001:0306b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "田夫令悅。",
        "from": "T21n1271_001:0306b04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此酒也。",
        "from": "T21n1271_001:0306b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能飲人藥成。",
        "from": "T21n1271_001:0306b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡飲人毒成。",
        "from": "T21n1271_001:0306b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此天菩薩人心令得歡喜故。",
        "from": "T21n1271_001:0306b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以酒供養此名歡喜水耳。",
        "from": "T21n1271_001:0306b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現生國王貴君。",
        "from": "T21n1271_001:0306b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如供養奉時。",
        "from": "T21n1271_001:0306b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必獲得靈驗。",
        "from": "T21n1271_001:0306b07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所供遺呪師服無咎。",
        "from": "T21n1271_001:0306b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲法所制也。",
        "from": "T21n1271_001:0306b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此法無制。",
        "from": "T21n1271_001:0306b08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "像末佛弟子發誠心信念。",
        "from": "T21n1271_001:0306b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至心慇懃供養奉。",
        "from": "T21n1271_001:0306b09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可生起疑心。",
        "from": "T21n1271_001:0306b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳法知法者問求具習悟信受奉行。",
        "from": "T21n1271_001:0306b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可疎略。",
        "from": "T21n1271_001:0306b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次第法阿闍梨含受具不載。",
        "from": "T21n1271_001:0306b11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儀軌文能次第法受習覺悟供養修行信受奉行皆大歡喜。",
        "from": "T21n1271_001:0306b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東寺之本也　　　　　　　俊乘記之",
        "from": "T21n1271_001:0306b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　　　　　　　　　　　寬　俊",
        "from": "T21n1271_001:0306b18_0",
        "context_with_next_sentence": true
