[
    {
        "source_sentence": "聖虛空藏菩薩陀羅尼經",
        "from": "T20n1147_001:0604c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天中印度摩伽陀國那爛陀寺",
        "from": "T20n1147_001:0604c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏傳教大師賜紫沙門臣",
        "from": "T20n1147_001:0604c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法天奉　詔譯如是我聞。",
        "from": "T20n1147_001:0604c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時世尊在喜樂山頂天宮不遠仙人住處。",
        "from": "T20n1147_001:0604c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大比丘眾五百人俱。",
        "from": "T20n1147_001:0604c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有菩薩摩訶薩眾。",
        "from": "T20n1147_001:0604c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此諸菩薩皆是一生得成無上正等菩提。",
        "from": "T20n1147_001:0604c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其名曰。",
        "from": "T20n1147_001:0604c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈氏菩薩摩訶薩。",
        "from": "T20n1147_001:0604c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普賢菩薩摩訶薩。",
        "from": "T20n1147_001:0604c17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無邊華菩薩摩訶薩。",
        "from": "T20n1147_001:0604c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普遍華菩薩摩訶薩。",
        "from": "T20n1147_001:0604c18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空藏菩薩摩訶薩。",
        "from": "T20n1147_001:0604c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等菩薩摩訶薩五百人俱。",
        "from": "T20n1147_001:0604c19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊及諸菩薩。",
        "from": "T20n1147_001:0604c20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見彼山下河邊林中。",
        "from": "T20n1147_001:0604c20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有二比丘裸形舒手叫喚啼泣是時虛空藏菩薩摩訶薩。",
        "from": "T20n1147_001:0605a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即從座起偏袒右肩。",
        "from": "T20n1147_001:0605a01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右膝著地合掌恭敬。",
        "from": "T20n1147_001:0605a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊。",
        "from": "T20n1147_001:0605a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殑伽河邊彼二比丘。",
        "from": "T20n1147_001:0605a02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何因何緣裸形舒手叫喚啼泣。",
        "from": "T20n1147_001:0605a03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告虛空藏菩薩摩訶薩言。",
        "from": "T20n1147_001:0605a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼二比丘疾病所纏部多所執是故裸形叫喚啼泣。",
        "from": "T20n1147_001:0605a05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時虛空藏菩薩摩訶薩。",
        "from": "T20n1147_001:0605a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊如是疾病云何消除。",
        "from": "T20n1147_001:0605a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是部多云何除遣。",
        "from": "T20n1147_001:0605a07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊以神通力召集諸佛。",
        "from": "T20n1147_001:0605a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時虛空中現六如來。",
        "from": "T20n1147_001:0605a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲證明第七世尊釋迦牟尼如來法故。",
        "from": "T20n1147_001:0605a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時尾鉢尸如來。",
        "from": "T20n1147_001:0605a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲利益一切眾生。",
        "from": "T20n1147_001:0605a11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "消除疾病祛遣部多。",
        "from": "T20n1147_001:0605a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨喜宣說陀羅尼曰。",
        "from": "T20n1147_001:0605a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨引沒馱引野一曩謨引達哩麼二合引野二曩謨僧伽引野三唵四引左羅左羅五呬㘑呬㘑六呬羅野七曩謨昝仁敢反謨八左曩引野九曩謨引曩莫十娑嚩二合引賀引是時尾鉢尸如來說是明已。",
        "from": "T20n1147_001:0605a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告虛空藏菩薩摩訶薩言。",
        "from": "T20n1147_001:0605a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有受持讀誦供養此陀羅尼者。",
        "from": "T20n1147_001:0605a18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知是人無疾病苦器杖不傷。",
        "from": "T20n1147_001:0605a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水不能漂毒不能害。",
        "from": "T20n1147_001:0605a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦無中夭飲食無患。",
        "from": "T20n1147_001:0605a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲得聞持壽命長遠。",
        "from": "T20n1147_001:0605a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切時中諸佛護念。",
        "from": "T20n1147_001:0605a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復耳聾呪油七遍。",
        "from": "T20n1147_001:0605a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滴油耳中自然消散。",
        "from": "T20n1147_001:0605a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時尸企如來。",
        "from": "T20n1147_001:0605a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲利益一切眾生。",
        "from": "T20n1147_001:0605a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "消除疾病祛遣部多。",
        "from": "T20n1147_001:0605a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨喜宣說陀羅尼曰。",
        "from": "T20n1147_001:0605a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨引沒馱引野一曩謨引達哩麼二合野二曩莫僧伽引野三唵引四波左波左五波左波左引野六薩哩嚩　二合部多引喃引七嚫儞枳㘑野八波羅尾儞野二合喃引九娑嚩二合引賀引十是時尸企如來說是明已。",
        "from": "T20n1147_001:0605a29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告虛空藏菩薩摩訶薩言。",
        "from": "T20n1147_001:0605b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我此心印大陀羅尼。",
        "from": "T20n1147_001:0605b01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百千俱胝無數諸佛同共宣說。",
        "from": "T20n1147_001:0605b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生晝夜六時憶持我此心陀羅尼。",
        "from": "T20n1147_001:0605b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "病疾消除鬼魅遠離惡夢除遣永離中夭。",
        "from": "T20n1147_001:0605b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "患難不侵毒不能害。",
        "from": "T20n1147_001:0605b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夢見如來入佛境界。",
        "from": "T20n1147_001:0605b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復風癀童子鬼病。",
        "from": "T20n1147_001:0605b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結青線索呪蘇摩那身上帶持耳邊誦呪。",
        "from": "T20n1147_001:0605b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切時中願皆除滅。",
        "from": "T20n1147_001:0605b07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時毘舍浮如來。",
        "from": "T20n1147_001:0605b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲利益一切眾生消除疾病祛遣部多。",
        "from": "T20n1147_001:0605b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨喜宣說陀羅尼曰。",
        "from": "T20n1147_001:0605b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨引沒馱引野一曩謨引達哩麼二合引野二曩莫引僧伽引野三唵引迦羅迦羅四迦羅迦羅五矩路引娑那曩　矩嚕六薩哩嚩二合誐囉二合賀引赦七娑嚩二合引賀引八是時毘舍浮如來說是明已。",
        "from": "T20n1147_001:0605b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告虛空藏菩薩摩訶薩言。",
        "from": "T20n1147_001:0605b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今諦聽此陀羅尼。",
        "from": "T20n1147_001:0605b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢劫如來三世諸佛同共宣說若有受持讀誦供養此陀羅尼者。",
        "from": "T20n1147_001:0605b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知彼人器杖不傷毒不能害。",
        "from": "T20n1147_001:0605b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無諸疾病水不能漂。",
        "from": "T20n1147_001:0605b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦無中夭宿業消除。",
        "from": "T20n1147_001:0605b18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有比丘比丘尼優婆塞優婆夷。",
        "from": "T20n1147_001:0605b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先當洗浴住世尊前。",
        "from": "T20n1147_001:0605b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦持此明滿八百遍一切疾病一切嬈惱不能侵害。",
        "from": "T20n1147_001:0605b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲得長命盡諸煩惱。",
        "from": "T20n1147_001:0605b21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當得庫藏財穀甚多。",
        "from": "T20n1147_001:0605b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若惡眾生一切怖畏。",
        "from": "T20n1147_001:0605b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思惟念誦此陀羅尼怖畏消除獲得愛樂。",
        "from": "T20n1147_001:0605b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有一切鬪諍論義。",
        "from": "T20n1147_001:0605b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白線作結誦陀羅尼一切和合。",
        "from": "T20n1147_001:0605b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患眼痛乳樹作線。",
        "from": "T20n1147_001:0605b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦陀羅尼繫於頸上即得除愈。",
        "from": "T20n1147_001:0605b27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復見有軍陣相向。",
        "from": "T20n1147_001:0605b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取蜜酪酥阿波摩誐。",
        "from": "T20n1147_001:0605b28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一誦一擲火中護摩。",
        "from": "T20n1147_001:0605b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿八千遍。",
        "from": "T20n1147_001:0605b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切軍眾互相收陣壽命長遠。",
        "from": "T20n1147_001:0605c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王及大臣為求安隱。",
        "from": "T20n1147_001:0605c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酪蜜稻花火中護摩滿八千遍。",
        "from": "T20n1147_001:0605c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切處身心快樂。",
        "from": "T20n1147_001:0605c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若為自身及諸眾生。",
        "from": "T20n1147_001:0605c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "消除蘊惡災難罪障不吉祥相。",
        "from": "T20n1147_001:0605c04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白乳樹柴呪胡麻子。",
        "from": "T20n1147_001:0605c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火中護摩滿八千遍。",
        "from": "T20n1147_001:0605c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切災難即得消除。",
        "from": "T20n1147_001:0605c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人自為成就一切明呪。",
        "from": "T20n1147_001:0605c06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當取百華曩羅那必哩焰牛頭栴檀世禮多枳恭句摩母娑怛迦印捺囉嚩嚕拏珊瑚惹底此藥一處盛賢瓶中。",
        "from": "T20n1147_001:0605c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩提樹枝蘇摩那葉。",
        "from": "T20n1147_001:0605c09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "插賢瓶口安世尊前。",
        "from": "T20n1147_001:0605c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白月十五日一日一夜於世尊前誦陀羅尼。",
        "from": "T20n1147_001:0605c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿八百遍悉地成就。",
        "from": "T20n1147_001:0605c11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時羯矩忖那如來。",
        "from": "T20n1147_001:0605c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲利益一切眾生。",
        "from": "T20n1147_001:0605c12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "消除疾病祛遣部多。",
        "from": "T20n1147_001:0605c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨喜宣說陀羅尼曰。",
        "from": "T20n1147_001:0605c13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨引沒馱引野一曩謨引達哩麼二合引野二曩莫引僧伽引野三曩莫羯囉二合矩忖那引野四怛他引誐哆引野引五囉賀二合諦引六三弭野二合三沒馱引野七唵引八迦吒迦吒九迦吒迦吒十枳致枳致十一枳致枳致十二曩謨薩哩嚩二合十三怛他引誐諦引毘喻二合引十四囉賀二合帝毘藥二合十五三弭野二合三沒地引毘喻二合引十六娑嚩二合引賀引十七是時羯矩忖那如來說是明已。",
        "from": "T20n1147_001:0605c22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告虛空藏菩薩摩訶薩言。",
        "from": "T20n1147_001:0605c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今諦聽。",
        "from": "T20n1147_001:0605c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我此心印大陀羅尼。",
        "from": "T20n1147_001:0605c23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殑伽沙等諸佛世尊同共宣說。",
        "from": "T20n1147_001:0605c24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有受持供養聽聞隨喜此陀羅尼者。",
        "from": "T20n1147_001:0605c25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知是人獲得聞持遠離疾病飲食無患繫縛解脫。",
        "from": "T20n1147_001:0605c26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後時後分若有比丘比丘尼優婆塞優婆夷。",
        "from": "T20n1147_001:0605c27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香華塗香供養三寶。",
        "from": "T20n1147_001:0605c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沐浴世尊發心念誦此陀羅尼。",
        "from": "T20n1147_001:0605c28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿八百遍得宿命通。",
        "from": "T20n1147_001:0605c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七生天上作諸天子。",
        "from": "T20n1147_001:0605c29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下生人間為轉輪王。",
        "from": "T20n1147_001:0606a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不依法隨緣念誦。",
        "from": "T20n1147_001:0606a01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常生人中。",
        "from": "T20n1147_001:0606a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人中命終生安樂剎。",
        "from": "T20n1147_001:0606a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若得種種飲食加持七遍。",
        "from": "T20n1147_001:0606a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切疾病悉皆遠離。",
        "from": "T20n1147_001:0606a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若為息除一切鬼魅八千障難。",
        "from": "T20n1147_001:0606a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當先澡浴念誦此明滿一七遍。",
        "from": "T20n1147_001:0606a04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患惡瘡呪瘡七遍。",
        "from": "T20n1147_001:0606a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患癰腫加持銅針。",
        "from": "T20n1147_001:0606a05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念呪下針即得除愈。",
        "from": "T20n1147_001:0606a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切鬪諍念誦此明。",
        "from": "T20n1147_001:0606a06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿一七遍即得除散。",
        "from": "T20n1147_001:0606a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切繫縛念誦此明。",
        "from": "T20n1147_001:0606a07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿一七遍便得解脫財寶增長。",
        "from": "T20n1147_001:0606a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念陀羅尼恒無間斷。",
        "from": "T20n1147_001:0606a08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尾曩野迦一切障難。",
        "from": "T20n1147_001:0606a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦陀羅尼滿一七遍即得遠離。",
        "from": "T20n1147_001:0606a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人要知心之所求事。",
        "from": "T20n1147_001:0606a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於清淨處若佛塔前。",
        "from": "T20n1147_001:0606a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚴結壇場燒沈水香。",
        "from": "T20n1147_001:0606a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦念此明滿八千遍。",
        "from": "T20n1147_001:0606a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭東欹臥夢見如來。",
        "from": "T20n1147_001:0606a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽命脩短好惡勝劣彼一切見。",
        "from": "T20n1147_001:0606a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時羯曩迦牟尼如來。",
        "from": "T20n1147_001:0606a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲利益一切眾生。",
        "from": "T20n1147_001:0606a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "消除疾病祛遣部多。",
        "from": "T20n1147_001:0606a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨喜宣說陀羅尼曰。",
        "from": "T20n1147_001:0606a15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨引沒馱引野一曩謨引達哩麼二合引野二曩莫僧　伽引野三曩莫迦曩迦母曩曳引四怛他引誐哆野引五囉賀二合諦引六三弭野二合三沒馱引野七唵引八娑囉娑囉九娑囉娑囉十悉哩悉哩十一悉哩悉哩十二悉囉引野十三馱摩馱摩十四馱摩馱摩十五度母度母十六度母度母十七度摩引野十八曩謨引曩莫十九迦曩迦母曩曳引二十怛他引誐哆引野引囉賀二合諦引二十一三弭野二合三沒駄引野二十二娑嚩二合引賀引二十三是時羯曩迦牟尼如來說是明已。",
        "from": "T20n1147_001:0606a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告虛空藏菩薩摩訶薩言。",
        "from": "T20n1147_001:0606a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有受持讀誦。",
        "from": "T20n1147_001:0606a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒常供養此陀羅尼者。",
        "from": "T20n1147_001:0606a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知是人一切刀杖水火。",
        "from": "T20n1147_001:0606a27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怖畏邪法呪術諸毒不能侵害亦不中夭。",
        "from": "T20n1147_001:0606a28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先世所造一切業障。",
        "from": "T20n1147_001:0606a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三時誦念皆得消除。",
        "from": "T20n1147_001:0606a29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復疥癩癥癖惡腫。",
        "from": "T20n1147_001:0606b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用菖蒱作末蜜和為丸。",
        "from": "T20n1147_001:0606b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火中護摩滿八千遍。",
        "from": "T20n1147_001:0606b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復瘧病一日二日三日四日。",
        "from": "T20n1147_001:0606b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持惹底蘇摩那華身上帶持。",
        "from": "T20n1147_001:0606b03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復耳聾耳邊誦呪。",
        "from": "T20n1147_001:0606b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復風癎羯吒鬼病。",
        "from": "T20n1147_001:0606b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持佛華身上帶持。",
        "from": "T20n1147_001:0606b04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是諸病願皆消除。",
        "from": "T20n1147_001:0606b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "富貴長壽恒見如來。",
        "from": "T20n1147_001:0606b05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切時中諸佛護念。",
        "from": "T20n1147_001:0606b06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時迦葉波如來。",
        "from": "T20n1147_001:0606b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲利益一切眾生。",
        "from": "T20n1147_001:0606b07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "消除疾病祛遣部多。",
        "from": "T20n1147_001:0606b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨喜宣說陀羅尼曰。",
        "from": "T20n1147_001:0606b08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨引沒馱引野一曩謨引達哩麼二合引野二曩莫僧伽引野三唵引四賀囉賀囉五賀囉賀囉六賀賀七曩莫迦設野二合波野八怛他引誐哆引野九引囉賀二合諦引十三弭野二合三沒馱引野十一悉殿覩十二滿怛羅二合波那十三娑嚩二合引賀引十四爾時迦葉波如來應正等覺說是明已。",
        "from": "T20n1147_001:0606b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告虛空藏菩薩摩訶薩言。",
        "from": "T20n1147_001:0606b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有受持讀誦供養此陀羅尼者。",
        "from": "T20n1147_001:0606b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知是人一切業障悉皆消除一切時中諸佛攝受。",
        "from": "T20n1147_001:0606b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離惡法安住善法。",
        "from": "T20n1147_001:0606b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人患白癩病。",
        "from": "T20n1147_001:0606b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持佛華滿八千遍。",
        "from": "T20n1147_001:0606b19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若常熱病加持佛華滿八千遍。",
        "from": "T20n1147_001:0606b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若病初得加持佛華。",
        "from": "T20n1147_001:0606b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿八千遍身上帶持。",
        "from": "T20n1147_001:0606b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切時中願皆除滅。",
        "from": "T20n1147_001:0606b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復女人懷孕難生。",
        "from": "T20n1147_001:0606b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持佛華油煎塗用。",
        "from": "T20n1147_001:0606b23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若皮風瘡加持佛華酥煎塗上。",
        "from": "T20n1147_001:0606b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復癰腫及諸痔瘻。",
        "from": "T20n1147_001:0606b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持佛華好蜜一處用塗瘡上。",
        "from": "T20n1147_001:0606b25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復瘧病一日二日三日四日。",
        "from": "T20n1147_001:0606b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食香食華一切鬼病。",
        "from": "T20n1147_001:0606b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或復癲癎加持佛華身上帶持。",
        "from": "T20n1147_001:0606b27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等病明力加持。",
        "from": "T20n1147_001:0606b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切時中願皆除滅。",
        "from": "T20n1147_001:0606b28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時釋迦牟尼如來。",
        "from": "T20n1147_001:0606b29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲利益一切眾生。",
        "from": "T20n1147_001:0606b29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "消除疾病祛遣部多。",
        "from": "T20n1147_001:0606c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此心印陀羅尼曰。",
        "from": "T20n1147_001:0606c01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨引沒馱引野一曩謨引達哩麼二合野二曩莫僧伽引野三怛儞野二合他引四唵引五具弭具弭六摩賀引具弭七底㘑底㘑八摩賀引底㘑九左㘑左㘑十摩賀引左㘑十一馱囉馱囉十二摩賀引馱囉十三鼻哩鼻哩十四摩賀引鼻哩十五枳㘑枳㘑十六摩賀引枳㘑十七𭉷嚕𭉷嚕十八摩賀引𭉷嚕十九弭㘑弭㘑二十摩賀引弭㘑二十一底㘑底㘑二十二摩賀引底㘑二十三度弭度弭二十四摩賀引度弭二十五左㘑左㘑二十六摩賀引左㘑二十七企哩企哩二十八摩賀引企哩二十九唧㘑唧㘑三十摩賀引唧㘑三十一娑嚩二合引賀引三十二爾時世尊釋迦牟尼如來說是明已。",
        "from": "T20n1147_001:0606c13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告虛空藏菩薩摩訶薩言。",
        "from": "T20n1147_001:0606c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空藏若有受持讀誦供養陀羅尼者。",
        "from": "T20n1147_001:0606c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知彼人身無疾病。",
        "from": "T20n1147_001:0606c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀杖不傷水不漂溺。",
        "from": "T20n1147_001:0606c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不受苦惱亦不中夭。",
        "from": "T20n1147_001:0606c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空藏汝今當知此陀羅尼有大威力。",
        "from": "T20n1147_001:0606c17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除疾病能除諸毒。",
        "from": "T20n1147_001:0606c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除不祥及諸怖畏。",
        "from": "T20n1147_001:0606c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若彼部多彼部多眾不能侵害。",
        "from": "T20n1147_001:0606c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若布怛曩布怛曩眾不能侵害。",
        "from": "T20n1147_001:0606c19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等眾各起慈心。",
        "from": "T20n1147_001:0606c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切時中令彼安隱。",
        "from": "T20n1147_001:0606c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時虛空藏菩薩摩訶薩白佛言。",
        "from": "T20n1147_001:0606c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊如來已說利益眾生成就安隱大陀羅尼。",
        "from": "T20n1147_001:0606c22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今亦說利益眾生法界清淨成就安隱陀羅尼曰。",
        "from": "T20n1147_001:0606c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞野二合他引左㘑左㘑摩賀引左㘑覩嚕覩嚕摩賀引覩嚕入嚩二合㘑入嚩二合㘑摩賀引入嚩二合㘑呬㘑呬㘑摩賀引呬㘑娑嚩二合引賀是時虛空藏菩薩摩訶薩。",
        "from": "T20n1147_001:0606c27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是明已復作是言。",
        "from": "T20n1147_001:0606c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲成就出世間殊勝功德。",
        "from": "T20n1147_001:0606c28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當受持此陀羅尼。",
        "from": "T20n1147_001:0606c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間所有刀杖繫縛。",
        "from": "T20n1147_001:0606c29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疾病若患黑雲障礙。",
        "from": "T20n1147_001:0607a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尾囉蔓毒不能侵害。",
        "from": "T20n1147_001:0607a01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復迦驅那切那儞阿底沒里瑟吒囉病。",
        "from": "T20n1147_001:0607a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明力護持願皆除滅。",
        "from": "T20n1147_001:0607a02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊說是法已。",
        "from": "T20n1147_001:0607a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空藏菩薩摩訶薩等。",
        "from": "T20n1147_001:0607a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切大眾聞佛所說。",
        "from": "T20n1147_001:0607a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心受持歡喜奉行。",
        "from": "T20n1147_001:0607a04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖虛空藏菩薩陀羅尼經",
        "from": "T20n1147_001:0607a05_100",
        "context_with_next_sentence": false
    }
]