[
    {
        "source_sentence": "佛說最勝妙吉祥根本智最上祕",
        "from": "T20n1187_001:0808a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密一切名義三摩地分卷上",
        "from": "T20n1187_001:0808a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝奉大夫試鴻臚卿",
        "from": "T20n1187_001:0808a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時大吉祥　　最勝金剛手",
        "from": "T20n1187_001:0808a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具一切功德　　能調難調者",
        "from": "T20n1187_001:0808a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三界中最勝　　最尊大無畏",
        "from": "T20n1187_001:0808a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸祕密部中　　自在為主宰",
        "from": "T20n1187_001:0808a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正智蓮華目　　眉相復如蓮",
        "from": "T20n1187_001:0808a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於佛世尊前　　戲擲金剛杵",
        "from": "T20n1187_001:0808a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復於須臾間　　出現無邊數",
        "from": "T20n1187_001:0808a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛杵圍繞　　當戲擲杵時",
        "from": "T20n1187_001:0808a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即有俱胝眾　　金剛光照曜",
        "from": "T20n1187_001:0808a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面現顰眉相　　勇猛大無畏",
        "from": "T20n1187_001:0808b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "威德復殊勝　　調伏於一切",
        "from": "T20n1187_001:0808b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具悲智方便　　自利利世間",
        "from": "T20n1187_001:0808b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身心大歡喜　　得最上妙樂",
        "from": "T20n1187_001:0808b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復現忿怒相　　佛世尊所作",
        "from": "T20n1187_001:0808b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切皆攝受　　普獲於利樂",
        "from": "T20n1187_001:0808b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時金剛手　　頂禮正覺尊",
        "from": "T20n1187_001:0808b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋迦牟尼佛　　合掌而白言",
        "from": "T20n1187_001:0808b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生類　　愚沒生死泥",
        "from": "T20n1187_001:0808b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起煩惱疑惑　　無由能解脫",
        "from": "T20n1187_001:0808b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願哀愍利樂　　我及諸眾生",
        "from": "T20n1187_001:0808b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令如理所得　　了知幻化網",
        "from": "T20n1187_001:0808b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛為世間師　　願宣說正法",
        "from": "T20n1187_001:0808b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利樂諸眾生　　令得無上果",
        "from": "T20n1187_001:0808b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛大三昧　　最上真實智",
        "from": "T20n1187_001:0808b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能了別眾生　　諸根欲性等",
        "from": "T20n1187_001:0808b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊正智眾　　現烏瑟膩沙",
        "from": "T20n1187_001:0808b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙德真實智　　最上自然智",
        "from": "T20n1187_001:0808b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了達甚深義　　一切廣大義",
        "from": "T20n1187_001:0808b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無等寂靜義　　最上最勝義",
        "from": "T20n1187_001:0808b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瑜伽大教中　　諸祕密名義",
        "from": "T20n1187_001:0808b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖三摩地分　　初中後皆善",
        "from": "T20n1187_001:0808b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過去佛已說　　未來佛當說",
        "from": "T20n1187_001:0808b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現在今世尊　　三世盡宣說",
        "from": "T20n1187_001:0808b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此幻化網　　我從是出生",
        "from": "T20n1187_001:0808b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喜執大金剛　　善持一切明",
        "from": "T20n1187_001:0808b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如我所得法　　愛樂而受持",
        "from": "T20n1187_001:0808b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最勝堅固意　　住佛祕密心",
        "from": "T20n1187_001:0808b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如眾生意樂　　願說最勝法",
        "from": "T20n1187_001:0808b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅煩惱無餘　　令得最勝智",
        "from": "T20n1187_001:0808c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是祕密主　　金剛手菩薩",
        "from": "T20n1187_001:0808c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作合掌恭敬　　勸請而前住",
        "from": "T20n1187_001:0808c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時二足尊　　釋迦牟尼佛",
        "from": "T20n1187_001:0808c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從舌及面門　　出廣大光明",
        "from": "T20n1187_001:0808c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普照於十方　　清淨諸惡趣",
        "from": "T20n1187_001:0808c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三界光所照　　四魔悉調伏",
        "from": "T20n1187_001:0808c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復出美梵音　　遍聞於三界",
        "from": "T20n1187_001:0808c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱讚祕密主　　大力智金剛",
        "from": "T20n1187_001:0808c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時佛作是言　　善哉執金剛",
        "from": "T20n1187_001:0808c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉祥最勝者　　勇猛金剛手",
        "from": "T20n1187_001:0808c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝大悲方便　　廣為利世間",
        "from": "T20n1187_001:0808c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故勸請我　　說祕密法門",
        "from": "T20n1187_001:0808c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此祕密名義　　妙德根本智",
        "from": "T20n1187_001:0808c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最勝大吉祥　　能伏諸魔等",
        "from": "T20n1187_001:0808c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今當善說　　汝大祕密主",
        "from": "T20n1187_001:0808c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住最上一心　　諦聽佛所說",
        "from": "T20n1187_001:0808c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時釋迦牟尼　　即說如是法",
        "from": "T20n1187_001:0808c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡諸真言部　　持明大呪部",
        "from": "T20n1187_001:0808c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀照三密部　　世間最上部",
        "from": "T20n1187_001:0808c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世出世間部　　最上大印部",
        "from": "T20n1187_001:0808c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烏瑟膩沙部　　如是等諸部",
        "from": "T20n1187_001:0808c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總攝一切法　　是祕密名義",
        "from": "T20n1187_001:0808c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六種大明王　　無二說其二",
        "from": "T20n1187_001:0808c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是無生法句　　從彼而出生",
        "from": "T20n1187_001:0808c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此法安住諸智心　　三世諸佛所共說",
        "from": "T20n1187_001:0808c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次大明曰。",
        "from": "T20n1187_001:0808c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿字為正智　　諸佛從是生",
        "from": "T20n1187_001:0809a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復為諸勝相　　是最上字義",
        "from": "T20n1187_001:0809a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大生無生義　　離飲食語言",
        "from": "T20n1187_001:0809a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨最上愛樂　　諸語業清淨",
        "from": "T20n1187_001:0809a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最上增勝貪　　諸眾生染行",
        "from": "T20n1187_001:0809a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最上增勝瞋　　諸煩惱大怨",
        "from": "T20n1187_001:0809a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最上增勝癡　　愚迷為智障",
        "from": "T20n1187_001:0809a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最上增勝忿　　大忿怒大怨",
        "from": "T20n1187_001:0809a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最上增勝染　　為諸染所纏",
        "from": "T20n1187_001:0809a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是貪瞋等　　皆最上增勝",
        "from": "T20n1187_001:0809a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祕密妙法門　　通達能遠離",
        "from": "T20n1187_001:0809a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執大智慧劍　　破諸煩惱頂",
        "from": "T20n1187_001:0809a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現大色大身　　大相及大力",
        "from": "T20n1187_001:0809a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大名大威德　　大廣博圓滿",
        "from": "T20n1187_001:0809a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大欲大自在　　大喜及大樂",
        "from": "T20n1187_001:0809a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大愛大名稱　　大光大明照",
        "from": "T20n1187_001:0809a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具一切廣大　　出生大智慧",
        "from": "T20n1187_001:0809a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了知大幻化　　成就大幻義",
        "from": "T20n1187_001:0809a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得大幻妙樂　　識根本幻網",
        "from": "T20n1187_001:0809a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大財大施主　　淨戒最上持",
        "from": "T20n1187_001:0809a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大忍辱大智　　發起大精進",
        "from": "T20n1187_001:0809a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大禪定等持　　大慧堅固聚",
        "from": "T20n1187_001:0809a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大方便大願　　大力大智海",
        "from": "T20n1187_001:0809a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大慈平等法　　大悲心增上",
        "from": "T20n1187_001:0809a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大慧悉通達　　大方便廣作",
        "from": "T20n1187_001:0809a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大神通變化　　大行大速疾",
        "from": "T20n1187_001:0809b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大神通自在　　大力大勇猛",
        "from": "T20n1187_001:0809b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善度諸有苦　　大金剛執持",
        "from": "T20n1187_001:0809b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大惡極忿怒　　作大怖皆畏",
        "from": "T20n1187_001:0809b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大明破諸喑　　大呪最上尊",
        "from": "T20n1187_001:0809b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大乘教濟度　　大乘最上教",
        "from": "T20n1187_001:0809b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法身大遍照　　大寂大牟尼",
        "from": "T20n1187_001:0809b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大明呪希有　　大明呪自在",
        "from": "T20n1187_001:0809b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得十波羅蜜　　依十波羅蜜",
        "from": "T20n1187_001:0809b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨十波羅蜜　　十波羅蜜教",
        "from": "T20n1187_001:0809b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十地自在尊　　十地善安住",
        "from": "T20n1187_001:0809b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十智清淨我　　十智淨堅固",
        "from": "T20n1187_001:0809b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十相十種義　　十力寂諸根",
        "from": "T20n1187_001:0809b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最勝真常義　　十相最寂靜",
        "from": "T20n1187_001:0809b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離諸戲論　　我及真如淨",
        "from": "T20n1187_001:0809b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如語真實語　　如理行無異",
        "from": "T20n1187_001:0809b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無二說二語　　皆住真實際",
        "from": "T20n1187_001:0809b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無我師子吼　　怖彼諸執相",
        "from": "T20n1187_001:0809b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切處解脫　　如來速疾意",
        "from": "T20n1187_001:0809b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降伏諸勝怨　　如轉輪大力",
        "from": "T20n1187_001:0809b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾法中最上　　主宰及敬愛",
        "from": "T20n1187_001:0809b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大威德大智　　諸教無能勝",
        "from": "T20n1187_001:0809b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸語中自在　　無邊正語業",
        "from": "T20n1187_001:0809b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真實語及義　　宣說四聖諦",
        "from": "T20n1187_001:0809b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨順聲聞眾　　不退阿那含",
        "from": "T20n1187_001:0809b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及彼阿羅漢　　漏盡諸苾芻",
        "from": "T20n1187_001:0809b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊上復利根　　勇猛諸緣覺",
        "from": "T20n1187_001:0809b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖種種出生　　同一真實相",
        "from": "T20n1187_001:0809b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆以智慧劍　　斷煩惱無餘",
        "from": "T20n1187_001:0809b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得安樂無畏　　善住真實際",
        "from": "T20n1187_001:0809c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得明行具足　　善逝世間解",
        "from": "T20n1187_001:0809c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無我相可生　　安住二諦法",
        "from": "T20n1187_001:0809c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出度輪迴際　　所作皆已辦",
        "from": "T20n1187_001:0809c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "純一智所生　　慧刀大猛利",
        "from": "T20n1187_001:0809c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正法光明聚　　照世出世間",
        "from": "T20n1187_001:0809c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法王法自在　　宣說最勝道",
        "from": "T20n1187_001:0809c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分別成就義　　離諸分別相",
        "from": "T20n1187_001:0809c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼分別界盡　　顯最上法界",
        "from": "T20n1187_001:0809c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福語福所生　　大智智所作",
        "from": "T20n1187_001:0809c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當起正智想　　二法從是生",
        "from": "T20n1187_001:0809c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畢竟離眾相　　相應定自在",
        "from": "T20n1187_001:0809c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不動大主宰　　三身勝根本",
        "from": "T20n1187_001:0809c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五身正智主　　五智大自在",
        "from": "T20n1187_001:0809c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五佛冠妙嚴　　五眼淨無著",
        "from": "T20n1187_001:0809c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛最勝尊　　出生諸佛子",
        "from": "T20n1187_001:0809c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧最上出生　　正法中所作",
        "from": "T20n1187_001:0809c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛一實理　　世主最上生",
        "from": "T20n1187_001:0809c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空自然智　　大智大慧眼",
        "from": "T20n1187_001:0809c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大光明遍照　　大智光大日",
        "from": "T20n1187_001:0809c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智光照世燈　　大光明熾盛",
        "from": "T20n1187_001:0809c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最上呪明王　　大呪王所作",
        "from": "T20n1187_001:0809c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大頂真實頂　　自在現眾相",
        "from": "T20n1187_001:0809c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛無我性　　普照無邊界",
        "from": "T20n1187_001:0809c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中現眾色　　供養大仙尊",
        "from": "T20n1187_001:0809c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持三部明呪　　大三昧呪法",
        "from": "T20n1187_001:0809c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持最勝三寶　　說最上三乘",
        "from": "T20n1187_001:0809c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最勝不空索　　金剛索大攝",
        "from": "T20n1187_001:0809c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛鉤大索　　大金剛能怖",
        "from": "T20n1187_001:0809c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛阿閦佛　　即大圓鏡智",
        "from": "T20n1187_001:0810a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從是智所生　　一切方便門",
        "from": "T20n1187_001:0810a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現大忿怒相　　其相有六面",
        "from": "T20n1187_001:0810a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六眼及六臂　　圓滿復大力",
        "from": "T20n1187_001:0810a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利牙而外出　　大惡極可怖",
        "from": "T20n1187_001:0810a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復現大笑相　　百眼所莊嚴",
        "from": "T20n1187_001:0810a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現焰鬘得迦　　怖畏金剛相",
        "from": "T20n1187_001:0810a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最上心金剛　　廣大幻金剛",
        "from": "T20n1187_001:0810a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最勝金剛部　　無喻金剛場",
        "from": "T20n1187_001:0810a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不動一髻尊　　象皮為嚴飾",
        "from": "T20n1187_001:0810a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "訶訶聲大惡　　呬呬聲大怖",
        "from": "T20n1187_001:0810a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿吒聲大笑　　金剛喜大聲",
        "from": "T20n1187_001:0810a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛士大士　　金剛王大樂",
        "from": "T20n1187_001:0810a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛相大喜　　吽字金剛智",
        "from": "T20n1187_001:0810a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持金剛智箭　　執金剛利劍",
        "from": "T20n1187_001:0810a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現金剛巧業　　金剛破他軍",
        "from": "T20n1187_001:0810a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛火惡視　　金剛熾盛頂",
        "from": "T20n1187_001:0810a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛大入寤　　百眼金剛照",
        "from": "T20n1187_001:0810a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛諸嚕摩　　一嚕摩所攝",
        "from": "T20n1187_001:0810a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱胝那珂數　　住金剛娑囉",
        "from": "T20n1187_001:0810a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖金剛持鬘　　金剛寶莊嚴",
        "from": "T20n1187_001:0810a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "訶訶吒大聲　　百字金剛聲",
        "from": "T20n1187_001:0810a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙音聲大吼　　普振於三界",
        "from": "T20n1187_001:0810a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空界無邊　　音聲亦無礙",
        "from": "T20n1187_001:0810a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真如無我相　　實際無罣礙",
        "from": "T20n1187_001:0810a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空性離語言　　說廣大甚深",
        "from": "T20n1187_001:0810a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出大法螺聲　　擊大法健稚",
        "from": "T20n1187_001:0810a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛威力無勝　　十方鳴法鼓",
        "from": "T20n1187_001:0810a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無色中現色　　種種色殊妙",
        "from": "T20n1187_001:0810a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾色吉祥光　　現無餘影像",
        "from": "T20n1187_001:0810b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最勝大自在　　三界大主宰",
        "from": "T20n1187_001:0810b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯示諸聖道　　建廣大法幢",
        "from": "T20n1187_001:0810b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彼三界中　　出現童子身",
        "from": "T20n1187_001:0810b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或現耆年相　　尊長大世主",
        "from": "T20n1187_001:0810b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛三十二相　　圓滿而具足",
        "from": "T20n1187_001:0810b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普遍三界中　　瞻仰生歡喜",
        "from": "T20n1187_001:0810b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間智功德　　世間行無畏",
        "from": "T20n1187_001:0810b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三界中最上　　為主為依怙",
        "from": "T20n1187_001:0810b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空藏出生　　一切智智海",
        "from": "T20n1187_001:0810b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無明藏所集　　三有諸結縛",
        "from": "T20n1187_001:0810b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅煩惱無餘　　出度輪迴岸",
        "from": "T20n1187_001:0810b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戴智冠灌頂　　正等覺莊嚴",
        "from": "T20n1187_001:0810b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三苦等滅除　　三世得解脫",
        "from": "T20n1187_001:0810b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離一切障　　等虛空清淨",
        "from": "T20n1187_001:0810b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過去煩惱垢　　三世悉同然",
        "from": "T20n1187_001:0810b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸眾生大龍　　現功德山頂",
        "from": "T20n1187_001:0810b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切得解脫　　如妙寶蓮華",
        "from": "T20n1187_001:0810b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼大如意寶　　眾寶中最上",
        "from": "T20n1187_001:0810b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅牢大劫樹　　最上大賢瓶",
        "from": "T20n1187_001:0810b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利一切眾生　　皆方便所作",
        "from": "T20n1187_001:0810b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨智淨分智　　三昧智最上",
        "from": "T20n1187_001:0810b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了別眾生根　　具三種解脫",
        "from": "T20n1187_001:0810b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功德智法智　　廣大利吉祥",
        "from": "T20n1187_001:0810b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉中大吉祥　　獲名稱廣愛",
        "from": "T20n1187_001:0810b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具聖財大富　　大喜復大樂",
        "from": "T20n1187_001:0810b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善義而善作　　喜稱大吉祥",
        "from": "T20n1187_001:0810b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最勝大誓願　　作最上救護",
        "from": "T20n1187_001:0810b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸有大怖畏　　悉令滅無餘",
        "from": "T20n1187_001:0810b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙頂功德頂　　最勝寂默頂",
        "from": "T20n1187_001:0810c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五眼五髻尊　　五唧囉頂髻",
        "from": "T20n1187_001:0810c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸有大苦行　　執自見解脫",
        "from": "T20n1187_001:0810c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以自所修行　　謂最上梵行",
        "from": "T20n1187_001:0810c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自謂得大仙　　已得究竟法",
        "from": "T20n1187_001:0810c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂沐身外道　　一切見等執",
        "from": "T20n1187_001:0810c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從梵天所生　　滅亦生梵天",
        "from": "T20n1187_001:0810c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得離繫解脫　　無礙寂靜法",
        "from": "T20n1187_001:0810c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證寂靜涅槃　　得最上圓寂",
        "from": "T20n1187_001:0810c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了苦樂邊際　　貪等法滅盡",
        "from": "T20n1187_001:0810c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無勝無比喻　　無相亦無著",
        "from": "T20n1187_001:0810c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂所得涅槃　　出過一切道",
        "from": "T20n1187_001:0810c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等邪執　　皆不正解脫",
        "from": "T20n1187_001:0810c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼不能了知　　無漏種微細",
        "from": "T20n1187_001:0810c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清淨離塵染　　遠離諸過失",
        "from": "T20n1187_001:0810c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯正智所覺　　一切智通達",
        "from": "T20n1187_001:0810c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼一切識法　　無二智可觀",
        "from": "T20n1187_001:0810c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離疑惑分別　　三世佛同體",
        "from": "T20n1187_001:0810c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無初亦無後　　一智眼所觀",
        "from": "T20n1187_001:0810c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來智頂生　　同體而無垢",
        "from": "T20n1187_001:0810c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "語自在大語　　勝語最上語",
        "from": "T20n1187_001:0810c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無勝師子吼　　無上無比等",
        "from": "T20n1187_001:0810c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普現身大喜　　或現火鬘相",
        "from": "T20n1187_001:0810c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或吉祥妙光　　大光明熾盛",
        "from": "T20n1187_001:0810c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切勝自在　　離染法最上",
        "from": "T20n1187_001:0810c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如無勝妙藥　　治諸煩惱怨",
        "from": "T20n1187_001:0810c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三界大吉祥　　眾星曼拏羅",
        "from": "T20n1187_001:0810c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建立大法幢　　表剎十方界",
        "from": "T20n1187_001:0810c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一廣大寶蓋　　遍覆於世間",
        "from": "T20n1187_001:0810c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈悲二種法　　作大曼拏羅",
        "from": "T20n1187_001:0811a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大吉祥蓮華　　自在為嚴飾",
        "from": "T20n1187_001:0811a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最上寶蓋等　　周匝而莊嚴",
        "from": "T20n1187_001:0811a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛大主宰　　諸佛心自性",
        "from": "T20n1187_001:0811a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛大相應　　諸佛同一教",
        "from": "T20n1187_001:0811a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛寶灌頂　　眾寶自在主",
        "from": "T20n1187_001:0811a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸觀自在王　　諸執金剛尊",
        "from": "T20n1187_001:0811a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛廣大心　　諸佛最上意",
        "from": "T20n1187_001:0811a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛廣博身　　諸佛真實語",
        "from": "T20n1187_001:0811a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛日大明　　金剛淨根本",
        "from": "T20n1187_001:0811a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾色熾盛光　　照貪等自性",
        "from": "T20n1187_001:0811a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正覺跏趺坐　　說妙法心義",
        "from": "T20n1187_001:0811a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切智智藏　　正覺蓮華生",
        "from": "T20n1187_001:0811a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾幻化中主　　持明大覺尊",
        "from": "T20n1187_001:0811a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大金剛利劍　　斷一切苦惱",
        "from": "T20n1187_001:0811a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最上清淨字　　金剛大法器",
        "from": "T20n1187_001:0811a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛甚深義　　金剛慧所了",
        "from": "T20n1187_001:0811a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具諸波羅蜜　　為大地莊嚴",
        "from": "T20n1187_001:0811a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法無我清淨　　正智根本心",
        "from": "T20n1187_001:0811a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幻化網大教　　一切教中主",
        "from": "T20n1187_001:0811a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最勝金剛生　　最勝大智聚",
        "from": "T20n1187_001:0811a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大普賢妙意　　地藏世間主",
        "from": "T20n1187_001:0811a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛大法藏　　現眾變化輪",
        "from": "T20n1187_001:0811a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸性勝自性　　諸性自性心",
        "from": "T20n1187_001:0811a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無生法眾義　　即諸法自性",
        "from": "T20n1187_001:0811a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大慧剎那間　　證悟一切法",
        "from": "T20n1187_001:0811a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現諸法三昧　　真實大寂默",
        "from": "T20n1187_001:0811a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現證一切佛　　正等正覺尊",
        "from": "T20n1187_001:0811a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發大智光明　　照無我自性",
        "from": "T20n1187_001:0811a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說最勝妙吉祥根本智最上祕密一切名義三摩地分卷上",
        "from": "T20n1187_001:0811b02_100",
        "context_with_next_sentence": false
    }
]