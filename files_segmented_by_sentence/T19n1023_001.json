[
    {
        "source_sentence": "一切如來正法祕密篋印心陀羅",
        "from": "T19n1023_001:0715a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尼經",
        "from": "T19n1023_001:0715a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天北印度烏填曩國帝釋宮寺",
        "from": "T19n1023_001:0715a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏傳法大師賜紫沙門臣施護奉　詔譯如是我聞。",
        "from": "T19n1023_001:0715a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在摩誐陀國無垢園中寶光明池側。",
        "from": "T19n1023_001:0715a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大菩薩及諸聲聞。",
        "from": "T19n1023_001:0715a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天龍夜叉犍闥婆阿蘇囉𦾨嚕茶緊那羅麼護囉誐人非人等。",
        "from": "T19n1023_001:0715a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并諸王眾長者居士。",
        "from": "T19n1023_001:0715a16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百千圍遶恭敬聽法。",
        "from": "T19n1023_001:0715a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時眾中有大富婆羅門。",
        "from": "T19n1023_001:0715a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名無垢妙光。",
        "from": "T19n1023_001:0715a17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於會中能解微妙曉達師法。",
        "from": "T19n1023_001:0715a18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聰明辯才愛樂多聞。",
        "from": "T19n1023_001:0715a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修崇十善信重三寶。",
        "from": "T19n1023_001:0715a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡喜踊躍而作是念。",
        "from": "T19n1023_001:0715a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切有情修善為因。",
        "from": "T19n1023_001:0715a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "財富無量資具眾多。",
        "from": "T19n1023_001:0715a20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當修習。",
        "from": "T19n1023_001:0715a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時大婆羅門無垢妙光。",
        "from": "T19n1023_001:0715a21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起立合掌遶佛七匝。",
        "from": "T19n1023_001:0715a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以種種花塗香粖香等供養世尊。",
        "from": "T19n1023_001:0715a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸衣服真珠瓔珞。",
        "from": "T19n1023_001:0715a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "價直百千持以施佛。",
        "from": "T19n1023_001:0715a23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭面禮足而白佛言。",
        "from": "T19n1023_001:0715a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊願起大悲願受我食。",
        "from": "T19n1023_001:0715a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是三請。",
        "from": "T19n1023_001:0715a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊默然受請。",
        "from": "T19n1023_001:0715a25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是無垢妙光大婆羅門。",
        "from": "T19n1023_001:0715a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見其世尊默然受請。",
        "from": "T19n1023_001:0715a26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而速歸還經於一宿。",
        "from": "T19n1023_001:0715a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "備辦種種眾多。",
        "from": "T19n1023_001:0715a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "美饍六味具足飲食。",
        "from": "T19n1023_001:0715a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辦已嚴飾舍宅樓閣浴池繒綵甚眾。",
        "from": "T19n1023_001:0715a28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "花香粖香種種諸香及塗身香等。",
        "from": "T19n1023_001:0715a29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸供養具無不周備。",
        "from": "T19n1023_001:0715b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即時婆羅門等。",
        "from": "T19n1023_001:0715b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執持花幡奏諸妓樂。",
        "from": "T19n1023_001:0715b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與其眷屬往詣無垢園中寶光明池側迎請世尊。",
        "from": "T19n1023_001:0715b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到已白佛言。",
        "from": "T19n1023_001:0715b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊食時已至飯食悉辦。",
        "from": "T19n1023_001:0715b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊與諸大眾。",
        "from": "T19n1023_001:0715b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往至我舍受我供養。",
        "from": "T19n1023_001:0715b04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊觀察眾會。",
        "from": "T19n1023_001:0715b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安慰無垢妙光大婆羅門曰。",
        "from": "T19n1023_001:0715b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉如汝所願。",
        "from": "T19n1023_001:0715b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又告諸眾并餘天等悉皆往彼。",
        "from": "T19n1023_001:0715b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是世尊從座而起。",
        "from": "T19n1023_001:0715b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與諸大眾菩薩聲聞。",
        "from": "T19n1023_001:0715b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天龍八部人非人等。",
        "from": "T19n1023_001:0715b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸王眾臣民眷屬。",
        "from": "T19n1023_001:0715b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量百千恭敬圍遶皆來集會。",
        "from": "T19n1023_001:0715b09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當行往彼無垢妙光大婆羅門家。",
        "from": "T19n1023_001:0715b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊身縱金色。",
        "from": "T19n1023_001:0715b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放種種光無量諸相。",
        "from": "T19n1023_001:0715b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變作種種光明之網。",
        "from": "T19n1023_001:0715b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍照十方一切佛剎。",
        "from": "T19n1023_001:0715b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復照曜彼佛剎中。",
        "from": "T19n1023_001:0715b12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛如來道場眾會。",
        "from": "T19n1023_001:0715b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變作種種供養之具。",
        "from": "T19n1023_001:0715b13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照曜彼已還復來入釋迦牟尼身毛孔中。",
        "from": "T19n1023_001:0715b14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即時無垢妙光大婆羅門。",
        "from": "T19n1023_001:0715b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與其眷屬一心恭敬欲供養佛。",
        "from": "T19n1023_001:0715b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掃除道路嚴飾清淨。",
        "from": "T19n1023_001:0715b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊。",
        "from": "T19n1023_001:0715b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與諸大眾天龍夜叉犍闥婆阿蘇洛𦾨嚕茶緊那羅麼護囉誐。",
        "from": "T19n1023_001:0715b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝釋梵王護世四王。",
        "from": "T19n1023_001:0715b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "麼醯首羅那羅延等百千之眾。",
        "from": "T19n1023_001:0715b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恭敬圍遶往詣於彼。",
        "from": "T19n1023_001:0715b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊當路而行。",
        "from": "T19n1023_001:0715b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於前中路有一園苑名為安樂。",
        "from": "T19n1023_001:0715b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼園苑內有大舊塔極甚損壞。",
        "from": "T19n1023_001:0715b21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四邊荊棘草木叢林。",
        "from": "T19n1023_001:0715b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅硬閉塞猶如丘埠。",
        "from": "T19n1023_001:0715b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊將近彼塔。",
        "from": "T19n1023_001:0715b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "丘埠四面周匝焰熾。",
        "from": "T19n1023_001:0715b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種色相光明照曜。",
        "from": "T19n1023_001:0715b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彼破塔遠近皆覩。",
        "from": "T19n1023_001:0715b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又於光中出妙音聲。",
        "from": "T19n1023_001:0715b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讚言善哉善哉釋迦牟尼佛。",
        "from": "T19n1023_001:0715b25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清淨微妙瑞相無量不可稱量。",
        "from": "T19n1023_001:0715b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼無垢妙光大婆羅門請於如來。",
        "from": "T19n1023_001:0715b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今現希奇難思大事。",
        "from": "T19n1023_001:0715b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時釋迦牟尼如來。",
        "from": "T19n1023_001:0715b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施遶破舊丘埠之塔欲伸供養。",
        "from": "T19n1023_001:0715b28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復解自身衣服嚴具。",
        "from": "T19n1023_001:0715b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覆蓋於彼舊塔塠埠。",
        "from": "T19n1023_001:0715b29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓋覆已畢悲泣雨淚。",
        "from": "T19n1023_001:0715c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復思念舍利出現及十方佛悉皆出現。",
        "from": "T19n1023_001:0715c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即時舍利從塔湧出。",
        "from": "T19n1023_001:0715c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆在十方諸佛如來手掌中住。",
        "from": "T19n1023_001:0715c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時十方諸佛如來亦皆雨淚。",
        "from": "T19n1023_001:0715c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而彼諸佛身放光明。",
        "from": "T19n1023_001:0715c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照曜故塔作光明網。",
        "from": "T19n1023_001:0715c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內外瑩徹清淨無比。",
        "from": "T19n1023_001:0715c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後光明及佛舍利還入塔中。",
        "from": "T19n1023_001:0715c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼時眾會。",
        "from": "T19n1023_001:0715c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆悲泣。",
        "from": "T19n1023_001:0715c06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "驚怪希奇歎未曾有。",
        "from": "T19n1023_001:0715c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時會中有大藥叉主名金剛手。",
        "from": "T19n1023_001:0715c07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身光焰熾臂膊𦟛直。",
        "from": "T19n1023_001:0715c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "柔軟光澤猶如象鼻。",
        "from": "T19n1023_001:0715c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輪金剛杵速疾往詣世尊之前。",
        "from": "T19n1023_001:0715c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭面禮足而白佛言。",
        "from": "T19n1023_001:0715c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊云何現此希奇瑞相。",
        "from": "T19n1023_001:0715c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及十方佛悉皆雨淚光明照曜。",
        "from": "T19n1023_001:0715c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願如來為我演說決於眾疑。",
        "from": "T19n1023_001:0715c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告大藥叉主金剛手言。",
        "from": "T19n1023_001:0715c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是一切如來全身舍利聚此塔中。",
        "from": "T19n1023_001:0715c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及一切如來百千俱胝胡麻形像。",
        "from": "T19n1023_001:0715c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正法心印陀羅尼亦在塔中。",
        "from": "T19n1023_001:0715c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應知金剛手此胡麻量諸佛形像。",
        "from": "T19n1023_001:0715c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正法心印陀羅尼止住塔中。",
        "from": "T19n1023_001:0715c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此百千俱胝如來全身舍利亦住此中。",
        "from": "T19n1023_001:0715c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若此正法心印陀羅尼在此塔中。",
        "from": "T19n1023_001:0715c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼八萬四千法門亦在此中。",
        "from": "T19n1023_001:0715c18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及九十九百千俱胝如來頂髻受記亦在此中。",
        "from": "T19n1023_001:0715c19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知金剛手若一切如來舍利之塔。",
        "from": "T19n1023_001:0715c20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及此正法心印陀羅尼所有功德無量無邊不可思議不可稱歎。",
        "from": "T19n1023_001:0715c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應知金剛手由此正法篋印心陀羅尼。",
        "from": "T19n1023_001:0715c22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而能增長圓滿一切正法及諸功德。",
        "from": "T19n1023_001:0715c23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊說此法時。",
        "from": "T19n1023_001:0715c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼諸大眾聞法歡喜。",
        "from": "T19n1023_001:0715c24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅除垢染得法眼淨。",
        "from": "T19n1023_001:0715c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其中有得預流一來不還阿羅漢果及緣覺乘其中有證初地二地乃至十地。",
        "from": "T19n1023_001:0715c26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於其中有得六波羅蜜悉皆圓滿。",
        "from": "T19n1023_001:0715c27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有得受菩提之記。",
        "from": "T19n1023_001:0715c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有獲得阿鞞跋致。",
        "from": "T19n1023_001:0715c28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時彼無垢妙光大婆羅門。",
        "from": "T19n1023_001:0715c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於會中離諸煩惱。",
        "from": "T19n1023_001:0715c29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅除障染獲五神通。",
        "from": "T19n1023_001:0716a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時金剛手大藥叉主。",
        "from": "T19n1023_001:0716a01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覩如是等希奇變現自在神通。",
        "from": "T19n1023_001:0716a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "踊躍歡喜歎未曾有。",
        "from": "T19n1023_001:0716a02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而白佛言。",
        "from": "T19n1023_001:0716a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊此正法印心陀羅尼。",
        "from": "T19n1023_001:0716a03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有如是等廣大利益。",
        "from": "T19n1023_001:0716a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若諸有情書寫莊嚴。",
        "from": "T19n1023_001:0716a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持讀誦尊重讚歎。",
        "from": "T19n1023_001:0716a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲何果報願佛演說。",
        "from": "T19n1023_001:0716a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告大藥叉金剛手言。",
        "from": "T19n1023_001:0716a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諦聽諦聽吾為汝說。",
        "from": "T19n1023_001:0716a06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有苾芻苾芻尼。",
        "from": "T19n1023_001:0716a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鄔波索迦鄔波斯迦。",
        "from": "T19n1023_001:0716a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持讀誦書寫尊重此正法印心陀羅尼。",
        "from": "T19n1023_001:0716a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲得九十九百千俱胝胡麻量等形像如來。",
        "from": "T19n1023_001:0716a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福德之聚善根果報。",
        "from": "T19n1023_001:0716a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又得逢事彼諸如來受菩提記。",
        "from": "T19n1023_001:0716a10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何。",
        "from": "T19n1023_001:0716a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此正法印心陀羅尼。",
        "from": "T19n1023_001:0716a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是彼九十九百千俱胝那瑜多胡麻量等形像如來同共宣說。",
        "from": "T19n1023_001:0716a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人受持讀誦此正法印心陀羅尼經。",
        "from": "T19n1023_001:0716a13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得值遇十方世界九十九百千俱胝那瑜多胡麻量等形像如來。",
        "from": "T19n1023_001:0716a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有苾芻苾芻尼。",
        "from": "T19n1023_001:0716a15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鄔波索迦鄔波斯迦。",
        "from": "T19n1023_001:0716a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "族姓男族姝女。",
        "from": "T19n1023_001:0716a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此正法印心陀羅尼經。",
        "from": "T19n1023_001:0716a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能日日恭敬供養花香。",
        "from": "T19n1023_001:0716a17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "粖香花鬘塗香。",
        "from": "T19n1023_001:0716a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "細妙衣服及莊嚴具等。",
        "from": "T19n1023_001:0716a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所獲功德如彼供養十方世界九十九百千俱胝胡麻等形像如來功德無異。",
        "from": "T19n1023_001:0716a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又得一一如來廣大莊嚴具等。",
        "from": "T19n1023_001:0716a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及獲無量七寶之聚如蘇彌盧。",
        "from": "T19n1023_001:0716a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時眾會天龍藥叉犍闥婆阿素囉𦾨嚕茶緊那羅摩護囉誐人非人等一切大眾。",
        "from": "T19n1023_001:0716a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆驚疑歎未曾有。",
        "from": "T19n1023_001:0716a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是朽爛破壞之塔。",
        "from": "T19n1023_001:0716a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常被荊棘閉塞。",
        "from": "T19n1023_001:0716a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "草木叢林堅密充滿猶如丘埠。",
        "from": "T19n1023_001:0716a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今日如來起大慈悲自在神通。",
        "from": "T19n1023_001:0716a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現如是等希奇瑞相七寶妙塔不可思議。",
        "from": "T19n1023_001:0716a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時諸大眾稱歎無盡。",
        "from": "T19n1023_001:0716a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即時會中有金剛手大藥叉主。",
        "from": "T19n1023_001:0716a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而白佛言。",
        "from": "T19n1023_001:0716a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊云何破壞丘埠之塔。",
        "from": "T19n1023_001:0716a29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變現如是眾妙莊嚴。",
        "from": "T19n1023_001:0716a29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛告金剛手言。",
        "from": "T19n1023_001:0716b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離此彼壞丘埠之塔。",
        "from": "T19n1023_001:0716b01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無別七寶眾妙之塔。",
        "from": "T19n1023_001:0716b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何。",
        "from": "T19n1023_001:0716b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨識變現或見荊棘丘埠之塔。",
        "from": "T19n1023_001:0716b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或見七寶眾妙莊嚴。",
        "from": "T19n1023_001:0716b03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又諸佛所現十方如來全身舍利所藏之塔。",
        "from": "T19n1023_001:0716b04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百珍七寶眾妙莊嚴。",
        "from": "T19n1023_001:0716b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生惡業乃見荊棘破壞之塔。",
        "from": "T19n1023_001:0716b05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知金剛手大藥叉主。",
        "from": "T19n1023_001:0716b06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於末世最後分時。",
        "from": "T19n1023_001:0716b06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂諸有情罪業深重不植善根。",
        "from": "T19n1023_001:0716b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣作不善三惡道業。",
        "from": "T19n1023_001:0716b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此因緣生無佛世。",
        "from": "T19n1023_001:0716b08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦常不聞諸佛名字。",
        "from": "T19n1023_001:0716b08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及恒遠離僧法之名。",
        "from": "T19n1023_001:0716b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是之事真實不虛。",
        "from": "T19n1023_001:0716b09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以諸佛及我雨淚悲歎。",
        "from": "T19n1023_001:0716b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次金剛手。",
        "from": "T19n1023_001:0716b10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若正法將滅安住此塔。",
        "from": "T19n1023_001:0716b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來全身舍利亦乃藏於此塔之中。",
        "from": "T19n1023_001:0716b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂以一切如來所說之法而加護之。",
        "from": "T19n1023_001:0716b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即時金剛手大藥叉主而白佛言。",
        "from": "T19n1023_001:0716b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊一切有情。",
        "from": "T19n1023_001:0716b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何植善云何正法安置此塔。",
        "from": "T19n1023_001:0716b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告金剛手言。",
        "from": "T19n1023_001:0716b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人信解受持讀誦書寫為人演說。",
        "from": "T19n1023_001:0716b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種莊嚴安置塔中。",
        "from": "T19n1023_001:0716b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是安置一切如來全身舍利藏此塔中。",
        "from": "T19n1023_001:0716b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復得一切如來總持祕密而加護之。",
        "from": "T19n1023_001:0716b18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復得十方世界九十九胡麻量等形像諸佛之塔而加護之。",
        "from": "T19n1023_001:0716b19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復得一切如來頂髻眼塔而加護之。",
        "from": "T19n1023_001:0716b20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人用於七寶如法加持。",
        "from": "T19n1023_001:0716b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雕作諸佛及菩薩像。",
        "from": "T19n1023_001:0716b21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種莊嚴安置塔中。",
        "from": "T19n1023_001:0716b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復用繖蓋相輪寶網鈴鐸。",
        "from": "T19n1023_001:0716b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾妙嚴飾。",
        "from": "T19n1023_001:0716b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鐘磬鐃鈸及諸器物。",
        "from": "T19n1023_001:0716b23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "花香瓔珞衣服飲食諸供具等。",
        "from": "T19n1023_001:0716b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以一切如來正法加持真實護淨。",
        "from": "T19n1023_001:0716b25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精進恒時離於懈怠。",
        "from": "T19n1023_001:0716b25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復此塔時遷變壞。",
        "from": "T19n1023_001:0716b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重修嚴飾種種供養。",
        "from": "T19n1023_001:0716b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精心信敬不生疑謗。",
        "from": "T19n1023_001:0716b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼當獲得無上菩提。",
        "from": "T19n1023_001:0716b27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人旋遶此塔。",
        "from": "T19n1023_001:0716b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一二匝等或一禮拜。",
        "from": "T19n1023_001:0716b28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得解脫無間之罪。",
        "from": "T19n1023_001:0716b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必當證得阿毘跋致。",
        "from": "T19n1023_001:0716b29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至究竟佛果菩提。",
        "from": "T19n1023_001:0716b29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有此塔所住之處。",
        "from": "T19n1023_001:0716c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切十方諸佛形像亦在此中。",
        "from": "T19n1023_001:0716c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次若有此塔之處由彼一切如來加護力故。",
        "from": "T19n1023_001:0716c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼地四方而無毒龍雷雹風雨非時損害及諸怖畏。",
        "from": "T19n1023_001:0716c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼處復無諸毒蟲蟻。",
        "from": "T19n1023_001:0716c04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂毒蛇狼鼠蝗蟲蝮蝎黃蜂黑蜂。",
        "from": "T19n1023_001:0716c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至蜈蚣百足悉皆遠離。",
        "from": "T19n1023_001:0716c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次彼地而無暴惡虎狼師子。",
        "from": "T19n1023_001:0716c06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飛禽走獸不能傷害。",
        "from": "T19n1023_001:0716c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復彼地一切災難而無嬈亂又無夜叉羅剎部多吠多拏又毘舍遮。",
        "from": "T19n1023_001:0716c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顛狂鬼魅不能傷害。",
        "from": "T19n1023_001:0716c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又無瘧病瘡腫痑𤴯痔瘻白癩疥癬。",
        "from": "T19n1023_001:0716c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切疾病不能侵害。",
        "from": "T19n1023_001:0716c10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人見此寶塔歡喜讚禮。",
        "from": "T19n1023_001:0716c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸損壞等不能侵害。",
        "from": "T19n1023_001:0716c11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復彼地牛馬羊犬種種傍生。",
        "from": "T19n1023_001:0716c12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸男女而無中夭。",
        "from": "T19n1023_001:0716c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及無刀兵器杖水火焚漂怖畏等事。",
        "from": "T19n1023_001:0716c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼地亦無飢饉疾疫種種怖畏。",
        "from": "T19n1023_001:0716c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼處常得四大天王圍遶擁護。",
        "from": "T19n1023_001:0716c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及二十八部大藥叉主。",
        "from": "T19n1023_001:0716c15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密隱形儀恒來擁護。",
        "from": "T19n1023_001:0716c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及三十三天二十八宿羅睺諸星。",
        "from": "T19n1023_001:0716c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至日月晝夜順行。",
        "from": "T19n1023_001:0716c17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若諸龍王恒常觀察應時降雨。",
        "from": "T19n1023_001:0716c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有欲色乃至無色一切諸天。",
        "from": "T19n1023_001:0716c18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於三際時來降此塔禮拜供養。",
        "from": "T19n1023_001:0716c19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來恭敬尊重憶念思惟。",
        "from": "T19n1023_001:0716c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人恒常修嚴佛塔形像。",
        "from": "T19n1023_001:0716c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或用木石泥土彩畫金銀鍮石等。",
        "from": "T19n1023_001:0716c21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及此正法印心陀羅尼經書寫莊嚴。",
        "from": "T19n1023_001:0716c22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以一切如來如法護持。",
        "from": "T19n1023_001:0716c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後安置寶塔之中。",
        "from": "T19n1023_001:0716c23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此寶塔四邊階道悉皆嚴飾。",
        "from": "T19n1023_001:0716c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相輪幢幡周匝懸鈴。",
        "from": "T19n1023_001:0716c24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七寶羅網高廣大小。",
        "from": "T19n1023_001:0716c25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意自在種種莊嚴。",
        "from": "T19n1023_001:0716c25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以十方如來正法而加持之。",
        "from": "T19n1023_001:0716c26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復作一切聖賢擁護之法而加護持。",
        "from": "T19n1023_001:0716c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後種種廣陳香花飲食乃至瓔珞精虔供養。",
        "from": "T19n1023_001:0716c28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是乃獲無量功德。",
        "from": "T19n1023_001:0716c28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即時會中有金剛手大藥叉主。",
        "from": "T19n1023_001:0716c29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復白佛言世尊有何因緣。",
        "from": "T19n1023_001:0717a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此正法印心陀羅尼。",
        "from": "T19n1023_001:0717a01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書寫受持供養恭敬。",
        "from": "T19n1023_001:0717a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲如是等無量功德。",
        "from": "T19n1023_001:0717a02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告金剛手言。",
        "from": "T19n1023_001:0717a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是法界一切如來全身舍利祕密篋印心陀羅尼。",
        "from": "T19n1023_001:0717a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以獲是無量功德。",
        "from": "T19n1023_001:0717a04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時金剛手大藥叉主。",
        "from": "T19n1023_001:0717a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛所說無量功德。",
        "from": "T19n1023_001:0717a05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡喜信受復白佛言。",
        "from": "T19n1023_001:0717a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊願佛慈悲演說此法。",
        "from": "T19n1023_001:0717a06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等樂聞。",
        "from": "T19n1023_001:0717a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告金剛手言。",
        "from": "T19n1023_001:0717a07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等諦聽今為汝說。",
        "from": "T19n1023_001:0717a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此正法印心陀羅尼。",
        "from": "T19n1023_001:0717a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是過去未來現在諸佛同共宣說。",
        "from": "T19n1023_001:0717a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有信受恭敬供養。",
        "from": "T19n1023_001:0717a09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至證入聲聞緣覺及於十地法報化身。",
        "from": "T19n1023_001:0717a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以名為全身正法祕密篋印。",
        "from": "T19n1023_001:0717a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊即說呪曰。",
        "from": "T19n1023_001:0717a11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫悉底哩三合野一地尾二合迦去喃去引二薩嚩怛他去引𦾨跢上南三唵四部悉底哩三合步平嚩曩嚩㘑五嚩嚩㘑六祖魯祖魯七他囉他囉八薩嚩怛他去引𦾨跢上馱引上覩他哩九鉢納麼二合三去婆吠十惹野他哩捫上佐裔十一娑麼二合囉達轉舌麼作訖囉二合十二鉢囉二合韈無鉢反轉舌哆曩嚩日哩二合十三冒去引地曼上監上迦囉上楞上訖哩二合帝十四薩嚩怛他去引𦾨跢上引地　瑟恥二合帝十五冒去引他野冒去引他野十六冒去引地冒准上地十七沒𠆙切身十八參去冒引去他野參去冒去引他野十九左攞左攞左懶去覩二十薩嚩薩怛嚩二合引喃二十一薩嚩引嚩囉拏儞二十二薩嚩播跛尾𦾨帝二十三護嚕護嚕二十四薩嚩戍引上迦尾𦾨帝二十五薩嚩怛他去引𦾨跢紇哩二合那野嚩日哩二合抳二十六參去婆去囉參去婆去囉二十七薩嚩多塢瑟膩二合灑二十八陀去引羅抳母捺哩二合二十九沒第蘇沒第三十薩轉舌嚩怛他去引𦾨哆上引地瑟恥二合哆三十一馱上引覩𦾨轉舌陛娑嚩二合引賀引三十二參去麼野引地瑟恥二合帝娑嚩二合引賀引三十三薩嚩怛他去引𦾨跢上引三十四紇哩二合那馱上引覩母捺哩二合娑嚩二合引賀引三十五阿鉢囉二合底瑟恥二合多窣覩二合閉三十六怛他去引𦾨哆上引地瑟恥二合帝三十七虎嚕虎嚕吽引吽引娑嚩二合引賀引三十八唵引三十九薩嚩怛他去引𦾨哆四十塢瑟膩二合灑陀去囉抳母捺哩二合四十一薩嚩怛他去引𦾨哆四十二馱上引覩尾部尸跢上引四十三地瑟恥二合帝吽吽吽娑嚩二合引賀引四十四爾時世尊說此呪已告金剛手言。",
        "from": "T19n1023_001:0717b10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此全身祕密篋印心陀羅尼。",
        "from": "T19n1023_001:0717b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸如來普皆宣說。",
        "from": "T19n1023_001:0717b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍十方界於一一方。",
        "from": "T19n1023_001:0717b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有九十九胡麻量等形像如來百千俱胝那瑜多諸佛如來。",
        "from": "T19n1023_001:0717b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞說此法皆來至此。",
        "from": "T19n1023_001:0717b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同共讚言。",
        "from": "T19n1023_001:0717b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉釋迦牟尼。",
        "from": "T19n1023_001:0717b14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善能演說正法篋印心陀羅尼。",
        "from": "T19n1023_001:0717b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣令流布行閻浮提。",
        "from": "T19n1023_001:0717b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復令一切諸有情等。",
        "from": "T19n1023_001:0717b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以全身舍利安置此塔。",
        "from": "T19n1023_001:0717b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此十方一切如來。",
        "from": "T19n1023_001:0717b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以諸三昧而加護之。",
        "from": "T19n1023_001:0717b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人受持讀誦解說書寫此全身舍利祕密篋印心陀羅尼經。",
        "from": "T19n1023_001:0717b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及以金銀栴檀雕彩莊嚴安置塔中。",
        "from": "T19n1023_001:0717b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼十方世界一切如來。",
        "from": "T19n1023_001:0717b20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒來在此而加護之。",
        "from": "T19n1023_001:0717b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊說此正法篋印心陀羅尼時。",
        "from": "T19n1023_001:0717b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此舊破壞丘埠之中。",
        "from": "T19n1023_001:0717b22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出現七寶微妙之塔。",
        "from": "T19n1023_001:0717b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相輪幢幡種種莊嚴。",
        "from": "T19n1023_001:0717b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人天大眾無不皆見。",
        "from": "T19n1023_001:0717b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "久現如故。",
        "from": "T19n1023_001:0717b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼時眾會諸大菩薩及聲聞眾。",
        "from": "T19n1023_001:0717b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天龍八部人非人等。",
        "from": "T19n1023_001:0717b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覩此希奇皆悉歡喜。",
        "from": "T19n1023_001:0717b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊說此法時。",
        "from": "T19n1023_001:0717b26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸大眾等金剛手大藥叉主。",
        "from": "T19n1023_001:0717b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無垢妙光大婆羅門。",
        "from": "T19n1023_001:0717b27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并諸菩薩諸大聲聞。",
        "from": "T19n1023_001:0717b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天龍藥叉犍闥婆阿素囉𦾨嚕荼緊那囉麼護囉誐人非人等。",
        "from": "T19n1023_001:0717b29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國王大臣長者居士。",
        "from": "T19n1023_001:0717b29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切大眾聞佛所說。",
        "from": "T19n1023_001:0717c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心信受歡喜而退。",
        "from": "T19n1023_001:0717c01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來正法祕密篋印心陀羅尼經",
        "from": "T19n1023_001:0717c02_100",
        "context_with_next_sentence": false
    }
]