[
    {
        "source_sentence": "佛說灌頂十二萬神王護比丘尼經佛說灌頂十二萬神王護比丘尼",
        "from": "T21n1331_002:0499b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經卷第二",
        "from": "T21n1331_002:0499b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東晉天竺三藏帛尸梨蜜多羅譯",
        "from": "T21n1331_002:0499b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞如是。",
        "from": "T21n1331_002:0499b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛遊於舍衛祇樹給孤獨園。",
        "from": "T21n1331_002:0499b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大比丘眾千二百五十人俱。",
        "from": "T21n1331_002:0499b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時有七比丘尼名修陀利。",
        "from": "T21n1331_002:0499b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在山中塚墓間禪思一心。",
        "from": "T21n1331_002:0499b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有諸惡鬼神噉人精氣者。",
        "from": "T21n1331_002:0499b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嬈是比丘尼脫其衣裳。",
        "from": "T21n1331_002:0499b14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不聽遊行入村乞食。",
        "from": "T21n1331_002:0499b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時修陀利比丘尼。",
        "from": "T21n1331_002:0499b15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "語同座諸比丘尼言。",
        "from": "T21n1331_002:0499b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等當正心憶念我師釋迦牟尼多陀阿伽度阿羅訶三藐三佛陀。",
        "from": "T21n1331_002:0499b17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是言已。",
        "from": "T21n1331_002:0499b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸噉精氣鬼神退散馳走。",
        "from": "T21n1331_002:0499b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是修陀利比丘尼等。",
        "from": "T21n1331_002:0499b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相將俱到佛所。",
        "from": "T21n1331_002:0499b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稽首作禮白佛言。",
        "from": "T21n1331_002:0499b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天尊我等七人受天中天無上真法。",
        "from": "T21n1331_002:0499b20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思惟一心求四道果。",
        "from": "T21n1331_002:0499b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸噉精氣鬼神七萬餘頭。",
        "from": "T21n1331_002:0499b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來亂我等不得正念。",
        "from": "T21n1331_002:0499b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦復遮圍不令行求飲食之具。",
        "from": "T21n1331_002:0499b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現惱如是當奈之何。",
        "from": "T21n1331_002:0499b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願天尊說於聖術而辟除之說是語時阿難在右邊。",
        "from": "T21n1331_002:0499b24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛顧語阿難言。",
        "from": "T21n1331_002:0499b25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝見是修陀利比丘尼七人等不。",
        "from": "T21n1331_002:0499b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難答曰見。",
        "from": "T21n1331_002:0499b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言阿難是諸比丘尼。",
        "from": "T21n1331_002:0499b26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常為七千萬鬼神所嬈。",
        "from": "T21n1331_002:0499b27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今當為其。",
        "from": "T21n1331_002:0499b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "召須彌頂上及海中諸大神。",
        "from": "T21n1331_002:0499b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當護是等諸比丘尼。",
        "from": "T21n1331_002:0499b28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不令諸小鬼神得其便也。",
        "from": "T21n1331_002:0499b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛於是便以神力。",
        "from": "T21n1331_002:0499c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "召須彌頂上諸鬼神王來已。",
        "from": "T21n1331_002:0499c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸鬼神王將從七萬鬼神。",
        "from": "T21n1331_002:0499c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來到佛所稽首禮足。",
        "from": "T21n1331_002:0499c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告諸鬼神王等。",
        "from": "T21n1331_002:0499c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我若在世及滅度後。",
        "from": "T21n1331_002:0499c04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在所國土城邑聚落。",
        "from": "T21n1331_002:0499c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護諸比丘尼不令諸小鬼神之所得便。",
        "from": "T21n1331_002:0499c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸比丘尼所到之處。",
        "from": "T21n1331_002:0499c06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常隨護助使得安隱。",
        "from": "T21n1331_002:0499c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸惡之鬼不得嬈近。",
        "from": "T21n1331_002:0499c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛復告勅大海居止水精山中龍宮所住處。",
        "from": "T21n1331_002:0499c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有五萬鬼神將其營從。",
        "from": "T21n1331_002:0499c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來到佛所各禮佛足却住一面。",
        "from": "T21n1331_002:0499c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛又告須彌頂上七萬神王。",
        "from": "T21n1331_002:0499c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及海中五萬神王等。",
        "from": "T21n1331_002:0499c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝從今以後。",
        "from": "T21n1331_002:0499c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當護諸比丘尼。",
        "from": "T21n1331_002:0499c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令得安隱離諸恐怖。",
        "from": "T21n1331_002:0499c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得定意得定行。",
        "from": "T21n1331_002:0499c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸小鬼退散馳走。",
        "from": "T21n1331_002:0499c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠於是處百千由旬不得作害。",
        "from": "T21n1331_002:0499c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說是已。",
        "from": "T21n1331_002:0499c14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢者阿難長跪叉手前白佛言。",
        "from": "T21n1331_002:0499c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是等神王其字云何。",
        "from": "T21n1331_002:0499c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願為解說。",
        "from": "T21n1331_002:0499c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言阿難其神名字我今說之。",
        "from": "T21n1331_002:0499c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灌頂章句其名如是。",
        "from": "T21n1331_002:0499c17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神名枝活吒貨𭏷字淨自在此神主護某頭神名倪提惒和𭏷字妙善生此神主護某眼神名波羅惒和𭏷字暉日光此神主護某鼻神名和沙頭提手字信堅固此神主護某耳神名頭荷尼迦移字開疑惑此神主護某口神名臏迦利迦移字光普攝此神主護某頸神名沙提舍鳩羅字善安吉此神主護某肩神名波羅闍迦提字耀雪山此神主護某臂神名波羅頭阿銖字演光明此神主護某手神名迦摩隷吒遮字香珍寶此神主護某胸神名俱波婁閱叉字如福輪此神主護某背神名沙善般遮壚字清微徹此神主護某腹神名旃遮𭭇摩休字音和柔此神主護某脇神名遮揵陀利手字福德光此神主護某心神名遮羅揵波頭字真寶種此神主護某肝神名波羅斯奴遮字遠聞聲此神主護某肺神名蘇賀迦闍羅字建行至此神主護某脾神名劍浮耆梨惒字慈悲普此神主護某腎神名迦俱壚絺迦字越眾行此神主護某腸神名恒多羅菩提字威解振此神主護某𦝩神名摩多羅和提字真如天此神主護某臗神名至那比舍尸字愛事業此神主護某髀神名曼比舍尸羅字除恐畏此神主護某膝神名迦羅鋪阿尼字願施廣此神主護某脚神名阿沙耶迷和字消諸惡此神主護某出神名挱伊摩陀伊字護世王此神主護某入神名阿提摩陀伊字加諸願此神主護某坐神名阿奴摩陀伊字星中王此神主護某臥神名破仇摩陀陀字首安寂此神主護某夢神名蘭脾留波利字行寂然此神主護某起神名耶頭破那坻字德明遠此神主護某食神名比尼槃頭倚字蓋天地此神主護某飲神名波斯離次離字真不邪此神主護某語神名具梨揵陀離字施願普此神主護某笑神名跪離那波羅字快善意此神主護某戲神名末梨遊沙梨字頭高明此神主護某樂佛語阿難。",
        "from": "T21n1331_002:0500a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是須彌頂上三十六神王名字。",
        "from": "T21n1331_002:0500a25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是有七萬鬼神以為眷屬。",
        "from": "T21n1331_002:0500a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當作擁護。",
        "from": "T21n1331_002:0500a26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令修陀利比丘尼。",
        "from": "T21n1331_002:0500a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及未來末世中諸比丘尼等。",
        "from": "T21n1331_002:0500a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若為邪鬼神所惱亂者不令得便。",
        "from": "T21n1331_002:0500a28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所到安寧不為邪惡所中。",
        "from": "T21n1331_002:0500a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設有嬈者心當存呼灌頂章句三十六神王。",
        "from": "T21n1331_002:0500b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應念即至導從左右。",
        "from": "T21n1331_002:0500b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸比丘尼現威神力。",
        "from": "T21n1331_002:0500b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攘諸魅魔使不得便。",
        "from": "T21n1331_002:0500b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辟除兇惡消滅不善令得吉祥。",
        "from": "T21n1331_002:0500b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛復告賢者阿難。",
        "from": "T21n1331_002:0500b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大海之中龍宮居止。",
        "from": "T21n1331_002:0500b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有三七大神王之女。",
        "from": "T21n1331_002:0500b04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我昔得道與其眷屬。",
        "from": "T21n1331_002:0500b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來到我所稽首作禮。",
        "from": "T21n1331_002:0500b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說如是言當於佛滅度後五濁末世之中護佛弟子。",
        "from": "T21n1331_002:0500b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我已面勅此諸神王。",
        "from": "T21n1331_002:0500b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令當擁護修陀利及未來末世諸比丘尼等。",
        "from": "T21n1331_002:0500b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此諸神王亦不違本誓。",
        "from": "T21n1331_002:0500b08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有危厄禍害之日。",
        "from": "T21n1331_002:0500b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常當淨心歸命三寶。",
        "from": "T21n1331_002:0500b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後呼其名字無不為護輒在左右。",
        "from": "T21n1331_002:0500b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難白佛言。",
        "from": "T21n1331_002:0500b10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸神王有如是利益。",
        "from": "T21n1331_002:0500b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願速說灌頂章句善神名字。",
        "from": "T21n1331_002:0500b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為守護故。",
        "from": "T21n1331_002:0500b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使諸比丘尼離諸恐懼。",
        "from": "T21n1331_002:0500b12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為邪妖之所惱近。",
        "from": "T21n1331_002:0500b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言阿難是大神王女名字如是。",
        "from": "T21n1331_002:0500b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神王女藍婆惟藍波字珠瓔珞此神女為某辟除邪惡魍魎媚鬼驅逐百千由旬不令得住。",
        "from": "T21n1331_002:0500b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神王女鴛那多烏那陀字摩尼寶此神女除去鳥鳴野狐變怪因衰嬈人者不令住某舍屋之中。",
        "from": "T21n1331_002:0500b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神王女蘇貿迦羅闍字好莊嚴此神女主夢寤顛倒。",
        "from": "T21n1331_002:0500b19_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見諸先亡傷毀之鬼悉能消伏。",
        "from": "T21n1331_002:0500b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神王女摩奴羅摩遮字嚴飾妙此神女若遊出時異道聚會。",
        "from": "T21n1331_002:0500b21_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲食有毒自然消化。",
        "from": "T21n1331_002:0500b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神王女劍浮耆梨惒字寶連珠此神女護諸怨家若相見時起諸毒惡。",
        "from": "T21n1331_002:0500b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即便和解相向。",
        "from": "T21n1331_002:0500b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神王女惟舍羅遮迦字流離光此神女若嫉妬惡心相向者。",
        "from": "T21n1331_002:0500b25_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以帶持故不生貪心。",
        "from": "T21n1331_002:0500b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神王女攎樓迦攎樓字身撤照此神女若有債主求諸財寶。",
        "from": "T21n1331_002:0500b27_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以神護故便寬賖消息。",
        "from": "T21n1331_002:0500c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神王女迦羅博多尼字華開敷此神女某若為盜賊惡伴所引至縣時即便解脫。",
        "from": "T21n1331_002:0500c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神王女澠迦陀羅遮字香烟氣此神女主治蜚尸客氣之鬼復連鬼神。",
        "from": "T21n1331_002:0500c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即便磨滅不現。",
        "from": "T21n1331_002:0500c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神王女稜迦移陀羅那字妙王頂此神女若為龍象所害種種恐畏。",
        "from": "T21n1331_002:0500c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以其誦持自然消滅。",
        "from": "T21n1331_002:0500c07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神王女劍蒱闍浮無耶字遊戲樂此神女護某苞腫眾衰頭痛寒熱即便不行嬈害。",
        "from": "T21n1331_002:0500c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神王女迦俱攎絺迦字淨如梵此神女護某不為山神樹神星死善死鬼神所嬈害。",
        "from": "T21n1331_002:0500c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神王女遮羅絺迦那字音深妙此神女護某至大小便利之時。",
        "from": "T21n1331_002:0500c12_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為惡鬼神所觸嬈也。",
        "from": "T21n1331_002:0500c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神王女慢羅鳩梨陀字信善法此神女護某屋舍床席帳幔。",
        "from": "T21n1331_002:0500c14_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不使他餘鬼神留停宅中。",
        "from": "T21n1331_002:0500c15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神王女臀頭梨架羅字師子音此神女守護人門戶宅舍。",
        "from": "T21n1331_002:0500c16_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四方八神之王勅令鎮護除去不祥。",
        "from": "T21n1331_002:0500c17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神王女訶栗提羅伽字樂音樂此神女典領八十億神諸神之母。",
        "from": "T21n1331_002:0500c18_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護某使萬病除愈百事吉祥。",
        "from": "T21n1331_002:0500c19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神王女薩遮摩陀利字聲清徹此神女護某不令他人厭禱增疾之者。",
        "from": "T21n1331_002:0500c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使厭禱不行害也。",
        "from": "T21n1331_002:0500c21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神王女慢多羅阿佉尼字欣樂快此神女使五溫疫毒若頭痛寒熱。",
        "from": "T21n1331_002:0500c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "某若呼名者即為作護。",
        "from": "T21n1331_002:0500c23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神王女鳩蘭𦪣吒羅子字結明誓此神女主諸毒蛇蚖蝮若嚙人者。",
        "from": "T21n1331_002:0500c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "存呼七遍毒即不行。",
        "from": "T21n1331_002:0500c25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神王女抄多摩尼摸字柔軟音此神女護某不令盜賊剝奪衣裳。",
        "from": "T21n1331_002:0501a01_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呼其名賊退散去。",
        "from": "T21n1331_002:0501a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神王女沙﨟波提敷字心安詳此神女護某牙齒若為蟲所嚙者。",
        "from": "T21n1331_002:0501a03_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若存呼其名七遍蟲即消滅。",
        "from": "T21n1331_002:0501a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告阿難。",
        "from": "T21n1331_002:0501a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我所說海中灌頂章句神王女。",
        "from": "T21n1331_002:0501a05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其名字如是。",
        "from": "T21n1331_002:0501a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此諸神王女。",
        "from": "T21n1331_002:0501a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與其眷屬五萬鬼神。",
        "from": "T21n1331_002:0501a06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遶海邊行一日一夜周匝八萬四千由旬。",
        "from": "T21n1331_002:0501a07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以血肉為食。",
        "from": "T21n1331_002:0501a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今皆得道。",
        "from": "T21n1331_002:0501a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "末世比丘尼輩。",
        "from": "T21n1331_002:0501a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現威神力作大護助。",
        "from": "T21n1331_002:0501a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辟除邪惡萬毒不行。",
        "from": "T21n1331_002:0501a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語修陀利及未來諸比丘尼等。",
        "from": "T21n1331_002:0501a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有能持是灌頂章句。",
        "from": "T21n1331_002:0501a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則離一切無數恐懼。",
        "from": "T21n1331_002:0501a11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若持此神呪夢安覺歡。",
        "from": "T21n1331_002:0501a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不畏縣官水火盜賊。",
        "from": "T21n1331_002:0501a12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怨家債主自然避去。",
        "from": "T21n1331_002:0501a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼神羅剎妖魅魍魎邪惡薜荔厭鎮之鬼。",
        "from": "T21n1331_002:0501a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樹木精神百蟲精神。",
        "from": "T21n1331_002:0501a14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畜生精魅溪谷精魅。",
        "from": "T21n1331_002:0501a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "門中內外鬼神戶中內外鬼神。",
        "from": "T21n1331_002:0501a15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍宅四方井竈鬼神。",
        "from": "T21n1331_002:0501a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洿池鬼神廁溷中鬼神。",
        "from": "T21n1331_002:0501a16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若比丘尼帶持經者。",
        "from": "T21n1331_002:0501a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此諸惡鬼終不得便。",
        "from": "T21n1331_002:0501a17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有厭禱呪咀之者。",
        "from": "T21n1331_002:0501a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人帶持神呪經故。",
        "from": "T21n1331_002:0501a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然辟除兩作和解。",
        "from": "T21n1331_002:0501a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱生慈心惡意悉滅。",
        "from": "T21n1331_002:0501a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無復惱害諸比丘尼。",
        "from": "T21n1331_002:0501a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若在山中溪谷曠路。",
        "from": "T21n1331_002:0501a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "抄賊劫掠自然不現。",
        "from": "T21n1331_002:0501a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師子虎狼熊羆蚖蛇。",
        "from": "T21n1331_002:0501a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉自縮藏不害人也。",
        "from": "T21n1331_002:0501a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故此十二萬神王大神呪經至尊至重。",
        "from": "T21n1331_002:0501a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能為諸比丘尼作大利安。",
        "from": "T21n1331_002:0501a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語修陀利及未來諸比丘尼等。",
        "from": "T21n1331_002:0501a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此灌頂無上章句。",
        "from": "T21n1331_002:0501a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若後九百歲中。",
        "from": "T21n1331_002:0501a25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸邪惡魅鬼嬈人因衰作害。",
        "from": "T21n1331_002:0501a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有惡魔吐種種雜毒之氣以害汝等。",
        "from": "T21n1331_002:0501a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有鬼神吸汝五脈。",
        "from": "T21n1331_002:0501a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又有鬼神噉汝精髓。",
        "from": "T21n1331_002:0501a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是惡鬼噉人肉者不可稱數。",
        "from": "T21n1331_002:0501a28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今為汝略說少耳。",
        "from": "T21n1331_002:0501a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告阿難。",
        "from": "T21n1331_002:0501b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若後末世諸比丘尼。",
        "from": "T21n1331_002:0501b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為惡鬼神所嬈惱者。",
        "from": "T21n1331_002:0501b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當洗浴身體著鮮潔之衣。",
        "from": "T21n1331_002:0501b02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當專心一意讚詠此經。",
        "from": "T21n1331_002:0501b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當以五色之綵作好幡蓋。",
        "from": "T21n1331_002:0501b03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香汁泥地縱廣七尺。",
        "from": "T21n1331_002:0501b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然十方燈散雜色華。",
        "from": "T21n1331_002:0501b04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒眾名香膠香婆香安息香等。",
        "from": "T21n1331_002:0501b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮拜十方。",
        "from": "T21n1331_002:0501b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七日七夜。",
        "from": "T21n1331_002:0501b05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長齋菜食。",
        "from": "T21n1331_002:0501b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不噉五辛審諦莫疑。",
        "from": "T21n1331_002:0501b06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸惡魔聞見此經神呪力故。",
        "from": "T21n1331_002:0501b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即馳散而去遠百千由旬不能為害。",
        "from": "T21n1331_002:0501b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "消滅不善吉祥感應。",
        "from": "T21n1331_002:0501b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是以後修陀利及未來諸比丘尼等。",
        "from": "T21n1331_002:0501b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉共讀誦書持是典。",
        "from": "T21n1331_002:0501b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而供養之中有闇鈍比丘尼輩。",
        "from": "T21n1331_002:0501b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能讀誦者但書持是典。",
        "from": "T21n1331_002:0501b11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以好繒綵作囊盛之。",
        "from": "T21n1331_002:0501b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲行來出入之時。",
        "from": "T21n1331_002:0501b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輒著衣前所往來處獲善吉安。",
        "from": "T21n1331_002:0501b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有惡魔自然消亡無敢當者。",
        "from": "T21n1331_002:0501b13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大神典至尊至妙極有威神。",
        "from": "T21n1331_002:0501b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若後末世書持此典佩帶在身。",
        "from": "T21n1331_002:0501b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遊行十方無所復畏。",
        "from": "T21n1331_002:0501b15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以是因緣出此神呪。",
        "from": "T21n1331_002:0501b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令人讀誦受持供養而帶佩之。",
        "from": "T21n1331_002:0501b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說是語時。",
        "from": "T21n1331_002:0501b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天帝釋如人屈申臂頃。",
        "from": "T21n1331_002:0501b17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從天來下往到佛所。",
        "from": "T21n1331_002:0501b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稽首作禮長跪叉手。",
        "from": "T21n1331_002:0501b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言天尊。",
        "from": "T21n1331_002:0501b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然諸佛至真德過須彌。",
        "from": "T21n1331_002:0501b19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智超江海慧踰虛空。",
        "from": "T21n1331_002:0501b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獨步三界無能及者。",
        "from": "T21n1331_002:0501b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方一切莫不蒙度。",
        "from": "T21n1331_002:0501b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天帝釋說此讚歎時。",
        "from": "T21n1331_002:0501b21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空中雨天香華以散佛上。",
        "from": "T21n1331_002:0501b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天歡喜鬼神亦然。",
        "from": "T21n1331_002:0501b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說經竟。",
        "from": "T21n1331_002:0501b22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難從坐而起長跪叉手白佛言。",
        "from": "T21n1331_002:0501b23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "演說此經當何名之。",
        "from": "T21n1331_002:0501b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言阿難及諸四輩。",
        "from": "T21n1331_002:0501b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我說此經名十二萬鬼神之王灌頂章句。",
        "from": "T21n1331_002:0501b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝善持之於吾滅後。",
        "from": "T21n1331_002:0501b25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有清信諸比丘尼。",
        "from": "T21n1331_002:0501b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命求者應當授與。",
        "from": "T21n1331_002:0501b26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說經已。",
        "from": "T21n1331_002:0501b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難叉手白佛言。",
        "from": "T21n1331_002:0501b27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設有諸比丘尼。",
        "from": "T21n1331_002:0501b27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲受者云何授與。",
        "from": "T21n1331_002:0501b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言當如大比丘受七萬二千神王灌頂大法無有異也。",
        "from": "T21n1331_002:0501b29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說是已四輩弟子天龍八部。",
        "from": "T21n1331_002:0501c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫不歡喜作禮奉行。",
        "from": "T21n1331_002:0501c01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灌頂經卷第二",
        "from": "T21n1331_002:0501c02_100",
        "context_with_next_sentence": false
    }
]