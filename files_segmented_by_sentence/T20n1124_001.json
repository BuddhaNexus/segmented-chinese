[
    {
        "source_sentence": "普賢金剛薩埵略瑜伽念誦儀",
        "from": "T20n1124_001:0531a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "軌",
        "from": "T20n1124_001:0531a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T20n1124_001:0531a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空謚大鑒正號大廣智大興善寺三藏沙門不空奉　詔譯我今說普賢菩薩身口意金剛念誦法。",
        "from": "T20n1124_001:0531a29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由修此法等同金剛薩埵。",
        "from": "T20n1124_001:0531b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行者住勝解行地。",
        "from": "T20n1124_001:0531b01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曾入金剛界大曼茶羅。",
        "from": "T20n1124_001:0531b02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受菩提心戒。",
        "from": "T20n1124_001:0531b02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸有情。",
        "from": "T20n1124_001:0531b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大悲愍拔濟安樂心。",
        "from": "T20n1124_001:0531b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不惜身命。",
        "from": "T20n1124_001:0531b03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剎那剎那常懷得普賢菩薩身。",
        "from": "T20n1124_001:0531b04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於身業懃常習。",
        "from": "T20n1124_001:0531b04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍觀一切諸佛菩薩。",
        "from": "T20n1124_001:0531b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如對目前。",
        "from": "T20n1124_001:0531b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所居山間阿蘭若。",
        "from": "T20n1124_001:0531b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於精室或於伽藍。",
        "from": "T20n1124_001:0531b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於宅舍建立道場。",
        "from": "T20n1124_001:0531b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面向東方或西隨取穩便。",
        "from": "T20n1124_001:0531b07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端身結跏趺坐或全跏或普賢跏或隨意坐。",
        "from": "T20n1124_001:0531b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心遍緣一切有情界。",
        "from": "T20n1124_001:0531b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令有情三業身口意淨密語曰。",
        "from": "T20n1124_001:0531b09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引娑嚩二合婆嚩二合戍入聲度憾次應觀如來相好圓備。",
        "from": "T20n1124_001:0531b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運心想供養。",
        "from": "T20n1124_001:0531b11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以天妙塗香花鬘香燈燭飲食種種讚歎。",
        "from": "T20n1124_001:0531b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則依四種禮印契密語。",
        "from": "T20n1124_001:0531b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮四方如來捨身供養。",
        "from": "T20n1124_001:0531b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則成受三世無礙智律儀戒。",
        "from": "T20n1124_001:0531b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則起右膝著地。",
        "from": "T20n1124_001:0531b14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結持金剛三麼耶印。",
        "from": "T20n1124_001:0531b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當心誦。",
        "from": "T20n1124_001:0531b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於頂上散。",
        "from": "T20n1124_001:0531b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由結此印誦密語。",
        "from": "T20n1124_001:0531b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則成遍禮供養承事一切如來密語曰。",
        "from": "T20n1124_001:0531b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合吻微一反次應發露說罪。",
        "from": "T20n1124_001:0531b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨喜勸請迴向發願已。",
        "from": "T20n1124_001:0531b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則結跏趺坐作是思惟。",
        "from": "T20n1124_001:0531b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願一切有情獲得出世無上悉地成就密語曰。",
        "from": "T20n1124_001:0531b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩嚩怛他引孽多餉悉多去引薩嚩薩怛嚩二合南引薩嚩悉馱藥二合三鉢撚二合耽怛他蘖多室者二合地底二合入聲瑟咤二合耽次結金剛掌印。",
        "from": "T20n1124_001:0531b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密語三遍密語曰。",
        "from": "T20n1124_001:0531b26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囒二合惹里二合引由結此印及誦密語。",
        "from": "T20n1124_001:0531b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所修瑜伽相應門悉地皆成就結金剛縛印誦密語曰。",
        "from": "T20n1124_001:0531b29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚩日囉二合滿馱由結使皆得解脫。",
        "from": "T20n1124_001:0531c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十波羅蜜頓得圓滿。",
        "from": "T20n1124_001:0531c02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次即以縛印三掣。",
        "from": "T20n1124_001:0531c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拍胸間誦密語曰。",
        "from": "T20n1124_001:0531c03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合滿馱怛囉二合吒半音由結此印及誦密語當入曼茶羅阿闍梨所引入。",
        "from": "T20n1124_001:0531c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛薩埵三業金剛令入弟子心。",
        "from": "T20n1124_001:0531c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自性金剛智令。",
        "from": "T20n1124_001:0531c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得發動顯現。",
        "from": "T20n1124_001:0531c07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結金剛遍入印。",
        "from": "T20n1124_001:0531c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前金剛縛。",
        "from": "T20n1124_001:0531c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指入掌安於無名指間誦密語曰。",
        "from": "T20n1124_001:0531c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合吠奢噁由結此印。",
        "from": "T20n1124_001:0531c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三業金剛。",
        "from": "T20n1124_001:0531c11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於身中作大阿吠奢。",
        "from": "T20n1124_001:0531c11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲大神驗威德。",
        "from": "T20n1124_001:0531c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以准前印。",
        "from": "T20n1124_001:0531c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二頭指各屈拄二大指背。",
        "from": "T20n1124_001:0531c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成金剛拳印密語曰。",
        "from": "T20n1124_001:0531c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合母瑟知二合𤚥由結此印令三業金剛堅住不散失。",
        "from": "T20n1124_001:0532a02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以前金剛縛印合竪二中指。",
        "from": "T20n1124_001:0532a03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是金剛薩埵印密語曰。",
        "from": "T20n1124_001:0532a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合三麼耶薩怛鑁二合由結此印。",
        "from": "T20n1124_001:0532a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行者當住普賢菩薩三摩地。",
        "from": "T20n1124_001:0532a06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐於月輪。",
        "from": "T20n1124_001:0532a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令身色相光明圓備。",
        "from": "T20n1124_001:0532a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結素囉多大誓真實印。",
        "from": "T20n1124_001:0532a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以縛印二大指二小指各以指頭相拄。",
        "from": "T20n1124_001:0532a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如獨𦙶杵。",
        "from": "T20n1124_001:0532a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指入掌竪合。",
        "from": "T20n1124_001:0532a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令拄心上即成。",
        "from": "T20n1124_001:0532a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是思惟。",
        "from": "T20n1124_001:0532a10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我身既成普賢菩薩。",
        "from": "T20n1124_001:0532a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發此心時成就無邊解脫觀一切有情。",
        "from": "T20n1124_001:0532a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自他無別。",
        "from": "T20n1124_001:0532a12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同體大悲即誦大誓真實密語曰。",
        "from": "T20n1124_001:0532a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合三麼耶斛素囉多娑怛鑁二合次應住勝三世忿怒金剛三摩地。",
        "from": "T20n1124_001:0532a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立印四面八臂威德赫奕。",
        "from": "T20n1124_001:0532a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光明熾盛如劫燒爛。",
        "from": "T20n1124_001:0532a17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左脚踏摩醯首羅。",
        "from": "T20n1124_001:0532a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右脚踏烏摩。",
        "from": "T20n1124_001:0532a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以二手金剛拳。",
        "from": "T20n1124_001:0532a18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二小指反相鉤。",
        "from": "T20n1124_001:0532a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二頭指。",
        "from": "T20n1124_001:0532a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印左旋辟除人天諸魔及作障者。",
        "from": "T20n1124_001:0532a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋即成結方隅界。",
        "from": "T20n1124_001:0532a20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛菩薩尚不違越。",
        "from": "T20n1124_001:0532a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況三界中作障者。",
        "from": "T20n1124_001:0532a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以印印心額喉頂四處密語曰。",
        "from": "T20n1124_001:0532a22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一遜婆去一儞遜婆儞吽短二仡哩二合恨拏二合仡哩二合恨拏二合吽三仡哩二合恨拏二合引波耶吽四阿娜耶斛五婆誐鑁六嚩日囉二合吽泮吒半音由結此印誦密語三密相應。",
        "from": "T20n1124_001:0532a27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿賴耶識中所有雜染種子。",
        "from": "T20n1124_001:0532a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此金剛智火。",
        "from": "T20n1124_001:0532a28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "焚燒悉盡。",
        "from": "T20n1124_001:0532a28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切外障不能為障難。",
        "from": "T20n1124_001:0532a29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結蓮華族三摩耶印。",
        "from": "T20n1124_001:0532b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手金剛縛。",
        "from": "T20n1124_001:0532b01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合竪二大詣及二小指。",
        "from": "T20n1124_001:0532b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印安於口。",
        "from": "T20n1124_001:0532b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想自身同金剛法菩薩密語曰。",
        "from": "T20n1124_001:0532b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一嚩日囉二合三麼耶薩竪鑁二合由結此印。",
        "from": "T20n1124_001:0532b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所修三摩地瑜伽悉皆現前。",
        "from": "T20n1124_001:0532b05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結定印。",
        "from": "T20n1124_001:0532b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍觀虛空諸佛猶如胡麻。",
        "from": "T20n1124_001:0532b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦密語曰。",
        "from": "T20n1124_001:0532b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一薩嚩瑜誐質多二母答波二合娜夜引彌由結此印誦密語。",
        "from": "T20n1124_001:0532b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅一切障。",
        "from": "T20n1124_001:0532b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲得安樂悅意。",
        "from": "T20n1124_001:0532b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "超魔境。",
        "from": "T20n1124_001:0532b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即同諸佛得一切世天供養。",
        "from": "T20n1124_001:0532b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦通達心密語曰。",
        "from": "T20n1124_001:0532b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一質多鉢囉二合底吠鄧迦嚕彌二次入九種緣生三摩地智。",
        "from": "T20n1124_001:0532b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀一切法。",
        "from": "T20n1124_001:0532b14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如幻如陽焰如夢如影像如聲響如光影如水月如變化如虛空。",
        "from": "T20n1124_001:0532b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是觀已。",
        "from": "T20n1124_001:0532b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是心於染於淨。",
        "from": "T20n1124_001:0532b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通達無礙猶若虛空。",
        "from": "T20n1124_001:0532b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應入菩提心觀。",
        "from": "T20n1124_001:0532b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中誦密語菩提心密語曰。",
        "from": "T20n1124_001:0532b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一冒地質多二母答波二合娜夜引彌由結此印則於身中當胸臆間。",
        "from": "T20n1124_001:0532b20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀圓滿月皎潔清涼。",
        "from": "T20n1124_001:0532b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無限數多誦通達菩提心密語畢。",
        "from": "T20n1124_001:0532b21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得心水澄淨。",
        "from": "T20n1124_001:0532b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩提心月影現於中。",
        "from": "T20n1124_001:0532b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此滿月上。",
        "from": "T20n1124_001:0532b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀五鈷金剛杵。",
        "from": "T20n1124_001:0532b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了了分明誦密語曰。",
        "from": "T20n1124_001:0532b23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引底瑟姹二合嚩日囉二合入引由作如是觀誦此密語。",
        "from": "T20n1124_001:0532b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是心成如金剛。",
        "from": "T20n1124_001:0532b25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次觀身如五鈷金剛杵誦密語曰。",
        "from": "T20n1124_001:0532b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚩日囉二合怛麼二合俱含次觀遍滿虛空中佛。",
        "from": "T20n1124_001:0532b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉來入金剛杵中合為一體。",
        "from": "T20n1124_001:0532b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由作如是觀智并誦密語。",
        "from": "T20n1124_001:0532b29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行者三業成如金剛。",
        "from": "T20n1124_001:0532c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行者當觀自身如普賢菩薩。",
        "from": "T20n1124_001:0532c01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戴其五佛冠身如水精月色。",
        "from": "T20n1124_001:0532c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持五鈷金剛杵。",
        "from": "T20n1124_001:0532c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持金剛鈴。",
        "from": "T20n1124_001:0532c03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身處在滿月輪。",
        "from": "T20n1124_001:0532c03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了了分明則誦密語曰。",
        "from": "T20n1124_001:0532c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉二合薩怛嚩二合三母他引含次用素囉多金剛印。",
        "from": "T20n1124_001:0532c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結金剛縛。",
        "from": "T20n1124_001:0532c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右大母指入左虎口中即成。",
        "from": "T20n1124_001:0532c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此印加持心額喉頂密語曰。",
        "from": "T20n1124_001:0532c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵素囉多裟怛鑁三合次二手金剛縛。",
        "from": "T20n1124_001:0532c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二中指屈上節如劍。",
        "from": "T20n1124_001:0532c10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指各屈。",
        "from": "T20n1124_001:0532c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輔二中指令相著。",
        "from": "T20n1124_001:0532c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指如結跏即成。",
        "from": "T20n1124_001:0532c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名金剛界印。",
        "from": "T20n1124_001:0532c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名五佛冠印密語曰。",
        "from": "T20n1124_001:0532c12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩嚩怛他蘖多囉怛曩二合毘曬迦噁引去次結灌頂印置印安於頂上同前印密語曰。",
        "from": "T20n1124_001:0533a01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉二合麼隷鼻詵上者𤚥鑁次以二手作金剛拳。",
        "from": "T20n1124_001:0533a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當額如繫鬘繒。",
        "from": "T20n1124_001:0533a03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次腦後亦如前繫已。",
        "from": "T20n1124_001:0533a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從小指散下。",
        "from": "T20n1124_001:0533a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如垂引繒帶勢。",
        "from": "T20n1124_001:0533a04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印誦密語曰。",
        "from": "T20n1124_001:0533a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引砧次結被甲印二手金剛拳。",
        "from": "T20n1124_001:0533a07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "展二頭指。",
        "from": "T20n1124_001:0533a07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於指端想唵砧二字。",
        "from": "T20n1124_001:0533a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便以二指當心。",
        "from": "T20n1124_001:0533a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相遶三遍。",
        "from": "T20n1124_001:0533a08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次背後却至臍二膝。",
        "from": "T20n1124_001:0533a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又至臍次腰却至心。",
        "from": "T20n1124_001:0533a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左右肩頸次頂後額腦後。",
        "from": "T20n1124_001:0533a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結拳如繫甲勢。",
        "from": "T20n1124_001:0533a10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密語曰。",
        "from": "T20n1124_001:0533a10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引阿婆曳嚩日囉二合迦縛制滿馱囉訖曬二合𤚥吽㟏呼甘反次二手旋拳如舞勢妙拍一遍令歡喜密語曰。",
        "from": "T20n1124_001:0533a13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合覩使也二合斛引次結金剛輪曼茶羅印。",
        "from": "T20n1124_001:0533a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手金剛拳。",
        "from": "T20n1124_001:0533a15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指二小指互相鉤即成。",
        "from": "T20n1124_001:0533a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印地及身五處密語曰。",
        "from": "T20n1124_001:0533a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉二合斫羯囉二合吽弱吽鑁斛次結警覺一切聖眾印。",
        "from": "T20n1124_001:0533a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手交臂右押左彈指。",
        "from": "T20n1124_001:0533a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每誦一遍一彈指。",
        "from": "T20n1124_001:0533a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令滿四遍密語曰。",
        "from": "T20n1124_001:0533a20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉二合三麼惹弱弱由作此印。",
        "from": "T20n1124_001:0533a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普賢菩薩及一切眷屬。",
        "from": "T20n1124_001:0533a22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應時雲集在於空中。",
        "from": "T20n1124_001:0533a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結四門菩薩印儀初東門金剛鉤菩薩。",
        "from": "T20n1124_001:0533a24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居曼茶羅中青色。",
        "from": "T20n1124_001:0533a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南門中金剛索菩薩。",
        "from": "T20n1124_001:0533a25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黃色持索為印。",
        "from": "T20n1124_001:0533a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西門中金剛鎖菩薩。",
        "from": "T20n1124_001:0533a26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "赤色持鎖為印。",
        "from": "T20n1124_001:0533a26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北門中金剛鈴菩薩。",
        "from": "T20n1124_001:0533a27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "綠色持鈴為印。",
        "from": "T20n1124_001:0533a27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此四菩薩各具五佛冠鬘種種嚴麗。",
        "from": "T20n1124_001:0533a28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為莊嚴。",
        "from": "T20n1124_001:0533a28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結鉤印二手金剛拳。",
        "from": "T20n1124_001:0533a29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二小指反相鉤。",
        "from": "T20n1124_001:0533a29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直竪左頭指。",
        "from": "T20n1124_001:0533b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈右頭指上下來去。",
        "from": "T20n1124_001:0533b01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是鉤印密語曰。",
        "from": "T20n1124_001:0533b01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日𠻴二合矩勢一弱次不解前印。",
        "from": "T20n1124_001:0533b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "改二頭指頭相跓如鐶。",
        "from": "T20n1124_001:0533b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是索印密語曰。",
        "from": "T20n1124_001:0533b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉二合播引勢一吽二次以前印二頭指二大指互相交頭相捻屈其臂。",
        "from": "T20n1124_001:0533b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是鎖印密語曰。",
        "from": "T20n1124_001:0533b07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉二合餉迦麗一𭷪二次如前鎖印二大指入掌搖動。",
        "from": "T20n1124_001:0533b09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成密語曰。",
        "from": "T20n1124_001:0533b09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉二合健𪘨一斛二次誦本尊菩薩一百八名讚。",
        "from": "T20n1124_001:0533b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦已則結金剛薩埵大誓真實印。",
        "from": "T20n1124_001:0533b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并誦密語。",
        "from": "T20n1124_001:0533b12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請聖眾降曼茶羅中密語曰。",
        "from": "T20n1124_001:0533b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "系摩賀素上佉一嚩日囉二合薩怛嚩二合夜引呬二試仡臨二合三摩賀引素佉嚩日囉二合母引佉三昧耶四摩拏播引攞野五鉢囉二合母𠆙二合母𠆙六素上囉多薩怛鑁三合七阿拏囉訖覩二合茗婆嚩八素上覩瑟庾二合茗婆嚩九素上涅哩二合住荼護反茗婆嚩十素上布瑟庾二合茗婆嚩十一婆誐鑁十二阿曩引儞𩕳馱諾薩怛嚩二合十三薩嚩悉朕提寢反茗十四鉢囉二合野車翳沙怛梵二合十五阿引訖哩二合使也二合鉢囉二合吠奢十六三摩曳囉麼二合特罔二合十七嚩試引迦嚕彌十八昧母娜囉十九二合引娑滿怛囉二合跛乃二十弱二十一吽二十二鑁二十三斛二十四由結印誦密語。",
        "from": "T20n1124_001:0533b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能警覺普賢菩薩并諸聖眾眷屬。",
        "from": "T20n1124_001:0533b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡悅不越本誓來降道場。",
        "from": "T20n1124_001:0533b28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此印及密語。",
        "from": "T20n1124_001:0533b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能召引入縛令喜悅。",
        "from": "T20n1124_001:0533b29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦至　弱字。",
        "from": "T20n1124_001:0533b29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掌中二指如鉤來去。",
        "from": "T20n1124_001:0533c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至吽字屈如環則成索至　鑁字則交結則名鎖。",
        "from": "T20n1124_001:0533c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至　斛字則如鈴搖動能悅諸聖。",
        "from": "T20n1124_001:0533c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或誦三遍或四遍。",
        "from": "T20n1124_001:0533c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結閼伽印。",
        "from": "T20n1124_001:0533c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手金剛掌。",
        "from": "T20n1124_001:0533c04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指頭相合。",
        "from": "T20n1124_001:0533c04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指在中指後如鉤形。",
        "from": "T20n1124_001:0533c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指各捻二頭指根下即成。",
        "from": "T20n1124_001:0533c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印捧閼伽器。",
        "from": "T20n1124_001:0533c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當額奉獻。",
        "from": "T20n1124_001:0533c06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若道場中先無閼伽。",
        "from": "T20n1124_001:0533c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但唯改二大指相交。",
        "from": "T20n1124_001:0533c07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成名閼伽印。",
        "from": "T20n1124_001:0533c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想八功德水浴諸聖尊足密語曰。",
        "from": "T20n1124_001:0533c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "跛囉摩素上佉引捨也一娑攞里多二尾羅娑曩彌帶囉曩二合麼弭婆伽汶擔二合弱吽鑁斛四呬呬呬呬五鉢囉二合底車上六矩素上滿惹里囉曩二合他由獻閼伽故。",
        "from": "T20n1124_001:0533c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能洗滌無始煩惱塵垢。",
        "from": "T20n1124_001:0533c13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速獲身口意清淨。",
        "from": "T20n1124_001:0533c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結外供養諸尊。",
        "from": "T20n1124_001:0534a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東南喜戲菩薩以二金剛拳當於心。",
        "from": "T20n1124_001:0534a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西南金剛笑菩薩以二拳各在傍向後散勢。",
        "from": "T20n1124_001:0534a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西北金剛歌菩薩左手作拳。",
        "from": "T20n1124_001:0534a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪臂展頭指。",
        "from": "T20n1124_001:0534a04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向身持箜篌為印。",
        "from": "T20n1124_001:0534a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右作彈弦勢。",
        "from": "T20n1124_001:0534a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東北金剛舞菩薩。",
        "from": "T20n1124_001:0534a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二拳旋轉結舞印。",
        "from": "T20n1124_001:0534a06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此四尊服形皆作金色如初嬉戲印。",
        "from": "T20n1124_001:0534a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二拳遶心。",
        "from": "T20n1124_001:0534a07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左右轉即誦密語曰。",
        "from": "T20n1124_001:0534a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "系囉底嚩日囉二合尾邏賜儞怛囉二合吒次結笑印如前印安口傍。",
        "from": "T20n1124_001:0534a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翻掌向外。",
        "from": "T20n1124_001:0534a10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從小指漸開。",
        "from": "T20n1124_001:0534a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各向後散。",
        "from": "T20n1124_001:0534a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住笑容誦密語曰。",
        "from": "T20n1124_001:0534a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "系囉底嚩日囉二合一賀引細二訶訶次以右手作拳。",
        "from": "T20n1124_001:0534a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪頭指屈臂。",
        "from": "T20n1124_001:0534a13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向身如箜篌。",
        "from": "T20n1124_001:0534a13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手拳竪頭指。",
        "from": "T20n1124_001:0534a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作彈弦勢。",
        "from": "T20n1124_001:0534a14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為歌印。",
        "from": "T20n1124_001:0534a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密語曰。",
        "from": "T20n1124_001:0534a14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "系囉底嚩日囉二合一擬引諦二諦諦三次二拳從心旋轉舞。",
        "from": "T20n1124_001:0534a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漸上至頂合掌便散。",
        "from": "T20n1124_001:0534a16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是舞印密語曰。",
        "from": "T20n1124_001:0534a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "系囉底嚩日囉二合儞哩二合諦吠波吠波次結雲金剛菩薩焚香印。",
        "from": "T20n1124_001:0534a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二拳相並。",
        "from": "T20n1124_001:0534a19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下擲即成。",
        "from": "T20n1124_001:0534a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想此尊在壇內院東南隅。",
        "from": "T20n1124_001:0534a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形服皆黑持香爐以為印。",
        "from": "T20n1124_001:0534a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是觀已誦密語曰。",
        "from": "T20n1124_001:0534a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵茗伽去嚩日哩二合麌魯麌魯次以二拳初如舞便以二拳相並上如散華勢是春金剛菩薩印。",
        "from": "T20n1124_001:0534a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當想此尊居內院西南隅。",
        "from": "T20n1124_001:0534a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色服俱白持華密語曰。",
        "from": "T20n1124_001:0534a25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵麼度嚩日哩二合共共次結秋金剛菩薩燈印。",
        "from": "T20n1124_001:0534a27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二拳並竪二大指即想此尊居內院西北隅。",
        "from": "T20n1124_001:0534a28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形服皆赤。",
        "from": "T20n1124_001:0534a28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持燈為印。",
        "from": "T20n1124_001:0534a28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密語曰。",
        "from": "T20n1124_001:0534a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵舍囉娜嚩二合日哩二合暗引暗引次結金剛雲菩薩印。",
        "from": "T20n1124_001:0534b02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並覆二拳摩其胸。",
        "from": "T20n1124_001:0534b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩向散想此尊居內院東北隅。",
        "from": "T20n1124_001:0534b03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形服皆綠色。",
        "from": "T20n1124_001:0534b03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持塗香器密語曰。",
        "from": "T20n1124_001:0534b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合勢始㘑吽短吽短次於曼荼羅中位想圓滿月。",
        "from": "T20n1124_001:0534b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於月輪中有普賢菩薩。",
        "from": "T20n1124_001:0534b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住金剛慢印。",
        "from": "T20n1124_001:0534b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次於大聖前想欲金剛。",
        "from": "T20n1124_001:0534b08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形服色赤。",
        "from": "T20n1124_001:0534b08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣冠鬘瓔珞種種嚴飾。",
        "from": "T20n1124_001:0534b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目瞻大聖住金剛弓箭印。",
        "from": "T20n1124_001:0534b09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次於大聖右。",
        "from": "T20n1124_001:0534b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想計里計羅尊。",
        "from": "T20n1124_001:0534b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色白以二金剛拳。",
        "from": "T20n1124_001:0534b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "交臂住抱印。",
        "from": "T20n1124_001:0534b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次於大聖後。",
        "from": "T20n1124_001:0534b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想愛金剛。",
        "from": "T20n1124_001:0534b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形服皆青。",
        "from": "T20n1124_001:0534b12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪左臂執摩竭幢。",
        "from": "T20n1124_001:0534b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手承其肘。",
        "from": "T20n1124_001:0534b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦共持於幢。",
        "from": "T20n1124_001:0534b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於大聖左邊。",
        "from": "T20n1124_001:0534b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想金剛慢尊。",
        "from": "T20n1124_001:0534b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形服皆黃。",
        "from": "T20n1124_001:0534b14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二金剛拳各安𦜮。",
        "from": "T20n1124_001:0534b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭向左少低。",
        "from": "T20n1124_001:0534b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此五尊皆住羯磨三摩地印。",
        "from": "T20n1124_001:0534b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次金剛密語曰。",
        "from": "T20n1124_001:0534b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弱一嚩日囉二涅哩二合瑟知二合娑去引也計三麼吒吒平音次誦計里計羅尊密語曰。",
        "from": "T20n1124_001:0534b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吽一嚩日囉二合計里引二吉麗吽三次誦金剛愛密語曰。",
        "from": "T20n1124_001:0534b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鑁一嚩日囉二合抳二娑麼二合囉囉吒平音次誦慢菩薩密語曰。",
        "from": "T20n1124_001:0534b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斛一嚩日囉二合迦引冥濕嚩二合哩二怛㘕二合三結以上十七尊聖眾形色衣服花座月輪中。",
        "from": "T20n1124_001:0534b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及己身住大印。",
        "from": "T20n1124_001:0534b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆有聖尊眷屬圍遶。",
        "from": "T20n1124_001:0534b28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆須明了。",
        "from": "T20n1124_001:0534b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當誦普賢菩薩讚曰。",
        "from": "T20n1124_001:0535a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薩嚩引弩囉引誐素上佉薩怛莽二合曩娑去一怛網二合嚩日囉二合薩怛嚩二合跛囉莫素上囉多入二婆嚩冥摩訶素上佉涅哩二合住掣囉也引惹三鉢囉二合底跛儞也二合悉𠆙者攞麌鉢囉二合拏多入四引耶誦第一句讚。",
        "from": "T20n1124_001:0535a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當結欲金剛羯磨印。",
        "from": "T20n1124_001:0535a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次誦第二句。",
        "from": "T20n1124_001:0535a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結計里吉羅金剛羯磨印。",
        "from": "T20n1124_001:0535a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次第三句。",
        "from": "T20n1124_001:0535a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結愛金剛羯摩印。",
        "from": "T20n1124_001:0535a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四句。",
        "from": "T20n1124_001:0535a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結慢金剛羯磨印。",
        "from": "T20n1124_001:0535a10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則成四種歌詠。",
        "from": "T20n1124_001:0535a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四種舞印。",
        "from": "T20n1124_001:0535a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦讚時入大三摩地用之。",
        "from": "T20n1124_001:0535a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次誦大乘不空三昧耶真實密語曰。",
        "from": "T20n1124_001:0535a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引摩賀素上佉嚩日囉二合薩怛嚩二合弱吽鑁斛素囉多薩怛鑁二合住大印。",
        "from": "T20n1124_001:0535a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "等同普賢菩薩。",
        "from": "T20n1124_001:0535a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲成就本尊應一月念誦。",
        "from": "T20n1124_001:0535a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每日四時無限數念誦。",
        "from": "T20n1124_001:0535a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若疲倦解印。",
        "from": "T20n1124_001:0535a17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "全身金剛合掌作禮。",
        "from": "T20n1124_001:0535a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此為憩息。",
        "from": "T20n1124_001:0535a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令其心不疲厭。",
        "from": "T20n1124_001:0535a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其滿月夜結大印。",
        "from": "T20n1124_001:0535a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一夜念誦至於晨朝。",
        "from": "T20n1124_001:0535a19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普賢菩薩來。",
        "from": "T20n1124_001:0535a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身光如月輪抱其行者自身。",
        "from": "T20n1124_001:0535a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入遍支分。",
        "from": "T20n1124_001:0535a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其行者身等同普賢。",
        "from": "T20n1124_001:0535a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五佛頭冠身著天妙瓔珞華鬘。",
        "from": "T20n1124_001:0535a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身口意如金剛薩埵。",
        "from": "T20n1124_001:0535a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有親族見彼人成如是威德。",
        "from": "T20n1124_001:0535a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆生驚愕恭敬禮拜。",
        "from": "T20n1124_001:0535a23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼人常在自家作大神通。",
        "from": "T20n1124_001:0535a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦作佛身現大神通。",
        "from": "T20n1124_001:0535a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦現三世勝金剛身。",
        "from": "T20n1124_001:0535a25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "調伏難調者悉皆調伏。",
        "from": "T20n1124_001:0535a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意騰空。",
        "from": "T20n1124_001:0535a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自在往於無量世界。",
        "from": "T20n1124_001:0535a26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養諸佛。",
        "from": "T20n1124_001:0535a26_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受天妙五欲樂。",
        "from": "T20n1124_001:0535a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽命盡虛。",
        "from": "T20n1124_001:0535a27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利樂無邊有情。",
        "from": "T20n1124_001:0535a27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成大利益。",
        "from": "T20n1124_001:0535a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成毘盧遮那佛身。",
        "from": "T20n1124_001:0535a28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普賢金剛薩埵略瑜伽念誦儀軌",
        "from": "T20n1124_001:0535a29_100",
        "context_with_next_sentence": false
    }
]