[
    {
        "source_sentence": "新編諸宗教藏總錄卷第三",
        "from": "T55n2184_003:1174c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高麗沙門義天錄",
        "from": "T55n2184_003:1174c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "海東有本見行錄下海東有本見行錄下",
        "from": "T55n2184_003:1174c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "橋景雅御本書寫了　　　　明　空",
        "from": "T55n2184_003:1178c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寬永二十有一年七月日。",
        "from": "T55n2184_003:1178c12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以栂尾法鼓臺之本。",
        "from": "T55n2184_003:1178c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寫于禪堂院。",
        "from": "T55n2184_003:1178c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陳簡蠹編多失文畫。",
        "from": "T55n2184_003:1178c13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或推而補苴焉。",
        "from": "T55n2184_003:1178c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或疑而闕如焉。",
        "from": "T55n2184_003:1178c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且又此錄本未為盡矣。",
        "from": "T55n2184_003:1178c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本朝見行之章疏。",
        "from": "T55n2184_003:1178c15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遺餘者惟多。",
        "from": "T55n2184_003:1178c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冀俟明眼博覽之考訂。",
        "from": "T55n2184_003:1178c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣行于世。",
        "from": "T55n2184_003:1178c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是余護法之微志也。",
        "from": "T55n2184_003:1178c16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洛下書林　　井上忠兵衛　壽梓",
        "from": "T55n2184_003:1178c19_0",
        "context_with_next_sentence": true
    }
]