[
    {
        "source_sentence": "摩醯首羅大自在天王神通化生",
        "from": "T21n1280_001:0340b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伎藝天女念誦法",
        "from": "T21n1280_001:0340b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時摩醯首羅天王。於大自在天上。與諸天",
        "from": "T21n1280_001:0340b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "女前後圍繞。",
        "from": "T21n1280_001:0340b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神通遊戲作諸伎樂。",
        "from": "T21n1280_001:0340b23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忽然之間於髮際中化出一天女。",
        "from": "T21n1280_001:0340b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顏容端正伎藝第一。",
        "from": "T21n1280_001:0340b24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸天無能勝者。",
        "from": "T21n1280_001:0340b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於大眾中而作是言。",
        "from": "T21n1280_001:0340b25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今為欲利益一切。",
        "from": "T21n1280_001:0340b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有所祈願豐饒吉祥富樂之事。",
        "from": "T21n1280_001:0340b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨心悕求悉能滿足。",
        "from": "T21n1280_001:0340b27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸業藝速能成就。",
        "from": "T21n1280_001:0340b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我有祕密陀羅尼法要。",
        "from": "T21n1280_001:0340b28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今當說之即說陀羅尼曰。",
        "from": "T21n1280_001:0340b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨引榅支摩貌施佉地尾鉢羅二合鉢地野二合引試引迦囉者嚕啉二合怛儞野二合他去引濕嚩二合惹底㘑吠囉摩惹哩儞吽引發吒半音娑嚩二合引賀引是時眾伎藝天女。",
        "from": "T21n1280_001:0340c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此陀羅尼已。",
        "from": "T21n1280_001:0340c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告諸天眾。",
        "from": "T21n1280_001:0340c05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲誦持我此陀羅尼者。",
        "from": "T21n1280_001:0340c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先須建立道場如法嚴飾。",
        "from": "T21n1280_001:0340c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種香花以為供養。",
        "from": "T21n1280_001:0340c07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於二七日或一七日。",
        "from": "T21n1280_001:0340c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持齋戒斷其婬欲。",
        "from": "T21n1280_001:0340c08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此陀羅尼滿其萬遍或十萬遍。",
        "from": "T21n1280_001:0340c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其中間勿食酒肉薰穢。",
        "from": "T21n1280_001:0340c09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至心誦持天女現前種種加備。",
        "from": "T21n1280_001:0340c10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切願求皆悉滿足。",
        "from": "T21n1280_001:0340c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從此已後無有障礙。",
        "from": "T21n1280_001:0340c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至妻子薰穢。",
        "from": "T21n1280_001:0340c11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不制斷。",
        "from": "T21n1280_001:0340c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能淨持最為速驗。",
        "from": "T21n1280_001:0340c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後有所用應令成就。",
        "from": "T21n1280_001:0340c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若持誦時先須結界。",
        "from": "T21n1280_001:0340c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或水或灰加持七遍。",
        "from": "T21n1280_001:0340c14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散灑四方隨心遠近。",
        "from": "T21n1280_001:0340c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成結界地。",
        "from": "T21n1280_001:0340c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切鬼神莫能博近。",
        "from": "T21n1280_001:0340c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦至一千遍。",
        "from": "T21n1280_001:0340c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令天龍人及非人見即歡喜。",
        "from": "T21n1280_001:0340c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦至二千遍。",
        "from": "T21n1280_001:0340c17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切鬼神皆悉衛護。",
        "from": "T21n1280_001:0340c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦至三千遍。",
        "from": "T21n1280_001:0340c18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "草藥精靈悉現其前。",
        "from": "T21n1280_001:0340c18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華菓林神歡喜隨從。",
        "from": "T21n1280_001:0340c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦至四千遍。",
        "from": "T21n1280_001:0340c19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切毘多羅鬼深生恭敬。",
        "from": "T21n1280_001:0340c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦至五千遍。",
        "from": "T21n1280_001:0340c20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切龍神眷屬男女歡喜敬仰愛念無厭。",
        "from": "T21n1280_001:0340c21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦至六千遍。",
        "from": "T21n1280_001:0340c21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切藥叉眷屬男女。",
        "from": "T21n1280_001:0340c22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡喜隨逐任其所使。",
        "from": "T21n1280_001:0340c22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦至七千遍。",
        "from": "T21n1280_001:0340c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切天王摩達嚕等。",
        "from": "T21n1280_001:0340c23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并其眷屬以給侍。",
        "from": "T21n1280_001:0340c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種驅役不以為倦。",
        "from": "T21n1280_001:0340c24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦至八千遍。",
        "from": "T21n1280_001:0340c24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切持明五通仙人等。",
        "from": "T21n1280_001:0340c25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深生歡喜仰無盡。",
        "from": "T21n1280_001:0340c25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦滿一萬遍。",
        "from": "T21n1280_001:0340c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至十萬遍。",
        "from": "T21n1280_001:0340c26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切天龍藥叉乾闥婆阿修羅緊那羅摩呼羅伽人非人等。",
        "from": "T21n1280_001:0340c27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至塞乾陀天一切鬼神。",
        "from": "T21n1280_001:0340c28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生大歡喜隨逐侍衛。",
        "from": "T21n1280_001:0340c28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常令行者豐饒吉祥。",
        "from": "T21n1280_001:0340c29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲祈止不以為難。",
        "from": "T21n1280_001:0341a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若天旱時如法結界壇。",
        "from": "T21n1280_001:0341a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護摩念誦誦此真言滿一萬遍。",
        "from": "T21n1280_001:0341a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其雨應時即下。",
        "from": "T21n1280_001:0341a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若雨過多惡欲止者。",
        "from": "T21n1280_001:0341a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持淨灰一百八遍。",
        "from": "T21n1280_001:0341a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空中擲之其雨即止。",
        "from": "T21n1280_001:0341a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若有怨家興起惡意。",
        "from": "T21n1280_001:0341a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以用朱砂或其赤土。",
        "from": "T21n1280_001:0341a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書名或書形。",
        "from": "T21n1280_001:0341a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於左脚下踏之念誦。",
        "from": "T21n1280_001:0341a06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜後作之一百八遍。",
        "from": "T21n1280_001:0341a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無問遠近彼怨自來求解怨結。",
        "from": "T21n1280_001:0341a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心生和順轉生愛敬。",
        "from": "T21n1280_001:0341a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲使役一切鬼神。",
        "from": "T21n1280_001:0341a09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆當召集生愍念心。",
        "from": "T21n1280_001:0341a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨分飲食加持一百八遍。",
        "from": "T21n1280_001:0341a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而散施之。",
        "from": "T21n1280_001:0341a10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後驅使速疾成辦。",
        "from": "T21n1280_001:0341a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常生歡喜終不離人。",
        "from": "T21n1280_001:0341a11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲鉤召一切人民。",
        "from": "T21n1280_001:0341a12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨心所念無問遠近。",
        "from": "T21n1280_001:0341a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持水七遍。",
        "from": "T21n1280_001:0341a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散於四方望彼稱誦一百八遍。",
        "from": "T21n1280_001:0341a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無問識與不識。",
        "from": "T21n1280_001:0341a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉來奔就隨心所為不敢違逆。",
        "from": "T21n1280_001:0341a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若有怨家來欲相害。",
        "from": "T21n1280_001:0341a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至心誦明一百八遍。",
        "from": "T21n1280_001:0341a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼即被打身契如火。",
        "from": "T21n1280_001:0341a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若迴心相伏。",
        "from": "T21n1280_001:0341a17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即與解之時。",
        "from": "T21n1280_001:0341a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦明七遍云放。",
        "from": "T21n1280_001:0341a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即放其身即平復如本。",
        "from": "T21n1280_001:0341a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若有夫婦。",
        "from": "T21n1280_001:0341a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外心相背不相喜見。",
        "from": "T21n1280_001:0341a20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如火水相憎者。",
        "from": "T21n1280_001:0341a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密書其名勿令知覺。",
        "from": "T21n1280_001:0341a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一依前法左脚踏之。",
        "from": "T21n1280_001:0341a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱名誦一千八遍。",
        "from": "T21n1280_001:0341a22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即相和順轉相怜愛如同膠漆。",
        "from": "T21n1280_001:0341a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永更不生憎背之意。",
        "from": "T21n1280_001:0341a23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若有所愛宣婦。",
        "from": "T21n1280_001:0341a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至諸天女婇及天帝釋妃后眷屬心所愛者。",
        "from": "T21n1280_001:0341a25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一依前法念誦召之。",
        "from": "T21n1280_001:0341a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應時即至自來求之請為給侍。",
        "from": "T21n1280_001:0341a26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不違之終不捨離。",
        "from": "T21n1280_001:0341a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲鉤召藥叉女等。",
        "from": "T21n1280_001:0341a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求其財寶隨心召之。",
        "from": "T21n1280_001:0341a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迅疾即至。",
        "from": "T21n1280_001:0341a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切所須悉皆奉獻。",
        "from": "T21n1280_001:0341a29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請為妻妾或為給侍。",
        "from": "T21n1280_001:0341b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲得一切貴人愛敬。",
        "from": "T21n1280_001:0341b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱名念誦一千八遍。",
        "from": "T21n1280_001:0341b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自來歸敬以為僕使。",
        "from": "T21n1280_001:0341b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若入官府。",
        "from": "T21n1280_001:0341b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至心誦明七遍而至。",
        "from": "T21n1280_001:0341b04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遙仰善相使得隨心。",
        "from": "T21n1280_001:0341b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見面時喜悅無量。",
        "from": "T21n1280_001:0341b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有事求之即能相為。",
        "from": "T21n1280_001:0341b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若有所愛女人。",
        "from": "T21n1280_001:0341b07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或其男子意相樂者。",
        "from": "T21n1280_001:0341b07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以加持胡燕脂或紅藍花汁。",
        "from": "T21n1280_001:0341b08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗其二脚掌下。",
        "from": "T21n1280_001:0341b08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以火炙之二手掩上。",
        "from": "T21n1280_001:0341b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱名念誦彼即立至。",
        "from": "T21n1280_001:0341b09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自來相求乃至深隱之處。",
        "from": "T21n1280_001:0341b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直來相來見乃至命終不肯相捨每稱名時於娑婆二合賀句上加之除其發吒二字。",
        "from": "T21n1280_001:0341b11_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若是女人為求男子者。",
        "from": "T21n1280_001:0341b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並依前法作之隨心即至。",
        "from": "T21n1280_001:0341b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲得令見諸鬼神身者。",
        "from": "T21n1280_001:0341b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取尸陀林燒死人柴。",
        "from": "T21n1280_001:0341b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護摩一明一燒。",
        "from": "T21n1280_001:0341b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至一百八遍。",
        "from": "T21n1280_001:0341b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切鬼神悉自現身即得相見。",
        "from": "T21n1280_001:0341b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若有所有一切財寶。",
        "from": "T21n1280_001:0341b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨心所須至心持誦自然獲得。",
        "from": "T21n1280_001:0341b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲縛人問事。",
        "from": "T21n1280_001:0341b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無問大小淨與不淨。",
        "from": "T21n1280_001:0341b18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心想本尊至心念誦。",
        "from": "T21n1280_001:0341b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應時即縛問知三世之事。",
        "from": "T21n1280_001:0341b19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切皆說無所不知。",
        "from": "T21n1280_001:0341b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非但能縛一人。",
        "from": "T21n1280_001:0341b20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至百千萬人至心誦之。",
        "from": "T21n1280_001:0341b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨心應念一時即縛。",
        "from": "T21n1280_001:0341b21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若有家人逃走極遠處去者。",
        "from": "T21n1280_001:0341b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "準前書名踏之。",
        "from": "T21n1280_001:0341b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦其人迴還奔走立至。",
        "from": "T21n1280_001:0341b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若來遲者即以灰書彼形。",
        "from": "T21n1280_001:0341b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每日三時作法。",
        "from": "T21n1280_001:0341b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每誦真言一遍打之。",
        "from": "T21n1280_001:0341b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一下乃至三七遍已。",
        "from": "T21n1280_001:0341b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼即恐怖如風即至。",
        "from": "T21n1280_001:0341b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲療治一切病者。",
        "from": "T21n1280_001:0341b27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取佉陀羅木。",
        "from": "T21n1280_001:0341b27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "截作二十一段。",
        "from": "T21n1280_001:0341b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦明一投火中燒之。",
        "from": "T21n1280_001:0341b28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍已但是疾病即得除差。",
        "from": "T21n1280_001:0341b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若有患神病者。",
        "from": "T21n1280_001:0341c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取阿魏藥安悉香雄黃白芥子青木香若練葉等。",
        "from": "T21n1280_001:0341c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並擣節相和作四十九丸。",
        "from": "T21n1280_001:0341c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一誦明一投火中其病即差。",
        "from": "T21n1280_001:0341c03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若患狐魅病者。",
        "from": "T21n1280_001:0341c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取雄黃白芥子芎藭獨頭萩犀角羖羊尿白馬懸蹄驢馬夜眼。",
        "from": "T21n1280_001:0341c05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並擣篩為丸。",
        "from": "T21n1280_001:0341c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持一百八遍。",
        "from": "T21n1280_001:0341c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒薰鼻孔之中。",
        "from": "T21n1280_001:0341c06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并塗身上狐魅消滅其病即差。",
        "from": "T21n1280_001:0341c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如上所說利益之法悉皆神驗。",
        "from": "T21n1280_001:0341c08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速疾成就。",
        "from": "T21n1280_001:0341c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡欲作法必須至誠。",
        "from": "T21n1280_001:0341c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不至心輕慢誦者無益法亦不成。",
        "from": "T21n1280_001:0341c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若至誠心一遍便獲効驗。",
        "from": "T21n1280_001:0341c10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今略說少耳。",
        "from": "T21n1280_001:0341c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘法莫能窮盡。",
        "from": "T21n1280_001:0341c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲持誦求其證驗。",
        "from": "T21n1280_001:0341c11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先畫我壇并畫我像安置壇中。",
        "from": "T21n1280_001:0341c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如法供養即疾證驗即疾成就。",
        "from": "T21n1280_001:0341c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫像法大自在天女畫像法先畫摩醯首羅天王。",
        "from": "T21n1280_001:0341c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三面六臂顏貌奇特端正可畏。",
        "from": "T21n1280_001:0341c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從其髮際化生一天女。",
        "from": "T21n1280_001:0341c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殊妙可喜天上人間無能勝者。",
        "from": "T21n1280_001:0341c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著天衣服瓔珞嚴身兩手腕上。",
        "from": "T21n1280_001:0341c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各有鐶釧左手向上捧一天花。",
        "from": "T21n1280_001:0341c18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手向下作捻裙勢。",
        "from": "T21n1280_001:0341c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身形可長三尺。",
        "from": "T21n1280_001:0341c19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或隨大小任取稱量畫像了已。",
        "from": "T21n1280_001:0341c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如法安置壇中供養。",
        "from": "T21n1280_001:0341c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈願壇法祈願壇法量高四指肘量隨其處所大小。",
        "from": "T21n1280_001:0341c22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壇開四門於東門內燒安悉香。",
        "from": "T21n1280_001:0341c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南門內燒沈水香。",
        "from": "T21n1280_001:0341c23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西門燒薰陸香。",
        "from": "T21n1280_001:0341c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北門燒兜樓婆香。",
        "from": "T21n1280_001:0341c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於壇中心安本尊像。",
        "from": "T21n1280_001:0341c24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於四角安刀一口懸明鏡。",
        "from": "T21n1280_001:0341c25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於四門各須安箭兩隻。",
        "from": "T21n1280_001:0341c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於南門兩邊安尾那夜迦位。",
        "from": "T21n1280_001:0341c26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於壇四緣周迴隨其力分。",
        "from": "T21n1280_001:0341c27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安置甜脆華果等以為供養。",
        "from": "T21n1280_001:0341c27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火壇法次說火壇法作護摩爐深十二指。",
        "from": "T21n1280_001:0341c29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爐可橫量一肘。",
        "from": "T21n1280_001:0341c29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於壇傍作彼怨家形貌。",
        "from": "T21n1280_001:0342a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以刀之割護摩念誦。",
        "from": "T21n1280_001:0342a01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以用皂夾刺柴燒諸毒藥。",
        "from": "T21n1280_001:0342a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一誦明一燒之。",
        "from": "T21n1280_001:0342a02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿四十九遍或一百八遍。",
        "from": "T21n1280_001:0342a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即大驗耳彼怨家即便消滅。",
        "from": "T21n1280_001:0342a03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若療一切疾病亦如上法。",
        "from": "T21n1280_001:0342a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作彼鬼神形狀。",
        "from": "T21n1280_001:0342a04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷截燒之決死之病應時得愈。",
        "from": "T21n1280_001:0342a05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩醯首羅大自在天神通遊戲變化頂生眾伎藝天女成就法印。",
        "from": "T21n1280_001:0342a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手合掌。",
        "from": "T21n1280_001:0342a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二名指二中指。",
        "from": "T21n1280_001:0342a08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外相叉指面著其手背。",
        "from": "T21n1280_001:0342a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指微蹙頭似寶。",
        "from": "T21n1280_001:0342a09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指頭並來去。",
        "from": "T21n1280_001:0342a09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成誦請召明曰。",
        "from": "T21n1280_001:0342a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨引摩醯濕嚩二合羅引野一榅支摩冒引試佉野曀醯去引呬娑嚩二合引賀引結此印已。",
        "from": "T21n1280_001:0342a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨誦此明七遍。",
        "from": "T21n1280_001:0342a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請召本天與諸眷屬。",
        "from": "T21n1280_001:0342a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆降至種種加備。",
        "from": "T21n1280_001:0342a14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結神通攝召一切印。",
        "from": "T21n1280_001:0342a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手中指直竪。",
        "from": "T21n1280_001:0342a15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以名指苾在中指後。",
        "from": "T21n1280_001:0342a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以頭指鉤召指頭。",
        "from": "T21n1280_001:0342a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大指橫押頭小二指甲。",
        "from": "T21n1280_001:0342a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以中指如鉤勢向外。",
        "from": "T21n1280_001:0342a17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鉤向身來去。",
        "from": "T21n1280_001:0342a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是誦根本明。",
        "from": "T21n1280_001:0342a18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於娑嚩二合賀字上加所召者名請之。",
        "from": "T21n1280_001:0342a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若結此印誦明攝召一切。",
        "from": "T21n1280_001:0342a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆立至。",
        "from": "T21n1280_001:0342a20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是神通攝召印。",
        "from": "T21n1280_001:0342a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦通一切處用。",
        "from": "T21n1280_001:0342a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若逢怨惡人。",
        "from": "T21n1280_001:0342a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲相刑害。",
        "from": "T21n1280_001:0342a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結印展其中指。",
        "from": "T21n1280_001:0342a22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "望彼撝之自然散壞。",
        "from": "T21n1280_001:0342a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸惡鬼神指之消滅。",
        "from": "T21n1280_001:0342a23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結此印誦根本明。",
        "from": "T21n1280_001:0342a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印心額喉頂即成護身。",
        "from": "T21n1280_001:0342a24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切鬼魅不敢親近。",
        "from": "T21n1280_001:0342a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其法必須從師親授。",
        "from": "T21n1280_001:0342a25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨室密持每念誦了。",
        "from": "T21n1280_001:0342a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以淨繒帛覆天形像。",
        "from": "T21n1280_001:0342a26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得輒說此法。",
        "from": "T21n1280_001:0342a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向餘人。",
        "from": "T21n1280_001:0342a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若妄泄之終身無効。",
        "from": "T21n1280_001:0342a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慎之慎之。",
        "from": "T21n1280_001:0342a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其天像或畫或素總得。",
        "from": "T21n1280_001:0342a29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於壇中心安置本天形像。",
        "from": "T21n1280_001:0342b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於天左右各安二女天。",
        "from": "T21n1280_001:0342b01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若壇中安坐位。",
        "from": "T21n1280_001:0342b01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形貌可喜。",
        "from": "T21n1280_001:0342b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如天女形狀。",
        "from": "T21n1280_001:0342b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其像後又置二侍從女天位。",
        "from": "T21n1280_001:0342b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯於前面留火爐處所。",
        "from": "T21n1280_001:0342b03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒諸香藥奉獻本天。",
        "from": "T21n1280_001:0342b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如不能安火爐。",
        "from": "T21n1280_001:0342b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著一寶盆。",
        "from": "T21n1280_001:0342b04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盛其香水著諸妙花獻之亦得。",
        "from": "T21n1280_001:0342b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於壇四角四門不能安其刀箭。",
        "from": "T21n1280_001:0342b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但於壇外畫之亦得。",
        "from": "T21n1280_001:0342b06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯移鏡安於壇內四角。",
        "from": "T21n1280_001:0342b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如餘部壇法。",
        "from": "T21n1280_001:0342b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安羯磨杵處即得仰安之。",
        "from": "T21n1280_001:0342b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若畫壇時。",
        "from": "T21n1280_001:0342b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於壇四面地上畫作五色綵雲。",
        "from": "T21n1280_001:0342b09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如捧壇從地踴出勢。",
        "from": "T21n1280_001:0342b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩醯首羅頂生天女法",
        "from": "T21n1280_001:0342b11_100",
        "context_with_next_sentence": false
    }
]