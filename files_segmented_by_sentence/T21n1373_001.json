[
    {
        "source_sentence": "佛說施一切無畏陀羅尼經",
        "from": "T21n1373_001:0889c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝奉大夫試光祿卿",
        "from": "T21n1373_001:0889c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳法大師賜紫臣施護等奉　詔譯",
        "from": "T21n1373_001:0889c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T21n1373_001:0889c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時世尊。",
        "from": "T21n1373_001:0889c21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遊行到於摩伽陀國菴沒羅林。",
        "from": "T21n1373_001:0889c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住韋提呬山帝釋巖中。",
        "from": "T21n1373_001:0889c22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時帝釋天主來詣佛所。",
        "from": "T21n1373_001:0889c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前白佛言世尊。",
        "from": "T21n1373_001:0889c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我有怖畏謂阿修羅。",
        "from": "T21n1373_001:0889c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於長夜中來相嬈亂。",
        "from": "T21n1373_001:0889c24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿修羅者我之怨敵。",
        "from": "T21n1373_001:0889c25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願世尊為我宣說擁護法門。",
        "from": "T21n1373_001:0889c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告帝釋天主言。",
        "from": "T21n1373_001:0889c27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天主我有陀羅尼。",
        "from": "T21n1373_001:0889c27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名施一切無畏。",
        "from": "T21n1373_001:0889c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此法善能作一切事。",
        "from": "T21n1373_001:0889c28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除一切病。",
        "from": "T21n1373_001:0889c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復能禁止一切傍生。",
        "from": "T21n1373_001:0889c29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能解一切怨縛。",
        "from": "T21n1373_001:0889c29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有阿修羅及諸羅剎部多一切邪異。",
        "from": "T21n1373_001:0890a01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至一切諸惡鬼神之所執持。",
        "from": "T21n1373_001:0890a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂邪惡所執。",
        "from": "T21n1373_001:0890a02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天執龍執夜叉執乾闥婆執。",
        "from": "T21n1373_001:0890a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緊那羅執頻那夜迦執。",
        "from": "T21n1373_001:0890a03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母鬼等執皆悉消滅。",
        "from": "T21n1373_001:0890a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復若有吸精氣者。",
        "from": "T21n1373_001:0890a04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食華鬘者障產生者。",
        "from": "T21n1373_001:0890a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食不淨者皆悉除遣。",
        "from": "T21n1373_001:0890a05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復若有一切瘧疾。",
        "from": "T21n1373_001:0890a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及風黃痰癊等諸疾病亦悉消散。",
        "from": "T21n1373_001:0890a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切鬪戰諍訟陣敵皆悉破壞。",
        "from": "T21n1373_001:0890a07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今為汝說此陀羅尼而作擁護。",
        "from": "T21n1373_001:0890a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陀羅尼曰。",
        "from": "T21n1373_001:0890a08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛𭔞切身下同他引一句伊抳二彌抳三鉢囉二合彌抳四鉢囉二合彌抳哩五吾哩六謨引里七謨引瑟恥二合八那胝九嚩胝十珂囉胝十一珂囉尼十二誐拏尼十三誐拏鉢囉二合誐尼十四誐𧹞誐尼引十五謨引尼引十六鉢囉二合底謨引尼引十七哥引里十八鉢囉二合哥引里引十九贊抳二十鉢囉二合底贊抳二十一尾詣引二十二鉢囉二合底尾詣引二十三阿底尾詣引二十四蘇引那引二十五吾引拏引二十六謨引訶引二十七鉢囉二合謨引訶引二十八謨引拏引二十九鉢囉二合謨引拏引三十那引設儞三十一鉢囉二合那引設儞三十二馱引嚩儞三十三鉢囉二合馱引嚩儞三十四嚩洛誐二合儞三十五鉢囉二合嚩洛誐二合儞三十六涅哩二合多儞三十七鉢囉二合涅哩二合多儞三十八骨嚕二合達儞三十九阿底骨嚕二合引達儞四十賀那賀那四十一尾賀那尾賀那四十二薩哩嚩二合訥瑟吒二合鉢囉二合訥瑟吒二合那引設野四十三薩哩嚩二合跋野鉢囉二合那四十四犖力角切下同叉犖叉𤚥引四十五薩哩嚩二合咩引提跋野四十六訥瑟吒二合設咄嚕二合怛塞哥二合囉能瑟吒嚕三合引波捺囉二合吠引毘藥二合四十七薩波哩嚩囕四十八詰禰詰禰四十九薩哩嚩二合薩埵呬帝引五十囉帝引那莫薩哩嚩二合沒馱引喃引莎引賀引五十一此施一切無畏陀羅尼。",
        "from": "T21n1373_001:0890b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若持誦者。",
        "from": "T21n1373_001:0890b03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當以淨線加持四十一遍繫於頂上。",
        "from": "T21n1373_001:0890b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或加持白芥子或加持牛黃。",
        "from": "T21n1373_001:0890b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨諸所用即得成就。",
        "from": "T21n1373_001:0890b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復若以塗香加持一遍塗自身分。",
        "from": "T21n1373_001:0890b06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即如被甲冑勇猛無畏。",
        "from": "T21n1373_001:0890b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為刀傷不為毒中。",
        "from": "T21n1373_001:0890b07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復不為一日二日乃至七日等諸瘧疾之所纏縛。",
        "from": "T21n1373_001:0890b08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為火焚不為水溺。",
        "from": "T21n1373_001:0890b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為呪害不為鬼魅不為病惱。",
        "from": "T21n1373_001:0890b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不於一切夭橫中死。",
        "from": "T21n1373_001:0890b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此陀羅尼善能作諸相應事業。",
        "from": "T21n1373_001:0890b11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切成就法皆悉和合。",
        "from": "T21n1373_001:0890b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作諸成就事無不成就者。",
        "from": "T21n1373_001:0890b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他所作法皆能制止。",
        "from": "T21n1373_001:0890b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為他縛能破邪明自法隨轉。",
        "from": "T21n1373_001:0890b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切魔障皆能除遣。",
        "from": "T21n1373_001:0890b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切執魅悉能解散。",
        "from": "T21n1373_001:0890b14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有所執不能除者。",
        "from": "T21n1373_001:0890b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭破作七分如阿梨樹枝。",
        "from": "T21n1373_001:0890b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時帝釋天主。",
        "from": "T21n1373_001:0890b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛所說踊躍歡喜作禮而去。",
        "from": "T21n1373_001:0890b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說施一切無畏陀羅尼經",
        "from": "T21n1373_001:0890b19_100",
        "context_with_next_sentence": false
    }
]