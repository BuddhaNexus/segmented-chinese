[
    {
        "source_sentence": "施諸餓鬼飲食及水法并手印",
        "from": "T21n1315_001:0466c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "特進試鴻臚卿大興善寺三藏沙門",
        "from": "T21n1315_001:0466c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大廣智不空奉　詔譯",
        "from": "T21n1315_001:0466c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先出眾生食。",
        "from": "T21n1315_001:0466c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "事須如法。",
        "from": "T21n1315_001:0466c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周匝種種皆著並須淨好。",
        "from": "T21n1315_001:0466c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或一分或少許或一器。",
        "from": "T21n1315_001:0466c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆須安淨銅器中如法。",
        "from": "T21n1315_001:0466c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如無銅器白瓷亦得。",
        "from": "T21n1315_001:0466c10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如無瓷器可用漆器。",
        "from": "T21n1315_001:0466c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其飲食須和清水。",
        "from": "T21n1315_001:0466c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面向東立坐亦得作法。",
        "from": "T21n1315_001:0466c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫欲施一切餓鬼飲食者。",
        "from": "T21n1315_001:0466c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先須發廣大心普請餓鬼。",
        "from": "T21n1315_001:0466c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先誦此偈至心一遍。",
        "from": "T21n1315_001:0466c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後作召請法。",
        "from": "T21n1315_001:0466c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所獲福利果報不可校量。",
        "from": "T21n1315_001:0466c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比丘比丘尼某甲發心奉持　　一器淨食",
        "from": "T21n1315_001:0466c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普施十方　　窮盡虛空　　周遍法界",
        "from": "T21n1315_001:0466c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微塵剎中　　所有國土　　一切餓鬼",
        "from": "T21n1315_001:0466c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先亡久遠　　山川地主　　乃至曠野",
        "from": "T21n1315_001:0466c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸鬼神等　　請來集此　　我今悲愍",
        "from": "T21n1315_001:0466c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普施汝食　　願汝各各　　受我此食",
        "from": "T21n1315_001:0466c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉將供養　　盡虛空界　　以佛及聖",
        "from": "T21n1315_001:0466c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切有情　　汝與有情　　普皆飽滿",
        "from": "T21n1315_001:0466c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦願汝身　　乘此呪食　　離苦解脫",
        "from": "T21n1315_001:0466c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生天受樂　　十方淨土　　隨意遊往",
        "from": "T21n1315_001:0466c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發菩提心　　行菩提道　　當來作佛",
        "from": "T21n1315_001:0466c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永莫退轉　　前得道者　　誓相度脫",
        "from": "T21n1315_001:0466c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又願汝等　　晝夜恒常　　擁護於我",
        "from": "T21n1315_001:0466c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿我所願　　願施此食　　所生功德",
        "from": "T21n1315_001:0466c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普將廻施　　法界有情　　與諸有情",
        "from": "T21n1315_001:0467a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平等共有　　共諸有情　　同將此福",
        "from": "T21n1315_001:0467a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡將迴向　　真如法界　　無上菩提",
        "from": "T21n1315_001:0467a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切智智　　願速成佛　　勿招餘果",
        "from": "T21n1315_001:0467a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願乘此法　　疾得成佛",
        "from": "T21n1315_001:0467a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌當心誦此偈。",
        "from": "T21n1315_001:0467a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印作召請　開喉印　以右手大指與中指。",
        "from": "T21n1315_001:0467a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面相捻。",
        "from": "T21n1315_001:0467a07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘三指相去。",
        "from": "T21n1315_001:0467a07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微作曲勢即是。",
        "from": "T21n1315_001:0467a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名普集印　呪曰。",
        "from": "T21n1315_001:0467a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄖩曩􄗭謨􄗞步􄖹布入􄙀哩􄑀迦􄙀哩􄕗多􄙀哩􄕗怛􄕤他引􄑘蘖􄕘多􄗱􇧆也作此印誦此呪一七遍。",
        "from": "T21n1315_001:0467a13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣運悲心。",
        "from": "T21n1315_001:0467a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願令法界微塵剎中。",
        "from": "T21n1315_001:0467a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切餓鬼悉皆雲集。",
        "from": "T21n1315_001:0467a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又誦開地獄門及咽喉呪曰。",
        "from": "T21n1315_001:0467a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵􄗞步􄖹布入􄕝帝􄙀哩􄑀迦􄕗多􄙀哩􄕗怛􄕤他􄑘蘖􄕘多引􄗱􇧆也誦此呪時。",
        "from": "T21n1315_001:0467a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左手執持食器。",
        "from": "T21n1315_001:0467a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手作前召請印。",
        "from": "T21n1315_001:0467a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯改一誦呪一彈指。",
        "from": "T21n1315_001:0467a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大指與中指頭相捻。",
        "from": "T21n1315_001:0467a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彈指作聲即是。",
        "from": "T21n1315_001:0467a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘三指開稍微曲此名破地獄門及開咽喉印。",
        "from": "T21n1315_001:0467a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時如來。",
        "from": "T21n1315_001:0467a23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說無量威德自在光明勝妙之力加持飲食陀羅尼曰。",
        "from": "T21n1315_001:0467a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄖩曩􄗰莫􄙺薩􅞣嚩􄕗怛􄕤他􄑘蘖􄕘多􄙖嚩􄙒嚕􄑂吉􄕝帝􇩂唵􄚦三􄗙娑􄗽羅􄚦三􄗙婆􄗽羅􇩴􇧆吽引誦此呪一七遍。",
        "from": "T21n1315_001:0467a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切餓鬼各皆得摩伽陀國所用之斗七七斛之食。",
        "from": "T21n1315_001:0467b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食已皆得生天或生淨土。",
        "from": "T21n1315_001:0467b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令行者業障消除增益壽命。",
        "from": "T21n1315_001:0467b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現世獲無量無邊福。",
        "from": "T21n1315_001:0467b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "況當來世。",
        "from": "T21n1315_001:0467b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即作手印誦此真言加持飲食。",
        "from": "T21n1315_001:0467b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手大指。",
        "from": "T21n1315_001:0467b04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩中指甲三兩遍。",
        "from": "T21n1315_001:0467b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三指直立之。",
        "from": "T21n1315_001:0467b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以大指捻頭指。",
        "from": "T21n1315_001:0467b05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彈指作聲。",
        "from": "T21n1315_001:0467b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一誦呪一彈指即是。",
        "from": "T21n1315_001:0467b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又誦蒙甘露法味真言。",
        "from": "T21n1315_001:0467b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作施無畏印。",
        "from": "T21n1315_001:0467b07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手竪臂。",
        "from": "T21n1315_001:0467b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "展五指直上即是。",
        "from": "T21n1315_001:0467b08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1315_001:0467b08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄖩曩􄗰莫􈂼蘇􄙃嚕􄖵頗􄗱也􄕗怛􄕤他􄑘蘖􄕘多􄗱也􄕗怛􄟯儞也􄕤他􇩂唵􄭺蘇嚕􄭺蘇嚕􇩄鉢羅􄭺蘇嚕􇩄鉢羅􄭺蘇嚕􇩉娑嚩二合􄚩􇧆賀引作前施無畏印。",
        "from": "T21n1315_001:0467b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此呪施甘露真言一七遍。",
        "from": "T21n1315_001:0467b15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令飲食及水。",
        "from": "T21n1315_001:0467b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變成無量乳及甘露。",
        "from": "T21n1315_001:0467b16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能開一切餓鬼咽喉。",
        "from": "T21n1315_001:0467b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令飲食廣得增多平等得喫也。",
        "from": "T21n1315_001:0467b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次作毘盧遮那一字心水輪觀真言印。",
        "from": "T21n1315_001:0467b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先想此􈆯鑁字於右手心中。",
        "from": "T21n1315_001:0467b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如乳色。",
        "from": "T21n1315_001:0467b20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變為八功德海。",
        "from": "T21n1315_001:0467b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流出一切甘露醍醐。",
        "from": "T21n1315_001:0467b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即引手臨食器上呪曰。",
        "from": "T21n1315_001:0467b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此􈆯鑁字一七遍。",
        "from": "T21n1315_001:0467b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即展開五指。",
        "from": "T21n1315_001:0467b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向下臨食器中。",
        "from": "T21n1315_001:0467b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀想乳等從字中流出。",
        "from": "T21n1315_001:0467b23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如日月乳海。",
        "from": "T21n1315_001:0467b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切鬼等皆得飽滿無有乏少。",
        "from": "T21n1315_001:0467b24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此名普施一切餓鬼印真言曰。",
        "from": "T21n1315_001:0467b25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄖩曩􄗰莫􄙺三􄗥滿􆩖多􈎭沒􇧺馱􈁀喃􄙠􇧆鑁去觀想誦此呪一七遍已。",
        "from": "T21n1315_001:0467b28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寫於淨地無人行處。",
        "from": "T21n1315_001:0467b28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或水池邊樹下。",
        "from": "T21n1315_001:0467b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯不得寫於桃樹柳樹石榴樹下。",
        "from": "T21n1315_001:0467c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寫訖更為至心。",
        "from": "T21n1315_001:0467c01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱五如來名號三遍功德無量。",
        "from": "T21n1315_001:0467c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄖩曩􄗭謨􄗙薄􄑘伽􄙖筏􄕝帝􇩄鉢囉􄗞步􄕗多􄗽囉􅑉怛曩􄗱也􄕗怛􄕤他􄑘蘖􄕘多引􄗱􇧆也曩謨寶勝如來除慳貪業福德圓滿。",
        "from": "T21n1315_001:0467c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄖩曩􄗭謨􄗙薄􄑘伽􄙖筏􄕝帝􈂼蘇􄙃嚕􄗲波􄗱耶􄕗怛􄕤他􄑘蘖􄕘多引􄗱􇧆也曩謨妙色身如來破醜陋形相好圓滿。",
        "from": "T21n1315_001:0467c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄖩曩􄗭謨􄗙婆􄑘伽􄙖筏􄕝帝􆿅阿􇩸蜜㗚􄕝帝􄗾囉􄒶惹􄗱耶􄕗怛􄕤他􄑘蘖􄕘多引􄗱􇧆耶曩謨甘露王如來灌法身心令受快樂。",
        "from": "T21n1315_001:0467c17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄖩曩􄗭謨􄗙婆􄑘伽􄙖筏􄕝帝􄙘尾􄖹布􄙊邏􄑘蘖􄩟怛羅二合􄗱耶􄕗怛􄕤他􄑘蘖􄕘多引􄗱􇧆也曩謨廣博身如來咽喉寬大受妙味。",
        "from": "T21n1315_001:0467c22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄖩曩􄗭謨􄗙婆􄑘伽􄙖筏􄕝帝􆿅阿􄗙婆􄗻演􄑀迦􄗾羅引􄗱耶􄕗怛􄕤他引􄑘蘖􄕘多引􄗱􇧆耶曩謨離怖畏如來恐怖悉除離餓鬼趣。",
        "from": "T21n1315_001:0468a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者若能如此。",
        "from": "T21n1315_001:0468a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為稱五如來名者。",
        "from": "T21n1315_001:0468a06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以佛威光加被彼故。",
        "from": "T21n1315_001:0468a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令一切餓鬼等。",
        "from": "T21n1315_001:0468a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量罪滅無量福生。",
        "from": "T21n1315_001:0468a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得妙色廣博得無怖畏所得飲食。",
        "from": "T21n1315_001:0468a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變成甘露美妙之食。",
        "from": "T21n1315_001:0468a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速離苦身生天淨土。",
        "from": "T21n1315_001:0468a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若施食已。",
        "from": "T21n1315_001:0468a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者當更為諸鬼神等。",
        "from": "T21n1315_001:0468a10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦受菩薩三昧耶戒陀羅尼。",
        "from": "T21n1315_001:0468a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每誦三遍真言曰印合掌也。",
        "from": "T21n1315_001:0468a11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵􄙺三􄗥摩􄗱耶􈁁􇧆薩怛梵誦三遍已。",
        "from": "T21n1315_001:0468a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切鬼神皆得堪聞甚深祕法。",
        "from": "T21n1315_001:0468a14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡得具足三昧耶戒獲無量福。",
        "from": "T21n1315_001:0468a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已施諸餓鬼悉皆飽滿訖。",
        "from": "T21n1315_001:0468a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當須以陀羅尼法發遣。",
        "from": "T21n1315_001:0468a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方得歸於本所。",
        "from": "T21n1315_001:0468a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發遣解脫真言曰。",
        "from": "T21n1315_001:0468a17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵􄙖嚩􄦽日囉􈁃目􄚴乞灑􈂨􇧆穆若誦發遣呪。",
        "from": "T21n1315_001:0468a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先作呪印。",
        "from": "T21n1315_001:0468a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手作拳。",
        "from": "T21n1315_001:0468a20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大指捻頭指。",
        "from": "T21n1315_001:0468a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仰掌彈指作聲。",
        "from": "T21n1315_001:0468a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名發遣啟。",
        "from": "T21n1315_001:0468a21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每寫食了誦一七遍彈指。",
        "from": "T21n1315_001:0468a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令一切鬼神。",
        "from": "T21n1315_001:0468a22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得此食已當得去也。",
        "from": "T21n1315_001:0468a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不發遣不得去也。",
        "from": "T21n1315_001:0468a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不具足如是法者。",
        "from": "T21n1315_001:0468a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施諸餓鬼皆不得周匝。",
        "from": "T21n1315_001:0468a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有得者或有不得者。",
        "from": "T21n1315_001:0468a25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛用功力深可愍哉。",
        "from": "T21n1315_001:0468a25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有行者發菩提心。",
        "from": "T21n1315_001:0468a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能如是修行者。",
        "from": "T21n1315_001:0468a26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足此法施諸餓鬼者。",
        "from": "T21n1315_001:0468a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切餓鬼皆得飽滿無有乏少。",
        "from": "T21n1315_001:0468a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持法之人悉應知之。",
        "from": "T21n1315_001:0468a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若以加持飲食陀羅尼。",
        "from": "T21n1315_001:0468a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持一器淨食寫淨流水中。",
        "from": "T21n1315_001:0468a29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令一切婆羅門仙皆得此食食已異口同音呪願。",
        "from": "T21n1315_001:0468b01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此人於現世中即得延壽。",
        "from": "T21n1315_001:0468b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人具足梵天威德行梵天行。",
        "from": "T21n1315_001:0468b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若以此呪呪一切供養佛物。",
        "from": "T21n1315_001:0468b03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若水若香花飲食。",
        "from": "T21n1315_001:0468b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆呪二十一遍。",
        "from": "T21n1315_001:0468b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而然供養佛。",
        "from": "T21n1315_001:0468b04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即如是種種。",
        "from": "T21n1315_001:0468b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以供養十方一切諸佛無異。",
        "from": "T21n1315_001:0468b05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施燋面餓鬼一切鬼神陀羅尼經要決別有救面然餓鬼陀羅尼經唐實叉難陀譯。",
        "from": "T21n1315_001:0468b07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "再校了　　尊教",
        "from": "T21n1315_001:0468b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十月二十二日一校了　　性寂",
        "from": "T21n1315_001:0468b12_0",
        "context_with_next_sentence": true
    }
]