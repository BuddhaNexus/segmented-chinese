[
    {
        "source_sentence": "護命法門神呪經",
        "from": "T20n1139_001:0584b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐天竺三藏菩提流志譯",
        "from": "T20n1139_001:0584b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。一時薄伽梵。在菩提樹下成正覺",
        "from": "T20n1139_001:0584b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處大制多中。",
        "from": "T20n1139_001:0584b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大苾芻眾千二百五十人俱。",
        "from": "T20n1139_001:0584b21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其名曰具壽舍利子。",
        "from": "T20n1139_001:0584b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具壽大目揵連。",
        "from": "T20n1139_001:0584b22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具壽阿難陀。",
        "from": "T20n1139_001:0584b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是大聲聞等而為上首。",
        "from": "T20n1139_001:0584b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及與諸大菩薩眾俱。",
        "from": "T20n1139_001:0584b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其名曰金剛幢菩薩。",
        "from": "T20n1139_001:0584b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛藏菩薩。",
        "from": "T20n1139_001:0584b24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛手菩薩。",
        "from": "T20n1139_001:0584b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈氏菩薩。",
        "from": "T20n1139_001:0584b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等於賢劫中一切菩薩摩訶薩眾而為上首。",
        "from": "T20n1139_001:0584b26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時金剛手菩薩摩訶薩。",
        "from": "T20n1139_001:0584b27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊。",
        "from": "T20n1139_001:0584b27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願如來應正等覺。",
        "from": "T20n1139_001:0584b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸有情說擁護壽命微妙法門。",
        "from": "T20n1139_001:0584b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此法門與一切有情。",
        "from": "T20n1139_001:0584b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作大光明作大覆護。",
        "from": "T20n1139_001:0584c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼有情得無所畏。",
        "from": "T20n1139_001:0584c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切怨讎諸惡知識。",
        "from": "T20n1139_001:0584c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若天若龍若藥叉。",
        "from": "T20n1139_001:0584c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若羅剎若部多鬼。",
        "from": "T20n1139_001:0584c02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及鳩畔茶人非人等。",
        "from": "T20n1139_001:0584c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼終不能起於惡心而為嬈亂奪其精氣。",
        "from": "T20n1139_001:0584c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遇刀劍飲食毒藥。",
        "from": "T20n1139_001:0584c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "厭禱諸患不能為害。",
        "from": "T20n1139_001:0584c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生行住坐臥。",
        "from": "T20n1139_001:0584c05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "睡眠惛醉及以醒覺。",
        "from": "T20n1139_001:0584c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切處我能守護。",
        "from": "T20n1139_001:0584c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊利益一切諸有情故。",
        "from": "T20n1139_001:0584c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "演說如是種種神呪。",
        "from": "T20n1139_001:0584c07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如我常時於如來所親近承事恒不捨離。",
        "from": "T20n1139_001:0584c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今守護一切有情亦復如是。",
        "from": "T20n1139_001:0584c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切時為義利故。",
        "from": "T20n1139_001:0584c09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為安樂故為饒益故。",
        "from": "T20n1139_001:0584c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故我今勸請如來。",
        "from": "T20n1139_001:0584c10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擁護一切諸善男子善女人住菩薩乘者。",
        "from": "T20n1139_001:0584c11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿令鬪諍罵辱毀訾心懷怨結。",
        "from": "T20n1139_001:0584c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸病苦非時夭橫。",
        "from": "T20n1139_001:0584c12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛手菩薩摩訶薩如是請已。",
        "from": "T20n1139_001:0584c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時薄伽梵告金剛手菩薩言。",
        "from": "T20n1139_001:0584c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉善男子。",
        "from": "T20n1139_001:0584c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝為利益諸有情故勸請如來。",
        "from": "T20n1139_001:0584c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今諦聽善思念之我今為汝演說護命法門。",
        "from": "T20n1139_001:0584c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為善門。",
        "from": "T20n1139_001:0584c16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛姪徒也反下皆同他鄔俱一莽莫補反下皆同俱二鄔俱摩比卑旨反尼三畔柁儞平聲呼下皆同自此已下字注平上去入者皆依平上去入聲呼之四婆上羅者麼儞平五婆上虎梨婆上虎梨六𭢟乃可反虎七虎虎梨八跋䫂丁可反耶藥何反下皆同九莎去聲呼之下皆同訶十爾時如來說此灌頂陀羅尼呪已。",
        "from": "T20n1139_001:0584c22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是世尊復說諸佛之所護念陀羅尼呪長壽法門。",
        "from": "T20n1139_001:0584c23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛姪他跋折常逸反下皆同𠼝自此已下口邊作字者皆轉舌呼之跋折𠼝跋折𠼝一跋折囉柁嚟二跋折囉婆上底丁履反下皆同三跋折囉蘗底四跋折囉柁啼田脂反下皆同五斫訖囉跋折𠼝六斫訖囉柁𠼝七麼𠼝八柁𠼝柁𠼝九鼻𠼝鼻𠼝十莽𠼝質𠼝十一虎虎𠼝十二朋去伽鼻鼻𠼝十三始𠼝十四𠼝𠼝尸十五主主𠼝十六嚕嚕旨十七婆上𠼝者之可反下皆同犁十八主𠼝莽𠼝者𠼝十九曼茶上聲下皆同𠼝二十曼茶儞二十一薩婆播跋鼻那捨尸可反下皆同儞二十二薩婆嚕伽鉢囉奢末儞二十三阿迦羅蜜哩柱丁庾反下皆同鉢囉底鎩柁儞二十四夜藥箇反下皆同婆柱婆思訖反下皆同婆上奔入那奔入𡆆底鎩柁儞二十五怛姪他曼茶儞一伽伽囉尼二謨茶儞三三鉢𡆆謨茶儞四那捨尸可反下皆同儞五三鉢羅那捨儞六薩婆毘蒲也反啼鉢𡆆捨末儞七夜藥可反婆杜娑思訖反婆上奔入那阿迦羅蜜哩柱鉢𡆆底鎩柁儞八怛姪他那捨儞一那捨儞二畔柁儞畔柁儞三瞋去聲呼下同陀瞋陀四鼻𠼝弭𠼝五娑上𠼝六訶怛泥七跋𠼝八婆上聲下皆同𠼝婆𠼝九毘上啼十毘上𠼝毘上𠼝毘上𠼝十一烏沙師可反𠼝十二奢𡆆泥十三婆上𡆆婆嚕泥十四婆𡆆迦茶曳十五婆𡆆𠿒訶訖反摩遮哩尼十六因達羅婆上底十七地地囉野尼十八納慕莫醯濕筏囉囉曳儞十九紇唎師婆社禰二十播跛闍時何反㰤呼可反儞二十一迦去羅婆地儞二十二部多婆地儞二十三薩多丁耶反羯挮二十四素摩跋低二十五素摩奔入𡆆鞞二十六莎訶二十七但姪他𠿒如字下皆同囉𠿒囉一呬虛利反下皆同犁二莎訶三怛姪他揭雉多一鉢雉多二阿曳那三阿𠼝蟾跋泥四翳迦羯柁泥五補多多訖反囉茶𠿒儞六末伽長聲呼毘盧𠿒儞七虎娑跋底八虎娑跋底九補娑跋底十補娑跋底十一呬梨呬梨十二也他長聲下同闍時箇反耶十三也他阿耆耆逸反儞十四也他鉢嚂者十五也他跋監十六也他跋折嚂十七也他訶哩訶訖反柁監十八佛告金剛手菩薩摩訶薩。",
        "from": "T20n1139_001:0585b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此護命善門陀羅尼神呪。",
        "from": "T20n1139_001:0585b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過去如來應正等覺。",
        "from": "T20n1139_001:0585b03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先所演說先已守護。",
        "from": "T20n1139_001:0585b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發大誓願真實建立。",
        "from": "T20n1139_001:0585b04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今亦為利益悲愍一切有情。",
        "from": "T20n1139_001:0585b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發大誓願真實建立。",
        "from": "T20n1139_001:0585b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故說此陀羅尼呪。",
        "from": "T20n1139_001:0585b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時薄伽梵放大人相眉間光明。",
        "from": "T20n1139_001:0585b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其光普曜諸佛剎土。",
        "from": "T20n1139_001:0585b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由佛神變諸剎土中一切如來見此光明。",
        "from": "T20n1139_001:0585b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各從自土俱時發趣索訶世界。",
        "from": "T20n1139_001:0585b09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到釋迦牟尼佛所。",
        "from": "T20n1139_001:0585b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讚言善哉善哉釋迦牟尼佛。",
        "from": "T20n1139_001:0585b10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃能利益擁護一切諸有情故。",
        "from": "T20n1139_001:0585b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此法門陀羅尼呪。",
        "from": "T20n1139_001:0585b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能作光明令得歡喜令得正念。",
        "from": "T20n1139_001:0585b12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故我今稱揚讚歎。",
        "from": "T20n1139_001:0585b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人。",
        "from": "T20n1139_001:0585b13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於晨朝時受持讀誦此陀羅尼神呪句義。",
        "from": "T20n1139_001:0585b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當獲二十種功德。",
        "from": "T20n1139_001:0585b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者當得諸佛之所攝受。",
        "from": "T20n1139_001:0585b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者常為諸佛之所憶念。",
        "from": "T20n1139_001:0585b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者當得永離受惡趣業。",
        "from": "T20n1139_001:0585b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者當得長壽富貴。",
        "from": "T20n1139_001:0585b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者當得名稱遠聞。",
        "from": "T20n1139_001:0585b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六者當得勇猛威力。",
        "from": "T20n1139_001:0585b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七者恒常無病復能精進。",
        "from": "T20n1139_001:0585b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八者常為諸佛之所覆護。",
        "from": "T20n1139_001:0585b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九者復為諸天之所憶念。",
        "from": "T20n1139_001:0585b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十者勤修善行。",
        "from": "T20n1139_001:0585b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十一者當得光明歡喜正念。",
        "from": "T20n1139_001:0585b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二者當得諸相具足。",
        "from": "T20n1139_001:0585b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十三者得無所畏。",
        "from": "T20n1139_001:0585b21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十四者當得具足尸羅。",
        "from": "T20n1139_001:0585b22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十五者當得成就一切善根。",
        "from": "T20n1139_001:0585b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十六者當得諸天守護。",
        "from": "T20n1139_001:0585b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十七者當得諸龍守護。",
        "from": "T20n1139_001:0585b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十八者當得藥叉守護。",
        "from": "T20n1139_001:0585b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十九者當得一切世間之所敬愛。",
        "from": "T20n1139_001:0585b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十者速疾得阿耨多羅三藐三菩提。",
        "from": "T20n1139_001:0585b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告金剛手。",
        "from": "T20n1139_001:0585b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今說此陀羅尼呪。",
        "from": "T20n1139_001:0585b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是諸佛如來應正等覺祕密之處。",
        "from": "T20n1139_001:0585b27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸佛之所守護。",
        "from": "T20n1139_001:0585b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時金剛手菩薩摩訶薩。",
        "from": "T20n1139_001:0585b29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊。",
        "from": "T20n1139_001:0585b29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今亦為擁護利益諸善男子善女人故。",
        "from": "T20n1139_001:0585c01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說陀羅尼。",
        "from": "T20n1139_001:0585c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人能受持此護命法門陀羅尼呪。",
        "from": "T20n1139_001:0585c02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於晨朝時當起讀誦。",
        "from": "T20n1139_001:0585c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我常守護如是等人令無惱害。",
        "from": "T20n1139_001:0585c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛姪他尸弃平聲呼之下同尸弃尸弃尸弃一旨旨旨旨二婆婆婆婆三鶻嚕嚕嚕嚕四恃恃恃恃五䞘其乞反囉䞘囉䞘囉䞘囉六欯許日反𠼝欯𠼝欯𠼝欯𠼝七㰤呼可反㰤㰤㰤八麼麼麼麼九末那末那十㰤那㰤迷十一薩婆鉢喇多丁也反㗚體他旨反迦十二柁訶柁訶謎十三薩婆設怛喇婆十四鉢遮鉢遮謎十五薩婆阿呬帶史拏十六莎訶十七爾時薄伽梵。",
        "from": "T20n1139_001:0585c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讚金剛手菩薩摩訶薩言。",
        "from": "T20n1139_001:0585c12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉善男子。",
        "from": "T20n1139_001:0585c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今說此陀羅尼神呪。",
        "from": "T20n1139_001:0585c13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就最勝有大義利。",
        "from": "T20n1139_001:0585c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為覆護諸有情故。",
        "from": "T20n1139_001:0585c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彼一切毛孔之中。",
        "from": "T20n1139_001:0585c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施其威光令得安樂。",
        "from": "T20n1139_001:0585c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時索訶世界主梵天王。",
        "from": "T20n1139_001:0585c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即從座起向佛合掌恭敬。",
        "from": "T20n1139_001:0585c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而白佛言世尊。",
        "from": "T20n1139_001:0585c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今亦為利益守護諸善男子及善女人。",
        "from": "T20n1139_001:0585c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說陀羅尼神呪。",
        "from": "T20n1139_001:0585c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛姪他呬𠼝一弭梨二旨梨三莎訶四勃𡆆黑訶訖反下同摩補𠼝五莎訶六勃𡆆黑摩娑思訖反嚩𠼐七莎訶八勃𡆆黑摩薜九蘇揭薜十補澁波僧娑思訖反䫂𠼐十一莎訶十二世尊此神呪句。",
        "from": "T20n1139_001:0585c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲為擁護利益諸善男子及善女人。",
        "from": "T20n1139_001:0585c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於晨朝時當起讀誦。",
        "from": "T20n1139_001:0585c24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時天帝釋白佛言。",
        "from": "T20n1139_001:0585c25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我今亦為擁護利益諸善男子及善女人。",
        "from": "T20n1139_001:0585c26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願如來垂哀護念。",
        "from": "T20n1139_001:0585c26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當說陀羅尼。",
        "from": "T20n1139_001:0585c27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人能受持此護命善門。",
        "from": "T20n1139_001:0585c27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於晨朝時憶念讀誦。",
        "from": "T20n1139_001:0585c28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛姪他毘儞平婆𡆆尼一憚茶摩󰸬茶徒皆反二旨胝胝儞平三具𠼝乾陀𠼝四薩𡆆麼訶儞平跋折㘕五多嚕尼六駐吒故反羅摩履儞平七斫訖囉婆上雞吉夷反八捨婆上𠼐九捨麼𠼐十鉢囉摩攞低十一㰤那摩蚶陀鹽反多𡆆尼十二苫跋𠼝十三莎訶十四爾時多聞天王持國天王增長天王醜目天王。",
        "from": "T20n1139_001:0586a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌恭敬白佛言世尊。",
        "from": "T20n1139_001:0586a07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等亦為擁護利益諸善男子及善女人。",
        "from": "T20n1139_001:0586a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說陀羅尼神呪。",
        "from": "T20n1139_001:0586a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人能受持此護命法門。",
        "from": "T20n1139_001:0586a09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於晨朝時當起讀誦。",
        "from": "T20n1139_001:0586a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛姪他補澁篦比奚反一蘇補澁篦二杜摩跛𠼝訶𠼐三阿㗚耶鉢𡆆捨悉低四扇低五儞㗚目訖低六瞢伽上例平七呬囉尼尼也反揭鞞八窣堵低九娑思訖反多鼻低十莎訶十一爾時鬼子母訶履底丁履反白佛言世尊。",
        "from": "T20n1139_001:0586a15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今亦為擁護利益諸善男子善女人故。",
        "from": "T20n1139_001:0586a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說陀羅尼。",
        "from": "T20n1139_001:0586a16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人能受持此護命神呪善門陀羅尼。",
        "from": "T20n1139_001:0586a17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於晨朝時當起讀誦。",
        "from": "T20n1139_001:0586a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛姪他悉梨一斯悉梨二悉梨三斯悉梨四必梨五摩訶卑必梨六悉弭履斯七堀嚕泥尼皆反八下皆同堀嚕泥九堀嚕泥十摩訶堀嚕泥普十一莎訶十二爾時五大山藥叉軍將。",
        "from": "T20n1139_001:0586a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌恭敬白佛言世尊。",
        "from": "T20n1139_001:0586a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今亦為擁護利益諸善男子善女人故。",
        "from": "T20n1139_001:0586a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說陀羅尼神呪。",
        "from": "T20n1139_001:0586a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人能受持此護命神呪法門陀羅尼。",
