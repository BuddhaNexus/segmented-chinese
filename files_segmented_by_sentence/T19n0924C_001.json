[
    {
        "source_sentence": "藥師儀軌一具",
        "from": "T19n0924C_001:0030c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲入向道場者。先澡浴清淨。以真言手印。",
        "from": "T19n0924C_001:0030c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灑淨所換之衣。次以真言加持其衣而著。真",
        "from": "T19n0924C_001:0030c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言曰。",
        "from": "T19n0924C_001:0030c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵微莾羅莎縛賀次即衣著。",
        "from": "T19n0924C_001:0030c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0030c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵鉢哩嚩羅多嚩曰里尼吽次飲水灑淨。",
        "from": "T19n0924C_001:0030c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0030c11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵摩訶入嚩羅吽次入向道場時。",
        "from": "T19n0924C_001:0030c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不起貪欲瞋恚等。",
        "from": "T19n0924C_001:0030c13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "專念本尊而住住自他本清淨。",
        "from": "T19n0924C_001:0030c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "步步想踐連華徃次至道場門外灑淨云云。",
        "from": "T19n0924C_001:0030c14_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次香水灑身。",
        "from": "T19n0924C_001:0030c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以後稱吽字三聲。",
        "from": "T19n0924C_001:0030c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後道場并觀想聖眾。",
        "from": "T19n0924C_001:0030c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍滿道場。",
        "from": "T19n0924C_001:0030c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如影重之。",
        "from": "T19n0924C_001:0030c16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不相妨礙。",
        "from": "T19n0924C_001:0030c16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次入道場已。",
        "from": "T19n0924C_001:0030c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面向於本尊。",
        "from": "T19n0924C_001:0030c17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌作禮。",
        "from": "T19n0924C_001:0030c17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮三部尊。",
        "from": "T19n0924C_001:0030c17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及本尊已。",
        "from": "T19n0924C_001:0030c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉頭諦觀佛菩薩金剛像也。",
        "from": "T19n0924C_001:0030c18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向佛陳說三世宿愆惡障。",
        "from": "T19n0924C_001:0030c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重懺本尊所。",
        "from": "T19n0924C_001:0030c19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦如是懺禮。",
        "from": "T19n0924C_001:0030c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T19n0924C_001:0030c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我從過去世　　流轉於生死",
        "from": "T19n0924C_001:0030c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今對大聖尊　　盡心而懺悔",
        "from": "T19n0924C_001:0030c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如先佛所懺　　我今亦如是",
        "from": "T19n0924C_001:0030c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願承加持力　　眾生悉清淨",
        "from": "T19n0924C_001:0030c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此大願故　　自他獲無垢",
        "from": "T19n0924C_001:0030c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次香水淨三業。",
        "from": "T19n0924C_001:0030c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0030c26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩嚩二合婆嚩輸馱薩嚩達麼薩嚩二合婆嚩輸度啥次定心已。",
        "from": "T19n0924C_001:0030c28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作三部三昧耶。",
        "from": "T19n0924C_001:0030c28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0030c28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵怛他引蘖覩納婆二合縛引也娑嚩二合引訶佛唵跛娜謨二合納婆二合縛引也娑嚩二合引賀蓮唵嚩曰盧二合納婆二合縛也婆嚩二合引賀引已上三部三昧耶。",
        "from": "T19n0924C_001:0031a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次遣除身中毘那夜迦。",
        "from": "T19n0924C_001:0031a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵闍波沒㗚帝賀囊迦囊吽泮吒次辟除諸障。",
        "from": "T19n0924C_001:0031a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031a06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨嚩日羅也吽賀囊莾他尾特夢婆輸󰸂羅也泮吒七遍次辟除諸物所有諸難在多意云云真言曰。",
        "from": "T19n0924C_001:0031a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵枳哩枳哩羅嚩嗟捺羅𤙖泮吒三遍次去垢真言曰。",
        "from": "T19n0924C_001:0031a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵枳哩枳哩嚩日羅𤙖泮吒七遍次清淨。",
        "from": "T19n0924C_001:0031a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵爾囊爾迦次光澤。",
        "from": "T19n0924C_001:0031a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵帝誓帝惹徒尾寧徒提沙馱吽泮吒三遍次神分。",
        "from": "T19n0924C_001:0031a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次唱禮。",
        "from": "T19n0924C_001:0031a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切恭敬。",
        "from": "T19n0924C_001:0031a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次供文。",
        "from": "T19n0924C_001:0031a17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次四方四佛四菩薩。",
        "from": "T19n0924C_001:0031a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次本尊稱名三遍。",
        "from": "T19n0924C_001:0031a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次三部界會一切佛菩薩等。",
        "from": "T19n0924C_001:0031a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次驚覺。",
        "from": "T19n0924C_001:0031a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩曰魯二合用底瑟吒二合吽印者。",
        "from": "T19n0924C_001:0031a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手各金剛拳。",
        "from": "T19n0924C_001:0031a21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舒頭指。",
        "from": "T19n0924C_001:0031a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各頭側相柱二小指相交。",
        "from": "T19n0924C_001:0031a21_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每誦一遍。",
        "from": "T19n0924C_001:0031a21_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作印拳動。",
        "from": "T19n0924C_001:0031a21_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次九方便　至心歸命。",
        "from": "T19n0924C_001:0031a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命十方正等覺　　三世一切具三身",
        "from": "T19n0924C_001:0031a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命一切大乘法　　歸命不退菩提眾",
        "from": "T19n0924C_001:0031a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命諸明真實言　　歸命一切諸密印",
        "from": "T19n0924C_001:0031a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以身口意清淨業　　慇懃無量恭敬禮",
        "from": "T19n0924C_001:0031a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命頂禮藥師瑠璃光佛。",
        "from": "T19n0924C_001:0031a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次禮方便。",
        "from": "T19n0924C_001:0031a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰印者。",
        "from": "T19n0924C_001:0031a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以金剛部印。",
        "from": "T19n0924C_001:0031a28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當額誦呪禮拜。",
        "from": "T19n0924C_001:0031a28_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引一曩莫薩縛怛他引蘗多二迦引野弭縛引吃質二合多三縛日羅二合滿娜南迦嚕弭次至心歸依。",
        "from": "T19n0924C_001:0031b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次真言印者金剛合掌次施身方便了。",
        "from": "T19n0924C_001:0031b02_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命頂禮。",
        "from": "T19n0924C_001:0031b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施身方便。",
        "from": "T19n0924C_001:0031b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言如例。",
        "from": "T19n0924C_001:0031b03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印者。",
        "from": "T19n0924C_001:0031b03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛合掌。",
        "from": "T19n0924C_001:0031b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "投身舒耳。",
        "from": "T19n0924C_001:0031b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若莊校投者。",
        "from": "T19n0924C_001:0031b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運心耳。",
        "from": "T19n0924C_001:0031b04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次至心發願了。",
        "from": "T19n0924C_001:0031b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命頂禮。",
        "from": "T19n0924C_001:0031b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發菩提心方便真言。",
        "from": "T19n0924C_001:0031b05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次至心懺悔了。",
        "from": "T19n0924C_001:0031b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命頂禮。",
        "from": "T19n0924C_001:0031b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次真言印者。",
        "from": "T19n0924C_001:0031b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先金剛合掌。",
        "from": "T19n0924C_001:0031b06_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指頭指舒著。",
        "from": "T19n0924C_001:0031b06_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次至心隨喜了。",
        "from": "T19n0924C_001:0031b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次歸命頂禮。",
        "from": "T19n0924C_001:0031b07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次真言印者。",
        "from": "T19n0924C_001:0031b07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛合掌次至心勸請了。",
        "from": "T19n0924C_001:0031b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次歸命頂禮。",
        "from": "T19n0924C_001:0031b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次真言印者。",
        "from": "T19n0924C_001:0031b08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛合掌次至心奉請誦了。",
        "from": "T19n0924C_001:0031b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次歸命頂禮。",
        "from": "T19n0924C_001:0031b09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次真言先作向清印。",
        "from": "T19n0924C_001:0031b09_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入內大指更出舌。",
        "from": "T19n0924C_001:0031b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭指鉤召次至心迴向誦了。",
        "from": "T19n0924C_001:0031b10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次歸命頂禮。",
        "from": "T19n0924C_001:0031b10_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次真言如例印金剛合掌云云。",
        "from": "T19n0924C_001:0031b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次護身。",
        "from": "T19n0924C_001:0031b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日羅二合引𡁬儞二合嚩羅捻跛跢二合引也娑縛二合賀次大護身真言曰。",
        "from": "T19n0924C_001:0031b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵入嚩羅囊也𤙖泮吒次被甲真言曰。",
        "from": "T19n0924C_001:0031b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵度比度比迦也度比鉢羅入嚩里寧娑婆賀次結髮。",
        "from": "T19n0924C_001:0031b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵蘇悉地羯哩莎嚩賀次結地界。",
        "from": "T19n0924C_001:0031b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵枳里枳里嚩日羅二合縛曰里二合部律二合半音滿馱滿馱吽發吒半音次結方隅金剛幡。",
        "from": "T19n0924C_001:0031b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩羅薩羅縛日羅二合伽羅吽泮吒半音次置寶座等。",
        "from": "T19n0924C_001:0031b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031b26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵毘莾嚕挪地𤙖三遍次阿者羅吽三遍。",
        "from": "T19n0924C_001:0031b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿者羅微𭋸娑婆賀三遍唵迦莾羅娑婆賀三遍唵囊莾薩嚩怛他蘖哆南薩羅嚩他欠烏跢蘖帝娑破羅呬莾闍伽伽囊釰娑嚩賀三遍次以我功德力。",
        "from": "T19n0924C_001:0031c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來加持力。",
        "from": "T19n0924C_001:0031c04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及以法界力。",
        "from": "T19n0924C_001:0031c04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普供養而住。",
        "from": "T19n0924C_001:0031c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次大虛空藏普通供養真言并印。",
        "from": "T19n0924C_001:0031c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵誐誐曩三去婆嚩嚩日羅二合斛引次遏伽以辨事真言持誦。",
        "from": "T19n0924C_001:0031c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後以部母呪呪之。",
        "from": "T19n0924C_001:0031c08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置於左邊。",
        "from": "T19n0924C_001:0031c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次部母。",
        "from": "T19n0924C_001:0031c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜牟婆伽婆都烏瑟尼沙耶唵盧盧娑普嚧若嚩羅帝瑟佗悉陀嚕者儞薩嚩羅他娑但儞莎訶次於寶座上。",
        "from": "T19n0924C_001:0031c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀本尊種子字。",
        "from": "T19n0924C_001:0031c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并結根本印云云。",
        "from": "T19n0924C_001:0031c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次轉字成本尊。",
        "from": "T19n0924C_001:0031c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱引南次欲請本尊。",
        "from": "T19n0924C_001:0031c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先執香爐。",
        "from": "T19n0924C_001:0031c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言。",
        "from": "T19n0924C_001:0031c18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨治空中道路。",
        "from": "T19n0924C_001:0031c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵蘇悉地迦里入嚩二合里多南多暮羅二合曳入嚩二合羅滿馱滿馱賀囊賀囊𤙖泮吒次發願啟白眾。",
        "from": "T19n0924C_001:0031c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次奉請本尊。",
        "from": "T19n0924C_001:0031c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更先想座已也。",
        "from": "T19n0924C_001:0031c23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就車輅。",
        "from": "T19n0924C_001:0031c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "送本尊所云云。",
        "from": "T19n0924C_001:0031c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶車輅。",
        "from": "T19n0924C_001:0031c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031c25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵都嚕都嚕吽次請車輅。",
        "from": "T19n0924C_001:0031c27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031c27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜麼悉底哩野二合地尾二合迦引南怛他引𮓌多引南引一唵嚩曰郎二合銀儞野三合引羯唎沙也二合莎訶次召請奉本尊。",
        "from": "T19n0924C_001:0032a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真真言曰。",
        "from": "T19n0924C_001:0032a02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵枳里枳里嚩日羅吽泮吒七遍次視三昧耶。",
        "from": "T19n0924C_001:0032a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0032a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵昇羯󰺜三莾烟莎訶七遍次奉閼伽。",
        "from": "T19n0924C_001:0032a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0032a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵藥乞私囊那也見捺羅達弩鉢捨莎訶三遍次奉蓮華座誦三部真言。",
        "from": "T19n0924C_001:0032a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獻成用道用。",
        "from": "T19n0924C_001:0032a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵味羅味羅也莎訶唵鉢那莾味羅也莎訶唵縛日羅味羅也莎訶次陳善來世尊真言。",
        "from": "T19n0924C_001:0032a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持誦奉獻。",
        "from": "T19n0924C_001:0032a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結上方金剛網。",
        "from": "T19n0924C_001:0032a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0032a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵尾娑普二合羅捺落二合乞刃二合縛日羅二合半惹羅吽泮吒次上下方。",
        "from": "T19n0924C_001:0032a15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次火院真言曰。",
        "from": "T19n0924C_001:0032a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿三莾擬儞吽泮吒次重結大界。",
        "from": "T19n0924C_001:0032a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0032a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵昇羯󰺜莾訶三摩焰莎訶七遍次奉塗香。",
        "from": "T19n0924C_001:0032a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0032a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伊𠰢言馱輸婆儞微夜二合輸者也輸者輸者輸者也莾夜儞吠儞始薄訖底也二合鉢羅二合底仡㗚呬也鉢羅二合四椰𠰢唵阿賀羅阿賀羅薩羅縛尾儞夜達羅布爾帝莎訶七遍奉華真言伊𠰢蘇莾囊素儞尾夜輪者輸輸者輸囊也莽夜寧唵伏儞多薄訖底二合夜鉢羅底仡㗚呬也鉢羅四挪𠰢唵阿賀羅阿賀羅薩羅縛尾儞夜達羅布爾帝莎訶奉燒香。",
        "from": "T19n0924C_001:0032b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0032b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿閻縛囊沙鉢底羅素賀㗚儞也健陀值也蘇羅部若曩莽夜寧吠儞始薄訖底夜度報閻鉢羅底仡㗚呬也難唵阿去賀羅阿賀羅薩羅縛尾儞夜達羅布爾莎訶奉食。",
        "from": "T19n0924C_001:0032b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0032b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奧灑涕難羅素賀㗚二合他也二合𪾼灑涌多羅二合設怒麼里莽也寧唵吠儞始薄訖底夜二合鉢羅底仡呬也二合鉢羅四椰吽唵阿去羅賀阿羅賀薩羅縛尾儞夜二合達羅布爾帝莎訶奉燈。",
        "from": "T19n0924C_001:0032b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0032b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅訖蒭二合近曩二合灑者二合跛尾怛羅二合室者二合瞻謨尾曇莽曩短名輸婆莽夜寧吠儞始薄訖底夜二合儞報閻鉢羅底訖㗚二合呬也難唵阿路迦也薩羅尾儞夜二合達羅布爾帝莎婆賀次運心供養。",
        "from": "T19n0924C_001:0032b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以心運想。",
        "from": "T19n0924C_001:0032b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水陸諸華。",
        "from": "T19n0924C_001:0032b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無曰主所。",
        "from": "T19n0924C_001:0032b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍滿虛空。",
        "from": "T19n0924C_001:0032b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡十方界。",
        "from": "T19n0924C_001:0032b18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及以人天。",
        "from": "T19n0924C_001:0032b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙塗香雲。",
        "from": "T19n0924C_001:0032b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒香燈明。",
        "from": "T19n0924C_001:0032b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幢幡寶蓋。",
        "from": "T19n0924C_001:0032b19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種鼓樂。",
        "from": "T19n0924C_001:0032b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歌舞妓唱。",
        "from": "T19n0924C_001:0032b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真珠羅網。",
        "from": "T19n0924C_001:0032b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懸諸寶領。",
        "from": "T19n0924C_001:0032b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髣白拂微妙聲。",
        "from": "T19n0924C_001:0032b20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鋒矜羯尼網。",
        "from": "T19n0924C_001:0032b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如意寶樹。",
        "from": "T19n0924C_001:0032b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣服之雲天。",
        "from": "T19n0924C_001:0032b21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸廚膳上。",
        "from": "T19n0924C_001:0032b21_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙香美味。",
        "from": "T19n0924C_001:0032b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宮殿樓閣寶柱。",
        "from": "T19n0924C_001:0032b22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莊嚴身。",
        "from": "T19n0924C_001:0032b22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭冠瓔珞。",
        "from": "T19n0924C_001:0032b22_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等雲。",
        "from": "T19n0924C_001:0032b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者運心。",
        "from": "T19n0924C_001:0032b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍滿虛空。",
        "from": "T19n0924C_001:0032b23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以至誠心。",
        "from": "T19n0924C_001:0032b23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是供養。",
        "from": "T19n0924C_001:0032b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最為勝上。",
        "from": "T19n0924C_001:0032b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故行者以決定心。",
        "from": "T19n0924C_001:0032b24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而行此法。",
        "from": "T19n0924C_001:0032b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運心供養。",
        "from": "T19n0924C_001:0032b25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦持真言。",
        "from": "T19n0924C_001:0032b25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及作手印。",
        "from": "T19n0924C_001:0032b25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如上所想。",
        "from": "T19n0924C_001:0032b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆悉成就。",
        "from": "T19n0924C_001:0032b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0032b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩羅嚩他龕搵娜𮓌帝娑頗羅呬門伽伽曩釰莎訶七遍次讚歎佛曰。",
        "from": "T19n0924C_001:0032b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩賀引迦引抳嚕引建曩引貪一捨娑跢二合引藍薩嚩吠得微閉反引儞泥以反南二合奔浦閟反去女鼻音娜地蠅二合𩄽拏引馱藍三鉢羅二合拏上摩引弭怛他引誐眈四佛唵羅儗野二合惹引曩南引󰡊詩佛反淡一戍婆引訥𮓌底丁以反謨引左釰二本播引羅沫體他以反迦謎引建躭三曩謎引達𤚥舍麼嚩憾四法穆訖躭二合穆訖底二合播引他引鉢羅二合跛躭二合引誐乞灑二合野索引弭野二嚩引沙體二合躭乞灑二合怛覽二合尾始瑟鵂𩄽上拏刎無本反一曩謎引僧去健左婆引嚩上跢都各反四僧薩嚩母馱婆羅二合捨跢二合引野三勃哩二合跢引野𩄽󰺜𩄽覽二阿嚩路引枳多僧去枳孃二合野三曩謨引𩕳髻摩賀答摩二合儞四菩薩摩賀引摩攞勤可反野贊拏野上尾儞野二合羅惹日羅反野娑引馱吠引准反二訥難上多娜摩迦夜引三曩謨引悉帝嚩日羅二合播引拏上曳引去四金剛眾阿引演引覩泥嚩左誐素羅一緊那上羅那囉鑠迦羅二合那野二鉢羅二合嚩羅二合達麼蘖里二合多地伽羅三尾達麼左鉢羅二合捨麼操爾也二合四儞銘二合多部多銘多鉢羅二合迦捨夜五怛儞賀室羅二合摩拏也馱引𤚥引六二合諸天次發願。",
        "from": "T19n0924C_001:0032c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次散念誦。",
        "from": "T19n0924C_001:0032c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先禮三寶。",
        "from": "T19n0924C_001:0032c23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次禮本尊。",
        "from": "T19n0924C_001:0032c23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後普禮諸佛。",
        "from": "T19n0924C_001:0032c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次持誦人先麻座。",
        "from": "T19n0924C_001:0032c24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捺婆等座謂跏趺半跏賢麻座。",
        "from": "T19n0924C_001:0032c25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令身端直。",
        "from": "T19n0924C_001:0032c25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次把數珠。",
        "from": "T19n0924C_001:0032c25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右三迴轉。",
        "from": "T19n0924C_001:0032c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次安珠二掌中頂戴云云次結根本印。",
        "from": "T19n0924C_001:0032c26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦根本真言七遍。",
        "from": "T19n0924C_001:0032c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次念誦了。",
        "from": "T19n0924C_001:0032c27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即頂戴珠。",
        "from": "T19n0924C_001:0032c27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發願安置本處。",
        "from": "T19n0924C_001:0032c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結根本印。",
        "from": "T19n0924C_001:0032c28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦根本真言七遍。",
        "from": "T19n0924C_001:0032c28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次更奉塗香華燒香飯燈。",
        "from": "T19n0924C_001:0032c29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次運心供養。",
        "from": "T19n0924C_001:0032c29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次讚。",
        "from": "T19n0924C_001:0032c29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次奉閼伽。",
        "from": "T19n0924C_001:0033a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次發願。",
        "from": "T19n0924C_001:0033a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次解界。",
        "from": "T19n0924C_001:0033a01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次示三昧。",
        "from": "T19n0924C_001:0033a01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及禮拜諸尊。",
        "from": "T19n0924C_001:0033a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次誦現在諸如來之偈。",
        "from": "T19n0924C_001:0033a02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次奉送真言。",
        "from": "T19n0924C_001:0033a02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次迴九方便。",
        "from": "T19n0924C_001:0033a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次迴向。",
        "from": "T19n0924C_001:0033a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次三部三昧耶。",
        "from": "T19n0924C_001:0033a03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次護身。",
        "from": "T19n0924C_001:0033a03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次護本尊等。",
        "from": "T19n0924C_001:0033a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次遍禮諸尊。",
        "from": "T19n0924C_001:0033a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後去。",
        "from": "T19n0924C_001:0033a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當堂道場。",
        "from": "T19n0924C_001:0033a04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "任意經行。",
        "from": "T19n0924C_001:0033a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小指檀慧上左下右無名指戒方上左下右中指忍願上右下左頭指進力上左下右大指禪智上左下右。",
        "from": "T19n0924C_001:0033a07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善來尊者。",
        "from": "T19n0924C_001:0033a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善來尊者。",
        "from": "T19n0924C_001:0033a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由本願力。",
        "from": "T19n0924C_001:0033a08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而來降赴。",
        "from": "T19n0924C_001:0033a08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顧乘加持。",
        "from": "T19n0924C_001:0033a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "授此請。",
        "from": "T19n0924C_001:0033a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并受我所辦諸供養。",
        "from": "T19n0924C_001:0033a09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起大慈悲。",
        "from": "T19n0924C_001:0033a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯垂納受。",
        "from": "T19n0924C_001:0033a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現在諸如來　　救世諸菩薩",
        "from": "T19n0924C_001:0033a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不斷大乘教　　到殊勝位者",
        "from": "T19n0924C_001:0033a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願聖天眾　　決定證知我",
        "from": "T19n0924C_001:0033a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各當隨處安　　後時垂哀赴",
        "from": "T19n0924C_001:0033a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終頌。",
        "from": "T19n0924C_001:0033a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今念誦祕密呪　　普通三界得清淨",
        "from": "T19n0924C_001:0033a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願我生中除業障　　福德圓滿利有情",
        "from": "T19n0924C_001:0033a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生生世世恒受持　　四恩眾生共成佛",
        "from": "T19n0924C_001:0033a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥師儀軌一具終　傳教大師將來",
        "from": "T19n0924C_001:0033a19_100",
        "context_with_next_sentence": false
    }
]