[
    {
        "source_sentence": "不空羂索呪心經",
        "from": "T20n1095_001:0406a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐南天竺三藏法師",
        "from": "T20n1095_001:0406a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩提流志奉　詔譯",
        "from": "T20n1095_001:0406a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T20n1095_001:0406a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時薄伽梵在布怛落迦山聖觀自在菩薩宮中。",
        "from": "T20n1095_001:0406a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其地有無量寶娑羅樹多摩羅樹。",
        "from": "T20n1095_001:0406a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞻博迦樹阿輸迦樹。",
        "from": "T20n1095_001:0406a26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿底目多迦等種種寶樹。",
        "from": "T20n1095_001:0406a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周匝莊嚴。",
        "from": "T20n1095_001:0406a27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大苾芻眾八千人俱。",
        "from": "T20n1095_001:0406a27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩摩訶薩九十九俱胝那庾多百千。",
        "from": "T20n1095_001:0406a28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及無量百千淨居天子。",
        "from": "T20n1095_001:0406a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自在天子大自在天子。",
        "from": "T20n1095_001:0406a29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大梵天子而為上首。",
        "from": "T20n1095_001:0406b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及餘無量百千天人。",
        "from": "T20n1095_001:0406b01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後圍繞佛為說法。",
        "from": "T20n1095_001:0406b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時聖觀自在菩薩摩訶薩。",
        "from": "T20n1095_001:0406b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從座而起偏袒右肩。",
        "from": "T20n1095_001:0406b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右膝著地向佛合掌。",
        "from": "T20n1095_001:0406b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舒顏微笑而白佛言。",
        "from": "T20n1095_001:0406b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我有神呪心名不空羂索。",
        "from": "T20n1095_001:0406b04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彼往昔第九十一劫。",
        "from": "T20n1095_001:0406b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時有世界名曰勝觀。",
        "from": "T20n1095_001:0406b05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其佛號世主王如來。",
        "from": "T20n1095_001:0406b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我從彼佛受不空羂索呪心。",
        "from": "T20n1095_001:0406b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我由受持是神呪故。",
        "from": "T20n1095_001:0406b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教化無量百千淨居天子。",
        "from": "T20n1095_001:0406b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自在天子大自在天子。",
        "from": "T20n1095_001:0406b08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令趣阿耨多羅三藐三菩提。",
        "from": "T20n1095_001:0406b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以是功德力故便獲十億三摩地。",
        "from": "T20n1095_001:0406b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空妙智而為上首。",
        "from": "T20n1095_001:0406b10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊若所在之處有此呪心。",
        "from": "T20n1095_001:0406b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其地即有大自在等十二億諸天而來擁護。",
        "from": "T20n1095_001:0406b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如佛制多。",
        "from": "T20n1095_001:0406b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊此神呪心。",
        "from": "T20n1095_001:0406b12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨所住處其中有情。",
        "from": "T20n1095_001:0406b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知已於無量百千俱胝那庾多百千佛所種諸善根。",
        "from": "T20n1095_001:0406b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊若復有人聞此呪心。",
        "from": "T20n1095_001:0406b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人先造惡業行於非法。",
        "from": "T20n1095_001:0406b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毀辱賢善誹謗正法。",
        "from": "T20n1095_001:0406b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及以誹謗一切諸佛菩薩聲聞緣覺。",
        "from": "T20n1095_001:0406b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "決定應墮無間大地獄中。",
        "from": "T20n1095_001:0406b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊彼人若聞此神呪心而生悔愧。",
        "from": "T20n1095_001:0406b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一日夜受持齋戒讀此神呪。",
        "from": "T20n1095_001:0406b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即能銷滅一切罪業。",
        "from": "T20n1095_001:0406b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或復令其所有罪業現世輕受。",
        "from": "T20n1095_001:0406b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若得熱病。",
        "from": "T20n1095_001:0406b20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或一日二日乃至七日。",
        "from": "T20n1095_001:0406b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若眼耳鼻舌脣齒齗齶心腹臍脇手足支節等痛。",
        "from": "T20n1095_001:0406b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若得痔病下痢祕澁白癩大癩。",
        "from": "T20n1095_001:0406b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若疥若癬若黑瘡赤瘡漏瘡疱瘡。",
        "from": "T20n1095_001:0406b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若癲癎等病若厭禱蠱毒。",
        "from": "T20n1095_001:0406b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫縛杖捶誹謗罵辱。",
        "from": "T20n1095_001:0406b24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及餘諸惡逼惱身心并諸怪夢。",
        "from": "T20n1095_001:0406b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今說以現受故。",
        "from": "T20n1095_001:0406b25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無間惡業即得銷滅。",
        "from": "T20n1095_001:0406b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "況諸眾生清淨信者受持此呪。",
        "from": "T20n1095_001:0406b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切罪業而不滅耶。",
        "from": "T20n1095_001:0406b27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊若有眾生。",
        "from": "T20n1095_001:0406b27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以其諂曲虛誑之心。",
        "from": "T20n1095_001:0406b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞我所說不空羂索呪心。",
        "from": "T20n1095_001:0406b28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詐現受持。",
        "from": "T20n1095_001:0406b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若自書若令他書。",
        "from": "T20n1095_001:0406b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若為他說令其聽受。",
        "from": "T20n1095_001:0406c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至向彼傍生耳邊誦此呪心。",
        "from": "T20n1095_001:0406c01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及思惟神呪章句。",
        "from": "T20n1095_001:0406c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不毀謗故無相故。",
        "from": "T20n1095_001:0406c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無生故無分別故。",
        "from": "T20n1095_001:0406c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遲至故無作故。",
        "from": "T20n1095_001:0406c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離染故平等故不捨故離蘊故。",
        "from": "T20n1095_001:0406c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是修習相應方便。",
        "from": "T20n1095_001:0406c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此憶念佛功德力。",
        "from": "T20n1095_001:0406c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼十方面各有千佛出現其前。",
        "from": "T20n1095_001:0406c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者見已所有罪業皆悉銷滅。",
        "from": "T20n1095_001:0406c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我今略說。",
        "from": "T20n1095_001:0406c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至有人抄寫此經。",
        "from": "T20n1095_001:0406c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置於家中禮拜供養。",
        "from": "T20n1095_001:0406c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦得無量無邊福德。",
        "from": "T20n1095_001:0406c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況受持讀誦之者。",
        "from": "T20n1095_001:0406c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊若有眾生。",
        "from": "T20n1095_001:0406c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各各自為欲勝他故。",
        "from": "T20n1095_001:0406c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或怖主故怖怨讐故。",
        "from": "T20n1095_001:0406c09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怖惡獸故怖危難故。",
        "from": "T20n1095_001:0406c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或隨他故求尊貴故求財寶故。",
        "from": "T20n1095_001:0406c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聽聞如是神呪心經。",
        "from": "T20n1095_001:0406c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖復聽聞不生恭敬。",
        "from": "T20n1095_001:0406c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或致誹謗輕慢毀訾。",
        "from": "T20n1095_001:0406c12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由觀自在菩薩威神力故。",
        "from": "T20n1095_001:0406c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令如是人亦生勝福。",
        "from": "T20n1095_001:0406c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如有人。",
        "from": "T20n1095_001:0406c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取栴檀香或沈麝等。",
        "from": "T20n1095_001:0406c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "罵詈毀訾而碎抹之用塗其身。",
        "from": "T20n1095_001:0406c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而彼香等終無是念。",
        "from": "T20n1095_001:0406c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此人毀訾我故。",
        "from": "T20n1095_001:0406c15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悋其香氣而不與之而栴檀等本性芬馥作其香事。",
        "from": "T20n1095_001:0406c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊此神呪心亦復如是。",
        "from": "T20n1095_001:0406c17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖有誹謗毀訾。",
        "from": "T20n1095_001:0406c17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或復諂詐書寫受持供養。",
        "from": "T20n1095_001:0406c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而皆與作善根因緣。",
        "from": "T20n1095_001:0406c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生生之處。",
        "from": "T20n1095_001:0406c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常不捨離戒定智慧福德資糧。",
        "from": "T20n1095_001:0406c19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於當來世戒香具足。",
        "from": "T20n1095_001:0406c20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊若有善男子善女人。",
        "from": "T20n1095_001:0406c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若苾芻苾芻尼。",
        "from": "T20n1095_001:0406c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鄔波索迦鄔波斯迦。",
        "from": "T20n1095_001:0406c21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於月八日專心齋戒不雜餘語。",
        "from": "T20n1095_001:0406c22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此不空羂索呪心七遍。",
        "from": "T20n1095_001:0406c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼人現身得二十種殊勝利益。",
        "from": "T20n1095_001:0406c23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何二十。",
        "from": "T20n1095_001:0406c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者身無餘病。",
        "from": "T20n1095_001:0406c24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者由先作業有諸疾病速得銷滅。",
        "from": "T20n1095_001:0406c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者其身光澤皮膚細軟見者歡喜。",
        "from": "T20n1095_001:0406c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者眾人愛敬密護諸根。",
        "from": "T20n1095_001:0406c26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者當得財寶。",
        "from": "T20n1095_001:0406c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六者得財寶已不為盜賊之所劫掠。",
        "from": "T20n1095_001:0406c27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七者不為水火之所漂焚。",
        "from": "T20n1095_001:0406c28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八者不為王力之所侵奪。",
        "from": "T20n1095_001:0406c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九者所作事業皆善成辦。",
        "from": "T20n1095_001:0406c29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十者所種苗稼。",
        "from": "T20n1095_001:0406c29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為惡風暴雨霜雹蟲蝗之所損害。",
        "from": "T20n1095_001:0407a01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十一者若誦此呪心七遍。",
        "from": "T20n1095_001:0407a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪灰及水八方上下灑散結界。",
        "from": "T20n1095_001:0407a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切災難皆得銷滅。",
        "from": "T20n1095_001:0407a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二者不為諸惡鬼等奪其精氣。",
        "from": "T20n1095_001:0407a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十三者一切有情愛樂喜見。",
        "from": "T20n1095_001:0407a04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十四者不怖怨讐。",
        "from": "T20n1095_001:0407a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十五者設有怨讐速疾和解。",
        "from": "T20n1095_001:0407a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十六者不畏人及非人之所侵害。",
        "from": "T20n1095_001:0407a06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "厭蠱邪魅不能中傷。",
        "from": "T20n1095_001:0407a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十七者無有猛利煩惱及隨煩惱。",
        "from": "T20n1095_001:0407a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十八者火刀毒藥傷害不死。",
        "from": "T20n1095_001:0407a08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十九者諸天善神常所擁護。",
        "from": "T20n1095_001:0407a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十者所生之處於中不離慈悲喜捨。",
        "from": "T20n1095_001:0407a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是二十種殊勝利益。",
        "from": "T20n1095_001:0407a10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當希求。",
        "from": "T20n1095_001:0407a10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有八法何等為八。",
        "from": "T20n1095_001:0407a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者臨命終時。",
        "from": "T20n1095_001:0407a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖觀自在菩薩作苾芻像現其人前。",
        "from": "T20n1095_001:0407a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者臨命終時。",
        "from": "T20n1095_001:0407a12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安樂捨壽無諸痛苦。",
        "from": "T20n1095_001:0407a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者臨命終時正念現前心不錯亂。",
        "from": "T20n1095_001:0407a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者臨命終時手不紛亂足不伸縮。",
        "from": "T20n1095_001:0407a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者臨命終時而不漏洩大小便利。",
        "from": "T20n1095_001:0407a15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六者設使有病不滯床枕。",
        "from": "T20n1095_001:0407a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七者臨命終時不覆面死。",
        "from": "T20n1095_001:0407a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八者臨命終時得無盡辯才。",
        "from": "T20n1095_001:0407a17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既命終已隨願往生諸佛淨土。",
        "from": "T20n1095_001:0407a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及不捨離諸善知識。",
        "from": "T20n1095_001:0407a18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊若善男子善女人等。",
        "from": "T20n1095_001:0407a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不食酒肉薰辛及殘惡觸。",
        "from": "T20n1095_001:0407a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日日三時時別三遍。",
        "from": "T20n1095_001:0407a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此神呪心法門。",
        "from": "T20n1095_001:0407a20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殊勝功德晝夜增長。",
        "from": "T20n1095_001:0407a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了知一切有情有力無力隨其聽聞。",
        "from": "T20n1095_001:0407a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩不應心生祕惜。",
        "from": "T20n1095_001:0407a22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永離諸惡慳悋嫉妬。",
        "from": "T20n1095_001:0407a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常為利益一切有情故。",
        "from": "T20n1095_001:0407a23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速趣菩提入菩薩位。",
        "from": "T20n1095_001:0407a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言菩提者說名般若。",
        "from": "T20n1095_001:0407a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言薩埵者即是方便。",
        "from": "T20n1095_001:0407a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此二種法。",
        "from": "T20n1095_001:0407a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸有情當得一切利益安樂。",
        "from": "T20n1095_001:0407a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我今欲為利益安樂諸四部眾。",
        "from": "T20n1095_001:0407a26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及餘有情造罪業者。",
        "from": "T20n1095_001:0407a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願世尊哀愍許可。",
        "from": "T20n1095_001:0407a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於如來前說此呪心。",
        "from": "T20n1095_001:0407a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊。",
        "from": "T20n1095_001:0407a28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告聖觀自在菩薩摩訶薩言。",
        "from": "T20n1095_001:0407a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有情清淨今正是時。",
        "from": "T20n1095_001:0407a29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦隨喜此神呪心。",
        "from": "T20n1095_001:0407b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於後時分為行菩薩乘者而作父母。",
        "from": "T20n1095_001:0407b01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸菩薩所作事業速得成就。",
        "from": "T20n1095_001:0407b02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時聖觀自在菩薩摩訶薩。",
        "from": "T20n1095_001:0407b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞻仰尊顏目不暫捨。",
        "from": "T20n1095_001:0407b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而白佛言。",
        "from": "T20n1095_001:0407b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊惟願如來聽我說此神呪。",
        "from": "T20n1095_001:0407b04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切菩薩所應敬禮。",
        "from": "T20n1095_001:0407b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此解脫法門哀愍世間。",
        "from": "T20n1095_001:0407b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利益安樂無量有情。",
        "from": "T20n1095_001:0407b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲受持者應先敬禮三世諸佛。",
        "from": "T20n1095_001:0407b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸菩薩獨覺聲聞。",
        "from": "T20n1095_001:0407b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮正至正行。",
        "from": "T20n1095_001:0407b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復應敬禮舍利子等大慧聲聞。",
        "from": "T20n1095_001:0407b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復應敬禮慈氏等上首菩薩摩訶薩眾。",
        "from": "T20n1095_001:0407b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮金色光明吼聲自在王如來。",
        "from": "T20n1095_001:0407b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮師子遊戲王如來。",
        "from": "T20n1095_001:0407b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮無量光如來。",
        "from": "T20n1095_001:0407b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮善住摩尼寶積王如來。",
        "from": "T20n1095_001:0407b11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮普光明讚歎功德積王如來。",
        "from": "T20n1095_001:0407b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮勝觀如來敬禮寶髻如來。",
        "from": "T20n1095_001:0407b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮現世間如來。",
        "from": "T20n1095_001:0407b13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮捨離損壞蘊如來。",
        "from": "T20n1095_001:0407b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮金色身寂如來。",
        "from": "T20n1095_001:0407b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮飲光如來。",
        "from": "T20n1095_001:0407b14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮能寂如來。",
        "from": "T20n1095_001:0407b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮善名稱如來。",
        "from": "T20n1095_001:0407b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮普光勝怨敵德如來。",
        "from": "T20n1095_001:0407b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮帝幢德如來。",
        "from": "T20n1095_001:0407b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮寶光明自在王如來。",
        "from": "T20n1095_001:0407b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮無礙藥王如來。",
        "from": "T20n1095_001:0407b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮勇猛遊步如來。",
        "from": "T20n1095_001:0407b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮善住無畏如來應正等覺。",
        "from": "T20n1095_001:0407b18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮三寶。",
        "from": "T20n1095_001:0407b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮聖觀自在菩薩摩訶薩具大悲者。",
        "from": "T20n1095_001:0407b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮如是諸聖者已。",
        "from": "T20n1095_001:0407b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復應念言聖觀自在菩薩。",
        "from": "T20n1095_001:0407b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於如來前說神呪心。",
        "from": "T20n1095_001:0407b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今亦當說此神呪。",
        "from": "T20n1095_001:0407b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願我所作事業速得成辦。",
        "from": "T20n1095_001:0407b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令我一切怖畏皆除。",
        "from": "T20n1095_001:0407b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時聖觀自在菩薩即說呪曰。",
        "from": "T20n1095_001:0407b23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "䫂丁可切姪徒也切下皆同他一闍者之可切下皆同𡆆攞音轉舌呼之自此已下非口傍字其傍加口者皆倣此者𡆆二旨𭌆旨𭌆三主嚕主嚕四母嚕母嚕五麼㰤呼可切迦訖可切下皆同嚕抳嬭旨切迦也藥可切下皆同音璽𭌆璽𭌆七旨𭌆旨𭌆八費房費切下皆同𭌆費𭌆九麼㰤跛達陀訖切下皆同麼㰤娑思訖切下皆同䫂也十迦攞迦攞十一枳吉旨切下皆同履枳履十二矩魯矩魯十三麼㰤輸上聲自此下注平上去入者依平上去入聲呼柁縒麁可切怛多訖切下皆同縛房可切下皆同也十四部姪部姪十五柁縛柁縛十六迦嬭拏可切下皆同迦嬭十七枳抳枳抳十八矩努拏古切下音同矩努十九跛𡆆麼輸上音柁縒怛縛也二十迦𡆆迦𡆆二十一枳𭌆枳𭌆二十二矩嚕矩嚕二十三麼㰤娑他上音麼般比訖切下皆同𡆆般䫂也二十四者攞者攞二十五散去音者攞散去音者𡆆二十六費者攞費者攞二十七般𡆆者攞般𡆆者攞二十八翳吒吒可切下皆同吒翳吒吒二十九婆上音𡆆婆上音攞三十鼻𭌆鼻𭌆三十一部嚕部嚕三十二䫂𡆆䫂𡆆三十三底丁旨切下皆同𭌆底𭌆三十四覩嚕覩嚕三十五翳欯火吉切裔呬火貳切下皆同三十六麼㰤迦嚕抳迦三十七麼㰤跛輸跛底吠灑沙河切下皆同柁𡆆三十八柁𡆆柁𡆆三十九縒𡆆縒𡆆四十者囉者囉四十一跛囉跛𡆆四十二縛𡆆縛𡆆四十三㰤𡆆㰤囉四十四㰤㰤呬呬虎虎四十五闇迦𡆆勃𡆆𠿒訶訖切下皆同麼吠灑柁𡆆四十六柁𡆆柁𡆆四十七地𭌆地𭌆四十八杜嚕杜嚕四十九䫂攞䫂攞五十縒𡆆縒𡆆五十一跛𡆆跛𡆆五十二縛𡆆縛𡆆五十三曷𡆆失弭捨尸可切下皆同䫂縒㰤娑𡆆五十四般𡆆底漫雉䫂捨𭌆囉五十五實時訖切下皆同縛攞實縛攞五十六䫂跛䫂跛五十七婆上音縒婆上音縒五十八勃𡆆麼勃𡆆麼五十九婆上音伽上音飯素麼地䋾丁也切六十也麼縛嚕嬭矩吠𡆆六十一勃𡆆𠿒麵達𡆆六十二曷𭌆史第縛伽上音嬭罷薄也切下皆同㗚旨䫂六十三者𡆆嬭六十四蘇上音嚕蘇上音嚕六十五主嚕主嚕六十六母嚕母嚕六十七補嚕補嚕六十八縒𭢟那可切下同怛矩麼𡆆鶻嚕達𡆆縛縒縛六十九費瑟努七十柁𭢟柁七十一縛逸縛䞘其訖切儞七十二第縛七十三曷𭌆史𭢟也迦七十四婆上音虎費費柁吠灑柁𡆆七十五柁𡆆柁囉七十六地𭌆地𭌆七十七杜嚕杜嚕七十八他上音𡆆他上音𡆆七十九伽上音𡆆伽上音𡆆八十也𡆆也𡆆八十一攞囉攞囉八十二㰤𡆆㰤𡆆八十三跛𡆆跛𡆆八十四麼𡆆麼𡆆八十五縛𡆆縛𡆆八十六縛𡆆柁也迦八十七縒漫䫂縛路枳䫂八十八費路枳䫂八十九路計失縛𡆆九十麼醯去音失縛𡆆九十一母虎母虎九十二母嚕母嚕九十三母也母也九十四悶者悶者九十五𠸪叉𠸪叉九十六此中誦呪者應自稱我某甲薩縛縒怛防去音失者九十七薩縛婆上音裔罷九十八薩部跛𡆆跛罷九十九薩部跛薩祁祁計切罷一百薩縛䞘𡆆醯去音羅一百一縛柁伴柁𭢟一百二䫂荼上音𭢟一百三䫂㗚社時可切下皆同𭢟一百四曷𡆆杜䫂娑迦𡆆一百五𭢔烏可切䞘弩那矩切柁迦一百六費灑捨娑怛𡆆一百七跛𭌆慕者迦一百八迦嬭迦嬭一百九枳抳枳抳一百一十矩努矩努一百一十一者𡆆者𡆆一百一十二旨𭌆旨𭌆一百一十三主嚕主嚕一百一十四印達𭌆也一百一十五婆上音攞步鄧陀證切伽上音一百一十六者覩𡆆㗚野縒䋾丁也切百十七三去音般𡆆迦捨迦一百十八䫂麼䫂麼一百一十九縒麼縒麼一百二十麼縒麼縒一百二十一麼㰤䫂悶柁迦𡆆一百二十二費柁麼𭢟一百二十三灑痓吒訖切下皆同跛𡆆弭䫂一百二十四跛𭌆補𡆆迦一百二十五弭履弭履一百二十六吒吒吒吒一百二十七侘勅可切下皆同侘侘侘一百二十八徵吒旨切下皆同徵徵徵一百二十九柱吒古切下皆同柱柱柱一百三十翳耐拏計切也折之設切麼一百三十一訖哩䫂跛𭌆迦𡆆一百三十二翳欯裔呬一百三十三伊上音失縛𡆆部䫂伽上音嬭伴社迦一百三十四矩嚕矩嚕一百三十五跛𡆆跛𡆆一百三十六者𡆆者𡆆一百三十七縒𡆆縒𡆆一百三十八迦𡆆迦𡆆一百三十九迦吒迦吒一百四十跛吒跛吒一百四十一麼吒麼吒一百四十二費輪上音柁費灑也儞縛信一百四十三麼㰤迦嚕抳迦一百四十四失吠䫂也寶乳跛費䫂一百四十五曷𡆆怛𭢟麼矩吒一百四十六麼攞柁𡆆一百四十七薩縛實若而可切始𡆆璽訖哩䫂一百四十八社吒麼矩吒一百四十九麼㰤達部䫂迦麼攞一百五十訖哩䫂迦𡆆䫂攞一百五十一姪𭢟縒麼地費木叉一百五十二般羅劍俱暗切𧟶必也切一百五十三婆上音虎縒怛縛散去音䫂底一百五十四跛𭌆跛者迦一百五十五麼㰤迦嚕抳迦一百五十六薩縛羯麼縛𡆆一百五十七費戍柁迦一百五十八薩縛筏房也切地一百五十九般𡆆慕者迦一百六十薩縛縒怛縛阿捨也一百六十一跛𭌆補𡆆迦一百六十二薩縛縛縒怛縛一百六十三縒麼失縛縒迦𡆆一百六十四𭢟慕娑覩諦娑縛㰤一百六十五𭢔慕伽上音也娑縛㰤一百六十六𭢔視䫂也娑縛㰤一百六十七𭢔跛囉視䫂也娑縛㰤一百六十八費囉也娑縛㰤一百六十九縛𡆆柁也娑縛㰤一百七十縛𡆆般𡆆柁也娑縛㰤一百七十一伊上音誕者謎薩縛羯麼矩嚕一百七十二𭢟慕娑都娑縛㰤一百七十三闍社也𧵊呼暗切下皆同娑縛㰤一百七十四闇𠿒𡆆視娑縛㰤一百七十五闇欯𭌆怛𡃤路加吉也切費社也一百七十六𭢔慕伽上音跛捨一百七十七𭢔般攞底㰤䫂一百七十八欯𭌆一百七十九㰤一百八十呬一百八十一𧵊一百八十二叵窒叵窒一百八十三娑縛㰤一百八十四此神呪心。",
        "from": "T20n1095_001:0408b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨誦有驗所作皆成。",
        "from": "T20n1095_001:0408b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日日三時一一時中各誦三遍。",
        "from": "T20n1095_001:0408b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五無間罪皆得銷滅。",
        "from": "T20n1095_001:0408b21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切業障皆得清淨。",
        "from": "T20n1095_001:0408b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒沈水香或散灰散水。",
        "from": "T20n1095_001:0408b22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或白芥子以為結界。",
        "from": "T20n1095_001:0408b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或取佉陀羅木為橛。",
        "from": "T20n1095_001:0408b23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍已而釘四方。",
        "from": "T20n1095_001:0408b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患一切寒熱瘧病。",
        "from": "T20n1095_001:0408b24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪線結索帶之。",
        "from": "T20n1095_001:0408b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "病得除愈。",
        "from": "T20n1095_001:0408b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸有病者。",
        "from": "T20n1095_001:0408b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或呪酥油或復呪水。",
        "from": "T20n1095_001:0408b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼病者若服若塗。",
        "from": "T20n1095_001:0408b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得除差。",
        "from": "T20n1095_001:0408b26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若被厭蠱。",
        "from": "T20n1095_001:0408b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應以麵泥蠟等為人形像。",
        "from": "T20n1095_001:0408b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以刀斷截。",
        "from": "T20n1095_001:0408b27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復以呪索令被厭者身常佩之。",
        "from": "T20n1095_001:0408b28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患腹痛。",
        "from": "T20n1095_001:0408b28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應呪鹽水與之令服。",
        "from": "T20n1095_001:0408b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遭諸毒。",
        "from": "T20n1095_001:0408b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪土或水。",
        "from": "T20n1095_001:0408b29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若塗若服即得銷滅。",
        "from": "T20n1095_001:0408c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患眼宜。",
        "from": "T20n1095_001:0408c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪白線為索用繫其耳。",
        "from": "T20n1095_001:0408c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患牙齒疼痛。",
        "from": "T20n1095_001:0408c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪迦羅費羅木而揩嚼之。",
        "from": "T20n1095_001:0408c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲結界。",
        "from": "T20n1095_001:0408c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以佉陀羅木為橛釘於四隅。",
        "from": "T20n1095_001:0408c03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪五色縷二十一遍周圍橛內。",
        "from": "T20n1095_001:0408c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲自護及護他者。",
        "from": "T20n1095_001:0408c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應呪索帶或呪水呪灰灑散其身。",
        "from": "T20n1095_001:0408c05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患一切鬼病。",
        "from": "T20n1095_001:0408c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪五色線為索帶之。",
        "from": "T20n1095_001:0408c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患一切熱病。",
        "from": "T20n1095_001:0408c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪白色線為索帶之。",
        "from": "T20n1095_001:0408c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患一切諸惡瘡腫若咽喉閉塞。",
        "from": "T20n1095_001:0408c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以蜜和蓽茇而呪服之。",
        "from": "T20n1095_001:0408c08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患眼病。",
        "from": "T20n1095_001:0408c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應呪香水或波羅賒水。",
        "from": "T20n1095_001:0408c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或甘草水而用洗之。",
        "from": "T20n1095_001:0408c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患耳痛。",
        "from": "T20n1095_001:0408c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪胡麻油滴彼耳中。",
        "from": "T20n1095_001:0408c10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若諸鬪戰諍訟毀謗。",
        "from": "T20n1095_001:0408c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應取呪水用洗其面。",
        "from": "T20n1095_001:0408c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲擁護王都聚落。",
        "from": "T20n1095_001:0408c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應取四瓶滿中盛水。",
        "from": "T20n1095_001:0408c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及以飲食作大供養。",
        "from": "T20n1095_001:0408c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其誦呪者著新淨衣。",
        "from": "T20n1095_001:0408c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讀誦此呪即得吉祥。",
        "from": "T20n1095_001:0408c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復以彼水散灑其處。",
        "from": "T20n1095_001:0408c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擁護一切諸有情等。",
        "from": "T20n1095_001:0408c14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有災厄皆得銷滅。",
        "from": "T20n1095_001:0408c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患邪病以水磨栴檀。",
        "from": "T20n1095_001:0408c15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪二十一遍塗其心上。",
        "from": "T20n1095_001:0408c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若犯四重五逆諸無間罪。",
        "from": "T20n1095_001:0408c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應常誦此呪其罪銷滅。",
        "from": "T20n1095_001:0408c17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若護宅舍。",
        "from": "T20n1095_001:0408c17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應取蓮華一百八枚。",
        "from": "T20n1095_001:0408c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各呪一遍於火中燒。",
        "from": "T20n1095_001:0408c18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲令一切有情隨順已者。",
        "from": "T20n1095_001:0408c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應取栴檀長二寸者一百八枚。",
        "from": "T20n1095_001:0408c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各呪一遍於火中燒。",
        "from": "T20n1095_001:0408c20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若著鬼魅及有怖畏。",
        "from": "T20n1095_001:0408c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應取社耶藥費社耶藥。",
        "from": "T20n1095_001:0408c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那矩梨藥健陀那矩梨藥。",
        "from": "T20n1095_001:0408c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婆刺尼藥阿婆野波抳藥。",
        "from": "T20n1095_001:0408c22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因達羅波抳藥乾陀鉢履樣瞿藥。",
        "from": "T20n1095_001:0408c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多伽羅藥斫訖羅藥。",
        "from": "T20n1095_001:0408c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩訶斫訖羅藥毘瑟怒訖爛多藥。",
        "from": "T20n1095_001:0408c24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘇摩羅時藥蘇難陀等。",
        "from": "T20n1095_001:0408c25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是諸藥擣篩水和為丸。",
        "from": "T20n1095_001:0408c25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦呪一百八遍。",
        "from": "T20n1095_001:0408c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若置頭上若繫兩臂小兒項上。",
        "from": "T20n1095_001:0408c26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼魅怖畏皆自銷滅。",
        "from": "T20n1095_001:0408c27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有婦人。",
        "from": "T20n1095_001:0408c27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由薄福故被人厭賤。",
        "from": "T20n1095_001:0408c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及求男者。",
        "from": "T20n1095_001:0408c28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著新淨衣呪彼藥水。",
        "from": "T20n1095_001:0408c28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿三七遍澡浴自身。",
        "from": "T20n1095_001:0408c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得勝福德惡相銷滅。",
        "from": "T20n1095_001:0408c29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求男得男一切獲益。",
        "from": "T20n1095_001:0409a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒火不侵災橫不著。",
        "from": "T20n1095_001:0409a01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遇惡風暴雨及災雹者。",
        "from": "T20n1095_001:0409a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪水三七遍用灑四方。",
        "from": "T20n1095_001:0409a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若呪迦羅費羅木杖滿三七遍。",
        "from": "T20n1095_001:0409a03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "指撝虛空風等便息。",
        "from": "T20n1095_001:0409a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖觀自在菩薩大神呪心。",
        "from": "T20n1095_001:0409a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就如是最勝事業。",
        "from": "T20n1095_001:0409a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未成辦者應以素㲲畫作佛像。",
        "from": "T20n1095_001:0409a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所用彩色和以香膠勿取餘膠。",
        "from": "T20n1095_001:0409a06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於佛像邊畫觀自在菩薩像。",
        "from": "T20n1095_001:0409a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其身黃白紺髮垂下首冠華冠。",
        "from": "T20n1095_001:0409a07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "披䃜泥耶皮如摩醯首羅狀。",
        "from": "T20n1095_001:0409a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "環釧皆以珍寶而嚴飾之。",
        "from": "T20n1095_001:0409a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫師將欲畫時。",
        "from": "T20n1095_001:0409a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先當受八戒齋法。",
        "from": "T20n1095_001:0409a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫像成已。",
        "from": "T20n1095_001:0409a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彼像前用瞿摩夷作曼荼羅。",
        "from": "T20n1095_001:0409a10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縱廣一丈六尺散以白華。",
        "from": "T20n1095_001:0409a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其壇八方安八瓶香水。",
        "from": "T20n1095_001:0409a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置八分食或六十四分。",
        "from": "T20n1095_001:0409a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是供養除薰辛等。",
        "from": "T20n1095_001:0409a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒沈水香當三日三夜不食。",
        "from": "T20n1095_001:0409a13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或一日一夜不食。",
        "from": "T20n1095_001:0409a13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若食之時但食三種日食。",
        "from": "T20n1095_001:0409a14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於日日中三時澡浴著新淨衣。",
        "from": "T20n1095_001:0409a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦呪一千八遍。",
        "from": "T20n1095_001:0409a15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦呪滿已。",
        "from": "T20n1095_001:0409a15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者即於像前自見其身。",
        "from": "T20n1095_001:0409a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光明熾盛猶如猛焰。",
        "from": "T20n1095_001:0409a16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是見已心生歡喜。",
        "from": "T20n1095_001:0409a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖觀自在菩薩便現其前。",
        "from": "T20n1095_001:0409a17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有願求皆令滿足。",
        "from": "T20n1095_001:0409a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲隱形。",
        "from": "T20n1095_001:0409a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應取雌黃或安繕那藥呪一千八遍。",
        "from": "T20n1095_001:0409a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得隱形乘空而行。",
        "from": "T20n1095_001:0409a19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲不空智上首莊嚴勝三摩地。",
        "from": "T20n1095_001:0409a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有意樂皆得成辦。",
        "from": "T20n1095_001:0409a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是說已。",
        "from": "T20n1095_001:0409a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時薄伽梵歡喜讚歎。",
        "from": "T20n1095_001:0409a21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時聖觀自在菩薩摩訶薩。",
        "from": "T20n1095_001:0409a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及淨居天子索訶世界主。",
        "from": "T20n1095_001:0409a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自在大自在天王。",
        "from": "T20n1095_001:0409a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸菩薩大聲聞等。",
        "from": "T20n1095_001:0409a23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "承佛所說歡喜奉行。",
        "from": "T20n1095_001:0409a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空羂索呪心經",
        "from": "T20n1095_001:0409a25_100",
        "context_with_next_sentence": false
    }
]