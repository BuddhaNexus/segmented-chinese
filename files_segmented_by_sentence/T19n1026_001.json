[
    {
        "source_sentence": "佛說造塔延命功德經",
        "from": "T19n1026_001:0726a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "罽賓國三藏沙門般若奉　詔譯",
        "from": "T19n1026_001:0726a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。一時佛在舍衛國祇樹給孤獨園。",
        "from": "T19n1026_001:0726a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大苾芻眾及菩薩摩訶薩眾俱。",
        "from": "T19n1026_001:0726a11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時波斯匿王在大眾中。",
        "from": "T19n1026_001:0726a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即從座起整理衣服。",
        "from": "T19n1026_001:0726a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偏袒右肩右膝著地。",
        "from": "T19n1026_001:0726a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮佛雙足合掌恭敬。",
        "from": "T19n1026_001:0726a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而白佛言。",
        "from": "T19n1026_001:0726a13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "希有世尊。",
        "from": "T19n1026_001:0726a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相師占我。",
        "from": "T19n1026_001:0726a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "却後七日。",
        "from": "T19n1026_001:0726a14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必當壽盡。",
        "from": "T19n1026_001:0726a14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為無常苦所逼惱。",
        "from": "T19n1026_001:0726a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無所依仰來投世尊。",
        "from": "T19n1026_001:0726a15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊為我救護。",
        "from": "T19n1026_001:0726a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何令我得離憂苦。",
        "from": "T19n1026_001:0726a16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告言。",
        "from": "T19n1026_001:0726a16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大王善自安慰勿得憂怖。",
        "from": "T19n1026_001:0726a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛如來有善方便。",
        "from": "T19n1026_001:0726a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令大王獲殊勝利近延壽命。",
        "from": "T19n1026_001:0726a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當得阿耨多羅三藐三菩提。",
        "from": "T19n1026_001:0726a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諦聽諦聽善思念之。",
        "from": "T19n1026_001:0726a19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾當為汝分別解說。",
        "from": "T19n1026_001:0726a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大王當知。",
        "from": "T19n1026_001:0726a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲免離無常苦惱超入如來法身壽量。",
        "from": "T19n1026_001:0726a21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應先發心持佛淨戒修最上福。",
        "from": "T19n1026_001:0726a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能發此心延王壽命。",
        "from": "T19n1026_001:0726a22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王白佛言。",
        "from": "T19n1026_001:0726a22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊為我救護。",
        "from": "T19n1026_001:0726a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所說發心持佛淨戒修最上福。",
        "from": "T19n1026_001:0726a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我便奉行亦令國人奉行此法。",
        "from": "T19n1026_001:0726a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未知發心何等心。",
        "from": "T19n1026_001:0726a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持何等戒何福最上。",
        "from": "T19n1026_001:0726a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願如來為我宣說。",
        "from": "T19n1026_001:0726a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告大王言。",
        "from": "T19n1026_001:0726a26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發心者發四無量心。",
        "from": "T19n1026_001:0726a26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言持戒者持不殺戒。",
        "from": "T19n1026_001:0726a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修上福者無過造塔。",
        "from": "T19n1026_001:0726a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悲愍救護一切眾生。",
        "from": "T19n1026_001:0726a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天善神常來守護。",
        "from": "T19n1026_001:0726a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不相捨離如影隨形。",
        "from": "T19n1026_001:0726a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發生大王無邊福利。",
        "from": "T19n1026_001:0726a29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大王建立佛塔福利難思。",
        "from": "T19n1026_001:0726b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三世如來所共稱讚。",
        "from": "T19n1026_001:0726b01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為大王略說因緣。",
        "from": "T19n1026_001:0726b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王當諦聽。",
        "from": "T19n1026_001:0726b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃往古昔有一小兒此地牧牛。",
        "from": "T19n1026_001:0726b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有諸相師來共占相謂言。",
        "from": "T19n1026_001:0726b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此牧牛兒却後七日必當壽盡。",
        "from": "T19n1026_001:0726b04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是牧牛兒又於異時。",
        "from": "T19n1026_001:0726b04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與諸小兒聚沙為戲。",
        "from": "T19n1026_001:0726b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中有小兒摧沙為堆。",
        "from": "T19n1026_001:0726b05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言作佛塔高一磔手。",
        "from": "T19n1026_001:0726b06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或二或三至四磔手。",
        "from": "T19n1026_001:0726b06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時此小兒戲聚沙塔高一磔手。",
        "from": "T19n1026_001:0726b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "却後更延七年壽命。",
        "from": "T19n1026_001:0726b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於聚沙時。",
        "from": "T19n1026_001:0726b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有辟支佛持鉢而行。",
        "from": "T19n1026_001:0726b08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時諸小兒以嬉戲心。",
        "from": "T19n1026_001:0726b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將沙奉施言我施󰉓。",
        "from": "T19n1026_001:0726b09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時辟支佛引鉢受之。",
        "from": "T19n1026_001:0726b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以神通力沙變成󰉓。",
        "from": "T19n1026_001:0726b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時諸小兒見此因緣。",
        "from": "T19n1026_001:0726b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆悉獲得清淨信心。",
        "from": "T19n1026_001:0726b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時辟支佛與諸小兒悉授記莂作如是言。",
        "from": "T19n1026_001:0726b12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝諸童子所造之塔高一磔手者。",
        "from": "T19n1026_001:0726b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於未來世作鐵輪王。",
        "from": "T19n1026_001:0726b13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王一天下。",
        "from": "T19n1026_001:0726b13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二磔手者作銅輪王。",
        "from": "T19n1026_001:0726b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王二天下。",
        "from": "T19n1026_001:0726b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三磔手者作銀輪王。",
        "from": "T19n1026_001:0726b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王三天下。",
        "from": "T19n1026_001:0726b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四磔手者作金輪王。",
        "from": "T19n1026_001:0726b15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王四天下。",
        "from": "T19n1026_001:0726b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時諸小兒以嬉戲心造如是塔感如是果。",
        "from": "T19n1026_001:0726b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況大王發至誠心。",
        "from": "T19n1026_001:0726b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人。",
        "from": "T19n1026_001:0726b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以決定心如法造塔。",
        "from": "T19n1026_001:0726b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至一肘量一磔手一指節一穬麥。",
        "from": "T19n1026_001:0726b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所得功德無有限量。",
        "from": "T19n1026_001:0726b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常有諸天雨花供養。",
        "from": "T19n1026_001:0726b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故大王。",
        "from": "T19n1026_001:0726b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人欲造塔時。",
        "from": "T19n1026_001:0726b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先於一切眾生起大悲心而為先導。",
        "from": "T19n1026_001:0726b21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以菩提心而為根本。",
        "from": "T19n1026_001:0726b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後淨地作壇供養。",
        "from": "T19n1026_001:0726b22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以瞿摩夷塗之燒香散花。",
        "from": "T19n1026_001:0726b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面正東坐作是思惟。",
        "from": "T19n1026_001:0726b23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛薄伽梵自利利他功德圓滿。",
        "from": "T19n1026_001:0726b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復能滿足眾生之願。",
        "from": "T19n1026_001:0726b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今成就第一法身發菩提心。",
        "from": "T19n1026_001:0726b25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於薄伽梵所有造塔軌儀法則。",
        "from": "T19n1026_001:0726b26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一次第如法奉行。",
        "from": "T19n1026_001:0726b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一採集妙華。",
        "from": "T19n1026_001:0726b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以陀羅尼加持七遍。",
        "from": "T19n1026_001:0726b28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散布壇上供養如來。",
        "from": "T19n1026_001:0726b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陀羅尼曰。",
        "from": "T19n1026_001:0726b29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄖩曩􄗰謨􄙺薩􈋹怛他二合􄗱耶􄕘埵􆿅阿􄖭耨􄑀羯􄕗多􄪼跛羅􄕙底􆵍瑟恥二合􄕝帝􈁂布􈆵數波􄑆罽􅇇答迷􄕗怛􄕤他􄑘蘖􄕘多􄗱耶􄚦三􄢦藐􈃱三􈎭母􇧺馱􄗱耶􄕗怛􄟯儞也二合􄕤他􈁂補􈆶澁閉􈁂補􈆶澁閉二合􈂼蘇􈁂補􈆶澁閉二合􅁥娑縛二合􄚩賀引第二埏埴作泥團時。",
        "from": "T19n1026_001:0726c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作􆿅婀字觀。",
        "from": "T19n1026_001:0726c11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是如來法界身。",
        "from": "T19n1026_001:0726c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先當歸命作如是言。",
        "from": "T19n1026_001:0726c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄖩曩􄗭謨􄕯捺􄙣闍􄖨溺􄚼崛芻二合􅚾悉底二合􄗱耶􄕯捺􄙖嚩平􄙺薩􅞣嚩􄕗怛􄕤他􄑘蘖􄕗多􄖳難􄙖滿􆩴泥上􄗽囉􅑈怛曩二合􄩞怛羅二合􄗻演􄙦輸􄙠梵􆿅阿􄚼閦􄡸鞞􆿅阿􄗧彌􄕘陀􄗙畔􄣢差子阿反􄗙薄􄑘伽􄙠梵􆿆阿􅝪哩也引􄙖嚩􄙂嚕􄑂枳􄕝帝􈂽濕嚩二合􄗾羅􄗱耶􄑜麌􄙂嚕正埏埴時。",
        "from": "T19n1026_001:0726c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以俱胝陀羅尼。",
        "from": "T19n1026_001:0726c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持其泥二十一遍。",
        "from": "T19n1026_001:0726c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造一佛塔如造一俱胝佛塔。",
        "from": "T19n1026_001:0726c24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所獲功德等無有異。",
        "from": "T19n1026_001:0726c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故此陀羅尼名為俱胝陀羅尼。",
        "from": "T19n1026_001:0726c25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陀羅尼曰。",
        "from": "T19n1026_001:0726c26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􄕗怛􄟯儞也二合􄕤他􇩂唵􄚢􄑌􅋀蘇上佉澁茗四合􄚢􄑌􅋀蘇佉澁茗四合􄚢􄑌􅋀蘇佉澁麼四合􆿅阿􄖭耨􄑀羯􄕝諦􄑼訕􆩜諦􄑼訕􆩜諦􄕗誕􆩖諦􄕗誕􆩖諦􄓦儞平􄗽囉􄑄拘􄙄黎􄗷曳八􈇎戍􄙖嚩􄕽提九􄕝諦􄙦主􄙖嚩􄕽提十􄙺薩􅞣嚩􈎭母􇧺馱十一􆿅阿􄕽地􆵋瑟姹二合􄖩曩􆿅阿􄕽地􆵍瑟恥二合􄕗多􆿅阿􈎭母􄕙底􄕝羝十四􅁥娑嚩二合引􄚩賀引第三次於泥團作􆷱娑頗二合字觀。",
        "from": "T19n1026_001:0727a09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當觀此泥團猶如水精。",
        "from": "T19n1026_001:0727a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又如頗胝迦寶。",
        "from": "T19n1026_001:0727a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內外清淨分明瑩徹。",
        "from": "T19n1026_001:0727a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以陀羅尼加持一遍。",
        "from": "T19n1026_001:0727a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陀羅尼曰。",
        "from": "T19n1026_001:0727a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵􄙖嚩􄦽日羅二合􆿉嗢􄕗多􄗙皤􄗙嚩􄗱耶􅁥娑嚩二合引􄚩賀引第四次塗赤土時。",
        "from": "T19n1026_001:0727a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作火色觀。",
        "from": "T19n1026_001:0727a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即知一切世間出世間法。",
        "from": "T19n1026_001:0727a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆從微塵而得成就。",
        "from": "T19n1026_001:0727a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作此觀已。",
        "from": "T19n1026_001:0727a17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取乾赤土遍塗泥團。",
        "from": "T19n1026_001:0727a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀一切法本來無生畢竟空寂。",
        "from": "T19n1026_001:0727a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作􄚲含字觀。",
        "from": "T19n1026_001:0727a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想此泥團如鎔金色。",
        "from": "T19n1026_001:0727a19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後造塔。",
        "from": "T19n1026_001:0727a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以陀羅尼加持一遍。",
        "from": "T19n1026_001:0727a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陀羅尼曰。",
        "from": "T19n1026_001:0727a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵􆿅阿􄗽羅􄑼左􄙘尾􄗽羅􄑼左􅁥娑嚩二合引􄚩賀第五次以泥團入模時。",
        "from": "T19n1026_001:0727a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用陀羅尼加持一遍。",
        "from": "T19n1026_001:0727a23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後入模。",
        "from": "T19n1026_001:0727a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陀羅尼曰。",
        "from": "T19n1026_001:0727a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵􄙖嚩􄦽日羅二合引􄕼馱􄕛都􄑘蘖􅝗陛􅁥娑嚩二合引􄚩賀引第六次入法身真言時。",
        "from": "T19n1026_001:0727a27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用此陀羅尼加持一遍。",
        "from": "T19n1026_001:0727a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後入印子中。",
        "from": "T19n1026_001:0727a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陀羅尼曰。",
        "from": "T19n1026_001:0727a28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵􄕻達􈆺摩􄕼馱􄕛都􄑘蘖􅝗陛􅁥娑嚩􄚩賀引第七次椎打塔印時。",
        "from": "T19n1026_001:0727b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用陀羅尼加持一遍。",
        "from": "T19n1026_001:0727b03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後椎打。",
        "from": "T19n1026_001:0727b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陀羅尼曰。",
        "from": "T19n1026_001:0727b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵􄙖嚩􄦽日囉二合引􈁂補􄖭特􄑤伽􄗽羅􄑄拘􄓚吒􄗱耶􄑄􄓚􄗱拘吒耶􅁥娑嚩二合引􄚩賀第八次椎打竟。",
        "from": "T19n1026_001:0727b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以手作塔階時。",
        "from": "T19n1026_001:0727b09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用陀羅尼加持一遍。",
        "from": "T19n1026_001:0727b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安置塔階。",
        "from": "T19n1026_001:0727b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陀羅尼曰。",
        "from": "T19n1026_001:0727b10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵􄙖嚩􄦽日羅二合引􄗱喻􄙴曬􅁥娑嚩二合引􄚩賀第九次塔正從模中出時。",
        "from": "T19n1026_001:0727b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用陀羅尼加持一遍。",
        "from": "T19n1026_001:0727b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後出之。",
        "from": "T19n1026_001:0727b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陀羅尼曰。",
        "from": "T19n1026_001:0727b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵􄕻達􈆺摩􄕙底􄙦珠􅁥娑嚩二合引􄚩賀第十次脫塔出模已安置塔時。",
        "from": "T19n1026_001:0727b17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用陀羅尼加持一遍。",
        "from": "T19n1026_001:0727b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陀羅尼曰。",
        "from": "T19n1026_001:0727b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵􈂼蘇􄪼鉢羅二合引􄕙底􆵍瑟恥二合􄕗多􄙖嚩􄦽日入羅二合􅁥娑嚩二合􄚩賀引第十一次安置塔座了。",
        "from": "T19n1026_001:0727b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以手按塔頂上。",
        "from": "T19n1026_001:0727b23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以陀羅尼加持一遍。",
        "from": "T19n1026_001:0727b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陀羅尼曰。",
        "from": "T19n1026_001:0727b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵􄖿􄙊跛羅藍三合􄗥摩􄕗哆􈁀喃􄗷曳平􅁥娑嚩二合引􄚩賀引第十二次於塔上安傘蓋時。",
        "from": "T19n1026_001:0727b27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以陀羅尼加持一遍。",
        "from": "T19n1026_001:0727b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陀羅尼曰。",
        "from": "T19n1026_001:0727b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􇩂唵􄖿􄙊跛羅藍三合􄗥摩􆿅婀􄗹喻􄗷曬􅁥娑嚩二合􄚩賀佛告大王。",
        "from": "T19n1026_001:0727c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人。",
        "from": "T19n1026_001:0727c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以清淨心依此軌儀造作佛塔。",
        "from": "T19n1026_001:0727c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若自作若教人作。",
        "from": "T19n1026_001:0727c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復讚歎若當信受。",
        "from": "T19n1026_001:0727c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所得功德與造佛塔等無差別。",
        "from": "T19n1026_001:0727c05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知是人於此一生。",
        "from": "T19n1026_001:0727c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為一切毒藥所中。",
        "from": "T19n1026_001:0727c06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽命長遠無有橫死。",
        "from": "T19n1026_001:0727c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "究竟當得不壞之身。",
        "from": "T19n1026_001:0727c07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切鬼神不敢逼近。",
        "from": "T19n1026_001:0727c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五星七曜隨順驅使。",
        "from": "T19n1026_001:0727c08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切怨家悉皆退散。",
        "from": "T19n1026_001:0727c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨所生處身常無病。",
        "from": "T19n1026_001:0727c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生見皆歡喜。",
        "from": "T19n1026_001:0727c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無淨戒者淨戒滿足。",
        "from": "T19n1026_001:0727c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不調伏者能令調伏。",
        "from": "T19n1026_001:0727c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不清淨者能令清淨。",
        "from": "T19n1026_001:0727c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破齋戒者齋戒復生。",
        "from": "T19n1026_001:0727c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若犯四重及五無間極重罪業悉得消滅。",
        "from": "T19n1026_001:0727c12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無始劫來障累皆盡。",
        "from": "T19n1026_001:0727c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有女人欲求男者。",
        "from": "T19n1026_001:0727c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即生勇健福德之男。",
        "from": "T19n1026_001:0727c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四大天王常隨擁護。",
        "from": "T19n1026_001:0727c14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造塔功德其福如是。",
        "from": "T19n1026_001:0727c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若塔破壞變作微塵。",
        "from": "T19n1026_001:0727c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風吹一塵散落他處。",
        "from": "T19n1026_001:0727c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塵所經過山林河海。",
        "from": "T19n1026_001:0727c16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生觸斯塵者。",
        "from": "T19n1026_001:0727c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永更不受雜類之身。",
        "from": "T19n1026_001:0727c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨身受生常得見佛。",
        "from": "T19n1026_001:0727c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大王若善男子善女人。",
        "from": "T19n1026_001:0727c18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於造塔及陀羅尼。",
        "from": "T19n1026_001:0727c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想觀分明一心憶念。",
        "from": "T19n1026_001:0727c19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永離水難火難王難賊難。",
        "from": "T19n1026_001:0727c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀兵等難無能傷害。",
        "from": "T19n1026_001:0727c20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於曠野若空閑處。",
        "from": "T19n1026_001:0727c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於聚落及大會中。",
        "from": "T19n1026_001:0727c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能以此經宣告大眾。",
        "from": "T19n1026_001:0727c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即為救護眾生身命。",
        "from": "T19n1026_001:0727c22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所獲福德量與空等。",
        "from": "T19n1026_001:0727c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於後世書寫流通。",
        "from": "T19n1026_001:0727c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塔雖未造造塔功德等無有異。",
        "from": "T19n1026_001:0727c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為大王及未來世一切眾生。",
        "from": "T19n1026_001:0727c24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣開造塔延命功德。",
        "from": "T19n1026_001:0727c25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王當憶持。",
        "from": "T19n1026_001:0727c25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時波斯匿王及諸大眾。",
        "from": "T19n1026_001:0727c26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛所說皆大歡喜信受奉行。",
        "from": "T19n1026_001:0727c27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　中天竺三藏牟尼室利　　　說梵語",
        "from": "T19n1026_001:0727c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　西明寺大德賜紫沙門圓照　筆　授",
        "from": "T19n1026_001:0728a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　章教寺大德沙門鑑虛　　　潤文",
        "from": "T19n1026_001:0728a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日一校了　　　　　　淨嚴四十九載",
        "from": "T19n1026_001:0728a04_0",
        "context_with_next_sentence": true
    }
]