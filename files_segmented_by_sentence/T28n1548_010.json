[
    {
        "source_sentence": "2舍利弗阿毘曇論卷第十",
        "from": "T28n1548_010:0596b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姚秦罽賓三藏曇摩耶舍",
        "from": "T28n1548_010:0596b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共曇摩崛多等譯",
        "from": "T28n1548_010:0596b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非問分智品第四之二云何退分智？",
        "from": "T28n1548_010:0596b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若智不善，是名退分智。",
        "from": "T28n1548_010:0596b07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何住分智？",
        "from": "T28n1548_010:0596b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若智無記，是名住分智。",
        "from": "T28n1548_010:0596b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何增長分智？",
        "from": "T28n1548_010:0596b08_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若智非聖、善，是名增長分智。",
        "from": "T28n1548_010:0596b09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何解分智？",
        "from": "T28n1548_010:0596b09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若智聖、有報、能斷煩惱，是名解分智。",
        "from": "T28n1548_010:0596b10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何退分智？",
        "from": "T28n1548_010:0596b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若智生，退於非聖善法、非住、非增長，是名退分智。",
        "from": "T28n1548_010:0596b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何住分智？",
        "from": "T28n1548_010:0596b12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若智生，於非聖善法住、不退、不增長，是名住分智。",
        "from": "T28n1548_010:0596b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何增長分智？",
        "from": "T28n1548_010:0596b13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若智生，增長非聖善法、不退、不住，是名增長分智。",
        "from": "T28n1548_010:0596b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何解分智？",
        "from": "T28n1548_010:0596b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若共解解相應，是名解分智。",
        "from": "T28n1548_010:0596b15_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何退分智非住分智？",
        "from": "T28n1548_010:0596b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有退非住，是名退分智非住分。",
        "from": "T28n1548_010:0596b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何住分智非退分智？",
        "from": "T28n1548_010:0596b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有住非退，是名住分智非退。",
        "from": "T28n1548_010:0596b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何退分住分智？",
        "from": "T28n1548_010:0596b18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無一智退分住分智。",
        "from": "T28n1548_010:0596b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼若住分智非退分、若退分者非住分，是名退分住分智。",
        "from": "T28n1548_010:0596b20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何非退非住分智？",
        "from": "T28n1548_010:0596b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除退分住分智，若餘智見，名非退非住分智。",
        "from": "T28n1548_010:0596b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何退分智非增長分智？",
        "from": "T28n1548_010:0596b22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有退非增長，是名退分智非增長。",
        "from": "T28n1548_010:0596b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何增長分智非退？",
        "from": "T28n1548_010:0596b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智若增長非退，是名增長分智非退。",
        "from": "T28n1548_010:0596b24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何退分增長分智？",
        "from": "T28n1548_010:0596b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無一智退分增長分。",
        "from": "T28n1548_010:0596b25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼若退分非增長分智、增長分智非退分，是名退分增長分智。",
        "from": "T28n1548_010:0596b27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何非退分非增長分智？",
        "from": "T28n1548_010:0596b27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除退分增長分智，若餘智，是名非退分非增長分智。",
        "from": "T28n1548_010:0596b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何住分智非解分智？",
        "from": "T28n1548_010:0596b29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有住分非解，是名住分智非解。",
        "from": "T28n1548_010:0596c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何解分智非住分？",
        "from": "T28n1548_010:0596c01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智若有解非住，是名解分智非住。",
        "from": "T28n1548_010:0596c02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何住分解分智？",
        "from": "T28n1548_010:0596c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無一智若住分解分。",
        "from": "T28n1548_010:0596c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若住分智非解分、解分智非住分，是名住分解分智。",
        "from": "T28n1548_010:0596c04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何非住分非解分智？",
        "from": "T28n1548_010:0596c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除住分解分智，若餘智，是名非住分非解分智。",
        "from": "T28n1548_010:0596c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何增長分智非解分智？",
        "from": "T28n1548_010:0596c06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有增長非解，是名增長分智非解分。",
        "from": "T28n1548_010:0596c07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何解分智非增長分？",
        "from": "T28n1548_010:0596c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智若有解非增長，是名解分智非增長分。",
        "from": "T28n1548_010:0596c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何增長分解分智？",
        "from": "T28n1548_010:0596c09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無一智若增長分解分。",
        "from": "T28n1548_010:0596c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若增長分智非解分、解分智非增長分，是名增長分解分智。",
        "from": "T28n1548_010:0596c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何非增長分非解分智？",
        "from": "T28n1548_010:0596c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除增長分解分智，若餘智，是名非增長分非解分智。",
        "from": "T28n1548_010:0596c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何五智？",
        "from": "T28n1548_010:0596c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如世尊說：",
        "from": "T28n1548_010:0596c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修無量義定心等明照，比丘修此定已，無量義定心等明照已，內五智生。",
        "from": "T28n1548_010:0596c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等五？",
        "from": "T28n1548_010:0596c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此定現世樂後受樂報內生智，此定聖無染內生智，此定聖人親近內生智，此定寂靜勝妙聖心得解脫得除惡法內生智，憶念入此定憶念出此定內生智。",
        "from": "T28n1548_010:0596c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修定無量無量心等明照，比丘修定已，無量心等明照生此五智，是名五智。",
        "from": "T28n1548_010:0596c21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何六通？",
        "from": "T28n1548_010:0596c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神足智證通、天耳智證通、觀心心數法智證通、憶念宿命智證通、眾生生死智證通、漏盡智證通。",
        "from": "T28n1548_010:0596c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何神足智證通？",
        "from": "T28n1548_010:0596c24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若智生受無量神足，動大地、以一為多以多為一、近處遠處牆壁山崖通達無礙如虛空，結加趺坐往來空中如飛鳥、入地如水履水如地、身出烟焰如大火聚、日月神力威德難量手能捫摸，乃至梵天身得自在，是名神足智證通。",
        "from": "T28n1548_010:0596c29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何天耳智證通？",
        "from": "T28n1548_010:0597a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若智生天耳過於人耳，聞二種聲：",
        "from": "T28n1548_010:0597a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人非人聲，是名天耳智證通。",
        "from": "T28n1548_010:0597a02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何觀心心數法智證通？",
        "from": "T28n1548_010:0597a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若智生知他眾生他人心心數，若有欲心如實知有欲心、無欲心如實知無欲心，有恚心如實知有恚心、無恚心如實知無恚心，有癡心如實知有癡心、無癡心如實知無癡心，疾心如實知疾心、亂心如實知亂心、少心如實知少心、貴心如實知貴心，不定心如實知不定心、定心如實知定心，非解脫心如實知非解脫心、解脫心如實知解脫心，有勝心如實知有勝心、無勝心如實知無勝心，是名觀心心數智證通。",
        "from": "T28n1548_010:0597a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何憶念宿命智證通？",
        "from": "T28n1548_010:0597a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若智生憶念無量若干宿命，憶念一生二三四五、十二十三十四十五十、百千生、萬生十萬生、無量十生無量百生無量千生無量百千萬生，乃至若劫成劫壞若劫成壞、無量劫成無量劫壞無量劫成壞，我本在彼，如是名、如是姓、如是生、如是食、如是命，如是久壽、如是短壽，如是受苦樂，從彼死生彼，從彼死生彼已後，從死生彼、從死生此，如是有行成就，憶念若干宿命，是名宿命智證通。",
        "from": "T28n1548_010:0597a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何眾生生死智證通？",
        "from": "T28n1548_010:0597a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若智生天眼清淨過人，見眾生生死好色惡色、善道惡道、卑勝，知眾生如所造業，眾生身惡行成就、口惡行成就、意惡行成就，謗聖人、邪見行，緣邪見故，身壞命終生惡道地獄、畜生、餓鬼。",
        "from": "T28n1548_010:0597a26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生身善行、口善行、意善行成就，不謗聖人、正見行，緣正見故，身壞命終生善道天上人中。",
        "from": "T28n1548_010:0597a28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是天眼清淨過人，是名眾生生死智證通。",
        "from": "T28n1548_010:0597a29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何漏盡智證通？",
        "from": "T28n1548_010:0597b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若智生有漏盡得無漏心解脫慧解脫，現世自證知成就行，我生已盡、梵行已立、所作已辦、不復還有，是名漏盡智證通。",
        "from": "T28n1548_010:0597b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名六通。",
        "from": "T28n1548_010:0597b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何七方便？",
        "from": "T28n1548_010:0597b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如世尊說：",
        "from": "T28n1548_010:0597b05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比丘七處方便三種觀，此法中純善遠聞，謂尊丈夫。",
        "from": "T28n1548_010:0597b06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘有七處方便？",
        "from": "T28n1548_010:0597b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如比丘知色、知色集、知色滅、知色滅道、知色味、知色過患、知色出。",
        "from": "T28n1548_010:0597b08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知受、知受集、知受滅、知受滅道、知受味、知受過患、知受出。",
        "from": "T28n1548_010:0597b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知想、知想集、知想滅、知想滅道、知想味、知想過患、知想出。",
        "from": "T28n1548_010:0597b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知行、知行集、知行滅、知行滅道、知行味、知行過患、知行出。",
        "from": "T28n1548_010:0597b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知識、知識集、知識滅、知識滅道、知識味、知識過患、知識出。",
        "from": "T28n1548_010:0597b13_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知色？",
        "from": "T28n1548_010:0597b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如比丘四大、四大所造色如實知，比丘如是知色。",
        "from": "T28n1548_010:0597b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知色集？",
        "from": "T28n1548_010:0597b15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如比丘以愛集知色，如是比丘知色知色集。",
        "from": "T28n1548_010:0597b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知色滅？",
        "from": "T28n1548_010:0597b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如比丘愛滅，以愛滅知色滅，比丘如是知色滅。",
        "from": "T28n1548_010:0597b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘如實知色滅道？",
        "from": "T28n1548_010:0597b18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如比丘如實知八聖道：",
        "from": "T28n1548_010:0597b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正見、正覺、正語、正業、正命、正精進、正念、正定，如是比丘如實知色滅道。",
        "from": "T28n1548_010:0597b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知色味？",
        "from": "T28n1548_010:0597b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若緣色生喜樂是色味，如是比丘知色味。",
        "from": "T28n1548_010:0597b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知色過患？",
        "from": "T28n1548_010:0597b22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若色無常、苦、變異法是色過患，如是比丘知色過患。",
        "from": "T28n1548_010:0597b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知色出？",
        "from": "T28n1548_010:0597b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若色欲染調伏、欲染斷滅是出，如是比丘知色出。",
        "from": "T28n1548_010:0597b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有沙門婆羅門，如是知色、知色集、知色滅、知色滅道、知色味、知色過患、知色出，若厭色、離欲、滅、趣道。",
        "from": "T28n1548_010:0597b27_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善趣若善趣人，於是法中明了，及餘沙門婆羅門，如是知色、知色集、知色滅、知色滅道、知色味、知色過患、知色出，若厭色、離欲、證滅、解脫、不復生，善解脫若善解脫人，純善若純善人，無復生處。",
        "from": "T28n1548_010:0597c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知受？",
        "from": "T28n1548_010:0597c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六受身：",
        "from": "T28n1548_010:0597c03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼觸受、耳鼻舌身意觸受，是名六受身。",
        "from": "T28n1548_010:0597c04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比丘如是知受。",
        "from": "T28n1548_010:0597c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知受集？",
        "from": "T28n1548_010:0597c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如比丘以觸集知受集，如是知受集。",
        "from": "T28n1548_010:0597c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知受滅？",
        "from": "T28n1548_010:0597c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如比丘以觸滅知受滅，如是比丘知受滅。",
        "from": "T28n1548_010:0597c07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知受滅道？",
        "from": "T28n1548_010:0597c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如比丘如實知八聖道：",
        "from": "T28n1548_010:0597c08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正見、正覺、正語、正業、正命、正進、正念、正定，如是比丘知受滅道。",
        "from": "T28n1548_010:0597c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知受味？",
        "from": "T28n1548_010:0597c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若緣受生喜樂是受味，如是比丘知受味。",
        "from": "T28n1548_010:0597c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知受過患？",
        "from": "T28n1548_010:0597c11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受無常、苦、變異法是受過患，如是比丘知受過患。",
        "from": "T28n1548_010:0597c12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知受出？",
        "from": "T28n1548_010:0597c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受欲染調伏、欲染斷滅是受出，如是比丘知受出。",
        "from": "T28n1548_010:0597c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有沙門婆羅門，如是知受、知受集、知受滅、知受滅道、知受味、知受過患、知受出，厭受、離欲、證滅、趣道。",
        "from": "T28n1548_010:0597c16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善趣若善趣人，於是法中明了，及餘沙門婆羅門，如是知受、知受集、知受滅、知受滅道、知受味、知受過患、知受出，厭受、離欲、證滅、解脫、不復生。",
        "from": "T28n1548_010:0597c19_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善解脫若善解脫人，純善若純善人，無復生處。",
        "from": "T28n1548_010:0597c20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知想？",
        "from": "T28n1548_010:0597c21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六想身：",
        "from": "T28n1548_010:0597c21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色想、聲香味觸法想，是名六想身，如是比丘知想。",
        "from": "T28n1548_010:0597c22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知想集？",
        "from": "T28n1548_010:0597c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如比丘以觸集知想集，如是比丘知想集。",
        "from": "T28n1548_010:0597c23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知想滅？",
        "from": "T28n1548_010:0597c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比丘以觸滅知想滅，如是比丘知想滅。",
        "from": "T28n1548_010:0597c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知想滅道？",
        "from": "T28n1548_010:0597c25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如比丘如實知八聖道：",
        "from": "T28n1548_010:0597c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正見乃至正定，如是比丘知想滅道。",
        "from": "T28n1548_010:0597c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知想味？",
        "from": "T28n1548_010:0597c27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若緣想生喜樂是想味，如是比丘知想味。",
        "from": "T28n1548_010:0597c28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知想過患？",
        "from": "T28n1548_010:0597c28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想無常、苦、變異法是想過患，如是比丘知想過患。",
        "from": "T28n1548_010:0598a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知想出？",
        "from": "T28n1548_010:0598a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若想欲染調伏、欲染斷滅是想出，如是比丘知想出。",
        "from": "T28n1548_010:0598a02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有沙門婆羅門，如是知想、知想集、知想滅、知想滅道、知想味、知想過患、知想出，厭想、離欲、證滅、趣道。",
        "from": "T28n1548_010:0598a04_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善趣若善趣人，於是法中明了，及餘沙門婆羅門，如是知想、知想集、知想滅、知想滅道、知想味、知想過患、知想出，厭想、離欲、證滅、解脫、不復生。",
        "from": "T28n1548_010:0598a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善解脫若善解脫人，純善若純善人，無復生處。",
        "from": "T28n1548_010:0598a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知行？",
        "from": "T28n1548_010:0598a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六思身：",
        "from": "T28n1548_010:0598a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色思、聲香味觸法思，是名六思身，如是比丘知行。",
        "from": "T28n1548_010:0598a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知行集？",
        "from": "T28n1548_010:0598a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如比丘以無明集知行集，如是比丘知行集。",
        "from": "T28n1548_010:0598a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知行滅？",
        "from": "T28n1548_010:0598a13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如比丘以無明滅知行滅，如是比丘知行滅。",
        "from": "T28n1548_010:0598a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知行滅道？",
        "from": "T28n1548_010:0598a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如比丘如實知八聖道：",
        "from": "T28n1548_010:0598a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正見乃至正定，如是比丘如實知行滅道。",
        "from": "T28n1548_010:0598a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知行味？",
        "from": "T28n1548_010:0598a16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若緣行生喜樂是行味，如是比丘知行味。",
        "from": "T28n1548_010:0598a17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知行過患？",
        "from": "T28n1548_010:0598a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行無常、苦、變異法是行過患，如是比丘知行過患。",
        "from": "T28n1548_010:0598a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知行出？",
        "from": "T28n1548_010:0598a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若行欲染調伏、欲染斷滅，如是比丘知行出。",
        "from": "T28n1548_010:0598a20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有沙門婆羅門，如是知行、知行集、知行滅、知行滅道、知行味、知行過患、知行出，厭行、離欲、證滅、趣道。",
        "from": "T28n1548_010:0598a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善趣若善趣人，於是法中明了，及餘沙門婆羅門，如是知行、知行集、知行滅、知行滅道、知行味、知行過患、知行出，厭行、離欲、證滅、解脫、不復生。",
        "from": "T28n1548_010:0598a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善解脫若善解脫人，純善若純善人，無復生處。",
        "from": "T28n1548_010:0598a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知識？",
        "from": "T28n1548_010:0598a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六識身：",
        "from": "T28n1548_010:0598a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼識、身耳鼻舌身意識身，是六識身，如是比丘知識。",
        "from": "T28n1548_010:0598a29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知識集？",
        "from": "T28n1548_010:0598b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如比丘以名色集知識集，如是比丘知識集。",
        "from": "T28n1548_010:0598b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知識滅？",
        "from": "T28n1548_010:0598b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如比丘以名色滅知識滅，如是比丘知識滅。",
        "from": "T28n1548_010:0598b03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知識滅道？",
        "from": "T28n1548_010:0598b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如比丘如實知八聖道：",
        "from": "T28n1548_010:0598b04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正見乃至正定，如是比丘知識滅道。",
        "from": "T28n1548_010:0598b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知識味？",
        "from": "T28n1548_010:0598b05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣識生喜樂是識味，如是比丘知識味。",
        "from": "T28n1548_010:0598b06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知識過患？",
        "from": "T28n1548_010:0598b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識無常、苦、變異法是識過患，如是比丘知識過患。",
        "from": "T28n1548_010:0598b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘知識出？",
        "from": "T28n1548_010:0598b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若識欲染調伏、欲染斷滅是識出，如是比丘知識出。",
        "from": "T28n1548_010:0598b09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有沙門婆羅門，如是知識、知識集、知識滅、知識滅道、知識味、知識過患、知識出，厭識、離欲、證滅、趣道。",
        "from": "T28n1548_010:0598b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善趣若善趣人，於是法中明了，及餘沙門婆羅門，如是知識、知識集、知識滅、知識滅道、知識味、知識過患、知識出，厭識、離欲、證滅、解脫、不復生。",
        "from": "T28n1548_010:0598b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善解脫若善解脫人，純善若純善人，無復生處。",
        "from": "T28n1548_010:0598b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是比丘七處方便。",
        "from": "T28n1548_010:0598b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何比丘三種觀？",
        "from": "T28n1548_010:0598b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如比丘觀界、觀入、觀緣，如是比丘三種觀。",
        "from": "T28n1548_010:0598b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七處方便三種觀，比丘於是法中純善遠聞，謂尊丈夫。",
        "from": "T28n1548_010:0598b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名七處方便。",
        "from": "T28n1548_010:0598b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何苦法智？",
        "from": "T28n1548_010:0598b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有漏有為苦諦所攝法，若見苦、若見無我、思惟苦，於聖無漏智非比類智相無餘，是名苦法智。",
        "from": "T28n1548_010:0598b22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何苦比智？",
        "from": "T28n1548_010:0598b22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人已行生苦法智，及餘苦諦所攝法中如彼生、如彼相、如彼比類、此如彼彼如此，若於彼聖無漏智比類智相無餘，是名苦比智。",
        "from": "T28n1548_010:0598b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何集法智？",
        "from": "T28n1548_010:0598b25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若苦因、苦緒、苦集，若見集、見無我、思惟集，於聖無漏智非比類智相無餘，是名集法智。",
        "from": "T28n1548_010:0598b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何集比智？",
        "from": "T28n1548_010:0598b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人以行生集法智，及餘集諦所攝法中如彼生、如彼相、如彼比類、此如彼彼如此，於聖無漏智比類智相無餘，是名集比智。",
        "from": "T28n1548_010:0598c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何滅法智？",
        "from": "T28n1548_010:0598c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若盡苦、盡煩惱、盡漏法中，若見滅、見無我、思惟滅，於聖無漏智非比類智相無餘，是名滅法智。",
        "from": "T28n1548_010:0598c04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何滅比智？",
        "from": "T28n1548_010:0598c04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人已行生滅法智，及餘滅諦所攝法中如彼生、如彼相、如彼比類、此如彼彼如此，若於聖無漏智比類智相無餘，是名滅比智。",
        "from": "T28n1548_010:0598c07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何道法智？",
        "from": "T28n1548_010:0598c07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若聖道出要正滅苦法中，見道、見無我、思惟道，於聖無漏智非比類智相無餘，是名道法智。",
        "from": "T28n1548_010:0598c09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何道比智？",
        "from": "T28n1548_010:0598c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人以法行生道法智，及餘道諦所攝法中如彼生、如彼相、如彼比類、此如彼彼如此，於聖無漏智比類智相無餘，是名道比智。",
        "from": "T28n1548_010:0598c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何九方便？",
        "from": "T28n1548_010:0598c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定定方便、定入定方便、定住方便、出定方便、定境界方便、定行處方便、定樂方便、轉定方便，順不順法，善法相善思惟善解。",
        "from": "T28n1548_010:0598c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何定定方便？",
        "from": "T28n1548_010:0598c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定定眾、定定名字、定定觸、定定思惟，是有覺有觀定、是無覺有觀定、是無覺無觀定、是空定、是無相定、是無願定，是定若知見解射方便。",
        "from": "T28n1548_010:0598c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何定入定方便？",
        "from": "T28n1548_010:0598c20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入定眾、入定名字、入定觸、入定思惟，是有覺有觀、是無覺有觀、是無覺無觀定、是空無相無願定入定，入是定。",
        "from": "T28n1548_010:0598c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入是定已，是法勝入定，若知見解射方便，是名定入定方便。",
        "from": "T28n1548_010:0598c24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何定住方便？",
        "from": "T28n1548_010:0598c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定住眾、定住名字、定住觸、定住思惟，是定住有覺有觀、是無覺有觀、是無覺無觀、是空無相無願定。",
        "from": "T28n1548_010:0598c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是定住。",
        "from": "T28n1548_010:0598c27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定住已，是法勝定住，若知見解射方便，是名定住方便。",
        "from": "T28n1548_010:0598c28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何出定方便？",
        "from": "T28n1548_010:0598c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出定眾、出定名字、出定觸、出定思惟，是出定有覺有觀、是無覺有觀、是無覺無觀、是空是無相是無願，是出定。",
        "from": "T28n1548_010:0599a02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是出定已，是法勝出定，若知見解射方便，是名出定方便。",
        "from": "T28n1548_010:0599a03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何定境界方便？",
        "from": "T28n1548_010:0599a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若思惟法定生，若法是定境界，若知見解射方便，名定境界方便。",
        "from": "T28n1548_010:0599a05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何定行處方便？",
        "from": "T28n1548_010:0599a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定行謂四念處，若知見解射方便，是名定行處方便。",
        "from": "T28n1548_010:0599a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何定樂方便？",
        "from": "T28n1548_010:0599a07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定樂謂除樂眾，定樂名字、定樂觸、定樂思惟，是有覺有觀定樂、是無覺有觀、是無覺無觀、是空是無相是無願定，是定樂。",
        "from": "T28n1548_010:0599a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是法勝定樂，若知見解射方便，是名定樂方便。",
        "from": "T28n1548_010:0599a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何轉定方便？",
        "from": "T28n1548_010:0599a11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於初禪心起入二禪心住、於初禪心起入三禪心住、於初禪心起入四禪心住，於二禪心起入三禪心住、於二禪心起入四禪心住，於三禪心起入四禪心住，若知見解射方便，是名轉定方便。",
        "from": "T28n1548_010:0599a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何謂善取？",
        "from": "T28n1548_010:0599a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "順不順法相善思惟善解。",
        "from": "T28n1548_010:0599a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何非定順法？",
        "from": "T28n1548_010:0599a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若法不善，是名非定順法。",
        "from": "T28n1548_010:0599a17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何定順法？",
        "from": "T28n1548_010:0599a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若法善，是名定順法。",
        "from": "T28n1548_010:0599a18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次非定順法，若法有勝，是名非定順法。",
        "from": "T28n1548_010:0599a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次定順法，若法無勝，是名定順法。",
        "from": "T28n1548_010:0599a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次非定順法，若思惟法定不生，是名非定順法。",
        "from": "T28n1548_010:0599a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次定順法，若思惟法定生，是名定順法。",
        "from": "T28n1548_010:0599a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善取法相善思惟善解，是謂善取順不順法相善思惟善解。",
        "from": "T28n1548_010:0599a23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名九方便。",
        "from": "T28n1548_010:0599a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何如來十力？",
        "from": "T28n1548_010:0599a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處非處智如來力，過去未來現在業受業處因報智如來力，他眾生他人根勝非勝智如來力，他眾生他人若干解智如來力，若干界無量世界智如來力，一切道至處智如來力，禪解脫定入定垢淨起智如來力，憶念宿命證智如來力，眾生生死證智如來力，有漏盡智如來力。",
        "from": "T28n1548_010:0599b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何謂處非處如來力？",
        "from": "T28n1548_010:0599b02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何處非處？",
        "from": "T28n1548_010:0599b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非處謂身行惡、口行惡、意行惡，謂受愛喜適意報，非處。",
        "from": "T28n1548_010:0599b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若身行惡、口行惡、意行惡，受不喜不愛不適意報，有是處。",
        "from": "T28n1548_010:0599b05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非處謂身行善、口行善、意行善，謂受不愛不喜不適意報，非處。",
        "from": "T28n1548_010:0599b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若身行善、口行善、意行善，受愛喜適意報，有是處。",
        "from": "T28n1548_010:0599b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非處謂身行不善、口行不善、意行不善，成就謗聖人邪見業故，彼因緣身壞命終，生善道人天中，非處。",
        "from": "T28n1548_010:0599b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若身行不善、口行不善、意行不善，成就謗聖人邪見行，緣邪見業故，彼因緣故身壞命終，生惡道地獄中，有是處非處。",
        "from": "T28n1548_010:0599b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若身善行、口善行、意善行，成就不謗聖人正見行，緣正見業因緣故，身壞命終生惡道地獄中，非處。",
        "from": "T28n1548_010:0599b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若身善行、口善行、意善行，成就不謗聖人正見行，緣行正見業因緣故，身壞命終生善道人天中，有是處非處。",
        "from": "T28n1548_010:0599b17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見具足人故斷母命，無有是處是處。",
        "from": "T28n1548_010:0599b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若凡夫人故斷母命，有是處非處。",
        "from": "T28n1548_010:0599b19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見具足人故斷父命，無有是處是處。",
        "from": "T28n1548_010:0599b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若凡夫人故斷父命，有是處非處。",
        "from": "T28n1548_010:0599b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見具足人故斷羅漢聲聞命，無是處是處。",
        "from": "T28n1548_010:0599b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若凡夫人故斷羅漢命，有是處非處。",
        "from": "T28n1548_010:0599b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見具足人破眾僧，無有是處是處。",
        "from": "T28n1548_010:0599b23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若凡夫人破眾僧，有是處非處。",
        "from": "T28n1548_010:0599b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見具足人於如來身惡心出血，無有是處是處。",
        "from": "T28n1548_010:0599b25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若凡夫人於如來身惡心出血，有是處非處。",
        "from": "T28n1548_010:0599b26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見具足人於是法外求餘尊勝，無有是處是處。",
        "from": "T28n1548_010:0599b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若凡夫人於是法外求餘尊勝，有是處非處。",
        "from": "T28n1548_010:0599b28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見具足人於是法外求餘受供養者，無有是處是處。",
        "from": "T28n1548_010:0599c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若凡夫人於是法外求餘受供養者，有是處非處。",
        "from": "T28n1548_010:0599c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見具足人於是法外求餘沙門婆羅門說正見，無有是處是處。",
        "from": "T28n1548_010:0599c03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若凡夫人於是法外求餘沙門婆羅門說正見，有是處非處。",
        "from": "T28n1548_010:0599c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見具足人於是法外，若餘沙門婆羅門說法，讚言此一切智一切見，無有是處是處。",
        "from": "T28n1548_010:0599c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若凡夫人於是法外，若餘沙門婆羅門說法，讚言此一切智一切見，有是處非處。",
        "from": "T28n1548_010:0599c08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見具足人若於是法外，若有沙門婆羅門，異緣實我世常，此實餘虛妄；",
        "from": "T28n1548_010:0599c10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我世非常，此實餘虛妄；",
        "from": "T28n1548_010:0599c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我世常非常，此實餘虛妄；",
        "from": "T28n1548_010:0599c11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我世非常非無常，此實餘虛妄；",
        "from": "T28n1548_010:0599c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我世有邊，此實餘虛妄；",
        "from": "T28n1548_010:0599c12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我世無邊，此實餘虛妄；",
        "from": "T28n1548_010:0599c13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我世有邊無邊，此實餘虛妄；",
        "from": "T28n1548_010:0599c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我世非有邊非無邊，此實餘虛妄；",
        "from": "T28n1548_010:0599c14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命是身，此實餘虛妄；",
        "from": "T28n1548_010:0599c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身是命，此實餘虛妄；",
        "from": "T28n1548_010:0599c15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身命異，此實餘虛妄；",
        "from": "T28n1548_010:0599c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無命無身，此實餘虛妄；",
        "from": "T28n1548_010:0599c16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有如去涅槃，此實餘虛妄；",
        "from": "T28n1548_010:0599c17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無如去涅槃，此實餘虛妄；",
        "from": "T28n1548_010:0599c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有如去無如去涅槃，此實餘虛妄；",
        "from": "T28n1548_010:0599c18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非有如去非無如去涅槃，此實餘虛妄，異緣實為真實，無有是處是處。",
        "from": "T28n1548_010:0599c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若凡夫人，若於是法外，若沙門婆羅門，有異緣實我世常，此實餘虛妄，乃至非有如去非無如去涅槃，異緣實為真實，有是處非處。",
        "from": "T28n1548_010:0599c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利弗阿毘曇論卷第十",
        "from": "T28n1548_010:0599c24_100",
        "context_with_next_sentence": false
    }
]