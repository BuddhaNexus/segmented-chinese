[
    {
        "source_sentence": "歷代三寶紀卷第二帝年次前漢新王後漢",
        "from": "T49n2034_002:0029b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開皇十七年翻經學士臣費長房上",
        "from": "T49n2034_002:0029b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "2 帝年次（前漢　新王　後漢）帝年次者。劉邦字季。沛郡豐人。母媼息于",
        "from": "T49n2034_002:0029b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大澤夢與神交。",
        "from": "T49n2034_002:0029b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "感而生季。",
        "from": "T49n2034_002:0029b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "首龍顏額隆準。",
        "from": "T49n2034_002:0029b06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寬仁大度美鬚髯。",
        "from": "T49n2034_002:0029b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左股髀間有七十二黑子。",
        "from": "T49n2034_002:0029b07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因託暴秦鼓其潛翼。",
        "from": "T49n2034_002:0029b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既吞強楚遂奮雲氣。",
        "from": "T49n2034_002:0029b08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後凡經七十二戰。",
        "from": "T49n2034_002:0029b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天下乃定。",
        "from": "T49n2034_002:0029b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黑子之應其瑞在茲。",
        "from": "T49n2034_002:0029b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既遠承周因爾稱漢。",
        "from": "T49n2034_002:0029b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "改咸陽曰長安。",
        "from": "T49n2034_002:0029b10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而都之。",
        "from": "T49n2034_002:0029b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自從開闢三皇五帝夏殷周秦迄文景前並以帝王標年。",
        "from": "T49n2034_002:0029b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無別立號。",
        "from": "T49n2034_002:0029b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至武帝世始製年名稱。",
        "from": "T49n2034_002:0029b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建元云元狩。",
        "from": "T49n2034_002:0029b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二年霍去病討匈奴。",
        "from": "T49n2034_002:0029b13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過居延山擒休屠王。",
        "from": "T49n2034_002:0029b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲其金人以為大神。",
        "from": "T49n2034_002:0029b14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "列置於宮。",
        "from": "T49n2034_002:0029b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒香禮之。",
        "from": "T49n2034_002:0029b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此即像初至。",
        "from": "T49n2034_002:0029b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三年鑿昆明池。",
        "from": "T49n2034_002:0029b15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍於下際悉是灰墨。",
        "from": "T49n2034_002:0029b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝以其事問東方朔。",
        "from": "T49n2034_002:0029b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朔奉答云。",
        "from": "T49n2034_002:0029b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非臣所知。",
        "from": "T49n2034_002:0029b17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陛下可訪西域胡人。",
        "from": "T49n2034_002:0029b17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其後張騫使大夏還。",
        "from": "T49n2034_002:0029c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漢始知有身毒國。",
        "from": "T49n2034_002:0029c01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又平帝世。",
        "from": "T49n2034_002:0029c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大夫劉向自稱。",
        "from": "T49n2034_002:0029c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "余覽典籍往往見有佛經。",
        "from": "T49n2034_002:0029c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及刪列仙傳云。",
        "from": "T49n2034_002:0029c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得藏書緬尋太史創撰列仙圖。",
        "from": "T49n2034_002:0029c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自六代迄到于今七百餘人。",
        "from": "T49n2034_002:0029c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向檢虛實定得一百四十六人。",
        "from": "T49n2034_002:0029c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其七十四人已見佛經。",
        "from": "T49n2034_002:0029c05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "推此豈非漢時經目及古舊二錄道安所載十二賢者等經例是。",
        "from": "T49n2034_002:0029c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所餘止有七十二人為列仙傳。",
        "from": "T49n2034_002:0029c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此蓋博識覩其大倫。",
        "from": "T49n2034_002:0029c08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將知周時久流釋典。",
        "from": "T49n2034_002:0029c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "情識未洽歸信者稀。",
        "from": "T49n2034_002:0029c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "秦雖爇除漢復興出。",
        "from": "T49n2034_002:0029c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故令劉向搜藏書見更列仙傳云。",
        "from": "T49n2034_002:0029c10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又哀帝世。",
        "from": "T49n2034_002:0029c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元壽年中景憲使於大月支國受得浮圖經。",
        "from": "T49n2034_002:0029c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "驗向所尋便實錄矣。",
        "from": "T49n2034_002:0029c12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王莽字巨君。",
        "from": "T49n2034_002:0029c12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元城人。",
        "from": "T49n2034_002:0029c12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為漢大司馬翼輔平帝。",
        "from": "T49n2034_002:0029c13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元始五年乙丑帝崩。",
        "from": "T49n2034_002:0029c13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莽立宣帝玄孫嬰號稱孺子。",
        "from": "T49n2034_002:0029c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莽為丞相。",
        "from": "T49n2034_002:0029c14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居攝三年。",
        "from": "T49n2034_002:0029c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即真十五年通數十八年。",
        "from": "T49n2034_002:0029c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實止十七載。",
        "from": "T49n2034_002:0029c15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莽為景帝第七世孫劉玄字聖公所滅。",
        "from": "T49n2034_002:0029c16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱更始元。",
        "from": "T49n2034_002:0029d01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玄立二年復為赤眉所殺。",
        "from": "T49n2034_002:0029d01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劉秀字文叔。",
        "from": "T49n2034_002:0029d01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高祖九世孫。",
        "from": "T49n2034_002:0029d02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長沙定王後。",
        "from": "T49n2034_002:0029d02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居乎南陽之白水鄉。",
        "from": "T49n2034_002:0029d03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父為濟陽令。",
        "from": "T49n2034_002:0029d03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生秀於縣中。",
        "from": "T49n2034_002:0029d03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有赤光之瑞。",
        "from": "T49n2034_002:0029d03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是歲嘉穀一莖九穗。",
        "from": "T49n2034_002:0029d04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因名為秀。",
        "from": "T49n2034_002:0029d04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隆準日角口大眉長面美髭髯。",
        "from": "T49n2034_002:0029d05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性善仁愛。",
        "from": "T49n2034_002:0029d05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起於南陽。",
        "from": "T49n2034_002:0029d05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除蕩赤眉。",
        "from": "T49n2034_002:0029d06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂定天下。",
        "from": "T49n2034_002:0029d06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為後漢光武帝焉。",
        "from": "T49n2034_002:0029d06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徙都洛陽。",
        "from": "T49n2034_002:0029d06_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號年建武。",
        "from": "T49n2034_002:0029d07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞昔世語光武起南陽。",
        "from": "T49n2034_002:0029d07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南陽無賤士。",
        "from": "T49n2034_002:0029d08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常謂此虛誕。",
        "from": "T49n2034_002:0029d08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見今經說。",
        "from": "T49n2034_002:0029d08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋迦出天竺。",
        "from": "T49n2034_002:0029d08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天竺多聖人。",
        "from": "T49n2034_002:0029d09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方驗斯真實。",
        "from": "T49n2034_002:0029d09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至明帝世永平七年。",
        "from": "T49n2034_002:0029d09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忽感神夢。",
        "from": "T49n2034_002:0029d10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覺已歡悅。",
        "from": "T49n2034_002:0029d10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命使西行。",
        "from": "T49n2034_002:0029d10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因爾宅心遊志福境。",
        "from": "T49n2034_002:0029d11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嘗從容暇問弟東平王處。",
        "from": "T49n2034_002:0029d11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "家居何者最樂。",
        "from": "T49n2034_002:0029d12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東平璝偉腰帶十圍。",
        "from": "T49n2034_002:0029d12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即奏對云。",
        "from": "T49n2034_002:0029d12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為善最樂。",
        "from": "T49n2034_002:0029d12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝稱之曰。",
        "from": "T49n2034_002:0029d13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此言甚大。",
        "from": "T49n2034_002:0029d13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "足可以副王之腰腹。",
        "from": "T49n2034_002:0029d13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後使迴還。",
        "from": "T49n2034_002:0029d14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仍詔宣譯。",
        "from": "T49n2034_002:0029d14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "源發自此流注達今。",
        "from": "T49n2034_002:0029d14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法海無窮玄河靡竭。",
        "from": "T49n2034_002:0029d15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後兩漢通及新王。",
        "from": "T49n2034_002:0029d15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合二十六君。",
        "from": "T49n2034_002:0029d16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四百二十五載。",
        "from": "T49n2034_002:0029d16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "紀錄帝世東西京云。",
        "from": "T49n2034_002:0029d16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前漢高帝十二年都長安　惠帝七年　呂后攝八年　文帝二十三年　景帝十六年　武帝五十四年　昭帝十三年　宣帝二十五年　元帝十六年　成帝二十六年　哀帝六年　平帝五年　新王莾十七年治長安　更始帝二年亦長安後漢光武帝二十三年都洛陽　明帝十八年十年譯經　章帝十三年　和帝十七年　殤帝一年　安帝十九年　順帝十九年　沖帝一年　質帝一年　桓帝二十一年　靈帝二十二年　獻帝三十年前漢乙未高帝元劉氏名邦字阿季。",
        "from": "T49n2034_002:0030a02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堯後。",
        "from": "T49n2034_002:0030a02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "治十二年都長安。",
        "from": "T49n2034_002:0030a02_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛入涅槃來至此已四百五年　丙申二　丁酉三　戊戌四　己亥五　庚子六　辛丑七改咸陽為長安　壬寅八　癸卯九　甲辰十　乙巳十一　丙午十二丁未惠帝盈元高帝子治七年。",
        "from": "T49n2034_002:0030a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛入涅槃來至此已四百一十五年　戊申二　己酉三舂發長安六百里男女十四萬六千人。",
        "from": "T49n2034_002:0030a05_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "築長安城舊城西面。",
        "from": "T49n2034_002:0030a05_59",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十日了。",
        "from": "T49n2034_002:0030a05_64",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六月又發諸侯率一十二萬人築南面　庚戌四　辛亥五正月又發六百里男女十四萬五千人築城北面。",
        "from": "T49n2034_002:0030a06_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十日了。",
        "from": "T49n2034_002:0030a06_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夏又發一十二萬五千人築城東面。",
        "from": "T49n2034_002:0030a06_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十日了。",
        "from": "T49n2034_002:0030a06_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "秋九月長安城四面一切悉訖　壬子六　癸丑七甲寅呂后元高帝后攝政八年佛入涅槃來至此已四百二十二年　乙卯二　丙辰三　丁巳四　戊午五　己未六　庚申七　辛酉八壬戌文帝恒元高帝庶子治二十三年佛涅槃來至此已四百三十二年　癸亥二　甲子三　乙丑四　丙寅五　丁卯六　戊辰七　己巳八　庚午九　辛未十　壬申十一　癸酉十二　甲戌十三　乙亥十四　丙子十五　丁丑十六　戊寅後元元年　己卯二　庚辰三　辛巳四文殊師利般泥洹經云。",
        "from": "T49n2034_002:0030a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛滅度後四百五十年。",
        "from": "T49n2034_002:0030a14_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊師利當至雪山為五百仙人說十二部經訖。",
        "from": "T49n2034_002:0030a14_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還本生地入涅槃五月大赦。",
        "from": "T49n2034_002:0030a14_55",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "免官奴婢為庶人　壬午五　癸未六　甲申七乙酉景帝啟元文帝子治十六年。",
        "from": "T49n2034_002:0030a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛入涅槃來至此已四百五十四年　丙戌二　丁亥三　戊子四　己丑五　庚寅六　辛卯七　壬辰中元元年　癸巳二　甲午三　乙未四　丙申五　丁酉六　戊戌後元元年　己亥二　庚子三辛丑武帝徹景帝子治五十四年。",
        "from": "T49n2034_002:0030a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自此始建年號稱建元元年。",
        "from": "T49n2034_002:0030a20_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東方朔上書自進。",
        "from": "T49n2034_002:0030a20_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時年二十二民年九十已上復子孫令侍養。",
        "from": "T49n2034_002:0030a20_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛入涅槃至此已四百七十年　壬寅二置茂陵邑徙郡國豪傑居之。",
        "from": "T49n2034_002:0030a20_79",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賜徙民戶錢二十萬田二頃　癸卯三　甲辰四　乙巳五　丙午六丁未元光元　戊申二　己酉三　庚戌四　辛亥五　壬子六外國傳云。",
        "from": "T49n2034_002:0030a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛滅度後四百八十年有神通羅漢名呵利難陀。",
        "from": "T49n2034_002:0030a23_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國王之子。",
        "from": "T49n2034_002:0030a23_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於優長國東北造牛頭栴檀彌勒像。",
        "from": "T49n2034_002:0030a23_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高八丈。",
        "from": "T49n2034_002:0030a23_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將巧匠三人上兜率。",
        "from": "T49n2034_002:0030a23_61",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "看真彌勒造。",
        "from": "T49n2034_002:0030a23_67",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後得成甚有神驗　癸丑元朔元　甲寅二　乙卯三　丙辰四　丁巳五　戊午六己未元狩元十月帝幸雍獵得一白狩如󱞗五蹄改元云　庚申二霍去病北討匃奴。",
        "from": "T49n2034_002:0031a01_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過居延山。",
        "from": "T49n2034_002:0031a01_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擒休屠王。",
        "from": "T49n2034_002:0031a01_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲其金人以為大神。",
        "from": "T49n2034_002:0031a01_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "列置甘泉宮燒香禮之。",
        "from": "T49n2034_002:0031a01_62",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此即漢地佛像先來也　辛酉三掘昆明池。",
        "from": "T49n2034_002:0031a01_80",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其下悉是炭墨。",
        "from": "T49n2034_002:0031a01_87",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "武帝以問東方朔令辯所由。",
        "from": "T49n2034_002:0031a01_99",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朔答云。",
        "from": "T49n2034_002:0031a01_103",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非臣所知。",
        "from": "T49n2034_002:0031a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陛下可問西域梵人　壬戌四　癸亥五　甲子六　乙丑元鼎元六月汾陰得寶鼎水中。",
        "from": "T49n2034_002:0031a02_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鼎大八尺一寸。",
        "from": "T49n2034_002:0031a02_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高三尺六寸。",
        "from": "T49n2034_002:0031a02_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故改元云。",
        "from": "T49n2034_002:0031a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔秦遣人入水求鼎不得漢乃自出。",
        "from": "T49n2034_002:0031a03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可謂神物有應則彰無感則隱。",
        "from": "T49n2034_002:0031a03_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈人求哉　丙寅二　丁卯三　戊辰四　己巳五　庚午六辛未元封元正月甲子帝祭嵩山。",
        "from": "T49n2034_002:0031a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起道宮齋七日祀訖還。",
        "from": "T49n2034_002:0031a05_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四月帝閑居。",
        "from": "T49n2034_002:0031a05_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方朔等侍。",
        "from": "T49n2034_002:0031a05_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忽見一青衣女子來告帝云。",
        "from": "T49n2034_002:0031a05_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從今去百日清齋至七月七日。",
        "from": "T49n2034_002:0031a05_59",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西王母許降。",
        "from": "T49n2034_002:0031a05_65",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至期二更果來赴帝　壬申二　癸酉三帝先承王母言。",
        "from": "T49n2034_002:0031a05_88",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以是年七月齋戒。",
        "from": "T49n2034_002:0031a05_96",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以五嶽圖授董仲君。",
        "from": "T49n2034_002:0031a05_105",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "董仲君登即寫受　甲戌四帝又先承上元夫人言。",
        "from": "T49n2034_002:0031a06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四年七月齋戒。",
        "from": "T49n2034_002:0031a06_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以五帝六甲靈飛十二事授李少君。",
        "from": "T49n2034_002:0031a06_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "登即寫受此二書傳行世者由先傳此二君所以存矣　乙亥五　丙子六　丁丑太初元十一月己酉天火焚柏梁臺。",
        "from": "T49n2034_002:0031a07_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是真形圖六甲五帝靈飛十二事并帝所受集訣凡四卷共函並為火燒失本　戊寅二　己卯三　庚辰四　辛巳天漢元　壬午二　癸未三　甲申四乙酉太始元　丙戌二　丁亥三　戊子四三月帝幸太山祠西王母求靈。",
        "from": "T49n2034_002:0031a09_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而神不應　己丑延和元　庚寅二　辛卯三　壬辰四正月幸東萊大海齋祠西王母。",
        "from": "T49n2034_002:0031a10_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上元夫人求應不降　癸巳後元元　甲午二乙未昭帝弗陵武帝子治十三年始元元年佛入涅槃至此已五百二十四年　丙申二　丁酉三　戊戌四　己亥五　庚子六　辛丑元鳳元　壬寅二　癸卯三　甲辰四　乙巳五　丙午六　丁未元平元戊申宣帝詢武帝曾孫治二十五年。",
        "from": "T49n2034_002:0031a14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "太始元年。",
        "from": "T49n2034_002:0031a14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛入涅槃來至此已五百三十年　己酉二　庚戌三　辛亥四壬子地節元　癸丑二　甲寅三　乙卯四　丙辰元康元　丁巳二　戊午三　己未四　庚申神雀元　辛酉二　壬戌三　癸亥四　甲子五鳳元　乙丑二　丙寅三　丁卯四　戊辰甘露元一云甘始元　己巳二　庚午三　辛未四壬申黃龍元癸酉元帝奭宣帝子治十六年初元元年。",
        "from": "T49n2034_002:0031a20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛入涅槃來至此已五百六十二年　甲戌二　乙亥三　丙子四　丁丑五　戊寅永光元　己卯二　庚辰三　辛巳四　壬午五　癸未建明元一云建昭元　甲申二　乙酉三　丙戌四　丁亥五　戊子竟寧元己丑成帝驁元帝子治二十六年稱建始元年。",
        "from": "T49n2034_002:0031a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛入涅槃來至此已五百七十八年　庚寅二　辛卯三　壬辰四癸巳河平元　甲午二　乙未三　丙申四　丁酉陽明元一云陽朔　戊戌二　己亥三　庚子四　辛丑鴻嘉元　壬寅二　癸卯三大夫劉向刪列仙傳云。",
        "from": "T49n2034_002:0032a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七十四人己見佛經　甲辰四　乙巳永始元　丙午二　丁未三　戊申四　己酉元延元　庚戌二　辛亥三　壬子四癸丑綏和元　甲寅二乙卯哀帝欣元帝孫治六年稱建平元。",
        "from": "T49n2034_002:0032a06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛入涅槃來至此六百四年　丙辰二　丁巳三　戊午四己未元壽元景憲使大月支國。",
        "from": "T49n2034_002:0032a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受得浮圖經。",
        "from": "T49n2034_002:0032a07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "浮圖者即佛陀也。",
        "from": "T49n2034_002:0032a07_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈非如來相續久矣　庚申二辛酉平帝衍哀帝子治五年。",
        "from": "T49n2034_002:0032a08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱元始元。",
        "from": "T49n2034_002:0032a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛入涅槃來至此六百一十年　壬戌二　癸亥三　甲子四　乙丑五　丙寅居攝元莾立宣帝玄孫嬰號孺子。",
        "from": "T49n2034_002:0032a09_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莾為丞相攝行政事。",
        "from": "T49n2034_002:0032a09_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛入涅槃來至此六百一十五年莾廢孺子諡為幼帝　丁卯二戊辰始初元新王己巳莽即真號建國元　庚午二　辛未三　壬申四　癸酉五甲戌天鳳元　乙亥二　丙子三　丁丑四　戊寅五　己卯六　庚辰地皇元　辛巳二　壬午三癸未更始元劉玄字聖公。",
        "from": "T49n2034_002:0032a14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅莽治二年。",
        "from": "T49n2034_002:0032a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為赤眉所殺。",
        "from": "T49n2034_002:0032a14_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玄景帝六世孫。",
        "from": "T49n2034_002:0032a14_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛入涅槃至此六百三十二年　甲申二後漢　乙酉光武帝秀字文叔。",
        "from": "T49n2034_002:0032a15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高帝九世孫。",
        "from": "T49n2034_002:0032a15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立三十三年。",
        "from": "T49n2034_002:0032a15_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徙都雒陽。",
        "from": "T49n2034_002:0032a15_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號建武元。",
        "from": "T49n2034_002:0032a15_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛入涅槃來至此六百三十四年　丙戌二　丁亥三　戊子四　己丑五　庚寅六　辛卯七　壬辰八　癸巳九　甲午十　乙未十一　丙申十二　丁酉十三　戊戌十四　己亥十五　庚子十六　辛丑十七　壬寅十八　癸卯十九　甲辰二十　乙巳二十一西域莎車王等十六國。",
        "from": "T49n2034_002:0032a19_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉遣子來入侍奉獻方物　丙午二十二　丁未二十三　戊申二十四　己酉二十五　庚戌二十六　辛亥二十七　壬子二十八　癸丑二十九　甲寅三十　乙卯三十一　丙辰中元元二月帝東巡太山。",
        "from": "T49n2034_002:0032a21_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "柴祭刻石。",
        "from": "T49n2034_002:0032a21_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有雲氣成宮闕封禪梁甫訖迴。",
        "from": "T49n2034_002:0032a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過魯祠孔子宅。",
        "from": "T49n2034_002:0032a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九月醴泉出。",
        "from": "T49n2034_002:0032a22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "京師飲者痼疾皆差也　丁巳二是歲取天下民戶凡四百二十七萬九千六百四十四民口二千一百萬七千八百二十戊午明帝莊光武第四子立十八年號永平元。",
        "from": "T49n2034_002:0032a23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛入涅槃至此已六百六十七年　己未二　庚申三　辛酉四　壬戌五　癸亥六　甲子七是年帝夢金人飛來殿庭。",
        "from": "T49n2034_002:0032a24_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即佛像經法應也。",
        "from": "T49n2034_002:0032a24_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命使西行尋求佛經　乙丑八　丙寅九　丁卯十使還。",
        "from": "T49n2034_002:0033a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得迦葉摩騰來到雒陽即翻四十二章經。",
        "from": "T49n2034_002:0033a01_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以白馬馱經來。",
        "from": "T49n2034_002:0033a01_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即起白馬寺　戊辰十一竺法蘭出佛本行經五卷。",
        "from": "T49n2034_002:0033a01_55",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陶隱居年歷云。",
        "from": "T49n2034_002:0033a01_62",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝夢金人遣使。",
        "from": "T49n2034_002:0033a01_69",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是此年與諸家小異。",
        "from": "T49n2034_002:0033a01_78",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "據終亦不爽　己巳十二　庚午十三竺法蘭出十地斷結經四卷　辛未十四　壬申十五　癸酉十六　甲戌十七　乙亥十八丙子章帝坦明帝子立十三年號建初元佛入涅槃至此六百八十五年　丁丑二　戊寅三　己卯四永平初通議郎班固作漢書訖。",
        "from": "T49n2034_002:0033a04_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此年始就。",
        "from": "T49n2034_002:0033a04_57",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡二十餘載　庚辰五　辛巳六　壬午七　癸未八　甲申元和元　乙酉二　丙戌三　丁亥章和元詔遣曹褒定禮儀。",
        "from": "T49n2034_002:0033a06_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡一百五十篇　戊子二己丑和帝肇章帝子立十七年號永元元。",
        "from": "T49n2034_002:0033a07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛入涅槃至此六百九十八年　庚寅二　辛卯三　壬辰四　癸巳五　甲午六　乙未七　丙申八　丁酉九　戊戌十　己亥十一　庚子十二　辛丑十三安息國獻師子　壬寅十四　癸卯十五　甲辰十六　乙巳元興元丙午殤帝隆和帝子立一年號延平元。",
        "from": "T49n2034_002:0033a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛入涅槃至此七百一十五年丁未安帝祐章帝孫。",
        "from": "T49n2034_002:0033a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立十九年號永初元。",
        "from": "T49n2034_002:0033a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛入涅槃至此七百一十六年　戊申二　己酉三　庚戌四　辛亥五　壬子六　癸丑七　甲寅元初元九真徼外夜郎等國內附闢境千八百里　乙卯二　丙辰三　丁巳四　戊午五　己未六庚申永寧元四月帝詔謁者僕射劉珍作建武已來名臣傅　辛酉建光元　壬戌延光元　癸亥二　甲子三　乙丑四丙寅順帝保安帝子立十九年。",
        "from": "T49n2034_002:0033a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號永建元。",
        "from": "T49n2034_002:0033a17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛入涅槃至此七百三十五年　丁卯二二月帝詔有司備玄纁聘南陽樊英及黃瓊楊厚等。",
        "from": "T49n2034_002:0033a17_54",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詣公車設壇席問得失訖還以禮遣之　戊辰三　己巳四　庚午五　辛未六　壬申陽嘉元　癸酉二　甲戌三　乙亥四　丙子永和元　丁丑二　戊寅三　己卯四　庚辰五　辛巳六時有張陵在蜀號天師。",
        "from": "T49n2034_002:0033a20_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作道書二十四卷。",
        "from": "T49n2034_002:0033a20_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論章醮之法道士章。",
        "from": "T49n2034_002:0033a20_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "醮起此　壬午漢安元　癸未二　甲申建康元乙酉沖帝炳順帝孫立一年號永嘉元。",
        "from": "T49n2034_002:0033a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "張陵子衡為保師。",
        "from": "T49n2034_002:0033a22_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛入涅槃來至此七百五十四年　丙戌質帝瓚章帝玄孫。",
        "from": "T49n2034_002:0033a22_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立一年號本初元。",
        "from": "T49n2034_002:0033a22_55",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛入涅槃至此七百五十五年丁亥桓帝志章帝曾孫立二十一年。",
        "from": "T49n2034_002:0033a23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號建初元。",
        "from": "T49n2034_002:0033a23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛入涅槃來至此已七百五十六年朱士行漢錄云。",
        "from": "T49n2034_002:0033a23_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "支婁迦讖此年譯阿閦佛經二卷　戊子二高僧傳云。",
        "from": "T49n2034_002:0033a23_62",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安世高從建初二年至靈帝建寧中。",
        "from": "T49n2034_002:0033a23_77",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡二十餘載。",
        "from": "T49n2034_002:0033a23_83",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合譯法句等經一百七十四部一百八十八卷　己丑三　庚寅和平元　辛卯元嘉元朱士行漢錄云世高此年譯五十校計經二卷七處三觀經二卷　壬辰二朱士行漢錄云。",
        "from": "T49n2034_002:0033a24_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世高此年譯普法義經一卷內藏經一卷癸巳永興元　甲午二　乙未永壽元　丙申二朱士行漢錄云。",
        "from": "T49n2034_002:0034a01_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世高此年譯人本欲生經一卷　丁酉三戊戌延嘉元　己亥二　庚子三　辛丑四　壬寅五　癸卯六　甲辰七　乙巳八　丙午九　丁未永康元支敏度錄云。",
        "from": "T49n2034_002:0034a03_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世高此年譯修行道地經七卷或六卷戊申靈帝閎章帝玄孫立二十二年。",
        "from": "T49n2034_002:0034a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號建寧元。",
        "from": "T49n2034_002:0034a04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛入涅槃至此七百七十七年　己酉二　庚戌三世高譯經至此年方訖。",
        "from": "T49n2034_002:0034a04_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非止一處在所即出　辛亥四壬子嘉平元朱士行漢錄云。",
        "from": "T49n2034_002:0034a05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竺佛朔此年於洛陽譯道行經一卷。",
        "from": "T49n2034_002:0034a05_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道安為之注序　癸丑二　甲寅三　乙卯四帝好書自造義皇篇五十章　丙辰五　丁巳六　戊午光和元　己未二支敏度錄云。",
        "from": "T49n2034_002:0034a06_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "支婁迦讖七月八日譯般若道行品等十卷　庚申三聶道真錄云。",
        "from": "T49n2034_002:0034a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "支婁迦讖十月八日於洛陽譯般舟三昧經二卷寶積經一卷　辛酉四都尉安玄譯法鏡經二卷阿含口解一卷。",
        "from": "T49n2034_002:0034a07_54",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於洛陽出。",
        "from": "T49n2034_002:0034a07_59",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚴佛調筆受　壬戌五　癸亥六竺佛朔。",
        "from": "T49n2034_002:0034a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於洛陽譯道行經一卷。",
        "from": "T49n2034_002:0034a08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "支讖傳語孟福張蓮筆受　甲子中平元　乙丑二支曜。",
        "from": "T49n2034_002:0034a08_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於洛陽譯成具光明等十一部經十一卷　丙寅三朱士行漢錄云。",
        "from": "T49n2034_002:0034a08_67",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "支婁迦讖二月八日於洛陽譯首楞嚴經二卷　丁卯四康巨於洛陽譯問地獄事經一卷魏文帝生　戊辰五高僧傳云。",
        "from": "T49n2034_002:0034a09_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "古維摩詰等六部經合十卷。",
        "from": "T49n2034_002:0034a09_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並臨淮嚴佛調於洛陽出之　己巳六正月改光嘉元。",
        "from": "T49n2034_002:0034a09_63",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十月改昭寧元十二月改永隆元庚午獻帝協靈帝子立三十年。",
        "from": "T49n2034_002:0034a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號初平元。",
        "from": "T49n2034_002:0034a10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二月董卓劫帝令遷長安袁紹起兵勃海。",
        "from": "T49n2034_002:0034a10_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孫堅起兵江南。",
        "from": "T49n2034_002:0034a10_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛入涅槃至此七百九十九年　辛未二　壬申三　癸酉四　甲戌興平元竺道祖漢錄云。",
        "from": "T49n2034_002:0034a11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此年康孟詳於洛陽譯四諦經一卷。",
        "from": "T49n2034_002:0034a11_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與世高出者小異　乙亥二　丙子建安元　丁丑二三月沙門竺大力於洛陽譯修行本起經二卷　戊寅三　己卯四高僧傳云。",
        "from": "T49n2034_002:0034a12_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "興起本行經等五部合八卷。",
        "from": "T49n2034_002:0034a12_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並康孟詳出　庚辰五　辛巳六　壬午七　癸未八　甲申九　乙酉十詔遣荀悅撰漢紀　丙戌十一　丁亥十二高僧傳云。",
        "from": "T49n2034_002:0034a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中本起經二卷曇果共孟詳此年出之　戊子十三　己丑十四　庚寅十五　辛卯十六高僧傳云。",
        "from": "T49n2034_002:0034a14_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行本起經二卷竺大力此年出之　壬辰十七　癸巳十八　甲午十九有張陵孫魯更造章。",
        "from": "T49n2034_002:0034a15_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "符自稱師君。",
        "from": "T49n2034_002:0034a15_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "率眾反拜為鎮夷中郎將　乙未二十　丙申二十一　丁酉二十二　戊戌二十三　己亥二十四歷代三寶紀卷第二帝年次前漢新王後漢",
        "from": "T49n2034_002:0034a17_100",
        "context_with_next_sentence": false
    }
]