[
    {
        "source_sentence": "救度佛母二十一種禮讚經",
        "from": "T20n1108B_001:0479c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "納摩阿阿喇鴉答阿喇葉",
        "from": "T20n1108B_001:0479c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮聖大悲藏觀自在菩薩。",
        "from": "T20n1108B_001:0479c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "波答拉勝境　　綠色答嘛字生",
        "from": "T20n1108B_001:0479c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三世佛業母　　頂嚴無量光",
        "from": "T20n1108B_001:0479c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天非天頂髻嚴　　恭敬跪捧兩足蓮",
        "from": "T20n1108B_001:0479c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "度諸窮苦中之母　　致禮救度佛母前",
        "from": "T20n1108B_001:0479c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵敬禮尊聖救度母。",
        "from": "T20n1108B_001:0479c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵敬禮多哩速疾勇　　咄多哩者除怖畏",
        "from": "T20n1108B_001:0479c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咄哩能授諸勝義　　具莎訶字我讚禮",
        "from": "T20n1108B_001:0479c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮救度速勇母　　目如剎那電光照",
        "from": "T20n1108B_001:0480a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三世界尊蓮華面　　從妙華中現端嚴",
        "from": "T20n1108B_001:0480a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮百秋朗月母　　普遍圓照無垢面",
        "from": "T20n1108B_001:0480a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如千星宿俱時聚　　殊勝威光超於彼",
        "from": "T20n1108B_001:0480a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮紫磨金色母　　妙蓮華手勝莊嚴",
        "from": "T20n1108B_001:0480b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施精勤行柔善靜　　忍辱禪定性無境",
        "from": "T20n1108B_001:0480b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮如來頂髻母　　最勝能滿無邊行",
        "from": "T20n1108B_001:0480b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "德到彼岸盡無餘　　勝勢佛子極所愛",
        "from": "T20n1108B_001:0480b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮怛囉吽字母　　聲愛方所滿虛空",
        "from": "T20n1108B_001:0480c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運足遍履七世界　　悉能鉤召攝無餘",
        "from": "T20n1108B_001:0480c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮釋梵火天母　　風神自在眾俱集",
        "from": "T20n1108B_001:0480c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "部多起屍尋香等　　諸藥叉眾作稱歎",
        "from": "T20n1108B_001:0480c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮特囉胝發母　　於他加行極摧壞",
        "from": "T20n1108B_001:0481a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "展左踡右作足踏　　頂髻熾盛極明耀",
        "from": "T20n1108B_001:0481a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮都哩大緊母　　勇猛能摧怨魔類",
        "from": "T20n1108B_001:0481a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於蓮華面作頻眉　　摧壞一切冤家眾",
        "from": "T20n1108B_001:0481a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮三寶嚴印母　　手指當心威嚴相",
        "from": "T20n1108B_001:0481b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚴飾方輪盡無餘　　自身熾盛光聚種",
        "from": "T20n1108B_001:0481b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮威德歡悅母　　寶光珠鬘眾光飾",
        "from": "T20n1108B_001:0481b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最極喜笑覩怛哩　　鎮世間魔作攝伏",
        "from": "T20n1108B_001:0481b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮守護眾地母　　亦能鉤召諸神眾",
        "from": "T20n1108B_001:0481c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "搖顰眉面吽聲字　　一切衰敗令度脫",
        "from": "T20n1108B_001:0481c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮頂冠月相母　　冠中現勝妙嚴光",
        "from": "T20n1108B_001:0481c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿彌陀佛髻中現　　常放眾妙寶光明",
        "from": "T20n1108B_001:0481c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮如盡劫火母　　安住熾盛頂髻中",
        "from": "T20n1108B_001:0482a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普遍喜悅半趺坐　　能摧滅壞惡冤輪",
        "from": "T20n1108B_001:0482a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮手按大地母　　以足踐蹋作鎮壓",
        "from": "T20n1108B_001:0482a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現顰眉面作吽聲　　能破七險鎮降伏",
        "from": "T20n1108B_001:0482a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮安隱柔善母　　涅槃寂滅最樂境",
        "from": "T20n1108B_001:0482b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莎訶命種以相應　　善能消滅大災禍",
        "from": "T20n1108B_001:0482b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮普遍極喜母　　諸怨支體令脫離",
        "from": "T20n1108B_001:0482b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十字呪句妙嚴布　　明呪吽聲常朗耀",
        "from": "T20n1108B_001:0482b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮都哩巴帝母　　足躡相勢吽字種",
        "from": "T20n1108B_001:0482c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彌嚕曼陀結辣薩　　於此三處能搖動",
        "from": "T20n1108B_001:0482c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮薩囉天海母　　手中執住神獸像",
        "from": "T20n1108B_001:0482c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦二怛囉作發聲　　能滅諸毒盡無餘",
        "from": "T20n1108B_001:0482c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮諸天集會母　　天緊那羅所依愛",
        "from": "T20n1108B_001:0483a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "威德歡悅若堅鎧　　滅除鬪諍及惡夢",
        "from": "T20n1108B_001:0483a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮日月廣圓母　　目覩猶勝普光照",
        "from": "T20n1108B_001:0483a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦二喝囉咄怛哩　　善除惡毒瘟熱病",
        "from": "T20n1108B_001:0483a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮具三真實母　　善靜威力皆具足",
        "from": "T20n1108B_001:0483b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥叉執魅尾怛辣　　都哩最極除災禍",
        "from": "T20n1108B_001:0483b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有智者勤精進　　至心誦此二十一",
        "from": "T20n1108B_001:0483b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救度尊處誠信禮　　是故讚嘆根本呪",
        "from": "T20n1108B_001:0483b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每晨早起夕時禮　　憶念施諸勝無畏",
        "from": "T20n1108B_001:0483b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切罪業盡消除　　悉能超越諸惡趣",
        "from": "T20n1108B_001:0483b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此等速能得聰慧　　七俱胝佛所灌頂",
        "from": "T20n1108B_001:0483b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現世富貴壽延安　　當來趣向諸佛位",
        "from": "T20n1108B_001:0483b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有時誤服諸毒物　　或自然生或合成",
        "from": "T20n1108B_001:0483b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憶念聖尊真實力　　諸惡毒藥盡消滅",
        "from": "T20n1108B_001:0483b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或見他人遭鬼魅　　或發熱病受諸苦",
        "from": "T20n1108B_001:0483b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若轉此讚二三七　　彼諸苦惱悉蠲除",
        "from": "T20n1108B_001:0483b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲乞男女得男女　　求財寶位獲富饒",
        "from": "T20n1108B_001:0483b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善能圓滿隨意願　　一切障礙不能侵",
        "from": "T20n1108B_001:0483b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛母尊聖救度母　　及諸十方三世中",
        "from": "T20n1108B_001:0483b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有諸佛并弟子　　我今一心敬讚禮",
        "from": "T20n1108B_001:0483b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "花香燈燭及香水　　妙食妓樂等供養",
        "from": "T20n1108B_001:0483c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實獻即於意所現　　皆願眾聖樂納受",
        "from": "T20n1108B_001:0483c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自從無始直至今　　十不善及五無間",
        "from": "T20n1108B_001:0483c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由心煩惱故所造　　一切罪惡皆懺悔",
        "from": "T20n1108B_001:0483c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聲聞緣覺諸菩薩　　乃至凡夫諸有情",
        "from": "T20n1108B_001:0483c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三世所積諸福善　　我皆隨喜盡無餘",
        "from": "T20n1108B_001:0483c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨諸有情一切愿　　宜其種種差別智",
        "from": "T20n1108B_001:0483c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應其大小通常類　　勸轉一切妙法輪",
        "from": "T20n1108B_001:0483c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從今直至輪迴空　　請常住世不涅槃",
        "from": "T20n1108B_001:0483c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為苦海中諸眾生　　悉願聖母慈眼觀",
        "from": "T20n1108B_001:0483c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今所集諸福善　　皆成無上菩提因",
        "from": "T20n1108B_001:0483c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願不久於眾生　　證得無上引導師",
        "from": "T20n1108B_001:0483c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊聖佛母具大悲　　於我一切諸眾生",
        "from": "T20n1108B_001:0483c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速淨諸障滿資糧　　願令獲得證菩提",
        "from": "T20n1108B_001:0483c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直至無上菩提位　　世世獲得常安樂",
        "from": "T20n1108B_001:0483c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於修菩提一切處　　一切間斷諸魔祟",
        "from": "T20n1108B_001:0483c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疾病一切天壽類　　並諸惡夢不利相",
        "from": "T20n1108B_001:0483c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八怖一切惱害事　　願令急速盡消除",
        "from": "T20n1108B_001:0483c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間及於出世間　　一切善樂於豐足",
        "from": "T20n1108B_001:0483c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增盛一切饒益事　　願令獲得自然成",
        "from": "T20n1108B_001:0483c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精進修行法增盛　　修汝常得見聖顏",
        "from": "T20n1108B_001:0483c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟入空義菩提意　　願令暫如朔月增",
        "from": "T20n1108B_001:0483c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼佛眾會咸清淨　　我時於勝蓮華生",
        "from": "T20n1108B_001:0483c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "親睹如來無量光　　現前受我菩提記",
        "from": "T20n1108B_001:0483c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我從住世所修佛　　三世一切佛業母",
        "from": "T20n1108B_001:0483c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一面二臂身綠色　　願執優婆花吉祥",
        "from": "T20n1108B_001:0483c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛母體微妙云何　　及於眷屬共壽量",
        "from": "T20n1108B_001:0483c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "境界及於號云何　　願我等皆亦復然",
        "from": "T20n1108B_001:0483c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讚祝爾尊微善力　　我等隨方所在處",
        "from": "T20n1108B_001:0483c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "病魔貧爭盡消除　　善祥增長祈皆賜",
        "from": "T20n1108B_001:0484a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮供世尊母　　我以所積善",
        "from": "T20n1108B_001:0484a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世界諸眾生　　皆生極樂界",
        "from": "T20n1108B_001:0484a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "棄身過莊嚴相好　　棄語過獲頻伽音",
        "from": "T20n1108B_001:0484a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "棄意過證一切智　　成勝吉祥功德瑞",
        "from": "T20n1108B_001:0484a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖救度佛母二十一種禮讚經竟",
        "from": "T20n1108B_001:0484a06_100",
        "context_with_next_sentence": false
    }
]