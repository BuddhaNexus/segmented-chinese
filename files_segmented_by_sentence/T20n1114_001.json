[
    {
        "source_sentence": "毘俱胝菩薩一百八名經",
        "from": "T20n1114_001:0501a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試鴻臚少卿",
        "from": "T20n1114_001:0501a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳教大師臣法天奉　詔譯",
        "from": "T20n1114_001:0501a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命一切如來應供遍知覺。",
        "from": "T20n1114_001:0501a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今說此一切如來心真言。",
        "from": "T20n1114_001:0501a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有天人持明仙眾。",
        "from": "T20n1114_001:0501a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命供養一切諸佛。",
        "from": "T20n1114_001:0501a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持讀誦及讚說真言通達法相。",
        "from": "T20n1114_001:0501a22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若稱唵字是圓滿義。",
        "from": "T20n1114_001:0501a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若稱曩字是離怖畏義。",
        "from": "T20n1114_001:0501a23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名破魔義。",
        "from": "T20n1114_001:0501a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若稱野字是破繫縛義。",
        "from": "T20n1114_001:0501a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若稱娑字是降伏冤家障礙義。",
        "from": "T20n1114_001:0501a25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若稱儞字是破壞冤敵義。",
        "from": "T20n1114_001:0501a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若怖畏者以真言力遠離怖畏。",
        "from": "T20n1114_001:0501a26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說陀羅尼曰。",
        "from": "T20n1114_001:0501a27_3",
        "context_with_next_sentence": true
    },
    {
