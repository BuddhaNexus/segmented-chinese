        "from": "T20n1114_001:0502a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有一心受持讀誦。",
        "from": "T20n1114_001:0502a15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若自書寫若為人解說。",
        "from": "T20n1114_001:0502a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增壽吉祥端正福相眾人愛敬。",
        "from": "T20n1114_001:0502a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離魔境出生死難。",
        "from": "T20n1114_001:0502a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而彼天人阿修羅恭敬供養。",
        "from": "T20n1114_001:0502a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復得天龍藥叉乾闥婆阿修羅迦樓羅緊那羅誐嚕拏等。",
        "from": "T20n1114_001:0502a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持明仙眾尊重讚嘆。",
        "from": "T20n1114_001:0502a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有枷鎖禁繫自然解脫。",
        "from": "T20n1114_001:0502a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師子虎狼諸惡餓鬼。",
        "from": "T20n1114_001:0502a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冤家盜賊不能逼害。",
        "from": "T20n1114_001:0502a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "暴風猛火雲雷雨雹。",
        "from": "T20n1114_001:0502a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "河海泛涱毒藥重病不能侵損。",
        "from": "T20n1114_001:0502a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有念此毘俱胝名者。",
        "from": "T20n1114_001:0502a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩恒時救護。",
        "from": "T20n1114_001:0502a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有諸惡魔冤而來逼惱。",
        "from": "T20n1114_001:0502a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時菩薩密放身光。",
        "from": "T20n1114_001:0502a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍照虛空如百千日。",
        "from": "T20n1114_001:0502a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其光熾盛爍殄魔冤。",
        "from": "T20n1114_001:0502a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至天地悉皆清淨。",
        "from": "T20n1114_001:0502a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復令彼人智慧增長。",
        "from": "T20n1114_001:0502a26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於七生中得宿命通。",
        "from": "T20n1114_001:0502a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生剎帝利族受國王身。",
        "from": "T20n1114_001:0502a27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從此命終往生西方極樂世界。",
        "from": "T20n1114_001:0502a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘俱胝菩薩一百八名經",
        "from": "T20n1114_001:0502a29_100",
        "context_with_next_sentence": false
    }
]