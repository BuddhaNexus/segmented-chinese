[
    {
        "source_sentence": "聖歡喜天式法",
        "from": "T21n1275_001:0324a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏法師般若惹羯羅撰",
        "from": "T21n1275_001:0324a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於聖天法大欲開靈驗者。先造立天地盤。",
        "from": "T21n1275_001:0324a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則天盤長三寸又四寸。",
        "from": "T21n1275_001:0324a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地盤長方七寸。",
        "from": "T21n1275_001:0324a07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以有乳木及香木。",
        "from": "T21n1275_001:0324a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日之內造之。",
        "from": "T21n1275_001:0324a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天地共以白木作之。",
        "from": "T21n1275_001:0324a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造畫時不可見下劣之人。",
        "from": "T21n1275_001:0324a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必必精進潔齋。",
        "from": "T21n1275_001:0324a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以吉日為之。",
        "from": "T21n1275_001:0324a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依通法擇日。",
        "from": "T21n1275_001:0324a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合耳一頭天盤圓作。",
        "from": "T21n1275_001:0324a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀東方安葉上唵字。",
        "from": "T21n1275_001:0324a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即變成日王歡喜天。",
        "from": "T21n1275_001:0324a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭上在日輪於俗形而有瞋相。",
        "from": "T21n1275_001:0324a12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面容赤色。",
        "from": "T21n1275_001:0324a12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持吠瑠璃收胸上。",
        "from": "T21n1275_001:0324a13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持獨股收左腰。",
        "from": "T21n1275_001:0324a13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摧壞部主名無憂大將。",
        "from": "T21n1275_001:0324a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被七阿僧祇眷屬圍繞。",
        "from": "T21n1275_001:0324a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居青石上。",
        "from": "T21n1275_001:0324a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南方葉上觀爾字。",
        "from": "T21n1275_001:0324a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即變為愛王歡喜天。",
        "from": "T21n1275_001:0324a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "似法師形。",
        "from": "T21n1275_001:0324a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及其鼻胸下如面目󱄎。",
        "from": "T21n1275_001:0324a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持團餅安結跏上。",
        "from": "T21n1275_001:0324a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持蘿富根舉左肩間。",
        "from": "T21n1275_001:0324a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "野干部主名象頭大將。",
        "from": "T21n1275_001:0324a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十八俱胝被眷屬圍繞。",
        "from": "T21n1275_001:0324a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居赤石上。",
        "from": "T21n1275_001:0324a19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方葉上觀頡哩字。",
        "from": "T21n1275_001:0324a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即變為月愛歡喜天。",
        "from": "T21n1275_001:0324a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其形美女也。",
        "from": "T21n1275_001:0324a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭有花鬘。",
        "from": "T21n1275_001:0324a21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持寶鏡。",
        "from": "T21n1275_001:0324a21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手支頰。",
        "from": "T21n1275_001:0324a21_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以寶篋為□足。",
        "from": "T21n1275_001:0324a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端正殊妙色。",
        "from": "T21n1275_001:0324a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一牙部主名嚴髻大將。",
        "from": "T21n1275_001:0324a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一百四千俱胝眷屬被圍繞。",
        "from": "T21n1275_001:0324a23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "座虎皮上。",
        "from": "T21n1275_001:0324a23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方葉上觀忙字。",
        "from": "T21n1275_001:0324a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即變議特歡喜天。",
        "from": "T21n1275_001:0324a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其形青色如憍尸迦天。",
        "from": "T21n1275_001:0324a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左右手各作刀印。",
        "from": "T21n1275_001:0324a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "收各方腰。",
        "from": "T21n1275_001:0324a25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍象部主名頂行大將。",
        "from": "T21n1275_001:0324a26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一俱胝那臾多眷屬圍繞踞黑石。",
        "from": "T21n1275_001:0324a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次明地盤眾。",
        "from": "T21n1275_001:0324a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東方安帝釋天。",
        "from": "T21n1275_001:0324a28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東南方安火天。",
        "from": "T21n1275_001:0324a28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南方安炎魔天。",
        "from": "T21n1275_001:0324a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西南方安羅剎天。",
        "from": "T21n1275_001:0324a29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方安水天。",
        "from": "T21n1275_001:0324b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西北方安風天。",
        "from": "T21n1275_001:0324b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方安毘沙門天。",
        "from": "T21n1275_001:0324b01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東北方安大自在天。",
        "from": "T21n1275_001:0324b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安廿八宿卅六禽。",
        "from": "T21n1275_001:0324b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是次第建立之矣。",
        "from": "T21n1275_001:0324b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天盤四天皆一體也。",
        "from": "T21n1275_001:0324b03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非異體者。",
        "from": "T21n1275_001:0324b03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若中□亦安多波天無妨礙。",
        "from": "T21n1275_001:0324b04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次召請印　以二小指二無名指相鉤向內。",
        "from": "T21n1275_001:0324b05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二中指立相叉。",
        "from": "T21n1275_001:0324b06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以二頭指各竪付中指。",
        "from": "T21n1275_001:0324b06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大指亦竪付近頭指側。",
        "from": "T21n1275_001:0324b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指去來。",
        "from": "T21n1275_001:0324b07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明曰四天通用。",
        "from": "T21n1275_001:0324b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵簸迦羅二合主拏禰去縛哆野二合次用普印伴呪曰。",
        "from": "T21n1275_001:0324b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵簸迦羅二合三萬眷屬從恒河沙守護自他祈祈決定急急如律令次地盤眾等召請印。",
        "from": "T21n1275_001:0324b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作帝殊羅施印。",
        "from": "T21n1275_001:0324b13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二小指二無名指叉於掌內竪。",
        "from": "T21n1275_001:0324b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指頭相捻。",
        "from": "T21n1275_001:0324b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二頭指各加中指背節下半分。",
        "from": "T21n1275_001:0324b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指來去。",
        "from": "T21n1275_001:0324b15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1275_001:0324b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一鑠都嚕二波羅摩馱儞曳三莎訶通八方天次二十八宿三十六禽召請印用輪壇印通呪曰。",
        "from": "T21n1275_001:0324b18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵知里安疑嚕儞曳莎訶次請一切諸天類等印呪。",
        "from": "T21n1275_001:0324b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛合掌立二空去來。",
        "from": "T21n1275_001:0324b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1275_001:0324b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "富順愛神相合部類宴坐引入伴族一億二億一切諸頭頭契來入入入娑婆呵次次供養契印。",
        "from": "T21n1275_001:0324b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉求他之中云云。",
        "from": "T21n1275_001:0324b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盛冥文符裏某甲耳祕攝符固對表畫斷也右欲望官位爵祿者。",
        "from": "T21n1275_001:0324c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以日輪天加帝釋天。",
        "from": "T21n1275_001:0324c01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲令他熱病者。",
        "from": "T21n1275_001:0324c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以日輪天加火天王。",
        "from": "T21n1275_001:0324c02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲得福德者。",
        "from": "T21n1275_001:0324c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以月愛天加毘沙門天祈之。",
        "from": "T21n1275_001:0324c03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲得他愛念者。",
        "from": "T21n1275_001:0324c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以月愛天加毘沙門天使者。",
        "from": "T21n1275_001:0324c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呼二十八宿。",
        "from": "T21n1275_001:0324c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲勝兵軍者。",
        "from": "T21n1275_001:0324c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以日輪天加大自在天。",
        "from": "T21n1275_001:0324c06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呼一萬眷屬。",
        "from": "T21n1275_001:0324c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲得人心遂吾思者。",
        "from": "T21n1275_001:0324c08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以議特天加炎魔天。",
        "from": "T21n1275_001:0324c08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一切人欲被愛敬者。",
        "from": "T21n1275_001:0324c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以愛王天加毘沙門天。",
        "from": "T21n1275_001:0324c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲得他人財寶者。",
        "from": "T21n1275_001:0324c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以月愛天加大自在天。",
        "from": "T21n1275_001:0324c11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若夫妻之中欲令愛念者。",
        "from": "T21n1275_001:0324c12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以月愛天加帝釋天。",
        "from": "T21n1275_001:0324c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若夫妻之中欲令相離者。",
        "from": "T21n1275_001:0324c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以議特天加水天王。",
        "from": "T21n1275_001:0324c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲忽死敵惡人怨敵者。",
        "from": "T21n1275_001:0324c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以愛王天加羅剎天。",
        "from": "T21n1275_001:0324c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若從者逃走欲令還來者。",
        "from": "T21n1275_001:0324c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以愛王天加大自在天。",
        "from": "T21n1275_001:0324c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呼其從者姓名。",
        "from": "T21n1275_001:0324c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若懷妊之女早安欲令產生者。",
        "from": "T21n1275_001:0324c20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以月愛天加炎魔天好矣。",
        "from": "T21n1275_001:0324c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若被呪咀時。",
        "from": "T21n1275_001:0324c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲被還者。",
        "from": "T21n1275_001:0324c22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以日輪天加羅剎天祈申之。",
        "from": "T21n1275_001:0324c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲令他人風病者。",
        "from": "T21n1275_001:0324c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以議特天加風天祈之。",
        "from": "T21n1275_001:0324c24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲止病瘡癰疸一切瘡病。",
        "from": "T21n1275_001:0324c25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以愛王天加炎魔天願祈之。",
        "from": "T21n1275_001:0324c26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲止長血赤痢者。",
        "from": "T21n1275_001:0324c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以日輪天加帝釋天祈申。",
        "from": "T21n1275_001:0324c27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲止雨降難者。",
        "from": "T21n1275_001:0324c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以愛王天加帝釋天。",
        "from": "T21n1275_001:0324c28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲止旱魃降雨者。",
        "from": "T21n1275_001:0324c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以議特天加水天王。",
        "from": "T21n1275_001:0324c29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲止腹病頭病者。",
        "from": "T21n1275_001:0325a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以日輪天加炎魔天。",
        "from": "T21n1275_001:0325a01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲遂論議決𬿥者。",
        "from": "T21n1275_001:0325a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以日輪天加大自在。",
        "from": "T21n1275_001:0325a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呼二十八宿等。",
        "from": "T21n1275_001:0325a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲付公私遂所聖事者。",
        "from": "T21n1275_001:0325a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以愛王天加帝釋天。",
        "from": "T21n1275_001:0325a04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲得愜年穀者。",
        "from": "T21n1275_001:0325a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以議特天加帝釋天。",
        "from": "T21n1275_001:0325a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲家內靜者。",
        "from": "T21n1275_001:0325a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以愛王天加毘沙門天。",
        "from": "T21n1275_001:0325a06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲止口舌事者。",
        "from": "T21n1275_001:0325a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以愛王天加自在天。",
        "from": "T21n1275_001:0325a07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲止物怪惡夢者。",
        "from": "T21n1275_001:0325a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以日輪天加炎魔天。",
        "from": "T21n1275_001:0325a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲使者遣遠早返來者。",
        "from": "T21n1275_001:0325a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以議特天加風天忽返來。",
        "from": "T21n1275_001:0325a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是一一合法對祈申。",
        "from": "T21n1275_001:0325a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所念之事必成就。",
        "from": "T21n1275_001:0325a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對時誦根本呪印云云。",
        "from": "T21n1275_001:0325a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結三部護身印明依諸通印。",
        "from": "T21n1275_001:0325a13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "都合攝鉤印。",
        "from": "T21n1275_001:0325a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內縛置頂上散。",
        "from": "T21n1275_001:0325a14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1275_001:0325a14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩羅薩羅天地都合禁禁吽吽跘次結對印不動謁哦印。",
        "from": "T21n1275_001:0325a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1275_001:0325a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吽吽吉儞吉儞吽泮半音娑婆呵次十方結界印　地結方牆虛空結印等真言亦爾云云。",
        "from": "T21n1275_001:0325a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以糸對之。",
        "from": "T21n1275_001:0325a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1275_001:0325a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵乞曳曳曳吽件法天法國於菩提寺所傳也。",
        "from": "T21n1275_001:0325a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其調度不多類。",
        "from": "T21n1275_001:0325a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時人皆號驗法焉。",
        "from": "T21n1275_001:0325a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　　　　　　一交了　興然之本",
        "from": "T21n1275_001:0325b24_0",
        "context_with_next_sentence": true
    }
]