[
    {
        "source_sentence": "難儞計濕嚩囉天說支輪經",
        "from": "T21n1312_001:0463a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試光祿卿",
        "from": "T21n1312_001:0463a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明教大師臣法賢奉　詔譯",
        "from": "T21n1312_001:0463a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時難儞計濕嚩囉天。",
        "from": "T21n1312_001:0463a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普觀世間一切男女。",
        "from": "T21n1312_001:0463a07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因於生時值諸宮宿所感善惡。",
        "from": "T21n1312_001:0463a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至壽命盡於其中間。",
        "from": "T21n1312_001:0463a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或貴或賤或富或貧。",
        "from": "T21n1312_001:0463a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有孝有忠好文好武。",
        "from": "T21n1312_001:0463a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至於端嚴醜陋愚鈍聰慧。",
        "from": "T21n1312_001:0463a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疾病夭枉鱞寡孤惸。",
        "from": "T21n1312_001:0463a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猛惡剛強溫柔恭順。",
        "from": "T21n1312_001:0463a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言正行直識愧知慚。",
        "from": "T21n1312_001:0463a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曉達天文明察世事。",
        "from": "T21n1312_001:0463a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或重佛法樂修善事。",
        "from": "T21n1312_001:0463a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或慕妖魔信邪造惡。",
        "from": "T21n1312_001:0463a13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又或好遊山水遂性全真。",
        "from": "T21n1312_001:0463a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又或樂作商估貿易寶貨。",
        "from": "T21n1312_001:0463a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或好布施或即慳貪。",
        "from": "T21n1312_001:0463a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並乃表於相狀顯著身胑。",
        "from": "T21n1312_001:0463a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為幢為螺作文作黶。",
        "from": "T21n1312_001:0463a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切美惡並稟一身。",
        "from": "T21n1312_001:0463a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦乃宿有因緣今值星曜。",
        "from": "T21n1312_001:0463a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恐其未曉略為宣揚。",
        "from": "T21n1312_001:0463a17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次天羊宮當火曜。",
        "from": "T21n1312_001:0463a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直在婁宿胃宿全分昴宿一分。",
        "from": "T21n1312_001:0463a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其日生者。",
        "from": "T21n1312_001:0463a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相貌端嚴身胑細妙。",
        "from": "T21n1312_001:0463a19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孝於父母敬事尊長。",
        "from": "T21n1312_001:0463a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多眷屬饒僕從具。",
        "from": "T21n1312_001:0463a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勇猛有精神有大福德。",
        "from": "T21n1312_001:0463a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不奈飢寒。",
        "from": "T21n1312_001:0463a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樂於法知慚愧。",
        "from": "T21n1312_001:0463a21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "好布施心堅固。",
        "from": "T21n1312_001:0463a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言行真實見解明了。",
        "from": "T21n1312_001:0463a22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合得君王愛重眾人欽仰。",
        "from": "T21n1312_001:0463a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼儍貪色。",
        "from": "T21n1312_001:0463a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多好遊歷一生多饒。",
        "from": "T21n1312_001:0463a23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "齒疾及有癀病。",
        "from": "T21n1312_001:0463a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽或中夭於生後第四年合有火難。",
        "from": "T21n1312_001:0463a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至年十八亦合中夭。",
        "from": "T21n1312_001:0463a25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至年二十五合有虎狼之難。",
        "from": "T21n1312_001:0463a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦主海中死當夭難時。",
        "from": "T21n1312_001:0463a26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遇吉曜臨照即延壽命至一百歲於八月內值火曜及昴星。",
        "from": "T21n1312_001:0463a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直日夜半時命終。",
        "from": "T21n1312_001:0463a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次金牛宮當金曜。",
        "from": "T21n1312_001:0463a28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直在昴宿三分畢宿參宿各二分。",
        "from": "T21n1312_001:0463a29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其日男女生者。",
        "from": "T21n1312_001:0463b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相貌端嚴身分長細。",
        "from": "T21n1312_001:0463b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面有星黶合主大富常受快樂。",
        "from": "T21n1312_001:0463b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多饒眷屬亦多奴僕。",
        "from": "T21n1312_001:0463b02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "氣性剛強不宜妻妾。",
        "from": "T21n1312_001:0463b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少小辛苦。",
        "from": "T21n1312_001:0463b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晚年之後復得父母資財即受快樂。",
        "from": "T21n1312_001:0463b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有精神善工巧見識明了。",
        "from": "T21n1312_001:0463b04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所作堅固好樂布施。",
        "from": "T21n1312_001:0463b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貪愛食味饒患喉病。",
        "from": "T21n1312_001:0463b05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至二十五歲命合中夭。",
        "from": "T21n1312_001:0463b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於是時復得吉曜臨照。",
        "from": "T21n1312_001:0463b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變惡為善得延壽命至於百歲。",
        "from": "T21n1312_001:0463b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後患風狂病。",
        "from": "T21n1312_001:0463b07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於九月中當水曜及畢宿。",
        "from": "T21n1312_001:0463b08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直日終於聖跡之地。",
        "from": "T21n1312_001:0463b08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次陰陽宮當水曜。",
        "from": "T21n1312_001:0463b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直在參宿二分嘴宿井宿各三分。",
        "from": "T21n1312_001:0463b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其日男女生者。",
        "from": "T21n1312_001:0463b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稟性柔善身相端正。",
        "from": "T21n1312_001:0463b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所言真實談對辯捷。",
        "from": "T21n1312_001:0463b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其交友分義長遠。",
        "from": "T21n1312_001:0463b11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偏好香華貪愛食味。",
        "from": "T21n1312_001:0463b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稟性滑稽厚於女色。",
        "from": "T21n1312_001:0463b12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目覩美麗神魂如失。",
        "from": "T21n1312_001:0463b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸幻術見者愛樂。",
        "from": "T21n1312_001:0463b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於成就法修習必得。",
        "from": "T21n1312_001:0463b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若學禁龍及呪水法。",
        "from": "T21n1312_001:0463b14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此二法最得精妙。",
        "from": "T21n1312_001:0463b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少小辛苦至中年後方有名譽見者愛重。",
        "from": "T21n1312_001:0463b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初生之後至第五年有中夭難。",
        "from": "T21n1312_001:0463b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於夭難時。",
        "from": "T21n1312_001:0463b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遇吉曜臨照變惡為善。",
        "from": "T21n1312_001:0463b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得延壽命至八十歲。",
        "from": "T21n1312_001:0463b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於十月內遇水曜及參宿。",
        "from": "T21n1312_001:0463b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直日於日中時因疾命終。",
        "from": "T21n1312_001:0463b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次於巨蟹宮當太陰。",
        "from": "T21n1312_001:0463b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直在井宿鬼宿柳宿全分。",
        "from": "T21n1312_001:0463b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其日男女生者。",
        "from": "T21n1312_001:0463b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身分長細稟性柔善。",
        "from": "T21n1312_001:0463b21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有智慧能忍事。",
        "from": "T21n1312_001:0463b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孝順父母最得父母憐愛。",
        "from": "T21n1312_001:0463b22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於手中足下合有蓮華紋螺紋輪紋及吉祥果紋相。",
        "from": "T21n1312_001:0463b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多有妻妾眷屬又多奴僕。",
        "from": "T21n1312_001:0463b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少年微賤晚歲豐富。",
        "from": "T21n1312_001:0463b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樂法勇猛好於布施。",
        "from": "T21n1312_001:0463b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養賢聖及諸師長。",
        "from": "T21n1312_001:0463b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性好遠遊。",
        "from": "T21n1312_001:0463b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初生後至二十歲合於樹上落下。",
        "from": "T21n1312_001:0463b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因此傷夭。",
        "from": "T21n1312_001:0463b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當此之時若遇吉曜臨照。",
        "from": "T21n1312_001:0463b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變惡為善得延壽命至八十歲。",
        "from": "T21n1312_001:0463b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於十一月當參宿及金曜。",
        "from": "T21n1312_001:0463b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直日夜半時命終。",
        "from": "T21n1312_001:0463b29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次於師子宮當太陽。",
        "from": "T21n1312_001:0463c01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直在星宿張宿翼宿各一分。",
        "from": "T21n1312_001:0463c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此日男女生者。",
        "from": "T21n1312_001:0463c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為性急躁舉措多瞋。",
        "from": "T21n1312_001:0463c02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生二三子每所憐愛過於己命。",
        "from": "T21n1312_001:0463c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖豐財寶心常知足。",
        "from": "T21n1312_001:0463c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "好樂供養一切賢聖。",
        "from": "T21n1312_001:0463c04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為人不拘散誕自足。",
        "from": "T21n1312_001:0463c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偏好食肉亦好食魚。",
        "from": "T21n1312_001:0463c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或往他國亦不受飢寒。",
        "from": "T21n1312_001:0463c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒怖疾病不懼大水。",
        "from": "T21n1312_001:0463c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年至十五有中夭難若遇吉曜臨照。",
        "from": "T21n1312_001:0463c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變惡為善得延壽命至於百歲。",
        "from": "T21n1312_001:0463c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於十二月內當土曜及星宿。",
        "from": "T21n1312_001:0463c08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直日於殑伽河北岸而趣命終。",
        "from": "T21n1312_001:0463c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次雙女宮當水曜。",
        "from": "T21n1312_001:0463c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直在翼宿三分軫宿角宿各二分。",
        "from": "T21n1312_001:0463c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其日男女生者。",
        "from": "T21n1312_001:0463c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猴面廣目肋下有黶。",
        "from": "T21n1312_001:0463c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人見敬愛。",
        "from": "T21n1312_001:0463c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樂法愛香華。",
        "from": "T21n1312_001:0463c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年少微賤晚歲豐財。",
        "from": "T21n1312_001:0463c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然多疾病偏饒患目。",
        "from": "T21n1312_001:0463c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "好在他國不宜妻妾。",
        "from": "T21n1312_001:0463c13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每覩美麗神魂失次。",
        "from": "T21n1312_001:0463c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年至三十有中夭難。",
        "from": "T21n1312_001:0463c14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當夭難時遇吉曜臨照。",
        "from": "T21n1312_001:0463c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變惡為善得延壽命至八十歲。",
        "from": "T21n1312_001:0463c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於正月內因患瘡命終。",
        "from": "T21n1312_001:0463c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次天秤宮當金曜。",
        "from": "T21n1312_001:0463c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直在角宿二分亢宿氐宿各三分。",
        "from": "T21n1312_001:0463c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其日男女生者。",
        "from": "T21n1312_001:0463c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為人孝順福祿自如。",
        "from": "T21n1312_001:0463c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稟性急躁多有瞋怒。",
        "from": "T21n1312_001:0463c19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然識見明達審察微細。",
        "from": "T21n1312_001:0463c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有氣義又能剛忍。",
        "from": "T21n1312_001:0463c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長於親友滑稽好色。",
        "from": "T21n1312_001:0463c20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多遊外國。",
        "from": "T21n1312_001:0463c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於非次得快樂事。",
        "from": "T21n1312_001:0463c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合主手中有幢蓋紋相。",
        "from": "T21n1312_001:0463c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至二十二歲有中夭難。",
        "from": "T21n1312_001:0463c22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於夭難時若遇吉曜臨照。",
        "from": "T21n1312_001:0463c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變惡為善得延壽命至九十七。",
        "from": "T21n1312_001:0463c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於二月內值火曜及房宿直日命終。",
        "from": "T21n1312_001:0463c24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次天蠍宮當火曜。",
        "from": "T21n1312_001:0463c25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直在氐宿及房宿心宿各一分。",
        "from": "T21n1312_001:0463c26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其日男女生者。",
        "from": "T21n1312_001:0463c26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合主利根所學易成。",
        "from": "T21n1312_001:0463c26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為人氣義雖豐財寶或聚或散。",
        "from": "T21n1312_001:0463c27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至年十八有蛇蠍虎狼之難。",
        "from": "T21n1312_001:0463c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至年二十再有中夭之難。",
        "from": "T21n1312_001:0463c28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當夭難時若遇吉曜臨照。",
        "from": "T21n1312_001:0463c29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變惡為善得延壽命至七十七歲。",
        "from": "T21n1312_001:0464a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於三月內值火曜及房宿直日命終。",
        "from": "T21n1312_001:0464a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次人馬宮當木曜。",
        "from": "T21n1312_001:0464a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直在尾宿箕宿斗宿各一分。",
        "from": "T21n1312_001:0464a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其日男女生者。",
        "from": "T21n1312_001:0464a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合主身相端嚴兼有福黶。",
        "from": "T21n1312_001:0464a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稟性柔善持戒堅固。",
        "from": "T21n1312_001:0464a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡所言說悉務真實。",
        "from": "T21n1312_001:0464a05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "足智慧好論義。",
        "from": "T21n1312_001:0464a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一生之中。",
        "from": "T21n1312_001:0464a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常處大富。",
        "from": "T21n1312_001:0464a06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於年少時又得伏藏。",
        "from": "T21n1312_001:0464a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至十八歲有中夭難。",
        "from": "T21n1312_001:0464a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於夭難時若得吉曜臨照。",
        "from": "T21n1312_001:0464a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變惡為善即延壽命至七十七歲。",
        "from": "T21n1312_001:0464a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於四月內值金曜及牛宿。",
        "from": "T21n1312_001:0464a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直日因斷食投水而終。",
        "from": "T21n1312_001:0464a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次摩竭宮當土曜。",
        "from": "T21n1312_001:0464a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直在斗宿三分牛宿女宿各二分。",
        "from": "T21n1312_001:0464a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其日男女生者。",
        "from": "T21n1312_001:0464a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身相端直隱處有黶。",
        "from": "T21n1312_001:0464a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為性惼急常多瞋怒目相姦儍。",
        "from": "T21n1312_001:0464a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然有氣義智慧不群。",
        "from": "T21n1312_001:0464a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "全忠孝多朋友。",
        "from": "T21n1312_001:0464a14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心勇猛樂鬪戰。",
        "from": "T21n1312_001:0464a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛歌舞好技術。",
        "from": "T21n1312_001:0464a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復愛香華多有妻妾。",
        "from": "T21n1312_001:0464a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年至三十有中夭難。",
        "from": "T21n1312_001:0464a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當夭難時遇吉曜臨照。",
        "from": "T21n1312_001:0464a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變惡為善得延壽命至七十五。",
        "from": "T21n1312_001:0464a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於五月內值土曜及危宿。",
        "from": "T21n1312_001:0464a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直日命終。",
        "from": "T21n1312_001:0464a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次寶瓶宮當土曜。",
        "from": "T21n1312_001:0464a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直在女宿二分危宿室宿各三分。",
        "from": "T21n1312_001:0464a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其日男女生者。",
        "from": "T21n1312_001:0464a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合主大富常受快樂。",
        "from": "T21n1312_001:0464a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多有眷屬亦多奴僕。",
        "from": "T21n1312_001:0464a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而於父母志切孝養。",
        "from": "T21n1312_001:0464a21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖有妻室不守財物。",
        "from": "T21n1312_001:0464a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所生之子有夭歿者。",
        "from": "T21n1312_001:0464a22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼目姦儍然有氣義。",
        "from": "T21n1312_001:0464a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡事明察所為長久。",
        "from": "T21n1312_001:0464a23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性不慳悋唯好布施。",
        "from": "T21n1312_001:0464a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所聚財物亦有破散。",
        "from": "T21n1312_001:0464a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生怖虎狼。",
        "from": "T21n1312_001:0464a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年至十八有虎狼之難。",
        "from": "T21n1312_001:0464a25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於有難之際若遇吉曜臨照。",
        "from": "T21n1312_001:0464a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變惡為善得延壽命至八十四。",
        "from": "T21n1312_001:0464a26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於六月內值太陽及奎宿。",
        "from": "T21n1312_001:0464a27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直日因患瘡而終。",
        "from": "T21n1312_001:0464a27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次雙魚宮當木曜。",
        "from": "T21n1312_001:0464a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直在室宿壁宿奎宿各一分。",
        "from": "T21n1312_001:0464a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其日男女生者。",
        "from": "T21n1312_001:0464a29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身分端正眼目姦儍。",
        "from": "T21n1312_001:0464a29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多有妻妾眷屬復多僮僕。",
        "from": "T21n1312_001:0464b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稟性急躁常多瞋怒。",
        "from": "T21n1312_001:0464b01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有筋力貪肉食。",
        "from": "T21n1312_001:0464b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復好食魚。",
        "from": "T21n1312_001:0464b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然好布施及供養賢聖。",
        "from": "T21n1312_001:0464b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛香華好技術。",
        "from": "T21n1312_001:0464b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貪著美色亦為女妬。",
        "from": "T21n1312_001:0464b03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言多真實亦合謗師。",
        "from": "T21n1312_001:0464b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "主患耳疾亦主長壽。",
        "from": "T21n1312_001:0464b04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年至十八有上樹上牆墮落之難。",
        "from": "T21n1312_001:0464b05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遇吉曜臨照變惡為善。",
        "from": "T21n1312_001:0464b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得延壽命至九十五於七月內值土曜及壁宿。",
        "from": "T21n1312_001:0464b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直日因患瘡而終。",
        "from": "T21n1312_001:0464b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說諸宿攝於三趣。",
        "from": "T21n1312_001:0464b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂奎婁參井鬼軫亢房牛等九宿攝於天趣。",
        "from": "T21n1312_001:0464b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "胃昴嘴柳星角氐心尾等九宿攝羅剎趣。",
        "from": "T21n1312_001:0464b10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "箕室張畢女危斗壁翼等九宿攝於人趣。",
        "from": "T21n1312_001:0464b11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遇天趣諸宿照臨生者。",
        "from": "T21n1312_001:0464b12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡是男女皆處富貴。",
        "from": "T21n1312_001:0464b12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "子孫昌盛一切吉祥最為第一。",
        "from": "T21n1312_001:0464b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時難儞計濕嚩囉天。",
        "from": "T21n1312_001:0464b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為於世間一切男女。",
        "from": "T21n1312_001:0464b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說支輪已歡喜而退。",
        "from": "T21n1312_001:0464b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難儞計濕嚩囉天說支輪經",
        "from": "T21n1312_001:0464b16_100",
        "context_with_next_sentence": false
    }
]