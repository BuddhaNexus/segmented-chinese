[
    {
        "source_sentence": "金剛頂一切如來真實攝大乘現",
        "from": "T18n0874_002:0317a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證大教王經卷下",
        "from": "T18n0874_002:0317a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐特進試鴻臚卿三藏沙門",
        "from": "T18n0874_002:0317a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空奉　詔譯2 真言羯磨部金剛界大曼拏囉毘盧遮那一切如來族祕密心地印真言羯磨部第二稽首薄伽梵、　　大毘盧遮那！",
        "from": "T18n0874_002:0317a22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能為自在王！",
        "from": "T18n0874_002:0317a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羯磨諸儀則，　　印契及真言，",
        "from": "T18n0874_002:0317a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養諸如來！",
        "from": "T18n0874_002:0317a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　次結羯磨印：",
        "from": "T18n0874_002:0317a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於心而修習，　　諦觀心月輪，",
        "from": "T18n0874_002:0317a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而有羯磨杵，　　應結金剛拳，",
        "from": "T18n0874_002:0317a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "等引而兩分。",
        "from": "T18n0874_002:0317a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以握力之端、　　左拳安於臍，",
        "from": "T18n0874_002:0317a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右羽垂觸地、　　左拳如前相，",
        "from": "T18n0874_002:0317b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右羽為施願、　　二羽仰相叉，",
        "from": "T18n0874_002:0317b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力竪相背，　　彈指橫其端、",
        "from": "T18n0874_002:0317b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左拳復安臍，　　右羽施無畏。",
        "from": "T18n0874_002:0317b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是五如來契。",
        "from": "T18n0874_002:0317b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼彼真言曰：",
        "from": "T18n0874_002:0317b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　嚩日囉二合馱覩　鑁唵引　阿屈芻二合毘野二合　吽唵引　囉怛曩二合三婆嚩　怛𠸪二合唵引　路計濕嚩二合囉囉引惹　紇哩以三合唵引　阿謨佉悉第　噁次當結羯磨，　　四波羅蜜契：",
        "from": "T18n0874_002:0317b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各如本佛印，　　而誦於真言。",
        "from": "T18n0874_002:0317b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼彼真言曰：",
        "from": "T18n0874_002:0317b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　薩怛嚩二合　嚩日哩二合　吽唵引　囉怛曩二合　嚩日哩二合　怛𠸪二合唵引　達囉摩二合　嚩日哩二合　紇哩以三合唵引　羯囉磨二合　嚩日哩二合　噁次結十六尊！",
        "from": "T18n0874_002:0317b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　羯磨契之儀：",
        "from": "T18n0874_002:0317b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右拳安腰側，　　右羽搊擲杵、",
        "from": "T18n0874_002:0317b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二拳交抱胸，　　進力鉤以招、",
        "from": "T18n0874_002:0317b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二拳如射法、　　當心作彈指；",
        "from": "T18n0874_002:0317b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力如寶形、　　於心旋日輪、",
        "from": "T18n0874_002:0317b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右肘拄左拳、　　二拳口仰散；",
        "from": "T18n0874_002:0317b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左蓮右開勢、　　左手想持華，",
        "from": "T18n0874_002:0317b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手如把劍、　　覆拳進力拄，",
        "from": "T18n0874_002:0317b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於臍而平轉、　　並至口仰散，",
        "from": "T18n0874_002:0317b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先從禪智舒；",
        "from": "T18n0874_002:0317b28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛掌於頂、　　二拳被甲冑、",
        "from": "T18n0874_002:0317b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力檀慧互、　　二拳而相合。",
        "from": "T18n0874_002:0317c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十六大士印，　　內、外八供養，",
        "from": "T18n0874_002:0317c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并及於四護！",
        "from": "T18n0874_002:0317c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　印相今當說：",
        "from": "T18n0874_002:0317c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二拳各腰側，　　向左小低頭、",
        "from": "T18n0874_002:0317c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二拳以繫鬘，　　從額頂後散，",
        "from": "T18n0874_002:0317c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二拳側相合，　　從臍至口散，",
        "from": "T18n0874_002:0317c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二拳如舞儀，　　旋轉掌於頂。",
        "from": "T18n0874_002:0317c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以金剛掌儀、　　燒香等四印、",
        "from": "T18n0874_002:0317c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以降三世印、　　鉤索等四攝。",
        "from": "T18n0874_002:0317c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並拳向下散、　　仰散如捧獻、",
        "from": "T18n0874_002:0317c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪智竪如針、　　開掌塗於胸。",
        "from": "T18n0874_002:0317c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進屈如鉤形、　　進力曲相捻、",
        "from": "T18n0874_002:0317c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二度便相鉤、　　合腕微搖動。",
        "from": "T18n0874_002:0317c13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼彼真言曰：",
        "from": "T18n0874_002:0317c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右心左按地，　　繞輪壇四面，",
        "from": "T18n0874_002:0318a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各一稱真言，　　安立賢劫位。",
        "from": "T18n0874_002:0318a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰：",
        "from": "T18n0874_002:0318a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吽引　吽短賢劫千如來！",
        "from": "T18n0874_002:0318a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　十六大名稱：",
        "from": "T18n0874_002:0318a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先畫彌勒尊！",
        "from": "T18n0874_002:0318a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　次明不空見！",
        "from": "T18n0874_002:0318a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切滅惡趣！",
        "from": "T18n0874_002:0318a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　離一切憂暗！",
        "from": "T18n0874_002:0318a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香象、勇猛尊！",
        "from": "T18n0874_002:0318a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　虛空藏、智幢！",
        "from": "T18n0874_002:0318a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量光、月光！",
        "from": "T18n0874_002:0318a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　賢護、光網尊！",
        "from": "T18n0874_002:0318a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次畫金剛藏！",
        "from": "T18n0874_002:0318a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　無盡慧、辯積！",
        "from": "T18n0874_002:0318a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普賢大光明，　　及餘上首尊！",
        "from": "T18n0874_002:0318a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最初置「阿」字，　　􆿅",
        "from": "T18n0874_002:0318a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或書十六名，　　金剛智種子，",
        "from": "T18n0874_002:0318a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖天之儀軌，　　依教而安立；",
        "from": "T18n0874_002:0318a28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地居空行天，　　巧智善安布，",
        "from": "T18n0874_002:0318a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸尊悉地相，　　次第應當明。",
        "from": "T18n0874_002:0318b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼彼真言曰：",
        "from": "T18n0874_002:0318b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　昧怛哩二合野　娑嚩二合賀引唵引　阿目佉娜㗚捨二合曩野　娑嚩二合賀引唵引　薩嚩　播野惹憾　娑嚩二合賀唵引　巘駄賀悉底二合　娑嚩二合賀引唵引　戌囉野　娑嚩二合賀引唵引　阿迦捨誐囉婆二合　娑嚩二合賀引唵引　誐惹二合抳曩計妬　娑嚩二合賀引唵引　阿弭哆鉢囉二合婆　娑嚩二合賀引唵引　贊捺囉二合嚩日囉二合鉢囉二合婆　娑嚩二合賀引唵引　婆捺囉二合　播囉　娑嚩二合賀引唵引　入嚩二合攞𩕳鉢囉二合婆吽　娑嚩二合賀引唵引　嚩日囉二合薩囉婆二合　娑嚩二合賀引唵引　阿乞叉二合摩底　娑嚩二合賀引唵引　三滿哆　婆捺攞二合野　娑嚩二合賀引3 真言三昧耶部金剛界大曼拏囉毘盧遮那一切如來族祕密心地印真言三昧耶部第三爾時薄伽梵、　　大毘盧遮那！",
        "from": "T18n0874_002:0318b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能為自在王！",
        "from": "T18n0874_002:0318b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　演說金剛界，三昧之儀軌。",
        "from": "T18n0874_002:0318b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　次結三昧耶：",
        "from": "T18n0874_002:0318b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於舌觀金剛，　　先合金剛掌，",
        "from": "T18n0874_002:0318b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便成金剛縛。",
        "from": "T18n0874_002:0318b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力附於背、　　忍願竪如針、",
        "from": "T18n0874_002:0318b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及屈如寶形、　　移屈如蓮葉、",
        "from": "T18n0874_002:0318b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面合於掌中，　　檀慧禪智合。",
        "from": "T18n0874_002:0318b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為五佛印。",
        "from": "T18n0874_002:0318b28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼彼真言曰：",
        "from": "T18n0874_002:0318b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚩日囉二合惹拏二合喃　婀去聲嚩日囉二合惹拏二合喃　吽嚩日囉二合惹拏二合喃　怛𠸪二合嚩日囉二合惹拏二合喃　噁次結三昧耶，　　四波羅蜜契：",
        "from": "T18n0874_002:0318c05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各如佛之契，　　別別誦真言。",
        "from": "T18n0874_002:0318c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼彼真言曰：",
        "from": "T18n0874_002:0318c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚩日囉二合室哩二合　吽嚩日囉二合嬌哩　怛㘕嚩日囉二合哆囉　紇哩以三合佉　嚩日哩二合抳　斛次結十六尊、　　八供養、四攝！",
        "from": "T18n0874_002:0318c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三昧耶印契：",
        "from": "T18n0874_002:0318c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小大鬪而竪，　　次以金剛縛，",
        "from": "T18n0874_002:0318c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力屈如鉤、　　因鉤便交竪、",
        "from": "T18n0874_002:0318c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不解縛彈指；",
        "from": "T18n0874_002:0318c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不改大與次，　　舒六而旋轉，",
        "from": "T18n0874_002:0318c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前二亦不改，　　中縛下四幢、",
        "from": "T18n0874_002:0318c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不易前印相，　　反開散於口；",
        "from": "T18n0874_002:0318c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由縛禪智竪，　　進力屈如蓮、",
        "from": "T18n0874_002:0318c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由縛竪忍願，　　屈上節如劍、",
        "from": "T18n0874_002:0318c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忍願從入縛，　　四竪五竪交、",
        "from": "T18n0874_002:0318c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由縛進力蓮，　　禪智開偃附；",
        "from": "T18n0874_002:0318c23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六度叉而覆，　　大各捻小甲、",
        "from": "T18n0874_002:0318c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力針當心、　　進力檀慧開，",
        "from": "T18n0874_002:0318c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小竪進力鉤、　　縛大捻小根，",
        "from": "T18n0874_002:0318c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力拄其背。",
        "from": "T18n0874_002:0318c27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此印展當額、　　從臍口仰散、",
        "from": "T18n0874_002:0318c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旋舞掌於頂；",
        "from": "T18n0874_002:0318c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從縛仰開獻，　　由縛禪智針、",
        "from": "T18n0874_002:0319a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解縛摩於胸。",
        "from": "T18n0874_002:0319a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪入智虎口、　　上四交如環、",
        "from": "T18n0874_002:0319a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪智入掌搖。",
        "from": "T18n0874_002:0319a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　四印而一縛，別別誦真言。",
        "from": "T18n0874_002:0319a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼彼真言曰：",
        "from": "T18n0874_002:0319a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三摩野薩　怛鑁三合　阿曩野薩　怛縛三合　阿斛　素佉　娑度　娑度　素摩賀　怛鑁三合　嚕補儞庾二合哆　阿他　鉢囉二合底　賀賀賀　吽郝　薩嚩　迦哩　耨佉砌娜　沒馱　󰂛地　鉢囉二合底捨左娜二合　素嚩始　怛鑁二合　𩕳㗚婆二合野　怛鑁二合　設咄嚕二合薄乞叉二合　薩嚩　悉地　摩賀囉底　嚕播戍陛　戍嚕二合怛囉二合　嫂佉也二合　薩縛　布𡁠　鉢囉二合賀攞二合禰𩕳　跛攞誐弭　素帝惹擬哩二合　素巘馱擬入聲　阿野　醯　𠺁　阿醯　吽　吽　呬　娑普二合吒　鑁　佉吒　噁　噁入聲4 真言供養部金剛界大曼拏囉毘盧遮那一切如來族祕密心地印真言供養部第四敬禮毘盧尊！",
        "from": "T18n0874_002:0319a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　能為自在王！",
        "from": "T18n0874_002:0319a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "演說供養部，　　供養諸如來！",
        "from": "T18n0874_002:0319a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結供養契：",
        "from": "T18n0874_002:0319a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　應結金剛縛，印相從心起。",
        "from": "T18n0874_002:0319a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　初結遍照尊！",
        "from": "T18n0874_002:0319a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羯磨之印儀。",
        "from": "T18n0874_002:0319a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰：",
        "from": "T18n0874_002:0319a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　薩嚩　怛他誐哆一　嚩日囉二合馱怛味二合二　弩哆囉　布惹三　娑頗二合囉拏二合四　三摩曳五　吽次結金剛薩埵羯磨印　觸地手：",
        "from": "T18n0874_002:0319a28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　薩嚩　怛他誐哆一　嚩日囉二合薩怛嚩二合二　弩哆囉　布惹三　娑頗二合囉拏四　三摩曳五　吽六次結金剛寶羯磨印　施願手：",
        "from": "T18n0874_002:0319b02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　薩嚩　怛他誐哆一　嚩日囉二合囉怛曩二合二　弩跢囉　布惹三　娑頗二合囉拏四　三摩曳五　吽六次結金剛法羯磨印　法定手：",
        "from": "T18n0874_002:0319b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　薩嚩　怛他誐哆一　嚩日囉二合達囉摩二合二　弩跢囉　布惹三　娑頗二合囉拏四　三摩曳五　吽六次結金剛業羯磨印　最上手：",
        "from": "T18n0874_002:0319b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　薩嚩　怛他誐哆一　嚩日囉二合迦囉磨二合二　弩哆囉　布惹三　娑頗二合囉拏四　三摩曳五　吽六次心上金剛縛密語曰：",
        "from": "T18n0874_002:0319b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　入嚩手十六唵引　薩嚩　怛他誐哆一　薩嚩　怛磨二合　𩕳哩也二合哆曩二　布惹　娑頗二合囉拏三　迦囉磨二合　嚩日哩二合四　噁五右脅密語曰：",
        "from": "T18n0874_002:0319b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　薩嚩　怛他誐哆一　薩嚩　怛摩二合　𩕳哩也二合怛曩二　娑度迦囉三　布惹　娑頗二合囉拏四　迦囉磨二合覩瑟致二合五　索六額上密語曰：",
        "from": "T18n0874_002:0319b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　曩莫薩嚩　怛他誐哆一　鼻曬迦二　囉怛寧二合毘喻二合三　嚩日囉二合摩抳　唵引四心上旋轉如日輪轉相密語曰：",
        "from": "T18n0874_002:0319b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　曩莫薩嚩　怛他誐哆一　捨　播哩布囉拏二　唧跢摩抳三　馱嚩二合惹詣哩二合毘喻二合四　嚩日囉二合馱嚩二合惹五　擬哩二合　怛覽二合引六口上笑處解散密語曰：",
        "from": "T18n0874_002:0319b26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　曩莫薩嚩　怛他誐哆一　摩賀必哩二合底二　鉢囉二合謨儞也二合迦𠼝毘喻二合三　嚩日囉二合賀細　郝四口上密語曰：",
        "from": "T18n0874_002:0319c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　薩嚩　怛他誐哆一　嚩日囉二合達囉磨二合二哆　三摩地　僻三　薩覩二合　弩　弭四　摩賀達囉磨二合五　擬哩二合　紇哩以三合六右耳密語曰：",
        "from": "T18n0874_002:0319c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　薩嚩　怛他誐哆一　鉢囉二合惹拏二合播囉弭哆二　鼻𩕳囉賀󰪯三　薩覩二合弩弭四　摩賀具灑　弩猊　淡五左耳密語曰：",
        "from": "T18n0874_002:0319c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　薩嚩　怛他誐哆一　作羯囉二合乞叉二合囉二　播哩嚩㗚哆二合曩三　薩嚩　素怛覽二合四　怛曩野曳五　薩覩二合　弩　弭六　薩嚩　曼拏囉　吽七頂後密語曰：",
        "from": "T18n0874_002:0319c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　薩嚩　怛他誐哆一　散馱　薄灑二　沒馱僧擬底　鼻　誐喃三　薩覩二合　弩　弭四　嚩日囉二合嚩際　作五香頂上密語曰：",
        "from": "T18n0874_002:0319c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　薩嚩　怛他誐哆一　度播　銘伽　三母捺囉二合二　娑頗二合囉拏三　布惹　迦囉弭二合四　迦囉　迦囉入聲引五華右肩上密語曰：",
        "from": "T18n0874_002:0319c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　薩嚩　怛他誐哆一　補瑟跛二合　鉢囉二合摩囉二　娑頗二合囉拏三　布惹　羯囉弭二合四　枳哩　枳哩五燈右跨上密語曰：",
        "from": "T18n0874_002:0319c25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　薩嚩　怛他誐哆一　路迦入嚩二合囉二　娑頗二合囉拏三　布惹　羯囉弭二合四　婆囉　婆囉五塗復置心上密語曰：",
        "from": "T18n0874_002:0319c28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　薩嚩　怛他誐哆一　彥馱　銘伽　三母捺囉二合二　娑頗二合囉拏三　布惹　羯囉弭二合四　俱嚕　俱嚕五次結散華契：",
        "from": "T18n0874_002:0320a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　觀察於十方，言我今勸請，　　諸佛轉法輪。",
        "from": "T18n0874_002:0320a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復應作是念：",
        "from": "T18n0874_002:0320a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及於十方界，　　人天意生華，",
        "from": "T18n0874_002:0320a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水陸所有華，　　皆持獻十方，",
        "from": "T18n0874_002:0320a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切大薩埵！",
        "from": "T18n0874_002:0320a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　部中諸眷屬！",
        "from": "T18n0874_002:0320a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "契明密諸天，　　我為普供養，",
        "from": "T18n0874_002:0320a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸如來！",
        "from": "T18n0874_002:0320a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　而作事業故。",
        "from": "T18n0874_002:0320a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密語曰：",
        "from": "T18n0874_002:0320a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　薩嚩　怛他誐哆一　補瑟波二合　布惹　銘伽二　三母捺囉二合三　娑頗二合囉拏四　三摩曳　吽五又結燒香契，　　而作是思惟：",
        "from": "T18n0874_002:0320a13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人天本體香，　　和合變易香，",
        "from": "T18n0874_002:0320a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來羯磨故，　　我今皆奉獻。",
        "from": "T18n0874_002:0320a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密語曰：",
        "from": "T18n0874_002:0320a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　薩嚩　怛他誐哆一　巘馱　布惹　銘伽二　三母捺囉二合三　娑頗二合囉拏四　三摩曳　吽五次結燈契已，　　而作是思惟：",
        "from": "T18n0874_002:0320a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人天本體生，　　及差別光明，",
        "from": "T18n0874_002:0320a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為作事業故，　　我今皆奉獻。",
        "from": "T18n0874_002:0320a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密語曰：",
        "from": "T18n0874_002:0320a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　薩嚩　怛他誐哆一　儞播　布惹　銘伽二　三母捺囉二合三　娑頗二合囉拏四　三摩曳　吽五三昧耶寶契，　　應作如是念：",
        "from": "T18n0874_002:0320a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此界及餘界，　　寶山諸寶類，",
        "from": "T18n0874_002:0320a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地中及海中，　　彼皆為供養，",
        "from": "T18n0874_002:0320a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來羯磨故，　　我今皆奉獻。",
        "from": "T18n0874_002:0320a28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當誦此密語。",
        "from": "T18n0874_002:0320a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰：",
        "from": "T18n0874_002:0320b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　薩嚩　怛他誐哆一　󰂛𠆙焰二合誐　囉怛曩二合　稜去聲迦囉二　布惹　銘伽　三母捺囉二合三　娑頗二合囉拏　三摩曳　吽四次結嬉戲契，　　應作是思惟：",
        "from": "T18n0874_002:0320b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人天之所有，　　種種諸戲弄，",
        "from": "T18n0874_002:0320b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玩笑妓樂具，　　皆為供養佛，",
        "from": "T18n0874_002:0320b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而作事業故，　　我今當奉獻。",
        "from": "T18n0874_002:0320b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縛大捻小根，　　進力拄其背。",
        "from": "T18n0874_002:0320b09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密語曰：",
        "from": "T18n0874_002:0320b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　薩嚩　怛他誐哆一　賀寫　囉寫二　枳哩二合弩囉底三　掃企也二合　弩跢囉四　布惹　銘伽　三母捺囉二合五　娑頗二合囉拏　三摩曳　吽六薩埵三昧耶，　　應作是思惟：",
        "from": "T18n0874_002:0320b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是劫樹等，　　能與種種衣，",
        "from": "T18n0874_002:0320b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚴身資具者，　　彼皆為供養，",
        "from": "T18n0874_002:0320b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而作事業故，　　我今當奉獻，",
        "from": "T18n0874_002:0320b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此祕密言。",
        "from": "T18n0874_002:0320b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰：",
        "from": "T18n0874_002:0320b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　薩嚩　怛他誐哆一　弩哆囉　嚩日嚕二合播摩　三摩地二　婆嚩曩　播曩三　部惹曩　嚩娑曩四　布惹　銘伽　三母捺囉二合五　娑頗二合囉拏　三摩曳　吽六羯磨三昧耶，　　而作是思惟：",
        "from": "T18n0874_002:0320b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於虛空藏中，　　所有諸如來！",
        "from": "T18n0874_002:0320b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為承事故，　　想一一佛前，",
        "from": "T18n0874_002:0320b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而皆有己身，　　以親近侍奉，",
        "from": "T18n0874_002:0320b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當誦此密語，真言曰：",
        "from": "T18n0874_002:0320b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　薩嚩　怛他誐哆一　迦野　𩕳哩也二合哆曩二　布惹　銘伽　三母捺囉二合三　娑頗二合囉拏　三摩曳　吽達磨三昧耶，　　而作是思惟：",
        "from": "T18n0874_002:0320c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今即此身，　　與諸菩薩等，",
        "from": "T18n0874_002:0320c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀得法實性，　　平等無有異，",
        "from": "T18n0874_002:0320c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既作無有異，　　而誦此密言。",
        "from": "T18n0874_002:0320c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密語曰：",
        "from": "T18n0874_002:0320c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　薩嚩　怛他誐哆一　唧跢　𩕳哩也二合怛曩二　布惹　銘伽　三母捺囉二合三　娑頗二合囉拏　三摩曳　吽寶幢三昧耶：",
        "from": "T18n0874_002:0320c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生類，　　苦惱之所纏，",
        "from": "T18n0874_002:0320c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深生哀愍故，　　我今為救護，",
        "from": "T18n0874_002:0320c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并護菩提心。",
        "from": "T18n0874_002:0320c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　未度者令度；",
        "from": "T18n0874_002:0320c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未安者令安，　　皆令得涅槃，",
        "from": "T18n0874_002:0320c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及雨種種寶，　　所求令滿足。",
        "from": "T18n0874_002:0320c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是思惟已，　　而誦此密言。",
        "from": "T18n0874_002:0320c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密語曰：",
        "from": "T18n0874_002:0320c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結香身契，　　三昧耶塗香，",
        "from": "T18n0874_002:0320c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而作是思惟：",
        "from": "T18n0874_002:0320c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三業諸不善，　　願悉皆遠離，",
        "from": "T18n0874_002:0320c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸善法，　　願悉皆成就。",
        "from": "T18n0874_002:0320c25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而誦此密言。",
        "from": "T18n0874_002:0320c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密語曰：",
        "from": "T18n0874_002:0320c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　薩嚩　怛他誐哆一　弩跢囉　摩賀󰂛地也二合　賀囉迦二　試攞播囉弭哆三　布惹　銘伽　三母捺囉二合四　娑頗二合囉拏　三摩曳　吽結羯磨觸地，　　復應作是念：",
        "from": "T18n0874_002:0321a02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願一切眾生，　　慈心無惱害，",
        "from": "T18n0874_002:0321a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離諸怖畏，　　相視心歡喜，",
        "from": "T18n0874_002:0321a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸相好莊嚴，　　成甚深法藏。",
        "from": "T18n0874_002:0321a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當誦此真言。",
        "from": "T18n0874_002:0321a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密語曰：",
        "from": "T18n0874_002:0321a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬪勝精進契，　　三昧耶甲冑，",
        "from": "T18n0874_002:0321a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而作是思惟：",
        "from": "T18n0874_002:0321a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　願一切眾生，行菩薩行者，　　被堅固甲冑。",
        "from": "T18n0874_002:0321a13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密語曰：",
        "from": "T18n0874_002:0321a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結三摩地契，　　華方佛羯磨，",
        "from": "T18n0874_002:0321a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應作是思惟：",
        "from": "T18n0874_002:0321a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "調伏於煩惱、　　隨煩惱冤讎，",
        "from": "T18n0874_002:0321a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲甚深禪定。",
        "from": "T18n0874_002:0321a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　而誦此密語。",
        "from": "T18n0874_002:0321a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密語曰：",
        "from": "T18n0874_002:0321a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結遍照尊，　　羯磨勝契已，",
        "from": "T18n0874_002:0321a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而作是思惟：",
        "from": "T18n0874_002:0321a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就五種明，　　世間出世間，",
        "from": "T18n0874_002:0321a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智慧普成就，　　而得真實見，",
        "from": "T18n0874_002:0321a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除煩惱障智，　　辯才無畏等，",
        "from": "T18n0874_002:0321b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛法嚴其心。",
        "from": "T18n0874_002:0321b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　而誦此密語。",
        "from": "T18n0874_002:0321b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密語曰：",
        "from": "T18n0874_002:0321b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　薩嚩　怛他誐哆一　弩哆囉　枳禮二合捨惹拏二合野二　嚩囉拏嚩娑曩三　尾曩野曩四　摩賀鉢囉二合惹拏二合播囉弭哆五　布惹　銘伽　三母捺囉二合六　娑頗二合囉拏　三摩曳　吽勝上三摩地，　　印契次應結：",
        "from": "T18n0874_002:0321b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二羽外相叉，　　禪智令相捻，",
        "from": "T18n0874_002:0321b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仰安於懷中。",
        "from": "T18n0874_002:0321b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　應作是思惟：",
        "from": "T18n0874_002:0321b10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證法真實性，　　空無相無作，",
        "from": "T18n0874_002:0321b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸法悉如是。",
        "from": "T18n0874_002:0321b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　觀已誦密言。",
        "from": "T18n0874_002:0321b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密語曰：",
        "from": "T18n0874_002:0321b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　薩嚩　怛他誐哆一　虞上聲醯野二合　摩賀鉢囉二合底播底二　布惹　銘伽　三母捺囉二合三　娑頗二合囉拏　三摩曳　吽次應合指爪，　　而作是思惟：",
        "from": "T18n0874_002:0321b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二羽金剛縛，　　進力禪智口。",
        "from": "T18n0874_002:0321b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今出語言，　　願一切眾生，",
        "from": "T18n0874_002:0321b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆令得聞。",
        "from": "T18n0874_002:0321b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　誦此祕密言。",
        "from": "T18n0874_002:0321b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰：",
        "from": "T18n0874_002:0321b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是廣作佛事已，　　次應諦心為念誦，",
        "from": "T18n0874_002:0321b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾會眷屬自圍繞，　　住於圓寂大鏡智。",
        "from": "T18n0874_002:0321b26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當結金剛三昧耶，　　而誦金剛百字明，",
        "from": "T18n0874_002:0321b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次誦金剛薩埵明，　　三遍五遍或七遍。",
        "from": "T18n0874_002:0321b28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰：",
        "from": "T18n0874_002:0321b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　嚩日囉二合　薩怛嚩二合　三摩野一　摩弩播攞野二　嚩日囉二合薩怛嚩二合　怛味二合三　弩播底瑟姹二合四　涅哩二合嗏　銘　婆嚩五　素妬瑟欲二合　銘　婆嚩六　阿努囉訖妬二合　銘　婆嚩七　素布瑟欲二合　銘　婆嚩八　薩嚩　悉朕　銘九　婆嚩　鉢囉二合野瑳十　薩嚩　羯磨　素十一　左　銘　唧哆室哩二合藥　俱嚕十二　吽十三　賀引　賀　賀　賀　斛十四　婆誐鑁十五　薩嚩　怛他引誐哆十六　嚩日囉二合　摩銘　悶左重聲十七　嚩日囉二合　婆嚩十八　摩賀引三摩野十九　薩怛嚩二合　噁二十次應捧珠鬘，　　誦真言七遍。",
        "from": "T18n0874_002:0321c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復以加持句，　　如法而加持，",
        "from": "T18n0874_002:0321c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端坐如儀則，　　應以金剛語，",
        "from": "T18n0874_002:0321c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一千或一百，　　隨意而念誦。",
        "from": "T18n0874_002:0321c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰：",
        "from": "T18n0874_002:0321c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二羽捧珠鬘，　　本真言七遍，",
        "from": "T18n0874_002:0321c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捧至頂及心，　　千轉以加持。",
        "from": "T18n0874_002:0321c18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰：",
        "from": "T18n0874_002:0321c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既加持珠已，　　住等引而誦，",
        "from": "T18n0874_002:0321c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不極動舌端，　　脣齒二俱合，",
        "from": "T18n0874_002:0321c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就諸密教，　　金剛語離聲，",
        "from": "T18n0874_002:0321c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "循身觀相好，　　四時不令間，",
        "from": "T18n0874_002:0321c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百千是為限。",
        "from": "T18n0874_002:0321c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　又復應過是，神通及福智，　　見世同薩埵。",
        "from": "T18n0874_002:0321c26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦分限畢，　　捧珠發大願，",
        "from": "T18n0874_002:0321c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結三摩地契，　　入法界三昧。",
        "from": "T18n0874_002:0321c28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者出三昧，　　即結根本印，",
        "from": "T18n0874_002:0321c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念本明七遍。",
        "from": "T18n0874_002:0322a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　復結八供養，以妙音讚歎，　　獻閼伽香水。",
        "from": "T18n0874_002:0322a02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以降三世印，　　左旋而解界。",
        "from": "T18n0874_002:0322a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結三昧拳，　　一誦而掣開。",
        "from": "T18n0874_002:0322a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結羯磨拳，　　三誦三開手。",
        "from": "T18n0874_002:0322a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從彼彼出生，　　所有一切印，",
        "from": "T18n0874_002:0322a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彼彼當解，　　由此真言心。",
        "from": "T18n0874_002:0322a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰：",
        "from": "T18n0874_002:0322a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　嚩日囉二合穆乞叉二合　穆入聲次結奉送印：",
        "from": "T18n0874_002:0322a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忍願如蓮葉，　　指端安時華，",
        "from": "T18n0874_002:0322a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦已而上擲，　　為奉送聖眾。",
        "from": "T18n0874_002:0322a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰：",
        "from": "T18n0874_002:0322a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　訖哩二合妬嚩入聲一　薩嚩　薩怛嚩二合二　囉他二合　悉第　娜二合三　哆　野他四　弩誐　孽瑳　馱鑁二合五　沒馱尾灑野六　布曩囉誐七　摩曩　野覩八　唵引嚩日囉二合　薩怛嚩二合九　目乞叉二合　目入聲十次當結寶印：",
        "from": "T18n0874_002:0322a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力如寶形，　　禪智亦復然，",
        "from": "T18n0874_002:0322a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印相從心起，　　安於灌頂處，",
        "from": "T18n0874_002:0322a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分手如繫鬘。",
        "from": "T18n0874_002:0322a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　次結甲冑印：",
        "from": "T18n0874_002:0322a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰：",
        "from": "T18n0874_002:0322a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　嚩日囉二合囉怛曩二合一　鼻詵左　𤚥二　薩嚩　母娜囕二合　銘三　涅哩二合稚　俱嚕四　嚩日囉二合迦嚩左曩　鑁五　唵引　砧次結被甲已，　　齊掌而三拍：",
        "from": "T18n0874_002:0322a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令聖眾歡喜，　　以此心真言，",
        "from": "T18n0874_002:0322a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解縛得歡喜，　　獲得金剛體。",
        "from": "T18n0874_002:0322a28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰：",
        "from": "T18n0874_002:0322a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引　嚩日囉二合覩瑟野二合　斛入聲奉送聖眾已，　　當結加持契：",
        "from": "T18n0874_002:0322b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦明加四處、　　灌頂、被甲冑、",
        "from": "T18n0874_002:0322b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又為拍印儀、　　如前禮四佛！",
        "from": "T18n0874_002:0322b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懺悔并發願，　　然後依閑靜。",
        "from": "T18n0874_002:0322b05_13",
        "context_with_next_sentence": true
    },
    {
