[
    {
        "source_sentence": "文殊所說最勝名義經",
        "from": "T20n1188_001:0814c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宋西天三藏明因妙善普濟法師",
        "from": "T20n1188_001:0814c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金總持等奉　詔譯",
        "from": "T20n1188_001:0814c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命妙吉祥　　所說最勝義",
        "from": "T20n1188_001:0814c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛掌菩薩　　降伏諸魔眾",
        "from": "T20n1188_001:0814c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身湧遍三界　　祕密王自在",
        "from": "T20n1188_001:0814c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目若青蓮葉　　面如蓮華敷",
        "from": "T20n1188_001:0814c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執持金剛杵　　以手擲復擲",
        "from": "T20n1188_001:0814c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現大忿怒相　　威振十方界",
        "from": "T20n1188_001:0814c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛手菩薩　　亦持金剛杵",
        "from": "T20n1188_001:0814c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精進相微妙　　勇猛降諸魔",
        "from": "T20n1188_001:0814c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大智利群生　　化億數金剛",
        "from": "T20n1188_001:0814c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或踴躍歡喜　　或現忿怒相",
        "from": "T20n1188_001:0814c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂禮佛世尊　　合掌心恭敬",
        "from": "T20n1188_001:0814c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願令法久住　　作眾生依怙",
        "from": "T20n1188_001:0814c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常於幻網中　　引接菩提路",
        "from": "T20n1188_001:0814c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無智愚癡人　　困苦煩惱者",
        "from": "T20n1188_001:0814c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救護令解脫　　皆護最上果",
        "from": "T20n1188_001:0814c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊調御師　　常住大三昧",
        "from": "T20n1188_001:0814c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能了諸法性　　根境亦如是",
        "from": "T20n1188_001:0814c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來智慧身　　烏瑟膩沙相",
        "from": "T20n1188_001:0814c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊大智者　　智觀善出生",
        "from": "T20n1188_001:0814c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甚深微妙義　　大法無等倫",
        "from": "T20n1188_001:0814c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於初中後善　　名義為最勝",
        "from": "T20n1188_001:0814c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過去佛已說　　未來佛當說",
        "from": "T20n1188_001:0814c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現在佛今說　　三世皆如是",
        "from": "T20n1188_001:0814c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於大幻網中　　能度諸群品",
        "from": "T20n1188_001:0815a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛掌菩薩　　歡喜伸讚歎",
        "from": "T20n1188_001:0815a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說真密語　　我今能受持",
        "from": "T20n1188_001:0815a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "決定心堅固　　至成等正覺",
        "from": "T20n1188_001:0815a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "演說祕密法　　引導諸眾生",
        "from": "T20n1188_001:0815a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起無漏智慧　　煩惱盡無餘",
        "from": "T20n1188_001:0815a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛手菩薩　　合掌住佛前",
        "from": "T20n1188_001:0815a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時釋迦佛　　最上兩足尊",
        "from": "T20n1188_001:0815a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舌相真實語　　清淨微妙音",
        "from": "T20n1188_001:0815a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普流於世間　　四種魔不現",
        "from": "T20n1188_001:0815a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍滿三界中　　梵音美讚歎",
        "from": "T20n1188_001:0815a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來祕密王　　宣說大法要",
        "from": "T20n1188_001:0815a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉金剛掌　　善哉金剛手",
        "from": "T20n1188_001:0815a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為作利益故　　大智得成就",
        "from": "T20n1188_001:0815a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最上大義利　　滅除諸罪垢",
        "from": "T20n1188_001:0815a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊大智者　　願聽如是義",
        "from": "T20n1188_001:0815a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉大導師　　方便為宣說",
        "from": "T20n1188_001:0815a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時釋迦佛　　開示祕密法",
        "from": "T20n1188_001:0815a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝持大明呪　　謂持明呪部",
        "from": "T20n1188_001:0815a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世出世間部　　周遍世界部",
        "from": "T20n1188_001:0815a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大印諸法部　　烏瑟膩沙部",
        "from": "T20n1188_001:0815a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六種大呪王　　相應無有二",
        "from": "T20n1188_001:0815a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出生伽陀法　　我今故宣說",
        "from": "T20n1188_001:0815a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安住心智觀　　如來三業轉",
        "from": "T20n1188_001:0815a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵字金剛利　　能斷諸苦惱",
        "from": "T20n1188_001:0815a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊大吉祥　　智慧妙觀察",
        "from": "T20n1188_001:0815a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宣說祕密門　　自在王神呪",
        "from": "T20n1188_001:0815a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命佛世尊　　出生菩提智",
        "from": "T20n1188_001:0815b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遏字一切中　　為大最上義",
        "from": "T20n1188_001:0815b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出生大有情　　離心離言相",
        "from": "T20n1188_001:0815b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及一切音聲　　一切光明相",
        "from": "T20n1188_001:0815b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此摩訶大貪　　一切眾生著",
        "from": "T20n1188_001:0815b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩訶大瞋恨　　一切苦惱因",
        "from": "T20n1188_001:0815b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩訶大愚癡　　眾生心深入",
        "from": "T20n1188_001:0815b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩訶大忿怒　　是為大怨害",
        "from": "T20n1188_001:0815b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩訶大愛樂　　心種種見著",
        "from": "T20n1188_001:0815b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大欲及大樂　　大喜及大染",
        "from": "T20n1188_001:0815b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大色及大身　　大相及大貌",
        "from": "T20n1188_001:0815b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大名及大施　　大圓滿道場",
        "from": "T20n1188_001:0815b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持大智慧劍　　斷大煩惱網",
        "from": "T20n1188_001:0815b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大顯及大稱　　大光及大明",
        "from": "T20n1188_001:0815b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大幻作幻師　　大幻義成就",
        "from": "T20n1188_001:0815b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大幻化染欲　　大幻化見網",
        "from": "T20n1188_001:0815b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大布施殊勝　　大持戒攝受",
        "from": "T20n1188_001:0815b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大忍辱執持　　大精進勇猛",
        "from": "T20n1188_001:0815b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大禪定安住　　大智慧出生",
        "from": "T20n1188_001:0815b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大方便大願　　大力及大智",
        "from": "T20n1188_001:0815b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大慈法無我　　大悲深妙行",
        "from": "T20n1188_001:0815b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大慧大勇猛　　大施大方便",
        "from": "T20n1188_001:0815b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大神通智力　　大威德勇健",
        "from": "T20n1188_001:0815b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大熾盛大力　　大流轉除斷",
        "from": "T20n1188_001:0815b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大金剛祕密　　大忿怒威猛",
        "from": "T20n1188_001:0815b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大怖畏能除　　大最勝明王",
        "from": "T20n1188_001:0815b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大乘最上行　　大乘法增長",
        "from": "T20n1188_001:0815b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大毘盧遮佛　　大寂默牟尼",
        "from": "T20n1188_001:0815b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出生大呪部　　成就祕密法",
        "from": "T20n1188_001:0815b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依十波羅蜜　　修習殊勝行",
        "from": "T20n1188_001:0815c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得十波羅蜜　　清淨妙法門",
        "from": "T20n1188_001:0815c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十種波羅蜜　　如來本真智",
        "from": "T20n1188_001:0815c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十地自在行　　十地安隱住",
        "from": "T20n1188_001:0815c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十智清淨身　　十智功德聚",
        "from": "T20n1188_001:0815c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十相十義利　　牟尼十力尊",
        "from": "T20n1188_001:0815c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十相廣大行　　周遍悉圓滿",
        "from": "T20n1188_001:0815c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來清淨身　　非實亦非假",
        "from": "T20n1188_001:0815c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真語與實語　　如語不異語",
        "from": "T20n1188_001:0815c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無二非無二　　如實際安住",
        "from": "T20n1188_001:0815c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處於無我義　　猶如獅子吼",
        "from": "T20n1188_001:0815c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "威振諸外道　　眾魔皆驚怖",
        "from": "T20n1188_001:0815c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切真空行　　如來親所證",
        "from": "T20n1188_001:0815c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勇猛破怨敵　　是名為最勝",
        "from": "T20n1188_001:0815c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大力轉輪王　　最上尊重者",
        "from": "T20n1188_001:0815c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸生恭敬心　　了法無差別",
        "from": "T20n1188_001:0815c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "演說大乘教　　讚歎無邊義",
        "from": "T20n1188_001:0815c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來真實語　　無實亦無虛",
        "from": "T20n1188_001:0815c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誘接諸群迷　　皆得不退轉",
        "from": "T20n1188_001:0815c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聲聞及緣覺　　佛乘菩提果",
        "from": "T20n1188_001:0815c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種空無著　　如實而了知",
        "from": "T20n1188_001:0815c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漏盡阿羅漢　　離欲勝根境",
        "from": "T20n1188_001:0815c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡諸煩惱結　　無畏得清涼",
        "from": "T20n1188_001:0815c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來無所著　　明行足善逝",
        "from": "T20n1188_001:0815c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間最上士　　調御十號尊",
        "from": "T20n1188_001:0815c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無我慢貢高　　真實心安住",
        "from": "T20n1188_001:0815c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離於輪迴　　億劫修勝行",
        "from": "T20n1188_001:0815c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所作皆已辦　　智慧心決定",
        "from": "T20n1188_001:0815c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊大法王　　演說微妙法",
        "from": "T20n1188_001:0815c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周遍於世間　　教化諸眾生",
        "from": "T20n1188_001:0816a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以最殊勝行　　成就諸義利",
        "from": "T20n1188_001:0816a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作大引導師　　令離諸疑惑",
        "from": "T20n1188_001:0816a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡諸眾生界　　悉獲安隱地",
        "from": "T20n1188_001:0816a04_0",
