[
    {
        "source_sentence": "佛說毘沙門天王經",
        "from": "T21n1245_001:0217a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試鴻臚卿",
        "from": "T21n1245_001:0217a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳教大師臣法天奉　詔譯",
        "from": "T21n1245_001:0217a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T21n1245_001:0217a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在舍衛國祇樹給孤獨園。",
        "from": "T21n1245_001:0217a07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時毘沙門天王。",
        "from": "T21n1245_001:0217a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與百千無數藥叉眷屬。",
        "from": "T21n1245_001:0217a08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於初夜分俱來佛所。",
        "from": "T21n1245_001:0217a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放大光明照祇陀園一切境界。",
        "from": "T21n1245_001:0217a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五體投地禮世尊足。",
        "from": "T21n1245_001:0217a10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住立一面合掌向佛以偈讚曰。",
        "from": "T21n1245_001:0217a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命大無畏　　正覺二足尊",
        "from": "T21n1245_001:0217a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天以天眼　　觀我無所見",
        "from": "T21n1245_001:0217a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過現未來佛　　三世慈悲主",
        "from": "T21n1245_001:0217a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一正遍知　　我今歸命禮",
        "from": "T21n1245_001:0217a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時毘沙門天王說此偈已。",
        "from": "T21n1245_001:0217a16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊。",
        "from": "T21n1245_001:0217a16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有諸聲聞苾芻苾芻尼優婆塞優婆夷。",
        "from": "T21n1245_001:0217a17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於曠野林間樹下經行坐臥。",
        "from": "T21n1245_001:0217a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我此藥叉非人之類。",
        "from": "T21n1245_001:0217a18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有信佛言者有少信之者。",
        "from": "T21n1245_001:0217a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有無數諸惡藥叉不信佛言。",
        "from": "T21n1245_001:0217a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惱亂有情令不安隱。",
        "from": "T21n1245_001:0217a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉世尊所有阿吒曩胝經能為明護。",
        "from": "T21n1245_001:0217a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有苾芻苾芻尼優婆塞優婆夷及諸天人。",
        "from": "T21n1245_001:0217a22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持讀誦禮敬供養廣為人說。",
        "from": "T21n1245_001:0217a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆能衛護為作吉祥。",
        "from": "T21n1245_001:0217a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時會中有諸正信藥叉之眾。",
        "from": "T21n1245_001:0217a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌白言。",
        "from": "T21n1245_001:0217a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願天王說此經典我等樂聞。",
        "from": "T21n1245_001:0217a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時毘沙門天王默然受請。",
        "from": "T21n1245_001:0217a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即向佛前頭面禮足。",
        "from": "T21n1245_001:0217a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "承佛威神告藥叉言。",
        "from": "T21n1245_001:0217a26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此經典若有所得宣布流通。",
        "from": "T21n1245_001:0217a27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除眾生一切煩惱。",
        "from": "T21n1245_001:0217a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故我今歸依頂禮。",
        "from": "T21n1245_001:0217a28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東方世界有乾闥婆主名曰持國。",
        "from": "T21n1245_001:0217a29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具大威德身放光明。",
        "from": "T21n1245_001:0217b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如日出普照世間。",
        "from": "T21n1245_001:0217b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "統領眷屬乾闥婆眾恭敬圍繞。",
        "from": "T21n1245_001:0217b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歌舞作唱而受快樂。",
        "from": "T21n1245_001:0217b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有九十一子同名帝釋。",
        "from": "T21n1245_001:0217b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大勢力勇猛暴惡。",
        "from": "T21n1245_001:0217b03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見佛世尊歸依頂禮尊重恭敬。",
        "from": "T21n1245_001:0217b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀此非人而能禮敬。",
        "from": "T21n1245_001:0217b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼持國天王守護東方。",
        "from": "T21n1245_001:0217b05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如佛行行如是護世。",
        "from": "T21n1245_001:0217b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故我今稽首歸命正遍知明行足無上寂靜。",
        "from": "T21n1245_001:0217b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南方世界有鳩盤拏主名尾嚕茶迦。",
        "from": "T21n1245_001:0217b08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具大威德身有光明。",
        "from": "T21n1245_001:0217b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如日照世亦如大海深廣無邊。",
        "from": "T21n1245_001:0217b09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼諸凡夫不可測度。",
        "from": "T21n1245_001:0217b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "統領眷屬鳩盤拏眾恭敬圍繞。",
        "from": "T21n1245_001:0217b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歌舞作唱而受快樂。",
        "from": "T21n1245_001:0217b11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有九十一子同名帝釋。",
        "from": "T21n1245_001:0217b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大勢力勇猛暴惡。",
        "from": "T21n1245_001:0217b12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見佛世尊歸依頂禮尊重恭敬。",
        "from": "T21n1245_001:0217b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀此非人而能禮敬。",
        "from": "T21n1245_001:0217b13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼鳩盤拏主守護南方。",
        "from": "T21n1245_001:0217b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如佛行行如是護世。",
        "from": "T21n1245_001:0217b14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故我今稽首歸命正遍知明行足無上寂靜。",
        "from": "T21n1245_001:0217b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方世界有大龍主名尾嚕博叉。",
        "from": "T21n1245_001:0217b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大威德光明遠照。",
        "from": "T21n1245_001:0217b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "統領眷屬諸大龍眾恭敬圍繞。",
        "from": "T21n1245_001:0217b17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歌舞作唱而受快樂。",
        "from": "T21n1245_001:0217b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有九十一子同名帝釋。",
        "from": "T21n1245_001:0217b18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大勢力勇猛暴惡。",
        "from": "T21n1245_001:0217b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見佛世尊歸命頂禮尊重恭敬。",
        "from": "T21n1245_001:0217b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀此非人而能禮敬。",
        "from": "T21n1245_001:0217b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼大龍主守護西方。",
        "from": "T21n1245_001:0217b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如佛行行如是護世。",
        "from": "T21n1245_001:0217b21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故我今稽首歸命正遍知明行足無上寂靜。",
        "from": "T21n1245_001:0217b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方世界有藥叉名俱吠囉。",
        "from": "T21n1245_001:0217b23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大威德身光熾盛如大火焰。",
        "from": "T21n1245_001:0217b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "統領眷屬藥叉之眾恭敬圍繞。",
        "from": "T21n1245_001:0217b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歌舞作唱而受快樂。",
        "from": "T21n1245_001:0217b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有九十一子同名帝釋。",
        "from": "T21n1245_001:0217b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大勢力勇猛暴惡。",
        "from": "T21n1245_001:0217b26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見佛世尊歸依頂禮尊重恭敬。",
        "from": "T21n1245_001:0217b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀此非人而能禮敬彼藥叉主守護北方。",
        "from": "T21n1245_001:0217b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如佛行行如是護世。",
        "from": "T21n1245_001:0217b28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故我今稽首歸命正遍知明行足無上寂靜。",
        "from": "T21n1245_001:0217b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次北方世界。",
        "from": "T21n1245_001:0217c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人壽千歲命不中夭。",
        "from": "T21n1245_001:0217c01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地無耕種人不執作。",
        "from": "T21n1245_001:0217c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲食自然色香具足。",
        "from": "T21n1245_001:0217c02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "資益諸根身體光潤。",
        "from": "T21n1245_001:0217c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處處皆有花果樹木流泉池沼。",
        "from": "T21n1245_001:0217c03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意遊戲如受天樂。",
        "from": "T21n1245_001:0217c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是東方持國南方尾嚕茶迦。",
        "from": "T21n1245_001:0217c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方尾嚕博叉北方俱吠囉。",
        "from": "T21n1245_001:0217c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各以威德護四大洲。",
        "from": "T21n1245_001:0217c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次乾闥婆主。",
        "from": "T21n1245_001:0217c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有藥叉女眾有乘象者。",
        "from": "T21n1245_001:0217c07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有乘馬者有乘駝者。",
        "from": "T21n1245_001:0217c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有乘水牛者有乘羊者。",
        "from": "T21n1245_001:0217c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有乘蛇者有乘飛禽者。",
        "from": "T21n1245_001:0217c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有乘童男者有乘童女者。",
        "from": "T21n1245_001:0217c09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以象前引。",
        "from": "T21n1245_001:0217c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於虛空中密詣諸方。",
        "from": "T21n1245_001:0217c10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種變化隨意自在。",
        "from": "T21n1245_001:0217c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能守護人不可見。",
        "from": "T21n1245_001:0217c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若諸藥叉形容醜惡種種差異。",
        "from": "T21n1245_001:0217c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦如飛禽往虛空中。",
        "from": "T21n1245_001:0217c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自在遊行亦密護人。",
        "from": "T21n1245_001:0217c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其名曰阿吒曩吒俱曩吒波里俱娑曩吒曩拏曩拏布里迦等。",
        "from": "T21n1245_001:0217c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及藥叉女眾。",
        "from": "T21n1245_001:0217c14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆住北方阿拏迦嚩帝大城。",
        "from": "T21n1245_001:0217c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此大城有一宮殿。",
        "from": "T21n1245_001:0217c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其四邊有九十九池。",
        "from": "T21n1245_001:0217c16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水甚深廣名曰地池。",
        "from": "T21n1245_001:0217c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "泉源通流亦能降雨。",
        "from": "T21n1245_001:0217c17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有多種花果樹木。",
        "from": "T21n1245_001:0217c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂供俱婆迦俱囉囉迦麼花等。",
        "from": "T21n1245_001:0217c18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果味甘美眾所愛樂。",
        "from": "T21n1245_001:0217c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頻伽孔雀種種諸鳥常出妙音。",
        "from": "T21n1245_001:0217c19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼有天子名曰勇猛。",
        "from": "T21n1245_001:0217c20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并諸眷屬亦住是宮守護國界。",
        "from": "T21n1245_001:0217c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時毘沙門天王承佛慈力。",
        "from": "T21n1245_001:0217c21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次第宣說真言曰。",
        "from": "T21n1245_001:0217c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也二合他引伊梨彌梨伊隷緊吒母隷呬隷緊致母隷賀那彌護努里努二合彌矯里巘馱引里虞努呬儞烏枳護枳娑嚩二合引賀引爾時毘沙門天王說真言已。",
        "from": "T21n1245_001:0217c26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊。",
        "from": "T21n1245_001:0217c26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有諸大乾闥婆眾。",
        "from": "T21n1245_001:0217c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與我而為兄弟。",
        "from": "T21n1245_001:0217c27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其名曰欲麼覩樂麼覩歌麼多麼羅麼覩麼度麼多花醉恒醉吉祥醉財醉難禰迦青蓮華白蓮華月半尼羅俱枳羅成凍母嚕五髻妙色金麼拏尾輸俱蜜里二合賀娑波二合帝王野嚩帝惹誐覩誐帝。",
        "from": "T21n1245_001:0218a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等乾闥婆眾。",
        "from": "T21n1245_001:0218a03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迷惑惱亂一切眾生。",
        "from": "T21n1245_001:0218a03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有惱亂有情而不捨離者。",
        "from": "T21n1245_001:0218a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞此真言頭破作七分。",
        "from": "T21n1245_001:0218a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如阿梨樹枝真言曰。",
        "from": "T21n1245_001:0218a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也二合他引供覩梨供覩羅曀呬娑那引曼底計娑那伴尼底計底瑟吒二合努瑟吒二合滿度細娑嚩二合引賀引世尊若有聲聞苾芻苾芻尼優婆塞優婆夷。",
        "from": "T21n1245_001:0218a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此經中受持讀誦禮敬供養廣為人說。",
        "from": "T21n1245_001:0218a10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼乾闥婆及父母兄弟男女眷屬等。",
        "from": "T21n1245_001:0218a11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆不能為害。",
        "from": "T21n1245_001:0218a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常來親近侍奉衛護。",
        "from": "T21n1245_001:0218a12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有惱害者即失威力。",
        "from": "T21n1245_001:0218a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得乾闥婆三昧。",
        "from": "T21n1245_001:0218a13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭破作七分如阿梨樹枝。",
        "from": "T21n1245_001:0218a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不得往阿拏迦嚩帝大城中住。",
        "from": "T21n1245_001:0218a14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊若有聲聞苾芻苾芻尼優婆塞優婆夷。",
        "from": "T21n1245_001:0218a15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此經中受持讀誦。",
        "from": "T21n1245_001:0218a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮敬供養廣為人說。",
        "from": "T21n1245_001:0218a16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼閉舍左及父母兄弟男女眷屬等皆不能為害。",
        "from": "T21n1245_001:0218a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常來親近侍奉衛護。",
        "from": "T21n1245_001:0218a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有惱害者即失威力。",
        "from": "T21n1245_001:0218a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得閉舍左三昧。",
        "from": "T21n1245_001:0218a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭破作七分如阿梨樹枝亦不得往阿拏迦嚩帝大城中住。",
        "from": "T21n1245_001:0218a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1245_001:0218a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也二合他引護彌引護彌引護彌羅引嚩帝曳引娑嚩二合引賀引世尊復有鳩盤拏眾。",
        "from": "T21n1245_001:0218a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其名曰難多烏波難多　訖里二合計輸麼賀波囉輸二合　摩斛那囉捺舍賀娑覩二合部彌左嚕末麼　訖里二合瑟拏二合路呬多　阿婆囉嚩囉拏尾麼路禰里伽二合囉拏誐里惹曩　惹致路捺舍難尼阿里祖二合諾迦麼迦麼迦曀羅巘拏　訖里二合野虞般多怛囉野二合劍末羅跋捺里二合迦　薩里嚩二合囕誐唧怛囉二合芻等。",
        "from": "T21n1245_001:0218b02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是鳩盤拏眾。",
        "from": "T21n1245_001:0218b02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迷惑惱亂一切有情。",
        "from": "T21n1245_001:0218b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有惱亂者聞此真言。",
        "from": "T21n1245_001:0218b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭破作七分如阿梨樹枝。",
        "from": "T21n1245_001:0218b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1245_001:0218b04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也二合他引悉里尾悉里悉里尾悉里訖里二合瑟拏二合蹉誐隷迦囉尼卑孕二合誐隷娑嚩二合引賀引世尊若有聲聞苾芻苾芻尼優婆塞優婆夷。",
        "from": "T21n1245_001:0218b08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此經中受持讀誦。",
        "from": "T21n1245_001:0218b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮敬供養廣為人說。",
        "from": "T21n1245_001:0218b09_17",
        "context_with_next_sentence": true
    },
    {
