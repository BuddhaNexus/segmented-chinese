[
    {
        "source_sentence": "大聖文殊師利菩薩讚佛法身禮",
        "from": "T20n1195_001:0936c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并序",
        "from": "T20n1195_001:0936c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　此禮出大乘一切境界智光明莊",
        "from": "T20n1195_001:0936c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚴經開府儀同三司特進試鴻臚卿肅國公食邑三千戶賜紫贈司空諡大鑑正號大廣智大興善寺三藏沙門不空奉　詔譯皇帝以深仁馭宇。",
        "from": "T20n1195_001:0936c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大明燭物。",
        "from": "T20n1195_001:0936c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普灑甘露。",
        "from": "T20n1195_001:0936c12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "󰪏蕩黎元。",
        "from": "T20n1195_001:0936c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空叨沐聖慈。",
        "from": "T20n1195_001:0936c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "濫當翻譯。",
        "from": "T20n1195_001:0936c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "特奉恩命。",
        "from": "T20n1195_001:0936c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令集上都義學沙門良賁等一十六人。",
        "from": "T20n1195_001:0936c15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於內道場。",
        "from": "T20n1195_001:0936c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翻仁王護國般若及大乘蜜嚴等經畢。",
        "from": "T20n1195_001:0936c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願讚揚次。",
        "from": "T20n1195_001:0936c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於至覺冀介福於聖躬。",
        "from": "T20n1195_001:0936c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竊見大聖文殊師利菩薩讚佛法身經。",
        "from": "T20n1195_001:0936c18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "據其梵本有四十一禮。",
        "from": "T20n1195_001:0936c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先道所行但唯有十禮。",
        "from": "T20n1195_001:0936c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於文不備歎德未圓。",
        "from": "T20n1195_001:0936c20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恐乖聖者懇誠。",
        "from": "T20n1195_001:0936c20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又闕群生勝利。",
        "from": "T20n1195_001:0936c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空先有所持梵本。",
        "from": "T20n1195_001:0936c21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並皆具足今譯流傳庶裨弘益。",
        "from": "T20n1195_001:0936c22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其餘懺悔儀軌等並如舊本。",
        "from": "T20n1195_001:0936c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此不復云。",
        "from": "T20n1195_001:0936c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "于時。",
        "from": "T20n1195_001:0936c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大唐永泰元年維夏四月也。",
        "from": "T20n1195_001:0936c24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經云。",
        "from": "T20n1195_001:0936c24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T20n1195_001:0936c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛住王舍城鷲峯山中。",
        "from": "T20n1195_001:0936c25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大比丘眾二萬五千人俱。",
        "from": "T20n1195_001:0936c26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆是阿羅漢。",
        "from": "T20n1195_001:0936c26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大菩薩摩訶薩七十二那庾多俱胝。",
        "from": "T20n1195_001:0936c27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊師利菩薩而為上首。",
        "from": "T20n1195_001:0936c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時文殊師利菩薩。",
        "from": "T20n1195_001:0936c28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從座而起整理衣服。",
        "from": "T20n1195_001:0936c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偏袒右肩頂禮佛足。",
        "from": "T20n1195_001:0936c29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌恭敬稱揚如來說伽他曰。",
        "from": "T20n1195_001:0937a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無色無形相　　無根無住處",
        "from": "T20n1195_001:0937a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不生不滅故　　敬禮無所觀",
        "from": "T20n1195_001:0937a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不去亦不住　　不取亦不捨",
        "from": "T20n1195_001:0937a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離六入故　　敬禮無所觀",
        "from": "T20n1195_001:0937a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不住於諸法　　離有離無故",
        "from": "T20n1195_001:0937a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行於平等故　　敬禮無所觀",
        "from": "T20n1195_001:0937a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出過於三界　　等同於虛空",
        "from": "T20n1195_001:0937a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸欲不染故　　敬禮無所觀",
        "from": "T20n1195_001:0937a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸威儀中　　去來及睡寤",
        "from": "T20n1195_001:0937a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常在寂靜故　　敬禮無所觀",
        "from": "T20n1195_001:0937a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "去來悉平等　　已住於平等",
        "from": "T20n1195_001:0937a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不壞平等故　　敬禮無所觀",
        "from": "T20n1195_001:0937a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入諸無相定　　見諸法寂靜",
        "from": "T20n1195_001:0937a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常在三昧故　　敬禮無所觀",
        "from": "T20n1195_001:0937a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無住無所觀　　於法得自在",
        "from": "T20n1195_001:0937a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧用常定故　　敬禮無所觀",
        "from": "T20n1195_001:0937a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不住於六根　　不著於六境",
        "from": "T20n1195_001:0937a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常在一相故　　敬禮無所觀",
        "from": "T20n1195_001:0937a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入於無相中　　能斷於諸染",
        "from": "T20n1195_001:0937a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離名色故　　敬禮無所觀",
        "from": "T20n1195_001:0937a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不住於有相　　亦離於諸相",
        "from": "T20n1195_001:0937a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入相於無中　　敬禮無所觀",
        "from": "T20n1195_001:0937a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無分別思惟　　心住無所住",
        "from": "T20n1195_001:0937a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸念不起故　　敬禮無所觀",
        "from": "T20n1195_001:0937a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無藏識如空　　無染無戲論",
        "from": "T20n1195_001:0937a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離三世故　　敬禮無所觀",
        "from": "T20n1195_001:0937a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空無中邊　　諸佛心亦然",
        "from": "T20n1195_001:0937a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心同虛空故　　敬禮無所觀",
        "from": "T20n1195_001:0937a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛虛空相　　虛空亦無相",
        "from": "T20n1195_001:0937b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離諸因果故　　敬禮無所觀",
        "from": "T20n1195_001:0937b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不著於諸法　　如水月無取",
        "from": "T20n1195_001:0937b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離於我相　　敬禮無所觀",
        "from": "T20n1195_001:0937b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不住於諸蘊　　不著於處界",
        "from": "T20n1195_001:0937b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離顛倒故　　敬禮無所觀",
        "from": "T20n1195_001:0937b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常等於法界　　我見悉皆斷",
        "from": "T20n1195_001:0937b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離二邊故　　敬禮無所觀",
        "from": "T20n1195_001:0937b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不住於諸色　　非取亦非捨",
        "from": "T20n1195_001:0937b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離非法故　　敬禮無所觀",
        "from": "T20n1195_001:0937b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證無障礙法　　通達於諸法",
        "from": "T20n1195_001:0937b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離魔法故　　敬禮無所觀",
        "from": "T20n1195_001:0937b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非有亦非無　　有無不可得",
        "from": "T20n1195_001:0937b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離諸言說故　　敬禮無所觀",
        "from": "T20n1195_001:0937b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摧折我慢憧　　非一亦非二",
        "from": "T20n1195_001:0937b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離一二故　　敬禮無所觀",
        "from": "T20n1195_001:0937b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身口意無失　　三業常寂靜",
        "from": "T20n1195_001:0937b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離譬喻故　　敬禮無所觀",
        "from": "T20n1195_001:0937b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切智常住　　應現無功用",
        "from": "T20n1195_001:0937b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離諸過故　　敬禮無所觀",
        "from": "T20n1195_001:0937b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微妙無漏念　　無限無分別",
        "from": "T20n1195_001:0937b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "等情非情故　　敬禮無所觀",
        "from": "T20n1195_001:0937b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以心無礙故　　悉知一切心",
        "from": "T20n1195_001:0937b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不住自他故　　敬禮無所觀",
        "from": "T20n1195_001:0937b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無礙無所觀　　常住無礙法",
        "from": "T20n1195_001:0937b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離諸心故　　敬禮無所觀",
        "from": "T20n1195_001:0937b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心常無所緣　　自性不可得",
        "from": "T20n1195_001:0937b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平等難量故　　敬禮無所觀",
        "from": "T20n1195_001:0937b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以無所依心　　悉見諸剎土",
        "from": "T20n1195_001:0937b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知諸有情故　　敬禮無所觀",
        "from": "T20n1195_001:0937c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸法薩婆若　　畢竟無所有",
        "from": "T20n1195_001:0937c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛心難測故　　敬禮無所觀",
        "from": "T20n1195_001:0937c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸法由如幻　　如幻不可得",
        "from": "T20n1195_001:0937c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離諸幻法故　　敬禮無所觀",
        "from": "T20n1195_001:0937c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛常在世間　　而不染世法",
        "from": "T20n1195_001:0937c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不染世間故　　敬禮無所觀",
        "from": "T20n1195_001:0937c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切智常住　　空性空境界",
        "from": "T20n1195_001:0937c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言說亦空故　　敬禮無所觀",
        "from": "T20n1195_001:0937c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證無分別定　　得如幻三昧",
        "from": "T20n1195_001:0937c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遊戲神通故　　敬禮無所觀",
        "from": "T20n1195_001:0937c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非一亦非異　　非近亦非遠",
        "from": "T20n1195_001:0937c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於法不動故　　敬禮無所觀",
        "from": "T20n1195_001:0937c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一念金剛定　　剎那成等覺",
        "from": "T20n1195_001:0937c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證無影像故　　敬禮無所觀",
        "from": "T20n1195_001:0937c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸三世法　　成就諸方便",
        "from": "T20n1195_001:0937c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不動涅槃故　　敬禮無所觀",
        "from": "T20n1195_001:0937c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "涅槃常不動　　無此岸彼岸",
        "from": "T20n1195_001:0937c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通達方便故　　敬禮無所觀",
        "from": "T20n1195_001:0937c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無相無所有　　無患無戲論",
        "from": "T20n1195_001:0937c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不住有無故　　敬禮無所觀",
        "from": "T20n1195_001:0937c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智處悉平等　　寂靜無分別",
        "from": "T20n1195_001:0937c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自他一相故　　敬禮無所觀",
        "from": "T20n1195_001:0937c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切平等禮　　無禮無不禮",
        "from": "T20n1195_001:0937c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一禮遍含識　　同歸實相體",
        "from": "T20n1195_001:0937c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊讚文殊師利菩薩言。",
        "from": "T20n1195_001:0937c26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉。",
        "from": "T20n1195_001:0937c26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今善說如來功德一切諸法本來清淨。",
        "from": "T20n1195_001:0937c27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊師利假使有人。",
        "from": "T20n1195_001:0937c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教化三千大千世界一切有情。",
        "from": "T20n1195_001:0937c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成辟支佛。",
        "from": "T20n1195_001:0937c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不如有人聞此功德一念信解。",
        "from": "T20n1195_001:0937c29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即超過彼百千萬倍。",
        "from": "T20n1195_001:0938a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是展轉無能稱讚譬喻挍量。",
        "from": "T20n1195_001:0938a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具如本經所說。",
        "from": "T20n1195_001:0938a02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大聖文殊師利菩薩讚佛法身禮",
        "from": "T20n1195_001:0938a03_100",
        "context_with_next_sentence": false
    }
]