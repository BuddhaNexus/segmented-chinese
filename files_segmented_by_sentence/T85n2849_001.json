[
    {
        "source_sentence": "大乘四齋日",
        "from": "T85n2849_001:1299c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二月八日。四月八日。五月八日。七月十五日。",
        "from": "T85n2849_001:1299c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年三長齋月。正月五月九月。六齋日。八日十",
        "from": "T85n2849_001:1299c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四日十五日二十三日二十八日三十日。",
        "from": "T85n2849_001:1299c14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十齋日。",
        "from": "T85n2849_001:1299c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月一日善惡憍童子下念。",
        "from": "T85n2849_001:1299c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十四日察命伺錄下。",
        "from": "T85n2849_001:1299c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十五日五道大神下。",
        "from": "T85n2849_001:1299c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十八日閻羅王下。",
        "from": "T85n2849_001:1299c16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十三日天大將軍下。",
        "from": "T85n2849_001:1299c17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十四日帝釋下。",
        "from": "T85n2849_001:1299c17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十八日太山府軍下。",
        "from": "T85n2849_001:1299c18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十九日四天王下。",
        "from": "T85n2849_001:1299c18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十日天曹地府下。",
        "from": "T85n2849_001:1299c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日童子下念定光如來佛除齋除罪四十劫不墮刀槍地獄。",
        "from": "T85n2849_001:1299c20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八日太子下念藥師琉璃光佛除齋除罪三十劫不墮粉草地獄。",
        "from": "T85n2849_001:1299c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十四日察命下念賢劫千佛除齋除罪一千劫不墮鑊湯地獄。",
        "from": "T85n2849_001:1299c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十五日五道大將軍下念阿彌陀佛除齋除罪二百劫不墮寒氷地獄。",
        "from": "T85n2849_001:1299c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十八日閻羅王下念觀世音菩薩除齋除罪九十劫不墮劍樹地獄。",
        "from": "T85n2849_001:1299c26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十三日天大將軍下念盧舍那佛除齋除罪一千劫不墮餓鬼地獄。",
        "from": "T85n2849_001:1299c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十四日太山府君下念地藏菩薩除齋除罪九十劫不墮纔截地獄。",
        "from": "T85n2849_001:1299c29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十八日天帝釋下念阿彌陀佛除齋除罪一千劫不墮鐵鋸地獄。",
        "from": "T85n2849_001:1300a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十九日四天王下念藥王藥上菩薩除齋除罪七千劫不墮磑磨地獄。",
        "from": "T85n2849_001:1300a03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十日大梵天王下念釋迦牟尼佛除齋除罪八千劫不墮寒氷。",
        "from": "T85n2849_001:1300a05_6",
        "context_with_next_sentence": true
    }
]