[
    {
        "source_sentence": "佛說穰麌梨童女經",
        "from": "T21n1264b_001:0293a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐三藏沙門大廣智不空奉　詔譯",
        "from": "T21n1264b_001:0293a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。一時薄伽梵。住舍衛國祇樹給",
        "from": "T21n1264b_001:0293a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孤獨園。",
        "from": "T21n1264b_001:0293a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大苾芻眾千二百五十人俱。",
        "from": "T21n1264b_001:0293a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有無量菩薩摩訶薩。",
        "from": "T21n1264b_001:0293a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸天龍藥叉乾闥婆阿修羅迦樓羅緊那羅摩睺羅伽人非人等皆來集會。",
        "from": "T21n1264b_001:0293a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告諸苾芻。",
        "from": "T21n1264b_001:0293a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我念往昔。",
        "from": "T21n1264b_001:0293a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住雪山北遊香醉山。",
        "from": "T21n1264b_001:0293a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見一童女。",
        "from": "T21n1264b_001:0293a16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百福相好莊嚴其身。",
        "from": "T21n1264b_001:0293a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鹿皮為衣以諸毒蛇而為瓔珞。",
        "from": "T21n1264b_001:0293a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將諸毒蟲蚖蝮之類。",
        "from": "T21n1264b_001:0293a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後圍繞常為伴戲。",
        "from": "T21n1264b_001:0293a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲毒漿食毒果。",
        "from": "T21n1264b_001:0293a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼女見我謂言。",
        "from": "T21n1264b_001:0293a19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仁者當知聽我宣說穰麌梨真言。",
        "from": "T21n1264b_001:0293a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除世間一切諸毒。",
        "from": "T21n1264b_001:0293a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人聞此真言。",
        "from": "T21n1264b_001:0293a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及持我名者。",
        "from": "T21n1264b_001:0293a21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不被一切諸毒所害。",
        "from": "T21n1264b_001:0293a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時童女為我宣說。",
        "from": "T21n1264b_001:0293a22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我從彼女聞已。",
        "from": "T21n1264b_001:0293a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常持此法饒益有情。",
        "from": "T21n1264b_001:0293a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今當說真言曰。",
        "from": "T21n1264b_001:0293a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也二合他去聲引唵引壹里蜜帝引一底丁以切里蜜帝引二壹里底里蜜帝引三努鼻迷努鼻麼引里引曳平聲引四訥泚訥蹉去聲引里引曳平聲五得羯囉抳尼呈切鼻六嚩無博切羯囉抳鼻上同七羯濕弭二合引㘑引八羯濕弭二合引囉穆訖帝二合引九惡祇祇曳切十惡伽去聲寧十一惡伽去聲曩伽寧十二壹里曳十三壹里壹里引曳十四阿上聲佉去聲引夜引曳平聲十五姶烏合切播引夜引曳平聲十六濕吠二合引帝引十七濕吠二合引多頓嬭引十八阿上曩引努鼻囉乞曬二合引娑嚩二合引賀十九佛告諸苾芻。",
        "from": "T21n1264b_001:0293b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人一切聞陀羅尼。",
        "from": "T21n1264b_001:0293b06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "却後七年遠離一切諸毒。",
        "from": "T21n1264b_001:0293b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若常受持。",
        "from": "T21n1264b_001:0293b07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切毒蟲及諸毒藥悉不能害。",
        "from": "T21n1264b_001:0293b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有毒蛇來嚙此人者。",
        "from": "T21n1264b_001:0293b08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭破作七分猶如蘭香䔠。",
        "from": "T21n1264b_001:0293b09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其有受持此真言法。",
        "from": "T21n1264b_001:0293b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應以白物先供養師。",
        "from": "T21n1264b_001:0293b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後受持必獲成就。",
        "from": "T21n1264b_001:0293b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苾芻當知。",
        "from": "T21n1264b_001:0293b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿於蛇前稱誦此明。",
        "from": "T21n1264b_001:0293b11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其蛇必死。",
        "from": "T21n1264b_001:0293b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當以此真言。",
        "from": "T21n1264b_001:0293b12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持死蛇令其蘇息。",
        "from": "T21n1264b_001:0293b12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1264b_001:0293b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也二合他去聲引一唵引壹羅引二尾羅引三斫句引嚩句去聲引四句引拏引句嬭引底丁以切五報引拏引報引嬭引底同前𩕳矩嚕拏引六𩕳矩魯嬭引底同前七普吒囉蒵引普吒囉蒵引八普吒𧹞讁簡切拏囉蒵引九薩跛囉蒵引十薩跛吒𧹞准上音拏囉蒵引十一曩引誐囉蒵引十二曩引誐吒𧹞同前拏囉蒵引十三阿上泚引蹉禮引十四揆攞尾曬引十五試引帝引試引多嚩寧引十六滿跢引黎引十七伊上里黎引十八賀黎賀黎十九旦嬭引旦嬭引二十怛𪘨二十一娑普二合吒娑普二合吒娑嚩二合賀引二十二爾時世尊說此陀羅尼已。",
        "from": "T21n1264b_001:0293b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告諸苾芻。",
        "from": "T21n1264b_001:0293b25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我此真言能解世間一切諸毒。",
        "from": "T21n1264b_001:0293b26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蟲毒魅毒蠱毒藥毒等不能為害。",
        "from": "T21n1264b_001:0293b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有被毒中者。",
        "from": "T21n1264b_001:0293b27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此真言加持皆得消滅。",
        "from": "T21n1264b_001:0293c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸苾芻此穰麌梨陀羅尼。",
        "from": "T21n1264b_001:0293c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切如來大會中說。",
        "from": "T21n1264b_001:0293c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真實不虛不顛倒語。",
        "from": "T21n1264b_001:0293c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如語不異語諸有藥毒呪毒蟲毒魅毒。",
        "from": "T21n1264b_001:0293c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲來相害。",
        "from": "T21n1264b_001:0293c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令却著本所與者。",
        "from": "T21n1264b_001:0293c04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令使諸毒入水入火入柱入壁。",
        "from": "T21n1264b_001:0293c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦令入地。",
        "from": "T21n1264b_001:0293c05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有諸毒不令成毒。",
        "from": "T21n1264b_001:0293c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉能除滅。",
        "from": "T21n1264b_001:0293c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人受持此經日誦一遍。",
        "from": "T21n1264b_001:0293c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非但滅世間諸毒。",
        "from": "T21n1264b_001:0293c07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能除滅身中三毒。",
        "from": "T21n1264b_001:0293c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊。",
        "from": "T21n1264b_001:0293c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說穰麌梨童女隨心真言及成就法。",
        "from": "T21n1264b_001:0293c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行者欲成就此法。",
        "from": "T21n1264b_001:0293c09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先斷五辛亦不食鹽不食油。",
        "from": "T21n1264b_001:0293c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷語於一淨處。",
        "from": "T21n1264b_001:0293c10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三時澡浴三時換衣。",
        "from": "T21n1264b_001:0293c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結印誦隨心真言滿一萬遍。",
        "from": "T21n1264b_001:0293c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則行法成就。",
        "from": "T21n1264b_001:0293c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復作一切事必獲成就隨心真言曰。",
        "from": "T21n1264b_001:0293c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引阿上聲枲思以切爾賀二合引一戌引攞爾賀吠二合引嚩囉二合迦引臾引三仡囉三合娑仡囉二合娑四入嚩二合羅入嚩二合羅五摩賀引迦引里六摩賀引喻祇霓夷切引七濕嚩二合里引曳引八唵引頗𪘨引九普吒囉奚引娑嚩二合引賀引十吽癹吒上半聲娑嚩二合引賀引十一我今復說穰虞梨印及觀行法。",
        "from": "T21n1264b_001:0293c20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其根本印。",
        "from": "T21n1264b_001:0293c20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手相搏如掬物勢。",
        "from": "T21n1264b_001:0293c21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二小指相並。",
        "from": "T21n1264b_001:0293c21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘八指各散開微曲即成。",
        "from": "T21n1264b_001:0293c22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結此印誦前根本真言。",
        "from": "T21n1264b_001:0293c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持自身五處。",
        "from": "T21n1264b_001:0293c23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂右肩左肩心喉額等項上散印。",
        "from": "T21n1264b_001:0293c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結隨心印。",
        "from": "T21n1264b_001:0293c24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手五指散開。",
        "from": "T21n1264b_001:0293c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微屈如師子爪形即成。",
        "from": "T21n1264b_001:0293c25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結此印誦隨心真言七遍加持五處。",
        "from": "T21n1264b_001:0293c26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行者作先行成就法已。",
        "from": "T21n1264b_001:0293c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲作法除毒之時。",
        "from": "T21n1264b_001:0293c27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀想自身為穰麌梨童女。",
        "from": "T21n1264b_001:0293c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身綠色狀如龍女。",
        "from": "T21n1264b_001:0293c28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足七頭項有圓光。",
        "from": "T21n1264b_001:0294a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應想四臂。",
        "from": "T21n1264b_001:0294a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右第一手持三戟叉第二手執三五莖孔雀尾。",
        "from": "T21n1264b_001:0294a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左第一手把一黑蛇第二手施無畏。",
        "from": "T21n1264b_001:0294a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又想七寶瓔珞耳璫環釧。",
        "from": "T21n1264b_001:0294a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臂脚釧莊嚴其身。",
        "from": "T21n1264b_001:0294a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并以諸蛇用為瓔珞。",
        "from": "T21n1264b_001:0294a04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想從一一毛孔流出火焰。",
        "from": "T21n1264b_001:0294a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作此觀已於被螫人前。",
        "from": "T21n1264b_001:0294a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結根本印及隨心印。",
        "from": "T21n1264b_001:0294a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持自身五處。",
        "from": "T21n1264b_001:0294a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取一熟銅椀盛水。",
        "from": "T21n1264b_001:0294a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦隨心真言加持七遍。",
        "from": "T21n1264b_001:0294a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手掬水打被螫人心上。",
        "from": "T21n1264b_001:0294a08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有毒氣漸漸銷除。",
        "from": "T21n1264b_001:0294a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於真言句中增加此句。",
        "from": "T21n1264b_001:0294a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂。",
        "from": "T21n1264b_001:0294a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左攞娑嚩二合引賀引尾鑠疏覺切娑縛二合引賀引即取淨土加持七遍。",
        "from": "T21n1264b_001:0294a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周匝圍繞被螫之人。",
        "from": "T21n1264b_001:0294a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸毒應時銷滅。",
        "from": "T21n1264b_001:0294a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法於被螫人前。",
        "from": "T21n1264b_001:0294a13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結本印數誦吽字一字真言。",
        "from": "T21n1264b_001:0294a14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或誦撲字一字真言加持。",
        "from": "T21n1264b_001:0294a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆能除毒。",
        "from": "T21n1264b_001:0294a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若常受持此穰麌梨法。",
        "from": "T21n1264b_001:0294a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能滅世間一切諸毒。",
        "from": "T21n1264b_001:0294a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求無不遂心。",
        "from": "T21n1264b_001:0294a16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊說此經已。",
        "from": "T21n1264b_001:0294a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時彼大會天龍藥叉人非人等。",
        "from": "T21n1264b_001:0294a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛所說皆大歡喜信受奉行。",
        "from": "T21n1264b_001:0294a18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說穰麌梨童女經",
        "from": "T21n1264b_001:0294a19_100",
        "context_with_next_sentence": false
    }
]