[
    {
        "source_sentence": "金有陀羅尼經",
        "from": "T85n2910_001:1455c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。一時薄伽梵住如籬𥬕。與藥叉",
        "from": "T85n2910_001:1455c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□金剛手俱。",
        "from": "T85n2910_001:1455c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時天百施往世尊所。",
        "from": "T85n2910_001:1455c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到已頂禮佛足。",
        "from": "T85n2910_001:1455c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "退□□坐一面已。",
        "from": "T85n2910_001:1455c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天帝百施白佛言。",
        "from": "T85n2910_001:1455c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "T85n2910_001:1455c21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我入戰陣而鬪戰時。",
        "from": "T85n2910_001:1455c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以阿修羅幻惑呪術藥力墮於負處□已不。",
        "from": "T85n2910_001:1455c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯然願世尊。",
        "from": "T85n2910_001:1455c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈愍於我為令摧伏阿修羅眾幻惑呪術及藥力故。",
        "from": "T85n2910_001:1455c24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善說最勝大密之呪。",
        "from": "T85n2910_001:1455c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時薄伽梵告天帝百施曰。",
        "from": "T85n2910_001:1455c25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憍尸迦如是如是。",
        "from": "T85n2910_001:1455c26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與阿修羅而鬪戰時。",
        "from": "T85n2910_001:1455c26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實以明呪祕呪藥力而墮負處。",
        "from": "T85n2910_001:1455c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憍尸迦。",
        "from": "T85n2910_001:1455c27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為哀愍故今說明呪。",
        "from": "T85n2910_001:1455c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲令幻惑時呪退散鬪戰諍訟悉皆消滅。",
        "from": "T85n2910_001:1455c28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切祕呪及諸藥等而得斷除。",
        "from": "T85n2910_001:1455c29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說於明呪。",
        "from": "T85n2910_001:1455c29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時薄伽梵說大金有明呪曰。",
        "from": "T85n2910_001:1456a01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今為說三無數劫。",
        "from": "T85n2910_001:1456a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸餘外道行者遍遊罣形。",
        "from": "T85n2910_001:1456a02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而起惡思作諸障礙。",
        "from": "T85n2910_001:1456a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我從彼來所有幻惑一切明呪悉能降伏。",
        "from": "T85n2910_001:1456a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六度圓滿。",
        "from": "T85n2910_001:1456a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷除諸餘外道行者遍遊罣形諸惱亂。",
        "from": "T85n2910_001:1456a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因明呪祕呪藥及一切諸魔朋黨大明之呪。",
        "from": "T85n2910_001:1456a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憍尸迦。",
        "from": "T85n2910_001:1456a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當攝受諸有情故受持最勝大祕密呪。",
        "from": "T85n2910_001:1456a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天帝白言。",
        "from": "T85n2910_001:1456a07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是世尊。",
        "from": "T85n2910_001:1456a07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯然受教。",
        "from": "T85n2910_001:1456a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊。",
        "from": "T85n2910_001:1456a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說金有大明呪曰。",
        "from": "T85n2910_001:1456a08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛也他唵　希儞希儞　希離希離　命離命離　希明離　儞希儞希　儞希囉祕儞　乾佐那波羝　哺羝哆滿怛囉　阿地訖梨羝　閉羝閉羝　閉哆滿怛囉　阿地迦囉羝　訶那訶那　訶婆訶婆　親駄親駄　頻那頻那　薄伽跋羝　佐曳祕佐曳　攢婆儞悉談婆儞　畔駄儞　牟訶儞　阿牟伽爍羝　馱囉儞　訖利那訖梨那　迦多㮈磨那婆　攢婆也攢婆也　畔佐也畔佐也　悉談婆也　悉噉婆也　畔馱也畔馱也　牟訶也牟訶也所有一切。",
        "from": "T85n2910_001:1456a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若天幻惑。",
        "from": "T85n2910_001:1456a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若龍幻惑。",
        "from": "T85n2910_001:1456a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若藥叉幻惑。",
        "from": "T85n2910_001:1456a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若羅剎幻惑。",
        "from": "T85n2910_001:1456a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若緊那羅幻惑。",
        "from": "T85n2910_001:1456a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若乾闥婆幻惑。",
        "from": "T85n2910_001:1456a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若阿修羅幻惑。",
        "from": "T85n2910_001:1456a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若莫呼洛迦幻惑。",
        "from": "T85n2910_001:1456a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若大腹行幻惑。",
        "from": "T85n2910_001:1456a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若持明呪幻惑。",
        "from": "T85n2910_001:1456a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若持明呪成就王幻惑。",
        "from": "T85n2910_001:1456a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若仙幻惑。",
        "from": "T85n2910_001:1456a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若持一切明呪幻惑。",
        "from": "T85n2910_001:1456a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若群生幻惑。",
        "from": "T85n2910_001:1456a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一切幻惑。",
        "from": "T85n2910_001:1456a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "囉羅囉羅囉佐也囉佐也　妬磨覩磨　妬妬磨　囉婆囉婆　囉婆那　作割蘭單　伽蘭他儞　訶那訶那　薩婆鞞哆　奢咄嚧難　悉談婆也　秀延悉談婆也　蘇南悉談婆也　婆嚧難悉談婆也　惡儞寅悉談婆也　欝乾　哆梨哆梨　梨馱　囉羝惟　馱囉寧　波奢訶悉羝　弊奢他也婆世那　若有於我能為怨敵諸賊嗔恚具惡心鬪諍極諍欲作一切無利益者　訶那訶那　哆訶哆訶　波佐波佐　半佐也半佐也　攢婆也攢婆也　悉談婆也悉談婆也　半馱也半馱也　牟訶也牟訶也　摩訶牟訶儞　薄伽跋羝䓾訶　於一切怖畏嬈惱疾疫願守護我叺馱䓾訶憍尸迦。",
        "from": "T85n2910_001:1456b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子若善女人。",
        "from": "T85n2910_001:1456b09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若王若王大臣。",
        "from": "T85n2910_001:1456b09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能憶念此金有明呪者。",
        "from": "T85n2910_001:1456b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼無他怖畏。",
        "from": "T85n2910_001:1456b10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彼部黨他所敵軍不能侵惱。",
        "from": "T85n2910_001:1456b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦非天亦非龍。",
        "from": "T85n2910_001:1456b11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦非藥叉亦非乾闥婆。",
        "from": "T85n2910_001:1456b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦非阿修羅。",
        "from": "T85n2910_001:1456b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦非緊那羅。",
        "from": "T85n2910_001:1456b12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦非莫呼洛迦。",
        "from": "T85n2910_001:1456b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦非持明呪者。",
        "from": "T85n2910_001:1456b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦非飛空母等。",
        "from": "T85n2910_001:1456b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不非時而持壽命明呪祕呪一切諸藥不能為害。",
        "from": "T85n2910_001:1456b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他所敵軍不能侵遶。",
        "from": "T85n2910_001:1456b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他所敵軍而不傷命。",
        "from": "T85n2910_001:1456b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀不能害。",
        "from": "T85n2910_001:1456b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水火毒藥明呪祕呪一切諸藥而不能侵。",
        "from": "T85n2910_001:1456b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還著於彼自作教隨喜造罪彼之處所。",
        "from": "T85n2910_001:1456b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憍尸迦。",
        "from": "T85n2910_001:1456b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故淨信苾芻苾芻尼。",
        "from": "T85n2910_001:1456b18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烏波索迦烏波斯迦。",
        "from": "T85n2910_001:1456b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子善女人等。",
        "from": "T85n2910_001:1456b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此明呪呪水七遍。",
        "from": "T85n2910_001:1456b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自洗其身能護於身。",
        "from": "T85n2910_001:1456b20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有欲令於一切怖畏一切嬈惱一切疫疾一切明呪一切祕呪一切諸藥一切厭蠱而超過者。",
        "from": "T85n2910_001:1456b22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當念此金有明呪。",
        "from": "T85n2910_001:1456b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若王若王大臣。",
        "from": "T85n2910_001:1456b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲摧他軍眾伏他軍眾。",
        "from": "T85n2910_001:1456b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦當念此金有明呪。",
        "from": "T85n2910_001:1456b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若呪線七遍作七結已繫於身上。",
        "from": "T85n2910_001:1456b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若呪水七遍能護自身。",
        "from": "T85n2910_001:1456b25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有書寫於一切怖畏無障礙陀羅尼。",
        "from": "T85n2910_001:1456b26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或能受持。",
        "from": "T85n2910_001:1456b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或繫脛下若置高幢入軍陣者。",
        "from": "T85n2910_001:1456b27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善安得脫。",
        "from": "T85n2910_001:1456b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此金呪威神之力。",
        "from": "T85n2910_001:1456b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內族眷囑善安超過。",
        "from": "T85n2910_001:1456b28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未成能成。",
        "from": "T85n2910_001:1456b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲摧伏諸明呪者。",
        "from": "T85n2910_001:1456b29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於白線上呪七遍已作七結者。",
        "from": "T85n2910_001:1456c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能繫摧伏。",
        "from": "T85n2910_001:1456c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲摧諸幻惑者。",
        "from": "T85n2910_001:1456c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取塚間土呪七遍已而散擲者。",
        "from": "T85n2910_001:1456c02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能摧幻惑。",
        "from": "T85n2910_001:1456c02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論競之時欲禁其口。",
        "from": "T85n2910_001:1456c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取秦萩蘺呪七遍已而嚙嚼者。",
        "from": "T85n2910_001:1456c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切言論悉能對答。",
        "from": "T85n2910_001:1456c04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持讀誦而稱讚者。",
        "from": "T85n2910_001:1456c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸罪悉皆消滅。",
        "from": "T85n2910_001:1456c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "却往於彼造作之者及思惟所。",
        "from": "T85n2910_001:1456c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或繫於繩及水自護者。",
        "from": "T85n2910_001:1456c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彼身。",
        "from": "T85n2910_001:1456c06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上。",
        "from": "T85n2910_001:1456c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切明呪祕呪諸藥不能為害。",
        "from": "T85n2910_001:1456c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未成辨者悉能成辨。",
        "from": "T85n2910_001:1456c08_4",
        "context_with_next_sentence": true
    },
    {
