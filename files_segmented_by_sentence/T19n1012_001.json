[
    {
        "source_sentence": "佛說出生無量門持經",
        "from": "T19n1012_001:0682b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東晉天竺三藏佛陀跋陀羅譯",
        "from": "T19n1012_001:0682b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。一時佛在毘舍離大林精舍重閣",
        "from": "T19n1012_001:0682b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "講堂。",
        "from": "T19n1012_001:0682b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大比丘眾四萬人俱。",
        "from": "T19n1012_001:0682b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊欲捨壽行。",
        "from": "T19n1012_001:0682b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "却後三月當般涅槃。",
        "from": "T19n1012_001:0682b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便告長老大目揵連。",
        "from": "T19n1012_001:0682b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝行遍告三千大千世界。",
        "from": "T19n1012_001:0682b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聲聞比丘并緣覺道。",
        "from": "T19n1012_001:0682b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸菩薩皆悉來會大林精舍重閣講堂。",
        "from": "T19n1012_001:0682b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目連受教往須彌頂。",
        "from": "T19n1012_001:0682b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入如意定以大音聲。",
        "from": "T19n1012_001:0682b18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍告三千大千世界無不聞知。",
        "from": "T19n1012_001:0682b19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長老舍利弗作是念。",
        "from": "T19n1012_001:0682b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今亦當入如意定。",
        "from": "T19n1012_001:0682b20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普請依止閻浮提內諸比丘。",
        "from": "T19n1012_001:0682b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆悉來會大林精舍重閣講堂。",
        "from": "T19n1012_001:0682b21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是念已即如其像。",
        "from": "T19n1012_001:0682b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入于三昧遍請諸比丘。",
        "from": "T19n1012_001:0682b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依止閻浮提者皆來集會。",
        "from": "T19n1012_001:0682b23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時大林精舍重閣講堂。",
        "from": "T19n1012_001:0682b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有三百萬比丘眾會。",
        "from": "T19n1012_001:0682b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是世尊告現無癡菩薩文殊師利童子。",
        "from": "T19n1012_001:0682b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離惡趣菩薩無憂冥菩薩。",
        "from": "T19n1012_001:0682b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離蔭蓋菩薩寂諸境界菩薩。",
        "from": "T19n1012_001:0682b26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀世音菩薩香象菩薩。",
        "from": "T19n1012_001:0682b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量辯菩薩彌勒菩薩。",
        "from": "T19n1012_001:0682b27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等善男子行詣十方恒河沙諸佛國土。",
        "from": "T19n1012_001:0682b28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍告諸一生補處菩薩。",
        "from": "T19n1012_001:0682b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不退轉菩薩得不起法忍菩薩信解菩薩。",
        "from": "T19n1012_001:0682c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆悉來集大林精舍重閣講堂。",
        "from": "T19n1012_001:0682c01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸菩薩即受教行。",
        "from": "T19n1012_001:0682c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍告十方恒沙佛土諸一生補處菩薩。",
        "from": "T19n1012_001:0682c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及不退轉得不起法忍信解菩薩。",
        "from": "T19n1012_001:0682c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆來集會大林精舍重閣講堂。",
        "from": "T19n1012_001:0682c04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時大林精舍重閣講堂。",
        "from": "T19n1012_001:0682c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有八十億百千一生補處菩薩。",
        "from": "T19n1012_001:0682c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一億百千得不退轉。",
        "from": "T19n1012_001:0682c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十億菩薩得不起法忍。",
        "from": "T19n1012_001:0682c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六十億菩薩得信解脫。",
        "from": "T19n1012_001:0682c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸大士一時來會大林精舍重閣講堂。",
        "from": "T19n1012_001:0682c08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時尊者舍利弗。",
        "from": "T19n1012_001:0682c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見諸菩薩大眾雲集。",
        "from": "T19n1012_001:0682c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便作是念我當承佛威神敬問世尊。",
        "from": "T19n1012_001:0682c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸菩薩得離疑網。",
        "from": "T19n1012_001:0682c10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疾成甚深無閡辯才。",
        "from": "T19n1012_001:0682c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逮聞十方恒沙剎土。",
        "from": "T19n1012_001:0682c11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛說法念持不忘。",
        "from": "T19n1012_001:0682c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疾成無上正真之道為最正覺。",
        "from": "T19n1012_001:0682c12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又使菩薩得四無盡清淨法門。",
        "from": "T19n1012_001:0682c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為四。",
        "from": "T19n1012_001:0682c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一曰眾生淨。",
        "from": "T19n1012_001:0682c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二曰法淨。",
        "from": "T19n1012_001:0682c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三曰辯才淨。",
        "from": "T19n1012_001:0682c14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四曰佛土嚴淨。",
        "from": "T19n1012_001:0682c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得四最勝和悅之法。",
        "from": "T19n1012_001:0682c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為四。",
        "from": "T19n1012_001:0682c15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一曰身和悅。",
        "from": "T19n1012_001:0682c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二曰口和悅。",
        "from": "T19n1012_001:0682c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三曰意和悅。",
        "from": "T19n1012_001:0682c16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四曰方便和悅。",
        "from": "T19n1012_001:0682c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疾成四種微妙持門。",
        "from": "T19n1012_001:0682c17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為四。",
        "from": "T19n1012_001:0682c17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一曰出生無量門持。",
        "from": "T19n1012_001:0682c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二曰甚深法忍持。",
        "from": "T19n1012_001:0682c18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三曰善於眾生諸根持。",
        "from": "T19n1012_001:0682c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四曰善於眾生因果持。",
        "from": "T19n1012_001:0682c19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊者舍利弗思惟。",
        "from": "T19n1012_001:0682c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是菩薩清淨無量慧地。",
        "from": "T19n1012_001:0682c20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲令佛說。",
        "from": "T19n1012_001:0682c20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以是義上問世尊。",
        "from": "T19n1012_001:0682c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是世尊讚舍利弗言。",
        "from": "T19n1012_001:0682c21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉舍利弗。",
        "from": "T19n1012_001:0682c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃能為諸菩薩問如是義。",
        "from": "T19n1012_001:0682c22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝舍利弗諦聽諦聽善思念之。",
        "from": "T19n1012_001:0682c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今當說菩薩所行疾成無上正真之道甚深功德。",
        "from": "T19n1012_001:0682c24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若舍利弗欲得一切智知一切法者。",
        "from": "T19n1012_001:0682c25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當學此門持當學此持句。",
        "from": "T19n1012_001:0682c26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂無句正句普句。",
        "from": "T19n1012_001:0682c26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就樂說光明順道。",
        "from": "T19n1012_001:0682c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善分別究竟分別。",
        "from": "T19n1012_001:0682c27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅固所說堅固伊羅伊梨。",
        "from": "T19n1012_001:0682c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伊羅悉帝旃貳。",
        "from": "T19n1012_001:0682c28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為履上迹不動寂靜。",
        "from": "T19n1012_001:0682c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離諸怨敵熾然永滅。",
        "from": "T19n1012_001:0682c29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出生無垢清淨自性。",
        "from": "T19n1012_001:0682c29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "巧說諸有者無所有無所著。",
        "from": "T19n1012_001:0683a01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善能降伏光明離垢。",
        "from": "T19n1012_001:0683a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善自攝持果而大勇。",
        "from": "T19n1012_001:0683a02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得大名稱動無動。",
        "from": "T19n1012_001:0683a02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以正動難堅固善住。",
        "from": "T19n1012_001:0683a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安隱遊無閡著開諸法門。",
        "from": "T19n1012_001:0683a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨順所應強而有勢。",
        "from": "T19n1012_001:0683a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精進勇猛得方便力。",
        "from": "T19n1012_001:0683a04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大光普照明曜無垢。",
        "from": "T19n1012_001:0683a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "意解平等普無不入。",
        "from": "T19n1012_001:0683a05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "事無不逝。",
        "from": "T19n1012_001:0683a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此善妙持諸佛所住。",
        "from": "T19n1012_001:0683a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是舍利弗行持菩薩。",
        "from": "T19n1012_001:0683a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於數無數不以分別亦無所得。",
        "from": "T19n1012_001:0683a07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸斷法增進功德。",
        "from": "T19n1012_001:0683a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而無所作亦無不作。",
        "from": "T19n1012_001:0683a08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不見諸法有合有離。",
        "from": "T19n1012_001:0683a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有起有滅。",
        "from": "T19n1012_001:0683a09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦無去來現在之知。",
        "from": "T19n1012_001:0683a09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不知法有力無力。",
        "from": "T19n1012_001:0683a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行念佛不念相好。",
        "from": "T19n1012_001:0683a10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不念種性亦不念眷屬。",
        "from": "T19n1012_001:0683a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不戒不定不慧。",
        "from": "T19n1012_001:0683a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不解亦無解脫度知見念。",
        "from": "T19n1012_001:0683a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無住無得不盡煩惱。",
        "from": "T19n1012_001:0683a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不以知無不知。",
        "from": "T19n1012_001:0683a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不言說法不淨眾生。",
        "from": "T19n1012_001:0683a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不我故非彼故。",
        "from": "T19n1012_001:0683a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不法不律不威儀。",
        "from": "T19n1012_001:0683a14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無清淨不念身不念意。",
        "from": "T19n1012_001:0683a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無涅槃究竟。",
        "from": "T19n1012_001:0683a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不念過去當來今現在。",
        "from": "T19n1012_001:0683a15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不因己不因他。",
        "from": "T19n1012_001:0683a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切法行無受者。",
        "from": "T19n1012_001:0683a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利弗是名執受一切諸法隨順念佛。",
        "from": "T19n1012_001:0683a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為一切諸法所入無畏持門微妙句義。",
        "from": "T19n1012_001:0683a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切所欲皆悉周備。",
        "from": "T19n1012_001:0683a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為隨順一道三昧。",
        "from": "T19n1012_001:0683a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名一切善本。",
        "from": "T19n1012_001:0683a19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不由他得種性相好。",
        "from": "T19n1012_001:0683a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因自法生。",
        "from": "T19n1012_001:0683a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為正覺度諸魔事。",
        "from": "T19n1012_001:0683a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利弗是為菩薩出生無量門持。",
        "from": "T19n1012_001:0683a21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其有聞是持者。",
        "from": "T19n1012_001:0683a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆於阿耨多羅三藐三菩提得不退轉。",
        "from": "T19n1012_001:0683a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何從是持。",
        "from": "T19n1012_001:0683a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "究竟一切諸佛功德。",
        "from": "T19n1012_001:0683a23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉能分別一切眾生之所行處。",
        "from": "T19n1012_001:0683a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得無所得故。",
        "from": "T19n1012_001:0683a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名出生無量門持。",
        "from": "T19n1012_001:0683a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊而說偈言。",
        "from": "T19n1012_001:0683a25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法空無疑惑　　為道不放逸",
        "from": "T19n1012_001:0683a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法性無忘想　　是行疲得持",
        "from": "T19n1012_001:0683a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是經如所說　　彼持無有盡",
        "from": "T19n1012_001:0683a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從是生妙智　　疾成無上道",
        "from": "T19n1012_001:0683b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若得此持者　　菩薩無所畏",
        "from": "T19n1012_001:0683b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方佛說法　　如所說悉聞",
        "from": "T19n1012_001:0683b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切能受持　　義味悉具足",
        "from": "T19n1012_001:0683b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日生無量光　　持生智如是",
        "from": "T19n1012_001:0683b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至無上法　　微妙諸持門",
        "from": "T19n1012_001:0683b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切從是句　　當善護此經",
        "from": "T19n1012_001:0683b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正使一切人　　窮劫思難問",
        "from": "T19n1012_001:0683b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉能決定疑　　其智不可盡",
        "from": "T19n1012_001:0683b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "近無上道坐　　為真法王子",
        "from": "T19n1012_001:0683b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持法者親厚　　常念斯經故",
        "from": "T19n1012_001:0683b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生所敬重　　十方佛護念",
        "from": "T19n1012_001:0683b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名稱遍十方　　執持此經故",
        "from": "T19n1012_001:0683b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八十億諸佛　　臨終悉能見",
        "from": "T19n1012_001:0683b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切皆授手　　接此持經者",
        "from": "T19n1012_001:0683b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於億千劫　　造諸不善業",
        "from": "T19n1012_001:0683b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一月可以除　　當受誦此經",
        "from": "T19n1012_001:0683b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩所興福　　億劫來積聚",
        "from": "T19n1012_001:0683b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善誦此持經　　一月等於彼",
        "from": "T19n1012_001:0683b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念力精進力　　三昧勝妙持",
        "from": "T19n1012_001:0683b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆現在前　　疾成最正覺",
        "from": "T19n1012_001:0683b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三界眾生類　　一切悉為魔",
        "from": "T19n1012_001:0683b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能令中斷　　修習是經者",
        "from": "T19n1012_001:0683b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從彼誠諦說　　從彼究竟法",
        "from": "T19n1012_001:0683b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從彼得成道　　逮得一切智",
        "from": "T19n1012_001:0683b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔聞是持故　　定光授我決",
        "from": "T19n1012_001:0683b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒沙等諸佛　　即時悉能現",
        "from": "T19n1012_001:0683b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲聞十方佛　　所說悉能解",
        "from": "T19n1012_001:0683b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當專習此持　　一切疾得成",
        "from": "T19n1012_001:0683b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莊嚴佛國土　　清淨眾成就",
        "from": "T19n1012_001:0683c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光相及種性　　當從是經得",
        "from": "T19n1012_001:0683c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得除心放逸　　七日專思念",
        "from": "T19n1012_001:0683c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八十億諸佛　　俱來授此經",
        "from": "T19n1012_001:0683c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不思思所思　　亦不思無思",
        "from": "T19n1012_001:0683c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此思亦無思　　從是疾得成",
        "from": "T19n1012_001:0683c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊崇是經教　　於道莫疑惑",
        "from": "T19n1012_001:0683c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如大海中　　不應疑無寶",
        "from": "T19n1012_001:0683c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲安諸天人　　一切願不難",
        "from": "T19n1012_001:0683c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此為道心生　　但當正意行",
        "from": "T19n1012_001:0683c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次舍利弗。",
        "from": "T19n1012_001:0683c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩成就四法。",
        "from": "T19n1012_001:0683c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疾得是持。",
        "from": "T19n1012_001:0683c11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為四。",
        "from": "T19n1012_001:0683c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "厭離愛欲。",
        "from": "T19n1012_001:0683c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不生疾心。",
        "from": "T19n1012_001:0683c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切眾生施一切有。",
        "from": "T19n1012_001:0683c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晝夜求法心不懈惓。",
        "from": "T19n1012_001:0683c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊而說偈言。",
        "from": "T19n1012_001:0683c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "厭離於色欲　　魔界地獄緣",
        "from": "T19n1012_001:0683c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放逸造眾惡　　惡令墮惡道",
        "from": "T19n1012_001:0683c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於他不起妬　　不為於名稱",
        "from": "T19n1012_001:0683c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愍愛視眾生　　尊貴色端嚴",
        "from": "T19n1012_001:0683c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生起怨諍　　皆以貪為本",
        "from": "T19n1012_001:0683c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故當捨愛　　愛捨成妙持",
        "from": "T19n1012_001:0683c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晝夜常樂法　　推異推道求",
        "from": "T19n1012_001:0683c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "專精求道者　　疾得成妙持",
        "from": "T19n1012_001:0683c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又舍利弗。",
        "from": "T19n1012_001:0683c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩復有四法。",
        "from": "T19n1012_001:0683c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就疾得是持。",
        "from": "T19n1012_001:0683c23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為四。",
        "from": "T19n1012_001:0683c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "閑居寂志。",
        "from": "T19n1012_001:0683c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得深法忍。",
        "from": "T19n1012_001:0683c24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不求利養。",
        "from": "T19n1012_001:0683c24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切內外盡施無遺。",
        "from": "T19n1012_001:0683c25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為四。",
        "from": "T19n1012_001:0683c25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊而說偈言。",
        "from": "T19n1012_001:0683c25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "閑居寂志佛所譽　　自守空靜不輕彼",
        "from": "T19n1012_001:0683c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常樂修習深法忍　　如救頭然不放逸",
        "from": "T19n1012_001:0683c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不求名譽及利養　　因斯造諸諂曲行",
        "from": "T19n1012_001:0683c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處處知足如飛鳥　　節身損己修仁行",
        "from": "T19n1012_001:0683c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "快哉得佛教　　出家受具戒",
        "from": "T19n1012_001:0684a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨家離眾苦　　遵修於佛教",
        "from": "T19n1012_001:0684a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不得佛教　　出家是戒利",
        "from": "T19n1012_001:0684a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信戒施聞慧　　皆悉不成就",
        "from": "T19n1012_001:0684a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此過離佛遠　　是故捨貪利",
        "from": "T19n1012_001:0684a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又舍利弗。",
        "from": "T19n1012_001:0684a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩復有四法。",
        "from": "T19n1012_001:0684a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就疾得是持。",
        "from": "T19n1012_001:0684a06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為四。",
        "from": "T19n1012_001:0684a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一曰八字是義隨義悉入其中。",
        "from": "T19n1012_001:0684a07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八字者波羅婆迦闍陀賒叉。",
        "from": "T19n1012_001:0684a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "波者第一義。",
        "from": "T19n1012_001:0684a08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸法無我悉來入門。",
        "from": "T19n1012_001:0684a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅者相好無相。",
        "from": "T19n1012_001:0684a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如如來身入於法性。",
        "from": "T19n1012_001:0684a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婆者愚癡之法及智慧法。",
        "from": "T19n1012_001:0684a10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨順入義。",
        "from": "T19n1012_001:0684a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦者業行果報及非業行報。",
        "from": "T19n1012_001:0684a11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆悉究竟隨順入義。",
        "from": "T19n1012_001:0684a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闍者生緣老死皆悉隨入不生不滅。",
        "from": "T19n1012_001:0684a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陀者一切諸持悉隨入空無相無願。",
        "from": "T19n1012_001:0684a13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賒者一切諸持皆隨順入。",
        "from": "T19n1012_001:0684a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "叉者盡一切諸法悉入於如是。",
        "from": "T19n1012_001:0684a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是八字是義悉入其中。",
        "from": "T19n1012_001:0684a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二曰善書此經執持不捨。",
        "from": "T19n1012_001:0684a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三曰半月半月誦習此經。",
        "from": "T19n1012_001:0684a16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四曰帝當供給供養持此經者。",
        "from": "T19n1012_001:0684a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊而說偈言。",
        "from": "T19n1012_001:0684a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當思八字義　　善書持此經",
        "from": "T19n1012_001:0684a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "半月次來誦　　勸發行持者",
        "from": "T19n1012_001:0684a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切十方恒沙國　　無量諸佛在其前",
        "from": "T19n1012_001:0684a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得坐道場妙相滿　　當勤修習此持行",
        "from": "T19n1012_001:0684a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次舍利弗。",
        "from": "T19n1012_001:0684a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩學此持者得四功德利。",
        "from": "T19n1012_001:0684a23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為四。",
        "from": "T19n1012_001:0684a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者十方諸佛所念。",
        "from": "T19n1012_001:0684a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者離諸魔事。",
        "from": "T19n1012_001:0684a24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者惡罪業障疾得清淨。",
        "from": "T19n1012_001:0684a25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者無礙斷辯才皆悉成就。",
        "from": "T19n1012_001:0684a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為四。",
        "from": "T19n1012_001:0684a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊而說偈言。",
        "from": "T19n1012_001:0684a26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛常護念　　眾魔不得便",
        "from": "T19n1012_001:0684a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "罪業障悉除　　得妙無量持",
        "from": "T19n1012_001:0684a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告舍利弗。",
        "from": "T19n1012_001:0684a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃昔過去無數劫時。",
        "from": "T19n1012_001:0684a29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有佛世尊號寶首焰王如來應供等正覺明行成善逝世間解無上士調御丈夫天人師佛世尊。",
        "from": "T19n1012_001:0684b02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利弗彼寶首焰王如來臨滅度時。",
        "from": "T19n1012_001:0684b03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有轉輪王名曰持光明。",
        "from": "T19n1012_001:0684b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王四天下快得自在。",
        "from": "T19n1012_001:0684b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王有太子名無念德道。",
        "from": "T19n1012_001:0684b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其初生時形如十六歲童子。",
        "from": "T19n1012_001:0684b05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從彼世尊得聞此持。",
        "from": "T19n1012_001:0684b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞此持已專精求學。",
        "from": "T19n1012_001:0684b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經七千歲不為睡眠之所蔭蓋。",
        "from": "T19n1012_001:0684b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七千歲中未曾起身愛。",
        "from": "T19n1012_001:0684b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未曾念王事。",
        "from": "T19n1012_001:0684b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又七千歲中身不猗臥端坐正受。",
        "from": "T19n1012_001:0684b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是即見九十億那由他諸佛。",
        "from": "T19n1012_001:0684b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從其聞法皆能受持。",
        "from": "T19n1012_001:0684b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便出家學道。",
        "from": "T19n1012_001:0684b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九萬歲中以是無量門出生之持廣為人說。",
        "from": "T19n1012_001:0684b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一生中成就八十億那由他人。",
        "from": "T19n1012_001:0684b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立阿耨多羅三藐三菩提心。",
        "from": "T19n1012_001:0684b12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勸進教化令不退轉。",
        "from": "T19n1012_001:0684b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼時眾中有長者子名曰月幢。",
        "from": "T19n1012_001:0684b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得聞是持至心隨喜。",
        "from": "T19n1012_001:0684b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼以隨喜善根因緣故。",
        "from": "T19n1012_001:0684b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常得親近九十億佛。",
        "from": "T19n1012_001:0684b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得最上持得最上處無量辯才。",
        "from": "T19n1012_001:0684b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復於三劫親近諸佛。",
        "from": "T19n1012_001:0684b16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三劫末時月幢得佛號燈光如來應供等正覺。",
        "from": "T19n1012_001:0684b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼時王子無念德道者。",
        "from": "T19n1012_001:0684b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今西方無量壽佛是。",
        "from": "T19n1012_001:0684b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利弗我亦與賢劫諸菩薩。",
        "from": "T19n1012_001:0684b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曾聞此持聞已隨喜。",
        "from": "T19n1012_001:0684b19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此隨喜善根因緣。",
        "from": "T19n1012_001:0684b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除却四十萬劫生死之罪。",
        "from": "T19n1012_001:0684b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又得親近九十億佛。",
        "from": "T19n1012_001:0684b21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故舍利弗若菩薩摩訶薩。",
        "from": "T19n1012_001:0684b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲得疾成阿耨多羅三藐三菩提者。",
        "from": "T19n1012_001:0684b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此妙持當勤隨喜。",
        "from": "T19n1012_001:0684b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨喜功德。",
        "from": "T19n1012_001:0684b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至菩薩不退轉地作大因緣。",
        "from": "T19n1012_001:0684b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "況復書持受誦如說修行。",
        "from": "T19n1012_001:0684b24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切人民莫能思量稱說其福。",
        "from": "T19n1012_001:0684b25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊而說偈言。",
        "from": "T19n1012_001:0684b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞經心隨喜　　書持敬諷誦",
        "from": "T19n1012_001:0684b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天人莫能計　　福報之限量",
        "from": "T19n1012_001:0684b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生生常見佛　　信向不迴動",
        "from": "T19n1012_001:0684b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體解深法要　　疾覺無上道",
        "from": "T19n1012_001:0684c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三昧不退減　　三明總持門",
        "from": "T19n1012_001:0684c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "財色常見佛　　乃至無上道",
        "from": "T19n1012_001:0684c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憶念過去世　　長者子聞持",
        "from": "T19n1012_001:0684c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即見九十億　　那由他諸佛",
        "from": "T19n1012_001:0684c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求如其願　　得成無上道",
        "from": "T19n1012_001:0684c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲疾成佛　　道力降眾魔",
        "from": "T19n1012_001:0684c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲滿百福相　　行此非有難",
        "from": "T19n1012_001:0684c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如滿恒沙剎　　積寶以布施",
        "from": "T19n1012_001:0684c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比書持此經　　終不得為比",
        "from": "T19n1012_001:0684c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又舍利弗菩薩行此持者。",
        "from": "T19n1012_001:0684c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有八大鬼是雪山之神。",
        "from": "T19n1012_001:0684c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共護視之。",
        "from": "T19n1012_001:0684c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其名曰勇健神強力神。",
        "from": "T19n1012_001:0684c12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自在神雄猛神。",
        "from": "T19n1012_001:0684c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知行神難勝神。",
        "from": "T19n1012_001:0684c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳩摩羅神善臂神。",
        "from": "T19n1012_001:0684c13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為八。",
        "from": "T19n1012_001:0684c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼欲令諸神來者。",
        "from": "T19n1012_001:0684c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當澡浴其身淨其衣服。",
        "from": "T19n1012_001:0684c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正色經行慈念眾生。",
        "from": "T19n1012_001:0684c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思是法要如其憶念。",
        "from": "T19n1012_001:0684c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼諸神等尋現在前。",
        "from": "T19n1012_001:0684c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有八菩薩生欲行天。",
        "from": "T19n1012_001:0684c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦常護念。",
        "from": "T19n1012_001:0684c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為八。",
        "from": "T19n1012_001:0684c17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照明十方天子離欲行天子。",
        "from": "T19n1012_001:0684c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧光天子如日天子。",
        "from": "T19n1012_001:0684c18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真諦天子願滿天子。",
        "from": "T19n1012_001:0684c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "星王天子知行天子。",
        "from": "T19n1012_001:0684c19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為八。",
        "from": "T19n1012_001:0684c19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是等菩薩常來護念。",
        "from": "T19n1012_001:0684c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼專精疾得妙持。",
        "from": "T19n1012_001:0684c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行是持者真實為本。",
        "from": "T19n1012_001:0684c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而常恭敬當念知恩。",
        "from": "T19n1012_001:0684c21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "學深法忍等觀無生無滅之法。",
        "from": "T19n1012_001:0684c22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平等持戒自護兼彼。",
        "from": "T19n1012_001:0684c22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是法時三十二恒河沙等菩薩尋得是持。",
        "from": "T19n1012_001:0684c23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不退轉於阿耨多羅三藐三菩提。",
        "from": "T19n1012_001:0684c24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六十萬人得不起法忍。",
        "from": "T19n1012_001:0684c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三萬二千天與人。",
        "from": "T19n1012_001:0684c25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆發無上正真道意。",
        "from": "T19n1012_001:0684c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時三千大千世界六種震動。",
        "from": "T19n1012_001:0684c26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天雨眾華普遍世界。",
        "from": "T19n1012_001:0684c27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天發伎樂不鼓自鳴。",
        "from": "T19n1012_001:0684c27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時舍利弗白佛言。",
        "from": "T19n1012_001:0684c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊當何名此經。",
        "from": "T19n1012_001:0684c28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告舍利弗。",
        "from": "T19n1012_001:0684c28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經之要名出生無量門持。",
        "from": "T19n1012_001:0684c29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名一生補處道行。",
        "from": "T19n1012_001:0685a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名成道降魔得一切智。",
        "from": "T19n1012_001:0685a01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當奉持之。",
        "from": "T19n1012_001:0685a01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是法時尊者舍利弗。",
        "from": "T19n1012_001:0685a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸十方來會菩薩。",
        "from": "T19n1012_001:0685a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并諸眾會天與人乾闥婆阿修羅等。",
        "from": "T19n1012_001:0685a03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛所說皆大歡喜。",
        "from": "T19n1012_001:0685a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說出生無量門持經",
        "from": "T19n1012_001:0685a05_100",
        "context_with_next_sentence": false
    }
]