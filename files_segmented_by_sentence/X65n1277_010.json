{
    "X65n1277_010:0037c10_0": "高峰龍泉院因師集賢語錄卷之十",
    "X65n1277_010:0037c12_0": "龍泉院住持小師　如瑛　錄",
    "X65n1277_010:0037c13_0": "諸家伏願門",
    "X65n1277_010:0037c14_0": "祝聖",
    "X65n1277_010:0037c15_0": "璿源衍慶。",
    "X65n1277_010:0037c15_1": "寶祚延供。",
    "X65n1277_010:0037c15_2": "黎民永賀於堯年。",
    "X65n1277_010:0037c15_3": "率土常瞻於舜日。",
    "X65n1277_010:0037c17_0": "又",
    "X65n1277_010:0037c18_0": "皇風蕩蕩。",
    "X65n1277_010:0037c18_1": "帝道巍巍。",
    "X65n1277_010:0037c18_2": "萬方歌有永之年。",
    "X65n1277_010:0037c18_3": "四海祝無疆　之壽。",
    "X65n1277_010:0037c20_0": "又",
    "X65n1277_010:0037c21_0": "大德必得名。",
    "X65n1277_010:0037c21_1": "大德必得壽。",
    "X65n1277_010:0037c21_2": "永符地久天長。",
    "X65n1277_010:0037c21_3": "八千歲為春。",
    "X65n1277_010:0037c22_4": "八千歲為秋。",
    "X65n1277_010:0037c22_5": "幾度河清海晏。",
    "X65n1277_010:0037c23_0": "又",
    "X65n1277_010:0037c24_0": "永馭金輪。",
    "X65n1277_010:0037c24_1": "常調玉燭。",
    "X65n1277_010:0037c24_2": "道越三皇之上。",
    "X65n1277_010:0037c24_3": "功超五帝之前。",
    "X65n1277_010:0038a01_0": "又",
    "X65n1277_010:0038a02_0": "皇帝天齊壽。",
    "X65n1277_010:0038a02_1": "擅崇高富貴之權。",
    "X65n1277_010:0038a02_2": "化國日舒長。",
    "X65n1277_010:0038a02_3": "享安靖和平之福。",
    "X65n1277_010:0038a04_0": "官員",
    "X65n1277_010:0038a05_0": "維石巖巖長翊南山之壽。",
    "X65n1277_010:0038a05_1": "泰階兩兩永扶北極之尊。",
    "X65n1277_010:0038a06_0": "更期千歲之神仙。",
    "X65n1277_010:0038a06_1": "長作三朝之元老。",
    "X65n1277_010:0038a07_0": "又",
    "X65n1277_010:0038a08_0": "步玉登金。",
    "X65n1277_010:0038a08_1": "秉鈞當軸。",
    "X65n1277_010:0038a08_2": "輔佐太平天子。",
    "X65n1277_010:0038a08_3": "鎮安率土民生。",
    "X65n1277_010:0038a09_0": "又",
    "X65n1277_010:0038a10_0": "當軸大臣。",
    "X65n1277_010:0038a10_1": "親民長吏。",
    "X65n1277_010:0038a10_2": "為上為德。",
    "X65n1277_010:0038a10_3": "為下為民。",
    "X65n1277_010:0038a10_4": "聖眷益隆不替鈞衡之任。",
    "X65n1277_010:0038a11_5": "璽書頻下佇升樞要之司。",
    "X65n1277_010:0038a12_0": "又",
    "X65n1277_010:0038a13_0": "台星正照。",
    "X65n1277_010:0038a13_1": "福地彌深。",
    "X65n1277_010:0038a13_2": "銓臺魁取於高名。",
    "X65n1277_010:0038a13_3": "吏部榮除於見闕。",
    "X65n1277_010:0038a14_4": "上慶双親之喜。",
    "X65n1277_010:0038a14_5": "遠揚三代之名。",
    "X65n1277_010:0038a15_0": "又",
    "X65n1277_010:0038a16_0": "官資日顯。",
    "X65n1277_010:0038a16_1": "祿位時迁。",
    "X65n1277_010:0038a16_2": "上答紫皇柬注之恩。",
    "X65n1277_010:0038a16_3": "下慰蒼生具瞻之意。",
    "X65n1277_010:0038a18_0": "又",
    "X65n1277_010:0038a19_0": "皇恩已沐。",
    "X65n1277_010:0038a19_1": "使版新登。",
    "X65n1277_010:0038a19_2": "常為父母於斯民。",
    "X65n1277_010:0038a19_3": "早作棟梁於上國。",
    "X65n1277_010:0038a21_0": "又",
    "X65n1277_010:0038a22_0": "台星高照。",
    "X65n1277_010:0038a22_1": "祿位榮迁。",
    "X65n1277_010:0038a22_2": "仰承聖主之恩。",
    "X65n1277_010:0038a22_3": "俯慰生民之望。",
    "X65n1277_010:0038a23_0": "又",
    "X65n1277_010:0038a24_0": "壽延有永。",
    "X65n1277_010:0038a24_1": "福享無窮。",
    "X65n1277_010:0038a24_2": "紫綬金魚常慶齊眉而不老。",
    "X65n1277_010:0038a24_3": "青袍木簡佇觀滿目以成行。",
    "X65n1277_010:0038b02_0": "士人",
    "X65n1277_010:0038b03_0": "文星順照。",
    "X65n1277_010:0038b03_1": "奎宿光臨。",
    "X65n1277_010:0038b03_2": "白屋奮身見一舉成名之日。",
    "X65n1277_010:0038b03_3": "青雲闊步正双親未老之時。",
    "X65n1277_010:0038b05_0": "又",
    "X65n1277_010:0038b06_0": "文成七步。",
    "X65n1277_010:0038b06_1": "學富三冬。",
    "X65n1277_010:0038b06_2": "桂子秋風行見同聯鶚薦。",
    "X65n1277_010:0038b06_3": "桃花春浪佇看高躍龍門。",
    "X65n1277_010:0038b08_0": "又",
    "X65n1277_010:0038b09_0": "魁星正照。",
    "X65n1277_010:0038b09_1": "吉運加臨。",
    "X65n1277_010:0038b09_2": "胸中富錦綉之文。",
    "X65n1277_010:0038b09_3": "筆下走龍蛇之字。",
    "X65n1277_010:0038b10_4": "場屋仗神明之助。",
    "X65n1277_010:0038b10_5": "姓名標日月之高。",
    "X65n1277_010:0038b11_0": "又",
    "X65n1277_010:0038b12_0": "文高萬丈。",
    "X65n1277_010:0038b12_1": "筆掃千軍。",
    "X65n1277_010:0038b12_2": "榮身早掛於藍袍。",
    "X65n1277_010:0038b12_3": "衣錦增光於梓里。",
    "X65n1277_010:0038b14_0": "又",
    "X65n1277_010:0038b15_0": "嶽瀆鍾靈。",
    "X65n1277_010:0038b15_1": "魁杓孕秀。",
    "X65n1277_010:0038b15_2": "今年秋試鶚書早薦於侯邦。",
    "X65n1277_010:0038b15_3": "來歲春官鴈塔高題於帝里。",
    "X65n1277_010:0038b17_0": "又",
    "X65n1277_010:0038b18_0": "棘闈奏捷。",
    "X65n1277_010:0038b18_1": "蘭省登名。",
    "X65n1277_010:0038b18_2": "鵰鶚離風塵振羽翰而奮迅。",
    "X65n1277_010:0038b18_3": "蛟龍得雲雨露頭角以崢嶸。",
    "X65n1277_010:0038b20_0": "僧",
    "X65n1277_010:0038b21_0": "密傳心印。",
    "X65n1277_010:0038b21_1": "上續祖燈。",
    "X65n1277_010:0038b21_2": "俾業障以時消。",
    "X65n1277_010:0038b21_3": "冀善芽而日長。",
    "X65n1277_010:0038b22_0": "大作人天耳目。",
    "X65n1277_010:0038b22_1": "永為佛法棟梁。",
    "X65n1277_010:0038b23_0": "又",
    "X65n1277_010:0038b24_0": "善根增長。",
    "X65n1277_010:0038b24_1": "魔障消除。",
    "X65n1277_010:0038b24_2": "願宗說以皆通。",
    "X65n1277_010:0038b24_3": "種福慧而双足。",
    "X65n1277_010:0038c01_0": "作法門之領神。",
    "X65n1277_010:0038c01_1": "為梵剎之紀綱。",
    "X65n1277_010:0038c02_0": "又",
    "X65n1277_010:0038c03_0": "色身堅固。",
    "X65n1277_010:0038c03_1": "道眼圓明。",
    "X65n1277_010:0038c03_2": "常結十方歡喜緣。",
    "X65n1277_010:0038c03_3": "永超萬劫輪廻路。",
    "X65n1277_010:0038c05_0": "又",
    "X65n1277_010:0038c06_0": "慈心海闊。",
    "X65n1277_010:0038c06_1": "法壽山高。",
    "X65n1277_010:0038c06_2": "棹般若之舟航。",
    "X65n1277_010:0038c06_3": "作人天之眼目。",
    "X65n1277_010:0038c07_0": "又",
    "X65n1277_010:0038c08_0": "袈裟之下常修清淨之因。",
    "X65n1277_010:0038c08_1": "印德之中永荷慈悲之主。",
    "X65n1277_010:0038c09_0": "宗風益振。",
    "X65n1277_010:0038c09_1": "戒月增輝。",
    "X65n1277_010:0038c10_0": "道",
    "X65n1277_010:0038c11_0": "道基堅固。",
    "X65n1277_010:0038c11_1": "觀宇興隆。",
    "X65n1277_010:0038c11_2": "心傳微妙以自然。",
    "X65n1277_010:0038c11_3": "意悟玄真而不昧。",
    "X65n1277_010:0038c13_0": "又",
    "X65n1277_010:0038c14_0": "性根清靜。",
    "X65n1277_010:0038c14_1": "道果圓成。",
    "X65n1277_010:0038c14_2": "永為苦海之堦梯。",
    "X65n1277_010:0038c14_3": "常作法門之梁棟。",
    "X65n1277_010:0038c16_0": "又",
    "X65n1277_010:0038c17_0": "玄機頓悟。",
    "X65n1277_010:0038c17_1": "福德益崇。",
    "X65n1277_010:0038c17_2": "朝翔鸞鶴而道力清高。",
    "X65n1277_010:0038c17_3": "夜步斗牛而性天澄徹。",
    "X65n1277_010:0038c19_0": "農人",
    "X65n1277_010:0038c20_0": "夫耕婦饁。",
    "X65n1277_010:0038c20_1": "春種夏耘。",
    "X65n1277_010:0038c20_2": "無旱乾水溢之虞。",
    "X65n1277_010:0038c20_3": "有黍稷稻粱之慶。",
    "X65n1277_010:0038c22_0": "又",
    "X65n1277_010:0038c23_0": "始勤東作。",
    "X65n1277_010:0038c23_1": "終望西成。",
    "X65n1277_010:0038c23_2": "家無八口之憂。",
    "X65n1277_010:0038c23_3": "戶有千倉之積。",
    "X65n1277_010:0038c24_0": "又",
    "X65n1277_010:0039a01_0": "東皐耕墾。",
    "X65n1277_010:0039a01_1": "南畝耘耔。",
    "X65n1277_010:0039a01_2": "蟊賊潛消。",
    "X65n1277_010:0039a01_3": "雨暘時若。",
    "X65n1277_010:0039a01_4": "糓粟桑麻豐阜。",
    "X65n1277_010:0039a02_5": "牛羊雞犬繁延。",
    "X65n1277_010:0039a03_0": "工藝",
    "X65n1277_010:0039a04_0": "性天巧妙。",
    "X65n1277_010:0039a04_1": "心匠高明。",
    "X65n1277_010:0039a04_2": "藝獨擅於無双。",
    "X65n1277_010:0039a04_3": "學其推於第一。",
    "X65n1277_010:0039a05_0": "早待天書之詔。",
    "X65n1277_010:0039a05_1": "得沾聖王之恩。",
    "X65n1277_010:0039a06_0": "又",
    "X65n1277_010:0039a07_0": "心機造妙。",
    "X65n1277_010:0039a07_1": "手藝精通。",
    "X65n1277_010:0039a07_2": "胸中有奇巧規模。",
    "X65n1277_010:0039a07_3": "掌上能斡旋造化。",
    "X65n1277_010:0039a08_4": "常獲公卿寵命。",
    "X65n1277_010:0039a08_5": "行親帝主恩光。",
    "X65n1277_010:0039a09_0": "商賈",
    "X65n1277_010:0039a10_0": "往來郡國。",
    "X65n1277_010:0039a10_1": "遊寓京華。",
    "X65n1277_010:0039a10_2": "陸行無盜賊之虞。",
    "X65n1277_010:0039a10_3": "舟運絕風波之險。",
    "X65n1277_010:0039a11_4": "多寶如來扶運動。",
    "X65n1277_010:0039a11_5": "招財童子護行藏。",
    "X65n1277_010:0039a12_0": "又",
    "X65n1277_010:0039a13_0": "生財有道。",
    "X65n1277_010:0039a13_1": "妙筭無遺。",
    "X65n1277_010:0039a13_2": "何煩龍斷之頻登。",
    "X65n1277_010:0039a13_3": "自見門闌之成市。",
    "X65n1277_010:0039a15_0": "又",
    "X65n1277_010:0039a16_0": "經營便利。",
    "X65n1277_010:0039a16_1": "貿易豐饒。",
    "X65n1277_010:0039a16_2": "舟運陸行絕巇險風波之厄。",
    "X65n1277_010:0039a16_3": "珠盈玉積無穿窬宼盜之虞。",
    "X65n1277_010:0039a18_0": "公吏",
    "X65n1277_010:0039a19_0": "公庭清吉。",
    "X65n1277_010:0039a19_1": "私舍安寧。",
    "X65n1277_010:0039a19_2": "事官常獲福祥。",
    "X65n1277_010:0039a19_3": "為吏舉無過失。",
    "X65n1277_010:0039a20_0": "貴人扶護。",
    "X65n1277_010:0039a20_1": "家道昌隆。",
    "X65n1277_010:0039a21_0": "又",
    "X65n1277_010:0039a22_0": "吉星臨照。",
    "X65n1277_010:0039a22_1": "貴官扶持。",
    "X65n1277_010:0039a22_2": "奉公而公事和平。",
    "X65n1277_010:0039a22_3": "事官而官災殄滅。",
    "X65n1277_010:0039a24_0": "醫士",
    "X65n1277_010:0039b01_0": "濟眾功深。",
    "X65n1277_010:0039b01_1": "活人利愽。",
    "X65n1277_010:0039b01_2": "砭劍獲十全之效。",
    "X65n1277_010:0039b01_3": "方書傅三世之良。",
    "X65n1277_010:0039b02_4": "藥王菩薩護行藏。",
    "X65n1277_010:0039b02_5": "如意輪王扶運用。",
    "X65n1277_010:0039b03_0": "又",
    "X65n1277_010:0039b04_0": "醫雲藥聖。",
    "X65n1277_010:0039b04_1": "術妙道行。",
    "X65n1277_010:0039b04_2": "収扶危起癈之功。",
    "X65n1277_010:0039b04_3": "得返死回生之效。",
    "X65n1277_010:0039b05_4": "全資佛力。",
    "X65n1277_010:0039b05_5": "大賜陰功。",
    "X65n1277_010:0039b05_6": "俾人人居安樂之邦。",
    "X65n1277_010:0039b05_7": "使在在躋康寧之域。",
    "X65n1277_010:0039b07_0": "術士",
    "X65n1277_010:0039b08_0": "藝術通靈。",
    "X65n1277_010:0039b08_1": "營謀逐意。",
    "X65n1277_010:0039b08_2": "揲蓍布象知吉凶悔吝於未兆之前。",
    "X65n1277_010:0039b09_3": "發筴擲龜推禍福災祥於方來之際。",
    "X65n1277_010:0039b09_4": "(□者)又藝情星命。",
    "X65n1277_010:0039b11_5": "術動公卿。",
    "X65n1277_010:0039b11_6": "斷生死以無差。",
    "X65n1277_010:0039b11_7": "定吉凶而有準。",
    "X65n1277_010:0039b12_0": "仰憑佛祐。",
    "X65n1277_010:0039b12_1": "益使道行。",
    "X65n1277_010:0039b12_2": "(星命)師巫師門安靜。",
    "X65n1277_010:0039b14_3": "法教流行。",
    "X65n1277_010:0039b14_4": "陰兵擁護而日日安寧。",
    "X65n1277_010:0039b14_5": "神力扶持而時時康泰。",
    "X65n1277_010:0039b16_0": "又",
    "X65n1277_010:0039b17_0": "法門興旺。",
    "X65n1277_010:0039b17_1": "師術通靈。",
    "X65n1277_010:0039b17_2": "神符篆處而邪崇寒心。",
    "X65n1277_010:0039b17_3": "法籙開時而妖魔屏跡。",
    "X65n1277_010:0039b18_4": "雖云神力。",
    "X65n1277_010:0039b18_5": "實藉佛威。",
    "X65n1277_010:0039b19_0": "師尼",
    "X65n1277_010:0039b20_0": "色身堅固。",
    "X65n1277_010:0039b20_1": "戒行端嚴。",
    "X65n1277_010:0039b20_2": "長福果於善根。",
    "X65n1277_010:0039b20_3": "開覺花於心地。",
    "X65n1277_010:0039b21_0": "觀音埀祐。",
    "X65n1277_010:0039b21_1": "王母齊年。",
    "X65n1277_010:0039b22_0": "老人",
    "X65n1277_010:0039b23_0": "紅顏長在。",
    "X65n1277_010:0039b23_1": "白髮無侵。",
    "X65n1277_010:0039b23_2": "丹臺書難老之年。",
    "X65n1277_010:0039b23_3": "紫府注長生之字。",
    "X65n1277_010:0039c01_0": "又",
    "X65n1277_010:0039c02_0": "身如山岳而地鎮長存。",
    "X65n1277_010:0039c02_1": "壽等椿松而歲寒不改。",
    "X65n1277_010:0039c02_2": "年德高邵。",
    "X65n1277_010:0039c03_3": "子孫眾多。",
    "X65n1277_010:0039c03_4": "賴神物之護持。",
    "X65n1277_010:0039c03_5": "仗龍天而庇祐。",
    "X65n1277_010:0039c04_0": "又",
    "X65n1277_010:0039c05_0": "壽山鞏固高而不危。",
    "X65n1277_010:0039c05_1": "福海宏深測之益遠。",
    "X65n1277_010:0039c05_2": "龐眉有永為蓬壺難老之仙。",
    "X65n1277_010:0039c06_3": "鶴髮長新作聖世太平之叟。",
    "X65n1277_010:0039c07_0": "婦人",
    "X65n1277_010:0039c08_0": "姆儀綽著。",
    "X65n1277_010:0039c08_1": "婦道常遵。",
    "X65n1277_010:0039c08_2": "臉如二月之桃。",
    "X65n1277_010:0039c08_3": "眉賽三春之柳。",
    "X65n1277_010:0039c09_0": "麻姑同筭。",
    "X65n1277_010:0039c09_1": "王母齊齡。",
    "X65n1277_010:0039c10_0": "又",
    "X65n1277_010:0039c11_0": "性天清淨。",
    "X65n1277_010:0039c11_1": "心地和柔。",
    "X65n1277_010:0039c11_2": "王母蟠桃永錫千秋之壽。",
    "X65n1277_010:0039c11_3": "觀音楊柳拂除四季之災。",
    "X65n1277_010:0039c13_0": "小兒",
    "X65n1277_010:0039c14_0": "命根鞏固。",
    "X65n1277_010:0039c14_1": "天性聰明。",
    "X65n1277_010:0039c14_2": "乳食均調。",
    "X65n1277_010:0039c14_3": "邪魔永殄。",
    "X65n1277_010:0039c14_4": "幼作掌珠之瑞。",
    "X65n1277_010:0039c15_5": "大為國寶之榮。",
    "X65n1277_010:0039c16_0": "又",
    "X65n1277_010:0039c17_0": "日享安寧。",
    "X65n1277_010:0039c17_1": "夜無驚怖。",
    "X65n1277_010:0039c17_2": "幼質易為恩養。",
    "X65n1277_010:0039c17_3": "天資大賜聰明。",
    "X65n1277_010:0039c18_0": "宮中二六化婆常蒙密祐。",
    "X65n1277_010:0039c18_1": "經內十五種鬼永不相侵。",
    "X65n1277_010:0039c19_0": "又",
    "X65n1277_010:0039c20_0": "幼庚清吉。",
    "X65n1277_010:0039c20_1": "乳食調和。",
    "X65n1277_010:0039c20_2": "善財童子護行藏。",
    "X65n1277_010:0039c20_3": "吉祥如來埀福祐。",
    "X65n1277_010:0039c21_4": "利宜父母。",
    "X65n1277_010:0039c21_5": "光顯門庭。",
    "X65n1277_010:0039c22_0": "令家",
    "X65n1277_010:0039c23_0": "家道益肥。",
    "X65n1277_010:0039c23_1": "門風增盛。",
    "X65n1277_010:0039c23_2": "謝家堦砌芝蘭永播於馨香。",
    "X65n1277_010:0039c23_3": "孔氏庭闈詩禮大揚於聲教。",
    "X65n1277_010:0040a01_0": "又",
    "X65n1277_010:0040a02_0": "門等于公之慶。",
    "X65n1277_010:0040a02_1": "家傳石氏之榮。",
    "X65n1277_010:0040a02_2": "田蚕歲歲全収。",
    "X65n1277_010:0040a02_3": "畜類旬旬進益。",
    "X65n1277_010:0040a03_4": "公私迪吉。",
    "X65n1277_010:0040a03_5": "水陸亨通。",
    "X65n1277_010:0040a03_6": "門闌馳綽綽之聲。",
    "X65n1277_010:0040a03_7": "子息播詵詵之譽。",
    "X65n1277_010:0040a05_0": "又",
    "X65n1277_010:0040a06_0": "雷送五天之福。",
    "X65n1277_010:0040a06_1": "電驅九橫之灾。",
    "X65n1277_010:0040a06_2": "千尋壽岳聳乾坤。",
    "X65n1277_010:0040a06_3": "一派福源綿日月。",
    "X65n1277_010:0040a07_4": "夫妻分上永無刑剋之虞。",
    "X65n1277_010:0040a07_5": "子息位中常保團圓之慶。",
    "X65n1277_010:0040a09_0": "祝壽",
    "X65n1277_010:0040a10_0": "壽齊泰岳。",
    "X65n1277_010:0040a10_1": "福等滄溟。",
    "X65n1277_010:0040a10_2": "詠南山不鶱之詩。",
    "X65n1277_010:0040a10_3": "歌泮水難老之頌。",
    "X65n1277_010:0040a12_0": "又",
    "X65n1277_010:0040a13_0": "壽祺天錫。",
    "X65n1277_010:0040a13_1": "福德日新。",
    "X65n1277_010:0040a13_2": "寶籙金書永注長生之字。",
    "X65n1277_010:0040a13_3": "瀛洲蓬島茂延有永之年。",
    "X65n1277_010:0040a15_0": "又",
    "X65n1277_010:0040a16_0": "壽數比河沙之數無量無邊。",
    "X65n1277_010:0040a16_1": "福基等天地之基益堅益固。",
    "X65n1277_010:0040a17_2": "仰干皇覺。",
    "X65n1277_010:0040a17_3": "大賜洪休。",
    "X65n1277_010:0040a18_0": "祈福",
    "X65n1277_010:0040a19_0": "時臻晏粲。",
    "X65n1277_010:0040a19_1": "既格豐穰。",
    "X65n1277_010:0040a19_2": "門迎簡簡之禧祥。",
    "X65n1277_010:0040a19_3": "戶納綿綿之戩糓。",
    "X65n1277_010:0040a20_4": "綽有照裕。",
    "X65n1277_010:0040a20_5": "罄無不宜。",
    "X65n1277_010:0040a21_0": "又",
    "X65n1277_010:0040a22_0": "吉祥駢集。",
    "X65n1277_010:0040a22_1": "疫癘潛消。",
    "X65n1277_010:0040a22_2": "營謀而萬種依心。",
    "X65n1277_010:0040a22_3": "運動而千般遂意。",
    "X65n1277_010:0040a24_0": "又",
    "X65n1277_010:0040b01_0": "千祥雲委。",
    "X65n1277_010:0040b01_1": "百順川增。",
    "X65n1277_010:0040b01_2": "壽山齊華岳之高。",
    "X65n1277_010:0040b01_3": "福海等滄溟之濬。",
    "X65n1277_010:0040b03_0": "又",
    "X65n1277_010:0040b04_0": "五福一曰壽。",
    "X65n1277_010:0040b04_1": "千倉萬斯箱。",
    "X65n1277_010:0040b04_2": "許史繁華假慈風而扇歸甲第。",
    "X65n1277_010:0040b05_3": "金張富貴隨佛力而化入千門。",
    "X65n1277_010:0040b06_0": "禳災",
    "X65n1277_010:0040b07_0": "福逐雲生。",
    "X65n1277_010:0040b07_1": "灾隨電掃。",
    "X65n1277_010:0040b07_2": "出入獲地天之泰。",
    "X65n1277_010:0040b07_3": "行藏無山水之蒙。",
    "X65n1277_010:0040b08_4": "火盜雙消。",
    "X65n1277_010:0040b08_5": "公私兩利。",
    "X65n1277_010:0040b08_6": "願心既剖。",
    "X65n1277_010:0040b08_7": "聖意想持。",
    "X65n1277_010:0040b09_0": "又",
    "X65n1277_010:0040b10_0": "天災地否無及其身。",
    "X65n1277_010:0040b10_1": "月厭年衰不臨其命。",
    "X65n1277_010:0040b10_2": "運之凶者變凶作吉。",
    "X65n1277_010:0040b11_3": "星之晦者易晦為明。",
    "X65n1277_010:0040b11_4": "動則眾聖維持。",
    "X65n1277_010:0040b11_5": "居則萬靈擁護。",
    "X65n1277_010:0040b12_6": "寒暄有慶。",
    "X65n1277_010:0040b12_7": "視履無虞。",
    "X65n1277_010:0040b13_0": "又",
    "X65n1277_010:0040b14_0": "時清道泰。",
    "X65n1277_010:0040b14_1": "國阜民安。",
    "X65n1277_010:0040b14_2": "三光全而寒暑平。",
    "X65n1277_010:0040b14_3": "五穀熟而人民育。",
    "X65n1277_010:0040b15_4": "處處有登臺之樂。",
    "X65n1277_010:0040b15_5": "人人恣鼓腹之遊。",
    "X65n1277_010:0040b15_6": "賈藏市而物阜財豐。",
    "X65n1277_010:0040b16_7": "士修文而功成名遂。",
    "X65n1277_010:0040b16_8": "旅途工肆俾爾熾俾爾昌。",
    "X65n1277_010:0040b17_9": "男畒女桑有餘粟有餘布。",
    "X65n1277_010:0040b17_10": "獄囚永息。",
    "X65n1277_010:0040b17_11": "疫癘俱消。",
    "X65n1277_010:0040b18_0": "火盜潛藏。",
    "X65n1277_010:0040b18_1": "里閭寧謐。",
    "X65n1277_010:0040b18_2": "(四民總願)保安所沾疾患。",
    "X65n1277_010:0040b20_3": "用保康寧。",
    "X65n1277_010:0040b20_4": "百骸無枯瘁之虞。",
    "X65n1277_010:0040b20_5": "四大獲輕便之樂。",
    "X65n1277_010:0040b21_6": "服藥如醍醐灌味。",
    "X65n1277_010:0040b21_7": "飲食如禪悅酥酡。",
    "X65n1277_010:0040b21_8": "行住坐臥無阻無虞。",
    "X65n1277_010:0040b22_9": "出入起居常安常樂。",
    "X65n1277_010:0040b23_0": "又",
    "X65n1277_010:0040b24_0": "慧日照臨。",
    "X65n1277_010:0040b24_1": "災雲消捲。",
    "X65n1277_010:0040b24_2": "吉曜光扶於動止。",
    "X65n1277_010:0040b24_3": "憂星不玷於行藏。",
    "X65n1277_010:0040c01_4": "更資救苦觀音豁除眾苦。",
    "X65n1277_010:0040c01_5": "仍賴消災菩薩永掃餘災。",
    "X65n1277_010:0040c03_0": "又",
    "X65n1277_010:0040c04_0": "沉痾脫體。",
    "X65n1277_010:0040c04_1": "吉曜臨宮。",
    "X65n1277_010:0040c04_2": "命運享而氣馬自和。",
    "X65n1277_010:0040c04_3": "星辰順而身龍益健。",
    "X65n1277_010:0040c05_4": "佛埀智劒削去病源。",
    "X65n1277_010:0040c05_5": "聖賜靈丹消除苦惱。",
    "X65n1277_010:0040c06_0": "命樹聳蒼蒼之色。",
    "X65n1277_010:0040c06_1": "壽山增兀兀之峯。",
    "X65n1277_010:0040c07_0": "又",
    "X65n1277_010:0040c08_0": "時災殄散。",
    "X65n1277_010:0040c08_1": "月厄蠲除。",
    "X65n1277_010:0040c08_2": "凶星退而吉星臨。",
    "X65n1277_010:0040c08_3": "妖氣消而和氣集。",
    "X65n1277_010:0040c09_4": "藥王菩薩藥調五臟以安寧。",
    "X65n1277_010:0040c09_5": "辟毒金剛辟去百邪而清淨。",
    "X65n1277_010:0040c11_0": "保胎",
    "X65n1277_010:0040c12_0": "明珠未吐。",
    "X65n1277_010:0040c12_1": "五葉含芳。",
    "X65n1277_010:0040c12_2": "恐難月之將臨。",
    "X65n1277_010:0040c12_3": "仗聖贒而保護。",
    "X65n1277_010:0040c13_0": "誕生之日如遊歡喜之園。",
    "X65n1277_010:0040c13_1": "坐草之時似攀無憂之樹。",
    "X65n1277_010:0040c14_0": "又",
    "X65n1277_010:0040c15_0": "保十月以輕安。",
    "X65n1277_010:0040c15_1": "護一時而無難。",
    "X65n1277_010:0040c15_2": "監生聖母內消分免之愆。",
    "X65n1277_010:0040c16_3": "護產靈童大賜吉祥之福。",
    "X65n1277_010:0040c16_4": "熊羆恊夢。",
    "X65n1277_010:0040c16_5": "孤矢張門。",
    "X65n1277_010:0040c17_0": "早生男子之祥。",
    "X65n1277_010:0040c17_1": "行繼祖風之裔。",
    "X65n1277_010:0040c18_0": "保蚕",
    "X65n1277_010:0040c19_0": "春蚕大熟。",
    "X65n1277_010:0040c19_1": "夏蠒倍常。",
    "X65n1277_010:0040c19_2": "繅就雪絲漢野未饒於成蠒。",
    "X65n1277_010:0040c19_3": "裁成春服周詩孰詠於無衣。",
    "X65n1277_010:0040c21_0": "又",
    "X65n1277_010:0040c22_0": "繭若霜明。",
    "X65n1277_010:0040c22_1": "絲如山積。",
    "X65n1277_010:0040c22_2": "馬鳴大士能變少以成多。",
    "X65n1277_010:0040c22_3": "如意輪王為轉無而作有。",
    "X65n1277_010:0040c23_4": "蔑有一毫虗耗。",
    "X65n1277_010:0040c23_5": "願令十倍全収。",
    "X65n1277_010:0040c24_0": "保苗",
    "X65n1277_010:0041a01_0": "田疇既播。",
    "X65n1277_010:0041a01_1": "苗稼方榮。",
    "X65n1277_010:0041a01_2": "無旱乾水溢之虞。",
    "X65n1277_010:0041a01_3": "絕稂莠蝗蟲之害。",
    "X65n1277_010:0041a02_4": "下土感夢魚之兆。",
    "X65n1277_010:0041a02_5": "上天應覆斗之祥。",
    "X65n1277_010:0041a03_0": "又",
    "X65n1277_010:0041a04_0": "秧苗競長。",
    "X65n1277_010:0041a04_1": "荑稗不生。",
    "X65n1277_010:0041a04_2": "水旱無虞。",
    "X65n1277_010:0041a04_3": "蝗螟永息。",
    "X65n1277_010:0041a04_4": "自從東作千郊碧浪色連天。",
    "X65n1277_010:0041a05_5": "佇見西成萬頃黃雲濃匝地。",
    "X65n1277_010:0041a05_6": "家有積倉之喜。",
    "X65n1277_010:0041a06_7": "人興擊壤之歌。",
    "X65n1277_010:0041a07_0": "保畜",
    "X65n1277_010:0041a08_0": "牛羊茁壯。",
    "X65n1277_010:0041a08_1": "狗彘蕃延。",
    "X65n1277_010:0041a08_2": "舉無瘴癘之虞。",
    "X65n1277_010:0041a08_3": "永絕竊偷之患。",
    "X65n1277_010:0041a09_0": "又",
    "X65n1277_010:0041a10_0": "五母雞二母彘無失其時。",
    "X65n1277_010:0041a10_1": "三為犬四為羊各遂其性。",
    "X65n1277_010:0041a11_0": "生生蕃殖。",
    "X65n1277_010:0041a11_1": "物物阜康。",
    "X65n1277_010:0041a12_0": "保牛",
    "X65n1277_010:0041a13_0": "朝放桃林。",
    "X65n1277_010:0041a13_1": "日餐草地。",
    "X65n1277_010:0041a13_2": "涉險無挫傷之患。",
    "X65n1277_010:0041a13_3": "當耕有壯健之功。",
    "X65n1277_010:0041a14_4": "常見生延。",
    "X65n1277_010:0041a14_5": "永無損害。",
    "X65n1277_010:0041a15_0": "又",
    "X65n1277_010:0041a16_0": "掃除疫癘。",
    "X65n1277_010:0041a16_1": "俾遂生延。",
    "X65n1277_010:0041a16_2": "朝遊野以成群。",
    "X65n1277_010:0041a16_3": "夕還欄而結隊。",
    "X65n1277_010:0041a17_0": "牲具有來思之詠。",
    "X65n1277_010:0041a17_1": "時和無問喘之勞。",
    "X65n1277_010:0041a18_0": "又",
    "X65n1277_010:0041a19_0": "爾牛來思。",
    "X65n1277_010:0041a19_1": "我牲既具。",
    "X65n1277_010:0041a19_2": "蕃滋而不疾不[病丙+蠡]。",
    "X65n1277_010:0041a19_3": "降飲而或寢或訛。",
    "X65n1277_010:0041a21_0": "禳火災",
    "X65n1277_010:0041a22_0": "熒惑藏輝。",
    "X65n1277_010:0041a22_1": "祝融遁跡。",
    "X65n1277_010:0041a22_2": "轉凶灾為吉慶。",
    "X65n1277_010:0041a22_3": "變熱惱作清涼。",
    "X65n1277_010:0041a23_0": "仰仗慈風。",
    "X65n1277_010:0041a23_1": "潛消虗焰。",
    "X65n1277_010:0041a24_0": "禳水火",
    "X65n1277_010:0041b01_0": "乘茲佛力。",
    "X65n1277_010:0041b01_1": "發彼慈心。",
    "X65n1277_010:0041b01_2": "順坎勢以安流。",
    "X65n1277_010:0041b01_3": "虗離中而息焰。",
    "X65n1277_010:0041b02_0": "兩溪綠漲安載舟溉物之功。",
    "X65n1277_010:0041b02_1": "萬室炊煙獲移突徙薪之利。",
    "X65n1277_010:0041b03_2": "既滅焚漂之苦趣。",
    "X65n1277_010:0041b03_3": "皆由禳謝之良緣。",
    "X65n1277_010:0041b04_0": "禳盜賊",
    "X65n1277_010:0041b05_0": "狗偷屏跡。",
    "X65n1277_010:0041b05_1": "鼠竊潛蹤。",
    "X65n1277_010:0041b05_2": "犬無夜吠之虞。",
    "X65n1277_010:0041b05_3": "雞止月攘之患。",
    "X65n1277_010:0041b06_0": "外戶不閉。",
    "X65n1277_010:0041b06_1": "高枕無憂。",
    "X65n1277_010:0041b07_0": "又",
    "X65n1277_010:0041b08_0": "戶常不閉。",
    "X65n1277_010:0041b08_1": "路絕拾遺。",
    "X65n1277_010:0041b08_2": "無隴上揭竿之耕夫。",
    "X65n1277_010:0041b08_3": "蔑潢池弄兵之赤子。",
    "X65n1277_010:0041b09_4": "聲沉夜柝。",
    "X65n1277_010:0041b09_5": "人樂春臺。",
    "X65n1277_010:0041b10_0": "還願",
    "X65n1277_010:0041b11_0": "還恩滿處如月現澄潭。",
    "X65n1277_010:0041b11_1": "賽願圓時似聲傳空谷。",
    "X65n1277_010:0041b11_2": "萬靈鑒格。",
    "X65n1277_010:0041b12_3": "三寶印明。",
    "X65n1277_010:0041b13_0": "又",
    "X65n1277_010:0041b14_0": "既蒙陰賜。",
    "X65n1277_010:0041b14_1": "敢負初盟。",
    "X65n1277_010:0041b14_2": "敬殫此日之忱。",
    "X65n1277_010:0041b14_3": "特賽前時之欵。",
    "X65n1277_010:0041b15_0": "又",
    "X65n1277_010:0041b16_0": "六曹案上。",
    "X65n1277_010:0041b16_1": "三界簿中。",
    "X65n1277_010:0041b16_2": "望銷始許之名。",
    "X65n1277_010:0041b16_3": "乞注終填之字。",
    "X65n1277_010:0041b17_0": "預修",
    "X65n1277_010:0041b18_0": "幸得人身在識脩身之理。",
    "X65n1277_010:0041b18_1": "叨居塵世當明出世之因。",
    "X65n1277_010:0041b19_0": "前生若不莊嚴。",
    "X65n1277_010:0041b19_1": "後報將何營種。",
    "X65n1277_010:0041b19_2": "預修功果。",
    "X65n1277_010:0041b19_3": "大布津梁。",
    "X65n1277_010:0041b20_0": "乘茲有作之良緣。",
    "X65n1277_010:0041b20_1": "必獲將來之餘慶。",
    "X65n1277_010:0041b21_0": "又",
    "X65n1277_010:0041b22_0": "寒必索裘豈待既寒之後。",
    "X65n1277_010:0041b22_1": "渴當掘井必須未渴之前。",
    "X65n1277_010:0041b23_0": "庸作善於今生。",
    "X65n1277_010:0041b23_1": "庶降祥於異日。",
    "X65n1277_010:0041b24_0": "總願",
    "X65n1277_010:0041c01_0": "聰明皇帝聖壽天齊。",
    "X65n1277_010:0041c01_1": "文武臣僚官資日顯。",
    "X65n1277_010:0041c01_2": "梯航萬國。",
    "X65n1277_010:0041c02_0": "冠帶百蠻。",
    "X65n1277_010:0041c02_1": "混庶俗於仁天。",
    "X65n1277_010:0041c02_2": "躋八荒於壽域。",
    "X65n1277_010:0041c02_3": "時調玉燭。",
    "X65n1277_010:0041c03_0": "歲恊金穰。",
    "X65n1277_010:0041c03_1": "冬無愆陽。",
    "X65n1277_010:0041c03_2": "夏無伏陰。",
    "X65n1277_010:0041c03_3": "風不鳴條。",
    "X65n1277_010:0041c03_4": "雨不破塊。",
    "X65n1277_010:0041c04_0": "百樂咸生於治國。",
    "X65n1277_010:0041c04_1": "四民各遂於生涯。",
    "X65n1277_010:0041c04_2": "修儒業者鴈塔題名。",
    "X65n1277_010:0041c05_3": "執工藝者金門待詔。",
    "X65n1277_010:0041c05_4": "出塗藏市富金刀泉布之饒。",
    "X65n1277_010:0041c06_5": "鑿井耕田有稼穡倉箱之積。",
    "X65n1277_010:0041c06_6": "蚕桑十倍。",
    "X65n1277_010:0041c06_7": "利祿兩全。",
    "X65n1277_010:0041c07_0": "曰壽曰富曰康寧。",
    "X65n1277_010:0041c07_1": "俾昌俾熾俾耆艾。",
    "X65n1277_010:0041c07_2": "戶納千祥百福。",
    "X65n1277_010:0041c08_0": "家無九橫三災。",
    "X65n1277_010:0041c08_1": "人盡樂於登春。",
    "X65n1277_010:0041c08_2": "犬不聞於吠夜。",
    "X65n1277_010:0041c08_3": "烽銷遠塞。",
    "X65n1277_010:0041c09_4": "草鞠圜扇。",
    "X65n1277_010:0041c09_5": "羽毛鱗甲咸遂生延。",
    "X65n1277_010:0041c09_6": "動植飛潛舉臻蕃殖。",
    "X65n1277_010:0041c10_7": "上窮有頂。",
    "X65n1277_010:0041c10_8": "下達無邊。",
    "X65n1277_010:0041c10_9": "凡屬有情。",
    "X65n1277_010:0041c10_10": "同登彼岸。",
    "X65n1277_010:0041c12_0": "高峰"
}