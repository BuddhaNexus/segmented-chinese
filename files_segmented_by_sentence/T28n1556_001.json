{
    "T28n1556_001:0995c03_0": "薩婆多宗五事論",
    "T28n1556_001:0995c05_0": "大番國大德三藏法師沙門法成",
    "T28n1556_001:0995c06_0": "於甘州修多寺道場",
    "T28n1556_001:0995c07_0": "法有五種。",
    "T28n1556_001:0995c07_1": "一者色法。",
    "T28n1556_001:0995c07_2": "二者心法。",
    "T28n1556_001:0995c07_3": "三者心所有法。",
    "T28n1556_001:0995c08_4": "四者心不相應行法。",
    "T28n1556_001:0995c08_5": "五者無為法。",
    "T28n1556_001:0995c08_6": "何名色法。",
    "T28n1556_001:0995c09_7": "謂彼一切從四大種。",
    "T28n1556_001:0995c09_8": "四大所造所生諸色。",
    "T28n1556_001:0995c10_0": "何謂四大。",
    "T28n1556_001:0995c10_1": "地界水界火界風界。",
    "T28n1556_001:0995c10_2": "何名造色。",
    "T28n1556_001:0995c10_3": "謂眼根耳根鼻根舌根身根。",
    "T28n1556_001:0995c11_4": "諸色諸聲諸香諸味及觸一分兼無表色。",
    "T28n1556_001:0995c13_0": "何名心法。",
    "T28n1556_001:0995c13_1": "謂心意識。",
    "T28n1556_001:0995c13_2": "彼復云何謂眼識耳鼻舌身意識六識之身。",
    "T28n1556_001:0995c14_3": "何名心所有法。",
    "T28n1556_001:0995c14_4": "謂與心相應諸法。",
    "T28n1556_001:0995c15_5": "彼復云何。",
    "T28n1556_001:0995c15_6": "謂受。",
    "T28n1556_001:0995c15_7": "相。",
    "T28n1556_001:0995c15_8": "思。",
    "T28n1556_001:0995c15_9": "觸。",
    "T28n1556_001:0995c15_10": "作意。",
    "T28n1556_001:0995c16_0": "欲。",
    "T28n1556_001:0995c16_1": "勝解。",
    "T28n1556_001:0995c16_2": "信。",
    "T28n1556_001:0995c16_3": "精進。",
    "T28n1556_001:0995c16_4": "念。",
    "T28n1556_001:0995c16_5": "定。",
    "T28n1556_001:0995c16_6": "慧。",
    "T28n1556_001:0995c16_7": "尋。",
    "T28n1556_001:0995c16_8": "伺。",
    "T28n1556_001:0995c16_9": "放逸。",
    "T28n1556_001:0995c16_10": "不放逸。",
    "T28n1556_001:0995c17_11": "善根。",
    "T28n1556_001:0995c17_12": "不善根。",
    "T28n1556_001:0995c17_13": "無記根。",
    "T28n1556_001:0995c17_14": "諸結。",
    "T28n1556_001:0995c17_15": "繫縛。",
    "T28n1556_001:0995c17_16": "微廣。",
    "T28n1556_001:0995c17_17": "諸隨煩惱。",
    "T28n1556_001:0995c18_18": "纏繞安住。",
    "T28n1556_001:0995c18_19": "知。",
    "T28n1556_001:0995c18_20": "見。",
    "T28n1556_001:0995c18_21": "所有現觀。",
    "T28n1556_001:0995c18_22": "復有餘法。",
    "T28n1556_001:0995c19_23": "與心相應如是等類。",
    "T28n1556_001:0995c19_24": "名為心所有法。",
    "T28n1556_001:0995c19_25": "何名心不相應行法。",
    "T28n1556_001:0995c20_26": "謂有諸法與心不相應。",
    "T28n1556_001:0995c20_27": "彼復云何。",
    "T28n1556_001:0995c21_28": "謂得。",
    "T28n1556_001:0995c21_29": "無相等至。",
    "T28n1556_001:0995c21_30": "滅盡等至。",
    "T28n1556_001:0995c21_31": "無相所有。",
    "T28n1556_001:0995c22_0": "命根。",
    "T28n1556_001:0995c22_1": "眾同分。",
    "T28n1556_001:0995c22_2": "得處所。",
    "T28n1556_001:0995c22_3": "得事。",
    "T28n1556_001:0995c22_4": "得處。",
    "T28n1556_001:0995c22_5": "生。",
    "T28n1556_001:0995c22_6": "老。",
    "T28n1556_001:0995c22_7": "住。",
    "T28n1556_001:0995c22_8": "無常性。",
    "T28n1556_001:0995c23_9": "名身。",
    "T28n1556_001:0995c23_10": "句身。",
    "T28n1556_001:0995c23_11": "及以文身。",
    "T28n1556_001:0995c23_12": "復有餘法與心不相應。",
    "T28n1556_001:0995c24_13": "如是等類。",
    "T28n1556_001:0995c24_14": "名為心不相應行法。",
    "T28n1556_001:0995c25_0": "何名無為法。",
    "T28n1556_001:0995c25_1": "謂三種無為。",
    "T28n1556_001:0995c25_2": "虛空。",
    "T28n1556_001:0995c25_3": "非擇滅。",
    "T28n1556_001:0995c25_4": "及以擇滅。",
    "T28n1556_001:0995c26_5": "此名無為法。",
    "T28n1556_001:0995c26_6": "云何地界。",
    "T28n1556_001:0995c26_7": "謂堅硬性。",
    "T28n1556_001:0995c26_8": "云何水界。",
    "T28n1556_001:0995c27_9": "謂薄潤性。",
    "T28n1556_001:0995c27_10": "云何火界謂熱瞹性。",
    "T28n1556_001:0995c27_11": "云何風界。",
    "T28n1556_001:0995c28_12": "謂輕動性。",
    "T28n1556_001:0995c28_13": "云何眼根。",
    "T28n1556_001:0995c28_14": "謂眼識依清淨色。",
    "T28n1556_001:0995c28_15": "云何耳根。",
    "T28n1556_001:0995c29_16": "謂耳識依清淨色。",
    "T28n1556_001:0995c29_17": "云何鼻根。",
    "T28n1556_001:0995c29_18": "謂鼻識依清淨色。",
    "T28n1556_001:0996a01_19": "云何舌根。",
    "T28n1556_001:0996a01_20": "謂舌識依清淨色。",
    "T28n1556_001:0996a01_21": "云何身根。",
    "T28n1556_001:0996a02_22": "謂身識依清淨色。",
    "T28n1556_001:0996a02_23": "云何諸色。",
    "T28n1556_001:0996a02_24": "謂諸好色及非好色。",
    "T28n1556_001:0996a03_25": "彼二中間所住諸色及顯色等。",
    "T28n1556_001:0996a04_26": "最初一識眼識所知。",
    "T28n1556_001:0996a04_27": "眼識所受。",
    "T28n1556_001:0996a04_28": "意識能知。",
    "T28n1556_001:0996a05_29": "是故諸色眼識意識二識了知。",
    "T28n1556_001:0996a05_30": "云何諸聲。",
    "T28n1556_001:0996a06_0": "聲有二種。",
    "T28n1556_001:0996a06_1": "謂執受大種因所生。",
    "T28n1556_001:0996a06_2": "及非執受大種因所生。",
    "T28n1556_001:0996a07_3": "最初一識耳識所知。",
    "T28n1556_001:0996a07_4": "耳識所受。",
    "T28n1556_001:0996a07_5": "意識能知。",
    "T28n1556_001:0996a08_6": "是故諸聲耳識意識二識了知。",
    "T28n1556_001:0996a08_7": "云何諸香。",
    "T28n1556_001:0996a09_8": "謂好香惡香平等了香。",
    "T28n1556_001:0996a09_9": "最初一識鼻識所知。",
    "T28n1556_001:0996a10_10": "鼻識所受。",
    "T28n1556_001:0996a10_11": "意識能知。",
    "T28n1556_001:0996a10_12": "是故諸香鼻識意識二識了知。",
    "T28n1556_001:0996a11_13": "云何諸味。",
    "T28n1556_001:0996a11_14": "謂可意及非可意平等甞味。",
    "T28n1556_001:0996a12_15": "最初一識舌識所知。",
    "T28n1556_001:0996a12_16": "舌識所受。",
    "T28n1556_001:0996a12_17": "意識能知。",
    "T28n1556_001:0996a13_18": "是故諸味舌識意識二識了知。",
    "T28n1556_001:0996a13_19": "云何所觸一分。",
    "T28n1556_001:0996a14_20": "謂滑性。",
    "T28n1556_001:0996a14_21": "澁性。",
    "T28n1556_001:0996a14_22": "輕性。",
    "T28n1556_001:0996a14_23": "重性。",
    "T28n1556_001:0996a14_24": "冷飢渴等。",
    "T28n1556_001:0996a15_25": "最初一識身識所知。",
    "T28n1556_001:0996a15_26": "身識所受。",
    "T28n1556_001:0996a15_27": "意識能知。",
    "T28n1556_001:0996a16_0": "是故諸觸一分身識意識二識了知。",
    "T28n1556_001:0996a16_1": "何等名為諸無表色。",
    "T28n1556_001:0996a17_2": "謂法處所攝色。",
    "T28n1556_001:0996a17_3": "於一切時唯一意識之所了知知。",
    "T28n1556_001:0996a18_4": "云何眼識。",
    "T28n1556_001:0996a18_5": "依於眼根。",
    "T28n1556_001:0996a18_6": "各別了色。",
    "T28n1556_001:0996a19_7": "云何耳識。",
    "T28n1556_001:0996a19_8": "依於耳根。",
    "T28n1556_001:0996a19_9": "各別了聲。",
    "T28n1556_001:0996a19_10": "云何鼻識。",
    "T28n1556_001:0996a20_11": "依於鼻根。",
    "T28n1556_001:0996a20_12": "各別了香。",
    "T28n1556_001:0996a20_13": "云何舌識。",
    "T28n1556_001:0996a20_14": "依於舌根。",
    "T28n1556_001:0996a21_15": "各別了味。",
    "T28n1556_001:0996a21_16": "云何身識。",
    "T28n1556_001:0996a21_17": "依於身根。",
    "T28n1556_001:0996a21_18": "各別了觸。",
    "T28n1556_001:0996a22_19": "云何意識。",
    "T28n1556_001:0996a22_20": "依於意根。",
    "T28n1556_001:0996a22_21": "各別了法。",
    "T28n1556_001:0996a23_0": "云何為受。",
    "T28n1556_001:0996a23_1": "謂領納性。",
    "T28n1556_001:0996a23_2": "此復三種。",
    "T28n1556_001:0996a23_3": "謂苦樂不苦不樂。",
    "T28n1556_001:0996a24_4": "云何為相。",
    "T28n1556_001:0996a24_5": "謂遍知性。",
    "T28n1556_001:0996a24_6": "此復三種。",
    "T28n1556_001:0996a24_7": "小大無量。",
    "T28n1556_001:0996a25_8": "云何為思。",
    "T28n1556_001:0996a25_9": "謂心造行意所作業。",
    "T28n1556_001:0996a25_10": "此復三種。",
    "T28n1556_001:0996a26_0": "謂善不善及以無記。",
    "T28n1556_001:0996a26_1": "云何為觸。",
    "T28n1556_001:0996a26_2": "謂三和合。",
    "T28n1556_001:0996a26_3": "此復三種。",
    "T28n1556_001:0996a27_4": "受樂受苦。",
    "T28n1556_001:0996a27_5": "及受不苦不樂。",
    "T28n1556_001:0996a27_6": "云何作意。",
    "T28n1556_001:0996a28_0": "謂心所轉。",
    "T28n1556_001:0996a28_1": "此復三種。",
    "T28n1556_001:0996a28_2": "謂學無學非學非無學。",
    "T28n1556_001:0996b01_0": "云何為欲。",
    "T28n1556_001:0996b01_1": "謂樂作性。",
    "T28n1556_001:0996b01_2": "云何勝解。",
    "T28n1556_001:0996b01_3": "謂心所樂樂性樂作。",
    "T28n1556_001:0996b02_4": "云何為信。",
    "T28n1556_001:0996b02_5": "謂心極淨。",
    "T28n1556_001:0996b02_6": "云何精進。",
    "T28n1556_001:0996b02_7": "謂心欣樂。",
    "T28n1556_001:0996b03_8": "云何為念。",
    "T28n1556_001:0996b03_9": "心明記性。",
    "T28n1556_001:0996b03_10": "云何為定。",
    "T28n1556_001:0996b03_11": "心一境性。",
    "T28n1556_001:0996b04_12": "云何為慧。",
    "T28n1556_001:0996b04_13": "謂決擇法。",
    "T28n1556_001:0996b04_14": "云何為尋。",
    "T28n1556_001:0996b04_15": "心廉為性。",
    "T28n1556_001:0996b05_16": "云何為伺。",
    "T28n1556_001:0996b05_17": "心細為性。",
    "T28n1556_001:0996b05_18": "云何放逸。",
    "T28n1556_001:0996b05_19": "不修諸善。",
    "T28n1556_001:0996b06_20": "云何不放逸。",
    "T28n1556_001:0996b06_21": "謂修諸善。",
    "T28n1556_001:0996b06_22": "諸善根者。",
    "T28n1556_001:0996b06_23": "謂三善根。",
    "T28n1556_001:0996b07_24": "無貪善根。",
    "T28n1556_001:0996b07_25": "無瞋善根。",
    "T28n1556_001:0996b07_26": "無癡善根。",
    "T28n1556_001:0996b07_27": "不善根者。",
    "T28n1556_001:0996b08_28": "三不善根。",
    "T28n1556_001:0996b08_29": "貪不善根。",
    "T28n1556_001:0996b08_30": "瞋不善根。",
    "T28n1556_001:0996b08_31": "癡不善根。",
    "T28n1556_001:0996b09_32": "無記根者。",
    "T28n1556_001:0996b09_33": "四無記根。",
    "T28n1556_001:0996b09_34": "謂無記愛見慢無明。",
    "T28n1556_001:0996b10_0": "云何無記愛。",
    "T28n1556_001:0996b10_1": "謂色無色五種所作。",
    "T28n1556_001:0996b10_2": "云何無記見。",
    "T28n1556_001:0996b11_3": "謂欲界中所行身見及以邊見。",
    "T28n1556_001:0996b11_4": "色無色中所行五見。",
    "T28n1556_001:0996b12_5": "云何無記慢。",
    "T28n1556_001:0996b12_6": "謂色無色五種所行。",
    "T28n1556_001:0996b13_0": "云何無記無明。",
    "T28n1556_001:0996b13_1": "謂欲界中所行身見及以邊見相應無明。",
    "T28n1556_001:0996b14_2": "色無色中五種所行。",
    "T28n1556_001:0996b14_3": "言諸結者。",
    "T28n1556_001:0996b15_0": "謂九種結。",
    "T28n1556_001:0996b15_1": "貪結恚結慢結無明結見結勝執結疑結疾結慳結。",
    "T28n1556_001:0996b16_2": "云何貪結。",
    "T28n1556_001:0996b16_3": "謂三界貪。",
    "T28n1556_001:0996b16_4": "云何恚結。",
    "T28n1556_001:0996b17_5": "依諸有情忿恨為性。",
    "T28n1556_001:0996b17_6": "云何慢結。",
    "T28n1556_001:0996b17_7": "謂七種慢名為慢結。",
    "T28n1556_001:0996b18_8": "謂慢。",
    "T28n1556_001:0996b18_9": "過慢。",
    "T28n1556_001:0996b18_10": "慢過慢。",
    "T28n1556_001:0996b18_11": "我慢。",
    "T28n1556_001:0996b18_12": "增上慢。",
    "T28n1556_001:0996b19_13": "卑慢。",
    "T28n1556_001:0996b19_14": "邪慢。",
    "T28n1556_001:0996b19_15": "云何為慢於劣計勝於等計等彼以為因。",
    "T28n1556_001:0996b20_16": "慢持慢性心之高舉。",
    "T28n1556_001:0996b20_17": "心之所慠。",
    "T28n1556_001:0996b20_18": "心之所執。",
    "T28n1556_001:0996b21_19": "此名為慢。",
    "T28n1556_001:0996b21_20": "云何過慢。",
    "T28n1556_001:0996b21_21": "於等計已。",
    "T28n1556_001:0996b21_22": "勝或於勝計。",
    "T28n1556_001:0996b22_23": "已等彼以為因。",
    "T28n1556_001:0996b22_24": "慢特慢性心之高舉。",
    "T28n1556_001:0996b23_0": "心之所傲。",
    "T28n1556_001:0996b23_1": "心之所執。",
    "T28n1556_001:0996b23_2": "此名慢過慢。",
    "T28n1556_001:0996b24_0": "云何我慢。",
    "T28n1556_001:0996b24_1": "謂於五取蘊隨觀為我。",
    "T28n1556_001:0996b24_2": "或為我所。",
    "T28n1556_001:0996b25_0": "彼以為因慢恃慢性。",
    "T28n1556_001:0996b25_1": "心之高舉。",
    "T28n1556_001:0996b25_2": "心之所慠。",
    "T28n1556_001:0996b26_0": "心之所執。",
    "T28n1556_001:0996b26_1": "此名我慢。",
    "T28n1556_001:0996b26_2": "云何增上慢。",
    "T28n1556_001:0996b26_3": "謂於未得上之殊勝計已謂得上之殊勝或未證觸亦未現前上之殊勝計已證明彼以為因。",
    "T28n1556_001:0996b28_4": "慢恃慢性心之高舉。",
    "T28n1556_001:0996c01_5": "心之所傲。",
    "T28n1556_001:0996c01_6": "心之所執。",
    "T28n1556_001:0996c01_7": "此名增上慢。",
    "T28n1556_001:0996c02_8": "云何卑慢。",
    "T28n1556_001:0996c02_9": "謂於多分殊勝計已少分下劣。",
    "T28n1556_001:0996c03_10": "彼以為因。",
    "T28n1556_001:0996c03_11": "慢恃慢性。",
    "T28n1556_001:0996c03_12": "心之高舉。",
    "T28n1556_001:0996c03_13": "心之所傲。",
    "T28n1556_001:0996c04_14": "心之所執。",
    "T28n1556_001:0996c04_15": "此名卑慢。",
    "T28n1556_001:0996c04_16": "云何邪慢。",
    "T28n1556_001:0996c04_17": "謂具非德計已具德彼以為因。",
    "T28n1556_001:0996c05_18": "慢恃慢性。",
    "T28n1556_001:0996c05_19": "心之高舉。",
    "T28n1556_001:0996c06_20": "心之所慠。",
    "T28n1556_001:0996c06_21": "心之所執。",
    "T28n1556_001:0996c06_22": "此名邪慢。",
    "T28n1556_001:0996c06_23": "此七種慢名為慢結。",
    "T28n1556_001:0996c07_24": "云何無明結。",
    "T28n1556_001:0996c07_25": "謂三界無知。",
    "T28n1556_001:0996c07_26": "此名無明結。",
    "T28n1556_001:0996c08_27": "云何見結。",
    "T28n1556_001:0996c08_28": "謂三種結名為見結。",
    "T28n1556_001:0996c08_29": "云何為三。",
    "T28n1556_001:0996c09_30": "一身見。",
    "T28n1556_001:0996c09_31": "二邊執見。",
    "T28n1556_001:0996c09_32": "三邪見。",
    "T28n1556_001:0996c09_33": "云何身見。",
    "T28n1556_001:0996c09_34": "謂於五取蘊隨觀為我。",
    "T28n1556_001:0996c10_35": "或為我所。",
    "T28n1556_001:0996c10_36": "彼以為因。",
    "T28n1556_001:0996c10_37": "忍樂慧觀。",
    "T28n1556_001:0996c11_38": "及以所見此名身見。",
    "T28n1556_001:0996c11_39": "云何邊執見。",
    "T28n1556_001:0996c11_40": "謂於五取蘊隨觀斷常。",
    "T28n1556_001:0996c12_41": "彼以為因。",
    "T28n1556_001:0996c12_42": "忍樂慧觀及以所見此名邊執見。",
    "T28n1556_001:0996c13_43": "云何邪見。",
    "T28n1556_001:0996c13_44": "謂謗因果及以作用壞有之事彼以為因。",
    "T28n1556_001:0996c14_45": "忍樂慧觀及以所見此名邪見。",
    "T28n1556_001:0996c15_46": "此三種見名為見結。",
    "T28n1556_001:0996c15_47": "云何勝執結。",
    "T28n1556_001:0996c16_48": "二種勝執名勝執結。",
    "T28n1556_001:0996c16_49": "云何為二。",
    "T28n1556_001:0996c16_50": "一見勝執。",
    "T28n1556_001:0996c17_51": "二戒禁勝執。",
    "T28n1556_001:0996c17_52": "云何見勝執。",
    "T28n1556_001:0996c17_53": "謂於五取蘊隨觀。",
    "T28n1556_001:0996c18_54": "為最為上為勝為極彼以為因。",
    "T28n1556_001:0996c18_55": "忍樂慧觀及以所見此名勝執見。",
    "T28n1556_001:0996c19_56": "云何戒禁勝取。",
    "T28n1556_001:0996c19_57": "謂於五取蘊隨觀為淨為解脫為出離彼以為因。",
    "T28n1556_001:0996c21_0": "忍樂慧觀及以所見此名戒禁勝取。",
    "T28n1556_001:0996c21_1": "此二種勝取名勝執結。",
    "T28n1556_001:0996c22_2": "云何疑結。",
    "T28n1556_001:0996c22_3": "謂於諸諦猶豫為性。",
    "T28n1556_001:0996c23_4": "云何疾結。",
    "T28n1556_001:0996c23_5": "心亂為性。",
    "T28n1556_001:0996c23_6": "云何慳結。",
    "T28n1556_001:0996c23_7": "心執為性。",
    "T28n1556_001:0996c24_8": "是故說為諸結。",
    "T28n1556_001:0996c24_9": "言繫縛者。",
    "T28n1556_001:0996c24_10": "所說諸結。",
    "T28n1556_001:0996c24_11": "彼亦名縛。",
    "T28n1556_001:0996c25_12": "然縛有三種。",
    "T28n1556_001:0996c25_13": "謂貪縛瞋縛癡縛故說為縛。",
    "T28n1556_001:0996c26_14": "言微廣者。",
    "T28n1556_001:0996c26_15": "謂七微廣。",
    "T28n1556_001:0996c26_16": "云何為七。",
    "T28n1556_001:0996c26_17": "一貪微廣。",
    "T28n1556_001:0996c27_18": "二瞋微廣。",
    "T28n1556_001:0996c27_19": "三有欲微廣。",
    "T28n1556_001:0996c27_20": "四慢微廣。",
    "T28n1556_001:0996c27_21": "五無明微廣。",
    "T28n1556_001:0996c28_22": "六見微廣。",
    "T28n1556_001:0996c28_23": "七疑微廣。",
    "T28n1556_001:0996c28_24": "云何貪微廣。",
    "T28n1556_001:0996c28_25": "五種微廣名貪微廣。",
    "T28n1556_001:0997a01_26": "云何為五。",
    "T28n1556_001:0997a01_27": "謂欲界所繫見苦所斷所有貪欲。",
    "T28n1556_001:0997a02_28": "欲界所繫見集滅道。",
    "T28n1556_001:0997a02_29": "及修所斷所有貪欲。",
    "T28n1556_001:0997a03_30": "此五微廣名貪微廣。",
    "T28n1556_001:0997a03_31": "云何瞋微廣此五種。",
    "T28n1556_001:0997a05_0": "微廣名瞋微廣。",
    "T28n1556_001:0997a05_1": "云何為五。",
    "T28n1556_001:0997a05_2": "謂見苦所斷所有瞋恚。",
    "T28n1556_001:0997a06_3": "見集滅道及修所斷所有瞋恚。",
    "T28n1556_001:0997a06_4": "此五微廣名瞋微廣。",
    "T28n1556_001:0997a07_5": "云何有欲微廣。",
    "T28n1556_001:0997a07_6": "此十微廣名有欲微廣。",
    "T28n1556_001:0997a08_7": "云何為十。",
    "T28n1556_001:0997a08_8": "謂色繫五。",
    "T28n1556_001:0997a08_9": "無色繫五。",
    "T28n1556_001:0997a08_10": "云何名為有貪微廣。",
    "T28n1556_001:0997a09_11": "色界繫五。",
    "T28n1556_001:0997a09_12": "謂色界繫見苦所斷所有請欲。",
    "T28n1556_001:0997a10_13": "及色界繫見集滅道及修所斷所有貪欲。",
    "T28n1556_001:0997a11_14": "如色繫五。",
    "T28n1556_001:0997a11_15": "無色繫五。",
    "T28n1556_001:0997a11_16": "亦復如是。",
    "T28n1556_001:0997a11_17": "此十微廣名有欲微廣。",
    "T28n1556_001:0997a12_18": "云何慢微廣。",
    "T28n1556_001:0997a12_19": "此十五種微廣名慢微廣。",
    "T28n1556_001:0997a13_20": "云何為十五。",
    "T28n1556_001:0997a13_21": "謂欲繫五。",
    "T28n1556_001:0997a13_22": "色界繫五。",
    "T28n1556_001:0997a14_23": "無色界繫五。",
    "T28n1556_001:0997a14_24": "何等名為欲界所繫五種慢微廣。",
    "T28n1556_001:0997a15_25": "謂欲界繫見苦所斷所有諸慢。",
    "T28n1556_001:0997a15_26": "欲界所繫見集滅道及修所斷所有諸慢。",
    "T28n1556_001:0997a16_27": "如欲繫五。",
    "T28n1556_001:0997a17_28": "色界繫五。",
    "T28n1556_001:0997a17_29": "無色繫五。",
    "T28n1556_001:0997a17_30": "亦復如是。",
    "T28n1556_001:0997a17_31": "此十五種微廣名慢微廣。",
    "T28n1556_001:0997a18_32": "云何無明微廣。",
    "T28n1556_001:0997a18_33": "此十五種微廣為無明微廣。",
    "T28n1556_001:0997a19_34": "何等十五。",
    "T28n1556_001:0997a19_35": "謂欲繫五。",
    "T28n1556_001:0997a19_36": "色界繫五。",
    "T28n1556_001:0997a20_37": "無色繫五。",
    "T28n1556_001:0997a20_38": "云何名為欲界所繫五種慢微廣。",
    "T28n1556_001:0997a21_39": "謂欲所繫見苦所斷所有光明。",
    "T28n1556_001:0997a21_40": "欲界所繫見集滅道及修所斷所有光明。",
    "T28n1556_001:0997a22_41": "如欲繫五。",
    "T28n1556_001:0997a22_42": "色界繫五。",
    "T28n1556_001:0997a23_43": "無色繫五。",
    "T28n1556_001:0997a23_44": "亦復如是。",
    "T28n1556_001:0997a23_45": "此十五種微廣名無明微廣。",
    "T28n1556_001:0997a24_46": "云何見微廣。",
    "T28n1556_001:0997a24_47": "謂三十六微廣名見微廣。",
    "T28n1556_001:0997a25_48": "何等為三十六。",
    "T28n1556_001:0997a25_49": "謂欲繫十二。",
    "T28n1556_001:0997a25_50": "色繫十二。",
    "T28n1556_001:0997a26_51": "無色繫十二。",
    "T28n1556_001:0997a26_52": "何等名為欲界所繫十二見微廣。",
    "T28n1556_001:0997a27_53": "謂欲界繫身見邊見。",
    "T28n1556_001:0997a27_54": "欲界所繫見苦所斷所有邪見及以見取。",
    "T28n1556_001:0997a28_55": "欲界所繫見滅所斷所有邪見及以見取。",
    "T28n1556_001:0997a29_56": "欲界所繫見道所斷所有邪見及以見取執禁戒取。",
    "T28n1556_001:0997b01_57": "如欲界所繫十二。",
    "T28n1556_001:0997b02_58": "色繫十二。",
    "T28n1556_001:0997b02_59": "無色繫十二。",
    "T28n1556_001:0997b02_60": "亦復如是。",
    "T28n1556_001:0997b02_61": "此三十六微廣名見微廣。",
    "T28n1556_001:0997b03_62": "云何疑微廣。",
    "T28n1556_001:0997b03_63": "十二種微廣名疑微廣。",
    "T28n1556_001:0997b04_64": "何等十二。",
    "T28n1556_001:0997b04_65": "謂欲繫四。",
    "T28n1556_001:0997b04_66": "色界繫四。",
    "T28n1556_001:0997b04_67": "無色繫四。",
    "T28n1556_001:0997b05_68": "云何名為欲界所繫四疑微廣。",
    "T28n1556_001:0997b05_69": "謂欲界繫見苦斷疑。",
    "T28n1556_001:0997b07_0": "欲界所繫見集滅道所斷諸疑。",
    "T28n1556_001:0997b07_1": "如欲界繫。",
    "T28n1556_001:0997b07_2": "色界繫四。",
    "T28n1556_001:0997b08_3": "無色繫四。",
    "T28n1556_001:0997b08_4": "亦復如是。",
    "T28n1556_001:0997b08_5": "此十二種微廣名疑微廣。",
    "T28n1556_001:0997b09_6": "故說名微廣。",
    "T28n1556_001:0997b09_7": "言諸隨煩惱者。",
    "T28n1556_001:0997b09_8": "所有微廣彼則名為諸隨煩惱。",
    "T28n1556_001:0997b10_9": "有是隨煩惱非微廣。",
    "T28n1556_001:0997b11_10": "謂除微廣餘心所生。",
    "T28n1556_001:0997b11_11": "染污行蘊。",
    "T28n1556_001:0997b11_12": "纏繞安住者。",
    "T28n1556_001:0997b12_13": "謂八種纏繞安住。",
    "T28n1556_001:0997b12_14": "云何為八。",
    "T28n1556_001:0997b12_15": "一惛沈。",
    "T28n1556_001:0997b12_16": "二睡眠。",
    "T28n1556_001:0997b13_17": "三掉舉。",
    "T28n1556_001:0997b13_18": "四惡作。",
    "T28n1556_001:0997b13_19": "五疾妬。",
    "T28n1556_001:0997b13_20": "六慳悋。",
    "T28n1556_001:0997b13_21": "七無慚。",
    "T28n1556_001:0997b14_0": "八無愧。",
    "T28n1556_001:0997b14_1": "是故說名纏繞安住。",
    "T28n1556_001:0997b14_2": "言知者謂十種智。",
    "T28n1556_001:0997b15_3": "何等為十。",
    "T28n1556_001:0997b15_4": "一法智。",
    "T28n1556_001:0997b15_5": "二隨類智。",
    "T28n1556_001:0997b15_6": "三他心智。",
    "T28n1556_001:0997b15_7": "四世俗智。",
    "T28n1556_001:0997b16_8": "五苦智。",
    "T28n1556_001:0997b16_9": "六集智。",
    "T28n1556_001:0997b16_10": "七滅智。",
    "T28n1556_001:0997b16_11": "八道智。",
    "T28n1556_001:0997b16_12": "九盡智。",
    "T28n1556_001:0997b17_13": "十無生智。",
    "T28n1556_001:0997b17_14": "云何法智。",
    "T28n1556_001:0997b17_15": "謂欲繫諸行了無漏智。",
    "T28n1556_001:0997b18_16": "欲繫行因無漏諸智。",
    "T28n1556_001:0997b18_17": "欲繫行滅無漏諸智。",
    "T28n1556_001:0997b19_0": "欲界繫行斷道之中無漏諸智。",
    "T28n1556_001:0997b19_1": "復次法智。",
    "T28n1556_001:0997b19_2": "法智地中無漏諸智。",
    "T28n1556_001:0997b20_3": "此名法智。",
    "T28n1556_001:0997b20_4": "云何隨類智。",
    "T28n1556_001:0997b20_5": "色無色繫諸行之中無漏諸智。",
    "T28n1556_001:0997b21_6": "色無色繫諸行目中無漏諸智。",
    "T28n1556_001:0997b22_7": "色無色繫行滅之中無漏諸智。",
    "T28n1556_001:0997b23_8": "色無色繫行斷道中無漏諸智。",
    "T28n1556_001:0997b23_9": "復次隨所了智。",
    "T28n1556_001:0997b24_10": "隨了地中無漏諸智。",
    "T28n1556_001:0997b24_11": "此等名為隨類智。",
    "T28n1556_001:0997b25_0": "云何他心智。",
    "T28n1556_001:0997b25_1": "從修生智。",
    "T28n1556_001:0997b25_2": "所修之果。",
    "T28n1556_001:0997b25_3": "依於所修所得無退。",
    "T28n1556_001:0997b26_4": "以其智慧。",
    "T28n1556_001:0997b26_5": "了欲所行及色所行似他心心法。",
    "T28n1556_001:0997b27_6": "證得現前了無漏法一分。",
    "T28n1556_001:0997b27_7": "此等名為了他心智。",
    "T28n1556_001:0997b28_8": "云何世俗智。",
    "T28n1556_001:0997b28_9": "謂有漏智此名世俗智。",
    "T28n1556_001:0997b29_10": "云何苦智。",
    "T28n1556_001:0997b29_11": "於五取蘊了無常性。",
    "T28n1556_001:0997b29_12": "苦性。",
    "T28n1556_001:0997b29_13": "空性。",
    "T28n1556_001:0997c01_14": "無我之性。",
    "T28n1556_001:0997c01_15": "作意所生無漏之智。",
    "T28n1556_001:0997c01_16": "此名苦智。",
    "T28n1556_001:0997c02_0": "云何集智。",
    "T28n1556_001:0997c02_1": "於有漏因而起。",
    "T28n1556_001:0997c02_2": "因集生緣作意無漏之智此名集智。",
    "T28n1556_001:0997c03_3": "云何滅智。",
    "T28n1556_001:0997c03_4": "謂於滅中起滅。",
    "T28n1556_001:0997c04_0": "靜妙出離作意。",
    "T28n1556_001:0997c04_1": "無漏之智此名滅智。",
    "T28n1556_001:0997c04_2": "云何道智。",
    "T28n1556_001:0997c05_3": "謂於道中起於道。",
    "T28n1556_001:0997c05_4": "如行出作意無漏之智此名道智。",
    "T28n1556_001:0997c06_5": "云何盡智。",
    "T28n1556_001:0997c06_6": "謂自遍了知若自知斷集。",
    "T28n1556_001:0997c07_7": "自知證滅。",
    "T28n1556_001:0997c07_8": "自知修道。",
    "T28n1556_001:0997c07_9": "以為其因。",
    "T28n1556_001:0997c07_10": "知見覺慧了知分明。",
    "T28n1556_001:0997c08_11": "現前證得。",
    "T28n1556_001:0997c08_12": "此名盡智。",
    "T28n1556_001:0997c08_13": "云何無生智。",
    "T28n1556_001:0997c09_0": "謂自遍了知諸苦故。",
    "T28n1556_001:0997c09_1": "更無可知之智自斷集故。",
    "T28n1556_001:0997c10_2": "更無可斷之智。",
    "T28n1556_001:0997c10_3": "自證滅故。",
    "T28n1556_001:0997c10_4": "更無可證之智。",
    "T28n1556_001:0997c11_0": "自修道故。",
    "T28n1556_001:0997c11_1": "更無可修之智。",
    "T28n1556_001:0997c11_2": "以為其因。",
    "T28n1556_001:0997c11_3": "知見覺慧了知分明。",
    "T28n1556_001:0997c12_4": "現前證得此名無生智。",
    "T28n1556_001:0997c12_5": "言見者。",
    "T28n1556_001:0997c13_0": "雖則彼智亦是其見亦復有見。",
    "T28n1556_001:0997c13_1": "非即是智。",
    "T28n1556_001:0997c13_2": "從現觀邊所生八忍。",
    "T28n1556_001:0997c14_3": "謂知苦法忍。",
    "T28n1556_001:0997c14_4": "知苦隨類忍。",
    "T28n1556_001:0997c15_0": "知集法忍。",
    "T28n1556_001:0997c15_1": "知集隨類忍。",
    "T28n1556_001:0997c15_2": "知滅法忍。",
    "T28n1556_001:0997c15_3": "知滅隨類忍。",
    "T28n1556_001:0997c16_4": "知道法忍。",
    "T28n1556_001:0997c16_5": "知道隨類忍。",
    "T28n1556_001:0997c16_6": "故說名見。",
    "T28n1556_001:0997c16_7": "言所有現觀者。",
    "T28n1556_001:0997c17_8": "即彼知見亦是現觀是故說為現觀。",
    "T28n1556_001:0997c18_0": "云何為得。",
    "T28n1556_001:0997c18_1": "謂諸法所獲。",
    "T28n1556_001:0997c18_2": "云何無想等至。",
    "T28n1556_001:0997c18_3": "謂已離遍淨貪。",
    "T28n1556_001:0997c19_4": "未離上貪。",
    "T28n1556_001:0997c19_5": "由出離想作意。",
    "T28n1556_001:0997c19_6": "為先心心法滅為性。",
    "T28n1556_001:0997c20_7": "云何滅盡等至。",
    "T28n1556_001:0997c20_8": "謂已離無所有處貪。",
    "T28n1556_001:0997c21_9": "由止息想作意為先心心法滅為性。",
    "T28n1556_001:0997c21_10": "云何無想所有。",
    "T28n1556_001:0997c22_11": "謂生無想有情天中心心法滅為性。",
    "T28n1556_001:0997c23_12": "云何命根。",
    "T28n1556_001:0997c23_13": "謂三界壽。",
    "T28n1556_001:0997c23_14": "云何眾同分。",
    "T28n1556_001:0997c23_15": "謂諸有情自類相似。",
    "T28n1556_001:0997c24_16": "云何得處所。",
    "T28n1556_001:0997c24_17": "謂獲諸境。",
    "T28n1556_001:0997c24_18": "云何得事。",
    "T28n1556_001:0997c25_19": "謂獲諸蘊。",
    "T28n1556_001:0997c25_20": "云何得處。",
    "T28n1556_001:0997c25_21": "謂獲內外處。",
    "T28n1556_001:0997c25_22": "云何為生。",
    "T28n1556_001:0997c26_23": "謂成就蘊。",
    "T28n1556_001:0997c26_24": "云何為老。",
    "T28n1556_001:0997c26_25": "謂諸蘊熟。",
    "T28n1556_001:0997c26_26": "云何為住。",
    "T28n1556_001:0997c27_27": "謂諸行不壞。",
    "T28n1556_001:0997c27_28": "云何無常性。",
    "T28n1556_001:0997c27_29": "謂諸行壞。",
    "T28n1556_001:0997c27_30": "云何名身。",
    "T28n1556_001:0997c28_31": "即彼增語。",
    "T28n1556_001:0997c28_32": "云何句身。",
    "T28n1556_001:0997c28_33": "謂文圓滿。",
    "T28n1556_001:0997c28_34": "云何文身。",
    "T28n1556_001:0997c29_35": "即彼字身。",
    "T28n1556_001:0997c29_36": "名為文身。",
    "T28n1556_001:0997c29_37": "云何處空。",
    "T28n1556_001:0997c29_38": "所行之因。",
    "T28n1556_001:0998a01_39": "即是虛空。",
    "T28n1556_001:0998a01_40": "非有障礙。",
    "T28n1556_001:0998a01_41": "諸色種類不能遍覆。",
    "T28n1556_001:0998a02_0": "此名虛空。",
    "T28n1556_001:0998a02_1": "云何非擇滅。",
    "T28n1556_001:0998a02_2": "謂滅非離。",
    "T28n1556_001:0998a02_3": "云何擇滅。",
    "T28n1556_001:0998a03_0": "謂滅亦離。",
    "T28n1556_001:0998a03_1": "是故說為法有五種。",
    "T28n1556_001:0998a03_2": "一者色法。",
    "T28n1556_001:0998a03_3": "二者心法。",
    "T28n1556_001:0998a04_4": "三者心所有法。",
    "T28n1556_001:0998a04_5": "四者心不相應行法。",
    "T28n1556_001:0998a05_0": "五者無為法。",
    "T28n1556_001:0998a06_0": "五事論一卷"
}