[
    {
        "source_sentence": "幻師颰陀神呪經亦云玄師颰陀所說神呪經",
        "from": "T21n1378b_001:0902a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東晉西域三藏竺曇無蘭譯",
        "from": "T21n1378b_001:0902a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞如是。一時佛遊於羅閱祇國竹園中鸚鵡",
        "from": "T21n1378b_001:0902a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樹間。",
        "from": "T21n1378b_001:0902a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時有一異比丘。",
        "from": "T21n1378b_001:0902a12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於竹園中去羅閱祇國。",
        "from": "T21n1378b_001:0902a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "適在中間道為毒蛇所齧。",
        "from": "T21n1378b_001:0902a13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復為鬼神所嬈。",
        "from": "T21n1378b_001:0902a13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復為賊所見劫。",
        "from": "T21n1378b_001:0902a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛見爾時便往到是比丘所時。",
        "from": "T21n1378b_001:0902a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幻師颰陀隨佛俱往。",
        "from": "T21n1378b_001:0902a15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幻師颰陀即白佛言。",
        "from": "T21n1378b_001:0902a15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我有術甚大神妙。",
        "from": "T21n1378b_001:0902a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今欲為呪。",
        "from": "T21n1378b_001:0902a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言止止颰陀。",
        "from": "T21n1378b_001:0902a16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝所說莫使有所傷害。",
        "from": "T21n1378b_001:0902a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "颰陀白佛言。",
        "from": "T21n1378b_001:0902a17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後當來世。",
        "from": "T21n1378b_001:0902a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當國國相攻伐賊賊更相劫。",
        "from": "T21n1378b_001:0902a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相憎者復更相劫。",
        "from": "T21n1378b_001:0902a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼神亦更相劫。",
        "from": "T21n1378b_001:0902a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒毒更相害。",
        "from": "T21n1378b_001:0902a19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有比丘在山中樹下坐。",
        "from": "T21n1378b_001:0902a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "披五納衣若在露地坐。",
        "from": "T21n1378b_001:0902a20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四輩弟子當令無有能嬈害者。",
        "from": "T21n1378b_001:0902a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆令安隱當令無有病痛如是。",
        "from": "T21n1378b_001:0902a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧莎梨嗪呢　欝遮梨　首黎　首黎　拔提珊提波陀尼　阿裴耶　達予　乙致䩚闍耶兮拔蹄樓𪝜陀僧莎傺𩋋　散提波陀尼曼陀羅睺那　比闍睺那如是祝　斷口語斷諸術斷諸病痛。",
        "from": "T21n1378b_001:0902a27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸閱叉皆作此術　須摩鬼　摩訶閱叉　瞿沙難摩訶閱叉　梵摩具摩閱叉　因阿羅具摩訶閱叉　阿祇眵摩訶閱叉。",
        "from": "T21n1378b_001:0902b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸閱叉在鬼神中尊。",
        "from": "T21n1378b_001:0902b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我字因偷諸相求者。",
        "from": "T21n1378b_001:0902b02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使諸欲病人者害人者。",
        "from": "T21n1378b_001:0902b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我皆使不知處。",
        "from": "T21n1378b_001:0902b03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "障蔽亂眼令不見。",
        "from": "T21n1378b_001:0902b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切人皆使不得其便。",
        "from": "T21n1378b_001:0902b04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當爾時呼言來。",
        "from": "T21n1378b_001:0902b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還持鬼神摩訶還持鬼神來。",
        "from": "T21n1378b_001:0902b05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兜勒摩訶兜勒來。",
        "from": "T21n1378b_001:0902b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因持兜勒羅摩訶因持兜勒羅來。",
        "from": "T21n1378b_001:0902b06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵摩兜羅摩訶梵摩兜羅來。",
        "from": "T21n1378b_001:0902b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因持兜羅摩訶因持兜羅來。",
        "from": "T21n1378b_001:0902b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩陀兜羅摩訶菩陀兜羅來。",
        "from": "T21n1378b_001:0902b08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "閱叉兜羅摩訶閱叉兜羅來。",
        "from": "T21n1378b_001:0902b09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿多得兜勒摩訶阿多得兜勒來。",
        "from": "T21n1378b_001:0902b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "首羅瞿沙兜勒摩訶首羅瞿沙兜勒。",
        "from": "T21n1378b_001:0902b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我字因偷諸怨家相求便者。",
        "from": "T21n1378b_001:0902b11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我皆救之覆蔽。",
        "from": "T21n1378b_001:0902b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使兩不相見。",
        "from": "T21n1378b_001:0902b12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為繩索所繫縛。",
        "from": "T21n1378b_001:0902b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "消滅所繫縛。",
        "from": "T21n1378b_001:0902b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡意所繫縛。",
        "from": "T21n1378b_001:0902b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲醉意所繫縛。",
        "from": "T21n1378b_001:0902b13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亂意所繫縛。",
        "from": "T21n1378b_001:0902b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但歸命佛一切皆得解脫。",
        "from": "T21n1378b_001:0902b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命佛時言南無佛告起作禮而去。",
        "from": "T21n1378b_001:0902b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幻師颰陀神呪經",
        "from": "T21n1378b_001:0902b16_100",
        "context_with_next_sentence": false
    }
]