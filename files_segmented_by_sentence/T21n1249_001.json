[
    {
        "source_sentence": "毘沙門儀軌",
        "from": "T21n1249_001:0227c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大興善寺三藏沙門大廣智",
        "from": "T21n1249_001:0227c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空奉　詔譯",
        "from": "T21n1249_001:0227c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿他渴嚧蔇室拏末拏寫摩訶羅闍寫摩羅滿怛藍鉢羅毘沙彌薩婆薩埵蘇訶鉢唅薩婆薩埵𠰘底史南怛姪他摩尼吠達馱羅野莎婆訶布嚕拏跋達囉野莎婆訶摩拏羅也野莎婆訶悉澄迦羅耶莎婆訶心真言唵薛室囉末拏耶娑婆訶有惡難及官府言語口舌事誦千遍心中心真言亦名諸佛同契陀羅尼。",
        "from": "T21n1249_001:0228a06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵彈上聲那馱羅野娑婆訶求財誦一萬遍功德天心呪唵枳儞枳儞薩婆迦哩野沙達尼辛儞辛儞阿溈慮蠶淴彌拏舍耶阿婆訶野淴彌弟毘室哩薜室羅末拏野娑婆訶淨身真言唵殑伽薩婆𭉐㗚他避木仚莎婆訶右手掬水誦七遍淨身召請真言娜謨薜室羅末拏寫摩訶翼史捺羅寫志他婆跋婆都哆莎婆訶欲召請供養誦七遍燈燭真言唵婆畢底梨娑婆訶七遍供養香華真言唵薩婆瞢褐羅陀哩尼莎婆訶淨器盛水及香等誦七遍發遣真言唵毘嚕迦野哆賴耶𭋼耶薩婆毒契瓢薩本惹三婆羅娑婆訶壇作法　牛糞塗地上。",
        "from": "T21n1249_001:0228a25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用香泥塗上。",
        "from": "T21n1249_001:0228a25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外稜兩肘一量。",
        "from": "T21n1249_001:0228a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內稜一肘一量。",
        "from": "T21n1249_001:0228a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "像在北面而坐南。",
        "from": "T21n1249_001:0228a26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內院著清水香華菓子。",
        "from": "T21n1249_001:0228a27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外院著乳粥餅等。",
        "from": "T21n1249_001:0228a27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香供養法真言摩拏阿羅他鉢哩布囉迦野莎婆訶安悉香。",
        "from": "T21n1249_001:0228b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "檀香。",
        "from": "T21n1249_001:0228b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍腦香。",
        "from": "T21n1249_001:0228b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天木香。",
        "from": "T21n1249_001:0228b02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魂膽香。",
        "from": "T21n1249_001:0228b02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右伴香擣訖蜜和。",
        "from": "T21n1249_001:0228b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於像前誦此呪一百八遍。",
        "from": "T21n1249_001:0228b03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持香精誠祝像前。",
        "from": "T21n1249_001:0228b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數滿訖由供養。",
        "from": "T21n1249_001:0228b04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如無香依時。",
        "from": "T21n1249_001:0228b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言亦當供養已上儀軌了。",
        "from": "T21n1249_001:0228b05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方大毘沙門天王。",
        "from": "T21n1249_001:0228b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐天寶元戴壬午歲。",
        "from": "T21n1249_001:0228b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大石康五國圍安西城其年二月十一日有表請兵救援。",
        "from": "T21n1249_001:0228b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖人告一行禪師曰。",
        "from": "T21n1249_001:0228b08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和尚安西被大石康□□□□□□國圍城。",
        "from": "T21n1249_001:0228b09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有表請兵。",
        "from": "T21n1249_001:0228b09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安西去京一萬二千里。",
        "from": "T21n1249_001:0228b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兵程八箇月然到其安西。",
        "from": "T21n1249_001:0228b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即無朕之。",
        "from": "T21n1249_001:0228b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有。",
        "from": "T21n1249_001:0228b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一行曰陛下何不請北方毘沙門天王神兵應援。",
        "from": "T21n1249_001:0228b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖人云朕如何請得。",
        "from": "T21n1249_001:0228b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一行曰喚取胡僧大廣智即請得。",
        "from": "T21n1249_001:0228b13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有勅喚得大廣智到內云。",
        "from": "T21n1249_001:0228b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖人所喚臣僧者。",
        "from": "T21n1249_001:0228b14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈不緣安西城被五國賊圍城聖人云是。",
        "from": "T21n1249_001:0228b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大廣智曰。",
        "from": "T21n1249_001:0228b15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陛下執香爐入道場。",
        "from": "T21n1249_001:0228b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與陛下請北方天王神兵救。",
        "from": "T21n1249_001:0228b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "急入道場請。",
        "from": "T21n1249_001:0228b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言未二七遍。",
        "from": "T21n1249_001:0228b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖人忽見有神人二三百人。",
        "from": "T21n1249_001:0228b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶甲於道場前立。",
        "from": "T21n1249_001:0228b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖人問僧曰此是何人。",
        "from": "T21n1249_001:0228b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大廣智曰。",
        "from": "T21n1249_001:0228b19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是北方毘沙門天王第二子獨健。",
        "from": "T21n1249_001:0228b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "領天兵救援安西故來辭。",
        "from": "T21n1249_001:0228b20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖人設食發遣。",
        "from": "T21n1249_001:0228b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至其年四月日。",
        "from": "T21n1249_001:0228b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安西表到云。",
        "from": "T21n1249_001:0228b21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "去二月十一日巳後午前。",
        "from": "T21n1249_001:0228b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "去城東北三十里。",
        "from": "T21n1249_001:0228b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有雲霧斗闇。",
        "from": "T21n1249_001:0228b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "霧中有人。",
        "from": "T21n1249_001:0228b23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身長一丈。",
        "from": "T21n1249_001:0228b23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "約三五百人盡著金甲。",
        "from": "T21n1249_001:0228b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至酉後鼓角大鳴。",
        "from": "T21n1249_001:0228b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聲震三百里。",
        "from": "T21n1249_001:0228b24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地動山崩停住三日。",
        "from": "T21n1249_001:0228b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五國大懼盡退軍。",
        "from": "T21n1249_001:0228b25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "抽兵諸營墜中。",
        "from": "T21n1249_001:0228b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並是金鼠咬弓弩絃。",
        "from": "T21n1249_001:0228b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及器械損斷盡不堪用。",
        "from": "T21n1249_001:0228b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有老弱去不得者。",
        "from": "T21n1249_001:0228b27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臣所管兵欲損之。",
        "from": "T21n1249_001:0228b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空中云放去不須殺。",
        "from": "T21n1249_001:0228b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尋聲反顧城北門樓上有大光明。",
        "from": "T21n1249_001:0228b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘沙門天王見身於樓上。",
        "from": "T21n1249_001:0228b29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其天王神樣。",
        "from": "T21n1249_001:0228c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謹隨表進上者。",
        "from": "T21n1249_001:0228c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中華天寶十四載。",
        "from": "T21n1249_001:0228c01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於內供養僧大悲處。",
        "from": "T21n1249_001:0228c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寫得經及像。",
        "from": "T21n1249_001:0228c02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至大曆五年。",
        "from": "T21n1249_001:0228c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於集洲見內供養僧良賁法師。",
        "from": "T21n1249_001:0228c03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "移住集洲開元寺。",
        "from": "T21n1249_001:0228c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勘經像與大悲本同。",
        "from": "T21n1249_001:0228c04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔防援國界。",
        "from": "T21n1249_001:0228c04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉佛教勅。",
        "from": "T21n1249_001:0228c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令第三子那吒捧塔隨天王。",
        "from": "T21n1249_001:0228c05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏大廣智云。",
        "from": "T21n1249_001:0228c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每月一日。",
        "from": "T21n1249_001:0228c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天王與諸天鬼神集會日。",
        "from": "T21n1249_001:0228c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十一日第二子獨健辭父王巡界日。",
        "from": "T21n1249_001:0228c07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十五日與四天王集會日。",
        "from": "T21n1249_001:0228c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十一日那吒與父王交塔日。",
        "from": "T21n1249_001:0228c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其日須乳粥供養。",
        "from": "T21n1249_001:0228c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無乳則用蘇蜜粥供養其天王。",
        "from": "T21n1249_001:0228c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有天靈異奉勅宣付十道節度。",
        "from": "T21n1249_001:0228c10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所在軍領令置形像。",
        "from": "T21n1249_001:0228c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈願供養。",
        "from": "T21n1249_001:0228c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天寶元載四月二十三日。",
        "from": "T21n1249_001:0228c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內謁者監高慧明宜天王第二子獨健。",
        "from": "T21n1249_001:0228c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常領天兵護其國界。",
        "from": "T21n1249_001:0228c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天王第三子那吒太子。",
        "from": "T21n1249_001:0228c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捧塔常隨天王。",
        "from": "T21n1249_001:0228c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉祥天女亦名功德天自有真言。",
        "from": "T21n1249_001:0228c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婆萸仙。",
        "from": "T21n1249_001:0228c15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大廣智云是觀世音菩薩化身。",
        "from": "T21n1249_001:0228c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若請召天王時。",
        "from": "T21n1249_001:0228c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結根本印。",
        "from": "T21n1249_001:0228c17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦請召真言七遍。",
        "from": "T21n1249_001:0228c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂上散印。",
        "from": "T21n1249_001:0228c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1249_001:0228c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也二合他一引曩謨吠室囉二合摩拏鼻音野二曩謨引馱曩上娜引野三馱曩濕嚩二合囉野四阿上引蘖瑳引跛哩弭多馱𩕳濕嚩二合囉跛囉麼迦引嚕抳迦五薩嚩薩怛嚩二合𠰘哆唧多二合麼麼六達曩麼弩鉢囉二合曳瑳七娑嚩野麼引蘖瑳八娑嚩二合賀已上上卷私記唵藥叉布多那吽因陀羅弊娑婆二合賀毘左者耶娑婆賀女使也即愛願也毘沙門天王心真言曩謨囉怛曩二合怛囉二合夜引也一曩謨吠室囉二合摩拏也二摩賀羅惹耶三薩縛薩怛嚩二合曩摩舍跛哩布囉拏也四悉地迦囉也五蘇騫娜也六怛娑每二合曩莫塞訖哩二合怛嚩二合七伊𤚥吠室囉摩拏也紇哩乃也八摩𮧬多以灑引弭九薩嚩薩怛嚩二合蘇佉縛憾十怛儞也二合他十一唵十二悉地悉地悉地悉地十三蘇母蘇母十四左左左左十五左羅左羅十六羯羅羯羅十七枳哩枳哩十八句嚕句嚕十九祖嚕祖嚕二十娑馱也二十一遏貪二十二摩摩𩕳底也末他拏婆去嚩娑縛二合賀二十三吠室囉摩拏也二十四娑嚩二合賀二十五達曩那也娑嚩二合賀二十七摩拏囉他跛哩布囉迦也二十八娑嚩二合賀二十九中天竺國三藏婆羅門達摩伽陀那譯南冒摩訶失利夜耶一南冒吠失囉二合漫那耶二摩訶藥叉二合細那拔陀曳三怛姪他四沮吠沮吠五別本云濕吠娑縛訶沮吠娑嚩訶莎縛莎嚩六扇演秣底娑嚩訶七施晚怛梨莎嚩訶八惹耶拔底梨娑縛訶我今說根本印。",
        "from": "T21n1249_001:0229a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手右押左內相叉。",
        "from": "T21n1249_001:0229a19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二無名指頭相合。",
        "from": "T21n1249_001:0229a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指如鉤。",
        "from": "T21n1249_001:0229a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若迎請即頂上散。",
        "from": "T21n1249_001:0229a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後取念珠專注念誦。",
        "from": "T21n1249_001:0229a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結吉祥天女身印。",
        "from": "T21n1249_001:0229a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手虛心合掌。",
        "from": "T21n1249_001:0229a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開二頭指二中指二無名指。",
        "from": "T21n1249_001:0229a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈如蓮華形。",
        "from": "T21n1249_001:0229a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指二小指竪合。",
        "from": "T21n1249_001:0229a23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若念誦時。",
        "from": "T21n1249_001:0229a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當心結印誦真言七遍。",
        "from": "T21n1249_001:0229a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂上散別本文。",
        "from": "T21n1249_001:0229a24_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法取白月及黑月十五日。",
        "from": "T21n1249_001:0229a25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於淨房中或露地面向北坐。",
        "from": "T21n1249_001:0229a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對像供養乳粥。",
        "from": "T21n1249_001:0229a26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及燒薰陸香。",
        "from": "T21n1249_001:0229a26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此陀羅尼三夜。",
        "from": "T21n1249_001:0229a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦一千八遍云云。",
        "from": "T21n1249_001:0229a27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若求官位對像前。",
        "from": "T21n1249_001:0229a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日一夜誦貴人名字一遍一稱之。",
        "from": "T21n1249_001:0229a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿一千八十遍。",
        "from": "T21n1249_001:0229a29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求官位即得。",
        "from": "T21n1249_001:0229a29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有鬪戰競處欲得強者。",
        "from": "T21n1249_001:0229b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用白㲲作索。",
        "from": "T21n1249_001:0229b01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一呪一結一千八十遍。",
        "from": "T21n1249_001:0229b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫左臂上。",
        "from": "T21n1249_001:0229b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更即勝。",
        "from": "T21n1249_001:0229b02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法用牛黃。",
        "from": "T21n1249_001:0229b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於銅器中盛。",
        "from": "T21n1249_001:0229b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對像前誦一百八遍。",
        "from": "T21n1249_001:0229b03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即取塗於身上。",
        "from": "T21n1249_001:0229b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得一切人愛敬。",
        "from": "T21n1249_001:0229b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲得國王憶念。",
        "from": "T21n1249_001:0229b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每日夜誦一千八十遍。",
        "from": "T21n1249_001:0229b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王即憶念所須皆得之。",
        "from": "T21n1249_001:0229b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同三藏之又譯也。",
        "from": "T21n1249_001:0229b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿陀伽部怛羅二合耶一地舍耶二吠賒羅三跋那寫四摩訶喝羅闍寫五夜叉地婆怛寫六婆摩怛寫七薩都怛寫八婆賒耶鉢囉二合婆訶那寫九喜摩達吒吒尼鉢羅二合沙夜迷十怛姪他十一俱灑彌俱灑彌俱沙毘賒羅摩那寫十二摩訶喝羅闍十三綺槃達伽寫十四哆羅摩車都十五娑婆訶行者上下衣服並淨潔。",
        "from": "T21n1249_001:0229b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一上廁一洗浴。",
        "from": "T21n1249_001:0229b15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白月十五日起首。",
        "from": "T21n1249_001:0229b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對像前誦呪滿十萬遍訖。",
        "from": "T21n1249_001:0229b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後取香復塗壇上。",
        "from": "T21n1249_001:0229b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復設種種飲食。",
        "from": "T21n1249_001:0229b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者取薰陸香一百八枚。",
        "from": "T21n1249_001:0229b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各呪一遍投著火中。",
        "from": "T21n1249_001:0229b18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天王即現。",
        "from": "T21n1249_001:0229b18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法行者若欲得。",
        "from": "T21n1249_001:0229b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曷羅闍所愛敬者。",
        "from": "T21n1249_001:0229b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取赤小豆。",
        "from": "T21n1249_001:0229b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一呪一遍投火中燒。",
        "from": "T21n1249_001:0229b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是滿一百八遍。",
        "from": "T21n1249_001:0229b20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其曷羅闍即遣人喚來極急。",
        "from": "T21n1249_001:0229b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲得大官愛敬者。",
        "from": "T21n1249_001:0229b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取白芥子一呪一遍投火中。",
        "from": "T21n1249_001:0229b22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是滿一百八遍。",
        "from": "T21n1249_001:0229b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即自來敬仰大喜。",
        "from": "T21n1249_001:0229b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲遠行。",
        "from": "T21n1249_001:0229b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪穀木柱。",
        "from": "T21n1249_001:0229b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行一切難更無著障礙。",
        "from": "T21n1249_001:0229b24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲人愛敬。",
        "from": "T21n1249_001:0229b25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取苦練子一百八顆。",
        "from": "T21n1249_001:0229b25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一呪一遍投火中燒之。",
        "from": "T21n1249_001:0229b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是一切人愛敬呪師如親父母。",
        "from": "T21n1249_001:0229b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉滿心所願。",
        "from": "T21n1249_001:0229b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲怨家遠相却者。",
        "from": "T21n1249_001:0229b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取苦練子百八顆。",
        "from": "T21n1249_001:0229b28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一呪一遍投火中燒。",
        "from": "T21n1249_001:0229b28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人即遠行不在。",
        "from": "T21n1249_001:0229b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲令人降伏。",
        "from": "T21n1249_001:0229b29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱前人姓名。",
        "from": "T21n1249_001:0229c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪酪一百八遍燒之。",
        "from": "T21n1249_001:0229c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法欲令自在威光者。",
        "from": "T21n1249_001:0229c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪墨塗額上。",
        "from": "T21n1249_001:0229c02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切見者無不愛仰。",
        "from": "T21n1249_001:0229c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若有鬼病心痛者。",
        "from": "T21n1249_001:0229c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪石榴華汁飲之即差。",
        "from": "T21n1249_001:0229c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若患野狐鬼魅。",
        "from": "T21n1249_001:0229c04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪楊枝打病者即差。",
        "from": "T21n1249_001:0229c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若患一切風。",
        "from": "T21n1249_001:0229c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪蘇二十一遍食之即差。",
        "from": "T21n1249_001:0229c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若患虐病者呪楊柳枝二十一遍。",
        "from": "T21n1249_001:0229c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令打病人即差。",
        "from": "T21n1249_001:0229c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法患狂言鬼語。",
        "from": "T21n1249_001:0229c07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪水令服之即差。",
        "from": "T21n1249_001:0229c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若患心痛。",
        "from": "T21n1249_001:0229c08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪黃土塗之。",
        "from": "T21n1249_001:0229c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患一切鬼病者。",
        "from": "T21n1249_001:0229c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪石榴杖百八遍。",
        "from": "T21n1249_001:0229c09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "打病人即差。",
        "from": "T21n1249_001:0229c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作印法。",
        "from": "T21n1249_001:0229c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二小指相鉤。",
        "from": "T21n1249_001:0229c10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二無名指向外直竪。",
        "from": "T21n1249_001:0229c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指二頭指反相叉。",
        "from": "T21n1249_001:0229c11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大母指向外直竪。",
        "from": "T21n1249_001:0229c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手掌相背是以上所說二法中皆用此印誦此真言數滿。",
        "from": "T21n1249_001:0229c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦使者真言七遍。",
        "from": "T21n1249_001:0229c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又誦八天真言各三遍。",
        "from": "T21n1249_001:0229c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘沙門天王使者呪娜謨吠失羅二合鉢那耶摩訶藥叉栖那鉢多耶唵薩婆羯囉二合那尾數陀抳薩嚩二合訶增益娜莫三去曼多勃馱引喃上引一阿鉢囉二合底丁以反賀多捨引娑曩引南二唵引却却佉去引呬佉呬聲異反上同三吽引吽引入嚩二合攞入嚩二合攞四鉢囉二合入嚩二合攞鉢囉二合入嚩二合攞五底瑟奼二合底瑟吒二合六瑟置哩三合引七娑發吒半音呼娑發吒半音娑嚩二合訶息災尾沙野娑嚩鉻又云波底唵藥叉布彈那吽因陀羅弊薩嚩賀毘沙遮那娑嚩賀降伏陀彌使者誦此真言用此契。",
        "from": "T21n1249_001:0229c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手作拳直竪二食指。",
        "from": "T21n1249_001:0229c27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次開二大母指。",
        "from": "T21n1249_001:0229c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次右食指超左食指背上入掌中。",
        "from": "T21n1249_001:0229c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左食指入右掌中。",
        "from": "T21n1249_001:0229c29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次二大母指。",
        "from": "T21n1249_001:0229c29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左把右食指。",
        "from": "T21n1249_001:0230a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭出外。",
        "from": "T21n1249_001:0230a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八大天王。",
        "from": "T21n1249_001:0230a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩尼跋陀羅　布嚕娜跋陀羅　半只迦　娑多祁里　醯摩嚩多　毘灑迦　阿陀嚩迦　半灑攞毘沙門儀軌八家錄云。",
        "from": "T21n1249_001:0230a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘沙門天王儀軌一卷睿。",
        "from": "T21n1249_001:0230a06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此軌以智積院慈順僧正御本。",
        "from": "T21n1249_001:0230a07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當山愛染院大宣等挍合。",
        "from": "T21n1249_001:0230a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "予亦挍訂而壽梓。",
        "from": "T21n1249_001:0230a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一挍加筆了　　　　　　　　慈順",
        "from": "T21n1249_001:0230a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梨本挍合之了　　　　　　　龍肝",
        "from": "T21n1249_001:0230a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曜寺本再挍之了朱注是也　　　　　龍肝",
        "from": "T21n1249_001:0230a14_0",
        "context_with_next_sentence": true
    }
]