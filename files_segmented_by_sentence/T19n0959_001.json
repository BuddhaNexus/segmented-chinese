[
    {
        "source_sentence": "頂輪王大曼荼羅灌頂儀軌",
        "from": "T19n0959_001:0327c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舊名　阿迦　𮓌㗚婆　𮗿弘",
        "from": "T19n0959_001:0327c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "新名　薩縛　娑　底迦　吉祥",
        "from": "T19n0959_001:0327c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂輪王大曼荼羅灌頂儀并金剛界眾形色東都聖善寺沙門􈎭􄕍􄑀吉祥集內院三昧耶界道中台院中。",
        "from": "T19n0959_001:0327c27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫佛世尊坐師子座。",
        "from": "T19n0959_001:0327c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其座種種寶莊嚴。",
        "from": "T19n0959_001:0327c28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖者身坐白蓮華。",
        "from": "T19n0959_001:0327c28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身白金色。",
        "from": "T19n0959_001:0327c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入最勝三摩地王相。",
        "from": "T19n0959_001:0327c29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結跏趺坐。",
        "from": "T19n0959_001:0327c29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作說法相。",
        "from": "T19n0959_001:0328a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從一切身普遍出十二幅金輪熾盛光明。",
        "from": "T19n0959_001:0328a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如輪圍繞。",
        "from": "T19n0959_001:0328a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從頂流出種種光明。",
        "from": "T19n0959_001:0328a02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "青黃赤白。",
        "from": "T19n0959_001:0328a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具大仗夫相。",
        "from": "T19n0959_001:0328a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倚菩提樹。",
        "from": "T19n0959_001:0328a03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其樹有種種葉。",
        "from": "T19n0959_001:0328a03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如真多摩尼。",
        "from": "T19n0959_001:0328a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樹形於上繫種種繒綵。",
        "from": "T19n0959_001:0328a04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或繫吠瑠璃寶天菓妙衣。",
        "from": "T19n0959_001:0328a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或垂雲降雨。",
        "from": "T19n0959_001:0328a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有種種芽。",
        "from": "T19n0959_001:0328a05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉祥眾鳥並坐其上。",
        "from": "T19n0959_001:0328a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於枝間種種寶葉已成劫樹。",
        "from": "T19n0959_001:0328a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊兩肩後倚著其樹。",
        "from": "T19n0959_001:0328a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛右邊座下畫轉輪王形。",
        "from": "T19n0959_001:0328a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐白蓮華。",
        "from": "T19n0959_001:0328a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作觀佛勢。",
        "from": "T19n0959_001:0328a08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其身金色。",
        "from": "T19n0959_001:0328a08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周遍光明。",
        "from": "T19n0959_001:0328a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七寶成就。",
        "from": "T19n0959_001:0328a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟輪寶以光圍繞。",
        "from": "T19n0959_001:0328a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在蓮華上。",
        "from": "T19n0959_001:0328a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘六寶如次安之。",
        "from": "T19n0959_001:0328a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東南角摧碎佛頂　南勝佛頂　西南角無邊聲佛頂　西白傘蓋佛頂　西北角高廣佛頂　北光聚佛頂坐白蓮花上。",
        "from": "T19n0959_001:0328a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種光圍繞。",
        "from": "T19n0959_001:0328a13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在熾光明中坐。",
        "from": "T19n0959_001:0328a13_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身金色。",
        "from": "T19n0959_001:0328a13_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手持真多摩尼寶　東北角最勝佛頂　正東高佛頂如大王形。",
        "from": "T19n0959_001:0328a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐白蓮花上。",
        "from": "T19n0959_001:0328a14_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手持俱緣菓□睹輪王勢已上中院十佛頂名號。",
        "from": "T19n0959_001:0328a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次第二院獨古杵界道從東北角起首畫向南也。",
        "from": "T19n0959_001:0328a17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東南角法波羅蜜菩薩　次大勢至觀音菩薩　葉衣觀音菩薩　多羅觀音菩薩種種嚴具莊著輕□衣。",
        "from": "T19n0959_001:0328a19_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其身形不麁不細中庸形。",
        "from": "T19n0959_001:0328a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持青蓮花。",
        "from": "T19n0959_001:0328a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手施願。",
        "from": "T19n0959_001:0328a20_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐花台上。",
        "from": "T19n0959_001:0328a20_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作淺綠色也　觀世音菩薩　毘俱胝觀音菩薩身白色。",
        "from": "T19n0959_001:0328a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三目四臂。",
        "from": "T19n0959_001:0328a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右上手持仗。",
        "from": "T19n0959_001:0328a21_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左上手持瓶。",
        "from": "T19n0959_001:0328a21_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右下手持念珠。",
        "from": "T19n0959_001:0328a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左下手持蓮花。",
        "from": "T19n0959_001:0328a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身儀寂靜　白衣觀音菩薩以蓮花鬘莊嚴身。",
        "from": "T19n0959_001:0328a22_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用寶繒角絡被。",
        "from": "T19n0959_001:0328a22_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持真多摩尼寶。",
        "from": "T19n0959_001:0328a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手施願。",
        "from": "T19n0959_001:0328a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐蓮花上。",
        "from": "T19n0959_001:0328a23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是一切蓮花族母　觀自在王菩薩坐寶蓮花上。",
        "from": "T19n0959_001:0328a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌作禮勢　西南角羯磨波羅蜜菩薩　次禪波羅蜜菩薩結定即身□如金　忍波羅蜜菩薩　施波羅蜜菩薩　虛空明妃如天形。",
        "from": "T19n0959_001:0328a26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐蓮花上。",
        "from": "T19n0959_001:0328a26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌作禮勢　戒波羅蜜菩薩　精進波羅蜜菩薩　惠波羅蜜菩薩　西北角金剛波羅蜜菩薩　次降三世金剛菩薩身相如瑜伽　金剛將明妃　金剛將童子如名別無相　金剛手菩薩右手持白拂。",
        "from": "T19n0959_001:0328b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持金剛杵。",
        "from": "T19n0959_001:0328b02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐蓮花上　金剛母菩薩名摩莫枳。",
        "from": "T19n0959_001:0328b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身相如童女。",
        "from": "T19n0959_001:0328b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淡紫青色。",
        "from": "T19n0959_001:0328b03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種瓔珞莊嚴。",
        "from": "T19n0959_001:0328b03_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐蓮花上。",
        "from": "T19n0959_001:0328b03_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住般若自性。",
        "from": "T19n0959_001:0328b03_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持梵夾。",
        "from": "T19n0959_001:0328b03_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持真多摩尼。",
        "from": "T19n0959_001:0328b03_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施願勢。",
        "from": "T19n0959_001:0328b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得高顏□悅意。",
        "from": "T19n0959_001:0328b04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是金剛族母　蘇婆呼童子菩薩作童子形　頂行童子菩薩　同前准被髮相　東北角寶波羅蜜菩薩　次般若波羅蜜菩薩　慈氏菩薩手執白拂　佛慈菩薩　佛眼菩薩形如天女。",
        "from": "T19n0959_001:0328b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐寶蓮花。",
        "from": "T19n0959_001:0328b07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種莊嚴。",
        "from": "T19n0959_001:0328b07_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身如金色。",
        "from": "T19n0959_001:0328b07_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目觀眾會。",
        "from": "T19n0959_001:0328b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著輕穀衣角被。",
        "from": "T19n0959_001:0328b08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持寶。",
        "from": "T19n0959_001:0328b08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手施願。",
        "from": "T19n0959_001:0328b08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓光周遍。",
        "from": "T19n0959_001:0328b08_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身相寂　佛豪相菩薩形如天母。",
        "from": "T19n0959_001:0328b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色相同前。",
        "from": "T19n0959_001:0328b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯右手持蓮花。",
        "from": "T19n0959_001:0328b09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手施願。",
        "from": "T19n0959_001:0328b09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光明同前無異　文殊菩薩身相金色　如意寶菩薩已上第二院三十二聖者名。",
        "from": "T19n0959_001:0328b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次第三院同第二界道。",
        "from": "T19n0959_001:0328b12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東南角歌供養菩薩形服皆金色　次如意輪菩薩　纔發心轉法輪菩薩　八聖觀音菩薩　蓮華吉祥菩薩　蓮華孫陀利菩薩四臂。",
        "from": "T19n0959_001:0328b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右第一手持羂索。",
        "from": "T19n0959_001:0328b15_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持鉞斧。",
        "from": "T19n0959_001:0328b15_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右第二手施願。",
        "from": "T19n0959_001:0328b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左第二手持菓。",
        "from": "T19n0959_001:0328b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐於蓮花。",
        "from": "T19n0959_001:0328b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光明同前　青頸觀音菩薩　蓮華髻菩薩　蓮華藥叉菩薩　十一面觀音菩薩　西南角　舞供養菩薩形服皆金色　次嚩哩迦龍王　嚩哩迦七頭　阿難陀龍王此上二龍王。",
        "from": "T19n0959_001:0328b19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曾見佛有。",
        "from": "T19n0959_001:0328b19_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七頭合掌跪地即上嚩哩迦阿難陀也。",
        "from": "T19n0959_001:0328b20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯持花鬘有異也　無染河天者即尼蓮河神也。",
        "from": "T19n0959_001:0328b20_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並見佛來。",
        "from": "T19n0959_001:0328b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身相如目止隣龍王更無別狀　地天供養身白色。",
        "from": "T19n0959_001:0328b21_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手捧寶花籠子。",
        "from": "T19n0959_001:0328b21_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二膝跪地無能勝姤菩薩　無能勝妃　無熱惱龍王持蓮花鬘曲躬合掌　目真鱗龍王并前二龍王青色黑色。",
        "from": "T19n0959_001:0328b23_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如女形。",
        "from": "T19n0959_001:0328b23_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七頭合掌。",
        "from": "T19n0959_001:0328b23_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作禮勢。",
        "from": "T19n0959_001:0328b23_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後同　龍女相端嚴　西北角獻喜菩薩形服皆金色　次烏芻瑟摩金剛　金剛藏菩薩　甘露軍吒利菩薩　金剛孫陀利菩薩此是明妃。",
        "from": "T19n0959_001:0328b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形如天女。",
        "from": "T19n0959_001:0328b26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種瓔珞莊嚴其身青色。",
        "from": "T19n0959_001:0328b26_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手執蓮花。",
        "from": "T19n0959_001:0328c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐寶山上　金剛愛菩薩　金剛拳菩薩　施無畏菩薩　鉢拏攝末里菩薩　金剛針菩薩　東北角鬘供養菩薩形服皆金色　談無竭菩薩　陀羅尼自在菩薩　摩麼鷄菩薩坐蓮花臺。",
        "from": "T19n0959_001:0328c04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如女形。",
        "from": "T19n0959_001:0328c04_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "首戴寶冠。",
        "from": "T19n0959_001:0328c04_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手執五古杵。",
        "from": "T19n0959_001:0328c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手施願。",
        "from": "T19n0959_001:0328c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身相端嚴。",
        "from": "T19n0959_001:0328c05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓光熾盛也　大惠菩薩身如作禮勢。",
        "from": "T19n0959_001:0328c05_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌坐蓮臺。",
        "from": "T19n0959_001:0328c05_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金色具端嚴　寂靜菩薩同前大惠相　無垢惠菩薩同前寂靜相　虛空無垢菩薩同前無垢惠　海惠菩薩　善象猛菩薩已上第三院四十聖名號。",
        "from": "T19n0959_001:0328c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛界外第四院辦事天等次第　外供養從東南角燈供養菩薩二手捧燈臺。",
        "from": "T19n0959_001:0328c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身形衣服並赤　次火天姤　焰魔天王　當門　金剛鈴菩薩形服並綠色。",
        "from": "T19n0959_001:0328c12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "搖鈴居本門　次焰魔王姤　羅剎天主　西南角塗香菩薩衣服身形黃色　次羅剎主姤　毘沙門天王　當門金剛鉤菩薩形服青。",
        "from": "T19n0959_001:0328c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執鉤在本位　次毘沙門天姤　風天主　西北角　香供養菩薩黑色二手執香爐　風天姤　水天王　當門金剛索菩薩形服並黃。",
        "from": "T19n0959_001:0328c17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執索當門位水天姤　伊舍那天　東北角花供養菩薩坐白蓮花。",
        "from": "T19n0959_001:0328c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣服面色具白。",
        "from": "T19n0959_001:0328c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手持花。",
        "from": "T19n0959_001:0328c19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養勢也　伊舍那天姤　帝釋天王　當門金剛鎖菩薩形服並赤。",
        "from": "T19n0959_001:0328c20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持鎖本位中　帝釋姤　火天主已上第四院二十天名號。",
        "from": "T19n0959_001:0328c22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從此第五食道院四角畫五古杵契焰鬘威相成。",
        "from": "T19n0959_001:0328c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已上曼荼羅輪王大灌頂儀軌中上壇聖者。",
        "from": "T19n0959_001:0328c25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "都計一百二十二尊號。",
        "from": "T19n0959_001:0328c26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿闍梨與弟子灌頂。",
        "from": "T19n0959_001:0328c27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應於大曼荼羅側。",
        "from": "T19n0959_001:0328c27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對聖眾面前。",
        "from": "T19n0959_001:0329a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別塗作曼荼羅。",
        "from": "T19n0959_001:0329a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以白粉參肘量。",
        "from": "T19n0959_001:0329a01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普於中畫蓮花。",
        "from": "T19n0959_001:0329a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中台上畫師子座。",
        "from": "T19n0959_001:0329a02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令灌頂弟子。",
        "from": "T19n0959_001:0329a02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "󰷦花了者坐上。",
        "from": "T19n0959_001:0329a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令一解法。",
        "from": "T19n0959_001:0329a03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弟子持蓋及拂。",
        "from": "T19n0959_001:0329a03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦吉慶聲讚徐徐不絕。",
        "from": "T19n0959_001:0329a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取中瓶水。",
        "from": "T19n0959_001:0329a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持一百八遍。",
        "from": "T19n0959_001:0329a04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教弟子結佛頂印安於頂上餘如灌頂儀中廣說然須吹䗍擊鼓。",
        "from": "T19n0959_001:0329a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作諸音樂等。",
        "from": "T19n0959_001:0329a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今依經說　　輪王畫像法",
        "from": "T19n0959_001:0329a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "近人具眾流　　作法方可為",
        "from": "T19n0959_001:0329a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如經廣稱述　　白疊須頓方",
        "from": "T19n0959_001:0329a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五肘或三肘畫聖者像。",
        "from": "T19n0959_001:0329a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先從大海踊出須彌山王四寶所成先北金。",
        "from": "T19n0959_001:0329a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東白銀。",
        "from": "T19n0959_001:0329a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南吠瑠璃色青色。",
        "from": "T19n0959_001:0329a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西頗胝迦寶色紅色於山上坐白蓮華。",
        "from": "T19n0959_001:0329a12_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身白金色。",
        "from": "T19n0959_001:0329a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正受入最勝王三摩地。",
        "from": "T19n0959_001:0329a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結跏趺坐。",
        "from": "T19n0959_001:0329a13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從一切身遍滿流出十二幅金輪熾盛光明。",
        "from": "T19n0959_001:0329a14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於上應畫山峯。",
        "from": "T19n0959_001:0329a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其峯以種種寶成。",
        "from": "T19n0959_001:0329a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖者從頂流出種種光明。",
        "from": "T19n0959_001:0329a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "青黃赤白。",
        "from": "T19n0959_001:0329a16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旋轉自在。",
        "from": "T19n0959_001:0329a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖者右邊畫持誦人。",
        "from": "T19n0959_001:0329a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手執香爐。",
        "from": "T19n0959_001:0329a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀如來面。",
        "from": "T19n0959_001:0329a17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下應畫蓮花池餘如經說也畫二大明王。",
        "from": "T19n0959_001:0329a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相如後說。",
        "from": "T19n0959_001:0329a18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊儀賀野仡哩二合嚩大明王。",
        "from": "T19n0959_001:0329a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身如火色。",
        "from": "T19n0959_001:0329a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忿怒形。",
        "from": "T19n0959_001:0329a19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鼻如猿猴。",
        "from": "T19n0959_001:0329a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以蛇莊嚴瓔珞。",
        "from": "T19n0959_001:0329a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臂釧膊釧頭繫蓮華鬘。",
        "from": "T19n0959_001:0329a20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作瞻睹勢。",
        "from": "T19n0959_001:0329a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次無能勝忿怒王。",
        "from": "T19n0959_001:0329a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身白色。",
        "from": "T19n0959_001:0329a22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四臂頻眉面嗔。",
        "from": "T19n0959_001:0329a22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虎皮為裙。",
        "from": "T19n0959_001:0329a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蟒蛇為耳。",
        "from": "T19n0959_001:0329a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "璫得叉迦龍王以為腰。",
        "from": "T19n0959_001:0329a23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縚婆蘇枳龍王以為神。",
        "from": "T19n0959_001:0329a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "線角絡被耽肚。",
        "from": "T19n0959_001:0329a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身形稍短。",
        "from": "T19n0959_001:0329a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以毒蛇莊嚴髻冠。",
        "from": "T19n0959_001:0329a25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口咬下唇。",
        "from": "T19n0959_001:0329a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍身火焰熾盛圓光。",
        "from": "T19n0959_001:0329a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右上手持金剛杵。",
        "from": "T19n0959_001:0329a26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右下手作期尅勢。",
        "from": "T19n0959_001:0329a26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左上手持三古戟叉。",
        "from": "T19n0959_001:0329a27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左下手執鉞斧。",
        "from": "T19n0959_001:0329a27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正面作阿吒吒賀娑󰺗勢。",
        "from": "T19n0959_001:0329a28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從口火焰種種色相。",
        "from": "T19n0959_001:0329a28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右邊面作觀瞻勢。",
        "from": "T19n0959_001:0329a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左邊面下看也。",
        "from": "T19n0959_001:0329a29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭上面作觀大眾勢。",
        "from": "T19n0959_001:0329b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於寶蓮華立也了。",
        "from": "T19n0959_001:0329b01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已上二聖者於曼荼羅門外兩邊畫。",
        "from": "T19n0959_001:0329b02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次觀本尊五支也。",
        "from": "T19n0959_001:0329b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿尾囉吽欠亦云闕寶量不空毘盧居頂後。",
        "from": "T19n0959_001:0329b03_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘從額配之　阿字金剛寶地大悲地界所執持故　微字功德乳海所資𨳝故　囉字三昧惠炬所溫育故　吽字除障智風所開發故　欠字無邊性空不障礙故。",
        "from": "T19n0959_001:0329b05_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　五支先布已次說瑜伽相。",
        "from": "T19n0959_001:0329b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿字真金色　　普現金剛輪",
        "from": "T19n0959_001:0329b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持於下體　　說名瑜伽座",
        "from": "T19n0959_001:0329b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微字素月光　　匪於霧聚中",
        "from": "T19n0959_001:0329b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持於齊上　　是名大悲海",
        "from": "T19n0959_001:0329b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "囉字初日暉　　形赤在三角",
        "from": "T19n0959_001:0329b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持於心位　　稱為智火威",
        "from": "T19n0959_001:0329b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吽字劫火焰　　塗玄若風輪",
        "from": "T19n0959_001:0329b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持白毫際　　名為自在力",
        "from": "T19n0959_001:0329b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欠字由大空　　含容一切色",
        "from": "T19n0959_001:0329b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持於頂上　　故號無等人",
        "from": "T19n0959_001:0329b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次五支從地水火風空配成及壇。",
        "from": "T19n0959_001:0329b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿字想座下。",
        "from": "T19n0959_001:0329b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛座已成。",
        "from": "T19n0959_001:0329b18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色相如黃金。",
        "from": "T19n0959_001:0329b18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即戒義也。",
        "from": "T19n0959_001:0329b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其壇相方。",
        "from": "T19n0959_001:0329b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曼荼囉亦耳。",
        "from": "T19n0959_001:0329b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微字想於臍。",
        "from": "T19n0959_001:0329b19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色白慈悲水。",
        "from": "T19n0959_001:0329b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即定義也。",
        "from": "T19n0959_001:0329b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其壇相圓外壇如滿月。",
        "from": "T19n0959_001:0329b20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "囉字想於心。",
        "from": "T19n0959_001:0329b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相色赤紅。",
        "from": "T19n0959_001:0329b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為惠義也。",
        "from": "T19n0959_001:0329b21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其壇三角形。",
        "from": "T19n0959_001:0329b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外壇亦三角。",
        "from": "T19n0959_001:0329b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吽字想眉間。",
        "from": "T19n0959_001:0329b22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色黑為無礙風。",
        "from": "T19n0959_001:0329b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即解脫義也。",
        "from": "T19n0959_001:0329b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其壇想青形如半月。",
        "from": "T19n0959_001:0329b23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外壇相亦同。",
        "from": "T19n0959_001:0329b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欠字安頂上。",
        "from": "T19n0959_001:0329b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以成大空點。",
        "from": "T19n0959_001:0329b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即解脫知見義也。",
        "from": "T19n0959_001:0329b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其壇如字義。",
        "from": "T19n0959_001:0329b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空點無色相。",
        "from": "T19n0959_001:0329b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能成諸事業。",
        "from": "T19n0959_001:0329b26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是陀羅尼義也。",
        "from": "T19n0959_001:0329b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引速乞灑二合摩嚩日囉二合此云細細如金剛。",
        "from": "T19n0959_001:0329b28_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此陀羅尼能令所觀成就唵引底丁以反瑟姹二合嚩日囉二合此云不動如金剛。",
        "from": "T19n0959_001:0329c02_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此陀羅尼能令所觀無失唵引薩頗二合囉拏嚩日囉二合此云周遍如金剛。",
        "from": "T19n0959_001:0329c04_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此陀羅尼能令所觀漸廣量周法界唵引僧賀囉二合嚩日囉二合此云了達如金剛。",
        "from": "T19n0959_001:0329c06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此陀羅尼能令所觀漸略。",
        "from": "T19n0959_001:0329c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如毫端。",
        "from": "T19n0959_001:0329c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住白毫際。",
        "from": "T19n0959_001:0329c07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或住鼻端。",
        "from": "T19n0959_001:0329c07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨心所樂次說畫。",
        "from": "T19n0959_001:0329c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘇婆呼童子。",
        "from": "T19n0959_001:0329c08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身形眷屬尊身如童子形。",
        "from": "T19n0959_001:0329c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金色如名號。",
        "from": "T19n0959_001:0329c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悚慄畫尊儀。",
        "from": "T19n0959_001:0329c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七曜圍繞。",
        "from": "T19n0959_001:0329c09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密莫雲漠。",
        "from": "T19n0959_001:0329c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "𠼬嗢沒斯。",
        "from": "T19n0959_001:0329c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那纈鷄󰺎。",
        "from": "T19n0959_001:0329c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各如本形畫。",
        "from": "T19n0959_001:0329c10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住賀野仡哩嚩明側立花上。",
        "from": "T19n0959_001:0329c11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次畫頂行童子。",
        "from": "T19n0959_001:0329c11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身形眷屬。",
        "from": "T19n0959_001:0329c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊身金色。",
        "from": "T19n0959_001:0329c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髻髮下垂。",
        "from": "T19n0959_001:0329c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持金剛杵。",
        "from": "T19n0959_001:0329c12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手叉腰。",
        "from": "T19n0959_001:0329c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八頻那夜迦圍繞所為。",
        "from": "T19n0959_001:0329c13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛莊嚴。",
        "from": "T19n0959_001:0329c13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛塵。",
        "from": "T19n0959_001:0329c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛索。",
        "from": "T19n0959_001:0329c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛鉞斧。",
        "from": "T19n0959_001:0329c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛極笑。",
        "from": "T19n0959_001:0329c14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛成莊嚴。",
        "from": "T19n0959_001:0329c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛頂。",
        "from": "T19n0959_001:0329c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛頻那夜釰。",
        "from": "T19n0959_001:0329c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "器仗如文配。",
        "from": "T19n0959_001:0329c15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已上並是頂輪壇法具足寫了。",
        "from": "T19n0959_001:0329c16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　　　　　　　一交了　僧賴昭",
        "from": "T19n0959_001:0329c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　　　　　　　　東寺沙門道快",
        "from": "T19n0959_001:0329c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朱筆此軌者𮗿弘和尚集歟。",
        "from": "T19n0959_001:0329c22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辨𮗿兩字通用之間。",
        "from": "T19n0959_001:0329c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "青龍和尚門人制作。",
        "from": "T19n0959_001:0329c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尤以可為規模者也。",
        "from": "T19n0959_001:0329c24_2",
        "context_with_next_sentence": true
    }
]