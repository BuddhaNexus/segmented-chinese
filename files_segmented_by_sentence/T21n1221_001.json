[
    {
        "source_sentence": "青色大金剛藥叉辟鬼魔法亦名辟鬼殊法",
        "from": "T21n1221_001:0099c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鮫龍洞阿闍梨位空蜝述",
        "from": "T21n1221_001:0099c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□根本印。二手合掌。以二受指屈入掌中。二",
        "from": "T21n1221_001:0099c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "節相背。",
        "from": "T21n1221_001:0099c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右想指柱左行端。",
        "from": "T21n1221_001:0099c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左想指柱右行端。",
        "from": "T21n1221_001:0099c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少如輪形。",
        "from": "T21n1221_001:0099c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨其印處摧滅一切魔鬼□害真言。",
        "from": "T21n1221_001:0099c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□□□□發吒吒二曩伽毘□□□□□□□□□□□□那耶發吒□□□婆薩羅那發吒五胡𡃖摩訶尼那耶發吒六俱槃荼□瑳陀那耶發吒七筏利多二合毘舍遮那耶發吒八藥乞叉毘舍遮那耶發吒九弩史耶摩薩頗馱囉□□□□□□□□□□□□吒二合也吽十二吽發吒吒吒十三阿鉢□底訶多婆囉十四帝哩二合盧迦十五婆蠅迦□十六□□二合荼□□□□□□曳十七羅質攘波耶底十八吽發吒吒吒十九娑嚩二合賀亦名身呪。",
        "from": "T21n1221_001:0099c22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人欲辟除傳屍病鬼難者。",
        "from": "T21n1221_001:0099c23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應知其病相。",
        "from": "T21n1221_001:0099c23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "病相者。",
        "from": "T21n1221_001:0099c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人身心熱惱。",
        "from": "T21n1221_001:0099c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漸漸□瘦。",
        "from": "T21n1221_001:0099c24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或□□飲食。",
        "from": "T21n1221_001:0100a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或時睡眠多。",
        "from": "T21n1221_001:0100a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□□□□或時□□□□□□□□□□□□□苦有□□□不能具述。",
        "from": "T21n1221_001:0100a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復此鬼病漸漸展轉處處流行。",
        "from": "T21n1221_001:0100a03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂傳夫妻子孫及兄弟姊妹等。",
        "from": "T21n1221_001:0100a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故時人號曰傳屍鬼□。",
        "from": "T21n1221_001:0100a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天下名醫不能療治。",
        "from": "T21n1221_001:0100a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛法澆薄時。",
        "from": "T21n1221_001:0100a05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國王大臣后妃婇女國中僧尼。",
        "from": "T21n1221_001:0100a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為此□所侵害。",
        "from": "T21n1221_001:0100a06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上品為癩兒。",
        "from": "T21n1221_001:0100a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中品為傳死。",
        "from": "T21n1221_001:0100a07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下品為狂亂。",
        "from": "T21n1221_001:0100a07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故父母忌親□妻子等奇恩義。",
        "from": "T21n1221_001:0100a08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此傳屍病鬼亦名天魔羅難室陀鬼。",
        "from": "T21n1221_001:0100a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總持集經言。",
        "from": "T21n1221_001:0100a09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其鬼病狀相似風𮨰。",
        "from": "T21n1221_001:0100a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦如狂人□□□□□□病□狀一一如此。",
        "from": "T21n1221_001:0100a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若四十九日內不療治□□之必死。",
        "from": "T21n1221_001:0100a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "治此病時。",
        "from": "T21n1221_001:0100a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦前大身呪三七□□以右手把白芥子。",
        "from": "T21n1221_001:0100a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦前身呪三七反。",
        "from": "T21n1221_001:0100a13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散□其病人頭面。",
        "from": "T21n1221_001:0100a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其鬼身碎裂。",
        "from": "T21n1221_001:0100a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如火所□告或把□□人。",
        "from": "T21n1221_001:0100a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦前身呪三七反。",
        "from": "T21n1221_001:0100a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "打□□□□□□□枝誦前身呪三七□□□□□□□□□□□□□其□即愈□□□若應炙其病人。",
        "from": "T21n1221_001:0100a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "炙有九處。",
        "from": "T21n1221_001:0100a17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者頂十字中。",
        "from": "T21n1221_001:0100a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者風門穴中。",
        "from": "T21n1221_001:0100a18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者𮧊鏡。",
        "from": "T21n1221_001:0100a18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者心臟。",
        "from": "T21n1221_001:0100a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者丹田。",
        "from": "T21n1221_001:0100a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六者左風市。",
        "from": "T21n1221_001:0100a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七者右風市。",
        "from": "T21n1221_001:0100a19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八者左彭矯穴。",
        "from": "T21n1221_001:0100a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九者右彭矯穴。",
        "from": "T21n1221_001:0100a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不炙矯穴其人趺壞生穴□□加𮧊背□□□□□大頂十字時。",
        "from": "T21n1221_001:0100a21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦􆿓􄑖吽發吒五七反。",
        "from": "T21n1221_001:0100a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具百光遍照王功德蘊相。",
        "from": "T21n1221_001:0100a22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次炙風門。",
        "from": "T21n1221_001:0100a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持誦􄚨􄚩吽發吒五七反。",
        "from": "T21n1221_001:0100a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次□炙𮧊鏡時。",
        "from": "T21n1221_001:0100a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦􆿅􆿆􆿓􆿔吽發吒五七反。",
        "from": "T21n1221_001:0100a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次炙心臟時。",
        "from": "T21n1221_001:0100a24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦􄗽􄗾􄙈􄙉吽發吒五七反。",
        "from": "T21n1221_001:0100a25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次炙丹田時。",
        "from": "T21n1221_001:0100a25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦􈅼􆿔􇩵吽發吒五七反。",
        "from": "T21n1221_001:0100a26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次炙左右風市時。",
        "from": "T21n1221_001:0100a26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦􄚨􄚩􄚲􄚳吽發吒。",
        "from": "T21n1221_001:0100a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次炙左右彭矯穴時。",
        "from": "T21n1221_001:0100a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦􄙖􄙗􄙠􄙡吽發吒五七反。",
        "from": "T21n1221_001:0100a28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已上以前根本印加持其炙處。",
        "from": "T21n1221_001:0100a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是炙□鬼兜□□鬼障難之處。",
        "from": "T21n1221_001:0100a29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若炙𮧊□□□□□□身呪各□反。",
        "from": "T21n1221_001:0100b01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以□申□□□□□□□□□□□□其驗□加預皆□□沈香白檀香等入銅器中。",
        "from": "T21n1221_001:0100b03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦前身呪三七反。",
        "from": "T21n1221_001:0100b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持其香□入芮草楊柳柘榴湯中。",
        "from": "T21n1221_001:0100b04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應浴洗其病人炙處。",
        "from": "T21n1221_001:0100b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳屍病止息。",
        "from": "T21n1221_001:0100b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持□印法。",
        "from": "T21n1221_001:0100b05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "者著新淨衣。",
        "from": "T21n1221_001:0100b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住靜室□□戒情□□上求菩提下化眾生。",
        "from": "T21n1221_001:0100b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願每日三時。",
        "from": "T21n1221_001:0100b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各誦前身呪。",
        "from": "T21n1221_001:0100b24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三七日已。",
        "from": "T21n1221_001:0100b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繞檀行道。",
        "from": "T21n1221_001:0100b25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦呪加持炙湯其病人。",
        "from": "T21n1221_001:0100b25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者常以孔雀明王根本印真言護持□□□除一切鬼神障難消滅惡夢雜夢□等。",
        "from": "T21n1221_001:0100b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "召請青色大金剛藥叉印。",
        "from": "T21n1221_001:0100b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手合掌。",
        "from": "T21n1221_001:0100b28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二色指入掌中背相似二識□□□甲上。",
        "from": "T21n1221_001:0100b29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦以二行指著二相背上開□右行招本尊五度亦成五藥叉請印。",
        "from": "T21n1221_001:0100c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦前身呪五反必起請來集真言。",
        "from": "T21n1221_001:0100c02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□□□□訶噌吽三烏呼嗚呼四□□□□□□□□□□□□□□□□□□□□□□□□□□□自□內造□。",
        "from": "T21n1221_001:0100c05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□中安置二尊。",
        "from": "T21n1221_001:0100c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二尊者。",
        "from": "T21n1221_001:0100c24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者六目二輪聖無動明王。",
        "from": "T21n1221_001:0100c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者青色大金剛藥叉。",
        "from": "T21n1221_001:0100c25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此二尊前安熾盛□日輪。",
        "from": "T21n1221_001:0100c26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日輪之中畫三尊像。",
        "from": "T21n1221_001:0100c26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三尊者。",
        "from": "T21n1221_001:0100c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者□光遍照王釋迦金輪佛。",
        "from": "T21n1221_001:0100c27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者藥□□光佛。",
        "from": "T21n1221_001:0100c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者聖如意輪王菩薩。",
        "from": "T21n1221_001:0100c28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此□□□佛部金剛部蓮花部□位如下圖。",
        "from": "T21n1221_001:0100c29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次造三鬼像入銅鐃器油中。",
        "from": "T21n1221_001:0101a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦聖無動□界真言一百八反。",
        "from": "T21n1221_001:0101a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煮三鬼像。",
        "from": "T21n1221_001:0101a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然□□□爐中護摩二十□夜若猶□驗者三七日修之云云此時三鬼□□其形其灰中有􆿅字。",
        "from": "T21n1221_001:0101a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "􆿅字變□□□寂靜聖無動尊。",
        "from": "T21n1221_001:0101a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名三鬼還住□覺無動真如大界如大日金輪法若有諸王勤□此法□□無病長壽國土安穩豐□□□□□□□法者其高驗無量□□□□□□□□□□□□□□道病□此□□明王誦一千八百反□呪即時。",
        "from": "T21n1221_001:0101a09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現其色身加持行者。",
        "from": "T21n1221_001:0101a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲□除鬼魅病難者。",
        "from": "T21n1221_001:0101a10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向此青色金剛□七百反。",
        "from": "T21n1221_001:0101a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦為其人現鬼魅身加護□者。",
        "from": "T21n1221_001:0101a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲除滅時氣天行病者□□枝打其病人即得除滅。",
        "from": "T21n1221_001:0101a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略說小□善願具如總持集經所說□□此辟鬼殊法祕中極祕密法。",
        "from": "T21n1221_001:0101a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常似火□界十五印加持護念有大日金輪灌頂法中故不戴之□金剛□□□青色具足三目四臂。",
        "from": "T21n1221_001:0101a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面貌大忿怒□□張狗牙上出三目□忿大恐怖相。",
        "from": "T21n1221_001:0101a17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂戴髑髏首髮逆生□□色頂纏大蛇。",
        "from": "T21n1221_001:0101a18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四臂相者。",
        "from": "T21n1221_001:0101a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左第一手□如意寶捧。",
        "from": "T21n1221_001:0101a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右第一手執羂索□□□手執大刀。",
        "from": "T21n1221_001:0101a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右第二手把金輪二膊□□□□以髑髏為頸瓔珞。",
        "from": "T21n1221_001:0101a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以虎皮為裙。",
        "from": "T21n1221_001:0101a21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又□左右足各踏一鬼。",
        "from": "T21n1221_001:0101a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左右下足鬼形如屍骸。",
        "from": "T21n1221_001:0101a22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大金剛藥叉以極大忿怒。",
        "from": "T21n1221_001:0101a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四大藥叉又□□眷屬黃色赤色藥叉俱持大刀□色黑色藥叉執三股刃。",
        "from": "T21n1221_001:0101a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此四大藥叉□足爪皆長利如釰。",
        "from": "T21n1221_001:0101a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "首各將八萬□□□叉眷屬護持南閻浮提一切眾生。",
        "from": "T21n1221_001:0101a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□此曼荼羅已。",
        "from": "T21n1221_001:0101a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日月蝕時。",
        "from": "T21n1221_001:0101a27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俄作一小壇莊嚴此曼荼羅。",
        "from": "T21n1221_001:0101a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨力所堪辦備飲食□□□□□□□待日月復滿之時□□□□□□□食之者□□□□□若有行□□□□□得六□□□法更無生□明其壇□者十三□□□一飲食四淨鏡四大刀□□四箭五色線。",
        "from": "T21n1221_001:0101b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是王宮供具。",
        "from": "T21n1221_001:0101b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次護□□供具准普通供儀。",
        "from": "T21n1221_001:0101b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□息災敬愛官位福祿降伏等一切□□一一成就速疾證得故。",
        "from": "T21n1221_001:0101b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "末世愚夫□□歸此法。",
        "from": "T21n1221_001:0101b07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翻經三藏深祕不傳大日金□灌頂以後傳受此文讚歎四句曰。",
        "from": "T21n1221_001:0101b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□訶嚩日囉二合藥乞叉二合羅闍曩謨□都帝一阿鉢囉二合底訶多野曩謨□□薩嚩摩羅婆野尾目乞□□□率都帝二嚩日囉二合入嚩攞布日羅唅曩謨率都帝四低頭禮青色大金剛藥叉明王□□祕密讚妄莫傳授。",
        "from": "T21n1221_001:0101b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□大金剛□□□別時授之。",
        "from": "T21n1221_001:0101b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以一切□□□□藏有諸財寶。",
        "from": "T21n1221_001:0101b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名悉地□□□□□□□□□□鬼魔□□□□□日為瀉□□□□□示印相如前輪印。",
        "from": "T21n1221_001:0101b18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但開□識指□□□□□□祕密中蜜。",
        "from": "T21n1221_001:0101b19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更於二人不□傳授。",
        "from": "T21n1221_001:0101b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□語言三鬼形者。",
        "from": "T21n1221_001:0101b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者人鬼。",
        "from": "T21n1221_001:0101b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如死屍設□□形。",
        "from": "T21n1221_001:0101b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者天鬼。",
        "from": "T21n1221_001:0101b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作鵄鳥形。",
        "from": "T21n1221_001:0101b22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是言天狐□□□三者地鬼。",
        "from": "T21n1221_001:0101b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作地狐形。",
        "from": "T21n1221_001:0101b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名癲癎鬼。",
        "from": "T21n1221_001:0101b23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□中住大勇猛心。",
        "from": "T21n1221_001:0101b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦不動火界呪一百□□□煮三鬼像則入不動尊火生三摩地常住此□其意見儀軌文。",
        "from": "T21n1221_001:0101b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□觀觀若鬼魔二病此須深觀行力□□□呪力乃得差耳。",
        "from": "T21n1221_001:0101b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若業病者當內□□□力外須懺悔乃可得差。",
        "from": "T21n1221_001:0101b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾治不□□□善得其意不可操刀把刃而自□□□□申求長生經云。",
        "from": "T21n1221_001:0101c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鼓涺在眼之暗□□□口臭齒落鼓質在腹中伐人□□□□藏少氣□忌人好惡敢食眾生鼓□□在足□□□閞挌擾五精踊動不□□□□□□□□神則害□□□□□□長□□□□□□庚□□□一申天帝記人罪□過絕人生籍欲令速死魄入三泉□□時。",
        "from": "T21n1221_001:0101c07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是曰鬼為人禍害心痛痓怦□□鬼病惱傳子孫及兄弟姊妹等□□□□為傳屍𭼩蝶復連骨□□□□病亦有一族內死盡莫究其由近□□漸多重者不逾數月而死。",
        "from": "T21n1221_001:0101c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以人□□極畏之父子絕親夫妻奇義□□□業病真如本經文。",
        "from": "T21n1221_001:0101c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□□□尸者困也可習之。",
        "from": "T21n1221_001:0101c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正觀觀亦由行者於坐禪中邪念利魔現種種衣服飲食七珍雜物。",
        "from": "T21n1221_001:0101c29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即領受觀□入心成病。",
        "from": "T21n1221_001:0101c29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此病難治文。",
        "from": "T21n1221_001:0102a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "承□□□□月二十六日。",
        "from": "T21n1221_001:0102a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以□□房御本書寫□□□□□慶範記之。",
        "from": "T21n1221_001:0102a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "承安二年正月十八日。",
        "from": "T21n1221_001:0102a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉隨乘乘房阿闍梨御房奉傳受了。",
        "from": "T21n1221_001:0102a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慶範記之。",
        "from": "T21n1221_001:0102a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建永二年正月十一日。",
        "from": "T21n1221_001:0102a06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以月輪房御本。",
        "from": "T21n1221_001:0102a06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　　　　　　　　　三井公雲□",
        "from": "T21n1221_001:0102a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "承久二年七月二十七日。",
        "from": "T21n1221_001:0102a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法蓮御房本書寫了。",
        "from": "T21n1221_001:0102a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此軌三井流請來祕本也。",
        "from": "T21n1221_001:0102a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此奧後人物云加者也。",
        "from": "T21n1221_001:0102a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　　　　　　　　　　　相覺記",
        "from": "T21n1221_001:0102a12_0",
        "context_with_next_sentence": true
    }
]