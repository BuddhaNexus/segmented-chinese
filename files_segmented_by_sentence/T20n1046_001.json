[
    {
        "source_sentence": "六字大陀羅尼呪經",
        "from": "T20n1046_001:0043c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "失譯人名今附梁錄",
        "from": "T20n1046_001:0043c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。一時婆伽婆。住王舍城耆闍崛山",
        "from": "T20n1046_001:0043c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中。",
        "from": "T20n1046_001:0043c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大比丘眾五百人俱。",
        "from": "T20n1046_001:0043c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時長老阿難。",
        "from": "T20n1046_001:0043c09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為旃陀梨女呪術所收爾時長老阿難。",
        "from": "T20n1046_001:0043c10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊。",
        "from": "T20n1046_001:0043c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今強為他收去。",
        "from": "T20n1046_001:0043c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婆伽婆我今強為他收去修伽陀。",
        "from": "T20n1046_001:0043c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時婆伽婆告長老阿難言。",
        "from": "T20n1046_001:0043c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝來阿難汝莫驚怖。",
        "from": "T20n1046_001:0043c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難汝當受持六字大陀羅尼呪。",
        "from": "T20n1046_001:0043c14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為令四眾利益安隱安樂吉祥行故。",
        "from": "T20n1046_001:0043c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而說呪曰。",
        "from": "T20n1046_001:0043c15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斯須何反地除隷反梯吐稽反曇徒紺反安於軻反茶徒嫁反隸般茶徒嫁反隸葛羅馳除寄反稽由隷　薩帝　婆帝　耶𮚒婆帝　底闍婆帝　頻頭摩帝阿難是呪。",
        "from": "T20n1046_001:0043c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令宿食不消尋得消化。",
        "from": "T20n1046_001:0043c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除吐下等病。",
        "from": "T20n1046_001:0043c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除風病。",
        "from": "T20n1046_001:0043c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "熱病冷病雜病。",
        "from": "T20n1046_001:0043c21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能滅一切諸邪呪術。",
        "from": "T20n1046_001:0043c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能滅起屍。",
        "from": "T20n1046_001:0043c22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能滅一切形像厭蠱。",
        "from": "T20n1046_001:0043c22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難若有人知此神呪姓名者。",
        "from": "T20n1046_001:0043c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼則不怖畏王難。",
        "from": "T20n1046_001:0043c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不怖畏怨敵難不怖畏賊難。",
        "from": "T20n1046_001:0043c24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不怖畏火難不怖畏水難。",
        "from": "T20n1046_001:0043c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於城邑聚落及在曠野悉無所畏。",
        "from": "T20n1046_001:0043c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不為他人伺求其過無過可說。",
        "from": "T20n1046_001:0043c26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若食毒藥毒不能害轉為利益。",
        "from": "T20n1046_001:0043c27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難此六字大陀羅尼呪乃是七三藐三佛陀所說。",
        "from": "T20n1046_001:0043c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦是梵王娑婆主說。",
        "from": "T20n1046_001:0043c29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋提桓因四大天王所說。",
        "from": "T20n1046_001:0044a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦皆隨喜破諸呪術消伏起屍。",
        "from": "T20n1046_001:0044a01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切形像厭蠱皆悉破壞斷滅。",
        "from": "T20n1046_001:0044a02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長老阿難聞佛所說歡喜奉行。",
        "from": "T20n1046_001:0044a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六字大陀羅尼呪經",
        "from": "T20n1046_001:0044a04_100",
        "context_with_next_sentence": false
    }
]