[
    {
        "source_sentence": "佛說華積陀羅尼神呪經",
        "from": "T21n1356_001:0874c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吳月支國居士支謙譯",
        "from": "T21n1356_001:0874c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。一時佛在阿耨達池龍王宮中。與",
        "from": "T21n1356_001:0874c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大比丘五百人俱。",
        "from": "T21n1356_001:0874c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及菩薩摩訶薩具足一千。",
        "from": "T21n1356_001:0874c11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉得一切陀羅尼。",
        "from": "T21n1356_001:0874c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘一生在住十住地。",
        "from": "T21n1356_001:0874c12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當作佛。",
        "from": "T21n1356_001:0874c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如王太子。",
        "from": "T21n1356_001:0874c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱於十方皆成佛道。",
        "from": "T21n1356_001:0874c13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具大莊嚴不疑如來一切功德。",
        "from": "T21n1356_001:0874c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天龍八部一時俱會。",
        "from": "T21n1356_001:0874c14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時師子威菩薩。",
        "from": "T21n1356_001:0874c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從座而起右膝著地。",
        "from": "T21n1356_001:0874c15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌向佛而白佛言。",
        "from": "T21n1356_001:0874c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊若善男子善女人。",
        "from": "T21n1356_001:0874c16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於如來所而修供養功德多不。",
        "from": "T21n1356_001:0874c17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告師子威勿作是語。",
        "from": "T21n1356_001:0874c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人。",
        "from": "T21n1356_001:0874c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於如來所而修供養功德多不。",
        "from": "T21n1356_001:0874c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故。",
        "from": "T21n1356_001:0874c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來無量戒定慧解脫解脫知見之所成就。",
        "from": "T21n1356_001:0874c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人。",
        "from": "T21n1356_001:0874c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於如來所若存若亡而修供養。",
        "from": "T21n1356_001:0874c21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師子威當知是人終必於三乘中。",
        "from": "T21n1356_001:0874c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨其因緣而得解脫。",
        "from": "T21n1356_001:0874c22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次師子威。",
        "from": "T21n1356_001:0874c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人見佛世尊。",
        "from": "T21n1356_001:0874c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心生歡喜尊重讚歎。",
        "from": "T21n1356_001:0874c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以諸供養施於如來。",
        "from": "T21n1356_001:0874c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復有人於如來滅後。",
        "from": "T21n1356_001:0874c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見舍利心生歡喜。",
        "from": "T21n1356_001:0874c25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是二功德等無有異。",
        "from": "T21n1356_001:0874c25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次師子威。",
        "from": "T21n1356_001:0874c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若在家菩薩以諸珍寶積如須彌。",
        "from": "T21n1356_001:0874c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養聲聞及辟支佛。",
        "from": "T21n1356_001:0874c27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不如出家菩薩發菩提心。",
        "from": "T21n1356_001:0874c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以一金錢供養如來。",
        "from": "T21n1356_001:0874c28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次師子威若復有人。",
        "from": "T21n1356_001:0874c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於千萬歲親近世尊備修供養。",
        "from": "T21n1356_001:0874c29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人如來滅後發菩提心。",
        "from": "T21n1356_001:0875a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以一香華入僧伽藍。",
        "from": "T21n1356_001:0875a01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉足下足作如是言。",
        "from": "T21n1356_001:0875a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無世尊。",
        "from": "T21n1356_001:0875a02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此深解殷重信心。",
        "from": "T21n1356_001:0875a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養如來舍利寶塔。",
        "from": "T21n1356_001:0875a03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不以求報不以疑惑。",
        "from": "T21n1356_001:0875a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未得解脫中間。",
        "from": "T21n1356_001:0875a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若千萬億劫不墮惡道。",
        "from": "T21n1356_001:0875a04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師子威諦聽。",
        "from": "T21n1356_001:0875a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有陀羅尼名曰華積。",
        "from": "T21n1356_001:0875a05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今當說。",
        "from": "T21n1356_001:0875a05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸天人多所饒益。",
        "from": "T21n1356_001:0875a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子。",
        "from": "T21n1356_001:0875a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能於華積陀羅尼呪。",
        "from": "T21n1356_001:0875a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持讀誦親近依行。",
        "from": "T21n1356_001:0875a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功德勝彼。",
        "from": "T21n1356_001:0875a07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人世世得一聞持。",
        "from": "T21n1356_001:0875a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不墮諸惡險棘道中離諸艱難。",
        "from": "T21n1356_001:0875a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常見妙寶常見諸佛諸根常具。",
        "from": "T21n1356_001:0875a09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不生下賤卑隷人家。",
        "from": "T21n1356_001:0875a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常得不離菩薩弘心。",
        "from": "T21n1356_001:0875a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常得種種無量慧辯。",
        "from": "T21n1356_001:0875a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為無量十方諸佛如來之所知見。",
        "from": "T21n1356_001:0875a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃說呪曰。",
        "from": "T21n1356_001:0875a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但施峙耶反他一杜羅禰二拖羅禰三拖羅尼四拖羅尼五侔尼六波羅婆負荷反娑禰七悉諦八栴禘九那侔紙十嫟訶引囇狸迤反十一魯伽婆底十二佛陀婆底十三底囇狸迤反十四柯羅知胝迤反十五羅竪波伽知胝迤反十六知胝迤反杜婆底十七毘捨羅佛地十八達摩婆徙十九惡叉耶葛俾二十葛波毘伽知胝迤二十一阿媚多葛波休多大多禰二十二闍波底二十三哆嫟微耶反沙摩憙吳音反知胝迤反二十四蜀伽羅婆底二十五陟沙拏佛朕持引反地耶佛地瑣訶引二十六師子威當知。",
        "from": "T21n1356_001:0875a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子能於此華積神呪。",
        "from": "T21n1356_001:0875a23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若讀若誦。",
        "from": "T21n1356_001:0875a23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人當於三月四月九月。",
        "from": "T21n1356_001:0875a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從八日至十五日。",
        "from": "T21n1356_001:0875a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心憶念如來相好。",
        "from": "T21n1356_001:0875a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜中三誦華積神呪。",
        "from": "T21n1356_001:0875a25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日中亦三。",
        "from": "T21n1356_001:0875a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至月圓時。",
        "from": "T21n1356_001:0875a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當以香華燈燭於形像前而修供養。",
        "from": "T21n1356_001:0875a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并誦華積陀羅尼呪。",
        "from": "T21n1356_001:0875a27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人是夜夢見如來。",
        "from": "T21n1356_001:0875a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相好具足坐蓮華座為眾說法。",
        "from": "T21n1356_001:0875a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦得華積陀羅尼呪。",
        "from": "T21n1356_001:0875a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於現世中常得強記。",
        "from": "T21n1356_001:0875a29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智慧深信。",
        "from": "T21n1356_001:0875a29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從今身乃至涅槃。",
        "from": "T21n1356_001:0875b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常一聞持一切聞見。",
        "from": "T21n1356_001:0875b01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切經教一切學解。",
        "from": "T21n1356_001:0875b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切技藝通達無礙。",
        "from": "T21n1356_001:0875b02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切三昧深得清淨。",
        "from": "T21n1356_001:0875b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解四聖諦無上法輪。",
        "from": "T21n1356_001:0875b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是經已。",
        "from": "T21n1356_001:0875b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師子威等天龍八部。",
        "from": "T21n1356_001:0875b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切大眾歡喜奉行。",
        "from": "T21n1356_001:0875b04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華積陀羅尼神呪經",
        "from": "T21n1356_001:0875b05_100",
        "context_with_next_sentence": false
    }
]