{
    "X65n1277_005:0019a01_0": "高峯龍泉院因師集賢語錄卷之五",
    "X65n1277_005:0019a03_0": "陳意伏願門",
    "X65n1277_005:0019a04_0": "生日",
    "X65n1277_005:0019a05_0": "生居聖世。",
    "X65n1277_005:0019a05_1": "忝處人倫。",
    "X65n1277_005:0019a05_2": "荷星辰臨照之恩。",
    "X65n1277_005:0019a05_3": "謝父母生成之德。",
    "X65n1277_005:0019a06_4": "幸逢是旦。",
    "X65n1277_005:0019a06_5": "適際始生。",
    "X65n1277_005:0019a06_6": "難酬仰答之私。",
    "X65n1277_005:0019a06_7": "用假投誠之志。",
    "X65n1277_005:0019a07_8": "伏願皇穹與善。",
    "X65n1277_005:0019a07_9": "上帝益謙。",
    "X65n1277_005:0019a07_10": "壽山疊疊以彌高。",
    "X65n1277_005:0019a07_11": "福海源源而益遠。",
    "X65n1277_005:0019a08_12": "丹臺列聖望書有永之年。",
    "X65n1277_005:0019a08_13": "紫府真仙永注無疆之福。",
    "X65n1277_005:0019a10_0": "生七",
    "X65n1277_005:0019a11_0": "言念知身是幻。",
    "X65n1277_005:0019a11_1": "覺世匪堅。",
    "X65n1277_005:0019a11_2": "倘為身後以修因。",
    "X65n1277_005:0019a11_3": "何不生前而努力。",
    "X65n1277_005:0019a12_4": "式崇佛果。",
    "X65n1277_005:0019a12_5": "預布津梁。",
    "X65n1277_005:0019a12_6": "伏願炳若功勳。",
    "X65n1277_005:0019a12_7": "且作現前延壽籙。",
    "X65n1277_005:0019a13_8": "大哉輪誥。",
    "X65n1277_005:0019a13_9": "永為後世護身符。",
    "X65n1277_005:0019a13_10": "觀音菩薩為作證明。",
    "X65n1277_005:0019a14_11": "地藏能仁不妨印可。",
    "X65n1277_005:0019a15_0": "受生",
    "X65n1277_005:0019a16_0": "種果得果遂成現在之身。",
    "X65n1277_005:0019a16_1": "知恩報恩爰說受生之教。",
    "X65n1277_005:0019a17_2": "向日既云借借。",
    "X65n1277_005:0019a17_3": "今時當為填還。",
    "X65n1277_005:0019a17_4": "仰祈乃聖乃神。",
    "X65n1277_005:0019a17_5": "俯察真心真意。",
    "X65n1277_005:0019a18_6": "津梁既布。",
    "X65n1277_005:0019a18_7": "業障潛消。",
    "X65n1277_005:0019a18_8": "福海壽山更冀他年之深固。",
    "X65n1277_005:0019a19_9": "春風和氣常如此日之雍容。",
    "X65n1277_005:0019a20_0": "血盆",
    "X65n1277_005:0019a21_0": "母號鞠我。",
    "X65n1277_005:0019a21_1": "既全撫育之恩。",
    "X65n1277_005:0019a21_2": "子之愛親。",
    "X65n1277_005:0019a21_3": "當報劬勞之德。",
    "X65n1277_005:0019a22_0": "爰崇上善。",
    "X65n1277_005:0019a22_1": "仰答生身。",
    "X65n1277_005:0019a22_2": "伏願母親。",
    "X65n1277_005:0019a22_3": "千生福長十惡罪消。",
    "X65n1277_005:0019a23_0": "血盆化作蓮盆。",
    "X65n1277_005:0019a23_1": "業海飜成法海。",
    "X65n1277_005:0019a23_2": "今生今世移災星為福星。",
    "X65n1277_005:0019a24_3": "他日他時從人道歸佛道。",
    "X65n1277_005:0019a24_4": "地藏能仁祈作證。",
    "X65n1277_005:0019a24_5": "目連教主望垂慈。",
    "X65n1277_005:0019b02_0": "送星",
    "X65n1277_005:0019b03_0": "生居下土。",
    "X65n1277_005:0019b03_1": "命屬上穹。",
    "X65n1277_005:0019b03_2": "恐惡運以加臨。",
    "X65n1277_005:0019b03_3": "慮灾星而不正。",
    "X65n1277_005:0019b04_0": "爰殫愚悃。",
    "X65n1277_005:0019b04_1": "仰扣天聰。",
    "X65n1277_005:0019b04_2": "望消無妄之災。",
    "X65n1277_005:0019b04_3": "大賜有餘之慶。",
    "X65n1277_005:0019b05_0": "凶運化為吉運。",
    "X65n1277_005:0019b05_1": "災星轉作福星。",
    "X65n1277_005:0019b05_2": "丹臺書不老之年。",
    "X65n1277_005:0019b05_3": "紫府注添粮之字。",
    "X65n1277_005:0019b06_4": "羅睺月孛永無陵犯之虞。",
    "X65n1277_005:0019b06_5": "驛馬攀鞍常被照臨之德。",
    "X65n1277_005:0019b07_6": "五行輻湊。",
    "X65n1277_005:0019b07_7": "四柱盤安。",
    "X65n1277_005:0019b07_8": "全仗高真大垂密佑。",
    "X65n1277_005:0019b09_0": "還願",
    "X65n1277_005:0019b10_0": "嚮干神造。",
    "X65n1277_005:0019b10_1": "務求覆護之恩。",
    "X65n1277_005:0019b10_2": "今閱歲時。",
    "X65n1277_005:0019b10_3": "果荷携持之力。",
    "X65n1277_005:0019b11_0": "既仰蒙於陰賜。",
    "X65n1277_005:0019b11_1": "寧敢負於初心。",
    "X65n1277_005:0019b11_2": "謹殫此日之誠。",
    "X65n1277_005:0019b11_3": "特賽前時之願。",
    "X65n1277_005:0019b12_4": "六曹案上望消結許之名。",
    "X65n1277_005:0019b12_5": "三界簿中乞注酧還之字。",
    "X65n1277_005:0019b13_6": "大賜始終之福。",
    "X65n1277_005:0019b13_7": "已成圓滿之功。",
    "X65n1277_005:0019b14_0": "祈男",
    "X65n1277_005:0019b15_0": "嵩岳降神遂生賢相。",
    "X65n1277_005:0019b15_1": "尼丘請禱果產聖人。",
    "X65n1277_005:0019b15_2": "天聽雖高。",
    "X65n1277_005:0019b16_0": "人情可格。",
    "X65n1277_005:0019b16_1": "念某蓬蒿賤類草木微生。",
    "X65n1277_005:0019b16_2": "久諧琴瑟之音。",
    "X65n1277_005:0019b17_0": "未兆熊羆之夢。",
    "X65n1277_005:0019b17_1": "爰殫愚悃。",
    "X65n1277_005:0019b17_2": "上瀆聖聰。",
    "X65n1277_005:0019b17_3": "望生智惠之男。",
    "X65n1277_005:0019b18_0": "早遂吉祥之願。",
    "X65n1277_005:0019b19_0": "謝男",
    "X65n1277_005:0019b20_0": "向干聖聽。",
    "X65n1277_005:0019b20_1": "願符墮燕之懷。",
    "X65n1277_005:0019b20_2": "果遂愚衷。",
    "X65n1277_005:0019b20_3": "默契夢熊之兆。",
    "X65n1277_005:0019b21_0": "繩繩宜子。",
    "X65n1277_005:0019b21_1": "袞袞生侯。",
    "X65n1277_005:0019b21_2": "知家道之必興。",
    "X65n1277_005:0019b21_3": "使祖風之不墜。",
    "X65n1277_005:0019b22_0": "實資陰助。",
    "X65n1277_005:0019b22_1": "敢後謝誠。",
    "X65n1277_005:0019b23_0": "保胎",
    "X65n1277_005:0019b24_0": "伏念某氏身懷胎甲。",
    "X65n1277_005:0019b24_1": "德荷乾坤。",
    "X65n1277_005:0019b24_2": "恐當生產之時。",
    "X65n1277_005:0019b24_3": "或有艱難之慮。",
    "X65n1277_005:0019c01_4": "須憑聖力。",
    "X65n1277_005:0019c01_5": "大賜陰功。",
    "X65n1277_005:0019c01_6": "保十月以輕安。",
    "X65n1277_005:0019c01_7": "護一時而無難。",
    "X65n1277_005:0019c02_8": "監生聖母內消分免之愆。",
    "X65n1277_005:0019c02_9": "護產靈童大賜吉祥之福。",
    "X65n1277_005:0019c03_10": "熊羆恊夢。",
    "X65n1277_005:0019c03_11": "弧矢張門。",
    "X65n1277_005:0019c03_12": "早生男子之祥。",
    "X65n1277_005:0019c03_13": "行繼祖風之裔。",
    "X65n1277_005:0019c05_0": "賽謝",
    "X65n1277_005:0019c06_0": "富貴自天。",
    "X65n1277_005:0019c06_1": "匪人力之能致。",
    "X65n1277_005:0019c06_2": "福祥有地。",
    "X65n1277_005:0019c06_3": "惟善果之可求。",
    "X65n1277_005:0019c07_0": "頃慈親孕秀之時。",
    "X65n1277_005:0019c07_1": "為幼子懷躭之日。",
    "X65n1277_005:0019c07_2": "無門可仗。",
    "X65n1277_005:0019c07_3": "有佛堪依。",
    "X65n1277_005:0019c08_4": "托聖力以護持。",
    "X65n1277_005:0019c08_5": "荷靈體而保芘。",
    "X65n1277_005:0019c08_6": "子母喜諧於分免。",
    "X65n1277_005:0019c09_7": "妻兒獲遂於安全。",
    "X65n1277_005:0019c09_8": "誕生果慶吉祥。",
    "X65n1277_005:0019c09_9": "抱送實憑釋氏。",
    "X65n1277_005:0019c10_0": "有願須當滿願。",
    "X65n1277_005:0019c10_1": "知恩豈至忘恩。",
    "X65n1277_005:0019c11_0": "保病",
    "X65n1277_005:0019c12_0": "切念某人累歲多迍經時抱疢。",
    "X65n1277_005:0019c12_1": "恐惡星而照運。",
    "X65n1277_005:0019c12_2": "致灾厄以纏外。",
    "X65n1277_005:0019c13_3": "無門伸禳度之方。",
    "X65n1277_005:0019c13_4": "有聖作歸投之主。",
    "X65n1277_005:0019c13_5": "伏願灾星退度。",
    "X65n1277_005:0019c14_6": "消除見在之灾。",
    "X65n1277_005:0019c14_7": "福曜來臨。",
    "X65n1277_005:0019c14_8": "大賜當來之福。",
    "X65n1277_005:0019c15_0": "天醫使者護正氣以長存。",
    "X65n1277_005:0019c15_1": "安樂藏王蕩邪風而永息。",
    "X65n1277_005:0019c16_0": "五神安靜。",
    "X65n1277_005:0019c16_1": "百脉調和。",
    "X65n1277_005:0019c16_2": "身田等松柏之堅。",
    "X65n1277_005:0019c16_3": "壽筭比崗陵之固。",
    "X65n1277_005:0019c18_0": "祈安",
    "X65n1277_005:0019c19_0": "邇來年庚不正。",
    "X65n1277_005:0019c19_1": "運命多災。",
    "X65n1277_005:0019c19_2": "致疾病以相纏。",
    "X65n1277_005:0019c19_3": "服藥餌而不瘥。",
    "X65n1277_005:0019c20_4": "欲伸禳度須仗慈悲。",
    "X65n1277_005:0019c20_5": "伏願精意調和。",
    "X65n1277_005:0019c20_6": "眾真孚祐。",
    "X65n1277_005:0019c21_0": "消釋未萌之咎。",
    "X65n1277_005:0019c21_1": "赦除已往之非。",
    "X65n1277_005:0019c21_2": "吉星照而命運光輝。",
    "X65n1277_005:0019c22_0": "惡曜消而身宮安泰。",
    "X65n1277_005:0019c22_1": "五行順適無交加衝戰之虞。",
    "X65n1277_005:0019c22_2": "六脉均調去急嗇沈浮之苦。",
    "X65n1277_005:0019c23_3": "龍神擁護。",
    "X65n1277_005:0019c23_4": "天意扶持。",
    "X65n1277_005:0019c24_0": "禳灾",
    "X65n1277_005:0020a01_0": "伏念生逢盛世性本下愚。",
    "X65n1277_005:0020a01_1": "荷乾坤覆載之恩。",
    "X65n1277_005:0020a01_2": "戴日月照臨之德。",
    "X65n1277_005:0020a02_3": "切慮災生不測。",
    "X65n1277_005:0020a02_4": "人貴預禳。",
    "X65n1277_005:0020a02_5": "共斂家貲虔修佛事。",
    "X65n1277_005:0020a03_6": "伏願善因禳度。",
    "X65n1277_005:0020a03_7": "灾星化作福星。",
    "X65n1277_005:0020a03_8": "聖力護持。",
    "X65n1277_005:0020a03_9": "妖氣變為瑞氣。",
    "X65n1277_005:0020a04_10": "處處總成安樂國。",
    "X65n1277_005:0020a04_11": "家家咸作太平人。",
    "X65n1277_005:0020a04_12": "陰陽以和。",
    "X65n1277_005:0020a05_13": "風雨以時。",
    "X65n1277_005:0020a05_14": "災害不生。",
    "X65n1277_005:0020a05_15": "禍亂不作。",
    "X65n1277_005:0020a05_16": "四民樂業。",
    "X65n1277_005:0020a05_17": "八口無飢。",
    "X65n1277_005:0020a06_18": "倉稟實而府庫充。",
    "X65n1277_005:0020a06_19": "盜賊衰而獄訟息。",
    "X65n1277_005:0020a06_20": "各符心願。",
    "X65n1277_005:0020a07_0": "俱沐聖休。",
    "X65n1277_005:0020a08_0": "禳火",
    "X65n1277_005:0020a09_0": "上天雖遠本自心通。",
    "X65n1277_005:0020a09_1": "厚福無疆悉由人致。",
    "X65n1277_005:0020a09_2": "切慮星流熒惑災及坊民。",
    "X65n1277_005:0020a10_3": "輿情謝過以無門。",
    "X65n1277_005:0020a10_4": "我佛投誠而有禱。",
    "X65n1277_005:0020a11_0": "仰祈天脊俯念凡情。",
    "X65n1277_005:0020a11_1": "轉禍釁以為祥。",
    "X65n1277_005:0020a11_2": "變凶災而成吉。",
    "X65n1277_005:0020a12_0": "星躔順度。",
    "X65n1277_005:0020a12_1": "分野無虞。",
    "X65n1277_005:0020a12_2": "融神斂迹於本鄉。",
    "X65n1277_005:0020a12_3": "火德迴光於仙闕。",
    "X65n1277_005:0020a13_4": "四民帖靜。",
    "X65n1277_005:0020a13_5": "萬戶安康。",
    "X65n1277_005:0020a13_6": "風雨調和。",
    "X65n1277_005:0020a13_7": "田園豐稔。",
    "X65n1277_005:0020a13_8": "全資至德大慰群情。",
    "X65n1277_005:0020a15_0": "禳水",
    "X65n1277_005:0020a16_0": "積雨過常伏陰為沴。",
    "X65n1277_005:0020a16_1": "失於潤下逮此橫流。",
    "X65n1277_005:0020a16_2": "大田之稼其空。",
    "X65n1277_005:0020a17_3": "比屋之農胥歎。",
    "X65n1277_005:0020a17_4": "亟憑梵典用殄天灾。",
    "X65n1277_005:0020a17_5": "仰祈護世之慈。",
    "X65n1277_005:0020a18_6": "俯濟薦瘥之困。",
    "X65n1277_005:0020a18_7": "散白毫於頂上。",
    "X65n1277_005:0020a18_8": "盡逐頑陰。",
    "X65n1277_005:0020a18_9": "吞巨浸於毛端。",
    "X65n1277_005:0020a19_10": "坐成沃野。",
    "X65n1277_005:0020a19_11": "能仁普鑒。",
    "X65n1277_005:0020a19_12": "大庇云依。",
    "X65n1277_005:0020a20_0": "保苗",
    "X65n1277_005:0020a21_0": "不稂不莠雖資地利之宜。",
    "X65n1277_005:0020a21_1": "實方實苞尤賴天時之美。",
    "X65n1277_005:0020a22_0": "念農務既興於南畝。",
    "X65n1277_005:0020a22_1": "而歲功莫計於西成。",
    "X65n1277_005:0020a22_2": "用瀝丹誠仰投皇覺。",
    "X65n1277_005:0020a23_3": "伏願金穰協應。",
    "X65n1277_005:0020a23_4": "玉燭均調。",
    "X65n1277_005:0020a23_5": "冬無愆陽。",
    "X65n1277_005:0020a23_6": "夏無伏陰。",
    "X65n1277_005:0020a24_7": "風不鳴條。",
    "X65n1277_005:0020a24_8": "雨不破塊。",
    "X65n1277_005:0020a24_9": "蝗蟲頓息。",
    "X65n1277_005:0020a24_10": "稂莠不生。",
    "X65n1277_005:0020a24_11": "上天應覆斗之祥。",
    "X65n1277_005:0020b01_12": "下土感夢魚之兆。",
    "X65n1277_005:0020b01_13": "眾姓喜積倉之慶。",
    "X65n1277_005:0020b01_14": "四方恣鼓腹之遊。",
    "X65n1277_005:0020b02_15": "全賴聖休。",
    "X65n1277_005:0020b02_16": "默符心願。",
    "X65n1277_005:0020b03_0": "保蚕",
    "X65n1277_005:0020b04_0": "生居女質。",
    "X65n1277_005:0020b04_1": "忝處閨門。",
    "X65n1277_005:0020b04_2": "欲全婦道無虧。",
    "X65n1277_005:0020b04_3": "莫匪蚕桑最急。",
    "X65n1277_005:0020b05_0": "雖資人力實假佛緣。",
    "X65n1277_005:0020b05_1": "伏願如意輪王能轉無而作有。",
    "X65n1277_005:0020b06_0": "馬鳴大士為變少以成多。",
    "X65n1277_005:0020b06_1": "繭若霜明。",
    "X65n1277_005:0020b06_2": "絲如山積。",
    "X65n1277_005:0020b06_3": "蔑有一毫虗耗。",
    "X65n1277_005:0020b07_4": "願令十倍全収。",
    "X65n1277_005:0020b08_0": "修造",
    "X65n1277_005:0020b09_0": "(住宅云樂業安居。",
    "X65n1277_005:0020b09_1": "墳墓云掘地安墳)必假經營之力。",
    "X65n1277_005:0020b09_2": "興工動土寧無觸犯之虞。",
    "X65n1277_005:0020b10_3": "特仗慈悲少伸安慰。",
    "X65n1277_005:0020b10_4": "伏願五龍就位。",
    "X65n1277_005:0020b10_5": "八將還方。",
    "X65n1277_005:0020b11_6": "四殺退藏。",
    "X65n1277_005:0020b11_7": "藉賢聖斡旋之力。",
    "X65n1277_005:0020b11_8": "百凶永釋。",
    "X65n1277_005:0020b11_9": "資乾坤運轉之功。",
    "X65n1277_005:0020b12_10": "戶殄千灾。",
    "X65n1277_005:0020b12_11": "門迎百福。",
    "X65n1277_005:0020b13_0": "解結",
    "X65n1277_005:0020b14_0": "夙緣夙業寧無已往之[億-音+(天*天)]冤。",
    "X65n1277_005:0020b14_1": "今世今生必有故為之罪垢。",
    "X65n1277_005:0020b15_2": "幸諸佛廣慈悲之路。",
    "X65n1277_005:0020b15_3": "許眾生陳懺悔之心。",
    "X65n1277_005:0020b15_4": "爰憑解結洪休。",
    "X65n1277_005:0020b16_5": "願滌無邊黑業。",
    "X65n1277_005:0020b16_6": "一結一解十惡五逆悉消除。",
    "X65n1277_005:0020b17_7": "一贊一楊六趣三塗俱解脫。",
    "X65n1277_005:0020b17_8": "惡果化為善果。",
    "X65n1277_005:0020b17_9": "罪根翻作福根。",
    "X65n1277_005:0020b18_10": "用集良因。",
    "X65n1277_005:0020b18_11": "克符善願。",
    "X65n1277_005:0020b19_0": "祈雨",
    "X65n1277_005:0020b20_0": "田苗將秀霖雨不滋。",
    "X65n1277_005:0020b20_1": "倘有失於高天。",
    "X65n1277_005:0020b20_2": "則無望於卒出。",
    "X65n1277_005:0020b21_0": "仰瞻聖造。",
    "X65n1277_005:0020b21_1": "望鑒愚誠。",
    "X65n1277_005:0020b21_2": "願憑一辨之香。",
    "X65n1277_005:0020b21_3": "早賜連宵之澤。",
    "X65n1277_005:0020b22_0": "庶少蘇於民望。",
    "X65n1277_005:0020b22_1": "俾均載於天休。",
    "X65n1277_005:0020b23_0": "謝雨",
    "X65n1277_005:0020b24_0": "天晴既久。",
    "X65n1277_005:0020b24_1": "恐稼穡之焦枯。",
    "X65n1277_005:0020b24_2": "聖力可求。",
    "X65n1277_005:0020b24_3": "歲雨霖而滂沱。",
    "X65n1277_005:0020c01_0": "大地既甦於春作。",
    "X65n1277_005:0020c01_1": "普天仰荷於秋成。",
    "X65n1277_005:0020c01_2": "特展微誠。",
    "X65n1277_005:0020c01_3": "少伸謝悃。",
    "X65n1277_005:0020c02_4": "更望作成之賜。",
    "X65n1277_005:0020c02_5": "即登稔之年。",
    "X65n1277_005:0020c03_0": "祈晴",
    "X65n1277_005:0020c04_0": "積陰作沴。",
    "X65n1277_005:0020c04_1": "潤茲常雨之愆。",
    "X65n1277_005:0020c04_2": "新穀未登。",
    "X65n1277_005:0020c04_3": "懼失有秋之望。",
    "X65n1277_005:0020c05_0": "用傾誠悃。",
    "X65n1277_005:0020c05_1": "仰扣聖聰。",
    "X65n1277_005:0020c05_2": "伏願放白毫光如一念頃。",
    "X65n1277_005:0020c05_3": "破頑陰於四極。",
    "X65n1277_005:0020c06_4": "瑩麗日於九宵。",
    "X65n1277_005:0020c06_5": "永賴庇存。",
    "X65n1277_005:0020c06_6": "益堅回向。",
    "X65n1277_005:0020c07_0": "謝晴",
    "X65n1277_005:0020c08_0": "近以霖潦為灾盛陰積[口*每]。",
    "X65n1277_005:0020c08_1": "涓敷[款-士+止]素詣叩慈仁。",
    "X65n1277_005:0020c08_2": "果膺六度之虗。",
    "X65n1277_005:0020c09_3": "尋弭三農之患。",
    "X65n1277_005:0020c09_4": "既垂法蔭。",
    "X65n1277_005:0020c09_5": "爰舉梵儀。",
    "X65n1277_005:0020c09_6": "彌慶歸依。",
    "X65n1277_005:0020c10_7": "庶申謝答。",
    "X65n1277_005:0020c11_0": "祈雪",
    "X65n1277_005:0020c12_0": "佛果如圓法力平等。",
    "X65n1277_005:0020c12_1": "雖無求而不獲。",
    "X65n1277_005:0020c12_2": "須有感而後通。",
    "X65n1277_005:0020c13_0": "敢竭精衷上干慈照。",
    "X65n1277_005:0020c13_1": "此者冬陰不固。",
    "X65n1277_005:0020c13_2": "時雪既愆。",
    "X65n1277_005:0020c13_3": "幾成焦旱之灾。",
    "X65n1277_005:0020c14_4": "切慮疾疫之變。",
    "X65n1277_005:0020c14_5": "伏願不違本誓力度有情。",
    "X65n1277_005:0020c15_0": "三白飛天類鵝毛之宛轉。",
    "X65n1277_005:0020c15_1": "六花匝地若鶴影以徘徊。",
    "X65n1277_005:0020c16_0": "壓萬里之妖氣。",
    "X65n1277_005:0020c16_1": "作三農之瑞慶。",
    "X65n1277_005:0020c17_0": "謝雪",
    "X65n1277_005:0020c18_0": "向者六出不飛。",
    "X65n1277_005:0020c18_1": "一年繫望。",
    "X65n1277_005:0020c18_2": "感陰功之憫物。",
    "X65n1277_005:0020c18_3": "變朔氣以迎寒。",
    "X65n1277_005:0020c19_4": "四野飛花。",
    "X65n1277_005:0020c19_5": "千山擘絮。",
    "X65n1277_005:0020c19_6": "大地從茲蒙厚澤。",
    "X65n1277_005:0020c19_7": "敷天自此賀昇平。",
    "X65n1277_005:0020c20_8": "實荷佛慈。",
    "X65n1277_005:0020c20_9": "庸殫謝禮。",
    "X65n1277_005:0020c21_0": "諸般偈讚門",
    "X65n1277_005:0020c22_0": "佛",
    "X65n1277_005:0020c23_0": "清和天氣屬朱明。",
    "X65n1277_005:0020c23_1": "正是如來降誕辰。",
    "X65n1277_005:0020c23_2": "萬聖傾心瞻寶相。",
    "X65n1277_005:0020c24_3": "九龍吐水浴金身。",
    "X65n1277_005:0020c24_4": "王宮不顧瓊花好。",
    "X65n1277_005:0020c24_5": "雪嶺勤修道果成。",
    "X65n1277_005:0021a01_6": "有願更祈從此願。",
    "X65n1277_005:0021a01_7": "攀迎諸聖賜光臨。",
    "X65n1277_005:0021a02_0": "藥師",
    "X65n1277_005:0021a03_0": "藥師教主願門開。",
    "X65n1277_005:0021a03_1": "濟度群生萬劫灾。",
    "X65n1277_005:0021a03_2": "菡萏馨香薰宇宙。",
    "X65n1277_005:0021a04_3": "琉璃光裏瀉樓臺。",
    "X65n1277_005:0021a04_4": "一音妙教通三界。",
    "X65n1277_005:0021a04_5": "七級銀釭耀九垓。",
    "X65n1277_005:0021a05_6": "今夜信心酬願後。",
    "X65n1277_005:0021a05_7": "祥光瑞氣擁門來。",
    "X65n1277_005:0021a06_0": "彌陀",
    "X65n1277_005:0021a07_0": "彌陀教法振西方。",
    "X65n1277_005:0021a07_1": "救度群生妙莫量。",
    "X65n1277_005:0021a07_2": "九品蓮臺垂接引。",
    "X65n1277_005:0021a08_3": "七重寶樹列芬芳。",
    "X65n1277_005:0021a08_4": "巍巍金相端嚴好。",
    "X65n1277_005:0021a08_5": "皎皎銀毫燦爛光。",
    "X65n1277_005:0021a09_6": "檀信若能勤讚念。",
    "X65n1277_005:0021a09_7": "頓超淨土悟真常。",
    "X65n1277_005:0021a10_0": "熾盛",
    "X65n1277_005:0021a11_0": "娑羅王佛現威通。",
    "X65n1277_005:0021a11_1": "會集諸天十二宮。",
    "X65n1277_005:0021a11_2": "足躡金輪光赫奕。",
    "X65n1277_005:0021a12_3": "身披寶服瑞玲瓏。",
    "X65n1277_005:0021a12_4": "吉祥成就無窮外。",
    "X65n1277_005:0021a12_5": "灾難消除不測中。",
    "X65n1277_005:0021a13_6": "欲識金晨蒙福處。",
    "X65n1277_005:0021a13_7": "門庭佳氣藹葱葱。",
    "X65n1277_005:0021a14_0": "圓通",
    "X65n1277_005:0021a15_0": "圓通聖主大慈尊。",
    "X65n1277_005:0021a15_1": "誓願弘深不可論。",
    "X65n1277_005:0021a15_2": "紅菡萏花開月面。",
    "X65n1277_005:0021a16_3": "碧琉璃水瀉羅紋。",
    "X65n1277_005:0021a16_4": "霏霏法雨祛邪霧。",
    "X65n1277_005:0021a16_5": "浩浩慈風滅障雲。",
    "X65n1277_005:0021a17_6": "惟願慈悲增福德。",
    "X65n1277_005:0021a17_7": "吉祥如意壽千春。",
    "X65n1277_005:0021a18_0": "地藏",
    "X65n1277_005:0021a19_0": "幽冥菩薩妙難論。",
    "X65n1277_005:0021a19_1": "應現真容處處分。",
    "X65n1277_005:0021a19_2": "六道三塗聞妙法。",
    "X65n1277_005:0021a20_3": "四生十類沐慈恩。",
    "X65n1277_005:0021a20_4": "寶珠照徹天堂路。",
    "X65n1277_005:0021a20_5": "金錫敲開地獄門。",
    "X65n1277_005:0021a21_6": "是夜亡魂蒙接引。",
    "X65n1277_005:0021a21_7": "蓮華臺上禮慈尊。",
    "X65n1277_005:0021a22_0": "目連",
    "X65n1277_005:0021a23_0": "目連尊者運神通。",
    "X65n1277_005:0021a23_1": "擲鉢騰空瞬息中。",
    "X65n1277_005:0021a23_2": "金錫振時開地獄。",
    "X65n1277_005:0021a24_3": "額珠照處徹虗空。",
    "X65n1277_005:0021a24_4": "欲令慈母生天界。",
    "X65n1277_005:0021a24_5": "屢啟誠心問佛功。",
    "X65n1277_005:0021b01_6": "崇建蘭盆燃惠炬。",
    "X65n1277_005:0021b01_7": "流傳萬世永無窮。",
    "X65n1277_005:0021b02_0": "泗洲",
    "X65n1277_005:0021b03_0": "泗洲菩薩號僧伽。",
    "X65n1277_005:0021b03_1": "歸命消灾長善芽。",
    "X65n1277_005:0021b03_2": "瀟洒祇園興雨露。",
    "X65n1277_005:0021b04_3": "巍峩寶塔插雲霞。",
    "X65n1277_005:0021b04_4": "教開六度驅瘟疫。",
    "X65n1277_005:0021b04_5": "德濟群生別正邪。",
    "X65n1277_005:0021b05_6": "願別蓮宮臨法會。",
    "X65n1277_005:0021b05_7": "介茲景福等河沙。",
    "X65n1277_005:0021b06_0": "定光",
    "X65n1277_005:0021b07_0": "定光祖佛駐汀洲。",
    "X65n1277_005:0021b07_1": "果滿真機累劫修。",
    "X65n1277_005:0021b07_2": "杖擊盤山泉湧注。",
    "X65n1277_005:0021b08_3": "偈投古岸水分流。",
    "X65n1277_005:0021b08_4": "求男求女隨心現。",
    "X65n1277_005:0021b08_5": "祈雨祈晴逐意酬。",
    "X65n1277_005:0021b09_6": "願別南岩臨法會。",
    "X65n1277_005:0021b09_7": "從今信善沐殊休。",
    "X65n1277_005:0021b10_0": "六祖",
    "X65n1277_005:0021b11_0": "祖師玄妙悟真空。",
    "X65n1277_005:0021b11_1": "智惠通靈般若功。",
    "X65n1277_005:0021b11_2": "明鏡非臺光顯化。",
    "X65n1277_005:0021b12_3": "菩提無樹果圓容。",
    "X65n1277_005:0021b12_4": "衣傳西國明師旨。",
    "X65n1277_005:0021b12_5": "法闡南宗領御封。",
    "X65n1277_005:0021b13_6": "萬古曹溪流不盡。",
    "X65n1277_005:0021b13_7": "灯灯續焰永無窮。",
    "X65n1277_005:0021b14_0": "僧伽",
    "X65n1277_005:0021b15_0": "雲州勝境冠群方。",
    "X65n1277_005:0021b15_1": "靈濟僧伽建道場。",
    "X65n1277_005:0021b15_2": "蛇岳威靈常擁護。",
    "X65n1277_005:0021b16_3": "屼山靈顯越昭彰。",
    "X65n1277_005:0021b16_4": "十方歸命消灾障。",
    "X65n1277_005:0021b16_5": "四海欽崇降吉祥。",
    "X65n1277_005:0021b17_6": "伏冀慈悲臨法會。",
    "X65n1277_005:0021b17_7": "殷勤虔爇一爐香。",
    "X65n1277_005:0021b18_0": "伽藍",
    "X65n1277_005:0021b19_0": "伽藍本是給孤尊。",
    "X65n1277_005:0021b19_1": "發願修行鎮法門。",
    "X65n1277_005:0021b19_2": "上祐能仁歸正道。",
    "X65n1277_005:0021b20_3": "下安釋子滅邪根。",
    "X65n1277_005:0021b20_4": "興崇寶剎千年盛。",
    "X65n1277_005:0021b20_5": "擁護精藍萬古存。",
    "X65n1277_005:0021b21_6": "此夜讚揚資聖力。",
    "X65n1277_005:0021b21_7": "法徒早早沐皇恩。",
    "X65n1277_005:0021b22_0": "社王",
    "X65n1277_005:0021b23_0": "靈壇古跡鎮山河。",
    "X65n1277_005:0021b23_1": "梓里祈求感應多。",
    "X65n1277_005:0021b23_2": "聖德昂藏垂福祚。",
    "X65n1277_005:0021b24_3": "威風顯跡殄妖魔。",
    "X65n1277_005:0021b24_4": "資陪景貺歸三寶。",
    "X65n1277_005:0021b24_5": "報答恩休仗六和。",
    "X65n1277_005:0021c01_6": "從此鄉閭常怗泰。",
    "X65n1277_005:0021c01_7": "人人壽岳聳巍峩。",
    "X65n1277_005:0021c02_0": "預修",
    "X65n1277_005:0021c03_0": "雞皮鶴髮不相饒。",
    "X65n1277_005:0021c03_1": "忙把明香對佛燒。",
    "X65n1277_005:0021c03_2": "惠燭熒煌侵夜月。",
    "X65n1277_005:0021c04_3": "寶幡搖曳逐寒飈。",
    "X65n1277_005:0021c04_4": "福田山下明真路。",
    "X65n1277_005:0021c04_5": "苦海波心架法橋。",
    "X65n1277_005:0021c05_6": "好是百年歸去日。",
    "X65n1277_005:0021c05_7": "翛翛平步上丹霄。",
    "X65n1277_005:0021c06_0": "道姑預修",
    "X65n1277_005:0021c07_0": "一身孤立歲寒中。",
    "X65n1277_005:0021c07_1": "禮塔持經事莫窮。",
    "X65n1277_005:0021c07_2": "功德林中重進步。",
    "X65n1277_005:0021c08_3": "菩提路上好収功。",
    "X65n1277_005:0021c08_4": "七層銀燭資陰帝。",
    "X65n1277_005:0021c08_5": "四部琅函答聖聦。",
    "X65n1277_005:0021c09_6": "要識預修圓滿處。",
    "X65n1277_005:0021c09_7": "一輪明月皎長空。",
    "X65n1277_005:0021c10_0": "看華嚴",
    "X65n1277_005:0021c11_0": "回心向善誦華嚴。",
    "X65n1277_005:0021c11_1": "見性分明自不凡。",
    "X65n1277_005:0021c11_2": "經閱三乘歸寶藏。",
    "X65n1277_005:0021c12_3": "僧邀八戒向精藍。",
    "X65n1277_005:0021c12_4": "仰瞻佛國三千界。",
    "X65n1277_005:0021c12_5": "數演靈文十二函。",
    "X65n1277_005:0021c13_6": "要識妙因圓滿處。",
    "X65n1277_005:0021c13_7": "一輪明月映寒潭。",
    "X65n1277_005:0021c14_0": "懺血盆",
    "X65n1277_005:0021c15_0": "翻思慈母產生年。",
    "X65n1277_005:0021c15_1": "萬種難辛不易言。",
    "X65n1277_005:0021c15_2": "十月懷擔難報德。",
    "X65n1277_005:0021c16_3": "三年齋素誓酬恩。",
    "X65n1277_005:0021c16_4": "血盆化作紅蓮沼。",
    "X65n1277_005:0021c16_5": "地獄翻成淨土門。",
    "X65n1277_005:0021c17_6": "富貴榮華從此得。",
    "X65n1277_005:0021c17_7": "良由行孝感乾坤。",
    "X65n1277_005:0021c18_0": "還燈願",
    "X65n1277_005:0021c19_0": "昔年跪許禱金仙。",
    "X65n1277_005:0021c19_1": "今夜酬恩建法筵。",
    "X65n1277_005:0021c19_2": "七級琉璃燃惠炬。",
    "X65n1277_005:0021c20_3": "一爐薝蔔裊祥烟。",
    "X65n1277_005:0021c20_4": "銀灯燦處消灾障。",
    "X65n1277_005:0021c20_5": "金磬敲時雪罪愆。",
    "X65n1277_005:0021c21_6": "好是功圓心滿處。",
    "X65n1277_005:0021c21_7": "合家眷屬福綿綿。",
    "X65n1277_005:0021c22_0": "建燈懺塔",
    "X65n1277_005:0021c23_0": "慶門信善竭誠心。",
    "X65n1277_005:0021c23_1": "禮畢浮圖滿願成。",
    "X65n1277_005:0021c23_2": "舍利光中含萬像。",
    "X65n1277_005:0021c24_3": "真如影裏現千身。",
    "X65n1277_005:0021c24_4": "如來公案崇殊福。",
    "X65n1277_005:0021c24_5": "熾盛星[火*((尤-尢+氏)/日)]建妙因。",
    "X65n1277_005:0022a01_6": "表懺周圓諸佛護。",
    "X65n1277_005:0022a01_7": "榮花富貴保千春。",
    "X65n1277_005:0022a02_0": "眾信懺塔",
    "X65n1277_005:0022a03_0": "眾心歸向各精虔。",
    "X65n1277_005:0022a03_1": "洒掃街衢集善緣。",
    "X65n1277_005:0022a03_2": "香爇愽山楊玉偈。",
    "X65n1277_005:0022a04_3": "塔瞻阿育禮金仙。",
    "X65n1277_005:0022a04_4": "人人電掃千灾滅。",
    "X65n1277_005:0022a04_5": "戶戶雲臻五福全。",
    "X65n1277_005:0022a05_6": "從此民安并國泰。",
    "X65n1277_005:0022a05_7": "君王聖壽萬斯年。",
    "X65n1277_005:0022a06_0": "祈雨",
    "X65n1277_005:0022a07_0": "傷嗟龜兆折田疇。",
    "X65n1277_005:0022a07_1": "火日燒空水絕流。",
    "X65n1277_005:0022a07_2": "旱魃為災何大酷。",
    "X65n1277_005:0022a08_3": "商羊罷舞實堪憂。",
    "X65n1277_005:0022a08_4": "人心懇切難伸訢。",
    "X65n1277_005:0022a08_5": "我佛慈悲應禱求。",
    "X65n1277_005:0022a09_6": "願霈甘霖蘇萬物。",
    "X65n1277_005:0022a09_7": "勃興苗稼富秋収。",
    "X65n1277_005:0022a10_0": "祈晴",
    "X65n1277_005:0022a11_0": "金爐玉穟氣氤氳。",
    "X65n1277_005:0022a11_1": "信士祈晴發意勤。",
    "X65n1277_005:0022a11_2": "佛號吟時千聖集。",
    "X65n1277_005:0022a12_3": "法音震處九天聞。",
    "X65n1277_005:0022a12_4": "清風吹散千山霧。",
    "X65n1277_005:0022a12_5": "紅日燒開萬里雲。",
    "X65n1277_005:0022a13_6": "但願聖功垂感應。",
    "X65n1277_005:0022a13_7": "更無淫雨下雰雰。",
    "X65n1277_005:0022a14_0": "散花",
    "X65n1277_005:0022a15_0": "此花元不費[栽-木+土]培。",
    "X65n1277_005:0022a15_1": "多謝佳人巧剪裁。",
    "X65n1277_005:0022a15_2": "錦綉千般呈雅態。",
    "X65n1277_005:0022a16_3": "綺羅五色絕纖埃。",
    "X65n1277_005:0022a16_4": "發生不假陽和力。",
    "X65n1277_005:0022a16_5": "點綴寧憂風雨摧。",
    "X65n1277_005:0022a17_6": "捧献佛前申善頌。",
    "X65n1277_005:0022a17_7": "願祈福祿日俱來。",
    "X65n1277_005:0022a18_0": "解結",
    "X65n1277_005:0022a19_0": "梵音清徹海潮音。",
    "X65n1277_005:0022a19_1": "旋遶銀灯午夜深。",
    "X65n1277_005:0022a19_2": "七七願拋功已滿。",
    "X65n1277_005:0022a20_3": "三三結解罪消沉。",
    "X65n1277_005:0022a20_4": "聲聲同入真如道。",
    "X65n1277_005:0022a20_5": "念念常無退轉心。",
    "X65n1277_005:0022a21_6": "保佑信人從此去。",
    "X65n1277_005:0022a21_7": "滔滔福海漲千尋。",
    "X65n1277_005:0022a22_0": "懺殺生",
    "X65n1277_005:0022a23_0": "浮生碌碌在樊籠。",
    "X65n1277_005:0022a23_1": "結作冤讎數莫窮。",
    "X65n1277_005:0022a23_2": "祈禱鬼神求福力。",
    "X65n1277_005:0022a24_3": "邀迎賓客逞家風。",
    "X65n1277_005:0022a24_4": "羽毛剉斫刀砧上。",
    "X65n1277_005:0022a24_5": "鱗介烹炮鼎鑊中。",
    "X65n1277_005:0022b01_6": "眾信燃灯伸懺悔。",
    "X65n1277_005:0022b01_7": "一時超脫步天宮。",
    "X65n1277_005:0022b02_0": "起幡",
    "X65n1277_005:0022b03_0": "神旛五綵竪長空。",
    "X65n1277_005:0022b03_1": "招信幽明冀感通。",
    "X65n1277_005:0022b03_2": "百尺竿頭騰彩鳳。",
    "X65n1277_005:0022b04_3": "九霄雲裏現飛龍。",
    "X65n1277_005:0022b04_4": "招搖六道歸真際。",
    "X65n1277_005:0022b04_5": "接引三途入正宗。",
    "X65n1277_005:0022b05_6": "戌亥二時伸召請。",
    "X65n1277_005:0022b05_7": "聖賢願降法筵中。",
    "X65n1277_005:0022b06_0": "放生",
    "X65n1277_005:0022b07_0": "羽鱗淨戒已周圓。",
    "X65n1277_005:0022b07_1": "法樂聲中破業昏。",
    "X65n1277_005:0022b07_2": "禽向碧空成鳳去。",
    "X65n1277_005:0022b08_3": "魚遊深澤化龍奔。",
    "X65n1277_005:0022b08_4": "翎毛齊刷歸丹穴。",
    "X65n1277_005:0022b08_5": "頭角輕搖透禹門。",
    "X65n1277_005:0022b09_6": "從此佛功資化育。",
    "X65n1277_005:0022b09_7": "他時福報信人恩。",
    "X65n1277_005:0022b11_0": "高峰龍泉院因師集賢語錄卷之五"
}