[
    {
        "source_sentence": "十天儀軌",
        "from": "T21n1296_001:0382c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東北方大自在天印",
        "from": "T21n1296_001:0382c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手作拳安腰右。左手五指直竪相著。地水",
        "from": "T21n1296_001:0382c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二指屈中節。",
        "from": "T21n1296_001:0382c27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火風空三指各少相去即成。",
        "from": "T21n1296_001:0382c27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1296_001:0382c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩麼三曼跢勃馱南唵伊去舍曩去曳娑嚩訶唵嚕捺囉耶娑婆訶東方帝釋天印右手同前。",
        "from": "T21n1296_001:0383a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手五指直竪相著。",
        "from": "T21n1296_001:0383a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地水二指屈中節。",
        "from": "T21n1296_001:0383a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以風著火背。",
        "from": "T21n1296_001:0383a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空屈中節。",
        "from": "T21n1296_001:0383a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1296_001:0383a04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵因捺囉耶娑嚩訶又唵斫羯也娑嚩呵東南方火天印左手作拳安腰左。",
        "from": "T21n1296_001:0383a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手五指直竪相著。",
        "from": "T21n1296_001:0383a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈空納著掌中。",
        "from": "T21n1296_001:0383a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風屈中節。",
        "from": "T21n1296_001:0383a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與娑婆訶相招。",
        "from": "T21n1296_001:0383a09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1296_001:0383a09_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿哦那曳娑婆訶南方焰摩天印先合掌二地屈中節背相著。",
        "from": "T21n1296_001:0383a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二風亦屈背著。",
        "from": "T21n1296_001:0383a12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大指押各風中節即成。",
        "from": "T21n1296_001:0383a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1296_001:0383a13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵焰摩耶娑嚩訶西南方羅剎天印右手作拳安腰。",
        "from": "T21n1296_001:0383a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手五指直竪相著。",
        "from": "T21n1296_001:0383a16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈地水中節。",
        "from": "T21n1296_001:0383a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以空端押地水甲即成。",
        "from": "T21n1296_001:0383a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1296_001:0383a17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵地哩底曳娑嚩訶西方水天印右手如前。",
        "from": "T21n1296_001:0383a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手掘拳。",
        "from": "T21n1296_001:0383a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空勿入掌中。",
        "from": "T21n1296_001:0383a20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風即直竪中節小屈即成。",
        "from": "T21n1296_001:0383a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1296_001:0383a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩嚕拏耶娑嚩訶西北方風天印右手如前。",
        "from": "T21n1296_001:0383a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手五指直竪相著。",
        "from": "T21n1296_001:0383a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地水屈中節即成。",
        "from": "T21n1296_001:0383a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1296_001:0383a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩耶吠微曳反娑嚩訶北方毘沙門天印先合掌十指內相叉。",
        "from": "T21n1296_001:0383a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二水竪頭相柱。",
        "from": "T21n1296_001:0383a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二風竪微屈。",
        "from": "T21n1296_001:0383a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左空入右掌中。",
        "from": "T21n1296_001:0383a29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "押左火甲。",
        "from": "T21n1296_001:0383a29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右空越左空背。",
        "from": "T21n1296_001:0383b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入左掌中押右火甲。",
        "from": "T21n1296_001:0383b01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二風與訶相招。",
        "from": "T21n1296_001:0383b01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1296_001:0383b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵吠室羅嚩拏耶娑嚩訶上方梵天印右手作拳安腰右。",
        "from": "T21n1296_001:0383b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手五指相著竪之小屈。",
        "from": "T21n1296_001:0383b05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其高少許過肩。",
        "from": "T21n1296_001:0383b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成蓮花相真言曰。",
        "from": "T21n1296_001:0383b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵沒羅二合含摩寧娑嚩訶下方地天印先合掌風以下四指頭相柱。",
        "from": "T21n1296_001:0383b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前方大開。",
        "from": "T21n1296_001:0383b09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二空各著風側。",
        "from": "T21n1296_001:0383b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1296_001:0383b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵畢哩體地以反微曳娑嚩訶日天印不異前地印。",
        "from": "T21n1296_001:0383b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但以二空頭各跓水下節。",
        "from": "T21n1296_001:0383b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1296_001:0383b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿去儞底也娑嚩訶月天印如前梵天印。",
        "from": "T21n1296_001:0383b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但作掌中有月潔白相。",
        "from": "T21n1296_001:0383b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1296_001:0383b17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵戰捺羅去也娑嚩訶七曜印先合掌十指相著。",
        "from": "T21n1296_001:0383b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二風與二空極相開。",
        "from": "T21n1296_001:0383b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1296_001:0383b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孽羅二合醯濕嚩二合哩耶鉢囉二合跛多而偷二合底羅摩耶娑嚩訶二十八宿印先虛心合掌。",
        "from": "T21n1296_001:0383b25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二火外相叉。",
        "from": "T21n1296_001:0383b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二空亦相叉。",
        "from": "T21n1296_001:0383b25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1296_001:0383b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諾乞灑二合怛羅二合涅寧去反蘇那儞曳娑嚩訶十天供儀軌",
        "from": "T21n1296_001:0383b28_100",
        "context_with_next_sentence": false
    }
]