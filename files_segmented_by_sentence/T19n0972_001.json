[
    {
        "source_sentence": "佛頂尊勝陀羅尼念誦儀軌法",
        "from": "T19n0972_001:0364b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一卷",
        "from": "T19n0972_001:0364b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T19n0972_001:0364b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空諡大鑒正號大廣智大興善寺三藏沙門不空奉　詔譯夫念誦陀羅尼法。",
        "from": "T19n0972_001:0364b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先於三昧耶曼荼羅。",
        "from": "T19n0972_001:0364b14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見聖眾得灌頂知本尊。",
        "from": "T19n0972_001:0364b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從師受得三昧耶。",
        "from": "T19n0972_001:0364b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於山間閑處或於淨室。",
        "from": "T19n0972_001:0364b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫本尊尊勝陀羅尼像。",
        "from": "T19n0972_001:0364b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安於東壁。",
        "from": "T19n0972_001:0364b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上持誦者以面對之。",
        "from": "T19n0972_001:0364b17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其念誦處掘地深一肘半。",
        "from": "T19n0972_001:0364b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地中若有瓦礫骨灰毛髮及諸穢物等。",
        "from": "T19n0972_001:0364b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並須除之。",
        "from": "T19n0972_001:0364b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無還取本土。",
        "from": "T19n0972_001:0364b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "填滿築令平正。",
        "from": "T19n0972_001:0364b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "土若有餘其地吉祥。",
        "from": "T19n0972_001:0364b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以瞿摩夷和好土。",
        "from": "T19n0972_001:0364b20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "泥地面令平正。",
        "from": "T19n0972_001:0364b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又取瞿摩夷和水。",
        "from": "T19n0972_001:0364b21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦無能勝陀羅尼二十一遍。",
        "from": "T19n0972_001:0364b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持瞿摩夷。",
        "from": "T19n0972_001:0364b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其無能勝真言曰。",
        "from": "T19n0972_001:0364b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼多沒入馱引喃引一唵引二虎魯虎魯三戰拏理四摩蹬儗五丁以反娑嚩二合引賀引加持已。",
        "from": "T19n0972_001:0364b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後從東北隅起首右旋塗之。",
        "from": "T19n0972_001:0364b27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次取蜀葵葉或蓮子葉。",
        "from": "T19n0972_001:0364b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "揩拭令其光淨。",
        "from": "T19n0972_001:0364b28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於上取白粉和水。",
        "from": "T19n0972_001:0364b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以繩分九位拼之。",
        "from": "T19n0972_001:0364b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "石上磨白檀香用塗九位。",
        "from": "T19n0972_001:0364c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其九位者。",
        "from": "T19n0972_001:0364c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中央安毘盧遮那佛位。",
        "from": "T19n0972_001:0364c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右邊安觀自在菩薩位。",
        "from": "T19n0972_001:0364c02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀自在後。",
        "from": "T19n0972_001:0364c02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安慈氏菩薩位。",
        "from": "T19n0972_001:0364c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘盧遮那佛位後。",
        "from": "T19n0972_001:0364c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安虛空藏菩薩位。",
        "from": "T19n0972_001:0364c03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此菩薩左邊。",
        "from": "T19n0972_001:0364c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安普賢菩薩位。",
        "from": "T19n0972_001:0364c04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘盧遮那佛位左邊。",
        "from": "T19n0972_001:0364c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安金剛手菩薩位。",
        "from": "T19n0972_001:0364c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛手菩薩位下。",
        "from": "T19n0972_001:0364c05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安文殊師利菩薩位。",
        "from": "T19n0972_001:0364c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘盧遮那佛前。",
        "from": "T19n0972_001:0364c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安除蓋障菩薩位。",
        "from": "T19n0972_001:0364c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除蓋障菩薩位右邊。",
        "from": "T19n0972_001:0364c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安地藏菩薩位。",
        "from": "T19n0972_001:0364c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名九位。",
        "from": "T19n0972_001:0364c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並用白檀香塗之。",
        "from": "T19n0972_001:0364c08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為迎請賢聖之位。",
        "from": "T19n0972_001:0364c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耳上安帳蓋四面懸幡。",
        "from": "T19n0972_001:0364c09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道場四邊。",
        "from": "T19n0972_001:0364c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晨朝奉獻乳糜。",
        "from": "T19n0972_001:0364c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "齋時獻酪飯并甜膩食。",
        "from": "T19n0972_001:0364c10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及以諸漿兼諸果子。",
        "from": "T19n0972_001:0364c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四門安四香爐。",
        "from": "T19n0972_001:0364c11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四隅安四淨瓶。",
        "from": "T19n0972_001:0364c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盛香水插花或青葉樹枝。",
        "from": "T19n0972_001:0364c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為供養。",
        "from": "T19n0972_001:0364c12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四角燃四盞酥燈。",
        "from": "T19n0972_001:0364c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道場前於念誦者座前。",
        "from": "T19n0972_001:0364c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安置閼伽香水兩椀。",
        "from": "T19n0972_001:0364c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所盛供養。",
        "from": "T19n0972_001:0364c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取金銀熟銅瓷器或新瓦器或螺盃或新淨葉。",
        "from": "T19n0972_001:0364c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘並不堪。",
        "from": "T19n0972_001:0364c15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲盛食時先淨洗器。",
        "from": "T19n0972_001:0364c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覆之以香煙熏內。",
        "from": "T19n0972_001:0364c16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既盛食已又須香烟熏之。",
        "from": "T19n0972_001:0364c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以無能勝陀羅尼加持水灑之。",
        "from": "T19n0972_001:0364c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則於壇中右旋布列。",
        "from": "T19n0972_001:0364c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後於壇前安卑脚床子。",
        "from": "T19n0972_001:0364c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "去地半寸。",
        "from": "T19n0972_001:0364c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或茅草薦或藉以淨物。",
        "from": "T19n0972_001:0364c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦者坐之。",
        "from": "T19n0972_001:0364c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦人應淨澡浴。",
        "from": "T19n0972_001:0364c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "澡浴法如蘇悉地中說。",
        "from": "T19n0972_001:0364c21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或以法澡浴。",
        "from": "T19n0972_001:0364c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀法實相以為澡浴。",
        "from": "T19n0972_001:0364c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或以在家出家護持本律儀戒無所毀犯。",
        "from": "T19n0972_001:0364c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為澡浴。",
        "from": "T19n0972_001:0364c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或每日三時佛前禮佛發露懺悔隨喜勸請發願迴向。",
        "from": "T19n0972_001:0364c24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為澡浴。",
        "from": "T19n0972_001:0364c24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或以清淨真言加持七遍。",
        "from": "T19n0972_001:0365a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為清淨。",
        "from": "T19n0972_001:0365a01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "澡浴正誦之時觀一切法本性清淨我亦得清淨。",
        "from": "T19n0972_001:0365a02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是作意即誦澡浴真言曰。",
        "from": "T19n0972_001:0365a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引娑嚩二合婆引嚩󰡊馱引薩嚩達摩入娑嚩二婆引嚩戍引度憾每日入道場念誦。",
        "from": "T19n0972_001:0365a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定其時節。",
        "from": "T19n0972_001:0365a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或二時謂早朝黃昏。",
        "from": "T19n0972_001:0365a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或三時加午時。",
        "from": "T19n0972_001:0365a07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或四時依瑜伽加中夜。",
        "from": "T19n0972_001:0365a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於本教尊勝陀羅尼經。",
        "from": "T19n0972_001:0365a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每於白月十五日。",
        "from": "T19n0972_001:0365a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除滅業障增福延命。",
        "from": "T19n0972_001:0365a09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要期誦一千遍。",
        "from": "T19n0972_001:0365a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證出世三摩地得不忘陀羅尼。",
        "from": "T19n0972_001:0365a10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其日一日一夜不食為上。",
        "from": "T19n0972_001:0365a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或食三白食。",
        "from": "T19n0972_001:0365a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂乳酪粳米飯。",
        "from": "T19n0972_001:0365a11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或粥為中。",
        "from": "T19n0972_001:0365a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或如常齋食為下。",
        "from": "T19n0972_001:0365a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身著淨衣服。",
        "from": "T19n0972_001:0365a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心應虔誠淨信。",
        "from": "T19n0972_001:0365a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心不猶豫。",
        "from": "T19n0972_001:0365a13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲持誦。",
        "from": "T19n0972_001:0365a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至道場先雙膝著地。",
        "from": "T19n0972_001:0365a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮毘盧遮那佛及八大菩薩。",
        "from": "T19n0972_001:0365a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發露懺悔發五大願。",
        "from": "T19n0972_001:0365a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一眾生無邊誓願度。",
        "from": "T19n0972_001:0365a15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二福智無邊誓願集。",
        "from": "T19n0972_001:0365a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三法門無邊誓願學。",
        "from": "T19n0972_001:0365a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四如來無邊誓願事。",
        "from": "T19n0972_001:0365a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五無上菩提誓願成。",
        "from": "T19n0972_001:0365a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結加趺坐以香塗手結三昧耶印誦真言曰。",
        "from": "T19n0972_001:0365a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼多沒馱喃一阿三冥二底哩二合三冥三三昧曳娑嚩二合引賀引其印。",
        "from": "T19n0972_001:0365a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手合掌竪二大指印。",
        "from": "T19n0972_001:0365a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於五處。",
        "from": "T19n0972_001:0365a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "額右肩左肩心喉。",
        "from": "T19n0972_001:0365a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各誦真言一遍加持頂上散。",
        "from": "T19n0972_001:0365a22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由三昧耶印真言威神力故。",
        "from": "T19n0972_001:0365a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能淨如來地地波羅蜜圓滿。",
        "from": "T19n0972_001:0365a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能成就世間出世間悉地。",
        "from": "T19n0972_001:0365a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結法界生印誦真言曰。",
        "from": "T19n0972_001:0365a25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼多沒馱引南一達囉麼二合馱引賭二娑嚩二合娑嚩二合句憾結印相。",
        "from": "T19n0972_001:0365a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手大指安於掌中。",
        "from": "T19n0972_001:0365a28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各作拳。",
        "from": "T19n0972_001:0365a28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二頭指側並合。",
        "from": "T19n0972_001:0365a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安於頂上。",
        "from": "T19n0972_001:0365a29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從頂向下徐徐下散。",
        "from": "T19n0972_001:0365b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言三遍。",
        "from": "T19n0972_001:0365b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即觀自身等同法界。",
        "from": "T19n0972_001:0365b01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離諸色相猶如虛空。",
        "from": "T19n0972_001:0365b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結金剛薩埵法輪印誦真言曰。",
        "from": "T19n0972_001:0365b03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼多嚩日囉二合喃引唵引嚩日囉二合引怛摩二合句引憾結印相。",
        "from": "T19n0972_001:0365b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手背相叉。",
        "from": "T19n0972_001:0365b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左大指安右手掌中。",
        "from": "T19n0972_001:0365b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與右手大指相拄即誦真言三遍。",
        "from": "T19n0972_001:0365b07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲得自身如金剛薩埵。",
        "from": "T19n0972_001:0365b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結金剛甲冑印真言曰。",
        "from": "T19n0972_001:0365b09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼多嚩日囉二合喃引唵引嚩日囉二合迦嚩者吽其印相。",
        "from": "T19n0972_001:0365b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手虛心合掌。",
        "from": "T19n0972_001:0365b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指各安於中指背。",
        "from": "T19n0972_001:0365b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指並合。",
        "from": "T19n0972_001:0365b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中指中節上成印。",
        "from": "T19n0972_001:0365b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同前五處各誦真言一遍。",
        "from": "T19n0972_001:0365b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此加持自身成被金剛甲冑。",
        "from": "T19n0972_001:0365b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切天魔無能親近。",
        "from": "T19n0972_001:0365b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結不動尊印真言曰。",
        "from": "T19n0972_001:0365b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼多嚩日囉二合喃一戰拏摩賀嚧沙拏二句薩叵二合吒耶三吽怛囉二合吒憾引𤚥引結印二手各以大指。",
        "from": "T19n0972_001:0365b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捻小指及無名指甲上。",
        "from": "T19n0972_001:0365b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各竪中指頭指並之。",
        "from": "T19n0972_001:0365b21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手為鞘右手為刀。",
        "from": "T19n0972_001:0365b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以刀入鞘狀右刀印左旋轉辟除道場中諸魔作障者。",
        "from": "T19n0972_001:0365b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋轉八方結上下方隅界。",
        "from": "T19n0972_001:0365b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後想道場中須彌山。",
        "from": "T19n0972_001:0365b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於山頂上想七寶樓閣。",
        "from": "T19n0972_001:0365b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於樓閣中。",
        "from": "T19n0972_001:0365b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘盧遮那佛與八十俱胝十地滿足菩薩摩訶薩。",
        "from": "T19n0972_001:0365b26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為眷屬而自圍遶。",
        "from": "T19n0972_001:0365b26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四門四隅各四菩薩及八供養。",
        "from": "T19n0972_001:0365b27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以內外供養。",
        "from": "T19n0972_001:0365b27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀想奉獻。",
        "from": "T19n0972_001:0365b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了了分明。",
        "from": "T19n0972_001:0365b28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結奉請聖眾如來鉤印誦真言曰。",
        "from": "T19n0972_001:0365b29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫上三去滿馱沒馱南引一噁二引薩嚩怛囉二合鉢囉二合底呵諦怛他引蘖當矩捨三󰂛地者囉哩耶二合跛哩布引囉迦四娑嚩二合引賀引結相二手內相叉作舉。",
        "from": "T19n0972_001:0366a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "申右手頭指。",
        "from": "T19n0972_001:0366a01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈如鉤形。",
        "from": "T19n0972_001:0366a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦三遍由印真言威神力故。",
        "from": "T19n0972_001:0366a02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛如來及以聖眾。",
        "from": "T19n0972_001:0366a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不違本誓皆悉集會。",
        "from": "T19n0972_001:0366a03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結奉獻座印誦真言曰。",
        "from": "T19n0972_001:0366a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼多沒馱喃引惡引急呼契相二手虛心合掌。",
        "from": "T19n0972_001:0366a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二小指二大指相合。",
        "from": "T19n0972_001:0366a06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘六指開舒微屈如蓮華敷。",
        "from": "T19n0972_001:0366a07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦三遍。",
        "from": "T19n0972_001:0366a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此印及真言威力。",
        "from": "T19n0972_001:0366a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流出一切微妙寶座。",
        "from": "T19n0972_001:0366a08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如雲海。",
        "from": "T19n0972_001:0366a08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉獻如來一切聖眾。",
        "from": "T19n0972_001:0366a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次奉獻閼伽誦真言曰。",
        "from": "T19n0972_001:0366a10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼多沒馱南一誐誐曩三摩三摩娑嚩二合賀引即以兩手捧器當額。",
        "from": "T19n0972_001:0366a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "䠒跪誦三遍。",
        "from": "T19n0972_001:0366a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉獻閼伽沐浴聖眾。",
        "from": "T19n0972_001:0366a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結奉獻塗香印誦真言曰。",
        "from": "T19n0972_001:0366a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫上三去曼多沒馱南引一微輸上馱巘引度上納婆嚩引耶娑嚩二合引賀契相右手竪掌向外。",
        "from": "T19n0972_001:0366a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左手握右手腕。",
        "from": "T19n0972_001:0366a18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦三遍由此印真言威力故。",
        "from": "T19n0972_001:0366a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流出一切塗香雲海。",
        "from": "T19n0972_001:0366a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養一切如來及諸聖眾。",
        "from": "T19n0972_001:0366a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結奉獻花鬘印誦真言曰。",
        "from": "T19n0972_001:0366a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼多沒馱南引摩賀引昧怛哩夜三合毘庾二合那蘖帝娑嚩二合賀契相二手。",
        "from": "T19n0972_001:0366a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內相叉仰掌。",
        "from": "T19n0972_001:0366a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭指相拄誦三遍。",
        "from": "T19n0972_001:0366a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此印及真言威力。",
        "from": "T19n0972_001:0366a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流出一切花鬘雲海。",
        "from": "T19n0972_001:0366a25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養一切如來聖眾。",
        "from": "T19n0972_001:0366a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結奉獻燒香印真言曰。",
        "from": "T19n0972_001:0366a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼多沒馱引南一達麼馱引怛嚩二合引拏鼻音蘖帝引娑嚩二合賀契相二手仰掌。",
        "from": "T19n0972_001:0366b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以小指中指無名指屈竪相背。",
        "from": "T19n0972_001:0366b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦三遍。",
        "from": "T19n0972_001:0366b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此印真言威力。",
        "from": "T19n0972_001:0366b02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流出一切燒香雲海。",
        "from": "T19n0972_001:0366b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養一切如來及諸聖眾。",
        "from": "T19n0972_001:0366b03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結奉獻飲食印誦真言曰。",
        "from": "T19n0972_001:0366b04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼多沒馱南引阿囉囉迦囉沫隣捺埿摩賀引沫履娑嚩二合賀契相虛心密令開掌如器。",
        "from": "T19n0972_001:0366b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦三遍。",
        "from": "T19n0972_001:0366b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此印真言威力。",
        "from": "T19n0972_001:0366b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流出無邊飲食雲海。",
        "from": "T19n0972_001:0366b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養一切如來及諸聖眾。",
        "from": "T19n0972_001:0366b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結奉獻燈明印誦真言曰。",
        "from": "T19n0972_001:0366b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼多沒馱引南引怛他引󰋲怛紫娑叵二合羅寧二嚩婆去娑娜三誐誐猱娜哩野二合娑嚩二合賀契相右手作拳竪中指。",
        "from": "T19n0972_001:0366b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大指捻中指中節。",
        "from": "T19n0972_001:0366b14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦三遍。",
        "from": "T19n0972_001:0366b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此印真言威力。",
        "from": "T19n0972_001:0366b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流出一切燈明雲海。",
        "from": "T19n0972_001:0366b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養一切如來及諸聖眾。",
        "from": "T19n0972_001:0366b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復如來大乘經所說。",
        "from": "T19n0972_001:0366b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當觀想幢蓋幡網瓔珞衣服繒綵等物諸供養雲海充遍法界誠實言伽他而讚曰。",
        "from": "T19n0972_001:0366b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以我功德力　　如來加持力",
        "from": "T19n0972_001:0366b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及以法界力　　普供養而住",
        "from": "T19n0972_001:0366b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結虛空藏明妃印真言曰。",
        "from": "T19n0972_001:0366b22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼多沒馱引南引薩嚩他欠烏娜蘖二合帝娑叵囉呬引𤚥誐誐劍娑嚩二合引賀引其契相二手相叉合掌右押左。",
        "from": "T19n0972_001:0366b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦七遍。",
        "from": "T19n0972_001:0366b26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次想自身於心中有圓明月輪。",
        "from": "T19n0972_001:0366b27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了了分明。",
        "from": "T19n0972_001:0366b27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於月輪上想。",
        "from": "T19n0972_001:0366b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欠字。",
        "from": "T19n0972_001:0366b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白色放大光明。",
        "from": "T19n0972_001:0366b29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍照十方一切世界。",
        "from": "T19n0972_001:0366b29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思欠字實相義。",
        "from": "T19n0972_001:0366c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂一切法等同虛空。",
        "from": "T19n0972_001:0366c01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離諸色相離諸障礙。",
        "from": "T19n0972_001:0366c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則於真實理中。",
        "from": "T19n0972_001:0366c02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀自身作金剛波羅蜜佛母菩薩像。",
        "from": "T19n0972_001:0366c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持蓮花。",
        "from": "T19n0972_001:0366c03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其蓮花上有五股金剛杵。",
        "from": "T19n0972_001:0367a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手作仰掌。",
        "from": "T19n0972_001:0367a01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "垂手為施願勢。",
        "from": "T19n0972_001:0367a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具頭冠瓔珞。",
        "from": "T19n0972_001:0367a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面貌慈愍拔濟一切眾生勢。",
        "from": "T19n0972_001:0367a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是觀已。",
        "from": "T19n0972_001:0367a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手外相叉作拳。",
        "from": "T19n0972_001:0367a03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二中指。",
        "from": "T19n0972_001:0367a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此金剛波羅蜜印。",
        "from": "T19n0972_001:0367a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持印於四處。",
        "from": "T19n0972_001:0367a04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂印心額喉頂。",
        "from": "T19n0972_001:0367a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印已便散手誦金剛波羅蜜真言曰。",
        "from": "T19n0972_001:0367a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩怛嚩二合嚩日哩二合吽當印四處時。",
        "from": "T19n0972_001:0367a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各誦一遍。",
        "from": "T19n0972_001:0367a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即用前印安於額上誦三遍。",
        "from": "T19n0972_001:0367a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為灌頂灌頂已。",
        "from": "T19n0972_001:0367a09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其印手分兩邊。",
        "from": "T19n0972_001:0367a09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以金剛拳繼頂後。",
        "from": "T19n0972_001:0367a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言曰。",
        "from": "T19n0972_001:0367a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉二合引鼻詵遮誦三遍。",
        "from": "T19n0972_001:0367a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結被甲印。",
        "from": "T19n0972_001:0367a12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手各作金剛拳。",
        "from": "T19n0972_001:0367a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各竪頭指當心相纏。",
        "from": "T19n0972_001:0367a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如繼甲冑。",
        "from": "T19n0972_001:0367a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於背後亦復如是。",
        "from": "T19n0972_001:0367a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次臍次兩膝。",
        "from": "T19n0972_001:0367a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "却繞腰後漸當心。",
        "from": "T19n0972_001:0367a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次兩肩亦如是繼。",
        "from": "T19n0972_001:0367a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次繼頂下項後額前。",
        "from": "T19n0972_001:0367a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "却於腦後以兩金剛拳。",
        "from": "T19n0972_001:0367a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繼漸垂手兩邊徐徐下。",
        "from": "T19n0972_001:0367a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如垂帶勢。",
        "from": "T19n0972_001:0367a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以兩手掌相拍三聲。",
        "from": "T19n0972_001:0367a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦被甲真言曰。",
        "from": "T19n0972_001:0367a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引砧誅唵反又拍掌真言曰。",
        "from": "T19n0972_001:0367a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉二合覩史耶二合縠自想己身成本尊已。",
        "from": "T19n0972_001:0367a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手合掌屈二頭指甲相背。",
        "from": "T19n0972_001:0367a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大指壓二頭指頭。",
        "from": "T19n0972_001:0367a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如彈指勢。",
        "from": "T19n0972_001:0367a23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦尊勝陀羅尼曰。",
        "from": "T19n0972_001:0367a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩慕引婆誐嚩帝一怛󰪯二合路引枳也二合二鉢囉二合底丁以反三尾始瑟吒二合引也四勃馱引耶五婆誐嚩帝六怛儞也二合他引七唵八尾戍引馱也九三麼三滿多嚩婆娑十薩頗二合囉拏十一孽底丁異反誐賀曩十二薩囉二合婆引嚩󰡊輸律反下並同第十三阿鼻詵左𤚥十四引素誐多嚩囉嚩左曩十五蜜㗚二合多引鼻曬罽十六入阿引賀囉阿引上賀囉十七阿引入欲散馱引囉抳十八戌引馱也戍馱也十九誐誐曩尾󰡊提二十鄔瑟抳三合沙二十尾惹也尾󰡊提二十二娑賀娑囉二合二十三囉濕弭二合散祖引禰帝二十四薩嚩怛他引孽多引二十五地瑟侘引二合曩引地瑟恥二合多二十六畆捺㘑二合二十七嚩日囉二合迦引耶土二十八僧去賀怛那󰡊第二十九薩嚩引嚩囉拏尾󰡊第三十鉢囉二合底儞靺多也三十一阿引欲󰡊第三十二三麼耶引地瑟恥二合帝三十三麼抳麼抳三十四怛闥多引步多俱胝三十五跛哩󰡊第三十六尾娑怖二合吒三十七勃地󰡊第三十八惹也惹也三十九尾惹也尾惹也四十娑麼二合囉娑麼二合囉四十一薩嚩勃馱引地瑟恥二合多󰡊第四十二嚩日㘑二合嚩日囉二合孽鞞四十三嚩日囕二合婆嚩覩四十四麼麼四十五某甲薩嚩薩怛嚩二合引四十六難上引左迦上引也尾󰡊第四十七薩嚩孽底四十八跛哩󰡊第四十九薩嚩怛他引孽多五十三摩引濕嚩二合引娑引地瑟恥二合帝五十一勃𠆙勃𠆙五十二󰂛引馱也冒引馱也五十三三滿多跛哩󰡊第五十四薩嚩怛他引孽多引五十五地瑟姹二合引曩引五十六地瑟恥二合多五十七摩賀引畝捺㘑二合五十八娑嚩二合引賀引五十九所誦之聲。",
        "from": "T19n0972_001:0367b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不高不下不緩不急。",
        "from": "T19n0972_001:0367b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心緣觀毘盧遮那佛。",
        "from": "T19n0972_001:0367c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了了分明。",
        "from": "T19n0972_001:0367c01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦七遍已印頂上散。",
        "from": "T19n0972_001:0367c01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取菩提子念珠安於掌中。",
        "from": "T19n0972_001:0367c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦加持念珠真言曰。",
        "from": "T19n0972_001:0367c02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵尾嚧遮那麼羅娑嚩二合引賀引誦七遍已。",
        "from": "T19n0972_001:0367c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安頂上以左手。",
        "from": "T19n0972_001:0367c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當心。",
        "from": "T19n0972_001:0367c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "承珠。",
        "from": "T19n0972_001:0367c04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手移珠。",
        "from": "T19n0972_001:0367c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每與娑嚩二合賀齊聲移一珠。",
        "from": "T19n0972_001:0367c05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦至一百八遍。",
        "from": "T19n0972_001:0368a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至一千遍。",
        "from": "T19n0972_001:0368a01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "却取珠蟠於掌中。",
        "from": "T19n0972_001:0368a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌安於頂上。",
        "from": "T19n0972_001:0368a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而發所求清淨妙願。",
        "from": "T19n0972_001:0368a02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願一切眾生皆悉獲得。",
        "from": "T19n0972_001:0368a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復結本尊印。",
        "from": "T19n0972_001:0368a03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦七遍或三遍。",
        "from": "T19n0972_001:0368a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復結金剛波羅蜜印。",
        "from": "T19n0972_001:0368a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復結五種供養印。",
        "from": "T19n0972_001:0368a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并誦五種真言而供養之。",
        "from": "T19n0972_001:0368a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具如上說。",
        "from": "T19n0972_001:0368a05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結不動尊印誦一遍。",
        "from": "T19n0972_001:0368a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印左轉即成解界。",
        "from": "T19n0972_001:0368a06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執閼伽器奉獻供養。",
        "from": "T19n0972_001:0368a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結前三昧耶印。",
        "from": "T19n0972_001:0368a07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當頂上奉送。",
        "from": "T19n0972_001:0368a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦三昧耶真言一遍。",
        "from": "T19n0972_001:0368a08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不解此印便誦金剛解脫真言曰。",
        "from": "T19n0972_001:0368a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉二合謨引訖叉二合穆奉送諸佛聖眾。",
        "from": "T19n0972_001:0368a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前禮拜發露懺悔隨喜勸請發願迴向已。",
        "from": "T19n0972_001:0368a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出道場。",
        "from": "T19n0972_001:0368a12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後即於靜處。",
        "from": "T19n0972_001:0368a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉讀大乘經。",
        "from": "T19n0972_001:0368a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀第一義諦。",
        "from": "T19n0972_001:0368a13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此妙福迴向所求。",
        "from": "T19n0972_001:0368a13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "助成悉地。",
        "from": "T19n0972_001:0368a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作息災法。",
        "from": "T19n0972_001:0368a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面向北其壇圓。",
        "from": "T19n0972_001:0368a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀聖眾白色。",
        "from": "T19n0972_001:0368a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道場中所供養物皆白。",
        "from": "T19n0972_001:0368a15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身著白衣面向北坐。",
        "from": "T19n0972_001:0368a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒沈水香若作增長法。",
        "from": "T19n0972_001:0368a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面向東坐。",
        "from": "T19n0972_001:0368a16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本尊及供養并自身衣服悉皆黃色。",
        "from": "T19n0972_001:0368a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒白壇香若作降伏法。",
        "from": "T19n0972_001:0368a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面向南坐。",
        "from": "T19n0972_001:0368a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本尊及供養并衣服並青色或黑色。",
        "from": "T19n0972_001:0368a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒安息香。",
        "from": "T19n0972_001:0368a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作敬愛法。",
        "from": "T19n0972_001:0368a19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面向西坐。",
        "from": "T19n0972_001:0368a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀本尊赤色。",
        "from": "T19n0972_001:0368a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及飲食衣服皆赤。",
        "from": "T19n0972_001:0368a20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒酥合香。",
        "from": "T19n0972_001:0368a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛頂尊勝陀羅尼念誦儀軌法一卷",
        "from": "T19n0972_001:0368a22_100",
        "context_with_next_sentence": false
    }
]