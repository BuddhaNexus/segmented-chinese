[
    {
        "source_sentence": "何耶揭唎婆觀世音菩薩受法",
        "from": "T20n1074_001:0171c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壇",
        "from": "T20n1074_001:0171c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有沙門。若婆羅門諸善男子善女人等。欲",
        "from": "T20n1074_001:0171c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持菩薩法者。",
        "from": "T20n1074_001:0172a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應作四肘壇。",
        "from": "T20n1074_001:0172a01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當覓勝地清淨之所掃灑清淨。",
        "from": "T20n1074_001:0172a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以香水牛糞埿地。",
        "from": "T20n1074_001:0172a02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懸於種種雜色幡蓋寶鈴珮鏡。",
        "from": "T20n1074_001:0172a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并諸金銀種種間錯嚴錺道場。",
        "from": "T20n1074_001:0172a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其道場中立五色壇。",
        "from": "T20n1074_001:0172a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縱廣四肘。",
        "from": "T20n1074_001:0172a04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先下白色次黃次赤次黑。",
        "from": "T20n1074_001:0172a05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而作四門。",
        "from": "T20n1074_001:0172a05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其壇中心作蓮華座。",
        "from": "T20n1074_001:0172a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安置何耶揭利婆觀世音像。",
        "from": "T20n1074_001:0172a06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東門作華座安十一面菩薩。",
        "from": "T20n1074_001:0172a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北門作蓮華座安八臂觀世音。",
        "from": "T20n1074_001:0172a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南方安八龍王。",
        "from": "T20n1074_001:0172a08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難陀龍王。",
        "from": "T20n1074_001:0172a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婆素難龍王。",
        "from": "T20n1074_001:0172a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "德叉迦龍王。",
        "from": "T20n1074_001:0172a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羯固奼龍王。",
        "from": "T20n1074_001:0172a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "般摩龍王。",
        "from": "T20n1074_001:0172a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩訶般摩龍王。",
        "from": "T20n1074_001:0172a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "商佉波羅龍王。",
        "from": "T20n1074_001:0172a10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳩利迦龍王八龍唯供粳米乳糜。",
        "from": "T20n1074_001:0172a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘者以種種供養亦得燃四十五燈。",
        "from": "T20n1074_001:0172a11_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先喚八箇龍王用何耶揭唎婆身印來去。",
        "from": "T20n1074_001:0172a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵闍去音耶毘闍去音耶二阿瑟吒二合那去聲伽羅闍耶三阿羅闡都四莎訶五其壇西門近。",
        "from": "T20n1074_001:0172a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南安一火爐以胡麻稻穀華等蘇蜜相和。",
        "from": "T20n1074_001:0172a15_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦心呪投火中燒之。",
        "from": "T20n1074_001:0172a15_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿一千八遍次請中心何耶揭唎婆菩薩。",
        "from": "T20n1074_001:0172a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次請八臂觀音身印來去。",
        "from": "T20n1074_001:0172a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一夜勢夜輒擔二莎訶次用華座印來去。",
        "from": "T20n1074_001:0172a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請十一面菩薩已。",
        "from": "T20n1074_001:0172a19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散種種花燒諸名香。",
        "from": "T20n1074_001:0172a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沈水薰陸栴檀香等而供養之。",
        "from": "T20n1074_001:0172a20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪師一日不食。",
        "from": "T20n1074_001:0172a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不忍飢唯得食蘇。",
        "from": "T20n1074_001:0172a21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香湯洗浴著新淨衣。",
        "from": "T20n1074_001:0172a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入道場中而作供養。",
        "from": "T20n1074_001:0172a22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常得觀世音菩薩憶念。",
        "from": "T20n1074_001:0172a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦兼通軍荼利法結界供養也。",
        "from": "T20n1074_001:0172a23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當以真言印護身及結界。",
        "from": "T20n1074_001:0172a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪木七返或呪水七返。",
        "from": "T20n1074_001:0172a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以印呪水。",
        "from": "T20n1074_001:0172a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或呪白芥子灰等七返。",
        "from": "T20n1074_001:0172a25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將木竪四角為界。",
        "from": "T20n1074_001:0172a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "芥子灰水散於十方閒成結界。",
        "from": "T20n1074_001:0172a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手外縛二食指直竪相去五分許。",
        "from": "T20n1074_001:0172a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並二大指相著。",
        "from": "T20n1074_001:0172a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各屈一節勿著食指。",
        "from": "T20n1074_001:0172a28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1074_001:0172a28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一鉢羅毘迦悉跢二跋折羅三涉筏二合囉囉上音支四莎訶何耶揭唎婆解禁刀印舒二手左手在外右手在內。",
        "from": "T20n1074_001:0172b02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左掌著右手背。",
        "from": "T20n1074_001:0172b02_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二手大指。",
        "from": "T20n1074_001:0172b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一阿娑忙義儞二莎呵三句何耶揭唎婆大法身印二手外縛以二小指竪相合。",
        "from": "T20n1074_001:0172b05_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並二大指竪相著屈。",
        "from": "T20n1074_001:0172b05_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怒大指來去。",
        "from": "T20n1074_001:0172b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一杜那杜那二摩他摩他三可馱可馱四訶耶揭唎婆五嗚吽泮六莎去音訶七次何耶揭唎婆心印二手外縛並二大指相著。",
        "from": "T20n1074_001:0172b09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各屈一節勿著食指。",
        "from": "T20n1074_001:0172b09_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指來去。",
        "from": "T20n1074_001:0172b09_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一阿蜜唎都智二合婆平音二嗚𤙖泮三莎去音訶次頭印二手外縛立二食指。",
        "from": "T20n1074_001:0172b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭相拄各屈二大指節。",
        "from": "T20n1074_001:0172b12_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小指頭尖也。",
        "from": "T20n1074_001:0172b12_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "跢姪他一斫迦羅𭈖怖二斫迦羅去音乞叉𭈖怖三斫迦羅跛曇摩叉唎怖四阿謨迦去音寫稱彼人名五始羅上音枳羅上音二合六跛羅舍網伽車上音覩七莎訶八叉去沙反餘處皆然次頂印準前頭印。",
        "from": "T20n1074_001:0172b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯改竪二食指頭相著。",
        "from": "T20n1074_001:0172b17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "列二中指在二食指前頭相拄。",
        "from": "T20n1074_001:0172b17_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一室唎二合羅唎至二嗚𤙖泮三莎去訶四次口印二手小指無名指內相叉二中指頭相拄。",
        "from": "T20n1074_001:0172b19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並竪二大指。",
        "from": "T20n1074_001:0172b19_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右風捻右大頭。",
        "from": "T20n1074_001:0172b19_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左頭指竪小曲在中指邊。",
        "from": "T20n1074_001:0172b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一鶻嚕嚧二鳩嚧馱那三嗚𤙖泮四莎訶次牙印準前觀世音不空羂索菩薩牙印。",
        "from": "T20n1074_001:0172b22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上準改二中指直竪頭相著。",
        "from": "T20n1074_001:0172b22_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指並竪博中指。",
        "from": "T20n1074_001:0172b22_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以左小指握右無名指背後。",
        "from": "T20n1074_001:0172b23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右小指握左小指背。",
        "from": "T20n1074_001:0172b23_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南上音謨上囉跢那二合怛囉二合夜耶一南謨阿唎耶二婆盧吉帝三攝婆二合羅去耶四菩提薩埵去耶五摩訶薩埵去耶摩訶迦盧尼迦耶七跢姪他八跢羅上二合吒九跢羅上二合吒十末吒末吒十一瞋陀瞋陀十二頻陀頻陀十三嗚𤙖十四泮泮十五莎訶何耶揭唎婆觀世音菩薩大呪。",
        "from": "T20n1074_001:0172c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又何耶揭唎婆別大呪在別是二呪者波帝吒悉陀波泮底唐云隨誦成驗雖不受持壇供養法隨誦成驗。",
        "from": "T20n1074_001:0172c04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲發遣何耶揭唎婆觀世音者。",
        "from": "T20n1074_001:0172c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手執素羅毘布瑟波二合反又云柏花呪一遍已。",
        "from": "T20n1074_001:0172c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一打像上。",
        "from": "T20n1074_001:0172c06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是七遍。",
        "from": "T20n1074_001:0172c06_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即歸去也真言曰。",
        "from": "T20n1074_001:0172c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "跢姪他一薩羅波羅薩羅二社耶毘社耶三跋折羅句羅娑三婆去音皤四莎跛那迦車目都起六莎去訶七",
        "from": "T20n1074_001:0172c10_100",
        "context_with_next_sentence": false
    }
]