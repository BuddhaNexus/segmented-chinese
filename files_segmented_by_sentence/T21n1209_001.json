[
    {
        "source_sentence": "金剛頂瑜伽降三世成就極深",
        "from": "T21n1209_001:0039b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密門一卷",
        "from": "T21n1209_001:0039b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T21n1209_001:0039b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命聖主宰　　普賢金剛手",
        "from": "T21n1209_001:0039b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為降伏一切　　現吽迦囉身",
        "from": "T21n1209_001:0039c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摧三世有毒　　令即證菩提",
        "from": "T21n1209_001:0039c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是甚深祕密　　降三世瑜伽",
        "from": "T21n1209_001:0039c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "首依真實王　　禮淨合縛摧",
        "from": "T21n1209_001:0039c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入閉普賢印　　按心稱本誓",
        "from": "T21n1209_001:0039c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發言身即同　　降三世金剛",
        "from": "T21n1209_001:0039c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四印加寶灌　　鬘甲二擬繫",
        "from": "T21n1209_001:0039c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拍掌同金剛　　即入三摩地",
        "from": "T21n1209_001:0039c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諦觀心阿字　　成月或日輪",
        "from": "T21n1209_001:0039c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中生五智杵　　次請尊及佛",
        "from": "T21n1209_001:0039c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入加三昧身　　又以本教中",
        "from": "T21n1209_001:0039c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四印加已滿　　五寶鬘甲拍",
        "from": "T21n1209_001:0039c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陳內外供養　　讚詠已念誦",
        "from": "T21n1209_001:0039c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作圓具法　　以五相成身",
        "from": "T21n1209_001:0039c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又普悅本誓　　忿四印五佛",
        "from": "T21n1209_001:0039c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶鬘甲拍等　　以吽迦囉印",
        "from": "T21n1209_001:0039c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法以火如蓮　　業建火逼風",
        "from": "T21n1209_001:0039c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "額喉頂四印　　五佛大日用",
        "from": "T21n1209_001:0039c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真實教王中　　金剛界自在",
        "from": "T21n1209_001:0039c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四佛同四印　　金剛佛真言",
        "from": "T21n1209_001:0040a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶用金剛寶餘三准此　　鬘甲印准前",
        "from": "T21n1209_001:0040a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬘真言曰。",
        "from": "T21n1209_001:0040a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵𮧬日囉二合莽㘑儞哩二合茶去被甲誦此明。",
        "from": "T21n1209_001:0040a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四攝八供養　　同金剛界儀",
        "from": "T21n1209_001:0040a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯現於擬叱鉤真言曰。",
        "from": "T21n1209_001:0040a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵𮧬日𠻴二合句捨麼賀句嚕二合馱羯灑耶沙去莽鹽引吽惹入索真言曰。",
        "from": "T21n1209_001:0040a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵𮧬日囉二合跛捨麼賀句嚕二合馱羯灑耶娑去莽鹽吽吽鎖真言曰。",
        "from": "T21n1209_001:0040a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵𮧬日囉二合寒普二合吒麼賀句嚕二合馱羯灑耶娑去莽鹽鑁鈴真言。",
        "from": "T21n1209_001:0040a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵𮧬日囉二合吠捨麼賀句嚕二合馱羯灑耶娑去莾鹽斛嬉戲真言曰。",
        "from": "T21n1209_001:0040a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又前觀智身　　自心流婀字",
        "from": "T21n1209_001:0040b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面前成月輪　　即出生金剛",
        "from": "T21n1209_001:0040b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五峯流猛焰　　生三悅我是",
        "from": "T21n1209_001:0040b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又丈於世尊　　即召滿空佛",
        "from": "T21n1209_001:0040b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入金剛鎖悅　　又稱明顯言",
        "from": "T21n1209_001:0040b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛即變成　　吽迦囉金剛",
        "from": "T21n1209_001:0040b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "暴怒處月輪　　身流火光聚",
        "from": "T21n1209_001:0040b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍體玄青色　　大自在天王",
        "from": "T21n1209_001:0040b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妃烏摩為座　　歷然分明見",
        "from": "T21n1209_001:0040b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即四印加持　　又示羯摩印",
        "from": "T21n1209_001:0040b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿月已五灌　　寶鬘甲拍等",
        "from": "T21n1209_001:0040b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施內外供養　　自住薩埵身",
        "from": "T21n1209_001:0040b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "扭擲金剛杵　　震動大千界",
        "from": "T21n1209_001:0040b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦百八名讚　　都請諸聖賢",
        "from": "T21n1209_001:0040b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鉤召索引入　　鎖止鈴悅喜",
        "from": "T21n1209_001:0040b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四明召智身　　入自體無二",
        "from": "T21n1209_001:0040b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又四加示誓　　滿月已五灌",
        "from": "T21n1209_001:0040b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶鬘甲拍已　　又陳內外供",
        "from": "T21n1209_001:0040b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即起平身立　　舉右足左旋",
        "from": "T21n1209_001:0040b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝彼傲慢者　　大自在欲王",
        "from": "T21n1209_001:0040b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "撲至於地已　　定按於彼頂",
        "from": "T21n1209_001:0040b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧踐彼王妃　　烏摩乳房上",
        "from": "T21n1209_001:0040b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摧彼我慢故　　以足加於頂",
        "from": "T21n1209_001:0040b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被害殞此已　　灰嚴界成佛",
        "from": "T21n1209_001:0040b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住是三昧時　　極力頻身蹋",
        "from": "T21n1209_001:0040b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或身掉出污　　當知尊攝受",
        "from": "T21n1209_001:0040b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定慧金剛拳　　二地結風背",
        "from": "T21n1209_001:0040b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "循身觀相好　　歷然見如前",
        "from": "T21n1209_001:0040b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旋時誦足鉤真言曰。",
        "from": "T21n1209_001:0040c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引迦引那引羯哩灑二合庾嚩日囉二合吽短步蹋時真言曰。",
        "from": "T21n1209_001:0040c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又見自心月　　金剛具五峯",
        "from": "T21n1209_001:0040c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中有所持明　　流射金剛火",
        "from": "T21n1209_001:0040c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住是大三昧　　持根本真言",
        "from": "T21n1209_001:0040c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或心一字明　　或業心中心",
        "from": "T21n1209_001:0040c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擿枳王真言　　住定疲極已",
        "from": "T21n1209_001:0040c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方坐誦讚歎　　陳供及發願",
        "from": "T21n1209_001:0040c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即作解脫法　　以本法四印",
        "from": "T21n1209_001:0040c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四處掣開之心上真言曰。",
        "from": "T21n1209_001:0040c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以真實王　　四印掣開之",
        "from": "T21n1209_001:0041a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心上真言曰。",
        "from": "T21n1209_001:0041a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖眾還宮已　　即寶鬘甲拍",
        "from": "T21n1209_001:0041a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護此常恒身　　四禮五向等",
        "from": "T21n1209_001:0041a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘皆同諸教　　降三世瑜伽",
        "from": "T21n1209_001:0041a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二羽印當心　　慧手持五鈷",
        "from": "T21n1209_001:0041a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "努臂如下擬　　次箭劍直執",
        "from": "T21n1209_001:0041a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定上五鈷鉤　　次弓次執索",
        "from": "T21n1209_001:0041a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆直引臂持　　四面正青色",
        "from": "T21n1209_001:0041a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右黃左綠色　　後紅咸忿怒",
        "from": "T21n1209_001:0041a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自在天王妃　　為座如前說",
        "from": "T21n1209_001:0041a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吽迦囉金剛　　作如是相好",
        "from": "T21n1209_001:0041a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛瑜伽降三世成就極深密門一卷",
        "from": "T21n1209_001:0041a14_100",
        "context_with_next_sentence": false
    }
]