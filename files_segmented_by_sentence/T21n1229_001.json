[
    {
        "source_sentence": "穢跡金剛禁百變法經一卷",
        "from": "T21n1229_001:0159b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北天竺國三藏沙門阿質",
        "from": "T21n1229_001:0159b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "達霰唐言無能勝將譯",
        "from": "T21n1229_001:0159b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時金剛復白佛言。",
        "from": "T21n1229_001:0159b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊若有善男子善女人。",
        "from": "T21n1229_001:0159b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持我此呪無効驗者無有是處。",
        "from": "T21n1229_001:0159b09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲令山摧者。",
        "from": "T21n1229_001:0159b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取白芥子三升上好安悉香。",
        "from": "T21n1229_001:0159b10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於山中疑有寶。",
        "from": "T21n1229_001:0159b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取鑌鐵刀一枚。",
        "from": "T21n1229_001:0159b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫四方為界。",
        "from": "T21n1229_001:0159b11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耳淨巾一枚香爐一枚。",
        "from": "T21n1229_001:0159b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒安悉香先呪一千八遍。",
        "from": "T21n1229_001:0159b12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取白芥子四散及至七遍。",
        "from": "T21n1229_001:0159b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是法其山自摧。",
        "from": "T21n1229_001:0159b13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有寶之處其藏神捨寶而出任意用之。",
        "from": "T21n1229_001:0159b14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲令海竭者。",
        "from": "T21n1229_001:0159b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先呪一千八遍。",
        "from": "T21n1229_001:0159b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以金銅作一龍形。",
        "from": "T21n1229_001:0159b15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擲於海中即時海竭。",
        "from": "T21n1229_001:0159b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲令江河逆流者。",
        "from": "T21n1229_001:0159b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取安悉香作一象形。",
        "from": "T21n1229_001:0159b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無問大小擲水中呪一百八遍。",
        "from": "T21n1229_001:0159b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "登時逆流。",
        "from": "T21n1229_001:0159b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令依舊者呪一淨石擲之水中其水如故。",
        "from": "T21n1229_001:0159b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有雷電霹靂毒龍卒風惡雨者。",
        "from": "T21n1229_001:0159b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即作止雷電印。",
        "from": "T21n1229_001:0159b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左手中指無名指小指並屈掌中。",
        "from": "T21n1229_001:0159b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭指以大母指。",
        "from": "T21n1229_001:0159b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捻頭指中節上。",
        "from": "T21n1229_001:0159b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦呪呪之。",
        "from": "T21n1229_001:0159b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印遙指雷雹之處自止。",
        "from": "T21n1229_001:0159b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲令一切鬼神自來歸伏為給使者。",
        "from": "T21n1229_001:0159b23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取水三斗盛銅器中。",
        "from": "T21n1229_001:0159b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以淨灰圍之。",
        "from": "T21n1229_001:0159b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即作教攝錄印。",
        "from": "T21n1229_001:0159b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二無名指並屈掌中。",
        "from": "T21n1229_001:0159b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令背相倚。",
        "from": "T21n1229_001:0159b25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指頭相捻。",
        "from": "T21n1229_001:0159b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指及小指各如開華。",
        "from": "T21n1229_001:0159b26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大母指捻中節。",
        "from": "T21n1229_001:0159b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "默呪一百八遍。",
        "from": "T21n1229_001:0159b27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其世界內所有諸惡鬼。",
        "from": "T21n1229_001:0159b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並來雲集自現其身。",
        "from": "T21n1229_001:0159b28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨毒惡心任行人驅使。",
        "from": "T21n1229_001:0159b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若禁山者所至之山。",
        "from": "T21n1229_001:0159b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦呪百遍大叫三聲。",
        "from": "T21n1229_001:0159c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即作業山印以右手無名指。",
        "from": "T21n1229_001:0159c01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈於掌中。",
        "from": "T21n1229_001:0159c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直竪中頭大母等指。",
        "from": "T21n1229_001:0159c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並直竪。",
        "from": "T21n1229_001:0159c02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向上印之七遍。",
        "from": "T21n1229_001:0159c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即却行七步。",
        "from": "T21n1229_001:0159c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後七印山。",
        "from": "T21n1229_001:0159c03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其山中即一切鳥狩並移出山。",
        "from": "T21n1229_001:0159c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作此印呪七遍。",
        "from": "T21n1229_001:0159c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印向空中印三七度。",
        "from": "T21n1229_001:0159c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其空中毫塵不過。",
        "from": "T21n1229_001:0159c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲令人不語者。",
        "from": "T21n1229_001:0159c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書前人姓名。",
        "from": "T21n1229_001:0159c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向口中含口。",
        "from": "T21n1229_001:0159c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人口不能言。",
        "from": "T21n1229_001:0159c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吐出即語得。",
        "from": "T21n1229_001:0159c07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦一切諸呪先須作壇。",
        "from": "T21n1229_001:0159c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦我此呪者即勿須作壇。",
        "from": "T21n1229_001:0159c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但剋一跋枳金剛榼杵。",
        "from": "T21n1229_001:0159c09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於佛塔中或於靜室中。",
        "from": "T21n1229_001:0159c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用香泥塗地隨其大小。",
        "from": "T21n1229_001:0159c10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著種種香華供養。",
        "from": "T21n1229_001:0159c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安杵壇中呪一百八遍。",
        "from": "T21n1229_001:0159c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其杵即自動。",
        "from": "T21n1229_001:0159c11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或變作種種異物亦勿怪之。",
        "from": "T21n1229_001:0159c12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更誦呪一百八遍。",
        "from": "T21n1229_001:0159c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其杵自去地三尺以來。",
        "from": "T21n1229_001:0159c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或五六七尺乃至一丈以來。",
        "from": "T21n1229_001:0159c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持法之人即須歸依懺悔發願。",
        "from": "T21n1229_001:0159c14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我於彼中即現真身。",
        "from": "T21n1229_001:0159c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨行人意所願樂者。",
        "from": "T21n1229_001:0159c15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並皆速得如意。",
        "from": "T21n1229_001:0159c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我即與授菩提之記。",
        "from": "T21n1229_001:0159c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得身心解脫。",
        "from": "T21n1229_001:0159c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先須誦十萬遍滿。",
        "from": "T21n1229_001:0159c17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後作法若課未充不得効驗。",
        "from": "T21n1229_001:0159c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印法第二印此印方一寸八分刻之。",
        "from": "T21n1229_001:0160a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪一千遍。",
        "from": "T21n1229_001:0160a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日白膠香度之。",
        "from": "T21n1229_001:0160a03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剋印日勿令人見。",
        "from": "T21n1229_001:0160a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用印印心。",
        "from": "T21n1229_001:0160a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得心智自然智宿命智。",
        "from": "T21n1229_001:0160a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持印百日。",
        "from": "T21n1229_001:0160a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得住種種大法門。",
        "from": "T21n1229_001:0160a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印方一寸二分呪六百遍。",
        "from": "T21n1229_001:0160a07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以安悉香度之。",
        "from": "T21n1229_001:0160a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶行令一切人愛樂得大自在永離眾苦。",
        "from": "T21n1229_001:0160a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印方一寸五分刻之。",
        "from": "T21n1229_001:0160a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪六百遍。",
        "from": "T21n1229_001:0160a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以白膠香度之。",
        "from": "T21n1229_001:0160a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用印印脚。",
        "from": "T21n1229_001:0160a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便得飛騰虛空所向自在。",
        "from": "T21n1229_001:0160a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印方一寸八分刻之。",
        "from": "T21n1229_001:0160a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用白膠香度之。",
        "from": "T21n1229_001:0160a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪七千遍。",
        "from": "T21n1229_001:0160a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用印印足。",
        "from": "T21n1229_001:0160a15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可日行三百萬里無人得見。",
        "from": "T21n1229_001:0160a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神變延命法印伏連。",
        "from": "T21n1229_001:0160a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書心上即差。",
        "from": "T21n1229_001:0160a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大吉急急如律令。",
        "from": "T21n1229_001:0160a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印心痛。",
        "from": "T21n1229_001:0160a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書之立即除差。",
        "from": "T21n1229_001:0160a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大吉利急急如律令先呪七遍。",
        "from": "T21n1229_001:0160a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼病。",
        "from": "T21n1229_001:0160a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朱書吞之。",
        "from": "T21n1229_001:0160a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精魅鬼病之人。",
        "from": "T21n1229_001:0160a27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朱吞之七大書枚立瘥神驗。",
        "from": "T21n1229_001:0160a27_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若依法之人。",
        "from": "T21n1229_001:0160b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取白檀綾二丈一尺七寸。",
        "from": "T21n1229_001:0160b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白練裹之。",
        "from": "T21n1229_001:0160b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置於地輪世界。",
        "from": "T21n1229_001:0160b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令人延年得七十歲。",
        "from": "T21n1229_001:0160b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無人送者。",
        "from": "T21n1229_001:0160b04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即安自宅中庭。",
        "from": "T21n1229_001:0160b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掘地七尺埋之亦得。",
        "from": "T21n1229_001:0160b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又得聰明多智辯才無礙。",
        "from": "T21n1229_001:0160b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此七道亦能治萬病。",
        "from": "T21n1229_001:0160b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吞之亦令人長壽益智大神驗。",
        "from": "T21n1229_001:0160b17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此上七道用朱書紙上。",
        "from": "T21n1229_001:0160b27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吞之千枚令人延年。",
        "from": "T21n1229_001:0160b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得與天地齊壽。",
        "from": "T21n1229_001:0160b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得令人見之。",
        "from": "T21n1229_001:0160b29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此上七道若有人患一切病。",
        "from": "T21n1229_001:0160c07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此符書之皆得除瘥。",
        "from": "T21n1229_001:0160c08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人書符吞之者。",
        "from": "T21n1229_001:0160c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "延年益智大驗効矣。",
        "from": "T21n1229_001:0160c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此上七道若有人求種種珍寶者。",
        "from": "T21n1229_001:0160c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以朱書此符吞之滿七日。",
        "from": "T21n1229_001:0160c18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即有種種妙寶自然而至。",
        "from": "T21n1229_001:0160c19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若求他人財物。",
        "from": "T21n1229_001:0160c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當書彼人姓名於符下。",
        "from": "T21n1229_001:0160c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人立即送物到。",
        "from": "T21n1229_001:0160c21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此上三符朱書床四脚上。",
        "from": "T21n1229_001:0160c27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常有八大金剛衛護悉不暫捨。",
        "from": "T21n1229_001:0160c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟須嚴淨勿令污染之物入房。",
        "from": "T21n1229_001:0160c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "切須慎之。",
        "from": "T21n1229_001:0160c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大火災起者。",
        "from": "T21n1229_001:0161a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書符擲一枚呪一百八遍。",
        "from": "T21n1229_001:0161a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向火中須臾災自滅。",
        "from": "T21n1229_001:0161a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火惡風起者。",
        "from": "T21n1229_001:0161a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書此符呪一百八遍。",
        "from": "T21n1229_001:0161a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擲向風中即止。",
        "from": "T21n1229_001:0161a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大水起者。",
        "from": "T21n1229_001:0161a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書此符擲於水中。",
        "from": "T21n1229_001:0161a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立即斷流水不溺人矣。",
        "from": "T21n1229_001:0161a09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大雨者。",
        "from": "T21n1229_001:0161a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書此符呪之一百八遍。",
        "from": "T21n1229_001:0161a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向雨擲之其雨立即自定之。",
        "from": "T21n1229_001:0161a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此符朱書吞三枚。",
        "from": "T21n1229_001:0161a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及可與他人書符。",
        "from": "T21n1229_001:0161a19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即有驗効。",
        "from": "T21n1229_001:0161a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不爾者用諸符無驗。",
        "from": "T21n1229_001:0161a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時穢跡金剛說此符已。",
        "from": "T21n1229_001:0161a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大眾同聲讚言。",
        "from": "T21n1229_001:0161a21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉大力士汝能說是大妙之法。",
        "from": "T21n1229_001:0161a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸眾生皆得解脫。",
        "from": "T21n1229_001:0161a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時金剛頃白諸大眾。",
        "from": "T21n1229_001:0161a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知我於汝等。",
        "from": "T21n1229_001:0161a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此法若流行之處。",
        "from": "T21n1229_001:0161a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等大天常當護此行法之人助令成就。",
        "from": "T21n1229_001:0161a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時金剛復作是言。",
        "from": "T21n1229_001:0161a25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生行此法者。",
        "from": "T21n1229_001:0161a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我即往彼現其人前。",
        "from": "T21n1229_001:0161a26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求願者我亦施與。",
        "from": "T21n1229_001:0161a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼得種種變現種種神通所作無礙。",
        "from": "T21n1229_001:0161a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常須念我本師釋迦牟尼佛。",
        "from": "T21n1229_001:0161a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我即常隨逐之。",
        "from": "T21n1229_001:0161a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令一切法皆助成就。",
        "from": "T21n1229_001:0161a29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時金剛說此法已。",
        "from": "T21n1229_001:0161b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大眾倍加悲喜。",
        "from": "T21n1229_001:0161b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸天龍大鬼神等。",
        "from": "T21n1229_001:0161b01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各奉聖言禮足而去。",
        "from": "T21n1229_001:0161b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "穢跡金剛禁百變經一卷古經本呪四十三字。",
        "from": "T21n1229_001:0161b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐太宗朝人多持誦感驗非一。",
        "from": "T21n1229_001:0161b04_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除去十字今就錄出。",
        "from": "T21n1229_001:0161b04_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速獲靈應無過是呪。",
        "from": "T21n1229_001:0161b04_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵𠲽咶　啒㖀摩訶般囉二合　很那𠵨　吻汁吻　（醯）（摩）（尼）𡀎咭𡀎　摩那棲（唵）（斫）（急）（那）烏深暮　啒㖀吽吽（吽）泮泮泮（泮）（泮）娑訶真覺彈師所傳神呪。",
        "from": "T21n1229_001:0161b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與今經呪同。",
        "from": "T21n1229_001:0161b09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但梵音賖切字語少異。",
        "from": "T21n1229_001:0161b09_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵佛㖃窟聿　摩訶般那很那詡　吻泮吻　尾劼尾　摩那棲　烏澁謨　窟聿吽吽吽　發發發　莎訶",
        "from": "T21n1229_001:0161b12_100",
        "context_with_next_sentence": false
    }
]