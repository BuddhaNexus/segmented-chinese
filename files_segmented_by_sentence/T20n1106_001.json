[
    {
        "source_sentence": "讚揚聖德多羅菩薩一百八名經",
        "from": "T20n1106_001:0474b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天中印度惹爛馱囉國密林寺",
        "from": "T20n1106_001:0474b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏明教大師賜紫沙門臣天息災",
        "from": "T20n1106_001:0474b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心歸命禮　　適悅最吉祥",
        "from": "T20n1106_001:0474b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "補多羅迦山　　其界以種種",
        "from": "T20n1106_001:0474b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "珍寶所嚴飾　　種種寶林樹",
        "from": "T20n1106_001:0474b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "枝蔓密垂布　　有種種成就",
        "from": "T20n1106_001:0474b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱蘇摩妙花　　其華光普照",
        "from": "T20n1106_001:0474c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有種種池沼　　泉流種種聲",
        "from": "T20n1106_001:0474c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有種種色　　香象及鹿王",
        "from": "T20n1106_001:0474c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蜂王妙歌音　　緊那女美曲",
        "from": "T20n1106_001:0474c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "犍闥婆奏樂　　聖天及人民",
        "from": "T20n1106_001:0474c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牟尼離欲眾　　恒集於其中",
        "from": "T20n1106_001:0474c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并餘菩薩眾　　及十地自在",
        "from": "T20n1106_001:0474c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖多羅菩薩　　與千明妃等",
        "from": "T20n1106_001:0474c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忿怒大明王　　馬首等圍繞",
        "from": "T20n1106_001:0474c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "于時聖具德　　觀自在菩薩",
        "from": "T20n1106_001:0474c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為利諸有情　　修大行已具",
        "from": "T20n1106_001:0474c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈悲喜捨者　　處彼胎藏生",
        "from": "T20n1106_001:0474c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉祥蓮華座　　安詳而端坐",
        "from": "T20n1106_001:0474c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與人天大眾　　而為說妙法",
        "from": "T20n1106_001:0474c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時金剛手　　大力忿怒王",
        "from": "T20n1106_001:0474c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為悲愍他故　　而問觀自在",
        "from": "T20n1106_001:0474c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師子象虎蛇　　水火賊枷鎖",
        "from": "T20n1106_001:0474c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等八難　　懈怠劣有情",
        "from": "T20n1106_001:0474c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何得免離　　長處輪迴海",
        "from": "T20n1106_001:0474c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貪欲瞋癡等　　輪迴縛所纏",
        "from": "T20n1106_001:0474c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若令得解脫　　我說彼能仁",
        "from": "T20n1106_001:0474c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為世所尊重　　彼時觀自在",
        "from": "T20n1106_001:0474c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出美妙言音　　警覺金剛手",
        "from": "T20n1106_001:0474c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祕密主諦聽　　我從無重壽",
        "from": "T20n1106_001:0474c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誓願之所生　　為諸世間母",
        "from": "T20n1106_001:0474c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手執優鉢華　　放光照此界",
        "from": "T20n1106_001:0474c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人間及天上　　驚動是世界",
        "from": "T20n1106_001:0474c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥叉羅剎等　　告言勿驚怖",
        "from": "T20n1106_001:0474c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我從佛變化　　為護於世間",
        "from": "T20n1106_001:0474c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種嶮難怖　　刀兵及飢饉",
        "from": "T20n1106_001:0475a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輪迴種種怖　　我救有情故",
        "from": "T20n1106_001:0475a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世稱為哆囉　　說為佛之子",
        "from": "T20n1106_001:0475a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉應尊敬　　言已便合掌",
        "from": "T20n1106_001:0475a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即踊空中住　　身光焰熾盛",
        "from": "T20n1106_001:0475a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃宣此語言　　此一百八名",
        "from": "T20n1106_001:0475a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是先佛所說　　十地自在等",
        "from": "T20n1106_001:0475a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量諸菩薩　　聞之生歡喜",
        "from": "T20n1106_001:0475a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人受持　　諸罪悉消除",
        "from": "T20n1106_001:0475a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福增名稱廣　　資財多吉祥",
        "from": "T20n1106_001:0475a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸病皆殄散　　安祥住福田",
        "from": "T20n1106_001:0475a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長壽乃安樂　　興慈度有情",
        "from": "T20n1106_001:0475a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼名大牟尼　　具德如是說",
        "from": "T20n1106_001:0475a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀自在微笑　　乃觀於十方",
        "from": "T20n1106_001:0475a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變化運慈心　　即申其右手",
        "from": "T20n1106_001:0475a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福相莊嚴臂　　告彼大智言",
        "from": "T20n1106_001:0475a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉金剛手　　今聽大福德",
        "from": "T20n1106_001:0475a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是妙寂靜名　　正直為人說",
        "from": "T20n1106_001:0475a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若聞得安樂　　豐財兼自在",
        "from": "T20n1106_001:0475a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸病得解脫　　具足諸功德",
        "from": "T20n1106_001:0475a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "息除於中夭　　後終生極樂",
        "from": "T20n1106_001:0475a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天等當知　　諦聽我今說",
        "from": "T20n1106_001:0475a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等深隨喜　　一百八名曰",
        "from": "T20n1106_001:0475a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以說此百八　　寂靜祕密名",
        "from": "T20n1106_001:0475c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能利諸有情　　希有祕福田",
        "from": "T20n1106_001:0475c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天上及人間　　亦甚難逢遇",
        "from": "T20n1106_001:0475c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令一切有情　　瞻視得安樂",
        "from": "T20n1106_001:0475c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智者應一心　　澡浴淨其身",
        "from": "T20n1106_001:0475c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三時專念誦　　時彼人不久",
        "from": "T20n1106_001:0476a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得王所愛敬　　離苦得安樂",
        "from": "T20n1106_001:0476a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貧者獲財寶　　愚得大智慧",
        "from": "T20n1106_001:0476a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聰睿不復疑　　枷鎖得解脫",
        "from": "T20n1106_001:0476a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不勝還得勝　　冤返為善友",
        "from": "T20n1106_001:0476a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鋒牙及利角　　鬪諍并嶮難",
        "from": "T20n1106_001:0476a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種怖畏等　　憶念此名者",
        "from": "T20n1106_001:0476a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能救是諸怖　　止息中天命",
        "from": "T20n1106_001:0476a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名稱得廣布　　恒生善貴族",
        "from": "T20n1106_001:0476a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼惟獨一身　　或因行坐臥",
        "from": "T20n1106_001:0476a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當獲人稱譽　　壽命得時長",
        "from": "T20n1106_001:0476a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人間受快樂　　天龍及藥叉",
        "from": "T20n1106_001:0476a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅剎犍闥嚩　　臭神食穢神",
        "from": "T20n1106_001:0476a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡心摩怛嚕　　女魅拏明努",
        "from": "T20n1106_001:0476a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塢娑怛羅等　　癲癎及膊行",
        "from": "T20n1106_001:0476a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吠怛拏大鬼　　拏枳儞等神",
        "from": "T20n1106_001:0476a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及餘惡心者　　不敢越其影",
        "from": "T20n1106_001:0476a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況鬪戰處　　暴惡之有情",
        "from": "T20n1106_001:0476a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禁呪及幻術　　悉不能侵害",
        "from": "T20n1106_001:0476a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自在有威德　　子孫及財產",
        "from": "T20n1106_001:0476a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增長無有量　　獲宿命智通",
        "from": "T20n1106_001:0476a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上族見皆喜　　無礙大辯才",
        "from": "T20n1106_001:0476a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了達諸論義　　得遇善知識",
        "from": "T20n1106_001:0476a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莊嚴菩提心　　於生生世世",
        "from": "T20n1106_001:0476a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒親近諸佛讚揚聖德多羅菩薩一百八名經",
        "from": "T20n1106_001:0476a26_100",
        "context_with_next_sentence": false
    }
]