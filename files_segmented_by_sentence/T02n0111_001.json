[
    {
        "source_sentence": "佛說相應相可經",
        "from": "T02n0111_001:0504b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西晉沙門法炬譯",
        "from": "T02n0111_001:0504b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛在舍衛國請諸比丘，比丘即到。佛告比丘，",
        "from": "T02n0111_001:0504b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比丘應：",
        "from": "T02n0111_001:0504c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「唯然，從佛聞。",
        "from": "T02n0111_001:0504c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」佛便說：",
        "from": "T02n0111_001:0504c01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「是不聞者，不聞者俱，相類、相聚、相應、相可；",
        "from": "T02n0111_001:0504c02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多聞者，多聞者俱，相類、相聚、相應、相可。",
        "from": "T02n0111_001:0504c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貪婬者，貪婬者俱，相類、相聚、相應、相可；",
        "from": "T02n0111_001:0504c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不貪婬者，不貪婬者俱相類、相聚、相應、相可。",
        "from": "T02n0111_001:0504c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞋恚者，瞋恚者俱，相類、相聚、相應、相可；",
        "from": "T02n0111_001:0504c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不瞋恚者，不瞋恚者俱，相類、相聚、相應、相可。",
        "from": "T02n0111_001:0504c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愚癡者，愚癡者俱，相類、相聚、相應、相可；",
        "from": "T02n0111_001:0504c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧者，慧者俱，相類、相聚、相應、相可。",
        "from": "T02n0111_001:0504c08_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "布施者，布施者俱，相類、相聚、相應、相可；",
        "from": "T02n0111_001:0504c09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慳貪者，慳貪者俱，相類、相聚、相應、相可。",
        "from": "T02n0111_001:0504c10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少欲者，少欲者俱，相類、相聚、相應、相可；",
        "from": "T02n0111_001:0504c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多欲者，多欲者俱，相類、相聚、相應、相可。",
        "from": "T02n0111_001:0504c12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不持戒者，不持戒者俱，相類、相聚、相應、相可；",
        "from": "T02n0111_001:0504c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持戒者，持戒者俱，相類、相聚、相應、相可。",
        "from": "T02n0111_001:0504c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難給者，難給者俱，相類、相聚、相應、相可；",
        "from": "T02n0111_001:0504c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "易給者，易給者俱，相類、相聚、相應、相可。",
        "from": "T02n0111_001:0504c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不知足者，不知足者俱，相類、相聚、相應、相可；",
        "from": "T02n0111_001:0504c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知足者，知足者俱，相類、相聚、相應、相可。",
        "from": "T02n0111_001:0504c17_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不自守者，不自守者俱，相類、相聚、相應、相可；",
        "from": "T02n0111_001:0504c18_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自守者，自守者俱，相類、相聚、相應、相可。",
        "from": "T02n0111_001:0504c19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」佛說：",
        "from": "T02n0111_001:0504c19_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「比丘！",
        "from": "T02n0111_001:0504c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是慧人當分別是因緣，可行者行之，不可行者莫行。",
        "from": "T02n0111_001:0504c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "」佛說如是。",
        "from": "T02n0111_001:0504c21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比丘受行著意。",
        "from": "T02n0111_001:0504c21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說相應相可經",
        "from": "T02n0111_001:0504c22_100",
        "context_with_next_sentence": false
    }
]