[
    {
        "source_sentence": "佛說大吉祥陀羅尼經",
        "from": "T21n1381_001:0903c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試光祿卿",
        "from": "T21n1381_001:0903c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明教大師臣法賢奉　詔譯",
        "from": "T21n1381_001:0903c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T21n1381_001:0903c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在蘇珂嚩帝佛剎。",
        "from": "T21n1381_001:0903c17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與諸大菩薩并部多眾俱。",
        "from": "T21n1381_001:0903c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時觀自在菩薩摩訶薩。",
        "from": "T21n1381_001:0903c18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來詣佛所到已頭面禮足於一面坐。",
        "from": "T21n1381_001:0903c19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有菩薩摩訶薩名大吉祥。",
        "from": "T21n1381_001:0903c20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦詣佛所到已繞佛三匝。",
        "from": "T21n1381_001:0903c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭面禮足却坐一面。",
        "from": "T21n1381_001:0903c21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊見大吉祥菩薩已。",
        "from": "T21n1381_001:0903c22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告觀自在菩薩言。",
        "from": "T21n1381_001:0903c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大吉祥菩薩有陀羅尼。",
        "from": "T21n1381_001:0903c23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若閻浮提濁惡世中。",
        "from": "T21n1381_001:0903c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有比丘比丘尼優婆塞優婆夷等。",
        "from": "T21n1381_001:0903c24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見聞隨喜讀誦受持。",
        "from": "T21n1381_001:0903c25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信解此法思惟記念。",
        "from": "T21n1381_001:0903c25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是貧苦者獲大富貴。",
        "from": "T21n1381_001:0903c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至部多普令愛敬。",
        "from": "T21n1381_001:0903c26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是功德不可具說。",
        "from": "T21n1381_001:0903c27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時觀自在菩薩聞佛語已。",
        "from": "T21n1381_001:0903c27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡喜踊躍即從座起。",
        "from": "T21n1381_001:0903c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌白佛言。",
        "from": "T21n1381_001:0903c28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我今樂聞。",
        "from": "T21n1381_001:0903c28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊為我宣說。",
        "from": "T21n1381_001:0903c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊即說大吉祥陀羅尼曰。",
        "from": "T21n1381_001:0904a01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛𭔞切身下同他引一洛叱彌二合二室哩二合鉢捺彌二合嚩呬抳引三馱曩引提鉢底引四藕哩引五摩賀引野舍引六鉢捺摩二合儞怛哩二合七摩賀引儞喻二合底八案曩那引曳九囉怛曩二合鉢囉二合婆摩賀引室哩二合十婆野替引那十一屹哩二合儞屹哩二合儞十二薩哩嚩二合遏迦哩也二合爍彌引十三提儞提儞十四悉悉悉悉十五儞底儞底十六呬儞呬儞十七阿洛叉彌二合十八彌曩舍野十九薩嚩洛叉彌二合彌娑𠆙切身引那野莎引賀二十曩莫薩哩嚩二合沒馱二十一冒提薩怛吠二合引毘野二合莎引賀引二十二爾時世尊說是經已。",
        "from": "T21n1381_001:0904a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀自在菩薩大吉祥菩薩。",
        "from": "T21n1381_001:0904a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并諸菩薩及部多眾。",
        "from": "T21n1381_001:0904a14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛所說皆大歡喜信受奉行。",
        "from": "T21n1381_001:0904a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說大吉祥陀羅尼經",
        "from": "T21n1381_001:0904a16_100",
        "context_with_next_sentence": false
    }
]