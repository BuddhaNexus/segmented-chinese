        "from": "T21n1400_001:0917a13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡喜頂戴憶念受持。",
        "from": "T21n1400_001:0917a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常與眷屬安樂而住。",
        "from": "T21n1400_001:0917a14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難此陀羅尼威力廣大。",
        "from": "T21n1400_001:0917a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不獨降伏阿脩羅王。",
        "from": "T21n1400_001:0917a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至一切天龍夜叉乾闥婆迦樓羅緊那羅。",
        "from": "T21n1400_001:0917a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及部多吠多拏等。",
        "from": "T21n1400_001:0917a16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惱亂眾生者。",
        "from": "T21n1400_001:0917a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以陀羅尼威力擁護不能為害。",
        "from": "T21n1400_001:0917a17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復能除飢饉疾疫毒藥夭歿。",
        "from": "T21n1400_001:0917a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夢寐不祥惡魔驚恐。",
        "from": "T21n1400_001:0917a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或復有人為求財寶。",
        "from": "T21n1400_001:0917a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或求子息。",
        "from": "T21n1400_001:0917a19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持誦此陀羅尼者。",
        "from": "T21n1400_001:0917a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉得隨意財富無量眷屬增盛。",
        "from": "T21n1400_001:0917a20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難此陀羅尼功德殊勝。",
        "from": "T21n1400_001:0917a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我若具說不可窮盡。",
        "from": "T21n1400_001:0917a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時阿難聞佛說此陀羅尼已。",
        "from": "T21n1400_001:0917a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "志心受持禮佛而退。",
        "from": "T21n1400_001:0917a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說消除一切災障寶髻陀羅尼經",
        "from": "T21n1400_001:0917a25_100",
        "context_with_next_sentence": false
    }
]