[
    {
        "source_sentence": "佛說消除一切災障寶髻陀羅",
        "from": "T21n1400_001:0916a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尼經",
        "from": "T21n1400_001:0916a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試光祿卿",
        "from": "T21n1400_001:0916a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明教大師臣法賢奉　詔譯爾時世尊告阿難言。",
        "from": "T21n1400_001:0916a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有陀羅尼名曰寶髻。",
        "from": "T21n1400_001:0916a15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能與眾生作大利益。",
        "from": "T21n1400_001:0916a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能滅眾生極重罪業。",
        "from": "T21n1400_001:0916a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難往昔帝釋與脩羅戰時。",
        "from": "T21n1400_001:0916a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天帝釋退敗奔走怖畏無量。",
        "from": "T21n1400_001:0916a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是疾速來詣佛所。",
        "from": "T21n1400_001:0916a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "哀告我言。",
        "from": "T21n1400_001:0916a18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊大慈願垂愍察。",
        "from": "T21n1400_001:0916a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我怖脩羅不能安住。",
        "from": "T21n1400_001:0916a19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願大慈賜我安隱。",
        "from": "T21n1400_001:0916a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難我聞帝釋作是語已。",
        "from": "T21n1400_001:0916a20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即告之言汝當勿怖施汝擁護。",
        "from": "T21n1400_001:0916a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天主過去劫時。",
        "from": "T21n1400_001:0916a21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有佛世界名曰觀照。",
        "from": "T21n1400_001:0916a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼土有佛名觀自在如來。",
        "from": "T21n1400_001:0916a22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼佛授我寶髻陀羅尼。",
        "from": "T21n1400_001:0916a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是陀羅尼一俱胝佛異口同說。",
        "from": "T21n1400_001:0916a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大威力能降吉祥。",
        "from": "T21n1400_001:0916a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人書寫頂戴讀誦受持。",
        "from": "T21n1400_001:0916a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我知是人滅一切罪增無量福。",
        "from": "T21n1400_001:0916a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常自擁護及能利他。",
        "from": "T21n1400_001:0916a26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天主汝常憶念書寫頂戴。",
        "from": "T21n1400_001:0916a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令汝所作一切隨心。",
        "from": "T21n1400_001:0916a27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此陀羅尼王亦名隨求。",
        "from": "T21n1400_001:0916a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能與眾生最上善利。",
        "from": "T21n1400_001:0916a28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼阿脩羅當自息心。",
        "from": "T21n1400_001:0916a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設來鬪戰退敗降伏。",
        "from": "T21n1400_001:0916a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我即為說寶髻陀羅尼曰。",
        "from": "T21n1400_001:0916b01_3",
        "context_with_next_sentence": true
    },
    {
