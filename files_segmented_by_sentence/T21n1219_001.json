[
    {
        "source_sentence": "曼殊室利焰曼德迦萬愛祕術如",
        "from": "T21n1219_001:0097b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "意法",
        "from": "T21n1219_001:0097b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一行撰譯",
        "from": "T21n1219_001:0097b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫真言教源。",
        "from": "T21n1219_001:0097b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自護摩法無上也。",
        "from": "T21n1219_001:0097b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此之中祕密教愛法也。",
        "from": "T21n1219_001:0097b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為求末世眾生敬愛。",
        "from": "T21n1219_001:0097b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說一印真言。",
        "from": "T21n1219_001:0097b17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能利眾生故。",
        "from": "T21n1219_001:0097b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先說像。",
        "from": "T21n1219_001:0097b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其身六面六臂六足。",
        "from": "T21n1219_001:0097b18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘水牛。",
        "from": "T21n1219_001:0097b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂六面。",
        "from": "T21n1219_001:0097b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂上三面中面菩薩形柔軟也。",
        "from": "T21n1219_001:0097b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其面頂有阿彌陀佛。",
        "from": "T21n1219_001:0097b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又六臂。",
        "from": "T21n1219_001:0097b20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂左一鉾二輪索三弓。",
        "from": "T21n1219_001:0097b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右一劍二寶杖三箭。",
        "from": "T21n1219_001:0097b21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以彼弓鑄勢也。",
        "from": "T21n1219_001:0097b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又六足。",
        "from": "T21n1219_001:0097b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左三足立輪。",
        "from": "T21n1219_001:0097b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右三足上彼輪下有木牛。",
        "from": "T21n1219_001:0097b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牛四足立花座。",
        "from": "T21n1219_001:0097b23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復像背有火焰。",
        "from": "T21n1219_001:0097b23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如虹。",
        "from": "T21n1219_001:0097b23_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又如吠瑠璃。",
        "from": "T21n1219_001:0097b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其火中當像右足下有寅神。",
        "from": "T21n1219_001:0097b24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次第十二神但未申對面。",
        "from": "T21n1219_001:0097b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次壇水圓壇。",
        "from": "T21n1219_001:0097b25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其壇中置輪可修行。",
        "from": "T21n1219_001:0097b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增益敬愛息災調伏。",
        "from": "T21n1219_001:0097b26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆可置。",
        "from": "T21n1219_001:0097b26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其所口傳又當身口傳。",
        "from": "T21n1219_001:0097b27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次說一印口傳。",
        "from": "T21n1219_001:0097b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1219_001:0097b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵瑟底哩迦羅嚕婆吽欠娑呵以是印呪向佛。",
        "from": "T21n1219_001:0097c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成普禮及次第觀念。",
        "from": "T21n1219_001:0097c01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說曼陀羅法。",
        "from": "T21n1219_001:0097c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內院明王。",
        "from": "T21n1219_001:0097c02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明王背有法形文殊。",
        "from": "T21n1219_001:0097c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外院東方召請童子。",
        "from": "T21n1219_001:0097c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其形如童子。",
        "from": "T21n1219_001:0097c03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髻上生登以弓矢。",
        "from": "T21n1219_001:0097c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東南方計設尼童子如上形。",
        "from": "T21n1219_001:0097c04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左拳右把三角鉾。",
        "from": "T21n1219_001:0097c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左拳置腰。",
        "from": "T21n1219_001:0097c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南方救護慧童子如上形。",
        "from": "T21n1219_001:0097c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左投輪。",
        "from": "T21n1219_001:0097c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右施無畏右足上。",
        "from": "T21n1219_001:0097c06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西南方烏波計設尼童子如上形。",
        "from": "T21n1219_001:0097c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手花鉢。",
        "from": "T21n1219_001:0097c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方光綱童子。",
        "from": "T21n1219_001:0097c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左拳置腰。",
        "from": "T21n1219_001:0097c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右捧孔雀尾。",
        "from": "T21n1219_001:0097c08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西北方地慧童子。",
        "from": "T21n1219_001:0097c08_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手把三𦙶。",
        "from": "T21n1219_001:0097c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如金剛藏王。",
        "from": "T21n1219_001:0097c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方無垢光童子如上形。",
        "from": "T21n1219_001:0097c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左拳置腰。",
        "from": "T21n1219_001:0097c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右索上。",
        "from": "T21n1219_001:0097c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東北方不思議童子。",
        "from": "T21n1219_001:0097c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左拳置腰。",
        "from": "T21n1219_001:0097c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右劍地上物打勢也。",
        "from": "T21n1219_001:0097c11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一身五色立輪。",
        "from": "T21n1219_001:0097c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輪下一一有師子。",
        "from": "T21n1219_001:0097c12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是說法。",
        "from": "T21n1219_001:0097c12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就說法。",
        "from": "T21n1219_001:0097c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高山頂造淨室。",
        "from": "T21n1219_001:0097c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四十九日無言。",
        "from": "T21n1219_001:0097c13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷火如法。",
        "from": "T21n1219_001:0097c13_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百萬遍滿足。",
        "from": "T21n1219_001:0097c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後法成就獲得。",
        "from": "T21n1219_001:0097c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次法成就說符。",
        "from": "T21n1219_001:0097c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　地符",
        "from": "T21n1219_001:0097c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天符如是說已。",
        "from": "T21n1219_001:0097c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "末世眾生信受奉行。",
        "from": "T21n1219_001:0097c29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "焰摩德迦萬愛法一卷保元二年七月二十三日。",
        "from": "T21n1219_001:0098a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於勸修寺西明院住房　一校了。",
        "from": "T21n1219_001:0098a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　　　　　　　　　　　僧智海",
        "from": "T21n1219_001:0098a04_0",
        "context_with_next_sentence": true
    }
]