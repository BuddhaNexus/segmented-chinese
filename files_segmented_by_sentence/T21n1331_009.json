[
    {
        "source_sentence": "佛說灌頂召五方龍王攝疫毒神呪上品經佛說灌頂召五方龍王攝疫毒神",
        "from": "T21n1331_009:0521a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪上品經卷第九",
        "from": "T21n1331_009:0521a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東晉天竺三藏帛尸梨蜜多羅譯",
        "from": "T21n1331_009:0521a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞如是。",
        "from": "T21n1331_009:0521a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛遊王舍大城竹林精舍。",
        "from": "T21n1331_009:0521a10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與四部弟子眷屬圍遶。",
        "from": "T21n1331_009:0521a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天龍八部悉來集會。",
        "from": "T21n1331_009:0521a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛為說法垂欲竟時。",
        "from": "T21n1331_009:0521a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是阿難從坐而起。",
        "from": "T21n1331_009:0521a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "齊整衣服稽首佛足。",
        "from": "T21n1331_009:0521a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長跪合掌而白佛言。",
        "from": "T21n1331_009:0521a13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "維耶離國癘氣疾疫。",
        "from": "T21n1331_009:0521a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猛盛赫赫猶如熾火。",
        "from": "T21n1331_009:0521a14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中毒病者頭痛寒熱百節欲解。",
        "from": "T21n1331_009:0521a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘇者甚少死者無數。",
        "from": "T21n1331_009:0521a15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊大慈愍加群生。",
        "from": "T21n1331_009:0521a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願為救護使得蘇息。",
        "from": "T21n1331_009:0521a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不遭苦患病愈熱除。",
        "from": "T21n1331_009:0521a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復為說法使得至道。",
        "from": "T21n1331_009:0521a17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告阿難維耶離國中。",
        "from": "T21n1331_009:0521a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以遭此疫毒病者。",
        "from": "T21n1331_009:0521a18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是其國人多殺群生。",
        "from": "T21n1331_009:0521a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有慈心以殺獵為業。",
        "from": "T21n1331_009:0521a19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸麞鹿麋麈禽獸熊羆之屬。",
        "from": "T21n1331_009:0521a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有智慧者作是誓願。",
        "from": "T21n1331_009:0521a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願我來世墮鬼神中。",
        "from": "T21n1331_009:0521a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願為鬼殃銜火燒人。",
        "from": "T21n1331_009:0521a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有犯觸我者若殺若縛。",
        "from": "T21n1331_009:0521a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "強取我者我當放火。",
        "from": "T21n1331_009:0521a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒其山野及所居村舍。",
        "from": "T21n1331_009:0521a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於我有怨悉令被害。",
        "from": "T21n1331_009:0521a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸眾生不得藏匿。",
        "from": "T21n1331_009:0521a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故阿難當化導一切令其慈仁。",
        "from": "T21n1331_009:0521a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿殺群生普慈一切。",
        "from": "T21n1331_009:0521a25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持禁戒行於十善。",
        "from": "T21n1331_009:0521a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能如此可得至道。",
        "from": "T21n1331_009:0521a26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛又告阿難其世間人遭諸火殃。",
        "from": "T21n1331_009:0521a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或燒一家乃至五三十家。",
        "from": "T21n1331_009:0521a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或燒一里乃至五三十里。",
        "from": "T21n1331_009:0521a28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或燒百里乃至五三百里。",
        "from": "T21n1331_009:0521a29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或燒千里乃至五千萬里。",
        "from": "T21n1331_009:0521b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如此因緣不可稱數。",
        "from": "T21n1331_009:0521b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與此眾生有業緣者。",
        "from": "T21n1331_009:0521b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必為火殃之所燒害。",
        "from": "T21n1331_009:0521b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故不得避藏得脫。",
        "from": "T21n1331_009:0521b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以是因緣勿行殺獵。",
        "from": "T21n1331_009:0521b03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化行十善可得至道。",
        "from": "T21n1331_009:0521b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛又告阿難此國中人。",
        "from": "T21n1331_009:0521b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又網水中黿鼉魚鼈。",
        "from": "T21n1331_009:0521b05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雜類眾生不可稱數。",
        "from": "T21n1331_009:0521b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而噉食之以為美饍。",
        "from": "T21n1331_009:0521b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸眾生等有智慧者。",
        "from": "T21n1331_009:0521b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各各懷忿。",
        "from": "T21n1331_009:0521b07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願我後世墮大海中。",
        "from": "T21n1331_009:0521b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作毒蛟龍吐種種毒氣猶如雲霧。",
        "from": "T21n1331_009:0521b08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以害維耶離國中人民。",
        "from": "T21n1331_009:0521b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食我肉者使病不死。",
        "from": "T21n1331_009:0521b09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萎黃困篤從死得生。",
        "from": "T21n1331_009:0521b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殺我命者毒著便死。",
        "from": "T21n1331_009:0521b10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今先敘彼諸因緣。",
        "from": "T21n1331_009:0521b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後請召五方諸大龍王勅語之曰。",
        "from": "T21n1331_009:0521b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫令諸小毒龍害於人民。",
        "from": "T21n1331_009:0521b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各使攝毒還其處所。",
        "from": "T21n1331_009:0521b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解於罪福攝取惡毒。",
        "from": "T21n1331_009:0521b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不復為害令一切人民。",
        "from": "T21n1331_009:0521b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得離病惱無復厄難。",
        "from": "T21n1331_009:0521b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拔度生死得涅槃道。",
        "from": "T21n1331_009:0521b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難白佛唯願速說五方龍王名字隨方所攝諸小龍輩。",
        "from": "T21n1331_009:0521b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吐惡毒氣害於人者。",
        "from": "T21n1331_009:0521b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使其檢校不行諸毒。",
        "from": "T21n1331_009:0521b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟耶離國受苦如是。",
        "from": "T21n1331_009:0521b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊一一解說。",
        "from": "T21n1331_009:0521b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼眾生得脫厄難。",
        "from": "T21n1331_009:0521b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解脫彼苦得聞法音。",
        "from": "T21n1331_009:0521b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告阿難汝當諦聽諦受。",
        "from": "T21n1331_009:0521b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東方青龍神王。",
        "from": "T21n1331_009:0521b20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其上首者名曰阿修訶。",
        "from": "T21n1331_009:0521b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七七四十九龍王。",
        "from": "T21n1331_009:0521b21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "典領東方小龍伴侶七十萬億。",
        "from": "T21n1331_009:0521b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "山精雜魅毒病厄難。",
        "from": "T21n1331_009:0521b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆當說其名字護病者身。",
        "from": "T21n1331_009:0521b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能使小龍攝毒不害病者。",
        "from": "T21n1331_009:0521b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使身中諸毒自然消滅。",
        "from": "T21n1331_009:0521b24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "病愈熱除平復如本。",
        "from": "T21n1331_009:0521b25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛圖那龍王　三物都路龍王　娑攬摩龍王　三物弗路龍王　因臺羅龍王　婆伽樓龍王　摩伽婁龍王　難陀那龍王　優鉢難陀龍王　修陀利舍龍王　婆修只龍王　德叉迦龍王　阿婁盧龍王　婆婁盧龍王　伽婁盧龍王　沙梁瞿龍王　思利曼陀龍王　思利健陀龍王　思利婆栗陀奴龍王　思梨颰陀龍王　阿婆盧龍王　思婆盧龍王　修婆睺龍王　須彌睺龍王　修彌弗多羅龍王　旃陀羅弗多羅龍王　那栗陀奴龍王　伽栗闍奴龍王　苾闍陀奴龍王　思普陀奴龍王　婆梨沙奴龍王　毘摩婁龍王　阿梨伽思梨沙龍王　婆伽梨思龍王　竭波思梨沙龍王　伽婆思梨沙龍王　無勒思利龍王　害坻思梨沙龍王　竭陀羅婆梨俱龍王　闍那栗陀龍王　質多斯龍王　質多勒叉龍王　那某旨龍王　阿羅婆婁龍王　某旨隣陀龍王　阿勒伽浮龍王　思利龍王　思梨祇吉龍王　攬菩盧龍王佛告阿難南方赤龍神王。",
        "from": "T21n1331_009:0521c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其上首者名曰那頭化提。",
        "from": "T21n1331_009:0521c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五七三十五龍王。",
        "from": "T21n1331_009:0521c19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "典領南方五十萬億諸山精魅鬼。",
        "from": "T21n1331_009:0521c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十萬億弊小龍輩。",
        "from": "T21n1331_009:0521c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吐毒氣者害於人民。",
        "from": "T21n1331_009:0521c21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令其攝毒莫復害於人民。",
        "from": "T21n1331_009:0521c21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有得毒病者。",
        "from": "T21n1331_009:0521c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆當呼此龍王名字令諸萬姓休息安寧病消熱除。",
        "from": "T21n1331_009:0521c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍王施命平復如本。",
        "from": "T21n1331_009:0521c23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "只利彌龍王　阿那俱龍王　阿難陀婁龍王　害多蓋朱龍王　慢陀俱龍王　氷伽婁龍王　伊羅颰陀羅龍王　傷瞿龍王　阿婆羅婁龍王　阿多俱龍王　優婆伽多俱龍王　波羅提浮龍王　那羅耶㝹龍王　達婆婁龍王　毘無龍王　阿勒叉龍王　思羅婆睺龍王　彊伽那龍王　思度龍王　薄芻龍王　思陀龍王　茫伽婁龍王　毘大都龍王　修波羅坻度龍王　毘羅婆奴龍王　陀羅難陀龍王　颰陀羅龍王　尼彌陀婁龍王　修颰陀羅龍王　修梵陀羅龍王　波羅旃陀羅龍王　慢陀奴龍王　摩根陀羅龍王　陀婆伽羅龍王　徒菩北多俱龍王佛告阿難西方白龍神王。",
        "from": "T21n1331_009:0522a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其上首者名曰訶樓薩叉提。",
        "from": "T21n1331_009:0522a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三七二十一龍王。",
        "from": "T21n1331_009:0522a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "典領西方九十萬億諸小龍輩。",
        "from": "T21n1331_009:0522a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及山精魅鬼二十萬億。",
        "from": "T21n1331_009:0522a10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有疾急者喚是龍王名字之時。",
        "from": "T21n1331_009:0522a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸小龍各攝毒惡。",
        "from": "T21n1331_009:0522a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不害萬民長得歡樂。",
        "from": "T21n1331_009:0522a12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾病除愈平復如本。",
        "from": "T21n1331_009:0522a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徒菩魯嘻多俱龍王　摩那利龍王　斯為多俱龍王　阿勒多龍王　敗兜多羅龍王　颰陀羅波兜龍王　肫頭毘龍王　安婆羅坻陀羅龍王　半陀修都龍王　提梨坻賴兜龍王　毘婁勒俱龍王　毘婁博叉龍王　毘舍羅摩龍王　奢伽某伽龍王　旃陀婆那龍王　瞿陀無那龍王　半闍婁龍王　半闍周婁龍王　波羅獨都龍王　便頭龍王　優婆便頭龍王佛告阿難北方黑龍神王　其上首者名曰那業提婁。",
        "from": "T21n1331_009:0522a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四七二十八龍王。",
        "from": "T21n1331_009:0522a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "典領北方百億諸小龍輩。",
        "from": "T21n1331_009:0522a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及山精魅鬼十三萬億。",
        "from": "T21n1331_009:0522a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒病疾厄恐怖之日。",
        "from": "T21n1331_009:0522a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喚是龍王名字之時。",
        "from": "T21n1331_009:0522a26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸小龍輩各攝毒氣。",
        "from": "T21n1331_009:0522a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不復害人萬民安樂。",
        "from": "T21n1331_009:0522a27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "病苦消除眾善補處。",
        "from": "T21n1331_009:0522a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平復如本龍王力也。",
        "from": "T21n1331_009:0522a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "波利俱龍王　只遮尼俱龍王　伽闍那俱龍王　深林婆俱龍王　某栗那瞿曇龍王　某羅摩奴須龍王　慰多羅摩㝹婁龍王　阿摩㝹婁龍王　佉伽婁那龍王　曼陀羅俱龍王　慰多祺龍王　阿羅婆羅龍王　摩羅摩㝹龍王　毘住龍王　伊他婆婁龍王　摩那思龍王　只羅哆俱龍王　伽毘盧龍王　思婆婁龍王　慰婆婁龍王　恒伽懼龍王　颰陀摩㝹龍王　木叉俱龍王　婆羅木叉龍王　俱佛提龍王　甘婆羅思和婁龍王　伊羅彌丘龍王　難陀優婆難陀喻龍王佛告阿難中央黃龍神王。",
        "from": "T21n1331_009:0522b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其上首者名曰闍羅波提。",
        "from": "T21n1331_009:0522b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四七二十八龍王。",
        "from": "T21n1331_009:0522b13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "典領中央諸小龍輩六十萬億。",
        "from": "T21n1331_009:0522b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及山精雜魅鬼十二萬億。",
        "from": "T21n1331_009:0522b14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若諸人民悲憂衰惱。",
        "from": "T21n1331_009:0522b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變怪眾災水火之厄諸怪屢生。",
        "from": "T21n1331_009:0522b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦當應寫上龍王名字。",
        "from": "T21n1331_009:0522b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各隨所主。",
        "from": "T21n1331_009:0522b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "厭除萬怪令其消滅。",
        "from": "T21n1331_009:0522b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呼是龍王名字之時。",
        "from": "T21n1331_009:0522b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸小龍山精雜魅。",
        "from": "T21n1331_009:0522b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即便隱毒不復害人。",
        "from": "T21n1331_009:0522b18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬民歡樂國土安寧。",
        "from": "T21n1331_009:0522b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雨澤以時民無荒亂。",
        "from": "T21n1331_009:0522b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人王喜悅稱善無量。",
        "from": "T21n1331_009:0522b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿周都龍王　波利伽留龍王　修某丘阿陀含龍王　健陀阿婁龍王　僧伽婁龍王　提梨彌阿婁龍王　周修訶龍王　那頭伽提那龍王　訶數薩伽提龍王　那僕提婁龍王　那速提龍王　薩迦陀那龍王　優耨薩龍王　禪然薩龍王　阿宿提龍王　娑伽羅龍王　和修吉龍王　阿那婆達多龍王　薩迦陀那伽龍王　優耨達龍王　禪然薩龍王　阿宿羅龍王　娑伽羅龍王　和修周龍王　阿那婆達龍王　摩那斯龍王　難陀龍王　颰難陀龍王佛告阿難是為請召龍王名字如是。",
        "from": "T21n1331_009:0522c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我勅諸方大龍王等。",
        "from": "T21n1331_009:0522c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及山精魅鬼攝諸小龍。",
        "from": "T21n1331_009:0522c05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五色之氣雲霧毒害侵陵萬民。",
        "from": "T21n1331_009:0522c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我以勅竟。",
        "from": "T21n1331_009:0522c06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍王受教魅鬼亦然。",
        "from": "T21n1331_009:0522c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便攝小龍使不行毒害於人也。",
        "from": "T21n1331_009:0522c07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍王歡喜受吾教命歡喜奉行。",
        "from": "T21n1331_009:0522c08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告阿難我說是五方龍神王竟。",
        "from": "T21n1331_009:0522c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此眾中誰能施行。",
        "from": "T21n1331_009:0522c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "召五方一百六十一諸龍王名。",
        "from": "T21n1331_009:0522c10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逐諸小龍及山精魅鬼。",
        "from": "T21n1331_009:0522c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使攝其毒不復得行嬈於人民。",
        "from": "T21n1331_009:0522c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使諸國土無諸苦者。",
        "from": "T21n1331_009:0522c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒氣既除人民歡樂。",
        "from": "T21n1331_009:0522c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我法興隆魔道隱閉諸毒消滅。",
        "from": "T21n1331_009:0522c13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖眾能化靡不受行。",
        "from": "T21n1331_009:0522c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時佛世尊唱是語已。",
        "from": "T21n1331_009:0522c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大眾寂然默無言者。",
        "from": "T21n1331_009:0522c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是眾中有一少年比丘名曰禪提。",
        "from": "T21n1331_009:0522c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從坐而起胡跪合掌。",
        "from": "T21n1331_009:0522c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而白佛言世尊。",
        "from": "T21n1331_009:0522c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難今日於大眾中請於如來。",
        "from": "T21n1331_009:0522c17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是五方召龍神呪。",
        "from": "T21n1331_009:0522c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊以說召龍王竟。",
        "from": "T21n1331_009:0522c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲令諸比丘輩宣天尊言。",
        "from": "T21n1331_009:0522c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使此法言流演世間。",
        "from": "T21n1331_009:0522c19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪提比丘不以愚憃。",
        "from": "T21n1331_009:0522c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今當承佛威神之力。",
        "from": "T21n1331_009:0522c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往彼維耶離大城之中。",
        "from": "T21n1331_009:0522c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說今如來所說龍王無上神呪。",
        "from": "T21n1331_009:0522c21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是語已受天尊教禮佛而去。",
        "from": "T21n1331_009:0522c22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是禪提比丘。",
        "from": "T21n1331_009:0522c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往彼維耶離大國之中。",
        "from": "T21n1331_009:0522c23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救治人民諸疾病者悉得除愈。",
        "from": "T21n1331_009:0522c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百姓歡樂喜慶無極。",
        "from": "T21n1331_009:0522c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是漸漸國王大臣。",
        "from": "T21n1331_009:0522c25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長者居士豪姓之屬。",
        "from": "T21n1331_009:0522c25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆供養為起精舍。",
        "from": "T21n1331_009:0522c26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四事供養無所乏少。",
        "from": "T21n1331_009:0522c26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人民無憂毒氣不行。",
        "from": "T21n1331_009:0522c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時是禪提比丘。",
        "from": "T21n1331_009:0522c28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住是國中二十九年。",
        "from": "T21n1331_009:0522c28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以佛世尊所說召龍摩訶神呪。",
        "from": "T21n1331_009:0522c29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化導人民悉皆奉受。",
        "from": "T21n1331_009:0523a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常為四輩之所供養。",
        "from": "T21n1331_009:0523a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至命終未曾休息。",
        "from": "T21n1331_009:0523a01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是禪提比丘命終已後。",
        "from": "T21n1331_009:0523a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸弟子輩以香樵薪。",
        "from": "T21n1331_009:0523a02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "焚燒其身收取舍利起于塔廟。",
        "from": "T21n1331_009:0523a03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪提比丘命終之後。",
        "from": "T21n1331_009:0523a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒氣復興疫病眾多。",
        "from": "T21n1331_009:0523a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "死亡者無數無人救療。",
        "from": "T21n1331_009:0523a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸人民作是念言。",
        "from": "T21n1331_009:0523a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憶我本昔有禪提比丘。",
        "from": "T21n1331_009:0523a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦說如來召龍神呪。",
        "from": "T21n1331_009:0523a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設有病苦皆悉除愈。",
        "from": "T21n1331_009:0523a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今已命過。",
        "from": "T21n1331_009:0523a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今遭諸厄難誰救我者。",
        "from": "T21n1331_009:0523a07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即便奔趣往至禪提先精舍所。",
        "from": "T21n1331_009:0523a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往彼住處。",
        "from": "T21n1331_009:0523a08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見禪提比丘所嚼楊枝。",
        "from": "T21n1331_009:0523a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擲地成樹樹下有清泉水。",
        "from": "T21n1331_009:0523a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸人民輩即禮拜此樹。",
        "from": "T21n1331_009:0523a10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如見禪提在世無異。",
        "from": "T21n1331_009:0523a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "折此楊枝取下泉水還歸到家。",
        "from": "T21n1331_009:0523a11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以楊柳枝拂除病者。",
        "from": "T21n1331_009:0523a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以水灑諸病人輩悉得休息。",
        "from": "T21n1331_009:0523a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身體清涼百病除愈。",
        "from": "T21n1331_009:0523a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是國中國王大臣。",
        "from": "T21n1331_009:0523a13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長者居士四輩人民。",
        "from": "T21n1331_009:0523a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有病苦者悉皆往彼故精舍所。",
        "from": "T21n1331_009:0523a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取此楊枝並取泉水。",
        "from": "T21n1331_009:0523a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "浴洗病者灑散五方。",
        "from": "T21n1331_009:0523a15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸魔惡鬼毒鬼消亡。",
        "from": "T21n1331_009:0523a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辟除眾惡萬事吉祥。",
        "from": "T21n1331_009:0523a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢者阿難語諸四輩言。",
        "from": "T21n1331_009:0523a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不但禪提比丘。",
        "from": "T21n1331_009:0523a17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行是神呪有效驗也。",
        "from": "T21n1331_009:0523a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔南天竺有一居士名曰求若先。",
        "from": "T21n1331_009:0523a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉持五戒守行十善。",
        "from": "T21n1331_009:0523a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦復誦是如來所說召龍神呪。",
        "from": "T21n1331_009:0523a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時世飢窮。",
        "from": "T21n1331_009:0523a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求若先居士遊他國土以求衣食。",
        "from": "T21n1331_009:0523a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漸漸遊行至迦羅葉國中。",
        "from": "T21n1331_009:0523a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往釋印迦羅越舍。",
        "from": "T21n1331_009:0523a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時是長者家門之中一百餘口。",
        "from": "T21n1331_009:0523a22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為此雲氣毒病所羅。",
        "from": "T21n1331_009:0523a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得病之人有死有蘇。",
        "from": "T21n1331_009:0523a23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居士既到諸有病者悉得除愈。",
        "from": "T21n1331_009:0523a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋印迦羅越。",
        "from": "T21n1331_009:0523a24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耳自得聞諸鬼神輩。",
        "from": "T21n1331_009:0523a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更相語言我前本時。",
        "from": "T21n1331_009:0523a25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲令此國疫氣流行。",
        "from": "T21n1331_009:0523a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "病者使死不令得活。",
        "from": "T21n1331_009:0523a26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今者遠方有神人居士從天竺來。",
        "from": "T21n1331_009:0523a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口中誦習如來所說召龍灌頂微妙法術。",
        "from": "T21n1331_009:0523a28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今當往無人之處。",
        "from": "T21n1331_009:0523a28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝取毒氣不能得行。",
        "from": "T21n1331_009:0523a29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一鬼語言何故爾乎。",
        "from": "T21n1331_009:0523a29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一鬼答言迦羅越舍車屋之中。",
        "from": "T21n1331_009:0523b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有此神人誦習呪術。",
        "from": "T21n1331_009:0523b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本欲行毒而今不能。",
        "from": "T21n1331_009:0523b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不去何待。",
        "from": "T21n1331_009:0523b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若留在此。",
        "from": "T21n1331_009:0523b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為彼神人之所傷害。",
        "from": "T21n1331_009:0523b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是以後諸鬼神輩各各惶怖。",
        "from": "T21n1331_009:0523b04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩兩相牽三三相隨。",
        "from": "T21n1331_009:0523b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五五相逐馳走而去。",
        "from": "T21n1331_009:0523b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往彼山谷無人之處。",
        "from": "T21n1331_009:0523b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各自隱藏不現身形。",
        "from": "T21n1331_009:0523b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是以後毒氣不行。",
        "from": "T21n1331_009:0523b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國中人民有病苦者悉得差愈。",
        "from": "T21n1331_009:0523b07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國王流聞迦羅越舍有是神人。",
        "from": "T21n1331_009:0523b08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即便嚴駕車乘往到其所。",
        "from": "T21n1331_009:0523b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到已前拜而問訊言。",
        "from": "T21n1331_009:0523b09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何來居士乃有如此微妙聖術。",
        "from": "T21n1331_009:0523b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居士答言南天竺來。",
        "from": "T21n1331_009:0523b10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居士名何。",
        "from": "T21n1331_009:0523b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答言求若先。",
        "from": "T21n1331_009:0523b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王將居士往到宮中。",
        "from": "T21n1331_009:0523b11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七日七夜作倡妓樂而娛樂之。",
        "from": "T21n1331_009:0523b12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設種種美膳金銀珍寶衣服數通而施與之。",
        "from": "T21n1331_009:0523b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求若居士為王說法示教利喜。",
        "from": "T21n1331_009:0523b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王聞法音喜踊無量。",
        "from": "T21n1331_009:0523b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國中人民無不獲福。",
        "from": "T21n1331_009:0523b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王於是後遂與居士相將。",
        "from": "T21n1331_009:0523b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往到竹林精舍禮拜世尊。",
        "from": "T21n1331_009:0523b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作禮畢訖却坐一面。",
        "from": "T21n1331_009:0523b16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛為說法。",
        "from": "T21n1331_009:0523b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王及臣民靡不歡喜。",
        "from": "T21n1331_009:0523b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各隨本緣悉得道迹。",
        "from": "T21n1331_009:0523b17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是阿難又告四眾。",
        "from": "T21n1331_009:0523b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今佛世尊說是灌頂召龍神呪。",
        "from": "T21n1331_009:0523b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不但為今維耶離也。",
        "from": "T21n1331_009:0523b19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "標心乃在像法之中。",
        "from": "T21n1331_009:0523b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千歲之末佛法欲滅魔道興盛。",
        "from": "T21n1331_009:0523b20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當有惡王斷滅三寶。",
        "from": "T21n1331_009:0523b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使法言不通破塔滅僧五濁亂時。",
        "from": "T21n1331_009:0523b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為是當來諸眾生輩。",
        "from": "T21n1331_009:0523b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "演說此法示於未聞。",
        "from": "T21n1331_009:0523b22_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普使宣傳流布世間。",
        "from": "T21n1331_009:0523b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人民受者不遭患難。",
        "from": "T21n1331_009:0523b23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾病除愈死得昇天。",
        "from": "T21n1331_009:0523b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難又言諸四輩弟子。",
        "from": "T21n1331_009:0523b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有國土有疾厄者。",
        "from": "T21n1331_009:0523b25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "橫為邪神諸惡龍輩。",
        "from": "T21n1331_009:0523b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸山精銜火燒人。",
        "from": "T21n1331_009:0523b26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又吐惡毒氣侵陵萬姓。",
        "from": "T21n1331_009:0523b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有此災變不吉祥時。",
        "from": "T21n1331_009:0523b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當澡嗽口齒清淨受行齋戒。",
        "from": "T21n1331_009:0523b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不食五辛不得飲酒噉肉。",
        "from": "T21n1331_009:0523b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當禮敬十方諸佛。",
        "from": "T21n1331_009:0523b29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懸繒旛蓋請召眾僧。",
        "from": "T21n1331_009:0523c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日七遍轉是召龍大神呪經。",
        "from": "T21n1331_009:0523c01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "邪氣雲除眾善集身。",
        "from": "T21n1331_009:0523c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解釋病者熱毒消除。",
        "from": "T21n1331_009:0523c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "病苦得愈得吉祥福。",
        "from": "T21n1331_009:0523c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢者阿難又復告言。",
        "from": "T21n1331_009:0523c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸四輩弟子若疾厄之日。",
        "from": "T21n1331_009:0523c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當以香泥泥地。",
        "from": "T21n1331_009:0523c04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然十方燈散雜色華。",
        "from": "T21n1331_009:0523c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒眾雜香兜婁婆畢力伽沈水膠香婆香安息香等。",
        "from": "T21n1331_009:0523c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒是香時亦能使魔隱藏不現不復害人。",
        "from": "T21n1331_009:0523c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得吉祥萬病除愈。",
        "from": "T21n1331_009:0523c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說經竟。",
        "from": "T21n1331_009:0523c07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四眾人民無不歡喜。",
        "from": "T21n1331_009:0523c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋梵諸天以好香華。",
        "from": "T21n1331_009:0523c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供散佛上以為供養。",
        "from": "T21n1331_009:0523c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難問佛言。",
        "from": "T21n1331_009:0523c09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是語已當何名之云何奉行。",
        "from": "T21n1331_009:0523c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言阿難此經名為灌頂召龍大神呪經。",
        "from": "T21n1331_009:0523c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又名行經之人因緣本事經如法受持大眾人民天龍鬼神聞經歡喜作禮奉行。",
        "from": "T21n1331_009:0523c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灌頂經卷第九",
        "from": "T21n1331_009:0523c14_100",
        "context_with_next_sentence": false
    }
]