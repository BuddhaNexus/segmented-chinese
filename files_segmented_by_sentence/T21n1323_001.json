[
    {
        "source_sentence": "除一切疾病陀羅尼經",
        "from": "T21n1323_001:0489c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T21n1323_001:0489c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空諡大鑑",
        "from": "T21n1323_001:0489c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正號大廣智大興善寺三藏沙門不空奉　詔譯如是我聞。",
        "from": "T21n1323_001:0489c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時薄伽梵。",
        "from": "T21n1323_001:0489c12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住室羅伐城逝多林給孤長者園。",
        "from": "T21n1323_001:0489c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大苾芻眾千二百五十人俱。",
        "from": "T21n1323_001:0489c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾多諸大菩薩摩訶薩。",
        "from": "T21n1323_001:0489c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告阿難陀言。",
        "from": "T21n1323_001:0489c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難陀有陀羅尼。",
        "from": "T21n1323_001:0489c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除世間一切疾病。",
        "from": "T21n1323_001:0489c15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當受持讀誦通利如理作意。",
        "from": "T21n1323_001:0489c16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說密言曰。",
        "from": "T21n1323_001:0489c16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也二合他一尾摩黎尾摩黎二嚩曩俱枳黎三室唎二合末底丁以反四軍拏黎五嫩奴鼻六印捺囉二合儗𩕳二合七母隷娑嚩二合訶佛告阿難陀。",
        "from": "T21n1323_001:0489c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此陀羅尼若誦持者。",
        "from": "T21n1323_001:0489c20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宿食不消癨亂風黃痰癊。",
        "from": "T21n1323_001:0489c21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "患痔瘻淋上氣。",
        "from": "T21n1323_001:0489c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嗽虐寒熱頭痛半痛。",
        "from": "T21n1323_001:0489c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著鬼魅者悉得除差。",
        "from": "T21n1323_001:0489c22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我以佛眼觀見彼人。",
        "from": "T21n1323_001:0489c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天魔梵沙門婆羅門能作障難。",
        "from": "T21n1323_001:0489c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除非決定業報盡者。",
        "from": "T21n1323_001:0489c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘無能違越作其障難。",
        "from": "T21n1323_001:0489c24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來應供正遍知說。",
        "from": "T21n1323_001:0489c25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切有情中如來為尊勝。",
        "from": "T21n1323_001:0489c25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切法中離欲法尊。",
        "from": "T21n1323_001:0489c26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾中僧伽為尊。",
        "from": "T21n1323_001:0489c26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此誠實言願我及一切有情。",
        "from": "T21n1323_001:0489c27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食飲喫啖入腹消化得正安樂。",
        "from": "T21n1323_001:0489c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娑嚩二合引訶引。",
        "from": "T21n1323_001:0489c28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊說是經已。",
        "from": "T21n1323_001:0489c29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸苾芻僧并諸菩薩摩訶薩。",
        "from": "T21n1323_001:0490a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切大眾天龍八部。",
        "from": "T21n1323_001:0490a01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持佛語歡喜奉行。",
        "from": "T21n1323_001:0490a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除一切疾病陀羅尼經",
        "from": "T21n1323_001:0490a03_100",
        "context_with_next_sentence": false
    }
]