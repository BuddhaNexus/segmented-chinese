[
    {
        "source_sentence": "佛說無量大慈教經",
        "from": "T85n2903_001:1445a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有。爾時阿難白佛言。世尊。唯有闡提眾生。",
        "from": "T85n2903_001:1445a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難化難育。爾時世尊答。如牛耕田由人所遣。",
        "from": "T85n2903_001:1445a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獼猴作舞由人所教。",
        "from": "T85n2903_001:1445a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造罪眾生由人所勸。",
        "from": "T85n2903_001:1445a14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時阿難白佛言。",
        "from": "T85n2903_001:1445a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "T85n2903_001:1445a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生藥生西方國者。",
        "from": "T85n2903_001:1445a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣我身及向我口眼中不淨我亦不辭。",
        "from": "T85n2903_001:1445a16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語阿難。",
        "from": "T85n2903_001:1445a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經大聖若有眾生勤誦此經者。",
        "from": "T85n2903_001:1445a17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "免得三難苦。",
        "from": "T85n2903_001:1445a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一難禁囚。",
        "from": "T85n2903_001:1445a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二難病人。",
        "from": "T85n2903_001:1445a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三難地獄。",
        "from": "T85n2903_001:1445a18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告言。",
        "from": "T85n2903_001:1445a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難。",
        "from": "T85n2903_001:1445a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為閻浮眾生難化難育故。",
        "from": "T85n2903_001:1445a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開此經藏出其殊別。",
        "from": "T85n2903_001:1445a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙經為汝演說。",
        "from": "T85n2903_001:1445a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人現此經者不生清淨。",
        "from": "T85n2903_001:1445a21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知是人與我無業。",
        "from": "T85n2903_001:1445a21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切經典廣說妙言。",
        "from": "T85n2903_001:1445a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "引度眾生。",
        "from": "T85n2903_001:1445a22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一稱我名者。",
        "from": "T85n2903_001:1445a22_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉聞悉見。",
        "from": "T85n2903_001:1445a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨聲往救。",
        "from": "T85n2903_001:1445a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今身劫剝師僧者死墮寒氷地獄。",
        "from": "T85n2903_001:1445a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又生蠶中為他煮剝。",
        "from": "T85n2903_001:1445a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如此等以逕八萬之劫。",
        "from": "T85n2903_001:1445a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘受畜生身。",
        "from": "T85n2903_001:1445a25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以逕五百劫。",
        "from": "T85n2903_001:1445a25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蠢動眾生皆悉作遍。",
        "from": "T85n2903_001:1445a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後受人身。",
        "from": "T85n2903_001:1445a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "𤸇殘盲跛。",
        "from": "T85n2903_001:1445a26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五百劫中恒受啞報。",
        "from": "T85n2903_001:1445a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊答言。",
        "from": "T85n2903_001:1445a27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難。",
        "from": "T85n2903_001:1445a27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行不淨行尼僧死墮鐵窟中地獄。",
        "from": "T85n2903_001:1445a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八萬刀輪一時來下。",
        "from": "T85n2903_001:1445a28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斬截其身。",
        "from": "T85n2903_001:1445a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時阿難白佛言。",
        "from": "T85n2903_001:1445a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如佛思量。",
        "from": "T85n2903_001:1445a29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此事超越三界。",
        "from": "T85n2903_001:1445b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊語阿難。",
        "from": "T85n2903_001:1445b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲酒醉亂死墮飲銅地獄中。",
        "from": "T85n2903_001:1445b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時阿難白佛言。",
        "from": "T85n2903_001:1445b02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "T85n2903_001:1445b02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酒之無命。",
        "from": "T85n2903_001:1445b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何故戒之。",
        "from": "T85n2903_001:1445b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊復語阿難。",
        "from": "T85n2903_001:1445b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲酒醉亂。",
        "from": "T85n2903_001:1445b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不識尊親。",
        "from": "T85n2903_001:1445b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我見振旦國有人因酒婬母。",
        "from": "T85n2903_001:1445b04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破其五戒。",
        "from": "T85n2903_001:1445b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是以禁之。",
        "from": "T85n2903_001:1445b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊語諸大菩薩等。",
        "from": "T85n2903_001:1445b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我從成佛以來。",
        "from": "T85n2903_001:1445b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於今五百餘劫。",
        "from": "T85n2903_001:1445b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經典披遍。",
        "from": "T85n2903_001:1445b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讀誦周匝。",
        "from": "T85n2903_001:1445b07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未聞是言。",
        "from": "T85n2903_001:1445b07_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未聞是說。",
        "from": "T85n2903_001:1445b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生聞此經者。",
        "from": "T85n2903_001:1445b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宿種善因。",
        "from": "T85n2903_001:1445b08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宿種善果。",
        "from": "T85n2903_001:1445b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思尋此經。",
        "from": "T85n2903_001:1445b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可思量。",
        "from": "T85n2903_001:1445b09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可稱盡。",
        "from": "T85n2903_001:1445b09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時如來語諸菩薩。",
        "from": "T85n2903_001:1445b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我說此語。",
        "from": "T85n2903_001:1445b10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重如太山。",
        "from": "T85n2903_001:1445b10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生聞者輕如微塵。",
        "from": "T85n2903_001:1445b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此法難聞亦復難見。",
        "from": "T85n2903_001:1445b11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語阿難。",
        "from": "T85n2903_001:1445b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生聞此經者。",
        "from": "T85n2903_001:1445b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心生歎喜。",
        "from": "T85n2903_001:1445b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如此之人盡心為說。",
        "from": "T85n2903_001:1445b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊復語阿難。",
        "from": "T85n2903_001:1445b13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我見眾生墮落三塗。",
        "from": "T85n2903_001:1445b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若將刀割。",
        "from": "T85n2903_001:1445b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我身體由斯可忍。",
        "from": "T85n2903_001:1445b14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我不忍見於眾生受大苦惱。",
        "from": "T85n2903_001:1445b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酸悲忍苦。",
        "from": "T85n2903_001:1445b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身上演出光明。",
        "from": "T85n2903_001:1445b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍照十方。",
        "from": "T85n2903_001:1445b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有我緣者得見我身。",
        "from": "T85n2903_001:1445b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有緣者不見我想爾時阿難白佛言。",
        "from": "T85n2903_001:1445b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "T85n2903_001:1445b17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如何二種心。",
        "from": "T85n2903_001:1445b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一種眾生則有見者。",
        "from": "T85n2903_001:1445b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則無見者。",
        "from": "T85n2903_001:1445b18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時如來復語阿難。",
        "from": "T85n2903_001:1445b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日月普照。",
        "from": "T85n2903_001:1445b19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盲者不見。",
        "from": "T85n2903_001:1445b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時阿難重白。",
        "from": "T85n2903_001:1445b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "T85n2903_001:1445b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何是盲。",
        "from": "T85n2903_001:1445b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何是明。",
        "from": "T85n2903_001:1445b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊答言。",
        "from": "T85n2903_001:1445b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難。",
        "from": "T85n2903_001:1445b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修福者是明。",
        "from": "T85n2903_001:1445b21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不修福者是盲。",
        "from": "T85n2903_001:1445b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難復白佛言。",
        "from": "T85n2903_001:1445b22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "T85n2903_001:1445b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修福者不見由何。",
        "from": "T85n2903_001:1445b22_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊告阿難。",
        "from": "T85n2903_001:1445b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修福不見我身。",
        "from": "T85n2903_001:1445b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "障閉佛光污埿三寶。",
        "from": "T85n2903_001:1445b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是以不得見我。",
        "from": "T85n2903_001:1445b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語菩薩。",
        "from": "T85n2903_001:1445b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞是此經者心生歡喜。",
        "from": "T85n2903_001:1445b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如子見母遠行得歸。",
        "from": "T85n2903_001:1445b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如飢得食如渴得漿。",
        "from": "T85n2903_001:1445b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如此之人盡心為說。",
        "from": "T85n2903_001:1445b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語阿難。",
        "from": "T85n2903_001:1445b26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食肉之者喻如群狗。",
        "from": "T85n2903_001:1445b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爭骨各各貪多見。",
        "from": "T85n2903_001:1445b27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其猪羊常作殺想見。",
        "from": "T85n2903_001:1445b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其臭肉如猫趣鼠。",
        "from": "T85n2903_001:1445b28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惠心用意。",
        "from": "T85n2903_001:1445b28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今身信解佛法者。",
        "from": "T85n2903_001:1445b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從人中來。",
        "from": "T85n2903_001:1445b29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今身不信佛者。",
        "from": "T85n2903_001:1445b29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從畜生中來。",
        "from": "T85n2903_001:1445c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造罪不懺悔者。",
        "from": "T85n2903_001:1445c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喻如運金填國損其寶物。",
        "from": "T85n2903_001:1445c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造罪悔者如病得藥還復差。",
        "from": "T85n2903_001:1445c02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "損汗衣水洗還得清淨。",
        "from": "T85n2903_001:1445c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語普廣菩薩。",
        "from": "T85n2903_001:1445c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用我語者一偈成佛。",
        "from": "T85n2903_001:1445c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不信我者喻如海中求針。",
        "from": "T85n2903_001:1445c04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "狂費功力無得見曰。",
        "from": "T85n2903_001:1445c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語諸菩薩。",
        "from": "T85n2903_001:1445c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今身盜他物者。",
        "from": "T85n2903_001:1445c05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來生與他作癡牛令他苦打。",
        "from": "T85n2903_001:1445c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非時苦使受牛身。",
        "from": "T85n2903_001:1445c06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以遙五百劫與他作奴。",
        "from": "T85n2903_001:1445c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "任他駈使心。",
        "from": "T85n2903_001:1445c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是逃避令被捉得苦刑。",
        "from": "T85n2903_001:1445c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印面非理苦持。",
        "from": "T85n2903_001:1445c08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語菩薩。",
        "from": "T85n2903_001:1445c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伽藍中二種心。",
        "from": "T85n2903_001:1445c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者善心。",
        "from": "T85n2903_001:1445c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者惡心。",
        "from": "T85n2903_001:1445c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為惡人。",
        "from": "T85n2903_001:1445c09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生入寺之時。",
        "from": "T85n2903_001:1445c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯從眾僧乞索。",
        "from": "T85n2903_001:1445c10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或求僧長短或噉德食都無慚愧。",
        "from": "T85n2903_001:1445c11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餅果菜茹懷狹歸家。",
        "from": "T85n2903_001:1445c11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如此之死墮鐵叉地獄。",
        "from": "T85n2903_001:1445c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名為善人。",
        "from": "T85n2903_001:1445c12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生入寺之時。",
        "from": "T85n2903_001:1445c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見僧恭敬。",
        "from": "T85n2903_001:1445c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見佛禮拜。",
        "from": "T85n2903_001:1445c13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受戒懺悔。",
        "from": "T85n2903_001:1445c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨於財物。",
        "from": "T85n2903_001:1445c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經營三寶。",
        "from": "T85n2903_001:1445c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不惜身命。",
        "from": "T85n2903_001:1445c14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護持大法。",
        "from": "T85n2903_001:1445c14_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如此之人舉足一步。",
        "from": "T85n2903_001:1445c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天堂自來受果。",
        "from": "T85n2903_001:1445c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如樹提伽。",
        "from": "T85n2903_001:1445c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提伽則名為最上善人也。",
        "from": "T85n2903_001:1445c16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告大眾。",
        "from": "T85n2903_001:1445c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我向所論種種因果。",
        "from": "T85n2903_001:1445c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經一名殊別。",
        "from": "T85n2903_001:1445c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二名殊勝。",
        "from": "T85n2903_001:1445c17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三名菩薩。",
        "from": "T85n2903_001:1445c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生聞此經者。",
        "from": "T85n2903_001:1445c18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一發善心得生淨土。",
        "from": "T85n2903_001:1445c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告菩薩。",
        "from": "T85n2903_001:1445c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞我說者心生歎喜。",
        "from": "T85n2903_001:1445c19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如旱得水。",
        "from": "T85n2903_001:1445c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苗稼蘇活。",
        "from": "T85n2903_001:1445c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不受我語者如石浸水。",
        "from": "T85n2903_001:1445c20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有潤時。",
        "from": "T85n2903_001:1445c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時阿難白佛言。",
        "from": "T85n2903_001:1445c21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "T85n2903_001:1445c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等見振旦國。",
        "from": "T85n2903_001:1445c21_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有人從七歲修福至於百年。",
        "from": "T85n2903_001:1445c22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨命終時破其五戒。",
        "from": "T85n2903_001:1445c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此人得福以不。",
        "from": "T85n2903_001:1445c23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊復語阿難。",
        "from": "T85n2903_001:1445c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喻如轂車。",
        "from": "T85n2903_001:1445c24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上萬里之坂。",
        "from": "T85n2903_001:1445c24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨頭飜車速本。",
        "from": "T85n2903_001:1445c25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所損何有得期。",
        "from": "T85n2903_001:1445c25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縱取少多如雲影。",
        "from": "T85n2903_001:1445c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日斥時之光喻如一口之食。",
        "from": "T85n2903_001:1445c26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能得久飽。",
        "from": "T85n2903_001:1445c26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語眾生。",
        "from": "T85n2903_001:1445c27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等廣說因緣共同。",
        "from": "T85n2903_001:1445c27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成佛普勸眾生。",
        "from": "T85n2903_001:1445c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同修淨行。",
        "from": "T85n2903_001:1445c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切世間天人阿修羅等。",
        "from": "T85n2903_001:1445c28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛所說。",
        "from": "T85n2903_001:1445c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆大歡喜作禮而去。",
        "from": "T85n2903_001:1445c29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說無量大慈教經一卷",
        "from": "T85n2903_001:1446a01_100",
        "context_with_next_sentence": false
    }
]