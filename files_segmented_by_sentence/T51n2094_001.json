[
    {
        "source_sentence": "梁京寺記",
        "from": "T51n2094_001:1024a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闕名",
        "from": "T51n2094_001:1024a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小莊嚴寺小莊嚴寺",
        "from": "T51n2094_001:1024a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梁小莊嚴寺。",
        "from": "T51n2094_001:1024a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在建業定陰里。",
        "from": "T51n2094_001:1024a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本是晉零陵王廟地。",
        "from": "T51n2094_001:1024a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天監六年度禪師起造。",
        "from": "T51n2094_001:1024a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時有邵文立者。",
        "from": "T51n2094_001:1024a21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世以烹屠為業。",
        "from": "T51n2094_001:1024a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嘗欲殺一鹿。",
        "from": "T51n2094_001:1024a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鹿跪而流淚。",
        "from": "T51n2094_001:1024a22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為不祥。",
        "from": "T51n2094_001:1024a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鹿懷一麑。",
        "from": "T51n2094_001:1024a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尋當產育。",
        "from": "T51n2094_001:1024a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就庖哀切。",
        "from": "T51n2094_001:1024a23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同被刳割。",
        "from": "T51n2094_001:1024a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因斯患疾。",
        "from": "T51n2094_001:1024a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眉鬚皆落。",
        "from": "T51n2094_001:1024a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身瘡並壞。",
        "from": "T51n2094_001:1024a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後乃深起悔責求道。",
        "from": "T51n2094_001:1024a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "度禪師發大誓願。",
        "from": "T51n2094_001:1024a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "罄捨家資迴買此地。",
        "from": "T51n2094_001:1024a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為立伽藍。",
        "from": "T51n2094_001:1024a26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同泰寺同泰寺梁武帝改年號大同。",
        "from": "T51n2094_001:1024a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起同泰寺。",
        "from": "T51n2094_001:1024a28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在臺城內。",
        "from": "T51n2094_001:1024a28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "窮竭帑藏。",
        "from": "T51n2094_001:1024a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造大佛閣七層。",
        "from": "T51n2094_001:1024a29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為火所焚。",
        "from": "T51n2094_001:1024a29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "武帝捨身施財。",
        "from": "T51n2094_001:1024b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以祈佛福。",
        "from": "T51n2094_001:1024b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自大通以後。",
        "from": "T51n2094_001:1024b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無年不幸。",
        "from": "T51n2094_001:1024b01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "興國禪寺興國禪寺梁武帝天監十三年。",
        "from": "T51n2094_001:1024b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以錢二十萬。",
        "from": "T51n2094_001:1024b03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "易定林前前岡獨龍阜。",
        "from": "T51n2094_001:1024b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以葬誌公。",
        "from": "T51n2094_001:1024b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永定公主以湯沐之資。",
        "from": "T51n2094_001:1024b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造浮圖五級於其上。",
        "from": "T51n2094_001:1024b05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十四年。",
        "from": "T51n2094_001:1024b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即塔前建開善寺。",
        "from": "T51n2094_001:1024b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昇元寺昇元寺昇元寺即瓦棺寺也。",
        "from": "T51n2094_001:1024b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在城西隅瞰江面。",
        "from": "T51n2094_001:1024b08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後踞崇岡最為古跡。",
        "from": "T51n2094_001:1024b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "累經兵火。",
        "from": "T51n2094_001:1024b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略無彷彿。",
        "from": "T51n2094_001:1024b09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "李王時昇元閣猶在。",
        "from": "T51n2094_001:1024b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃梁朝故物。",
        "from": "T51n2094_001:1024b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大愛敬寺大愛敬寺梁武帝普通元年造。",
        "from": "T51n2094_001:1024b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在蔣山之北高峯上。",
        "from": "T51n2094_001:1024b12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法寶寺法寶寺梁同泰寺基之半也。",
        "from": "T51n2094_001:1024b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建康剎錄。",
        "from": "T51n2094_001:1024b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梁武帝大通元年。",
        "from": "T51n2094_001:1024b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "創同泰寺。",
        "from": "T51n2094_001:1024b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寺處宮後。",
        "from": "T51n2094_001:1024b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別開一門。",
        "from": "T51n2094_001:1024b15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名大通門。",
        "from": "T51n2094_001:1024b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝晨夕講議。",
        "from": "T51n2094_001:1024b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多遊此門。",
        "from": "T51n2094_001:1024b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法光寺法光寺即梁之蕭帝寺。",
        "from": "T51n2094_001:1024b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舊傳。",
        "from": "T51n2094_001:1024b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天監十三年造。",
        "from": "T51n2094_001:1024b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元絳寺記云。",
        "from": "T51n2094_001:1024b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不知從昔之名。",
        "from": "T51n2094_001:1024b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故後人以帝氏目之。",
        "from": "T51n2094_001:1024b19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶林寺寶林寺梁天監中。",
        "from": "T51n2094_001:1024b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "武帝與寶公。",
        "from": "T51n2094_001:1024b21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同遊此山。",
        "from": "T51n2094_001:1024b21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見林巒殊勝。",
        "from": "T51n2094_001:1024b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命建精藍。",
        "from": "T51n2094_001:1024b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長于寺長于寺建康南五里有山岡。",
        "from": "T51n2094_001:1024b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其間平地。",
        "from": "T51n2094_001:1024b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庶民雜居。",
        "from": "T51n2094_001:1024b24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大長于．小長于．東長于．並是地名。",
        "from": "T51n2094_001:1024b25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小長于在瓦棺寺南。",
        "from": "T51n2094_001:1024b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "巷西頭出大江。",
        "from": "T51n2094_001:1024b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梁初起長于寺。",
        "from": "T51n2094_001:1024b26_18",
        "context_with_next_sentence": true
    }
]