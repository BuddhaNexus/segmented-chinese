[
    {
        "source_sentence": "觀自在菩薩如意輪念誦儀軌一",
        "from": "T20n1085_001:0203c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卷",
        "from": "T20n1085_001:0203c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T20n1085_001:0203c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空諡大鑒正號大廣智大興善寺三藏沙門不空奉　詔譯依灌頂道場經。",
        "from": "T20n1085_001:0203c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說脩陀羅尼法門。",
        "from": "T20n1085_001:0203c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求速出離生死大海。",
        "from": "T20n1085_001:0203c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應須先入諸佛如來海會灌頂道場。",
        "from": "T20n1085_001:0203c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受灌頂已。",
        "from": "T20n1085_001:0203c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發歡喜心。",
        "from": "T20n1085_001:0203c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從師親受念誦法則。",
        "from": "T20n1085_001:0203c16_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後於淨室。",
        "from": "T20n1085_001:0203c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "山林流水最為上勝。",
        "from": "T20n1085_001:0203c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建立道場安置本尊。",
        "from": "T20n1085_001:0203c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修真言者面向東方。",
        "from": "T20n1085_001:0203c18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應以瞿摩夷塗拭其地。",
        "from": "T20n1085_001:0203c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以白檀香磨為香泥。",
        "from": "T20n1085_001:0203c19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以用塗壇。",
        "from": "T20n1085_001:0203c19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或方或圓隨意大小。",
        "from": "T20n1085_001:0203c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而於壇上散諸名花。",
        "from": "T20n1085_001:0203c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒香燈明供養。",
        "from": "T20n1085_001:0203c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取二淨器盛滿香水。",
        "from": "T20n1085_001:0203c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安置壇中以用供養。",
        "from": "T20n1085_001:0203c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者澡浴或不澡浴悉無障礙。",
        "from": "T20n1085_001:0203c22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但當運心思惟觀察。",
        "from": "T20n1085_001:0203c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生本性清淨。",
        "from": "T20n1085_001:0203c23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸客塵之所覆蔽。",
        "from": "T20n1085_001:0203c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不見清淨真如法性。",
        "from": "T20n1085_001:0203c24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為令清淨。",
        "from": "T20n1085_001:0203c24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當至心蓮華合掌誦此密語真言曰。",
        "from": "T20n1085_001:0203c25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵娑嚩二合婆引嚩戍度憾由此真言加持力故。",
        "from": "T20n1085_001:0203c27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身口意業悉得清淨。",
        "from": "T20n1085_001:0203c27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後五輪著地。",
        "from": "T20n1085_001:0203c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命禮十方一切諸佛諸大菩薩方廣大乘。",
        "from": "T20n1085_001:0203c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右膝著地懺悔隨喜勸請發願以至心每誦此偈。",
        "from": "T20n1085_001:0204a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命十方正等覺　　最勝妙法菩薩眾",
        "from": "T20n1085_001:0204a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以身口意清淨業　　慇懃合掌恭敬禮",
        "from": "T20n1085_001:0204a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無始輪迴諸有中　　身口意業所生罪",
        "from": "T20n1085_001:0204a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如佛菩薩所懺悔　　我今陳懺亦如是",
        "from": "T20n1085_001:0204a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛菩薩行願中　　金剛三業所生福",
        "from": "T20n1085_001:0204a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣覺聲聞及有情　　所集善根盡隨喜",
        "from": "T20n1085_001:0204a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切世燈坐道場　　覺眼開敷照三有",
        "from": "T20n1085_001:0204a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今䠒跪先勸請　　轉於無上妙法輪",
        "from": "T20n1085_001:0204a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有如來三界主　　臨般無餘涅槃者",
        "from": "T20n1085_001:0204a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我皆勸請令久住　　不捨悲願救世間",
        "from": "T20n1085_001:0204a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懺悔隨喜勸請福　　願我不失菩提心",
        "from": "T20n1085_001:0204a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛菩薩妙眾中　　常為善友不厭捨",
        "from": "T20n1085_001:0204a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離於八難生無難　　宿命住智相嚴身",
        "from": "T20n1085_001:0204a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離愚迷具悲智　　悉能滿足波羅蜜",
        "from": "T20n1085_001:0204a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "富樂豐饒生勝族　　眷屬廣多恒熾盛",
        "from": "T20n1085_001:0204a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四無礙辯十自在　　六通諸禪悉圓滿",
        "from": "T20n1085_001:0204a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如金剛幢及普賢　　願讚迴向亦如是",
        "from": "T20n1085_001:0204a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次對本尊前。",
        "from": "T20n1085_001:0204a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結跏趺坐或半跏坐。",
        "from": "T20n1085_001:0204a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起大慈心。",
        "from": "T20n1085_001:0204a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我修此法。",
        "from": "T20n1085_001:0204a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為一切眾生願速證無上正等菩提。",
        "from": "T20n1085_001:0204a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先磨諸香以用塗手。",
        "from": "T20n1085_001:0204a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後結於佛部三昧耶陀羅尼印。",
        "from": "T20n1085_001:0204a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手虛心合掌。",
        "from": "T20n1085_001:0204a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開二頭指屈。",
        "from": "T20n1085_001:0204a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輔二中指上節。",
        "from": "T20n1085_001:0204a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指屈。",
        "from": "T20n1085_001:0204a23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輔二頭指下節。",
        "from": "T20n1085_001:0204a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其印即成。",
        "from": "T20n1085_001:0204a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置印當心。",
        "from": "T20n1085_001:0204a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想於如來三十二相八十種好。",
        "from": "T20n1085_001:0204a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了了分明如對目前。",
        "from": "T20n1085_001:0204a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至心誦此真言七遍真言曰。",
        "from": "T20n1085_001:0204a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵怛他引蘖覩引納婆二合嚩引也娑嚩二合訶引由結此印及誦真言故。",
        "from": "T20n1085_001:0204a29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即警覺一切如來。",
        "from": "T20n1085_001:0204a29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉當護念加持行者。",
        "from": "T20n1085_001:0204b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以光明照觸。",
        "from": "T20n1085_001:0204b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有罪障皆得消滅。",
        "from": "T20n1085_001:0204b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽命長遠福慧增長。",
        "from": "T20n1085_001:0204b02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛部聖眾擁護歡喜。",
        "from": "T20n1085_001:0204b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生生世世離諸惡趣。",
        "from": "T20n1085_001:0204b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮花化生速證無上正等菩提。",
        "from": "T20n1085_001:0204b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結蓮花部三昧耶印。",
        "from": "T20n1085_001:0204b04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手虛心合掌。",
        "from": "T20n1085_001:0204b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散開二頭指二中指二無名指。",
        "from": "T20n1085_001:0204b05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微屈如開敷蓮花形安於當心。",
        "from": "T20n1085_001:0204b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想觀自在菩薩相好持妙蓮花。",
        "from": "T20n1085_001:0204b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦此真言七遍於頂右散真言曰。",
        "from": "T20n1085_001:0204b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵跛娜謨二合引納婆二合嚩引也娑嚩二合引訶由結此印及誦真言故。",
        "from": "T20n1085_001:0204b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即警覺觀自在菩薩等持蓮花者。",
        "from": "T20n1085_001:0204b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切菩薩蓮花部聖眾悉皆歡喜。",
        "from": "T20n1085_001:0204b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持護念。",
        "from": "T20n1085_001:0204b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切菩薩光明照觸。",
        "from": "T20n1085_001:0204b12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有罪障皆得消滅。",
        "from": "T20n1085_001:0204b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切菩薩常為善友。",
        "from": "T20n1085_001:0204b13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結金剛部三昧耶印。",
        "from": "T20n1085_001:0204b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左手翻掌向外。",
        "from": "T20n1085_001:0204b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手掌背。",
        "from": "T20n1085_001:0204b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安左手背。",
        "from": "T20n1085_001:0204b15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用左右大指小指互相鉤。",
        "from": "T20n1085_001:0204b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如金剛杵形。",
        "from": "T20n1085_001:0204b16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安置於當心。",
        "from": "T20n1085_001:0204b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想金剛藏菩薩持妙金剛杵威光相好。",
        "from": "T20n1085_001:0204b17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦真言七遍於頂上左散之真言曰。",
        "from": "T20n1085_001:0204b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日嚧二合納婆二合嚩引也娑嚩二合引訶引由結此印及誦真言故。",
        "from": "T20n1085_001:0204b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即警覺一切金剛聖眾。",
        "from": "T20n1085_001:0204b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持擁護。",
        "from": "T20n1085_001:0204b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有罪障悉皆除滅。",
        "from": "T20n1085_001:0204b22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切痛苦終不著身。",
        "from": "T20n1085_001:0204b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當得金剛堅固之體。",
        "from": "T20n1085_001:0204b23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸魔不能侵嬈。",
        "from": "T20n1085_001:0204b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結護身被金剛甲冑印。",
        "from": "T20n1085_001:0204b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手內相叉。",
        "from": "T20n1085_001:0204b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右押左。",
        "from": "T20n1085_001:0204b26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二中指頭相中屈二頭指如鉤形。",
        "from": "T20n1085_001:0204b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中指背勿令相著。",
        "from": "T20n1085_001:0204b27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並二大指押無名指即成。",
        "from": "T20n1085_001:0204b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印加持自身五處。",
        "from": "T20n1085_001:0204b28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂額次右肩次左肩次心次喉。",
        "from": "T20n1085_001:0204b29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於頂上散印。",
        "from": "T20n1085_001:0204b29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各誦真言一遍真言曰。",
        "from": "T20n1085_001:0204c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉二合引𭨆儞二合引鉢囉二合捻奴揖反引跛跢二合也娑嚩二合引訶由結此印及誦真言。",
        "from": "T20n1085_001:0204c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持故。",
        "from": "T20n1085_001:0204c04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成被金剛甲冑。",
        "from": "T20n1085_001:0204c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有毘那夜迦及諸天魔作障礙者。",
        "from": "T20n1085_001:0204c05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "退散馳走。",
        "from": "T20n1085_001:0204c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉見行者光明被身威德自在。",
        "from": "T20n1085_001:0204c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若居山林及在嶮難。",
        "from": "T20n1085_001:0204c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆悉無畏。",
        "from": "T20n1085_001:0204c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有水火等災一切厄難虎狼師子刀杖枷鎖。",
        "from": "T20n1085_001:0204c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等事皆悉消滅。",
        "from": "T20n1085_001:0204c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見者歡喜。",
        "from": "T20n1085_001:0204c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命終已後不墮惡趣。",
        "from": "T20n1085_001:0204c09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當生諸佛淨妙國土。",
        "from": "T20n1085_001:0204c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結地界金剛橛印。",
        "from": "T20n1085_001:0204c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右無名指入左無名指小指內。",
        "from": "T20n1085_001:0204c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中指入左中指頭指內左亦如之。",
        "from": "T20n1085_001:0204c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆頭外出。",
        "from": "T20n1085_001:0204c13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘指並頭相拄。",
        "from": "T20n1085_001:0204c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即想印成火焰金剛杵形。",
        "from": "T20n1085_001:0204c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指著地掣之。",
        "from": "T20n1085_001:0204c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一掣一誦至三便止。",
        "from": "T20n1085_001:0204c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意大小標心即成堅固地界真言曰。",
        "from": "T20n1085_001:0204c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵枳里枳里一嚩日囉二合引嚩日哩二合二部𠷈二合半音滿馱滿馱吽引癹吒半音由結此印及誦真言加持地界故。",
        "from": "T20n1085_001:0204c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下至水際如金剛座。",
        "from": "T20n1085_001:0204c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天魔及諸障者不為惱害。",
        "from": "T20n1085_001:0204c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少加功力速得成就。",
        "from": "T20n1085_001:0204c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結方隅金剛牆印。",
        "from": "T20n1085_001:0204c22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前地界印。",
        "from": "T20n1085_001:0204c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開二大指直竪。",
        "from": "T20n1085_001:0204c23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "側如牆形。",
        "from": "T20n1085_001:0204c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想印如金剛杵形。",
        "from": "T20n1085_001:0205a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右遶身三轉。",
        "from": "T20n1085_001:0205a01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "標心大小。",
        "from": "T20n1085_001:0205a01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成金剛堅固之城。",
        "from": "T20n1085_001:0205a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛菩薩尚不違越。",
        "from": "T20n1085_001:0205a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況諸餘難調伏者。",
        "from": "T20n1085_001:0205a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘那夜迦及毒蟲利牙爪者。",
        "from": "T20n1085_001:0205a03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不輔近真言曰。",
        "from": "T20n1085_001:0205a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩囉薩囉嚩日囉二合引鉢囉二合迦引囉吽引癹吒半音行者次應想於壇中八葉大蓮花花。",
        "from": "T20n1085_001:0205a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上有妙師子座。",
        "from": "T20n1085_001:0205a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "座上有七寶樓閣。",
        "from": "T20n1085_001:0205a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "垂諸瓔珞繒綵幡蓋。",
        "from": "T20n1085_001:0205a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶柱行列垂妙天衣。",
        "from": "T20n1085_001:0205a09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周布香雲普雨雜花奏諸音樂。",
        "from": "T20n1085_001:0205a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶瓶閼伽天妙飲食。",
        "from": "T20n1085_001:0205a10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩尼為燈。",
        "from": "T20n1085_001:0205a10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作此觀已而誦此偈。",
        "from": "T20n1085_001:0205a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以我功德力　　如來加持力",
        "from": "T20n1085_001:0205a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及以法界力　　普供養而住",
        "from": "T20n1085_001:0205a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此偈已。",
        "from": "T20n1085_001:0205a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結大虛空藏普通供養印。",
        "from": "T20n1085_001:0205a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手合掌。",
        "from": "T20n1085_001:0205a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二中指右壓左外相叉。",
        "from": "T20n1085_001:0205a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "博著手背。",
        "from": "T20n1085_001:0205a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二頭指相拄。",
        "from": "T20n1085_001:0205a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "反蹙如寶形。",
        "from": "T20n1085_001:0205a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結印成已。",
        "from": "T20n1085_001:0205a16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言四遍。",
        "from": "T20n1085_001:0205a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普通供養真言曰。",
        "from": "T20n1085_001:0205a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵誐誐曩三去婆去嚩嚩日囉二合引斛引由結此印及誦真言加持力故。",
        "from": "T20n1085_001:0205a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所想供養真實無異。",
        "from": "T20n1085_001:0205a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切聖眾皆得受用。",
        "from": "T20n1085_001:0205a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應結寶車輅印。",
        "from": "T20n1085_001:0205a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉仰掌。",
        "from": "T20n1085_001:0205a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指橫相拄。",
        "from": "T20n1085_001:0205a22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大指各捻頭指根下。",
        "from": "T20n1085_001:0205a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想印成七寶車輅。",
        "from": "T20n1085_001:0205a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛駕御寶車。",
        "from": "T20n1085_001:0205a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘空而去至於極樂世界。",
        "from": "T20n1085_001:0205a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請諸聖眾即誦真言三遍真言曰。",
        "from": "T20n1085_001:0205a25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵都嚕都嚕吽由結此印真言加持故。",
        "from": "T20n1085_001:0205a27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七寶車輅至彼極樂國土。",
        "from": "T20n1085_001:0205a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又想如意輪觀自在菩薩及諸聖眾眷屬圍遶。",
        "from": "T20n1085_001:0205a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘寶車輅至道場中。",
        "from": "T20n1085_001:0205a29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空而住。",
        "from": "T20n1085_001:0205b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結請車輅印。",
        "from": "T20n1085_001:0205b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便誦請事輅真言。",
        "from": "T20n1085_001:0205b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大指向身。",
        "from": "T20n1085_001:0205b03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "撥二中指。",
        "from": "T20n1085_001:0205b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭誦三遍真言曰。",
        "from": "T20n1085_001:0205b04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜麼悉底囉三合野地尾二合迦引南引二合怛他引孽多引喃引唵嚩日𠻴二合𭨆儞二合引羯唎沙二合也娑嚩二合引訶引由此真言印加持故。",
        "from": "T20n1085_001:0205b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖眾從本土來。",
        "from": "T20n1085_001:0205b08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至此道場空虛而住。",
        "from": "T20n1085_001:0205b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結請本尊三昧耶降至於道場印。",
        "from": "T20n1085_001:0205b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉作拳。",
        "from": "T20n1085_001:0205b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左大指屈入掌中以右大母指向身招之。",
        "from": "T20n1085_001:0205b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言七遍真言曰。",
        "from": "T20n1085_001:0205b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿嚧引力迦半音阿孽車阿孽車娑嚩二合引訶由此真言印加持。",
        "from": "T20n1085_001:0205b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀自在菩薩不越本誓故。",
        "from": "T20n1085_001:0205b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即赴集於道場受此供養。",
        "from": "T20n1085_001:0205b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應辟除諸作障者。",
        "from": "T20n1085_001:0205b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結蓮花部明王馬頭觀自在菩薩真言印。",
        "from": "T20n1085_001:0205b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手合掌屈二頭指無名指。",
        "from": "T20n1085_001:0205b19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於掌內甲相背。",
        "from": "T20n1085_001:0205b19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪開二大指。",
        "from": "T20n1085_001:0205b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微屈離頭指。",
        "from": "T20n1085_001:0205b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二小指二中指各各合竪即成。",
        "from": "T20n1085_001:0205b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨誦真言以印頂上左旋三匝。",
        "from": "T20n1085_001:0205b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想辟除諸作難障者。",
        "from": "T20n1085_001:0205b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸魔見此印已。",
        "from": "T20n1085_001:0205b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "退散馳走便右旋三匝即成。",
        "from": "T20n1085_001:0205b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結界明王真言曰。",
        "from": "T20n1085_001:0205b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿密哩二合覩引納皤二合嚩吽引癹吒半音娑嚩二合引訶次結上方金剛網印。",
        "from": "T20n1085_001:0205b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前牆印。",
        "from": "T20n1085_001:0205b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指捻二頭指下節。",
        "from": "T20n1085_001:0205b28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言三遍。",
        "from": "T20n1085_001:0205b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印頂上右轉三匝。",
        "from": "T20n1085_001:0205b29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1085_001:0205b29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵尾娑普二合囉捺囉二合乞叉二合嚩日囉二合引半惹囉吽癹吒半音由結此印真言加持故。",
        "from": "T20n1085_001:0205c03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成金剛堅固不壞之網。",
        "from": "T20n1085_001:0205c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結金剛火院密縫印。",
        "from": "T20n1085_001:0205c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左手掩右手背。",
        "from": "T20n1085_001:0205c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二大指誦真言三遍。",
        "from": "T20n1085_001:0205c06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右遶身三匝。",
        "from": "T20n1085_001:0205c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想金剛牆外有三重火焰圍遶真言曰。",
        "from": "T20n1085_001:0205c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿三莽𡁬儞二合吽癹吒半音次獻閼伽香水真言印。",
        "from": "T20n1085_001:0205c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手捧器當額奉獻。",
        "from": "T20n1085_001:0205c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想洛聖眾足。",
        "from": "T20n1085_001:0205c11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言三遍真言曰。",
        "from": "T20n1085_001:0205c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱引南誐誐曩娑莽娑忙娑嚩二合引訶引由獻閼伽香水故。",
        "from": "T20n1085_001:0205c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者獲得三業清淨。",
        "from": "T20n1085_001:0205c15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洗滌煩惱垢。",
        "from": "T20n1085_001:0205c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當結獻蓮花座印。",
        "from": "T20n1085_001:0206a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手虛心合掌。",
        "from": "T20n1085_001:0206a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舒開左右無名指中指頭指。",
        "from": "T20n1085_001:0206a02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈如微敷蓮花形。",
        "from": "T20n1085_001:0206a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在寶樓閣內諸聖及本尊。",
        "from": "T20n1085_001:0206a03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各坐本位眷屬圍遶了了分別。",
        "from": "T20n1085_001:0206a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言三遍真言曰。",
        "from": "T20n1085_001:0206a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵迦麼攞娑嚩二合引訶引由結蓮花座印加持奉獻故。",
        "from": "T20n1085_001:0206a07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者當得十地滿足。",
        "from": "T20n1085_001:0206a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當得金剛之座。",
        "from": "T20n1085_001:0206a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結普供養印。",
        "from": "T20n1085_001:0206a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手合掌以十指右押左。",
        "from": "T20n1085_001:0206a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "互相交上節即成誦真言三遍。",
        "from": "T20n1085_001:0206a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想從印流出無量無邊塗香雲海花鬘雲海燒香雲海飲食燈明雲海。",
        "from": "T20n1085_001:0206a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆成清淨廣大供養。",
        "from": "T20n1085_001:0206a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦普供養真言三遍。",
        "from": "T20n1085_001:0206a13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1085_001:0206a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜莫薩嚩怛他去引蘖帝引毘喻二合引一尾濕嚩二合目契引毘藥二合一薩嚩他引欠搵娜𮒶二合底娑頗三合囉呬引𤚥誐誐曩劍娑嚩二合引訶次應誦讚歎偈。",
        "from": "T20n1085_001:0206a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦麼攞目佉一迦麼攞路引左娜二迦麼攞引娑那三迦麼攞賀娑跢二合四迦麼攞引娑母儞五迦麼攞迦麼攞六三娑嚩七娑迦攞麼攞八乞叉二合囉娜九那麼悉帝次應思惟。",
        "from": "T20n1085_001:0206a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想於自身中正當胸間。",
        "from": "T20n1085_001:0206a23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如滿月形光明晃曜。",
        "from": "T20n1085_001:0206a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月上有八葉蓮花。",
        "from": "T20n1085_001:0206a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於蓮花胎中有如意寶珠。",
        "from": "T20n1085_001:0206a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如紅頗梨色。",
        "from": "T20n1085_001:0206a25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爀奕光明照無量世界。",
        "from": "T20n1085_001:0206a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於光明中涌出本尊如意輪觀自在菩薩具足六臂相好圓滿住思惟相作是觀已。",
        "from": "T20n1085_001:0206a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起大悲心。",
        "from": "T20n1085_001:0206a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結如意輪根本印。",
        "from": "T20n1085_001:0206a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手合掌二頭指屈如寶形。",
        "from": "T20n1085_001:0206a29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指屈相拄如蓮花葉。",
        "from": "T20n1085_001:0206a29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合竪二大指即成。",
        "from": "T20n1085_001:0206b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相如意輪菩薩如對目前了了分明即誦根本陀羅尼七遍。",
        "from": "T20n1085_001:0206b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂上散印真言曰。",
        "from": "T20n1085_001:0206b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨囉怛曩二合怛羅夜引也二合曩莫阿引哩夜二合引嚩𡀔引枳帝濕嚩二合囉引也󰂛引地薩怛嚩二合引也摩賀引薩怛嚩二合引也摩賀引迦引嚕抳迦引也怛儞也二合他去引唵斫訖囉二合𮧬底振多引麼抳麼賀引跛納銘二合嚕嚕底瑟姹二合入嚩二合攞阿迦囉灑二合也吽泮吒半音娑嚩二合引訶次結如意輪心印。",
        "from": "T20n1085_001:0206b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T20n1085_001:0206b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二無名指二小指右壓左外相叉即成。",
        "from": "T20n1085_001:0206b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦心真言七遍頂上散之真言曰。",
        "from": "T20n1085_001:0206b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵跛娜麼二合振跢引麼抳入嚩二合攞吽次結心中心印。",
        "from": "T20n1085_001:0206b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前心印中指外相叉。",
        "from": "T20n1085_001:0206b17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小指橫竪即成。",
        "from": "T20n1085_001:0206b17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦心中心真言七遍頂上散之真言曰。",
        "from": "T20n1085_001:0206b18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩囉娜跛納銘二合吽即持念珠蟠於掌中。",
        "from": "T20n1085_001:0206b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以心中心真言加持七遍。",
        "from": "T20n1085_001:0206b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後持珠當心。",
        "from": "T20n1085_001:0206b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次第記數念誦滿一百八遍。",
        "from": "T20n1085_001:0206b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或一千八十遍心住本尊三摩地。",
        "from": "T20n1085_001:0206b22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更莫異緣了了分明。",
        "from": "T20n1085_001:0206b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍數了了。",
        "from": "T20n1085_001:0206b23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "珠安掌中。",
        "from": "T20n1085_001:0206b23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂戴安置本處。",
        "from": "T20n1085_001:0206b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後結根本印心印心中心三印。",
        "from": "T20n1085_001:0206b24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即入三摩地觀即觀心圓明漸舒。",
        "from": "T20n1085_001:0206b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廓周法界。",
        "from": "T20n1085_001:0206b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不見身心成清淨法界。",
        "from": "T20n1085_001:0206b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至食頃。",
        "from": "T20n1085_001:0206b26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從三昧出。",
        "from": "T20n1085_001:0206b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結普供養印。",
        "from": "T20n1085_001:0206b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獻香花等及閼伽水。",
        "from": "T20n1085_001:0206b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讚歎發願。",
        "from": "T20n1085_001:0206b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結火院結界印。",
        "from": "T20n1085_001:0206b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭上左轉一遍即成解界。",
        "from": "T20n1085_001:0206b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結車輅印向外撥之。",
        "from": "T20n1085_001:0206b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結迎請印向外撥之。",
        "from": "T20n1085_001:0206c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結護身印印五處已。",
        "from": "T20n1085_001:0206c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結三部三昧耶印即出道場任意經行。",
        "from": "T20n1085_001:0206c02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讀誦大乘經典迴助心中所求上中下悉地。",
        "from": "T20n1085_001:0206c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者若能日日三時依此念誦。",
        "from": "T20n1085_001:0206c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "罪障消滅得大智慧。",
        "from": "T20n1085_001:0206c04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三昧成就本尊現前能獲功德如經所述。",
        "from": "T20n1085_001:0206c05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀自在菩薩如意輪念誦儀軌一卷",
        "from": "T20n1085_001:0206c06_100",
        "context_with_next_sentence": false
    }
]