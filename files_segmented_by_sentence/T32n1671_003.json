{
    "T32n1671_003:0723a03_0": "福蓋正行所集經卷第三",
    "T32n1671_003:0723a05_0": "龍樹菩薩集",
    "T32n1671_003:0723a06_0": "西天譯經三藏朝散大夫試鴻臚卿",
    "T32n1671_003:0723a07_0": "宣梵大師賜紫沙門臣日稱等",
    "T32n1671_003:0723a08_0": "奉　詔譯",
    "T32n1671_003:0723a09_0": "爾時帝釋天主白佛言。",
    "T32n1671_003:0723a09_1": "世尊。",
    "T32n1671_003:0723a09_2": "唯願安住此王舍城。",
    "T32n1671_003:0723a10_3": "我當供給飲食衣服。",
    "T32n1671_003:0723a10_4": "臥具醫藥。",
    "T32n1671_003:0723a10_5": "令無所乏。",
    "T32n1671_003:0723a11_6": "佛言。",
    "T32n1671_003:0723a11_7": "天主。",
    "T32n1671_003:0723a11_8": "且止斯事。",
    "T32n1671_003:0723a11_9": "有諸人民。",
    "T32n1671_003:0723a11_10": "以清淨心。",
    "T32n1671_003:0723a12_11": "咸欲於我興其福業。",
    "T32n1671_003:0723a12_12": "是時帝釋復白佛言。",
    "T32n1671_003:0723a13_0": "唯願於此。",
    "T32n1671_003:0723a13_1": "五夏安居。",
    "T32n1671_003:0723a13_2": "受我供養。",
    "T32n1671_003:0723a13_3": "作是說已。",
    "T32n1671_003:0723a13_4": "佛復止之。",
    "T32n1671_003:0723a14_5": "帝釋復言我今請佛。",
    "T32n1671_003:0723a14_6": "五日供養。",
    "T32n1671_003:0723a14_7": "唯願大慈受我虔請。",
    "T32n1671_003:0723a15_8": "爾時世尊。",
    "T32n1671_003:0723a15_9": "愍惻天主。",
    "T32n1671_003:0723a15_10": "現福力故。",
    "T32n1671_003:0723a16_11": "令於未來。",
    "T32n1671_003:0723a16_12": "善因相續。",
    "T32n1671_003:0723a16_13": "默然許可。",
    "T32n1671_003:0723a16_14": "于時帝釋知佛受請。",
    "T32n1671_003:0723a17_15": "心大忻悅。",
    "T32n1671_003:0723a17_16": "即還天宮。",
    "T32n1671_003:0723a17_17": "召集工巧敏匠天子。",
    "T32n1671_003:0723a18_18": "而謂之曰。",
    "T32n1671_003:0723a18_19": "卿等當知。",
    "T32n1671_003:0723a18_20": "我今欲於王舍城。",
    "T32n1671_003:0723a19_21": "迦蘭陀竹園。",
    "T32n1671_003:0723a19_22": "建大精舍。",
    "T32n1671_003:0723a19_23": "嚴辦四事。",
    "T32n1671_003:0723a20_0": "供養如來。",
    "T32n1671_003:0723a20_1": "及諸弟子。",
    "T32n1671_003:0723a20_2": "汝當持取最上摩尼。",
    "T32n1671_003:0723a20_3": "金銀瑠璃。",
    "T32n1671_003:0723a21_4": "珊瑚玻璃。",
    "T32n1671_003:0723a21_5": "帝青寶等。",
    "T32n1671_003:0723a21_6": "詣彼造作殊勝清淨廣大宮殿。",
    "T32n1671_003:0723a22_7": "要如天中。",
    "T32n1671_003:0723a22_8": "等無有異。",
    "T32n1671_003:0723a22_9": "是諸天子盡意工巧。",
    "T32n1671_003:0723a23_10": "造立宮殿。",
    "T32n1671_003:0723a23_11": "廊廡柱栿。",
    "T32n1671_003:0723a23_12": "戶牖欄檻。",
    "T32n1671_003:0723a24_0": "悉以眾寶。",
    "T32n1671_003:0723a24_1": "間錯莊嚴。",
    "T32n1671_003:0723a24_2": "如千日光。",
    "T32n1671_003:0723a24_3": "更相顯照。",
    "T32n1671_003:0723a25_0": "復以雕飾諸妙珍禽。",
    "T32n1671_003:0723a25_1": "真珠花鬘。",
    "T32n1671_003:0723a25_2": "綜雜羅列。",
    "T32n1671_003:0723a25_3": "燒微妙香。",
    "T32n1671_003:0723a26_4": "散眾名華。",
    "T32n1671_003:0723a26_5": "氤氳普薰。",
    "T32n1671_003:0723a26_6": "靡不周遍。",
    "T32n1671_003:0723a26_7": "又復造作重門樓閣。",
    "T32n1671_003:0723a27_8": "峻峙嚴麗。",
    "T32n1671_003:0723a27_9": "亦以百寶。",
    "T32n1671_003:0723a27_10": "而裝校之。",
    "T32n1671_003:0723a28_11": "諸妙摩尼。",
    "T32n1671_003:0723a28_12": "以為其戶。",
    "T32n1671_003:0723a28_13": "彼諸寶柱。",
    "T32n1671_003:0723a28_14": "一一皆用金剛諸珍。",
    "T32n1671_003:0723a29_15": "而共合成。",
    "T32n1671_003:0723a29_16": "無數寶鑑。",
    "T32n1671_003:0723a29_17": "淨無瑕垢。",
    "T32n1671_003:0723a29_18": "懸挂空中。",
    "T32n1671_003:0723b01_19": "如百千月。",
    "T32n1671_003:0723b01_20": "復以鎔金。",
    "T32n1671_003:0723b01_21": "作三足床。",
    "T32n1671_003:0723b01_22": "殊妙天衣。",
    "T32n1671_003:0723b02_23": "以敷其上。",
    "T32n1671_003:0723b02_24": "又復刻成眾妙天女。",
    "T32n1671_003:0723b02_25": "容止端雅。",
    "T32n1671_003:0723b03_26": "如能往來。",
    "T32n1671_003:0723b03_27": "手持蓮花。",
    "T32n1671_003:0723b03_28": "而以奉獻。",
    "T32n1671_003:0723b03_29": "妙玻璃寶。",
    "T32n1671_003:0723b04_30": "以甃其地。",
    "T32n1671_003:0723b04_31": "有妙池沼。",
    "T32n1671_003:0723b04_32": "蓮華莊嚴。",
    "T32n1671_003:0723b04_33": "黃金為葉。",
    "T32n1671_003:0723b05_34": "綠寶為幹。",
    "T32n1671_003:0723b05_35": "無數天女。",
    "T32n1671_003:0723b05_36": "住立其中。",
    "T32n1671_003:0723b05_37": "作眾伎樂。",
    "T32n1671_003:0723b06_38": "而為供養。",
    "T32n1671_003:0723b06_39": "表剎高顯。",
    "T32n1671_003:0723b06_40": "懸眾繒幡。",
    "T32n1671_003:0723b06_41": "迴旋空中。",
    "T32n1671_003:0723b07_42": "遠邇皆覩。",
    "T32n1671_003:0723b07_43": "有諸力士。",
    "T32n1671_003:0723b07_44": "守護閈閎。",
    "T32n1671_003:0723b07_45": "持青寶杖。",
    "T32n1671_003:0723b08_46": "行列左右。",
    "T32n1671_003:0723b08_47": "處處皆植曼陀羅華。",
    "T32n1671_003:0723b08_48": "龍自在華。",
    "T32n1671_003:0723b09_0": "芬馥開榮。",
    "T32n1671_003:0723b09_1": "繽紛于地。",
    "T32n1671_003:0723b09_2": "翠葉交映。",
    "T32n1671_003:0723b09_3": "時飄香風。",
    "T32n1671_003:0723b09_4": "有諸天子。",
    "T32n1671_003:0723b10_5": "形體光潔。",
    "T32n1671_003:0723b10_6": "顏貌熙怡。",
    "T32n1671_003:0723b10_7": "遊憩其下。",
    "T32n1671_003:0723b10_8": "又諸天女。",
    "T32n1671_003:0723b11_9": "姝妙無比。",
    "T32n1671_003:0723b11_10": "徐步鳴璫。",
    "T32n1671_003:0723b11_11": "聲韻和雅。",
    "T32n1671_003:0723b11_12": "諸藥叉女。",
    "T32n1671_003:0723b12_13": "嬉遊林內。",
    "T32n1671_003:0723b12_14": "美目四盼。",
    "T32n1671_003:0723b12_15": "遺花墜香。",
    "T32n1671_003:0723b12_16": "有諸龍象。",
    "T32n1671_003:0723b13_17": "徐行林中。",
    "T32n1671_003:0723b13_18": "舉鼻嚙枝。",
    "T32n1671_003:0723b13_19": "宛轉為戲。",
    "T32n1671_003:0723b13_20": "及諸飛禽。",
    "T32n1671_003:0723b14_21": "其羽五色。",
    "T32n1671_003:0723b14_22": "鼓翼自若。",
    "T32n1671_003:0723b14_23": "喙啄花葩。",
    "T32n1671_003:0723b14_24": "復以金籠。",
    "T32n1671_003:0723b15_25": "置妙鸚鵡。",
    "T32n1671_003:0723b15_26": "美音清巧。",
    "T32n1671_003:0723b15_27": "其舌如簧。",
    "T32n1671_003:0723b15_28": "復有河源。",
    "T32n1671_003:0723b16_29": "清泠可愛流泉激響。",
    "T32n1671_003:0723b16_30": "蛙黽驚避。",
    "T32n1671_003:0723b16_31": "又有廣大清淨池沼。",
    "T32n1671_003:0723b17_32": "七寶雜廁。",
    "T32n1671_003:0723b17_33": "以為階岸。",
    "T32n1671_003:0723b17_34": "其水彌滿。",
    "T32n1671_003:0723b17_35": "澄湛無垢。",
    "T32n1671_003:0723b18_36": "優鉢羅花。",
    "T32n1671_003:0723b18_37": "酤牟那花。",
    "T32n1671_003:0723b18_38": "行列開敷。",
    "T32n1671_003:0723b18_39": "眾色相雜。",
    "T32n1671_003:0723b19_40": "翡翠鸂鷘。",
    "T32n1671_003:0723b19_41": "翔集其中。",
    "T32n1671_003:0723b19_42": "有諸戲魚。",
    "T32n1671_003:0723b19_43": "往來遊泳。",
    "T32n1671_003:0723b20_44": "騰躍翻浪。",
    "T32n1671_003:0723b20_45": "鷗鷺驚翥。",
    "T32n1671_003:0723b20_46": "復有水鳥。",
    "T32n1671_003:0723b20_47": "鸛鶴鳧鴈。",
    "T32n1671_003:0723b21_48": "舉聲嘹唳。",
    "T32n1671_003:0723b21_49": "人所樂聞。",
    "T32n1671_003:0723b21_50": "於池四岸。",
    "T32n1671_003:0723b21_51": "有劫波木。",
    "T32n1671_003:0723b22_52": "柔條拂水。",
    "T32n1671_003:0723b22_53": "生清涼風。",
    "T32n1671_003:0723b22_54": "叢植諸花。",
    "T32n1671_003:0723b22_55": "脩藤滋布。",
    "T32n1671_003:0723b23_56": "枝葉繁盛。",
    "T32n1671_003:0723b23_57": "其香遠聞。",
    "T32n1671_003:0723b23_58": "有諸遊蜂。",
    "T32n1671_003:0723b23_59": "競採其蘂。",
    "T32n1671_003:0723b24_60": "發聲微響。",
    "T32n1671_003:0723b24_61": "猶如詠歌。",
    "T32n1671_003:0723b24_62": "復有金河。",
    "T32n1671_003:0723b24_63": "周匝圍繞。",
    "T32n1671_003:0723b25_64": "流注園囿。",
    "T32n1671_003:0723b25_65": "縈紆不絕。",
    "T32n1671_003:0723b25_66": "時諸人民。",
    "T32n1671_003:0723b25_67": "迭相呼籲。",
    "T32n1671_003:0723b26_68": "親屬朋友。",
    "T32n1671_003:0723b26_69": "咸共往觀。",
    "T32n1671_003:0723b26_70": "有諸仕族。",
    "T32n1671_003:0723b26_71": "富樂莊嚴。",
    "T32n1671_003:0723b27_72": "僕從雍容。",
    "T32n1671_003:0723b27_73": "執持幰蓋。",
    "T32n1671_003:0723b27_74": "作諸伎樂。",
    "T32n1671_003:0723b27_75": "具設餚膳。",
    "T32n1671_003:0723b28_76": "遊行周覽。",
    "T32n1671_003:0723b28_77": "心無厭捨。",
    "T32n1671_003:0723b28_78": "其地柔軟。",
    "T32n1671_003:0723b28_79": "如兜羅綿。",
    "T32n1671_003:0723b29_80": "徘徊往來。",
    "T32n1671_003:0723b29_81": "足無疲惰。",
    "T32n1671_003:0723b29_82": "如是最勝廣大園林。",
    "T32n1671_003:0723c01_83": "清淨池沼。",
    "T32n1671_003:0723c01_84": "唯除天中餘無及者。",
    "T32n1671_003:0723c02_0": "是時工巧諸天子等。",
    "T32n1671_003:0723c02_1": "所作已辦。",
    "T32n1671_003:0723c02_2": "即詣天宮。",
    "T32n1671_003:0723c02_3": "前白帝釋。",
    "T32n1671_003:0723c03_4": "聞是說已。",
    "T32n1671_003:0723c03_5": "心生忻慶。",
    "T32n1671_003:0723c03_6": "得未曾有。",
    "T32n1671_003:0723c03_7": "即與無量百千天子。",
    "T32n1671_003:0723c04_8": "前後圍遶。",
    "T32n1671_003:0723c04_9": "作眾伎樂。",
    "T32n1671_003:0723c04_10": "簫笛箜篌。",
    "T32n1671_003:0723c05_11": "琴瑟鼓吹。",
    "T32n1671_003:0723c05_12": "一時具奏。",
    "T32n1671_003:0723c05_13": "從空而下。",
    "T32n1671_003:0723c05_14": "時彼天主。",
    "T32n1671_003:0723c06_15": "身出光明。",
    "T32n1671_003:0723c06_16": "照諸山川。",
    "T32n1671_003:0723c06_17": "皆悉盈爛。",
    "T32n1671_003:0723c06_18": "戴摩尼冠。",
    "T32n1671_003:0723c07_19": "諸珍嚴瑩。",
    "T32n1671_003:0723c07_20": "其光晃耀。",
    "T32n1671_003:0723c07_21": "如日盛明。",
    "T32n1671_003:0723c07_22": "面貌端嚴。",
    "T32n1671_003:0723c08_23": "額廣平正。",
    "T32n1671_003:0723c08_24": "其目紺長。",
    "T32n1671_003:0723c08_25": "鼻修高直。",
    "T32n1671_003:0723c08_26": "頤頰鮮白。",
    "T32n1671_003:0723c09_27": "紅潤無比。",
    "T32n1671_003:0723c09_28": "耳帶寶璫。",
    "T32n1671_003:0723c09_29": "頸垂纓珞。",
    "T32n1671_003:0723c09_30": "指環臂釧。",
    "T32n1671_003:0723c10_31": "眾寶所成。",
    "T32n1671_003:0723c10_32": "展轉舒光。",
    "T32n1671_003:0723c10_33": "其猶珂月。",
    "T32n1671_003:0723c10_34": "形儀挺特。",
    "T32n1671_003:0723c11_35": "常如盛年。",
    "T32n1671_003:0723c11_36": "修短充實。",
    "T32n1671_003:0723c11_37": "各得其宜。",
    "T32n1671_003:0723c11_38": "公酤摩香。",
    "T32n1671_003:0723c12_39": "及多摩羅跋旃檀香。",
    "T32n1671_003:0723c12_40": "以塗其身。",
    "T32n1671_003:0723c12_41": "柔軟光澤。",
    "T32n1671_003:0723c13_0": "手指纖圓。",
    "T32n1671_003:0723c13_1": "甲如銅葉。",
    "T32n1671_003:0723c13_2": "發語巧妙。",
    "T32n1671_003:0723c13_3": "清徹遠聞。",
    "T32n1671_003:0723c13_4": "如迦陵音。",
    "T32n1671_003:0723c14_5": "聽者無厭。",
    "T32n1671_003:0723c14_6": "復以上妙奇絕細[疊*毛]。",
    "T32n1671_003:0723c14_7": "研金飾之。",
    "T32n1671_003:0723c15_8": "以為其服。",
    "T32n1671_003:0723c15_9": "諸妙珍琦。",
    "T32n1671_003:0723c15_10": "為其束帶。",
    "T32n1671_003:0723c15_11": "眾寶間錯。",
    "T32n1671_003:0723c16_12": "成光明聚。",
    "T32n1671_003:0723c16_13": "復以天上諸妙花鬘。",
    "T32n1671_003:0723c16_14": "以嚴其身。",
    "T32n1671_003:0723c17_15": "周匝埀委。",
    "T32n1671_003:0723c17_16": "行步平正。",
    "T32n1671_003:0723c17_17": "猶如象王。",
    "T32n1671_003:0723c17_18": "進止優遊。",
    "T32n1671_003:0723c18_19": "不假扶掖。",
    "T32n1671_003:0723c18_20": "時諸人民。",
    "T32n1671_003:0723c18_21": "咸獲瞻奉。",
    "T32n1671_003:0723c18_22": "懷瞋恚者。",
    "T32n1671_003:0723c19_23": "即生歡悅。",
    "T32n1671_003:0723c19_24": "起怨惡者。",
    "T32n1671_003:0723c19_25": "即發慈心。",
    "T32n1671_003:0723c19_26": "妉睡眠者。",
    "T32n1671_003:0723c20_27": "即得惺寤。",
    "T32n1671_003:0723c20_28": "是時天主至彼園已。",
    "T32n1671_003:0723c20_29": "遍觀所作。",
    "T32n1671_003:0723c21_0": "皆悉如意。",
    "T32n1671_003:0723c21_1": "踊躍無量。",
    "T32n1671_003:0723c21_2": "乃易舊稱迦蘭陀。",
    "T32n1671_003:0723c21_3": "名為歡喜園。",
    "T32n1671_003:0723c22_4": "即以上妙眾色茵褥。",
    "T32n1671_003:0723c22_5": "遍覆其地。",
    "T32n1671_003:0723c22_6": "復以金銀。",
    "T32n1671_003:0723c23_7": "真珠摩尼。",
    "T32n1671_003:0723c23_8": "帝青瑠璃。",
    "T32n1671_003:0723c23_9": "末囉迦寶。",
    "T32n1671_003:0723c23_10": "為高廣座。",
    "T32n1671_003:0723c24_11": "請佛世尊。",
    "T32n1671_003:0723c24_12": "安處其上。",
    "T32n1671_003:0723c24_13": "眾寶為几。",
    "T32n1671_003:0723c24_14": "以承其足。",
    "T32n1671_003:0723c25_15": "五色繒線。",
    "T32n1671_003:0723c25_16": "設為眾位。",
    "T32n1671_003:0723c25_17": "命諸比丘。",
    "T32n1671_003:0723c25_18": "亦各就坐。",
    "T32n1671_003:0723c26_19": "是時天主合掌恭敬。",
    "T32n1671_003:0723c26_20": "尊重讚歎。",
    "T32n1671_003:0723c26_21": "頂禮佛足。",
    "T32n1671_003:0723c27_22": "具設天中上饌美味。",
    "T32n1671_003:0723c27_23": "是時如來舒金色臂。",
    "T32n1671_003:0723c28_0": "如象王鼻。",
    "T32n1671_003:0723c28_1": "而受其食。",
    "T32n1671_003:0723c28_2": "食已置鉢。",
    "T32n1671_003:0723c28_3": "盥掌澡漱。",
    "T32n1671_003:0723c28_4": "安住威儀。",
    "T32n1671_003:0723c29_5": "寂然清淨。",
    "T32n1671_003:0723c29_6": "時彼天主。",
    "T32n1671_003:0723c29_7": "與諸眷屬。",
    "T32n1671_003:0723c29_8": "生希有心。",
    "T32n1671_003:0724a01_9": "樂欲聞法。",
    "T32n1671_003:0724a01_10": "自處卑座。",
    "T32n1671_003:0724a01_11": "專注肅然。",
    "T32n1671_003:0724a01_12": "爾時世尊。",
    "T32n1671_003:0724a02_13": "以大方便。",
    "T32n1671_003:0724a02_14": "為說法要。",
    "T32n1671_003:0724a02_15": "讚施等行。",
    "T32n1671_003:0724a02_16": "示教利喜。",
    "T32n1671_003:0724a03_17": "令生信解。",
    "T32n1671_003:0724a03_18": "如是供養。",
    "T32n1671_003:0724a03_19": "經于四日。",
    "T32n1671_003:0724a03_20": "時阿闍世王。",
    "T32n1671_003:0724a04_21": "聞是事已。",
    "T32n1671_003:0724a04_22": "即陞宮中最上樓閣。",
    "T32n1671_003:0724a04_23": "俯視諦觀。",
    "T32n1671_003:0724a05_24": "見佛世尊。",
    "T32n1671_003:0724a05_25": "及諸弟子。",
    "T32n1671_003:0724a05_26": "在竹林園。",
    "T32n1671_003:0724a05_27": "寂靜安隱。",
    "T32n1671_003:0724a06_28": "嚴設供養。",
    "T32n1671_003:0724a06_29": "作大佛事。",
    "T32n1671_003:0724a06_30": "是時國王。",
    "T32n1671_003:0724a06_31": "心生覺悟。",
    "T32n1671_003:0724a07_32": "悔過自責。",
    "T32n1671_003:0724a07_33": "我大愚癡。",
    "T32n1671_003:0724a07_34": "無智錯謬。",
    "T32n1671_003:0724a07_35": "造極重罪。",
    "T32n1671_003:0724a08_36": "如須彌山。",
    "T32n1671_003:0724a08_37": "今佛世尊。",
    "T32n1671_003:0724a08_38": "出現于世。",
    "T32n1671_003:0724a08_39": "具足廣大清淨功德。",
    "T32n1671_003:0724a09_40": "而我不能信受教誨。",
    "T32n1671_003:0724a09_41": "由此業緣。",
    "T32n1671_003:0724a10_42": "必墮苦處。",
    "T32n1671_003:0724a10_43": "彼諸天子。",
    "T32n1671_003:0724a10_44": "尚捨天中上妙欲樂。",
    "T32n1671_003:0724a11_45": "來詣佛所。",
    "T32n1671_003:0724a11_46": "我等云何不興善利。",
    "T32n1671_003:0724a11_47": "作是語已。",
    "T32n1671_003:0724a12_0": "即嚴寶駕。",
    "T32n1671_003:0724a12_1": "往詣佛所。",
    "T32n1671_003:0724a12_2": "頭面禮足。",
    "T32n1671_003:0724a12_3": "心懷憂惱。",
    "T32n1671_003:0724a12_4": "涕淚悲泣。",
    "T32n1671_003:0724a13_5": "懺謝先咎。",
    "T32n1671_003:0724a13_6": "時王舍城清信士女。",
    "T32n1671_003:0724a13_7": "高聲唱言。",
    "T32n1671_003:0724a14_8": "善哉國王。",
    "T32n1671_003:0724a14_9": "於佛法中。",
    "T32n1671_003:0724a14_10": "得大覺悟。",
    "T32n1671_003:0724a14_11": "我等今者同獲善利。",
    "T32n1671_003:0724a15_12": "是時國王鳴鐘宣令。",
    "T32n1671_003:0724a15_13": "召集一切大臣人民。",
    "T32n1671_003:0724a16_14": "及諸眷屬。",
    "T32n1671_003:0724a16_15": "而告之曰。",
    "T32n1671_003:0724a16_16": "我佛出世。",
    "T32n1671_003:0724a16_17": "難得遭遻。",
    "T32n1671_003:0724a17_18": "今既獲覩。",
    "T32n1671_003:0724a17_19": "當發淨心。",
    "T32n1671_003:0724a17_20": "供養恭敬。",
    "T32n1671_003:0724a17_21": "樂聞正法。",
    "T32n1671_003:0724a18_22": "即以上妙香花燈明。",
    "T32n1671_003:0724a18_23": "塗香末香。",
    "T32n1671_003:0724a18_24": "及諸花鬘。",
    "T32n1671_003:0724a19_25": "珍玩衣服。",
    "T32n1671_003:0724a19_26": "種種莊嚴。",
    "T32n1671_003:0724a19_27": "而為供養。",
    "T32n1671_003:0724a19_28": "作是事已。",
    "T32n1671_003:0724a20_29": "心大歡悅。",
    "T32n1671_003:0724a20_30": "是時如來以梵音聲。",
    "T32n1671_003:0724a20_31": "為彼時會。",
    "T32n1671_003:0724a21_0": "開示演說苦集滅道四聖諦法。",
    "T32n1671_003:0724a21_1": "無量人天。",
    "T32n1671_003:0724a21_2": "聞法解了。",
    "T32n1671_003:0724a22_3": "得真實見。",
    "T32n1671_003:0724a22_4": "時佛世尊。",
    "T32n1671_003:0724a22_5": "問諸比丘。",
    "T32n1671_003:0724a22_6": "汝等見是諸天人民。",
    "T32n1671_003:0724a23_7": "今於我前。",
    "T32n1671_003:0724a23_8": "廣大施設供養已不。",
    "T32n1671_003:0724a24_9": "時諸比丘。",
    "T32n1671_003:0724a24_10": "合掌頂上。",
    "T32n1671_003:0724a24_11": "歎未曾有。",
    "T32n1671_003:0724a24_12": "唯然已見。",
    "T32n1671_003:0724a25_13": "佛言。",
    "T32n1671_003:0724a25_14": "於我法中。",
    "T32n1671_003:0724a25_15": "若人淨心。",
    "T32n1671_003:0724a25_16": "於他福業。",
    "T32n1671_003:0724a25_17": "生隨喜者。",
    "T32n1671_003:0724a26_18": "當知是人得不壞信。",
    "T32n1671_003:0724a26_19": "具淨智眼。",
    "T32n1671_003:0724a27_0": "諸比丘。",
    "T32n1671_003:0724a27_1": "我念往昔無量生中。",
    "T32n1671_003:0724a27_2": "有佛出世。",
    "T32n1671_003:0724a27_3": "名曰寶山如來應供正遍知明行足善逝世間解無上士調御丈夫天人師。",
    "T32n1671_003:0724a29_4": "佛世尊。",
    "T32n1671_003:0724a29_5": "住世說法。",
    "T32n1671_003:0724a29_6": "饒益眾生。",
    "T32n1671_003:0724b01_7": "爾時有王。",
    "T32n1671_003:0724b01_8": "請彼如來。",
    "T32n1671_003:0724b01_9": "及諸弟子。",
    "T32n1671_003:0724b01_10": "於王宮中。",
    "T32n1671_003:0724b02_11": "度夏三月。",
    "T32n1671_003:0724b02_12": "即以種種飲食衣服。",
    "T32n1671_003:0724b02_13": "恭敬供養。",
    "T32n1671_003:0724b03_14": "過是已後彼諸人民。",
    "T32n1671_003:0724b03_15": "咸詣佛所。",
    "T32n1671_003:0724b03_16": "請佛五年。",
    "T32n1671_003:0724b04_0": "設諸供養。",
    "T32n1671_003:0724b04_1": "一切所須。",
    "T32n1671_003:0724b04_2": "無不充足。",
    "T32n1671_003:0724b04_3": "諸比丘。",
    "T32n1671_003:0724b04_4": "於意云何。",
    "T32n1671_003:0724b05_5": "往昔國王供彼佛者。",
    "T32n1671_003:0724b05_6": "則我身是。",
    "T32n1671_003:0724b05_7": "由曩植因。",
    "T32n1671_003:0724b06_8": "今方成熟。",
    "T32n1671_003:0724b06_9": "得諸人天廣興供養。",
    "T32n1671_003:0724b06_10": "宿因今果。",
    "T32n1671_003:0724b07_11": "不差毫髮。",
    "T32n1671_003:0724b07_12": "由因感果。",
    "T32n1671_003:0724b07_13": "果如其因。",
    "T32n1671_003:0724b07_14": "於此正知。",
    "T32n1671_003:0724b08_15": "除諸疑網。",
    "T32n1671_003:0724b08_16": "善惡業報。",
    "T32n1671_003:0724b08_17": "決定不虛。",
    "T32n1671_003:0724b08_18": "猶如瀑流。",
    "T32n1671_003:0724b09_19": "勢不可制。",
    "T32n1671_003:0724b09_20": "彼業力故。",
    "T32n1671_003:0724b09_21": "各招其報。",
    "T32n1671_003:0724b09_22": "地界水界。",
    "T32n1671_003:0724b10_23": "火界風界。",
    "T32n1671_003:0724b10_24": "及蘊處等。",
    "T32n1671_003:0724b10_25": "自作福因。",
    "T32n1671_003:0724b10_26": "則受樂果。",
    "T32n1671_003:0724b11_27": "于時世尊說伽陀曰。",
    "T32n1671_003:0724b12_0": "　經於百千劫　　彼業不可壞",
    "T32n1671_003:0724b13_0": "　因緣和合時　　果報決定受",
    "T32n1671_003:0724b14_0": "諸比丘。",
    "T32n1671_003:0724b14_1": "是名福蓋正行。",
    "T32n1671_003:0724b14_2": "汝當受持。",
    "T32n1671_003:0724b14_3": "勤修施戒。",
    "T32n1671_003:0724b15_0": "及諸禪定。",
    "T32n1671_003:0724b15_1": "受用福蓋。",
    "T32n1671_003:0724b15_2": "得離熱惱。",
    "T32n1671_003:0724b16_0": "此義略明先佛所說。",
    "T32n1671_003:0724b16_1": "結集契經。",
    "T32n1671_003:0724b16_2": "緣起行相。",
    "T32n1671_003:0724b16_3": "最初經首。",
    "T32n1671_003:0724b17_4": "標其如是我聞之言。",
    "T32n1671_003:0724b17_5": "由何所以。",
    "T32n1671_003:0724b17_6": "稱如是耶。",
    "T32n1671_003:0724b18_7": "如佛所說。",
    "T32n1671_003:0724b18_8": "無別異故。",
    "T32n1671_003:0724b18_9": "何義名我。",
    "T32n1671_003:0724b18_10": "指現在身。",
    "T32n1671_003:0724b19_11": "順世俗故。",
    "T32n1671_003:0724b19_12": "何義為聞。",
    "T32n1671_003:0724b19_13": "謂耳發識。",
    "T32n1671_003:0724b19_14": "現前了知若文若義。",
    "T32n1671_003:0724b20_15": "離於增減顛倒失故。",
    "T32n1671_003:0724b20_16": "最初結集。",
    "T32n1671_003:0724b21_0": "謂梵網經。",
    "T32n1671_003:0724b21_1": "是時眾集。",
    "T32n1671_003:0724b21_2": "大阿羅漢。",
    "T32n1671_003:0724b21_3": "數有四百九十九人。",
    "T32n1671_003:0724b22_4": "唯除阿難獨居學地。",
    "T32n1671_003:0724b22_5": "及餘一切天龍鬼神。",
    "T32n1671_003:0724b23_6": "初聞唱言如是我聞。",
    "T32n1671_003:0724b23_7": "舉聲悲泣。",
    "T32n1671_003:0724b23_8": "不能自勝。",
    "T32n1671_003:0724b24_9": "我等在昔。",
    "T32n1671_003:0724b24_10": "親覩世尊具足十力。",
    "T32n1671_003:0724b24_11": "四無所畏。",
    "T32n1671_003:0724b25_0": "以梵音聲。",
    "T32n1671_003:0724b25_1": "為眾演說。",
    "T32n1671_003:0724b25_2": "如何今者乃稱我聞。",
    "T32n1671_003:0724b25_3": "將知無常。",
    "T32n1671_003:0724b26_4": "力不可脫。",
    "T32n1671_003:0724b26_5": "時彼眾會。",
    "T32n1671_003:0724b26_6": "皆得覺悟。",
    "T32n1671_003:0724b26_7": "離三毒垢。",
    "T32n1671_003:0724b27_8": "安住正念。",
    "T32n1671_003:0724b27_9": "任持不忘。",
    "T32n1671_003:0724b27_10": "何義稱一。",
    "T32n1671_003:0724b27_11": "乃數之始。",
    "T32n1671_003:0724b28_12": "謂一聖生。",
    "T32n1671_003:0724b28_13": "一出離法。",
    "T32n1671_003:0724b28_14": "一遊止處。",
    "T32n1671_003:0724b28_15": "一淨梵行。",
    "T32n1671_003:0724b29_16": "一解脫音。",
    "T32n1671_003:0724b29_17": "皆悉同故。",
    "T32n1671_003:0724b29_18": "何義名時。",
    "T32n1671_003:0724b29_19": "依世俗立。",
    "T32n1671_003:0724c01_20": "說經事已。",
    "T32n1671_003:0724c01_21": "大眾喜懌。",
    "T32n1671_003:0724c01_22": "名為一時。",
    "T32n1671_003:0724c01_23": "如來說法。",
    "T32n1671_003:0724c02_24": "如日照世。",
    "T32n1671_003:0724c02_25": "破三有暗。",
    "T32n1671_003:0724c02_26": "今出離故。",
    "T32n1671_003:0724c02_27": "諸魔外道。",
    "T32n1671_003:0724c03_28": "毀謗正法。",
    "T32n1671_003:0724c03_29": "今佛顯現甚深希有難得之法。",
    "T32n1671_003:0724c04_0": "令降伏故。",
    "T32n1671_003:0724c04_1": "何義稱薄伽梵。",
    "T32n1671_003:0724c04_2": "具足殊勝廣大名聞。",
    "T32n1671_003:0724c05_3": "世出世間。",
    "T32n1671_003:0724c05_4": "無與等者。",
    "T32n1671_003:0724c05_5": "畢竟超越界趣輪迴。",
    "T32n1671_003:0724c06_6": "應受人天妙供養故。",
    "T32n1671_003:0724c06_7": "云何舍衛城。",
    "T32n1671_003:0724c06_8": "豐諸妙物。",
    "T32n1671_003:0724c07_9": "及文行士。",
    "T32n1671_003:0724c07_10": "清信士女。",
    "T32n1671_003:0724c07_11": "豪富長者。",
    "T32n1671_003:0724c07_12": "受用勝故。",
    "T32n1671_003:0724c08_13": "何義名住。",
    "T32n1671_003:0724c08_14": "遊化依止。",
    "T32n1671_003:0724c08_15": "離諸思構。",
    "T32n1671_003:0724c08_16": "若遠若近。",
    "T32n1671_003:0724c09_17": "隨意至故。",
    "T32n1671_003:0724c10_0": "福蓋正行所集經卷第三"
}