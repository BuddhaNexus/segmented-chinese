[
    {
        "source_sentence": "金剛頂經觀自在王如來修行法",
        "from": "T19n0931_001:0072c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一卷",
        "from": "T19n0931_001:0072c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T19n0931_001:0072c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空諡大鑒正號大廣智大興善寺三藏沙門不空奉　詔譯我今依金剛頂經。",
        "from": "T19n0931_001:0072c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "演金剛蓮花達摩法要。",
        "from": "T19n0931_001:0072c10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先須入於灌頂三昧耶曼茶羅。",
        "from": "T19n0931_001:0073a01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發大菩提心。",
        "from": "T19n0931_001:0073a01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨身命財饒益一切。",
        "from": "T19n0931_001:0073a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勇猛精進隨念相應。",
        "from": "T19n0931_001:0073a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈悲喜捨無有間斷。",
        "from": "T19n0931_001:0073a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是之人方應修習。",
        "from": "T19n0931_001:0073a03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其曼茶羅畫像等法廣如餘經所說。",
        "from": "T19n0931_001:0073a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡入精舍欲念誦時。",
        "from": "T19n0931_001:0073a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以五輪著地。",
        "from": "T19n0931_001:0073a05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂禮本尊觀自在王如來。",
        "from": "T19n0931_001:0073a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次禮北方不空成就如來乃至無動寶生遍照如來。",
        "from": "T19n0931_001:0073a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆依法至誠敬禮。",
        "from": "T19n0931_001:0073a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雙膝而跪蓮花合掌。",
        "from": "T19n0931_001:0073a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懺悔三業一切過咎。",
        "from": "T19n0931_001:0073a08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我從無量劫　　淪滯生死海",
        "from": "T19n0931_001:0073a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今以清淨心　　發露而悔過",
        "from": "T19n0931_001:0073a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如諸佛所懺　　我今亦如是",
        "from": "T19n0931_001:0073a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願我及眾生　　一切皆清淨",
        "from": "T19n0931_001:0073a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T19n0931_001:0073a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一馺嚩二合婆去嚩秝入陀二薩嚩達摩三馺嚩二合婆去嚩四秝入度澸五次應隨喜過現未來諸佛菩薩所集福智。",
        "from": "T19n0931_001:0073a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過去三世佛　　菩薩與眾生",
        "from": "T19n0931_001:0073a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所集諸善根　　合掌盡隨喜",
        "from": "T19n0931_001:0073a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應右膝著地。",
        "from": "T19n0931_001:0073a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛心合掌置於頂上。",
        "from": "T19n0931_001:0073a19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想禮諸佛如來及菩薩足。",
        "from": "T19n0931_001:0073a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0073a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵鉢納摩二合上尾多多字半音與尾字合呼禮諸佛已。",
        "from": "T19n0931_001:0073a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依座印而坐入定思惟。",
        "from": "T19n0931_001:0073a22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀無量如來周遍法界。",
        "from": "T19n0931_001:0073a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者己身悉在彼會。",
        "from": "T19n0931_001:0073a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後結祕密三昧耶印。",
        "from": "T19n0931_001:0073a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅固縛以檀慧禪智。",
        "from": "T19n0931_001:0073a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相合而竪。",
        "from": "T19n0931_001:0073a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此驚覺聖眾。",
        "from": "T19n0931_001:0073a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0073a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一嚩日囉二合二跛娜莾二合三三昧耶四薩怛梵三合五次應結一切如來歡喜悅意三昧耶印。",
        "from": "T19n0931_001:0073a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前印唯以忍願入掌中。",
        "from": "T19n0931_001:0073a28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T19n0931_001:0073a28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一三莾耶呼引二蘇去囉多三薩怛梵三合四次結開心印。",
        "from": "T19n0931_001:0073b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀二乳上有怛囉二合吒字。",
        "from": "T19n0931_001:0073b02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以金剛縛。",
        "from": "T19n0931_001:0073b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三拍開之。",
        "from": "T19n0931_001:0073b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T19n0931_001:0073b03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一嚩日囉二合二滿馱三怛囉二合吒四次結入智字印。",
        "from": "T19n0931_001:0073b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於己面前。",
        "from": "T19n0931_001:0073b05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀一紅蓮華上有娑字。",
        "from": "T19n0931_001:0073b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以堅固縛禪智入中。",
        "from": "T19n0931_001:0073b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力相拄。",
        "from": "T19n0931_001:0073b06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想捻其字內於心中。",
        "from": "T19n0931_001:0073b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0073b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一嚩日囉二合引吠捨二惡三次結闔智字印。",
        "from": "T19n0931_001:0073b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不易前印。",
        "from": "T19n0931_001:0073b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力拄禪智背。",
        "from": "T19n0931_001:0073b09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想闔心門。",
        "from": "T19n0931_001:0073b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T19n0931_001:0073b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一嚩日囉二合二母瑟致二合三𤚥四次結辟除結界印。",
        "from": "T19n0931_001:0073b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戒方右押左外相交。",
        "from": "T19n0931_001:0073b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忍願相拄。",
        "from": "T19n0931_001:0073b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以進力鉤戒方。",
        "from": "T19n0931_001:0073b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪智捻檀慧頭。",
        "from": "T19n0931_001:0073b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T19n0931_001:0073b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一阿密栗二合覩娜婆二合嚩二吽三泮吒吒半音呼之四以此印左轉三匝。",
        "from": "T19n0931_001:0073b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辟除不善。",
        "from": "T19n0931_001:0073b17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋三匝。",
        "from": "T19n0931_001:0073b17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意遠近以為結界。",
        "from": "T19n0931_001:0073b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結三摩地印。",
        "from": "T19n0931_001:0073b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二羽仰叉。",
        "from": "T19n0931_001:0073b19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力相背而竪。",
        "from": "T19n0931_001:0073b19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪智捻進力頭。",
        "from": "T19n0931_001:0073b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置於跏上。",
        "from": "T19n0931_001:0073b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0073b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一三麼引地二鉢娜名二合三頡哩二合引其出入息一一明了。",
        "from": "T19n0931_001:0073b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀虛空中無量諸佛。",
        "from": "T19n0931_001:0073b22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時彈指警覺行者而告之言。",
        "from": "T19n0931_001:0073b23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今云何成無上覺。",
        "from": "T19n0931_001:0073b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不知諸佛實相法要。",
        "from": "T19n0931_001:0073b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者爾時聞警覺已。",
        "from": "T19n0931_001:0073b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言云何名真實。",
        "from": "T19n0931_001:0073b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願如來為我解說。",
        "from": "T19n0931_001:0073b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時諸佛告行者言。",
        "from": "T19n0931_001:0073b26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉能作是問。",
        "from": "T19n0931_001:0073b26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝想心中所內惡字瑩徹於心。",
        "from": "T19n0931_001:0073b27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0073b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一唧多鉢囉二合底二味鄧迦𡀔弭三當默誦一遍。",
        "from": "T19n0931_001:0073b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便想為月輪。",
        "from": "T19n0931_001:0073b29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如在輕霧。",
        "from": "T19n0931_001:0073b29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲令其月輪得清淨故。",
        "from": "T19n0931_001:0073c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此密言曰。",
        "from": "T19n0931_001:0073c01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一冒地唧多母怛跛二二娜夜弭誦已諦觀心月極令清淨。",
        "from": "T19n0931_001:0073c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如處大虛廓無瑕翳。",
        "from": "T19n0931_001:0073c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復於心月想一蓮華。",
        "from": "T19n0931_001:0073c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0073c04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一底瑟吒二合二嚩日囉二合三鉢娜麼二合四想其蓮花與月。",
        "from": "T19n0931_001:0073c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漸漸而引周遍法界。",
        "from": "T19n0931_001:0073c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "量等虛空。",
        "from": "T19n0931_001:0073c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0073c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一薩頗二合囉二嚩日囉二合三鉢娜麼二合四其引蓮華中放無量光。",
        "from": "T19n0931_001:0073c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨光流出無量無邊際極樂世界。",
        "from": "T19n0931_001:0073c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一世界妙寶莊嚴。",
        "from": "T19n0931_001:0073c11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆有觀自在王如來。",
        "from": "T19n0931_001:0073c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與聖眾前後圍遶。",
        "from": "T19n0931_001:0073c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是觀已漸斂其蓮華。",
        "from": "T19n0931_001:0073c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0073c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一僧賀囉二合二鉢娜麼二合三如是一切世界諸佛如來。",
        "from": "T19n0931_001:0073c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨華而斂量等本身。",
        "from": "T19n0931_001:0073c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即變己身為無量壽佛身。",
        "from": "T19n0931_001:0073c16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "紅頗梨色放大光明。",
        "from": "T19n0931_001:0073c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結三麼地印金剛蓮華座。",
        "from": "T19n0931_001:0073c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想行者己身為如來已。",
        "from": "T19n0931_001:0073c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復恐散亂而有退失。",
        "from": "T19n0931_001:0073c18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次作加持印而加持之。",
        "from": "T19n0931_001:0073c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅固縛以進力屈如蓮葉。",
        "from": "T19n0931_001:0073c19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪智並竪印心額喉頂隨所印。",
        "from": "T19n0931_001:0073c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想頡哩二合字。",
        "from": "T19n0931_001:0073c20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一嚩日囉二合二達麼三頡哩二合次結灌頂印。",
        "from": "T19n0931_001:0074a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮華合掌進力相蹙如寶。",
        "from": "T19n0931_001:0074a03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "檀慧竪相遠。",
        "from": "T19n0931_001:0074a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置於額上。",
        "from": "T19n0931_001:0074a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T19n0931_001:0074a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一鉢娜麼二合二囉怛那二合引三毘詵遮四薩嚩𤚥五頡哩二合六怛𡀔二合七次結蓮華鬘印。",
        "from": "T19n0931_001:0074a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以前印從額分手。",
        "from": "T19n0931_001:0074a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繞至頂後。",
        "from": "T19n0931_001:0074a07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結蓮華拳。",
        "from": "T19n0931_001:0074a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如繫華鬘勢。",
        "from": "T19n0931_001:0074a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徐徐前下散誦密言曰。",
        "from": "T19n0931_001:0074a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一鉢娜麼二合二囉怛那二合三麼引隷四𤚥牟含反五次結甲冑印。",
        "from": "T19n0931_001:0074a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結蓮華拳。",
        "from": "T19n0931_001:0074a12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後繞身如擐甲法。",
        "from": "T19n0931_001:0074a12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一阿婆耶二鉢娜麼二合三迦嚩制四滿馱五囉乞灑二合𤚥六吽七憾八次結歡喜印。",
        "from": "T19n0931_001:0074a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮華合掌微拍三聲。",
        "from": "T19n0931_001:0074a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074a16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一鉢娜麼二合二覩使野二合護引三復於面前。",
        "from": "T19n0931_001:0074a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀安樂世界。",
        "from": "T19n0931_001:0074a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "琉璃為地功德乳海。",
        "from": "T19n0931_001:0074a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其海中觀頡哩二合字。",
        "from": "T19n0931_001:0074a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變為微妙開敷蓮華。",
        "from": "T19n0931_001:0074a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即變其華為觀自在王如來。",
        "from": "T19n0931_001:0074a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色相莊嚴如前身觀。",
        "from": "T19n0931_001:0074a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結蓮華鉤印。",
        "from": "T19n0931_001:0074a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮華合掌進力如鉤。",
        "from": "T19n0931_001:0074a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一鉢娜懵二合引二句捨三吽四若五次結蓮華索印。",
        "from": "T19n0931_001:0074a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以前印進力如環。",
        "from": "T19n0931_001:0074a25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一鉢娜麼二合引二母伽播捨三吽四次結蓮華鎖印。",
        "from": "T19n0931_001:0074a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "芙蓉合掌進禪相捻。",
        "from": "T19n0931_001:0074a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "力智亦爾。",
        "from": "T19n0931_001:0074a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相鉤成鎖。",
        "from": "T19n0931_001:0074a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一鉢娜麼二合二塞怖二合吒三𤚥四次結蓮華鈴印。",
        "from": "T19n0931_001:0074b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以蓮華合掌。",
        "from": "T19n0931_001:0074b02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪智入中進力如環。",
        "from": "T19n0931_001:0074b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一鉢娜麼二合引二吠捨惡三次結蓮華部百字密言。",
        "from": "T19n0931_001:0074b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捧閼伽器。",
        "from": "T19n0931_001:0074b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以欝金龍腦相和香水。",
        "from": "T19n0931_001:0074b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而為供養。",
        "from": "T19n0931_001:0074b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦百字密言曰。",
        "from": "T19n0931_001:0074b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一鉢娜麼二合二薩怛嚩二合三三麼耶四麼弩播攞耶五鉢那麼二合六薩怛嚩二合七帝尾二合弩波底瑟姹二合八涅哩二合擢茶護反茗皤嚩九蘇覩灑諭二合茗皤嚩十阿弩囉訖覩二合茗皤嚩十一蘇報灑諭二合茗皤嚩十二薩嚩悉簟寐鉢囉二合也瑳十三薩婆羯麼蘇者茗十四唧多失唎二合藥十五句嚧十六吽十七訶十八訶十九訶二十訶二十一護二十二薄伽梵二十三薩婆怛他孽多二十四鉢娜麼二合二十五莽茗悶遮二十六鉢娜茗二合皤嚩二十七麼訶三麼野二十八薩怛嚩二合二十九惡引三十次結蓮華內四供養嬉戲印。",
        "from": "T19n0931_001:0074b18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "芙蓉合掌禪智並竪誦。",
        "from": "T19n0931_001:0074b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T19n0931_001:0074b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一鉢娜麼二合二邏細三呼四引次結蓮華鬘印。",
        "from": "T19n0931_001:0074b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以前印。",
        "from": "T19n0931_001:0074b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二臂俱申。",
        "from": "T19n0931_001:0074b21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一鉢娜麼二合二麼隷三怛囉二合吒半音輕呼次結蓮華歌印。",
        "from": "T19n0931_001:0074b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以芙蓉合掌。",
        "from": "T19n0931_001:0074b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈拄諸度。",
        "from": "T19n0931_001:0074b24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從臍而上。",
        "from": "T19n0931_001:0074b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至口方散。",
        "from": "T19n0931_001:0074b25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074b25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一鉢娜麼二合二擬妍以反帝三擬同上音四次結蓮華舞印。",
        "from": "T19n0931_001:0074b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以芙蓉合掌。",
        "from": "T19n0931_001:0074b27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左右而旋。",
        "from": "T19n0931_001:0074b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於頂上散。",
        "from": "T19n0931_001:0074b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一鉢娜麼二合二儞哩二合諦三訖哩二合吒半聲四次結蓮華外四供養燒香印。",
        "from": "T19n0931_001:0074c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮華合掌下散。",
        "from": "T19n0931_001:0074c03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如焚香。",
        "from": "T19n0931_001:0074c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074c04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一鉢娜麼二合二度閉二合三噁四次結蓮華散華印。",
        "from": "T19n0931_001:0074c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮華合掌前上散。",
        "from": "T19n0931_001:0074c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如散華勢。",
        "from": "T19n0931_001:0074c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一鉢娜麼二合二補瑟閉二合三唵四次結蓮華燈印。",
        "from": "T19n0931_001:0074c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮華合掌禪智竪相逼。",
        "from": "T19n0931_001:0074c09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一鉢娜麼二合二儞引閉三儞四次結蓮華塗香印。",
        "from": "T19n0931_001:0074c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十度作蓮華合掌。",
        "from": "T19n0931_001:0074c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當心分散。",
        "from": "T19n0931_001:0074c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如塗香勢。",
        "from": "T19n0931_001:0074c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一鉢娜麼二合二囐提三虐四次結根本印。",
        "from": "T19n0931_001:0074c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅固縛以忍願相拄。",
        "from": "T19n0931_001:0074c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如蓮葉形。",
        "from": "T19n0931_001:0074c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一路計什嚩二合囉二囉引惹三頡哩二合四結根本印。",
        "from": "T19n0931_001:0074c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦明七遍已。",
        "from": "T19n0931_001:0074c19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後執蓮子珠。",
        "from": "T19n0931_001:0074c19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相應念誦。",
        "from": "T19n0931_001:0074c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於四時中隨力而作。",
        "from": "T19n0931_001:0074c20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲止時。",
        "from": "T19n0931_001:0074c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重結根本印復誦七遍根本陀羅尼。",
        "from": "T19n0931_001:0075a01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結八供養印。",
        "from": "T19n0931_001:0075a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後發遣本尊。",
        "from": "T19n0931_001:0075a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發遣印。",
        "from": "T19n0931_001:0075a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以前根本印。",
        "from": "T19n0931_001:0075a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從臍至面方散。",
        "from": "T19n0931_001:0075a03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌於頂上。",
        "from": "T19n0931_001:0075a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想觀自在王如來復還本宮。",
        "from": "T19n0931_001:0075a04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0075a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一嚩日囉二合二鉢娜麼二合三穆四發遣本尊已。",
        "from": "T19n0931_001:0075a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意發願。",
        "from": "T19n0931_001:0075a07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復以甲印護身。",
        "from": "T19n0931_001:0075a07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以馬頭明王結界印。",
        "from": "T19n0931_001:0075a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左旋解界。",
        "from": "T19n0931_001:0075a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意經行往諸淨處。",
        "from": "T19n0931_001:0075a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讀大乘大般若經華嚴涅槃及楞伽等經。",
        "from": "T19n0931_001:0075a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行住坐臥常念本尊無令間斷。",
