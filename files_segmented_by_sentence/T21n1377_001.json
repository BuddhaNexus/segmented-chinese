[
    {
        "source_sentence": "佛說寶帶陀羅尼經",
        "from": "T21n1377_001:0898c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝奉大夫試光祿卿",
        "from": "T21n1377_001:0898c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳法大師賜紫臣施護奉　詔譯",
        "from": "T21n1377_001:0898c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T21n1377_001:0898c27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在釋種住處迦毘羅城尼拘律陀樹園。",
        "from": "T21n1377_001:0898c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大眾俱。",
        "from": "T21n1377_001:0898c28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時羅睺羅童子居止異處。",
        "from": "T21n1377_001:0898c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於夜分中忽為大惡羅剎所怖。",
        "from": "T21n1377_001:0898c29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時羅睺羅即於是夜。",
        "from": "T21n1377_001:0899a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與諸童子眾前後圍繞。",
        "from": "T21n1377_001:0899a01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出迦毘羅城往詣佛所。",
        "from": "T21n1377_001:0899a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到已頭面禮佛雙足。",
        "from": "T21n1377_001:0899a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃於佛前涕泣而住。",
        "from": "T21n1377_001:0899a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊知是事已。",
        "from": "T21n1377_001:0899a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即告尊者阿難言。",
        "from": "T21n1377_001:0899a04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難當知我有大明。",
        "from": "T21n1377_001:0899a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名曰寶帶有大功德。",
        "from": "T21n1377_001:0899a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能為羅睺羅童子而作擁護。",
        "from": "T21n1377_001:0899a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今宣說汝當受持。",
        "from": "T21n1377_001:0899a06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而此大明若諸天眾龍眾。",
        "from": "T21n1377_001:0899a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜叉眾羅剎眾。",
        "from": "T21n1377_001:0899a07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿修羅眾迦樓羅眾。",
        "from": "T21n1377_001:0899a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乾闥婆眾緊那羅眾。",
        "from": "T21n1377_001:0899a08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必舍左眾必隷多眾。",
        "from": "T21n1377_001:0899a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳩盤茶眾布單那眾。",
        "from": "T21n1377_001:0899a09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羯吒布單那眾諸宿曜眾。",
        "from": "T21n1377_001:0899a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十八大夜叉將。",
        "from": "T21n1377_001:0899a10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四大夜叉主四大天王。",
        "from": "T21n1377_001:0899a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸離欲仙眾雪山住者。",
        "from": "T21n1377_001:0899a11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至十方一切諸部多眾并羅剎女眾。",
        "from": "T21n1377_001:0899a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦儞里布寫那哩計吒羯蘭拏蘇摩那摩賀訶哩等諸母鬼眾。",
        "from": "T21n1377_001:0899a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三母訥誐彌羯臘波大羅剎。",
        "from": "T21n1377_001:0899a14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及東方訶哩大羅剎女。",
        "from": "T21n1377_001:0899a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南方虞拏西大羅剎女。",
        "from": "T21n1377_001:0899a15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方計舍哩大羅剎女。",
        "from": "T21n1377_001:0899a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方珠髻大羅剎女。",
        "from": "T21n1377_001:0899a16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此四大羅剎女各與六十眷屬俱。",
        "from": "T21n1377_001:0899a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等眾若聞宣說寶帶大明。",
        "from": "T21n1377_001:0899a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切處能為一切眾生而作擁護。",
        "from": "T21n1377_001:0899a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊即說寶帶大明曰。",
        "from": "T21n1377_001:0899a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竭儞引屹囉二合引一句竭那鉢底二瞿彌引屹囉二合引三瞿摩鉢底四鉢哩引迦五竭哩尼六竭囉鉢底七娑引囉鉢底八達那鉢底九蘇囉鉢底十阿梨波十一阿梨波鉢底十二摩鉢底十三阿鉢底十四末虎鉢底十五虎羅十六呬梨十七虎盧十八底梨十九呬梨二十唧致二十一梨鉢多二合二十二鉢跓引吒吒吠引二十三唵引二十四藍尾引二十五摩尾薩哩引二十六摩賀引呬梨二十七虎虎二十八三滿多薩薩哩引二十九阿三滿多薩囉鉢梨引三十珂薩哩引三十一阿引嚩哩多二合三十二摩賀引嚩哩多二合三十三竭儞引三十四彌引珂梨引三十五佛言阿難。",
        "from": "T21n1377_001:0899b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我此宣說寶帶大明有大功德。",
        "from": "T21n1377_001:0899b03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有持是明者。",
        "from": "T21n1377_001:0899b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨諸所求皆得圓滿。",
        "from": "T21n1377_001:0899b04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有持明天眾部多眾。",
        "from": "T21n1377_001:0899b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吠多拏眾大力天眾。",
        "from": "T21n1377_001:0899b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸神通精進仙眾。",
        "from": "T21n1377_001:0899b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於持明者不能為障。",
        "from": "T21n1377_001:0899b06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難當知今此寶帶大明。",
        "from": "T21n1377_001:0899b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛世尊共所宣說。",
        "from": "T21n1377_001:0899b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普為一切眾生利益安樂。",
        "from": "T21n1377_001:0899b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至於一切處密作擁護。",
        "from": "T21n1377_001:0899b08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難大地所有大惡難調攝人威光者極惡業者。",
        "from": "T21n1377_001:0899b09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若方若隅乃至一切處。",
        "from": "T21n1377_001:0899b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞是寶帶大明時皆大驚怖。",
        "from": "T21n1377_001:0899b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此大明周匝十由旬內。",
        "from": "T21n1377_001:0899b11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能為一切眾生而作擁護。",
        "from": "T21n1377_001:0899b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救諸難事攝持護念。",
        "from": "T21n1377_001:0899b12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸眾生息災安樂。",
        "from": "T21n1377_001:0899b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為杖打不為刀傷不為毒中。",
        "from": "T21n1377_001:0899b13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切惡毒自然消滅。",
        "from": "T21n1377_001:0899b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡友遠離一切惡者。",
        "from": "T21n1377_001:0899b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作禁縛事自然解脫。",
        "from": "T21n1377_001:0899b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人及非人。",
        "from": "T21n1377_001:0899b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲於一切眾生作惱害者。",
        "from": "T21n1377_001:0899b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當依法建立壇場。",
        "from": "T21n1377_001:0899b16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以諸香華供養如來。",
        "from": "T21n1377_001:0899b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依法結界沐浴清淨。",
        "from": "T21n1377_001:0899b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦是寶帶大明。",
        "from": "T21n1377_001:0899b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼諸惡者自然攝伏不能為害。",
        "from": "T21n1377_001:0899b18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至一切病苦亦得息除。",
        "from": "T21n1377_001:0899b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說大明曰。",
        "from": "T21n1377_001:0899b19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛𭔞切身下同他引一句儞𪏭儞𪏭二尾摩梨引三多摩梨引四虎唧虎唧五悉哩六底哩七薩梨引囉八復次阿難又此大明有大功德。",
        "from": "T21n1377_001:0899b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是大明時。",
        "from": "T21n1377_001:0899b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若鬼眾聞者驚怖戰掉。",
        "from": "T21n1377_001:0899b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若天眾聞者悉來集會。",
        "from": "T21n1377_001:0899b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若龍王若部多王聞者亦悉來集。",
        "from": "T21n1377_001:0899b24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若梵王帝釋大自在天童子天等。",
        "from": "T21n1377_001:0899b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞是大明者皆從異處咸來集此。",
        "from": "T21n1377_001:0899b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱讚隨喜寶帶大明。",
        "from": "T21n1377_001:0899b26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時慈氏菩薩摩訶薩。",
        "from": "T21n1377_001:0899b27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊。",
        "from": "T21n1377_001:0899b27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦為羅睺羅童子及一切眾生。",
        "from": "T21n1377_001:0899b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作擁護法救諸難事。",
        "from": "T21n1377_001:0899b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝持護念息災安樂。",
        "from": "T21n1377_001:0899b29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀杖諸毒不能為害。",
        "from": "T21n1377_001:0899b29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡友禁縛自然遠離。",
        "from": "T21n1377_001:0899c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以是利益故隨喜宣說寶帶大明曰。",
        "from": "T21n1377_001:0899c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛𭔞他引一句阿嚩那引底二鉢那引儞三阿尼那引努四祖盧祖盧五鳴哩馱二合難尼引六阿馱難尼引七三滿多難尼引八儞哩捫二合尼引九儞哩彌二合引牟儞引十扇引底莎悉爹二合野曩十一摩摩薩哩嚩二合薩埵引喃引左十二犖力角切下同叉俱嚕莎引賀引十三爾時娑婆世界主大梵天王。",
        "from": "T21n1377_001:0899c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦隨喜宣說寶帶大明曰。",
        "from": "T21n1377_001:0899c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薩拽替引唐一句盎虞哩引二半虞哩引三阿訥哩引四農訥哩引五室契六室珂哩引七訥捺沒二合哩引八訥哩訥二合囉野底九阿左阿左十尾母左十一扇引鼎莎悉爹二合野曩俱嚕十二摩摩薩哩嚩二合薩埵引難引左莎引賀引十三爾時夜摩天子。",
        "from": "T21n1377_001:0899c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦隨喜宣說寶帶大明曰。",
        "from": "T21n1377_001:0899c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薩拽替引唐一句囉引瑟致哩三合二鉢那引瑟致哩三合引三嚕地囉羯蘭尼二合引四儼鼻引哩引五訥哩那二合哩引六母唧舍引計也二合囉引𡁠引七扇引鼎莎悉爹二合野曩羯噌彌八摩摩犖叉俱嚕沙引賀引九爾時帝釋天主。",
        "from": "T21n1377_001:0899c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦隨喜宣說寶帶大明曰。",
        "from": "T21n1377_001:0899c22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薩拽替引唐一句難尼引二達努試引三蘇難尼引蘇難尼引四帝引難尼引帝引難尼引五訥哩彌二合引六蘇彌引七捺摩泥引八捺彌引九底梨十彌梨十一摩摩薩哩嚩二合薩埵引難引左犖叉引羯嚕彌十二扇引鼎莎悉爹二合野曩十三野引嚩細引摩滿唐羯嚕彌莎引賀引十四帝釋天主說是大明已。",
        "from": "T21n1377_001:0899c29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說大明曰。",
        "from": "T21n1377_001:0899c29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薩拽替引唐一句難尼引二達努試引三羯捺踰二合引多訖哩二合四目軌引五入嚩二合梨引六捺彌引七尾訶梨引八娑引哩引九娑引囉嚩底十扇引鼎莎悉爹二合野曩十一摩摩薩哩嚩二合薩埵引難引左十二野引嚩細引摩引滿度引婆嚩覩莎引賀引十三爾時持國天王。",
        "from": "T21n1377_001:0900a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦隨喜宣說寶帶大明曰。",
        "from": "T21n1377_001:0900a07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薩拽替引唐一句達哩引達哩引二達囉尼引達哩引三阿疸帝引四布嚕五母嚕六扇引鼎莎悉爹二合野曩七摩摩薩哩嚩二合薩埵引難引左犖叉羯嚕引彌八野引嚩細引摩引滿度引婆嚩覩莎引賀引九爾時增長天王。",
        "from": "T21n1377_001:0900a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦隨喜宣說寶帶大明曰。",
        "from": "T21n1377_001:0900a13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薩拽替引唐一句案拏哩引二半拏哩引三羯囉引尼引四羯囉鉢帝引五羯囉嚕毘引六羯哩達哩引七羯諾羯鉢帝哩二合引八嚕嚕彌引九叉叉彌引十阿引叱彌引十一博叱扇引底莎悉爹二合野曩十二摩摩薩哩嚩二合薩埵引難引左犖叉引俱嚕十三野引嚩細引摩引滿馱婆嚩覩莎引賀引十四爾時廣目天王。",
        "from": "T21n1377_001:0900a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦隨喜宣說寶帶大明曰。",
        "from": "T21n1377_001:0900a21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薩拽替引唐一句摩惹羅摩惹羅二惹惹摩羅三地哩地哩四計哩計哩五三滿多計哩六扇引鼎莎悉爹二合野曩七摩摩薩哩嚩二合薩埵引難引左八野引嚩細引摩引滿唐羯嚕彌莎引賀引九爾時月天子。",
        "from": "T21n1377_001:0900a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦隨喜宣說寶帶大明曰。",
        "from": "T21n1377_001:0900a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薩栧替引唐一句唵引二嚩引禰引三尾摩梨引四鉢囉二合婆引莎哩引五設多囉濕彌二合引六薩賀薩囉二合囉濕彌二合引七鉢囉二合底薩囉底八扇引鼎莎悉爹二合野曩俱嚕九摩摩薩哩嚩二合薩埵引難引左十野引嚩細引摩引滿唐羯嚕彌莎引賀引十一爾時諸天眾。",
        "from": "T21n1377_001:0900b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦隨喜宣說寶帶大明曰。",
        "from": "T21n1377_001:0900b05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薩拽替引唐一句俱嚕俱嚕二蘇嚕蘇嚕三布嚕布嚕四扇引鼎莎悉爹二合野曩俱嚕五摩摩薩哩嚩二合薩埵引難引左六野引嚩細引摩引滿唐羯嚕彌莎引賀引七爾時諸星宿眾。",
        "from": "T21n1377_001:0900b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦隨喜宣說寶帶大明曰。",
        "from": "T21n1377_001:0900b10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薩拽替引唐一句怛梨引二怛摩梨引三阿梨引四捺梨五阿引迦引舍尾摩梨引六扇引鼎莎悉爹二合野曩羯嚕彌七摩摩薩哩嚩二合薩埵引難引左八野引嚩細引摩引滿唐羯嚕彌莎引賀引九爾時諸龍眾。",
        "from": "T21n1377_001:0900b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦隨喜宣說寶帶大明曰。",
        "from": "T21n1377_001:0900b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薩拽替引唐一句末梨引二摩梨引三阿左梨引四末提引五阿引戍末提引六布引計引七扇引鼎莎悉爹二合野曩俱嚕八摩摩薩哩嚩二合薩埵引難引左九野引嚩細引摩引滿唐羯嚕彌莎引賀引十爾時二十八大夜叉將。",
        "from": "T21n1377_001:0900b22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦隨喜宣說寶帶大明曰。",
        "from": "T21n1377_001:0900b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薩拽替引唐一句捺哩二合茶鉢底捺哩二合茶鉢底二嚕嚕達囉引嚕嚕達囉引三扇引鼎莎悉爹二合野曩羯嚕彌四摩摩薩哩嚩二合薩埵引難引左五野引嚩細引摩引滿唐羯嚕彌莎引賀引六爾時必隷多眾必舍左眾鳩盤茶眾。",
        "from": "T21n1377_001:0900b29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦同隨喜宣說寶帶大明曰。",
        "from": "T21n1377_001:0900c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薩拽替引唐一句尾部底二三嚩哩多二合那引尾部底三扇引鼎莎悉爹二合野曩四摩摩薩哩嚩二合薩埵引難引左五野引嚩細引摩引滿唐羯嚕彌莎引賀引六爾時布單那羯吒布單那眾。",
        "from": "T21n1377_001:0900c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦隨喜宣說寶帶大明曰。",
        "from": "T21n1377_001:0900c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薩拽替引唐一句鉢訥彌引二羯囉引尼引三嗢迭多四祖蘭拏二合薩彌引囉尼引五麁麁鉢帝引六扇引鼎莎悉爹二合野曩羯嚕彌七摩摩薩哩嚩二合薩埵引室左二合八野引嚩細引摩引滿唐羯嚕彌莎引賀引九爾時諸母鬼眾。",
        "from": "T21n1377_001:0900c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦隨喜宣說寶帶大明曰。",
        "from": "T21n1377_001:0900c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薩拽替引唐一句計引梨引二摩計引梨引三拏哩引四拏摩哩引五扇引鼎莎悉爹二合野曩羯嚕彌六摩摩薩哩嚩二合薩埵引難引左七野引嚩細引摩引滿唐羯嚕彌莎引賀引八爾時四方四大羅剎女。",
        "from": "T21n1377_001:0900c18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦隨喜宣說寶帶大明曰。",
        "from": "T21n1377_001:0900c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薩拽替引唐一句阿引泥引播引泥二渴𡁠引三渴嚩𡁠引四枯枯哩五梨必六鉢哩賀哩引七扇引鼎莎悉爹一合野曩羯嚕彌八摩摩薩哩嚩二合薩埵引難引左九野引嚩細引摩引滿唐羯嚕彌莎引賀引十爾時會中有一大羅剎女名囉彌拏。",
        "from": "T21n1377_001:0900c25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦隨喜宣說寶帶大明曰。",
        "from": "T21n1377_001:0900c26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薩拽替引唐一句捺哩那二合哩引捺哩那二合哩引二賀賀賀賀三呬呬呬呬四虎虎虎虎五試軌軌試軌軌六唧軌七尾目契引八摩珂九惹珂十契吒十一彌致十二遏吒十三遏吒吒十四囉吒吒十五訖囉二合摩惹十六俱儒仁祖切下同俱儒十七訥訥訥十八訥嚕二合訥嚕二合訥嚕二合十九砌彌梨二十彌彌梨二十一阿那聒聒二十二渴訥彌二合二十三訥訥訥二十四底體二十五底彌二十六底儞二十七體梨二十八彌吒二十九唧底三十彌致三十一梨致三十二必致三十三必帝哩二合三十四閉引帝哩二合尼三十五訥呬儞三十六母呬儞三十七羅係引三十八嚩係引三十九𭡠波曳引四十時羅剎女說是大明已。",
        "from": "T21n1377_001:0901a10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前白佛言我此所說寶帶大明中眷屬大明。",
        "from": "T21n1377_001:0901a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善為一切眾生而作擁護息災安樂。",
        "from": "T21n1377_001:0901a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀杖惡毒不能侵害。",
        "from": "T21n1377_001:0901a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切惡友自然遠離。",
        "from": "T21n1377_001:0901a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過一由旬二由旬乃至十二由旬。",
        "from": "T21n1377_001:0901a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若天若龍若夜叉若羅剎。",
        "from": "T21n1377_001:0901a14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若乾闥婆若阿修羅。",
        "from": "T21n1377_001:0901a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若迦樓羅若緊那羅。",
        "from": "T21n1377_001:0901a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若摩睺羅伽若鳩盤茶。",
        "from": "T21n1377_001:0901a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若必舍左若必隷多。",
        "from": "T21n1377_001:0901a16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若布單那若羯吒布單那。",
        "from": "T21n1377_001:0901a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若塞健馱若阿波三摩囉。",
        "from": "T21n1377_001:0901a17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若吠多拏。",
        "from": "T21n1377_001:0901a17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一日瘧若二日瘧。",
        "from": "T21n1377_001:0901a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至人非人等。",
        "from": "T21n1377_001:0901a18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伺求其便欲惱害者皆不得便。",
        "from": "T21n1377_001:0901a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若得便者無有是處。",
        "from": "T21n1377_001:0901a19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除宿業障不可制止。",
        "from": "T21n1377_001:0901a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此大明能令一切眾生。",
        "from": "T21n1377_001:0901a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切處息一切怖。",
        "from": "T21n1377_001:0901a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離一切嬈亂等事。",
        "from": "T21n1377_001:0901a21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告尊者阿難言。",
        "from": "T21n1377_001:0901a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如我所說寶帶大明。",
        "from": "T21n1377_001:0901a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及此羅剎女等所說諸寶帶大明眷屬明句。",
        "from": "T21n1377_001:0901a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大功德汝當受持宣通流布。",
        "from": "T21n1377_001:0901a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此大明亦能禁止諸惡風雲。",
        "from": "T21n1377_001:0901a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若持是明者善自作護復能護他。",
        "from": "T21n1377_001:0901a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若城邑聚落乃至一切處悉能擁護。",
        "from": "T21n1377_001:0901a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此大明於一切天龍鬼神。",
        "from": "T21n1377_001:0901a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至沙門婆羅門外道梵志。",
        "from": "T21n1377_001:0901a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尼乾陀等中。",
        "from": "T21n1377_001:0901a28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有惡毒心者不調伏者。",
        "from": "T21n1377_001:0901a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作惱害者不吉祥者。",
        "from": "T21n1377_001:0901a29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉能調伏不為惱害。",
        "from": "T21n1377_001:0901b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至一切大惡宿曜悉能制止。",
        "from": "T21n1377_001:0901b01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復阿難寶帶大明最上最勝。",
        "from": "T21n1377_001:0901b02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有沙門婆羅門。",
        "from": "T21n1377_001:0901b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外道梵志尼乾陀。",
        "from": "T21n1377_001:0901b03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及梵王帝釋四大天王。",
        "from": "T21n1377_001:0901b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲自在天日月星宿天龍鬼神夜叉羅剎人與非人。",
        "from": "T21n1377_001:0901b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "山峯住者山下住者。",
        "from": "T21n1377_001:0901b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空住者。",
        "from": "T21n1377_001:0901b05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至隨一切處住者。",
        "from": "T21n1377_001:0901b06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸部多等。",
        "from": "T21n1377_001:0901b06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼彼所有一切呪句。",
        "from": "T21n1377_001:0901b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此悉能斷不為他呪之所斷故。",
        "from": "T21n1377_001:0901b07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復阿難寶帶大明。",
        "from": "T21n1377_001:0901b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善作擁護善救善攝。",
        "from": "T21n1377_001:0901b08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能與眾生息災安樂。",
        "from": "T21n1377_001:0901b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀杖惡毒不能侵害。",
        "from": "T21n1377_001:0901b09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切惡友自然遠離。",
        "from": "T21n1377_001:0901b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過一由旬二由旬。",
        "from": "T21n1377_001:0901b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至十二由旬。",
        "from": "T21n1377_001:0901b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復阿難我不見彼一切世間天人魔梵等。",
        "from": "T21n1377_001:0901b12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有違越此寶帶大明者。",
        "from": "T21n1377_001:0901b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若違越者無有是處。",
        "from": "T21n1377_001:0901b13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故阿難當知一切佛真實法真實僧真實實語真實一切哩尸眾真實。",
        "from": "T21n1377_001:0901b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故於此祕密法門無違越者。",
        "from": "T21n1377_001:0901b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難此五種真實不可違越。",
        "from": "T21n1377_001:0901b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛真實不可違越。",
        "from": "T21n1377_001:0901b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法真實不可違越。",
        "from": "T21n1377_001:0901b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧真實不可違越。",
        "from": "T21n1377_001:0901b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實語真實不可違越。",
        "from": "T21n1377_001:0901b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "哩尸眾真實不可違越。",
        "from": "T21n1377_001:0901b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故阿難此寶帶大明。",
        "from": "T21n1377_001:0901b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有受持者。",
        "from": "T21n1377_001:0901b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得離一切病。",
        "from": "T21n1377_001:0901b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解脫一切縛。",
        "from": "T21n1377_001:0901b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "息除一切惡。",
        "from": "T21n1377_001:0901b20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得大安樂最上吉祥。",
        "from": "T21n1377_001:0901b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說此經已。",
        "from": "T21n1377_001:0901b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅睺羅童子得離怖畏。",
        "from": "T21n1377_001:0901b22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難等諸大眾。",
        "from": "T21n1377_001:0901b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛所說皆大歡喜信受奉行。",
        "from": "T21n1377_001:0901b23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說寶帶陀羅尼經",
        "from": "T21n1377_001:0901b24_100",
        "context_with_next_sentence": false
    }
]