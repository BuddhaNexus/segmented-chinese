[
    {
        "source_sentence": "佛說北斗七星延命經",
        "from": "T21n1307_001:0425b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婆羅門僧將到此經唐朝受持",
        "from": "T21n1307_001:0425b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　イ",
        "from": "T21n1307_001:0425c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "子生人。",
        "from": "T21n1307_001:0425c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向此星下生。",
        "from": "T21n1307_001:0425c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祿食黍。",
        "from": "T21n1307_001:0425c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有厄宜供養此經及帶本星符。",
        "from": "T21n1307_001:0425c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大吉。",
        "from": "T21n1307_001:0425c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　ロ丑生亥生人。",
        "from": "T21n1307_001:0425c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同向此星下生。",
        "from": "T21n1307_001:0425c09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祿食粟。",
        "from": "T21n1307_001:0425c09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有厄宜供養此經及帶本星符。",
        "from": "T21n1307_001:0425c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大吉。",
        "from": "T21n1307_001:0425c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　ハ寅生戌生人。",
        "from": "T21n1307_001:0425c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同向此星下生。",
        "from": "T21n1307_001:0425c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祿食粳米。",
        "from": "T21n1307_001:0425c12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有厄宜供養此經及帶本星符。",
        "from": "T21n1307_001:0425c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大吉。",
        "from": "T21n1307_001:0425c13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　ニ卯生酉生人。",
        "from": "T21n1307_001:0425c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同向此星下生。",
        "from": "T21n1307_001:0425c17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祿食小豆。",
        "from": "T21n1307_001:0425c17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有厄宜供養此經及帶本星符。",
        "from": "T21n1307_001:0425c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大吉。",
        "from": "T21n1307_001:0425c18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　ホ辰生申生人。",
        "from": "T21n1307_001:0425c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同向此星下生。",
        "from": "T21n1307_001:0425c20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祿食麻子。",
        "from": "T21n1307_001:0425c20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有厄宜供養此經及帶本星符。",
        "from": "T21n1307_001:0425c21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大吉。",
        "from": "T21n1307_001:0425c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　ヘト巳生未生人。",
        "from": "T21n1307_001:0425c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同向此星下生。",
        "from": "T21n1307_001:0425c24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祿食大豆。",
        "from": "T21n1307_001:0425c24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有厄宜供養此經及帶本星符。",
        "from": "T21n1307_001:0425c25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大吉。",
        "from": "T21n1307_001:0425c25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　チ午生人。",
        "from": "T21n1307_001:0425c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向此星下生。",
        "from": "T21n1307_001:0425c28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祿食小豆。",
        "from": "T21n1307_001:0425c28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有厄宜供養此經及帶本星符。",
        "from": "T21n1307_001:0425c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大吉。",
        "from": "T21n1307_001:0425c29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無貪狼星。",
        "from": "T21n1307_001:0426a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是東方最勝世界運意通證如來佛。",
        "from": "T21n1307_001:0426a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無巨門星。",
        "from": "T21n1307_001:0426a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是東方妙寶世界光音自在如來佛。",
        "from": "T21n1307_001:0426a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無祿存星。",
        "from": "T21n1307_001:0426a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是東方圓滿世界金色成就如來佛。",
        "from": "T21n1307_001:0426a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無文曲星。",
        "from": "T21n1307_001:0426a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是東方無憂世界最勝吉祥如來佛。",
        "from": "T21n1307_001:0426a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無廉貞星。",
        "from": "T21n1307_001:0426a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是東方淨住世界廣達智辨如來佛。",
        "from": "T21n1307_001:0426a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無武曲星。",
        "from": "T21n1307_001:0426a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是東方法意世界法海遊戲如來佛。",
        "from": "T21n1307_001:0426a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無破軍星。",
        "from": "T21n1307_001:0426a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是東方琉璃世界藥師琉璃光如來佛。",
        "from": "T21n1307_001:0426a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遇行年災厄禮此經七拜。",
        "from": "T21n1307_001:0426a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛告文殊師利菩薩。",
        "from": "T21n1307_001:0426a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所說此經。",
        "from": "T21n1307_001:0426a16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大威神有大威力。",
        "from": "T21n1307_001:0426a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能救一切眾生重罪能滅一切業障。",
        "from": "T21n1307_001:0426a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有比丘僧比丘。",
        "from": "T21n1307_001:0426a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宰官居士善男子善女人。",
        "from": "T21n1307_001:0426a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若貴若賤大小生命。",
        "from": "T21n1307_001:0426a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆屬北斗七星所管。",
        "from": "T21n1307_001:0426a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若聞此經受持供養轉讀。",
        "from": "T21n1307_001:0426a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勸於朋友親族骨肉受持者現世獲福後世得生天上。",
        "from": "T21n1307_001:0426a21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人。",
        "from": "T21n1307_001:0426a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或先亡過者墮於地獄。",
        "from": "T21n1307_001:0426a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或受種種苦楚。",
        "from": "T21n1307_001:0426a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若聞此經信敬供養。",
        "from": "T21n1307_001:0426a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得先亡離於地獄。",
        "from": "T21n1307_001:0426a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生於極樂世界　若有善男子善女人。",
        "from": "T21n1307_001:0426a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或被鬼魅所侵邪魔所嬈。",
        "from": "T21n1307_001:0426a25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡夢怪異魂魄驚恐。",
        "from": "T21n1307_001:0426a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若聞此經受持供養。",
        "from": "T21n1307_001:0426a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得魂魄安寧。",
        "from": "T21n1307_001:0426a26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永無恐怖　若善男子善女人。",
        "from": "T21n1307_001:0426a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有仕官身役善發征行。",
        "from": "T21n1307_001:0426a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遇此經信敬供養。",
        "from": "T21n1307_001:0426a28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得仕官高遷甚大吉　若善男子善女人。",
        "from": "T21n1307_001:0426a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或是疾病纏身。",
        "from": "T21n1307_001:0426a29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲求輕差。",
        "from": "T21n1307_001:0426b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當於淨室燒香供養此經。",
        "from": "T21n1307_001:0426b01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疾病痊瘥　若善男子善女人。",
        "from": "T21n1307_001:0426b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲得進達及以出行經紀。",
        "from": "T21n1307_001:0426b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求財稱遂興生貨賣。",
        "from": "T21n1307_001:0426b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遇此經信敬供養。",
        "from": "T21n1307_001:0426b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得求財稱遂出入大吉。",
        "from": "T21n1307_001:0426b04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人。",
        "from": "T21n1307_001:0426b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或養蠶虛耗六畜不安。",
        "from": "T21n1307_001:0426b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於淨室燒香供養此經。",
        "from": "T21n1307_001:0426b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得田蠶遂意六畜孳榮。",
        "from": "T21n1307_001:0426b06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永無損失亦無災障　若有女人懷胎難月。",
        "from": "T21n1307_001:0426b07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遇此經信敬供養。",
        "from": "T21n1307_001:0426b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即一得母子分解厄難消除。",
        "from": "T21n1307_001:0426b08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所生兒女。",
        "from": "T21n1307_001:0426b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆得端正長命果報　若善男子善女人。",
        "from": "T21n1307_001:0426b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須知北斗七星管人生命。",
        "from": "T21n1307_001:0426b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一生之中所有災厄。",
        "from": "T21n1307_001:0426b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "官事口舌釜鳴百怪。",
        "from": "T21n1307_001:0426b11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遇此經信敬供養。",
        "from": "T21n1307_001:0426b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一無妨害　爾時文殊師利言善男子善女人恭敬信受作禮而散。",
        "from": "T21n1307_001:0426b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說北斗七星延命經薄草決十七本命下。",
        "from": "T21n1307_001:0426b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "引用此經。",
        "from": "T21n1307_001:0426b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今年幸得栂尾山法鼓臺傳習之本於洛北福勝密寺。",
        "from": "T21n1307_001:0426b15_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施國訓鑄版。",
        "from": "T21n1307_001:0426b15_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "于時享和二歲次壬戌紀號仲夏月沙門快道記豐山小池坊輪下總持院　　一挍加筆訖慈順。",
        "from": "T21n1307_001:0426b16_35",
        "context_with_next_sentence": true
    }
]