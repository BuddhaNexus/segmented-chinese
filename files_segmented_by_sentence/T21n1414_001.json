[
    {
        "source_sentence": "佛說金身陀羅尼經",
        "from": "T21n1414_001:0928a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝奉大夫試光祿卿",
        "from": "T21n1414_001:0928a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳法大師賜紫臣施護奉　詔譯",
        "from": "T21n1414_001:0928a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊。",
        "from": "T21n1414_001:0928a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普為利益一切眾生。",
        "from": "T21n1414_001:0928a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此陀羅尼曰。",
        "from": "T21n1414_001:0928a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那謨引沒馱引喃引一句摩帝引多引那引誐多鉢囉二合怛踰二合怛半二合那引喃引二那謨引阿彌多引婆引野怛他引誐多引野阿囉喝二合帝引三藐訖三二合沒馱引野三怛𭔞切身下同他引四唵引五摩引尾引哩引摩引尾引哩引六彌引馱引嚩帝引彌引馱引嚩帝引七阿蜜哩二合覩引訥婆二合吠引八沒馱摩帝引九沒馱婆引始多十薩哩嚩二合達哩謨二合引勒迦二合引入嚩二合引里儞十一沒提沒提十二摩賀引沒提十三摩賀引尾引哩引十四吠引誐嚩底十五誐嚕拏吠引誐嚩底十六印捺囉二合嚩日囉二合吠引誐嚩底十七沒馱尾路吉帝引十八牟儞牟儞十九摩賀引牟儞發吒半音二十那謨引沒馱達哩摩二合僧伽喃引末梨引那二十一薩哩嚩二合藥叉囉引叉娑二十二必舍引左二十三酤引瑟曼二合引拏二十四布引怛那二十五羯吒布怛那二十六薩哩嚩二合屹囉二合賀禰引嚩引𭔞引二十七訥瑟吒二合唧當引二十八波囉閉引拏剛引二十九屹哩二合恨拏二合屹哩二合恨拏二合三十屹囉二合娑屹囉二合娑三十一摩引囉摩引囉三十二伴惹伴惹三十三捺賀捺賀三十四鉢左鉢左三十五賀那賀那三十六薩哩嚩二合沒馱引喃引末梨引那三十七那引設野那引設野三十八頻捺頻捺三十九親捺親捺四十牟嚕牟嚕四十一尾捺囉二合引鉢野尾捺囉二合引鉢野四十二薩哩嚩二合囉引叉桑引四十三阿𭔞引室左二合摩耨賒阿摩耨賒引四十四滿馱滿馱四十五桑酤引左桑酤引左四十六尾酤引左尾酤引左四十七塞怖二合引吒塞怖二合引吒四十八怛哩惹二合怛哩惹二合四十九誐哩惹二合誐哩惹二合五十賀那賀那五十一薩哩嚩二合滿怛囕二合五十二薩哩吠二合引嚕引昂引五十三尾賀那尾賀那五十四犖力角切叉𤚥引薩波哩囉引囉五十五薩哩嚩二合薩埵引難引左莎引賀引五十六佛言此陀羅尼有大功德。",
        "from": "T21n1414_001:0928b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人能戴於頂上。",
        "from": "T21n1414_001:0928b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有一切羅剎必舍左鬼等。",
        "from": "T21n1414_001:0928b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於千由旬內不能侵害。",
        "from": "T21n1414_001:0928b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又若有人當誦此陀羅尼一遍。",
        "from": "T21n1414_001:0928b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時。",
        "from": "T21n1414_001:0928b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有諸天子及其眷屬。",
        "from": "T21n1414_001:0928b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住千由旬外為作擁護。",
        "from": "T21n1414_001:0928b16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又若有人誦此陀羅尼時。",
        "from": "T21n1414_001:0928b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有一切設咄嚕及彼大惡虎狼蟲獸。",
        "from": "T21n1414_001:0928b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而自攝伏不能為害。",
        "from": "T21n1414_001:0928b18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又若有人能發至誠。",
        "from": "T21n1414_001:0928b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於日日中誦此陀羅尼數滿百遍。",
        "from": "T21n1414_001:0928b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人即得安膳那藥成就。",
        "from": "T21n1414_001:0928b20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用點眼即能見彼三世中事。",
        "from": "T21n1414_001:0928b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又若有人誦此陀羅尼時。",
        "from": "T21n1414_001:0928b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令忿怒心者生敬愛心。",
        "from": "T21n1414_001:0928b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又若有人至心誦此陀羅尼二洛叉遍。",
        "from": "T21n1414_001:0928b24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切煩惱悉能息滅　又若有人長時誦此陀羅尼。",
        "from": "T21n1414_001:0928b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人即得宿命智。",
        "from": "T21n1414_001:0928b26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又若有人以此陀羅尼。",
        "from": "T21n1414_001:0928b27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持淨油塗於頭上腹上。",
        "from": "T21n1414_001:0928b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人若有頭病腹病及餘一切惡毒等病。",
        "from": "T21n1414_001:0928b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨所塗處病得消散。",
        "from": "T21n1414_001:0928b29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又若有人取一粒胡椒。",
        "from": "T21n1414_001:0928c01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此陀羅尼加持二十一遍。",
        "from": "T21n1414_001:0928c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持詣囉惹處。",
        "from": "T21n1414_001:0928c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨所願求皆得如意。",
        "from": "T21n1414_001:0928c02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此陀羅尼有大功德。",
        "from": "T21n1414_001:0928c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲成就如上等事。",
        "from": "T21n1414_001:0928c03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當受持。",
        "from": "T21n1414_001:0928c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說金身陀羅尼經",
        "from": "T21n1414_001:0928c05_100",
        "context_with_next_sentence": false
    }
]