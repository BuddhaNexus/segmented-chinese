[
    {
        "source_sentence": "佛說普賢曼拏羅經",
        "from": "T20n1126_001:0539a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試鴻臚少卿",
        "from": "T20n1126_001:0539a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳法大師臣施護奉　詔譯",
        "from": "T20n1126_001:0539a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T20n1126_001:0539a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在王舍城鷲峯山中。",
        "from": "T20n1126_001:0539a29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大比丘眾五十百千人俱。",
        "from": "T20n1126_001:0539b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有諸大菩薩摩訶薩。",
        "from": "T20n1126_001:0539b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆是一切白法具足。",
        "from": "T20n1126_001:0539b02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得無量智善巧方便能師子吼。",
        "from": "T20n1126_001:0539b03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其名曰。",
        "from": "T20n1126_001:0539b03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普賢菩薩摩訶薩。",
        "from": "T20n1126_001:0539b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶印手菩薩摩訶薩。",
        "from": "T20n1126_001:0539b04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嬉戲菩薩摩訶薩。",
        "from": "T20n1126_001:0539b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功德莊嚴菩薩摩訶薩。",
        "from": "T20n1126_001:0539b05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福德音聲菩薩摩訶薩。",
        "from": "T20n1126_001:0539b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大意菩薩摩訶薩。",
        "from": "T20n1126_001:0539b06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "德嚴菩薩摩訶薩。",
        "from": "T20n1126_001:0539b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等菩薩摩訶薩眾皆來集會。",
        "from": "T20n1126_001:0539b07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊以大慈大悲。",
        "from": "T20n1126_001:0539b08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普為現在未來諸修行人。",
        "from": "T20n1126_001:0539b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲修金剛薩埵祕密相應法。",
        "from": "T20n1126_001:0539b09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速疾成就者。",
        "from": "T20n1126_001:0539b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宣說金剛界大曼拏羅法。",
        "from": "T20n1126_001:0539b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言若有善男子。",
        "from": "T20n1126_001:0539b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲入金剛界大曼拏羅。",
        "from": "T20n1126_001:0539b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修習普賢金剛薩埵相應祕密法。",
        "from": "T20n1126_001:0539b12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速疾成就者。",
        "from": "T20n1126_001:0539b12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先當發起勇猛堅固殊勝之心。",
        "from": "T20n1126_001:0539b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求彼世間清淨勝師。",
        "from": "T20n1126_001:0539b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請受灌頂已。",
        "from": "T20n1126_001:0539b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彼師處傳受祕密相應印契儀法。",
        "from": "T20n1126_001:0539b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一不謬專注記憶勿令忘誤。",
        "from": "T20n1126_001:0539b15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後隨欲於清淨勝處。",
        "from": "T20n1126_001:0539b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修習是法以求悉地。",
        "from": "T20n1126_001:0539b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得勝處已。",
        "from": "T20n1126_001:0539b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先當澡浴清淨嚴潔身心。",
        "from": "T20n1126_001:0539b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便於彼處先結界護身。",
        "from": "T20n1126_001:0539b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後依法以香泥塗曼拏羅極令如法。",
        "from": "T20n1126_001:0539b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗畢便於此曼拏羅中。",
        "from": "T20n1126_001:0539b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒香散華種種供養。",
        "from": "T20n1126_001:0539b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向此曼拏羅作觀想。",
        "from": "T20n1126_001:0539b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想彼曼拏羅中有本尊如來。",
        "from": "T20n1126_001:0539b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得現前已不起于座。",
        "from": "T20n1126_001:0539b21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即取塗香塗手。",
        "from": "T20n1126_001:0539b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結印及念密言。",
        "from": "T20n1126_001:0539b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結淨三業印。",
        "from": "T20n1126_001:0539b22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手金剛掌三誦本密言。",
        "from": "T20n1126_001:0539b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引娑嚩二合婆引嚩󰡊馱引薩哩嚩二合達哩摩二合娑嚩二合婆引嚩󰡊度憾誦此密言已散印。",
        "from": "T20n1126_001:0539b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復言我今自無始已來。",
        "from": "T20n1126_001:0539b26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有一切麁重身口意業。",
        "from": "T20n1126_001:0539b27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今已清淨。",
        "from": "T20n1126_001:0539b27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既清淨已。",
        "from": "T20n1126_001:0539b27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願今所作一切事業。",
        "from": "T20n1126_001:0539b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則為入彼金剛法因。",
        "from": "T20n1126_001:0539b28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然更作意觀於本尊如來。",
        "from": "T20n1126_001:0539b29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願我所作事業皆得成就便歸命讚歎。",
        "from": "T20n1126_001:0539c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦密言曰。",
        "from": "T20n1126_001:0539c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引薩哩嚩二合怛他誐多引迦引野嚩枳唧二合多鉢囉二合拏昧嚩日囉二合滿那喃迦引嚕弭誦此密言已。",
        "from": "T20n1126_001:0539c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以四種禮禮曼拏羅中本尊如來。",
        "from": "T20n1126_001:0539c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮已長跪。",
        "from": "T20n1126_001:0539c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作金剛合掌而說是言。",
        "from": "T20n1126_001:0539c06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今發無上菩提心。",
        "from": "T20n1126_001:0539c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願應正等覺三世之師方便攝受。",
        "from": "T20n1126_001:0539c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令我正得三種學戒。",
        "from": "T20n1126_001:0539c08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸眾生以金剛堅固力。",
        "from": "T20n1126_001:0539c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住持三寶利樂眾生。",
        "from": "T20n1126_001:0539c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今於如來金剛界金剛薩埵真如相應祕密法中。",
        "from": "T20n1126_001:0539c10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從阿闍梨受大金剛族。",
        "from": "T20n1126_001:0539c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於六時修習行四種施法。",
        "from": "T20n1126_001:0539c11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於大寶族受平等相應法。",
        "from": "T20n1126_001:0539c12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於大蓮華族受三乘微妙祕密法。",
        "from": "T20n1126_001:0539c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於大事業族受具足一切供養事業。",
        "from": "T20n1126_001:0539c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從清淨真如生大菩提法。",
        "from": "T20n1126_001:0539c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我發無上菩提心。",
        "from": "T20n1126_001:0539c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受如是法已當依法修習。",
        "from": "T20n1126_001:0539c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為一切眾生而作饒益。",
        "from": "T20n1126_001:0539c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有一切眾生無主宰者為作主宰。",
        "from": "T20n1126_001:0539c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未解脫者令得解脫。",
        "from": "T20n1126_001:0539c17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未安樂者令得安樂。",
        "from": "T20n1126_001:0539c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未到彼岸者令到彼岸。",
        "from": "T20n1126_001:0539c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者發願已。",
        "from": "T20n1126_001:0539c18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦金剛視密言。",
        "from": "T20n1126_001:0539c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩吒於兩目　　應觀為日月",
        "from": "T20n1126_001:0539c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手金剛拳　　各安於腰側",
        "from": "T20n1126_001:0539c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曼拏與香華　　及餘供養具",
        "from": "T20n1126_001:0539c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因此目瞻視　　去垢成清淨",
        "from": "T20n1126_001:0539c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛視密言曰。",
        "from": "T20n1126_001:0539c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合捺哩二合瑟致二合摩吒二手金剛縛。",
        "from": "T20n1126_001:0539c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能解諸結使。",
        "from": "T20n1126_001:0539c26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨第八識中。",
        "from": "T20n1126_001:0539c26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切雜染種。",
        "from": "T20n1126_001:0539c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛縛密言曰。",
        "from": "T20n1126_001:0539c27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合滿馱三說以此印誦密言故。",
        "from": "T20n1126_001:0539c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當得自身無始已來一切結使之縛悉皆解脫。",
        "from": "T20n1126_001:0540a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當令修習一切功德圓滿。",
        "from": "T20n1126_001:0540a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次即以縛印於當心磔開。",
        "from": "T20n1126_001:0540a02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復誦密言曰。",
        "from": "T20n1126_001:0540a02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合滿馱怛囉二合吒半音以此印及誦密言。",
        "from": "T20n1126_001:0540a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當得自性金剛智慧頓顯發故。",
        "from": "T20n1126_001:0540a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以金剛鈴三振已。",
        "from": "T20n1126_001:0540a05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即次觀自身中。",
        "from": "T20n1126_001:0540a05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有惡字輪顯發。",
        "from": "T20n1126_001:0540a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大威德。",
        "from": "T20n1126_001:0540a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以是之故當使一切魔障不能惱亂。",
        "from": "T20n1126_001:0540a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復得金剛堅固心不退轉。",
        "from": "T20n1126_001:0540a07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得一切成就。",
        "from": "T20n1126_001:0540a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次誦心密言曰。",
        "from": "T20n1126_001:0540a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賀賀賀賀斛誦心密言已。",
        "from": "T20n1126_001:0540a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即入金剛薩埵堅固智。",
        "from": "T20n1126_001:0540a10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀想盡虛空界一切性悉皆平等。",
        "from": "T20n1126_001:0540a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同彼十六音聲。",
        "from": "T20n1126_001:0540a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次觀想自心成圓月輪。",
        "from": "T20n1126_001:0540a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於月輪中想迦字。",
        "from": "T20n1126_001:0540a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得字了了分明現已復想月輪外有一迦字。",
        "from": "T20n1126_001:0540a13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如星旋遶得現前已。",
        "from": "T20n1126_001:0540a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復想迦字。",
        "from": "T20n1126_001:0540a14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心月輪中生一金剛蓮華。",
        "from": "T20n1126_001:0540a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又於此蓮華中心出蓮華印。",
        "from": "T20n1126_001:0540a15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此印上觀一憾字。",
        "from": "T20n1126_001:0540a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得字了了分明現已。",
        "from": "T20n1126_001:0540a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復觀自身等同金剛薩埵無異無別不動不搖。",
        "from": "T20n1126_001:0540a17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即擲金剛杵作如是言。",
        "from": "T20n1126_001:0540a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今不易此身。",
        "from": "T20n1126_001:0540a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從金剛法生為金剛薩埵。",
        "from": "T20n1126_001:0540a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次又想自身成大三摩耶薩埵。",
        "from": "T20n1126_001:0540a19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結金剛大三摩耶印。",
        "from": "T20n1126_001:0540a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T20n1126_001:0540a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引三摩喻憾摩賀引三摩喻憾即以前三摩耶印。",
        "from": "T20n1126_001:0540a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印心額喉頂。",
        "from": "T20n1126_001:0540a22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結世尊大印誦密言曰。",
        "from": "T20n1126_001:0540a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引三摩耶薩怛嚩二合阿地底瑟姹二合娑嚩二合𤚥復誦密言曰。",
        "from": "T20n1126_001:0540a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合達哩摩二合呬哩二合由此印密言故令我身口意成金剛三業。",
        "from": "T20n1126_001:0540a28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結成就大印誦密言曰。",
        "from": "T20n1126_001:0540a29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引三摩耶惡次想前心月輪中。",
        "from": "T20n1126_001:0540b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛法惡字輪及蓮華。",
        "from": "T20n1126_001:0540b02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至蓮華中憾字。",
        "from": "T20n1126_001:0540b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一皆是我金剛法自性身中所顯發故。",
        "from": "T20n1126_001:0540b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是想已。",
        "from": "T20n1126_001:0540b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結金剛鉤印。",
        "from": "T20n1126_001:0540b04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手作金剛拳。",
        "from": "T20n1126_001:0540b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二小指反相鉤。",
        "from": "T20n1126_001:0540b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指屈如鉤。",
        "from": "T20n1126_001:0540b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T20n1126_001:0540b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合俱舍𠺁次結金剛索印。",
        "from": "T20n1126_001:0540b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不改前印。",
        "from": "T20n1126_001:0540b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以二頭指反相鉤。",
        "from": "T20n1126_001:0540b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名索印。",
        "from": "T20n1126_001:0540b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦密言曰。",
        "from": "T20n1126_001:0540b09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合播引舍吽次結金剛鎖印。",
        "from": "T20n1126_001:0540b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手內相叉。",
        "from": "T20n1126_001:0540b11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名金剛鎖。",
        "from": "T20n1126_001:0540b11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦密言曰。",
        "from": "T20n1126_001:0540b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合娑普二合吒鑁次結金剛鈴印。",
        "from": "T20n1126_001:0540b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手金剛縛。",
        "from": "T20n1126_001:0540b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三振如振鈴。",
        "from": "T20n1126_001:0540b14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名金剛鈴。",
        "from": "T20n1126_001:0540b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦密言曰。",
        "from": "T20n1126_001:0540b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合吠舍惡次觀自身同金剛薩埵智身。",
        "from": "T20n1126_001:0540b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得見前已。",
        "from": "T20n1126_001:0540b17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦密言曰。",
        "from": "T20n1126_001:0540b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "𠺁吽鑁斛次結三昧耶印。",
        "from": "T20n1126_001:0540b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦密言曰。",
        "from": "T20n1126_001:0540b20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引三昧耶娑怛鑁三合誦此密言已。",
        "from": "T20n1126_001:0540b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即復觀自心成圓月輪。",
        "from": "T20n1126_001:0540b22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想自身入此月輪。",
        "from": "T20n1126_001:0540b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成大金剛薩埵。",
        "from": "T20n1126_001:0540b23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦密言曰。",
        "from": "T20n1126_001:0540b23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引摩訶三昧耶娑怛鑁三合次誦金剛法密言曰。",
        "from": "T20n1126_001:0540b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合達哩摩二合呬哩二合誦此密言已。",
        "from": "T20n1126_001:0540b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復觀前心月輪變成赤色。",
        "from": "T20n1126_001:0540b27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中有本尊如來相好殊異。",
        "from": "T20n1126_001:0540b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目如蓮葉作微笑容。",
        "from": "T20n1126_001:0540b28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈悲歡喜安詳而住。",
        "from": "T20n1126_001:0540b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是觀已。",
        "from": "T20n1126_001:0540b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結四如來三昧耶密印。",
        "from": "T20n1126_001:0540c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各以本真言而用加持身。",
        "from": "T20n1126_001:0540c01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先結金剛薩埵印。",
        "from": "T20n1126_001:0540c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手作金剛縛。",
        "from": "T20n1126_001:0540c02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指竪如針。",
        "from": "T20n1126_001:0540c02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名金剛薩埵印。",
        "from": "T20n1126_001:0540c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此印安於心。",
        "from": "T20n1126_001:0540c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦密言曰。",
        "from": "T20n1126_001:0540c03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引薩哩嚩二合嚩日哩二合阿地底瑟姹二合娑嚩二合𤚥不改前印相。",
        "from": "T20n1126_001:0540c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二中指屈如寶。",
        "from": "T20n1126_001:0540c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名金剛寶。",
        "from": "T20n1126_001:0540c06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此印印於額。",
        "from": "T20n1126_001:0540c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦密言曰。",
        "from": "T20n1126_001:0540c07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引囉怛曩二合嚩日哩二合阿地底瑟姹二合娑嚩二合𤚥不改前印相。",
        "from": "T20n1126_001:0540c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二中指如蓮葉。",
        "from": "T20n1126_001:0540c10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名金剛蓮華印。",
        "from": "T20n1126_001:0540c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此印安於喉。",
        "from": "T20n1126_001:0540c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦密言曰。",
        "from": "T20n1126_001:0540c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引達哩摩二合嚩日哩二合阿地底瑟姹二合娑嚩二合𤚥不改前印相。",
        "from": "T20n1126_001:0540c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二中指相交竪。",
        "from": "T20n1126_001:0540c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為羯磨印。",
        "from": "T20n1126_001:0540c14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此印安於頂。",
        "from": "T20n1126_001:0540c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦密言曰。",
        "from": "T20n1126_001:0540c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引羯哩摩二合嚩日哩二合阿地底瑟姹二合娑嚩二合𤚥次復觀想前赤色月輪中本尊如來。",
        "from": "T20n1126_001:0540c18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結彈指請召印。",
        "from": "T20n1126_001:0540c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "交臂作金剛彈指。",
        "from": "T20n1126_001:0540c19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至召請一切佛平等亦同。",
        "from": "T20n1126_001:0540c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手彈指得一切善事速疾成就。",
        "from": "T20n1126_001:0540c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若右手彈指得速疾集會。",
        "from": "T20n1126_001:0540c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彈指密言曰。",
        "from": "T20n1126_001:0540c21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合三摩惹𠺁次稱一百八名頌曰。",
        "from": "T20n1126_001:0540c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛生大士　　金剛諸如來",
        "from": "T20n1126_001:0540c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛乘普賢　　金剛手頂禮",
        "from": "T20n1126_001:0540c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛王妙覺　　金剛鉤如來",
        "from": "T20n1126_001:0540c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅固不空王　　金剛仗頂禮",
        "from": "T20n1126_001:0540c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛愛大樂　　金剛箭降伏",
        "from": "T20n1126_001:0540c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛大魔欲　　金剛弓頂禮",
        "from": "T20n1126_001:0540c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛意妙峯　　金剛寂大愛",
        "from": "T20n1126_001:0541a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛歡喜王　　金剛喜頂禮",
        "from": "T20n1126_001:0541a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛寶妙利　　金剛空大寶",
        "from": "T20n1126_001:0541a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛虛空藏　　金剛藏頂禮",
        "from": "T20n1126_001:0541a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛光大焰　　金剛光佛日",
        "from": "T20n1126_001:0541a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛焰大照　　金剛光頂禮",
        "from": "T20n1126_001:0541a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛幢妙利　　金剛旗妙喜",
        "from": "T20n1126_001:0541a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛大寶幢　　堅固𢾛頂禮",
        "from": "T20n1126_001:0541a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛笑大喜　　金剛思大希",
        "from": "T20n1126_001:0541a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛愛歡喜　　金剛愛頂禮",
        "from": "T20n1126_001:0541a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛法妙利　　金剛蓮妙淨",
        "from": "T20n1126_001:0541a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙眼觀自在　　金剛眼頂禮",
        "from": "T20n1126_001:0541a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛利大乘　　金剛鉤大冑",
        "from": "T20n1126_001:0541a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙吉祥甚深　　金剛智頂禮",
        "from": "T20n1126_001:0541a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛因大剎　　金剛輪大乘",
        "from": "T20n1126_001:0541a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛妙慧轉　　金剛剎頂禮",
        "from": "T20n1126_001:0541a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛言妙明　　金剛念妙成",
        "from": "T20n1126_001:0541a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛成無言　　金剛言頂禮",
        "from": "T20n1126_001:0541a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛業妙智　　金剛事一切",
        "from": "T20n1126_001:0541a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛空大力　　金剛業頂禮",
        "from": "T20n1126_001:0541a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛護大進　　金剛冑大牢",
        "from": "T20n1126_001:0541a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忿怒妙精進　　堅精進頂禮",
        "from": "T20n1126_001:0541a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅食大善巧　　金剛牙大怖",
        "from": "T20n1126_001:0541a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛峯破魔　　堅威怒頂禮",
        "from": "T20n1126_001:0541a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛拳妙意　　金剛縛解脫",
        "from": "T20n1126_001:0541a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛拳平等　　堅固拳頂禮",
        "from": "T20n1126_001:0541a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由稱念此一百八名最上句故。",
        "from": "T20n1126_001:0541a27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是得一切最初灌頂故。",
        "from": "T20n1126_001:0541a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有於此名能尊重讚歎者。",
        "from": "T20n1126_001:0541a28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是受持大金剛故。",
        "from": "T20n1126_001:0541a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若常歌諷此名句者。",
        "from": "T20n1126_001:0541a29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是得持金剛譽。",
        "from": "T20n1126_001:0541b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故我已先說觀想。",
        "from": "T20n1126_001:0541b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本尊如來得見前已。",
        "from": "T20n1126_001:0541b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結金剛鉤印事業印召請。",
        "from": "T20n1126_001:0541b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得降臨已。",
        "from": "T20n1126_001:0541b02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結三昧耶印。",
        "from": "T20n1126_001:0541b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復誦密言曰。",
        "from": "T20n1126_001:0541b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合達哩摩二合𠺁吽鑁斛三昧耶薩怛鑁三合摩憾復誦金剛法密言曰。",
        "from": "T20n1126_001:0541b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合達哩摩二合呬哩二合次以二頭指相拄。",
        "from": "T20n1126_001:0541b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈如蓮華葉。",
        "from": "T20n1126_001:0541b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名金剛法三昧耶印。",
        "from": "T20n1126_001:0541b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次大羯摩印世尊大印次灌頂印。",
        "from": "T20n1126_001:0541b09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次獻五供養已。",
        "from": "T20n1126_001:0541b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即持念本所修習密言。",
        "from": "T20n1126_001:0541b10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數滿足已。",
        "from": "T20n1126_001:0541b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結印誦八供養明。",
        "from": "T20n1126_001:0541b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養佛賢聖等。",
        "from": "T20n1126_001:0541b11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以二手作金剛拳相應。",
        "from": "T20n1126_001:0541b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安腰側向左小低頭。",
        "from": "T20n1126_001:0541b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此名嬉戲印。",
        "from": "T20n1126_001:0541b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦密言曰。",
        "from": "T20n1126_001:0541b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合羅細次以二手作金剛拳。",
        "from": "T20n1126_001:0541b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二頭指向額。",
        "from": "T20n1126_001:0541b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及腦後如繫鬘勢。",
        "from": "T20n1126_001:0541b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此名曰鬘印。",
        "from": "T20n1126_001:0541b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦密言曰。",
        "from": "T20n1126_001:0541b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合麼隷次以二手如金剛掌反。",
        "from": "T20n1126_001:0541b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "展開五指如捧華勢。",
        "from": "T20n1126_001:0541b18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此名曰歌印。",
        "from": "T20n1126_001:0541b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦密言曰。",
        "from": "T20n1126_001:0541b19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合儗帝次以二手作金剛拳。",
        "from": "T20n1126_001:0541b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先虛心合掌後如舞勢。",
        "from": "T20n1126_001:0541b21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此名曰舞印。",
        "from": "T20n1126_001:0541b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦密言曰。",
        "from": "T20n1126_001:0541b22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合涅里二合帝也二合次以二手作金剛縛。",
        "from": "T20n1126_001:0541b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後下散。",
        "from": "T20n1126_001:0541b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此名獻香印。",
        "from": "T20n1126_001:0541b24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦密言曰。",
        "from": "T20n1126_001:0541b25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合度閉次以八指作金剛內縛。",
        "from": "T20n1126_001:0541b27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於心前仰開如捧華勢。",
        "from": "T20n1126_001:0541b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此名獻華印。",
        "from": "T20n1126_001:0541b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦密言曰。",
        "from": "T20n1126_001:0541b28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合補瑟閉二合次以二手作金剛縛。",
        "from": "T20n1126_001:0541c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪開二頭指此名獻燈印。",
        "from": "T20n1126_001:0541c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦密言曰。",
        "from": "T20n1126_001:0541c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合路計次以二手作金剛縛。",
        "from": "T20n1126_001:0541c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當自心上開塗。",
        "from": "T20n1126_001:0541c04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是獻塗香印。",
        "from": "T20n1126_001:0541c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦密言曰。",
        "from": "T20n1126_001:0541c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引嚩日囉二合巘第如是諸密言印契有大威力。",
        "from": "T20n1126_001:0541c07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當須運心利益自他。",
        "from": "T20n1126_001:0541c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復自住三昧耶相應。",
        "from": "T20n1126_001:0541c08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀想心中有唵字輪現。",
        "from": "T20n1126_001:0541c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得字現已。",
        "from": "T20n1126_001:0541c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復想唵字中。",
        "from": "T20n1126_001:0541c09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流出寶蓋寶幢寶幡腕釧耳鐶瓔珞衣冠等寶莊嚴具。",
        "from": "T20n1126_001:0541c10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至百味飲食七寶等雲。",
        "from": "T20n1126_001:0541c11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一運心。",
        "from": "T20n1126_001:0541c11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍於佛菩薩前虔恭奉獻以伸供養。",
        "from": "T20n1126_001:0541c12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即作金剛合掌印。",
        "from": "T20n1126_001:0541c12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散漫想獻十方一切佛菩薩等。",
        "from": "T20n1126_001:0541c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是供養已。",
        "from": "T20n1126_001:0541c13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即將持誦或所作善事迴向發願已。",
        "from": "T20n1126_001:0541c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即依法發遣奉送賢聖。",
        "from": "T20n1126_001:0541c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊說是法已。",
        "from": "T20n1126_001:0541c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在會諸大菩薩摩訶薩。",
        "from": "T20n1126_001:0541c16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及一切大苾芻眾等。",
        "from": "T20n1126_001:0541c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛所說歡喜信受禮佛而退。",
        "from": "T20n1126_001:0541c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說普賢曼拏羅經",
        "from": "T20n1126_001:0541c19_100",
        "context_with_next_sentence": false
    }
]