[
    {
        "source_sentence": "受用三水要行法",
        "from": "T45n1902_001:0902c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐三藏法師義淨撰",
        "from": "T45n1902_001:0902c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准依聖教。及西方現今眾生所用之水。有其",
        "from": "T45n1902_001:0902c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三別。",
        "from": "T45n1902_001:0902c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時水。",
        "from": "T45n1902_001:0902c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二非時水。",
        "from": "T45n1902_001:0902c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三觸用水。",
        "from": "T45n1902_001:0902c15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言時水者。",
        "from": "T45n1902_001:0902c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂是沙彌俗人。",
        "from": "T45n1902_001:0902c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自手濾漉觀知無蟲。",
        "from": "T45n1902_001:0902c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "午前任受而飲。",
        "from": "T45n1902_001:0902c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若大僧手觸盆羅及杓水即不堪入口。",
        "from": "T45n1902_001:0902c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而況食用。",
        "from": "T45n1902_001:0902c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有惡觸故即如僧家常用之水。",
        "from": "T45n1902_001:0902c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大僧豈可得觸。",
        "from": "T45n1902_001:0902c19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖大僧不觸。",
        "from": "T45n1902_001:0902c19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於午後時不合飲用。",
        "from": "T45n1902_001:0902c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然水體無觸。",
        "from": "T45n1902_001:0902c20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已是俗人等觸。",
        "from": "T45n1902_001:0902c20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶於染膩非全極淨。",
        "from": "T45n1902_001:0902c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故須受。",
        "from": "T45n1902_001:0902c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二非時淨水者。",
        "from": "T45n1902_001:0902c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂大苾芻及沙彌等用意之人。",
        "from": "T45n1902_001:0902c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並須澡豆及上屑等。",
        "from": "T45n1902_001:0902c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "連腕四指。",
        "from": "T45n1902_001:0902c23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸須淨洗無有垢膩。",
        "from": "T45n1902_001:0902c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瓦盆及羅并須新淨。",
        "from": "T45n1902_001:0902c24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不與垢膩相染者方得。",
        "from": "T45n1902_001:0902c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅濾此水。",
        "from": "T45n1902_001:0902c25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆用銅椀銅杓。",
        "from": "T45n1902_001:0902c25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灰揩去膩始得取水。",
        "from": "T45n1902_001:0902c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無此等可求。",
        "from": "T45n1902_001:0902c26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必有染木之器。",
        "from": "T45n1902_001:0902c26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不曾與觸膩相染。",
        "from": "T45n1902_001:0902c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每日淨洗塵垢不停者。",
        "from": "T45n1902_001:0902c27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通用亦得。",
        "from": "T45n1902_001:0902c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若常用水可貯在淨瓶。",
        "from": "T45n1902_001:0902c28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨瓶須是瓦。",
        "from": "T45n1902_001:0902c28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非銅澡罐。",
        "from": "T45n1902_001:0902c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由其瓶內有銅青不淨不得灰揩故。",
        "from": "T45n1902_001:0903a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拔出銅釵揩拭。",
        "from": "T45n1902_001:0903a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即知淨穢。",
        "from": "T45n1902_001:0903a01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然銅以灰揩為淨。",
        "from": "T45n1902_001:0903a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖教親說。",
        "from": "T45n1902_001:0903a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若澡豆洗。",
        "from": "T45n1902_001:0903a02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但去食膩。",
        "from": "T45n1902_001:0903a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "銅垢不除。",
        "from": "T45n1902_001:0903a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可取銅匙灰揩。",
        "from": "T45n1902_001:0903a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "足為目驗。",
        "from": "T45n1902_001:0903a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其瓦瓶水盡。",
        "from": "T45n1902_001:0903a03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每須洗濯中間方盛新水。",
        "from": "T45n1902_001:0903a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然五天之地。",
        "from": "T45n1902_001:0903a04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無將銅瓶為淨瓶者。",
        "from": "T45n1902_001:0903a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一為垢生帶觸。",
        "from": "T45n1902_001:0903a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二為銅腥損人。",
        "from": "T45n1902_001:0903a05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此之淨水時與非時。",
        "from": "T45n1902_001:0903a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "任情取飲。",
        "from": "T45n1902_001:0903a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是佛別開。",
        "from": "T45n1902_001:0903a06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以其淨故。",
        "from": "T45n1902_001:0903a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更不勞受。",
        "from": "T45n1902_001:0903a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若苾芻在非時中。",
        "from": "T45n1902_001:0903a07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煎藥煮茶作蜜漿等皆用此水。",
        "from": "T45n1902_001:0903a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得用前時水。",
        "from": "T45n1902_001:0903a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以有過故。",
        "from": "T45n1902_001:0903a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然用鎗杓椀器。",
        "from": "T45n1902_001:0903a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆須離食染。",
        "from": "T45n1902_001:0903a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並悉灰揩方合煮物。",
        "from": "T45n1902_001:0903a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其淨水盆瓶。",
        "from": "T45n1902_001:0903a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宜於淨處安置。",
        "from": "T45n1902_001:0903a10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瓫須淨物覆蓋。",
        "from": "T45n1902_001:0903a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瓶即置在竹籠。",
        "from": "T45n1902_001:0903a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得輒令觸。",
        "from": "T45n1902_001:0903a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲用水時先淨洗手。",
        "from": "T45n1902_001:0903a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或用乾牛糞。",
        "from": "T45n1902_001:0903a12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨揩手已。",
        "from": "T45n1902_001:0903a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無膩方觸。",
        "from": "T45n1902_001:0903a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或以淨絹布及葉。",
        "from": "T45n1902_001:0903a13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用替瓶咽。",
        "from": "T45n1902_001:0903a13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後方捉。",
        "from": "T45n1902_001:0903a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "律云。",
        "from": "T45n1902_001:0903a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除水及楊枝者。",
        "from": "T45n1902_001:0903a14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂此清淨之水。",
        "from": "T45n1902_001:0903a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非是餘二。",
        "from": "T45n1902_001:0903a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "楊枝若是新條濕者。",
        "from": "T45n1902_001:0903a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應須火淨受而嚼。",
        "from": "T45n1902_001:0903a15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故知不可直執戒文。",
        "from": "T45n1902_001:0903a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡欲以水入口。",
        "from": "T45n1902_001:0903a16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若飲若漱時與非時。",
        "from": "T45n1902_001:0903a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆須澡豆淨洗手洗淨兩脣。",
        "from": "T45n1902_001:0903a17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漱口再三方合飲水。",
        "from": "T45n1902_001:0903a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喫食亦然。",
        "from": "T45n1902_001:0903a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又中食了時。",
        "from": "T45n1902_001:0903a18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若恐淨瓶水少。",
        "from": "T45n1902_001:0903a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須令俗人授前時水。",
        "from": "T45n1902_001:0903a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚼齒木澡漱已。",
        "from": "T45n1902_001:0903a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然口津未得輒咽。",
        "from": "T45n1902_001:0903a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要須以此淨水三漱口已。",
        "from": "T45n1902_001:0903a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方是清淨得咽口津。",
        "from": "T45n1902_001:0903a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目見西方南海僧眾共行此法又此方古德律師有知斯事。",
        "from": "T45n1902_001:0903a22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然行之者希。",
        "from": "T45n1902_001:0903a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不如是。",
        "from": "T45n1902_001:0903a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘膩不除咽咽得罪。",
        "from": "T45n1902_001:0903a23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦齋不成。",
        "from": "T45n1902_001:0903a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三觸用水者。",
        "from": "T45n1902_001:0903a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但使無蟲。",
        "from": "T45n1902_001:0903a25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不論淨觸即得受用。",
        "from": "T45n1902_001:0903a25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂添觸瓶向大小便處。",
        "from": "T45n1902_001:0903a26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及洗手足。",
        "from": "T45n1902_001:0903a26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更餘用不得輒將入口。",
        "from": "T45n1902_001:0903a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "況食用耶。",
        "from": "T45n1902_001:0903a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此等三水觀知無蟲。",
        "from": "T45n1902_001:0903a27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至明相未出已來。",
        "from": "T45n1902_001:0903a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆隨事得用。",
        "from": "T45n1902_001:0903a28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明相既出即便不合無問多少。",
        "from": "T45n1902_001:0903a29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至瓶內一抄盆中一合。",
        "from": "T45n1902_001:0903b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉須銅盞。",
        "from": "T45n1902_001:0903b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明目觀察。",
        "from": "T45n1902_001:0903b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無蟲者。",
        "from": "T45n1902_001:0903b01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖經多日任用無犯。",
        "from": "T45n1902_001:0903b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方僧徒及俗人五戒以為急。",
        "from": "T45n1902_001:0903b02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若外方客僧不解此水淨觸法者。",
        "from": "T45n1902_001:0903b03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無容入寺。",
        "from": "T45n1902_001:0903b03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復西方寺法。",
        "from": "T45n1902_001:0903b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見有僧將淨瓶上廁飲觸瓶水者。",
        "from": "T45n1902_001:0903b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為滅法。",
        "from": "T45n1902_001:0903b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即擯出寺。",
        "from": "T45n1902_001:0903b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此言之。",
        "from": "T45n1902_001:0903b05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冀諸行人。",
        "from": "T45n1902_001:0903b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共為存護。",
        "from": "T45n1902_001:0903b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令佛法久住。",
        "from": "T45n1902_001:0903b06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能依教行者。",
        "from": "T45n1902_001:0903b06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是與佛在世無有異也。",
        "from": "T45n1902_001:0903b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又舊律十誦五十九云。",
        "from": "T45n1902_001:0903b08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有淨澡罐廁澡罐。",
        "from": "T45n1902_001:0903b08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四十一云。",
        "from": "T45n1902_001:0903b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有淨水瓶常水瓶。",
        "from": "T45n1902_001:0903b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又新譯有部律文。",
        "from": "T45n1902_001:0903b09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨瓶觸器極分明。",
        "from": "T45n1902_001:0903b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此並金口親言。",
        "from": "T45n1902_001:0903b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非是人造。",
        "from": "T45n1902_001:0903b10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寧容唯一銅瓶不分淨觸。",
        "from": "T45n1902_001:0903b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖同告語不齒在心。",
        "from": "T45n1902_001:0903b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈可以習俗生常故違聖教。",
        "from": "T45n1902_001:0903b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准如此理。",
        "from": "T45n1902_001:0903b12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸寺房中及行方等處。",
        "from": "T45n1902_001:0903b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有用水非常狼藉。",
        "from": "T45n1902_001:0903b13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或大盆貯。",
        "from": "T45n1902_001:0903b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或瓫子盛安瓶內。",
        "from": "T45n1902_001:0903b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆不合用。",
        "from": "T45n1902_001:0903b14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以是大僧手觸不好羅濾。",
        "from": "T45n1902_001:0903b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經宿不觀。",
        "from": "T45n1902_001:0903b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "停貯多時定有生命。",
        "from": "T45n1902_001:0903b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設使無蟲。",
        "from": "T45n1902_001:0903b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當第三水飲用得罪。",
        "from": "T45n1902_001:0903b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂由盆羅及杓是不淨手觸有不淨塵故。",
        "from": "T45n1902_001:0903b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又如尋常用水銅瓶。",
        "from": "T45n1902_001:0903b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若自取飲者。",
        "from": "T45n1902_001:0903b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以其不淨不受而飲。",
        "from": "T45n1902_001:0903b18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有不受罪。",
        "from": "T45n1902_001:0903b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若中前觸至過午飲。",
        "from": "T45n1902_001:0903b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增觸罪。",
        "from": "T45n1902_001:0903b19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若今日觸明日飲。",
        "from": "T45n1902_001:0903b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有宿觸罪。",
        "from": "T45n1902_001:0903b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不受而捉。",
        "from": "T45n1902_001:0903b20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有惡觸罪。",
        "from": "T45n1902_001:0903b20_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不淨洗手。",
        "from": "T45n1902_001:0903b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有污手捉飲器罪。",
        "from": "T45n1902_001:0903b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不淨洗脣口飲水。",
        "from": "T45n1902_001:0903b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有不淨罪。",
        "from": "T45n1902_001:0903b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此之六罪。",
        "from": "T45n1902_001:0903b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各有方便成十二罪。",
        "from": "T45n1902_001:0903b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆有不敬聖教罪。",
        "from": "T45n1902_001:0903b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此有因本。",
        "from": "T45n1902_001:0903b23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復二十四。",
        "from": "T45n1902_001:0903b23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "添前成三十六罪。",
        "from": "T45n1902_001:0903b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經宿覆藏。",
        "from": "T45n1902_001:0903b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有因本成七十二。",
        "from": "T45n1902_001:0903b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "添前有一百八罪。",
        "from": "T45n1902_001:0903b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此等若其不學。",
        "from": "T45n1902_001:0903b25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有不學無知二罪。",
        "from": "T45n1902_001:0903b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有因本成四百三十二罪。",
        "from": "T45n1902_001:0903b26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "添前總成五百四十罪。",
        "from": "T45n1902_001:0903b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復從覆藏已下。",
        "from": "T45n1902_001:0903b27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及不學無知。",
        "from": "T45n1902_001:0903b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆有不敬教方便根本。",
        "from": "T45n1902_001:0903b28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更有一千八罪。",
        "from": "T45n1902_001:0903b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "添前五百四十。",
        "from": "T45n1902_001:0903b29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總有一千五百四十罪。",
        "from": "T45n1902_001:0903b29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此等略據一咽一宿生罪多少。",
        "from": "T45n1902_001:0903c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若論十咽經宿罪。",
        "from": "T45n1902_001:0903c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有一萬五千四百八十。",
        "from": "T45n1902_001:0903c02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若多時覆藏展轉生罪。",
        "from": "T45n1902_001:0903c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至未懺已來。",
        "from": "T45n1902_001:0903c03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫知其數重者。",
        "from": "T45n1902_001:0903c03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸須對清淨人說悔。",
        "from": "T45n1902_001:0903c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若先知者無無知罪。",
        "from": "T45n1902_001:0903c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若非時飲鹽湯及蜜漿等。",
        "from": "T45n1902_001:0903c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鎗杓椀器用水羅濾。",
        "from": "T45n1902_001:0903c05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能如法。",
        "from": "T45n1902_001:0903c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或帶膩觸。",
        "from": "T45n1902_001:0903c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與時食相霑。",
        "from": "T45n1902_001:0903c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手口不淨。",
        "from": "T45n1902_001:0903c06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不仰手受。",
        "from": "T45n1902_001:0903c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸悉有罪然杏子茶湯西方不飲。",
        "from": "T45n1902_001:0903c07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若寺內皆須親自撿看。",
        "from": "T45n1902_001:0903c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必在俗家細問知其淨不。",
        "from": "T45n1902_001:0903c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不如法即不可飲。",
        "from": "T45n1902_001:0903c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈以口腹暫時所須。",
        "from": "T45n1902_001:0903c09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而當來長受辛苦。",
        "from": "T45n1902_001:0903c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共知㗨羅鉢龍故損一葉。",
        "from": "T45n1902_001:0903c10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現今未脫不記了時。",
        "from": "T45n1902_001:0903c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "況此常為能無業道。",
        "from": "T45n1902_001:0903c11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身吞一咽。",
        "from": "T45n1902_001:0903c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "罪乃無邊。",
        "from": "T45n1902_001:0903c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "負擔終身。",
        "from": "T45n1902_001:0903c12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誠可悲矣。",
        "from": "T45n1902_001:0903c12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故經云。",
        "from": "T45n1902_001:0903c12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿輕小過以為無殃。",
        "from": "T45n1902_001:0903c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又曰善護浮囊防其微隙。",
        "from": "T45n1902_001:0903c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大乘小乘意皆同此。",
        "from": "T45n1902_001:0903c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略陳要行。",
        "from": "T45n1902_001:0903c14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘如大律。",
        "from": "T45n1902_001:0903c14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願諸行者。",
        "from": "T45n1902_001:0903c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准聖教行。",
        "from": "T45n1902_001:0903c15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受用三水要行法",
        "from": "T45n1902_001:0903c16_100",
        "context_with_next_sentence": false
    }
]