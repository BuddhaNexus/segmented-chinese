[
    {
        "source_sentence": "佛說聖寶藏神儀軌經卷下",
        "from": "T21n1284_002:0352a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試鴻臚少卿",
        "from": "T21n1284_002:0352a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳教大師臣法天奉　詔譯",
        "from": "T21n1284_002:0352a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次曼拏羅內所用供獻之物。",
        "from": "T21n1284_002:0352a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須具二十件。",
        "from": "T21n1284_002:0352a11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每日三時供獻。",
        "from": "T21n1284_002:0352a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辰時供物。",
        "from": "T21n1284_002:0352a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "午時酉時不得再用。",
        "from": "T21n1284_002:0352a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須別換新者。",
        "from": "T21n1284_002:0352a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦者力所不及。",
        "from": "T21n1284_002:0352a13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "許納價直迴贖。",
        "from": "T21n1284_002:0352a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於第二時第三時再將供獻。",
        "from": "T21n1284_002:0352a14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每日三時皆然。",
        "from": "T21n1284_002:0352a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直候得壇法。",
        "from": "T21n1284_002:0352a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求成就日。",
        "from": "T21n1284_002:0352a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速納前件一切價直。",
        "from": "T21n1284_002:0352a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若供獻時。",
        "from": "T21n1284_002:0352a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結幢印誦幢真言曰。",
        "from": "T21n1284_002:0352a16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "囉怛曩二合婆捺囉二合曩帝覩瑟也二合迦羅娑特嚩二合惹必里二合焰特嚩二合惹布惹迦娑嚩二合引賀復說衣物儀則。",
        "from": "T21n1284_002:0352a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以無蟲匹帛造白衣二對。",
        "from": "T21n1284_002:0352a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於衣上畫金彩莊嚴塔相。",
        "from": "T21n1284_002:0352a21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是言我今獻衣物。",
        "from": "T21n1284_002:0352a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以兩手大指各捻頭指屈如環。",
        "from": "T21n1284_002:0352a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩環相鉤。",
        "from": "T21n1284_002:0352a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘指磔開作衣物印。",
        "from": "T21n1284_002:0352a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復誦衣物真言七遍。",
        "from": "T21n1284_002:0352a23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1284_002:0352a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "囉怛曩二合婆捺囉二合曩帝地里地里摩賀引三母捺覽二合麼囉馱娑嚩二合引賀復說扇儀則。",
        "from": "T21n1284_002:0352a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用無蟲匹帛作扇。",
        "from": "T21n1284_002:0352a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其扇白色制度合儀。",
        "from": "T21n1284_002:0352a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "扇柄端直以金寶莊畫二柄。",
        "from": "T21n1284_002:0352a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是言我今獻扇。",
        "from": "T21n1284_002:0352a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以左手屈小指於掌內。",
        "from": "T21n1284_002:0352a29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪臂搖手作扇印。",
        "from": "T21n1284_002:0352b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復誦扇真言七遍真言曰。",
        "from": "T21n1284_002:0352b01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "囉怛曩二合婆捺囉二合曩帝吠引覽婆嚩引喻馱引囉迦迦羅娑嚩二合賀復說拂儀則。",
        "from": "T21n1284_002:0352b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作白拂如法莊畫拂柄。",
        "from": "T21n1284_002:0352b04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用拂真言加持七遍。",
        "from": "T21n1284_002:0352b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後於東北方安置曩婆夜叉王。",
        "from": "T21n1284_002:0352b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方安毘沙門。",
        "from": "T21n1284_002:0352b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西北方安摩賀曩婆。",
        "from": "T21n1284_002:0352b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次安阿嚕尼所二合大力夜叉王。",
        "from": "T21n1284_002:0352b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次安鉢捺摩護努護努迦。",
        "from": "T21n1284_002:0352b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及酥細拏贊捺嚩昝婆句謨吒迦囉訖哆𡁠麼摩護捺囉。",
        "from": "T21n1284_002:0352b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次安尾布羅左夜叉女。",
        "from": "T21n1284_002:0352b09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此夜叉女最上端嚴。",
        "from": "T21n1284_002:0352b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次安尾布覽　左　贊捺隣二合引輸摩呬摩呬誐挽引　左　酥路引左曩引　左　鉢納摩二合娑囉引　酥部嚕二合嚩尾　左　夜叉尼　摩賀引麼曳　酥枳細　左　嚩曩麼引羅引　左　必里二合引濕嚩二合梨　捺嚕二合摩麼黎迦　左　薩里嚩二合迦引摩頗羅鉢羅二合那引　及摩努引惹嚩引迦摩嚕閉　酥嚕二合哆嚩引吉也二合　酥㮈里舍二合儞　摩護引誐囉二合引　唧引帝尾契也二合哆引等二十。",
        "from": "T21n1284_002:0352b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或一百依次第安置於外方位。",
        "from": "T21n1284_002:0352b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因陀羅等賢聖前。",
        "from": "T21n1284_002:0352b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作金剛網輪槍杖螺羂索幢三股叉。",
        "from": "T21n1284_002:0352b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及於東門安戟金剛焰等。",
        "from": "T21n1284_002:0352b20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是可及百種已上。",
        "from": "T21n1284_002:0352b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以摩羅帝花摩梨迦花。",
        "from": "T21n1284_002:0352b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "裛五色粉。",
        "from": "T21n1284_002:0352b21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獻塗香燒沈香。",
        "from": "T21n1284_002:0352b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦心真言請召寶藏神及諸賢聖。",
        "from": "T21n1284_002:0352b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦本真言請召夜叉女。",
        "from": "T21n1284_002:0352b23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次獻花鬘摩羅帝花殊妙蓮花。",
        "from": "T21n1284_002:0352b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "優鉢羅花迦難麼花。",
        "from": "T21n1284_002:0352b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱魯嚩花及緊輸迦花。",
        "from": "T21n1284_002:0352b25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切賢聖依次第獻。",
        "from": "T21n1284_002:0352b25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以紫礦安悉香龍腦香白檀香同作護摩。",
        "from": "T21n1284_002:0352b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於寶藏賢聖前。",
        "from": "T21n1284_002:0352b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置種種花種種供養之物。",
        "from": "T21n1284_002:0352b27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或聞大惡聲如得破壞。",
        "from": "T21n1284_002:0352b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是祥瑞不得恐怖。",
        "from": "T21n1284_002:0352b28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求之事決定成就。",
        "from": "T21n1284_002:0352b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當得寶藏神與大富貴。",
        "from": "T21n1284_002:0352b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此則曼拏羅儀則。",
        "from": "T21n1284_002:0352c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及所誦真言。",
        "from": "T21n1284_002:0352c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵引惹敢二合婆羅惹隣引捺羅二合引野娑嚩二合引賀　阿地鉢帝摩賀引夜叉囉引惹地摩呬引擔努引夜叉鉢囉二合冒引捺夜夜叉娑引尾怛里二合引唵引夜乞叉二合娑嚩二合引賀次心真言。",
        "from": "T21n1284_002:0352c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿南哆鉢捺摩二合引野娑嚩二合引賀　夜乞叉二合尾儞也二合引野娑嚩二合引賀次蓮華藏真言。",
        "from": "T21n1284_002:0352c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿喃哆商佉引野娑嚩二合賀次螺藏真言。",
        "from": "T21n1284_002:0352c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "囉惹哆曩引婆野馱曩引地鉢哆曳引娑嚩二合引賀　酥曩引婆引野馱曩引地鉢哆曳引娑嚩二合引賀　摩尼曩引婆引野馱曩引地鉢哆曳引娑嚩二合引賀　囉怛曩二合曩引婆引野馱曩引地鉢哆曳引娑嚩二合引賀　酥曩婆野馱曩地鉢哆曳引娑嚩二合引賀　暗沒曩引婆引野馱曩引地鉢哆曳引娑嚩二合引賀　儞羅曩引婆引野馱曩地鉢哆曳引娑嚩二合引賀　吠引室囉二合摩拏野馱曩地鉢哆曳引娑嚩二合引賀　摩賀引曩引婆引野馱曩引地鉢哆曳引娑嚩二合引賀　賀囉賀囉惹敢二合婆迦吽𠷑吒娑嚩二合引賀　惹敢二合婆迦野羼捺囉二合引曳引娑嚩二合引賀　護嚕護嚕喃引曩引酥史拏引野娑嚩二合引賀　賀囉賀囉贊捺囉二合細引曩野娑嚩二合引賀　惹敢二合婆曩引野娑嚩二合引賀　摩護引捺囉引野娑嚩二合引賀　尾摩羅尾摩羅捺里舍二合儞引娑嚩二合賀　尾摩羅尾摩隷引尾舍引羅曩野儞引󰂛馱野娑嚩二合引賀贊捺覽二合引輸摩儞數布即以二合帝數布即以二合帝引贊捺里二合引贊捺囉二合摩帝娑嚩二合賀　贊捺覽二合引輸摩帝娑嚩二合引賀　阿嚩引賀儞酥路引左儞也鉢捺彌二合引娑嚩二合引賀　鉢捺摩二合娑囉引酥部嚕二合摩努引囉彌引娑嚩二合引賀　酥部嚕二合酥部嚕二合嚩引護鉢囉二合怛也二合曳引娑嚩二合引賀　嚩呬尾二合摩隷波致謨引致娑嚩二合引賀　摩賀引彌引佉酥吉引舍摩引隷酥吉引舍摩引隷吉引舍嚩囉尼娑嚩二合引賀　曩嚩摩引隷儞娑嚩二合引賀引烏骨二合底瑟姹二合布室左二合隷引娑嚩二合引賀引　捺嚕二合摩麼引隷迦引頗隷捺嚕二合摩枳隷枳隷娑嚩二合引賀迦摩摩隷尾羅細儞娑嚩二合賀薩里嚩二合誐囉二合賀引摩努引惹味引嚩誐嚩帝娑嚩二合引賀引　迦引摩嚕閉婆引尾儞娑嚩二合引賀酥嚕二合哆嚩引吉也二合緊迦哩娑嚩二合引賀　酥捺里舍二合儞引迦引彌儞娑嚩二合引賀　摩護引誐里二合引婆野迦里娑嚩二合引賀　印捺囉二合引野禰引嚩引地鉢哆曳引娑嚩二合引賀　阿誐曩二合曳引賀尾也二合地鉢哆曳引娑嚩二合引賀　夜摩引野必里二合引哆地鉢哆曳引娑嚩二合引賀此等真言。",
        "from": "T21n1284_002:0353a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令一切天龍夜叉乾闥婆阿脩羅迦樓羅緊那羅摩睺羅伽必里哆毘舍左人非人等恭敬讚歎皆大歡喜。",
        "from": "T21n1284_002:0353a26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此真言利益一切眾生如如意寶如彼賢瓶如彼劫樹。",
        "from": "T21n1284_002:0353a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸有所求皆得成就。",
        "from": "T21n1284_002:0353a28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今說此真言之法令彼持誦。",
        "from": "T21n1284_002:0353a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若彼不種善根眾生暫聞此法。",
        "from": "T21n1284_002:0353a29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼人永不墮於惡趣之難。",
        "from": "T21n1284_002:0353b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有種善根者眾生之類。",
        "from": "T21n1284_002:0353b01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樂聞書寫受持讀誦。",
        "from": "T21n1284_002:0353b02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所得功德如佛如來功德無異。",
        "from": "T21n1284_002:0353b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人受持心真言者。",
        "from": "T21n1284_002:0353b03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人得入阿毘跋致。",
        "from": "T21n1284_002:0353b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不久當成阿耨多羅三藐三菩提。",
        "from": "T21n1284_002:0353b04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永離惡道。",
        "from": "T21n1284_002:0353b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人不貧不老無病無苦。",
        "from": "T21n1284_002:0353b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種之事俱得圓滿。",
        "from": "T21n1284_002:0353b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復得一切有情愛樂尊重恭敬供養。",
        "from": "T21n1284_002:0353b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今略說持此真言所獲功德。",
        "from": "T21n1284_002:0353b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "假使一切賢聖及天龍夜叉乾闥婆阿脩羅迦樓羅緊那羅等。",
        "from": "T21n1284_002:0353b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能知此功德之量。",
        "from": "T21n1284_002:0353b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說不能盡。",
        "from": "T21n1284_002:0353b09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我等及彼諸佛如來應供正等正覺。",
        "from": "T21n1284_002:0353b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此真言功德亦不能盡。",
        "from": "T21n1284_002:0353b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況菩薩摩訶薩聲聞辟支佛及諸天人。",
        "from": "T21n1284_002:0353b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知此功德之量而說盡耶。",
        "from": "T21n1284_002:0353b12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來一切智智功德無邊。",
        "from": "T21n1284_002:0353b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大力寶藏神所有功德。",
        "from": "T21n1284_002:0353b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦復如是。",
        "from": "T21n1284_002:0353b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此真言王一切如來真實之心。",
        "from": "T21n1284_002:0353b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能為一切世間主宰。",
        "from": "T21n1284_002:0353b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救諸眾生惡心者。",
        "from": "T21n1284_002:0353b15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "調伏眾生苦惱者。",
        "from": "T21n1284_002:0353b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而能出生一切智慧。",
        "from": "T21n1284_002:0353b16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若求寶藏神。",
        "from": "T21n1284_002:0353b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當於如來右邊畫神像。",
        "from": "T21n1284_002:0353b17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白色手執拂子儀相圓滿。",
        "from": "T21n1284_002:0353b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神像右邊畫金剛手菩薩。",
        "from": "T21n1284_002:0353b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如青優鉢羅花色。",
        "from": "T21n1284_002:0353b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以真珠莊嚴儀相圓滿。",
        "from": "T21n1284_002:0353b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩前畫夜叉大悲寶賢子。",
        "from": "T21n1284_002:0353b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如清水色身相圓滿。",
        "from": "T21n1284_002:0353b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面帶善顏手執毘惹布羅果藏。",
        "from": "T21n1284_002:0353b21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著黃衣服大頭冠破損座垂左足。",
        "from": "T21n1284_002:0353b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是誦人從頭至足。",
        "from": "T21n1284_002:0353b22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一觀察儀軌具足。",
        "from": "T21n1284_002:0353b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是正觀經一洛叉時。",
        "from": "T21n1284_002:0353b23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起心別作供養觀行。",
        "from": "T21n1284_002:0353b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想於水中所生諸花。",
        "from": "T21n1284_002:0353b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陸地諸花種種好香。",
        "from": "T21n1284_002:0353b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切珍寶殊妙衣服。",
        "from": "T21n1284_002:0353b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及飲食燈鬘等而用供獻。",
        "from": "T21n1284_002:0353b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得一切願圓滿。",
        "from": "T21n1284_002:0353b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是專心觀想已。",
        "from": "T21n1284_002:0353b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦人復自白言。",
        "from": "T21n1284_002:0353b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我依三寶儀則信受奉行。",
        "from": "T21n1284_002:0353b27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後依天龍儀則作護摩法。",
        "from": "T21n1284_002:0353b28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀想寶賢大將。",
        "from": "T21n1284_002:0353b28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執持火來著護摩爐內。",
        "from": "T21n1284_002:0353b29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿賢大將爐中燃火。",
        "from": "T21n1284_002:0353b29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲財大將作清淨法。",
        "from": "T21n1284_002:0353c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多聞大將鋪吉祥草座。",
        "from": "T21n1284_002:0353c01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來儀則觀想成就。",
        "from": "T21n1284_002:0353c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次想頭真言燃火。",
        "from": "T21n1284_002:0353c02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口真言請召。",
        "from": "T21n1284_002:0353c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "足真言作擁護求於成就。",
        "from": "T21n1284_002:0353c03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即作護摩滿。",
        "from": "T21n1284_002:0353c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手擲三度。",
        "from": "T21n1284_002:0353c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言曰。",
        "from": "T21n1284_002:0353c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿誐曩二合曳引娑嚩二合引賀即結吉祥等印。",
        "from": "T21n1284_002:0353c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手作合掌。",
        "from": "T21n1284_002:0353c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中間安置賢聖。",
        "from": "T21n1284_002:0353c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀想誦真言。",
        "from": "T21n1284_002:0353c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經須臾之間如風吹火。",
        "from": "T21n1284_002:0353c07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀想前所說乳汁作護摩一洛叉先獻八大明王作護摩一洛叉。",
        "from": "T21n1284_002:0353c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次獻寶賢大明王。",
        "from": "T21n1284_002:0353c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別作護摩一洛叉得大富貴。",
        "from": "T21n1284_002:0353c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二洛叉得子孫興盛。",
        "from": "T21n1284_002:0353c10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三洛叉有大罪之者。",
        "from": "T21n1284_002:0353c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦得成就之法。",
        "from": "T21n1284_002:0353c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是儀則專心恒作。",
        "from": "T21n1284_002:0353c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以水作護摩。",
        "from": "T21n1284_002:0353c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用花裛水。",
        "from": "T21n1284_002:0353c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以香水一千合作護摩獻寶藏神。",
        "from": "T21n1284_002:0353c13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒令得財恒獲快樂。",
        "from": "T21n1284_002:0353c13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復作一千護摩得增長法。",
        "from": "T21n1284_002:0353c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依如是儀則。",
        "from": "T21n1284_002:0353c14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前觀想作護摩滿。",
        "from": "T21n1284_002:0353c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獻三合水家得增長。",
        "from": "T21n1284_002:0353c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作護摩五洛叉。",
        "from": "T21n1284_002:0353c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令㡧像隱身及令伏藏出現。",
        "from": "T21n1284_002:0353c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受用無盡獲最上快樂。",
        "from": "T21n1284_002:0353c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復作足真言口真言等。",
        "from": "T21n1284_002:0353c17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令一切所求之事成就。",
        "from": "T21n1284_002:0353c18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能禁怨家得過魔界永離深苦。",
        "from": "T21n1284_002:0353c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行住坐臥所受福德與天帝釋等。",
        "from": "T21n1284_002:0353c19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其法如是。",
        "from": "T21n1284_002:0353c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說聖寶藏神儀軌經卷下",
        "from": "T21n1284_002:0353c21_100",
        "context_with_next_sentence": false
    }
]