[
    {
        "source_sentence": "千手千眼觀世音菩薩治病合藥",
        "from": "T20n1059_001:0103c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經",
        "from": "T20n1059_001:0103c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天竺國三藏伽梵達摩奉　詔譯",
        "from": "T20n1059_001:0103c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛告阿難言。",
        "from": "T20n1059_001:0103c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此觀世音菩薩所說廣大圓滿無礙大悲心陀羅尼神呪者。",
        "from": "T20n1059_001:0103c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真實不虛。",
        "from": "T20n1059_001:0103c12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等所求願。",
        "from": "T20n1059_001:0103c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而欲請求觀世音自在菩薩摩訶薩者。",
        "from": "T20n1059_001:0103c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取拙具羅香。",
        "from": "T20n1059_001:0103c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍燒。",
        "from": "T20n1059_001:0103c14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時觀世音菩薩必即來到拙具羅香者安悉香是也。",
        "from": "T20n1059_001:0103c15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等橫鬼所著。",
        "from": "T20n1059_001:0103c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取弭哩吒那燒作灰。",
        "from": "T20n1059_001:0103c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和淨泥捻作橫鬼形。",
        "from": "T20n1059_001:0103c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於千眼像前。",
        "from": "T20n1059_001:0103c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取鑌鐵刀子。",
        "from": "T20n1059_001:0103c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪一百八遍。",
        "from": "T20n1059_001:0103c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "段段切斷為一百八遍。",
        "from": "T20n1059_001:0103c18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即稱彼名即差永差還復不著弭哩吒那猫兒頭骨是也。",
        "from": "T20n1059_001:0103c19_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等蠱毒所害者。",
        "from": "T20n1059_001:0103c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取劫布羅香以和拙具羅香。",
        "from": "T20n1059_001:0103c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各等分以井花水一斗和大上。",
        "from": "T20n1059_001:0103c21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煮取半升去滓取汁。",
        "from": "T20n1059_001:0103c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於千眼像前呪一百遍呪即差劫布羅香者龍腦香是也。",
        "from": "T20n1059_001:0103c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等。",
        "from": "T20n1059_001:0103c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為惡毒蛇蠍所螫者。",
        "from": "T20n1059_001:0103c24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乾薑大小末呪一七遍著瘡上。",
        "from": "T20n1059_001:0103c25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立即除愈。",
        "from": "T20n1059_001:0103c25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等邪惡怨敵以邪辭相謨傷害者。",
        "from": "T20n1059_001:0103c26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取淨土或麵或蠟捨。",
        "from": "T20n1059_001:0103c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作彼惡人形相。",
        "from": "T20n1059_001:0103c27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於千眼像前。",
        "from": "T20n1059_001:0103c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取鑌鐵刀子。",
        "from": "T20n1059_001:0103c28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪一百八遍。",
        "from": "T20n1059_001:0103c28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一呪一截。",
        "from": "T20n1059_001:0103c28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一稱其名。",
        "from": "T20n1059_001:0103c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒盡一百八段。",
        "from": "T20n1059_001:0103c29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼人即歡喜修身厚重相愛恭敬。",
        "from": "T20n1059_001:0104a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼是無怨。",
        "from": "T20n1059_001:0104a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等患眼精壞者。",
        "from": "T20n1059_001:0104a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有清盲暗者。",
        "from": "T20n1059_001:0104a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若白暈赤瞙無光明者。",
        "from": "T20n1059_001:0104a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取呵梨勒果菴摩勒果鞞醯勒果。",
        "from": "T20n1059_001:0104a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三各一顆擣破油下篩當研。",
        "from": "T20n1059_001:0104a04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時唯須淨護。",
        "from": "T20n1059_001:0104a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫令新產婦人及狗見。",
        "from": "T20n1059_001:0104a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口中念佛。",
        "from": "T20n1059_001:0104a05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以白蜜若人乳汁和封眼中者。",
        "from": "T20n1059_001:0104a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人乳汁要須男孩子母乳汁。",
        "from": "T20n1059_001:0104a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若女兒乳汁者。",
        "from": "T20n1059_001:0104a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成其藥和竟者。",
        "from": "T20n1059_001:0104a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還須千眼像前。",
        "from": "T20n1059_001:0104a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪一千八遍。",
        "from": "T20n1059_001:0104a08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著眼中。",
        "from": "T20n1059_001:0104a08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿七日。",
        "from": "T20n1059_001:0104a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在深室內。",
        "from": "T20n1059_001:0104a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慎風房室五辛諸不淨物。",
        "from": "T20n1059_001:0104a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得精還明淨光盛。",
        "from": "T20n1059_001:0104a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等患虐鬼病者。",
        "from": "T20n1059_001:0104a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取虎豹犲狼虎。",
        "from": "T20n1059_001:0104a11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍。",
        "from": "T20n1059_001:0104a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼著身上即差。",
        "from": "T20n1059_001:0104a12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師子皮最上。",
        "from": "T20n1059_001:0104a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等被蛇螫。",
        "from": "T20n1059_001:0104a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取被螫人結𦡲。",
        "from": "T20n1059_001:0104a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍。",
        "from": "T20n1059_001:0104a13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著瘡中即差結聹者所聹耳穴內結耳屎是也。",
        "from": "T20n1059_001:0104a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等患惡痓入心悶絕欲死者。",
        "from": "T20n1059_001:0104a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取桃膠一丸。",
        "from": "T20n1059_001:0104a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大小亦如桃實大。",
        "from": "T20n1059_001:0104a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以清水一升和煎。",
        "from": "T20n1059_001:0104a16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取半升呪七遍。",
        "from": "T20n1059_001:0104a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頓服盡即差。",
        "from": "T20n1059_001:0104a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其藥莫令婦人煮。",
        "from": "T20n1059_001:0104a17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須淨護造也。",
        "from": "T20n1059_001:0104a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫諸藥造不須人無驗。",
        "from": "T20n1059_001:0104a18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等患傳屍鬼氣伏連病者。",
        "from": "T20n1059_001:0104a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取拙具羅香。",
        "from": "T20n1059_001:0104a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍。",
        "from": "T20n1059_001:0104a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒熏鼻孔中。",
        "from": "T20n1059_001:0104a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又取七丸如菟失。",
        "from": "T20n1059_001:0104a20_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大呪二七遍吞即差。",
        "from": "T20n1059_001:0104a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慎酒肉五辛及惡罵。",
        "from": "T20n1059_001:0104a21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又取摩那屎羅和白芥子。",
        "from": "T20n1059_001:0104a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印成鹽呪三七遍。",
        "from": "T20n1059_001:0104a22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於病者床下燒。",
        "from": "T20n1059_001:0104a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其作病鬼身即壁裂也。",
        "from": "T20n1059_001:0104a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼逆走不敢住也摩那屎羅者雄黃是也。",
        "from": "T20n1059_001:0104a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白者淨也。",
        "from": "T20n1059_001:0104a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印成鹽美是也。",
        "from": "T20n1059_001:0104a24_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等患一邊偏風。",
        "from": "T20n1059_001:0104a25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耳鼻不通手脚不便者。",
        "from": "T20n1059_001:0104a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取胡麻油內木香煎。",
        "from": "T20n1059_001:0104a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍。",
        "from": "T20n1059_001:0104a26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩拭身上永得除差。",
        "from": "T20n1059_001:0104a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又取純牛蘇。",
        "from": "T20n1059_001:0104a27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍。",
        "from": "T20n1059_001:0104a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩身上差好。",
        "from": "T20n1059_001:0104a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有婦人患產難者。",
        "from": "T20n1059_001:0104a28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取胡麻油。",
        "from": "T20n1059_001:0104a28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍。",
        "from": "T20n1059_001:0104a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩產婦臍中及玉門中。",
        "from": "T20n1059_001:0104a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若令口吞易生。",
        "from": "T20n1059_001:0104a29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有女人懷妊死腹中者。",
        "from": "T20n1059_001:0104b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取阿婆末唎草一大兩。",
        "from": "T20n1059_001:0104b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以水二升和煮絞去滓。",
        "from": "T20n1059_001:0104b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取一升汁。",
        "from": "T20n1059_001:0104b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍。",
        "from": "T20n1059_001:0104b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "服即出一無苦痛。",
        "from": "T20n1059_001:0104b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不出胎衣者。",
        "from": "T20n1059_001:0104b03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦服此藥即出差阿婆末唎草牛膝草是也。",
        "from": "T20n1059_001:0104b04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等卒患心痛不可忍者。",
        "from": "T20n1059_001:0104b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為道屍疰取杜嚕香如乳頭成者一丸呪三七遍。",
        "from": "T20n1059_001:0104b06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口中嚼咽不限多小。",
        "from": "T20n1059_001:0104b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令變吐即差。",
        "from": "T20n1059_001:0104b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慎五辛酒肉油物諸不淨物及房內杜嚕香者薰陸香是也。",
        "from": "T20n1059_001:0104b08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等被大燒瘡者。",
        "from": "T20n1059_001:0104b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取熱瞿摩夷。",
        "from": "T20n1059_001:0104b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪二七遍。",
        "from": "T20n1059_001:0104b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗瘡上即差瞿摩夷者烏牛屎是也。",
        "from": "T20n1059_001:0104b10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等患蛔虫齩心痛者。",
        "from": "T20n1059_001:0104b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取骨嚕末遮半升。",
        "from": "T20n1059_001:0104b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍。",
        "from": "T20n1059_001:0104b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "服即差。",
        "from": "T20n1059_001:0104b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若重者一升。",
        "from": "T20n1059_001:0104b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虫即如綟索出來差骨嚕末遮者白馬屎也。",
        "from": "T20n1059_001:0104b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等患下瘡者。",
        "from": "T20n1059_001:0104b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取菱󰼋葉搗絞取汁。",
        "from": "T20n1059_001:0104b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍。",
        "from": "T20n1059_001:0104b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜臥著眼中即差。",
        "from": "T20n1059_001:0104b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若須用白牡馬失汁。",
        "from": "T20n1059_001:0104b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍。",
        "from": "T20n1059_001:0104b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如上差骨魯怛佉者新驢糞是也。",
        "from": "T20n1059_001:0104b16_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等患腹中病痛者。",
        "from": "T20n1059_001:0104b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取井花水和印成鹽二顆。",
        "from": "T20n1059_001:0104b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍。",
        "from": "T20n1059_001:0104b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "服半升即差。",
        "from": "T20n1059_001:0104b18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等患赤眼者。",
        "from": "T20n1059_001:0104b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及眼中有努肉。",
        "from": "T20n1059_001:0104b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及有翳者。",
        "from": "T20n1059_001:0104b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取奢弭葉。",
        "from": "T20n1059_001:0104b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "搗綟取汁。",
        "from": "T20n1059_001:0104b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪七遍。",
        "from": "T20n1059_001:0104b20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "𣷽濱錢一宿更呪七遍。",
        "from": "T20n1059_001:0104b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著眼中即差奢弭葉者苟杞葉是也。",
        "from": "T20n1059_001:0104b21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等患畏夜不安。",
        "from": "T20n1059_001:0104b22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恐怖出入驚怕者。",
        "from": "T20n1059_001:0104b22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白線作索。",
        "from": "T20n1059_001:0104b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍。",
        "from": "T20n1059_001:0104b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作二十一結。",
        "from": "T20n1059_001:0104b23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫項恐即除。",
        "from": "T20n1059_001:0104b23_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非但除怖。",
        "from": "T20n1059_001:0104b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦得福生滅罪壽命延長。",
        "from": "T20n1059_001:0104b24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等於家內橫起災難者。",
        "from": "T20n1059_001:0104b25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取石榴枝等截一千段。",
        "from": "T20n1059_001:0104b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩頭塗蘇酪蜜。",
        "from": "T20n1059_001:0104b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一呪一燒盡一千八遍。",
        "from": "T20n1059_001:0104b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切災難患悉除滅。",
        "from": "T20n1059_001:0104b27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要在千眼像前作之。",
        "from": "T20n1059_001:0104b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若取白昌蒲。",
        "from": "T20n1059_001:0104b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪一百八遍。",
        "from": "T20n1059_001:0104b29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫著右臂上。",
        "from": "T20n1059_001:0104b29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切聞處論議得勝。",
        "from": "T20n1059_001:0104c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若取奢弭葉。",
        "from": "T20n1059_001:0104c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若枝柯寸截兩頭。",
        "from": "T20n1059_001:0104c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗真牛蘇白蜜牛酪。",
        "from": "T20n1059_001:0104c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一呪一燒盡一千八段。",
        "from": "T20n1059_001:0104c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日別三時。",
        "from": "T20n1059_001:0104c03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時別一百遍。",
        "from": "T20n1059_001:0104c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿七日呪師自悟通智。",
        "from": "T20n1059_001:0104c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有行人欲降諸大力鬼神者。",
        "from": "T20n1059_001:0104c05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取阿利瑟迦紫。",
        "from": "T20n1059_001:0104c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍。",
        "from": "T20n1059_001:0104c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火中燒還須蘇酪蜜。",
        "from": "T20n1059_001:0104c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要須於悲像前作之阿梨瑟迦紫者木槵子是也。",
        "from": "T20n1059_001:0104c07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若取胡盧遮那一大兩。",
        "from": "T20n1059_001:0104c08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著流漓瓶中。",
        "from": "T20n1059_001:0104c08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置大悲像前。",
        "from": "T20n1059_001:0104c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪一千八遍。",
        "from": "T20n1059_001:0104c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗身點額。",
        "from": "T20n1059_001:0104c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切天龍鬼神人及非人皆悉歡喜胡盧遮那牛黃是也。",
        "from": "T20n1059_001:0104c10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人身被杻械枷鎖者。",
        "from": "T20n1059_001:0104c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取白領失。",
        "from": "T20n1059_001:0104c11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪一百八遍。",
        "from": "T20n1059_001:0104c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗於手上。",
        "from": "T20n1059_001:0104c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用摩杻械枷鎖者。",
        "from": "T20n1059_001:0104c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即自解脫。",
        "from": "T20n1059_001:0104c12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有夫婦不和狀如水火者。",
        "from": "T20n1059_001:0104c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取鴛鴦尾。",
        "from": "T20n1059_001:0104c13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於大悲心像前呪一千八遍。",
        "from": "T20n1059_001:0104c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上帶之。",
        "from": "T20n1059_001:0104c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼是即終身歡喜相愛敬。",
        "from": "T20n1059_001:0104c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等被虫食田苗及五菓子者。",
        "from": "T20n1059_001:0104c16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取淨灰砂。",
        "from": "T20n1059_001:0104c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪一千八遍。",
        "from": "T20n1059_001:0104c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散田四邊。",
        "from": "T20n1059_001:0104c17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虫退散也。",
        "from": "T20n1059_001:0104c17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有諸菓樹兼呪水一百八遍。",
        "from": "T20n1059_001:0104c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至一千八遍。",
        "from": "T20n1059_001:0104c18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灑著樹上。",
        "from": "T20n1059_001:0104c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虫不敢食害菓也。",
        "from": "T20n1059_001:0104c19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等逢鬼口禁吐沫不知人色應急死者。",
        "from": "T20n1059_001:0104c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取虎魄玉細屑。",
        "from": "T20n1059_001:0104c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪一百八遍。",
        "from": "T20n1059_001:0104c21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以酒令服。",
        "from": "T20n1059_001:0104c21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又取骨嚕萬異絞汁。",
        "from": "T20n1059_001:0104c22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪二七遍令服。",
        "from": "T20n1059_001:0104c22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又造其額上鬼神建。",
        "from": "T20n1059_001:0104c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪一百一八遍即差骨嚕萬異者白馬屎是也。",
        "from": "T20n1059_001:0104c23_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等被湯爛傷者。",
        "from": "T20n1059_001:0104c24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取木灰和水。",
        "from": "T20n1059_001:0104c24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍。",
        "from": "T20n1059_001:0104c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "泥瘡上。",
        "from": "T20n1059_001:0104c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日三遍。",
        "from": "T20n1059_001:0104c25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又熱瞿摩夷呪三七遍。",
        "from": "T20n1059_001:0104c25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗瘡上即差。",
        "from": "T20n1059_001:0104c26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等食諸畜生災毒。",
        "from": "T20n1059_001:0104c27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煩心悶欲死者。",
        "from": "T20n1059_001:0104c27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取淨土一升。",
        "from": "T20n1059_001:0104c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以水三升煮取一升汁。",
        "from": "T20n1059_001:0104c28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍令服即差。",
        "from": "T20n1059_001:0104c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等卒得諸腫𤹿者。",
        "from": "T20n1059_001:0105a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取蕪菁葉搗和清酒。",
        "from": "T20n1059_001:0105a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍。",
        "from": "T20n1059_001:0105a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "𭡺腫上即差。",
        "from": "T20n1059_001:0105a02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等惡腫入腹欲死者。",
        "from": "T20n1059_001:0105a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取瞿摩夷燒和酒。",
        "from": "T20n1059_001:0105a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍塗腫上。",
        "from": "T20n1059_001:0105a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又口令服即差。",
        "from": "T20n1059_001:0105a04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等患鼻大衄下欲死者。",
        "from": "T20n1059_001:0105a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取生蓬萊和水煮取汁。",
        "from": "T20n1059_001:0105a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不限大小呪三七遍。",
        "from": "T20n1059_001:0105a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令吞即留生。",
        "from": "T20n1059_001:0105a06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等亦血痢血者。",
        "from": "T20n1059_001:0105a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取桃脂大如鷄子。",
        "from": "T20n1059_001:0105a07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍令吞即差。",
        "from": "T20n1059_001:0105a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等患喈咳者。",
        "from": "T20n1059_001:0105a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取桃子人一升。",
        "from": "T20n1059_001:0105a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "熱火和𥹋糖。",
        "from": "T20n1059_001:0105a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪一百八遍。",
        "from": "T20n1059_001:0105a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頓令服盡。",
        "from": "T20n1059_001:0105a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至須造三四劑病即愈。",
        "from": "T20n1059_001:0105a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人患大便孔癢名。",
        "from": "T20n1059_001:0105a12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取草纏羅菓。",
        "from": "T20n1059_001:0105a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "熱細末和糖。",
        "from": "T20n1059_001:0105a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪一百八遍。",
        "from": "T20n1059_001:0105a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗孔日三即差草纏羅菓者菟糸子是也。",
        "from": "T20n1059_001:0105a13_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人患大便不通。",
        "from": "T20n1059_001:0105a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取葵子二升。",
        "from": "T20n1059_001:0105a14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以水四升煮取一升汁。",
        "from": "T20n1059_001:0105a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍數服即下。",
        "from": "T20n1059_001:0105a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人患小便不通者。",
        "from": "T20n1059_001:0105a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取瞿摩犢夷絞取汁。",
        "from": "T20n1059_001:0105a16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍令服即愈瞿摩夷者犢牛子之失是也。",
        "from": "T20n1059_001:0105a17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等患小便不通者。",
        "from": "T20n1059_001:0105a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取房內樑上塵細下。",
        "from": "T20n1059_001:0105a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "篩以三指撮和清水。",
        "from": "T20n1059_001:0105a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍服差。",
        "from": "T20n1059_001:0105a19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人等小便數忽起者。",
        "from": "T20n1059_001:0105a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若去斗許者。",
        "from": "T20n1059_001:0105a20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取括樓根一兩。",
        "from": "T20n1059_001:0105a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以清水三升煮。",
        "from": "T20n1059_001:0105a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "絞取半汁。",
        "from": "T20n1059_001:0105a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍。",
        "from": "T20n1059_001:0105a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一百八遍。",
        "from": "T20n1059_001:0105a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頓服即差。",
        "from": "T20n1059_001:0105a22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有婦人任身卒得病。",
        "from": "T20n1059_001:0105a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煮取小豆五升豉三升。",
        "from": "T20n1059_001:0105a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以清水一斗煮。",
        "from": "T20n1059_001:0105a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取三升汁。",
        "from": "T20n1059_001:0105a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪一百八遍。",
        "from": "T20n1059_001:0105a24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分為二服。",
        "from": "T20n1059_001:0105a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即差病產生安樂。",
        "from": "T20n1059_001:0105a25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有婦人患倒子產難生欲死者。",
        "from": "T20n1059_001:0105a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取蓬萊一升。",
        "from": "T20n1059_001:0105a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以水三升煮取一升汁。",
        "from": "T20n1059_001:0105a27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍。",
        "from": "T20n1059_001:0105a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令服即生無病。",
        "from": "T20n1059_001:0105a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有小兒患夜啼不安眠者。",
        "from": "T20n1059_001:0105a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其兒目下書鬼之字。",
        "from": "T20n1059_001:0105b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍三唾即差息。",
        "from": "T20n1059_001:0105b01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有小兒頭生諸瘡者。",
        "from": "T20n1059_001:0105b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取瞿摩摩角鰓。",
        "from": "T20n1059_001:0105b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒末和猪暏。",
        "from": "T20n1059_001:0105b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍。",
        "from": "T20n1059_001:0105b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗瘡上即差半角中子。",
        "from": "T20n1059_001:0105b03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有小兒患舌腫不能哺乳者。",
        "from": "T20n1059_001:0105b04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取東方乘汁。",
        "from": "T20n1059_001:0105b04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪一百八遍。",
        "from": "T20n1059_001:0105b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗舌立愈。",
        "from": "T20n1059_001:0105b05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有小兒口中生瘡不能食者。",
        "from": "T20n1059_001:0105b06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取黃連根。",
        "from": "T20n1059_001:0105b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "細搗篩下。",
        "from": "T20n1059_001:0105b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以和男子母乳汁。",
        "from": "T20n1059_001:0105b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍。",
        "from": "T20n1059_001:0105b07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗口瘡上即差。",
        "from": "T20n1059_001:0105b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時觀世音菩薩摩訶薩告諸大眾言。",
        "from": "T20n1059_001:0105b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於如來般涅槃。",
        "from": "T20n1059_001:0105b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "末世眾生造蓬口。",
        "from": "T20n1059_001:0105b10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸疾病應在無量。",
        "from": "T20n1059_001:0105b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故我今說療治法亦復如是。",
        "from": "T20n1059_001:0105b11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若比丘比丘尼。",
        "from": "T20n1059_001:0105b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "優婆塞優婆夷。",
        "from": "T20n1059_001:0105b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子善女人等。",
        "from": "T20n1059_001:0105b12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能受持此療治法救眾生苦者。",
        "from": "T20n1059_001:0105b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知是即我化身。",
        "from": "T20n1059_001:0105b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大悲心救眾生故。",
        "from": "T20n1059_001:0105b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必我到其所。",
        "from": "T20n1059_001:0105b14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作諸法令有勝驗。",
        "from": "T20n1059_001:0105b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人受持大悲心隨心自在陀羅尼神妙章句能救世間苦。",
        "from": "T20n1059_001:0105b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人如我無異我身也。",
        "from": "T20n1059_001:0105b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有淨信男子女人。",
        "from": "T20n1059_001:0105b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有能受持是救苦陀羅尼呪者。",
        "from": "T20n1059_001:0105b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持世翳法。",
        "from": "T20n1059_001:0105b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖療諸病。",
        "from": "T20n1059_001:0105b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以慈心故。",
        "from": "T20n1059_001:0105b19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必以至心應呪一百八遍。",
        "from": "T20n1059_001:0105b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀音力故無不著者。",
        "from": "T20n1059_001:0105b20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生能持我法。",
        "from": "T20n1059_001:0105b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人若在命終時。",
        "from": "T20n1059_001:0105b21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我急迎來。",
        "from": "T20n1059_001:0105b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以無邊樂乘寶雲車。",
        "from": "T20n1059_001:0105b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速令往生安樂世界。",
        "from": "T20n1059_001:0105b22_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮華化生成佛不久。",
        "from": "T20n1059_001:0105b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時釋迦牟尼佛讚觀世音菩薩言。",
        "from": "T20n1059_001:0105b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉大士。",
        "from": "T20n1059_001:0105b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是如是如汝所說。",
        "from": "T20n1059_001:0105b25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善士善女等。",
        "from": "T20n1059_001:0105b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以一分藥施疾病者。",
        "from": "T20n1059_001:0105b26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現在得福滅罪。",
        "from": "T20n1059_001:0105b26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於當來世福報無盡。",
        "from": "T20n1059_001:0105b27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生生世世受無病身受諸快樂。",
        "from": "T20n1059_001:0105b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天上人中受諸勝利。",
        "from": "T20n1059_001:0105b28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天上人中壽命無量。",
        "from": "T20n1059_001:0105b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得道不久。",
        "from": "T20n1059_001:0105b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時會中大眾菩薩摩訶薩等。",
        "from": "T20n1059_001:0105c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵釋四王天龍夜叉諸神鬼王人非人等。",
        "from": "T20n1059_001:0105c02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛所說皆大歡喜。",
        "from": "T20n1059_001:0105c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信受頂禮迴向奉行。",
        "from": "T20n1059_001:0105c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千手千眼觀世音菩薩療病合藥經",
        "from": "T20n1059_001:0105c04_100",
        "context_with_next_sentence": false
    }
]