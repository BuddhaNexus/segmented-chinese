[
    {
        "source_sentence": "佛說內身觀章句經",
        "from": "T15n0610_001:0239a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "失譯人名在後漢錄",
        "from": "T15n0610_001:0239a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切一其心，　　皆聽美言訓，",
        "from": "T15n0610_001:0239a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛所從得道，　　且聽我彼經。",
        "from": "T15n0610_001:0239a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼空亦不斷，　　有行皆非常，",
        "from": "T15n0610_001:0239a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫行不敗壞，　　佛以講授經。",
        "from": "T15n0610_001:0239a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深微難見事，　　非章原之句，",
        "from": "T15n0610_001:0239a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通彼能敷演，　　是以故為師。",
        "from": "T15n0610_001:0239a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從本以存本，　　有造法之積，",
        "from": "T15n0610_001:0239a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從慧以除棄，　　上士所講說。",
        "from": "T15n0610_001:0239a29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所從因緣有，　　有行皆無有，",
        "from": "T15n0610_001:0239b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有前世除，　　無知彼諸行；",
        "from": "T15n0610_001:0239b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦如欲演身，　　日親之所講，",
        "from": "T15n0610_001:0239b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大災患無數，　　皆以歸流身。",
        "from": "T15n0610_001:0239b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刪定如本文，　　所演眾要言，",
        "from": "T15n0610_001:0239b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "靡麗五字句，　　明誓以勸勉。",
        "from": "T15n0610_001:0239b06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以頌文具足，　　偶字音商備，",
        "from": "T15n0610_001:0239b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若花實雜糅，　　皆聽彼我誨。",
        "from": "T15n0610_001:0239b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身非人非命，　　不丈夫非士，",
        "from": "T15n0610_001:0239b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若體若不艶，　　斯事都無彼。",
        "from": "T15n0610_001:0239b10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫身造而有，　　以有即敗亡，",
        "from": "T15n0610_001:0239b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無強皆歸命，　　如沫蹈踐碎；",
        "from": "T15n0610_001:0239b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無強則無常，　　無常即無樂；",
        "from": "T15n0610_001:0239b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無常亦為苦，　　非身身非我；",
        "from": "T15n0610_001:0239b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身非常亦苦，　　非彼應為身。",
        "from": "T15n0610_001:0239b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若體艶我有，　　身都無可有；",
        "from": "T15n0610_001:0239b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若以都無體，　　有存亦有亡。",
        "from": "T15n0610_001:0239b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧者以本末，　　何彼已有體？",
        "from": "T15n0610_001:0239b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若以無主者，　　不得以自由，",
        "from": "T15n0610_001:0239b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有校計若此，　　何彼體我有？",
        "from": "T15n0610_001:0239b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斯體身為空，　　體我有為虛，",
        "from": "T15n0610_001:0239b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應因緣為有，　　非身逝心造，",
        "from": "T15n0610_001:0239b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非眾人身造，　　亦非自身造，",
        "from": "T15n0610_001:0239b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "都無身有造，　　亦身無不造。",
        "from": "T15n0610_001:0239b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以身無造者，　　從彼為得諦。",
        "from": "T15n0610_001:0239b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從前世方來，　　亦非天造身，",
        "from": "T15n0610_001:0239b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非神所化城，　　非無行無本，",
        "from": "T15n0610_001:0239b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無因為自有，　　是身有所由，",
        "from": "T15n0610_001:0239b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有事亦有物。",
        "from": "T15n0610_001:0239c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稍稍為生有，　　本以癡亦愛，",
        "from": "T15n0610_001:0239c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心與愛有漏，　　亦有縛亦結，",
        "from": "T15n0610_001:0239c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行二品為漏。",
        "from": "T15n0610_001:0239c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世與受因緣，　　以由彼斯身，",
        "from": "T15n0610_001:0239c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以漸能致有。",
        "from": "T15n0610_001:0239c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精沫為轉凝，　　足為生兩兩，",
        "from": "T15n0610_001:0239c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以兩為轉厚，　　以序有四體，",
        "from": "T15n0610_001:0239c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為生頭第五，　　若干骨積聚，",
        "from": "T15n0610_001:0239c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從行為用成，　　頭九為髑髏，",
        "from": "T15n0610_001:0239c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頞䪼為二骨，　　齒根三十二，",
        "from": "T15n0610_001:0239c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "齒三十二骨，　　頸為與耳本，",
        "from": "T15n0610_001:0239c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鼻為與上腭，　　心與頸嚨喉，",
        "from": "T15n0610_001:0239c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡為骨八十。",
        "from": "T15n0610_001:0239c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咽亦為骨四，　　左臂與右臂，",
        "from": "T15n0610_001:0239c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡有骨五十。",
        "from": "T15n0610_001:0239c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　若其斯左脇，應有十三肋；",
        "from": "T15n0610_001:0239c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　亦斯右脇然，應有肋十三，　　斯為四十八。",
        "from": "T15n0610_001:0239c18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三三三相連，　　二為二相連，",
        "from": "T15n0610_001:0239c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其餘不相連，　　身者以為彊。",
        "from": "T15n0610_001:0239c20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如束葦無彊，　　脊膂三十二，",
        "from": "T15n0610_001:0239c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尻與腰為三，　　若其斯左髀，",
        "from": "T15n0610_001:0239c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "骨為二十五；",
        "from": "T15n0610_001:0239c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　右髀為亦然，為骨二十五；",
        "from": "T15n0610_001:0239c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　肩髀有骨四，凡三百二十。",
        "from": "T15n0610_001:0239c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛以日斷嗣，　　彼叚悉以聚，",
        "from": "T15n0610_001:0239c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸根為以縫，　　非瘡而裹之，",
        "from": "T15n0610_001:0239c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "肉血以塗𢴉，　　如木機關縷，",
        "from": "T15n0610_001:0239c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為如幻師幻。",
        "from": "T15n0610_001:0239c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以筋纏縛成，　　合聚骨若此，",
        "from": "T15n0610_001:0240a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為是形體。",
        "from": "T15n0610_001:0240a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　愚者莫不著，智者而不著。",
        "from": "T15n0610_001:0240a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九孔為大瘡，　　周匝為滲漏，",
        "from": "T15n0610_001:0240a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不淨腐臭處，　　口喙如為孔，",
        "from": "T15n0610_001:0240a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿之以諸穀，　　是身為若此。",
        "from": "T15n0610_001:0240a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以進若干腑，　　毛髮與爪齒，",
        "from": "T15n0610_001:0240a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塵埃亦皮革，　　骨節亦骨髓，",
        "from": "T15n0610_001:0240a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為凡筋與脈，　　胃與心亦脾，",
        "from": "T15n0610_001:0240a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大腸亦小腸，　　肝與肺亦腎，",
        "from": "T15n0610_001:0240a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脂𦠻亦大便，　　淚與唾亦汗，",
        "from": "T15n0610_001:0240a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鼻涕膏亦血，　　寒熱肪小便，",
        "from": "T15n0610_001:0240a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "𮌗之與𮌗膜，　　皆以沈沒彼，",
        "from": "T15n0610_001:0240a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如泥塗老牛，　　如其成不知。",
        "from": "T15n0610_001:0240a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身之內與外，　　夫城骨與牆，",
        "from": "T15n0610_001:0240a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "肉血為塗嚴，　　為怨所破壞。",
        "from": "T15n0610_001:0240a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒為以內外，　　彼央若干百，",
        "from": "T15n0610_001:0240a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為貪其肉，　　其外亦災害，",
        "from": "T15n0610_001:0240a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆以多尤彼，　　斯身腐敗壞。",
        "from": "T15n0610_001:0240a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如久故城，　　晝夜供侍之，",
        "from": "T15n0610_001:0240a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壞如已復壞，　　如坎與空聚；",
        "from": "T15n0610_001:0240a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒盜賊俱止，　　取之欺殆人，",
        "from": "T15n0610_001:0240a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身為毒虺穴，　　夫毒虺劇毒；",
        "from": "T15n0610_001:0240a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒于身居止，　　喜怒毒奸弊，",
        "from": "T15n0610_001:0240a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孚不和大毒。",
        "from": "T15n0610_001:0240a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒以和安隱，　　忿則無反復，",
        "from": "T15n0610_001:0240a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須臾復不安。",
        "from": "T15n0610_001:0240a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嫌而有恐畏，　　如虎遊荒澤，",
        "from": "T15n0610_001:0240a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有畏多恐怖。",
        "from": "T15n0610_001:0240a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　諸念為以仰，為一切苦器，　　亦為諸劇事。",
        "from": "T15n0610_001:0240b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斯身主為受，　　沈於苦之法，",
        "from": "T15n0610_001:0240b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸病宅，　　為老死之法。",
        "from": "T15n0610_001:0240b03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身為增恩愛，　　苦以寒與熱，",
        "from": "T15n0610_001:0240b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或風而不和，　　是則病如生。",
        "from": "T15n0610_001:0240b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為敗壞諸根，　　失疾賊害人，",
        "from": "T15n0610_001:0240b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "老死笮厄人，　　如雪聚得火，",
        "from": "T15n0610_001:0240b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疾而為解釋。",
        "from": "T15n0610_001:0240b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身之多災禍，　　吾所演一切，",
        "from": "T15n0610_001:0240b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未能已備具。",
        "from": "T15n0610_001:0240b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "腐身為不實，　　多因緣以成，",
        "from": "T15n0610_001:0240b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大耶以為軀。",
        "from": "T15n0610_001:0240b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說內身觀章句經十一因緣章佛言：",
        "from": "T15n0610_001:0240b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「行者有十一因緣，滅道制令人不墮惡道，當不識者，謂萬物。",
        "from": "T15n0610_001:0240b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一為大會，謂人眾；",
        "from": "T15n0610_001:0240b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者多食，謂諸美，亦謂過飽；",
        "from": "T15n0610_001:0240b17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者為多行，謂多業；",
        "from": "T15n0610_001:0240b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四為多喧，謂多語；",
        "from": "T15n0610_001:0240b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五為多睡眠；",
        "from": "T15n0610_001:0240b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六為會聚，謂禪中；",
        "from": "T15n0610_001:0240b19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七為習行，謂多事；",
        "from": "T15n0610_001:0240b19_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八為愛身；",
        "from": "T15n0610_001:0240b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九為輕，謂非法語；",
        "from": "T15n0610_001:0240b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十為貪，謂多欲；",
        "from": "T15n0610_001:0240b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十一為不好善處居，謂惡人中。",
        "from": "T15n0610_001:0240b21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行道者當斷是十一因緣，得道疾。",
        "from": "T15n0610_001:0240b22_11",
