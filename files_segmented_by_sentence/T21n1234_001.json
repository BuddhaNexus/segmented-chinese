        "from": "T21n1234_001:0175c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令梵天之眾及沙門婆羅門。",
        "from": "T21n1234_001:0175c13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切世間天人阿修羅眾。",
        "from": "T21n1234_001:0175c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永不見於諸魔境界。",
        "from": "T21n1234_001:0175c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此真言能作救護。",
        "from": "T21n1234_001:0175c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "饒益有情息除災患。",
        "from": "T21n1234_001:0175c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "消眾毒藥令得安樂。",
        "from": "T21n1234_001:0175c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人非人眾天子天眾及天女眾。",
        "from": "T21n1234_001:0175c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿修羅阿修羅眾及阿修羅女眾。",
        "from": "T21n1234_001:0175c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乾闥婆乾闥婆眾及乾闥婆女眾。",
        "from": "T21n1234_001:0175c18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍及龍眾及龍女眾。",
        "from": "T21n1234_001:0175c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥叉藥叉眾及藥叉女眾。",
        "from": "T21n1234_001:0175c19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅剎羅剎眾及羅剎女眾。",
        "from": "T21n1234_001:0175c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘舍遮毘舍遮眾及毘舍遮女眾。",
        "from": "T21n1234_001:0175c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "部多部多眾及部多女眾。",
        "from": "T21n1234_001:0175c21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羯吒布單那羯吒布單曩眾及羯吒布單曩女眾。",
        "from": "T21n1234_001:0175c22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "車夜車夜眾及車夜女眾。",
        "from": "T21n1234_001:0175c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烏麼那烏麼那眾及烏麼那女眾。",
        "from": "T21n1234_001:0175c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿鉢娑麼二合引囉阿鉢娑麼二合引囉眾及阿鉢娑麼二合引囉女眾。",
        "from": "T21n1234_001:0175c25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烏娑多二合引囉烏娑多二合引囉眾及烏娑多二合引囉女眾。",
        "from": "T21n1234_001:0175c26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仡囉二合賀仡囉二合賀眾及仡囉二合賀女眾。",
        "from": "T21n1234_001:0175c27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦軀哩那二合迦軀哩那二合眾及迦軀哩那二合女眾。",
        "from": "T21n1234_001:0175c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有起屍鬼黑鬼。",
        "from": "T21n1234_001:0175c29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祖囉拏二合瑜虞吠哆尼所二合嚩毒藥鬼。",
        "from": "T21n1234_001:0176a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "丁瘡病鬼漏瘡病鬼。",
        "from": "T21n1234_001:0176a01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "癩病鬼噎病鬼吐病鬼。",
        "from": "T21n1234_001:0176a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顛倒鬼迷癡鬼。",
        "from": "T21n1234_001:0176a02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡心者瞋心者。",
        "from": "T21n1234_001:0176a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼等諸鬼所著所魅之時。",
        "from": "T21n1234_001:0176a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆不能為害終不得其便。",
        "from": "T21n1234_001:0176a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大明力火不能燒。",
        "from": "T21n1234_001:0176a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水不能漂毒不能害。",
        "from": "T21n1234_001:0176a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀杖弓箭而不能傷。",
        "from": "T21n1234_001:0176a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凶惡賊盜不能怖畏。",
        "from": "T21n1234_001:0176a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "橫病不侵命不中夭壽年永遠。",
        "from": "T21n1234_001:0176a06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生見者和順。",
        "from": "T21n1234_001:0176a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切有情見者愛樂名譽遠聞。",
        "from": "T21n1234_001:0176a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "假使冤家亦如兄弟。",
        "from": "T21n1234_001:0176a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前所說種種利益皆令獲得。",
        "from": "T21n1234_001:0176a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說明曰。",
        "from": "T21n1234_001:0176a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫薩哩嚩二合沒馱達哩摩二合僧契引毘藥二合悉馱野二合哆引摩波囉引𡁠哆引尾儞野二合摩摩達哩麼二合引迦囉細引曩寫薩波哩嚩引囉寫阿曩囉他二合引鉢囉二合底嚩引哩焰二合檐引阿哩體二合數左散捺哩二合設孕二合檐引娑嚩二合悉諦野三合娑覩二合摩摩達哩麼二合引迦囉細引曩寫薩波哩嚩引囉寫悉馱尾儞野二合娑嚩二合引賀引無能勝大明陀羅尼經",
        "from": "T21n1234_001:0176a18_100",
        "context_with_next_sentence": false
    }
