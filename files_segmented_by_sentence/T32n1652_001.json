[
    {
        "source_sentence": "緣生論",
        "from": "T32n1652_001:0482b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖者欝楞迦造",
        "from": "T32n1652_001:0482b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大隋南印度三藏達磨笈多譯",
        "from": "T32n1652_001:0482b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從一生於三，　　從三轉生六，",
        "from": "T32n1652_001:0482b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六二二更六，　　從六亦生六。",
        "from": "T32n1652_001:0482b06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從六有於三，　　此三復有三，",
        "from": "T32n1652_001:0482b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三復生於四，　　四復生於三。",
        "from": "T32n1652_001:0482b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從三生於一，　　彼一復生七，",
        "from": "T32n1652_001:0482b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中所有苦，　　牟尼說皆攝。",
        "from": "T32n1652_001:0482b10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二種差別，　　善淨說為空，",
        "from": "T32n1652_001:0482b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣生分力故，　　應知十二法。",
        "from": "T32n1652_001:0482b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無智與業識，　　名色根三和，",
        "from": "T32n1652_001:0482b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知渴及以取，　　集出熟後邊。",
        "from": "T32n1652_001:0482b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初八九煩惱，　　第二第十業，",
        "from": "T32n1652_001:0482b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘七皆是苦，　　三攝十二法。",
        "from": "T32n1652_001:0482b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初二是過去，　　後二未來時，",
        "from": "T32n1652_001:0482b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘八是現在，　　此謂三時法。",
        "from": "T32n1652_001:0482b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惱起業感報，　　報還生煩惱，",
        "from": "T32n1652_001:0482b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煩惱復生業，　　亦由業有報。",
        "from": "T32n1652_001:0482b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離惱何有業？",
        "from": "T32n1652_001:0482b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　業壞亦無報？",
        "from": "T32n1652_001:0482b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無報則離惱，　　此三各自滅。",
        "from": "T32n1652_001:0482b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五分因生果，　　名為煩惱業，",
        "from": "T32n1652_001:0482b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七分以為果，　　七種苦應念。",
        "from": "T32n1652_001:0482b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因中空無果，　　因中亦無因，",
        "from": "T32n1652_001:0482b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果中空無因，　　果中亦無果，",
        "from": "T32n1652_001:0482b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因果二俱空，　　智者與相應。",
        "from": "T32n1652_001:0482b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世中四種分，　　因果合故有，",
        "from": "T32n1652_001:0482b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煩惱業果合，　　念欲為六分。",
        "from": "T32n1652_001:0482c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有節所攝故，　　二節及三略，",
        "from": "T32n1652_001:0482c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因果雜為節，　　三四節總略。",
        "from": "T32n1652_001:0482c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二二三三二，　　苦時有五法，",
        "from": "T32n1652_001:0482c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作者胎境界，　　發轉生流行。",
        "from": "T32n1652_001:0482c05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迷惑發起果，　　報流果為二，",
        "from": "T32n1652_001:0482c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相應根分中，　　一一三二分。",
        "from": "T32n1652_001:0482c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "熱惱貧乏果，　　轉出津流果，",
        "from": "T32n1652_001:0482c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相應他分中，　　二一一一法。",
        "from": "T32n1652_001:0482c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此有十二種，　　等力緣自生，",
        "from": "T32n1652_001:0482c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無眾生無命，　　無動以慧知。",
        "from": "T32n1652_001:0482c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無我無我所，　　無我無我因，",
        "from": "T32n1652_001:0482c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四種無智空，　　餘分亦如是。",
        "from": "T32n1652_001:0482c13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷常二邊離，　　此即是中道，",
        "from": "T32n1652_001:0482c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若覺已成就，　　覺體是諸佛。",
        "from": "T32n1652_001:0482c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覺已於眾中，　　仙聖說無我，",
        "from": "T32n1652_001:0482c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曾於城喻經，　　導師說此義。",
        "from": "T32n1652_001:0482c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦栴延經說，　　正見及空見，",
        "from": "T32n1652_001:0482c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破邏具膩經張宿名也，亦說殊勝空。",
        "from": "T32n1652_001:0482c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼知空相應，　　緣生若不知，",
        "from": "T32n1652_001:0482c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不知彼空。",
        "from": "T32n1652_001:0482c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則不厭受眾，　　若有彼無見，",
        "from": "T32n1652_001:0482c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則迷緣生義。",
        "from": "T32n1652_001:0482c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離慢彼知空，　　及厭受眾故，",
        "from": "T32n1652_001:0482c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不迷業果合。",
        "from": "T32n1652_001:0482c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦非不緣此，　　空緣當有此，",
        "from": "T32n1652_001:0482c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "業報受用具。",
        "from": "T32n1652_001:0482c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前已說緣生，　　彼煩惱業苦，",
        "from": "T32n1652_001:0482c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三中如法攝。",
        "from": "T32n1652_001:0483a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中生於七，　　七中復生三，",
        "from": "T32n1652_001:0483a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有輪如是轉。",
        "from": "T32n1652_001:0483a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無別有眾生，　　唯是於空法，",
        "from": "T32n1652_001:0483a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還自生空法。",
        "from": "T32n1652_001:0483a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藉緣亦生業，　　藉緣亦生報，",
        "from": "T32n1652_001:0483a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無一不有緣。",
        "from": "T32n1652_001:0483a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日光種子酢，　　眾續不超到，",
        "from": "T32n1652_001:0483a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智應觀彼二。",
        "from": "T32n1652_001:0483a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣生三十論本竟緣生三十論，我當隨順次第解釋。",
        "from": "T32n1652_001:0483a11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從一生於三，　　從三轉生六，",
        "from": "T32n1652_001:0483a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六二二更六，　　從六亦生六。",
        "from": "T32n1652_001:0483a13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從一生於三者，一謂無智，此無智者說名無明，於苦集滅道中不覺知故，名為無智。",
        "from": "T32n1652_001:0483a15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由無智故則有福、非福、不動說名三行，及身行、口行、心行等從其轉生。",
        "from": "T32n1652_001:0483a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從三轉生六者，從彼三行生六識身，所謂眼識、耳識、鼻識、舌識、身識、意識。",
        "from": "T32n1652_001:0483a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六二者，彼六識身轉生二種，所謂名、色。",
        "from": "T32n1652_001:0483a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二更六者，名色二種轉生六入，所謂眼入、耳入、鼻入、舌入、身入、意入。",
        "from": "T32n1652_001:0483a21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從六亦生六者，從彼六入轉生六觸，所謂眼觸、耳觸、鼻觸、舌觸、身觸、意觸。",
        "from": "T32n1652_001:0483a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從六有於三，　　此三復有三，",
        "from": "T32n1652_001:0483a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三復生於四，　　四復生於三。",
        "from": "T32n1652_001:0483a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從六有於三者，從彼六觸轉生三受，所謂樂受、苦受、不苦不樂受。",
        "from": "T32n1652_001:0483a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此三復有三者，還從彼等三受轉生三種渴愛，所謂欲渴愛、有渴愛、無有渴愛。",
        "from": "T32n1652_001:0483a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三復生於四者，從彼三種渴愛轉生四取，所謂欲取、見取、戒苦行取、我語取。",
        "from": "T32n1652_001:0483b01_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四復生於三者，從彼四取轉生三有，所謂欲有、色有、無色有。",
        "from": "T32n1652_001:0483b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從三生於一，　　彼一復生七，",
        "from": "T32n1652_001:0483b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中所有苦，　　牟尼說皆攝。",
        "from": "T32n1652_001:0483b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從三生於一者，還以彼等三有作緣，生當來一種生。",
        "from": "T32n1652_001:0483b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼一復生七者，還從彼一，當有老、死、憂、悲、苦、惱、困等七種。",
        "from": "T32n1652_001:0483b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中所有苦牟尼說皆攝者，於中無明為始、困為終，無量種苦，世尊略說，皆此所攝。",
        "from": "T32n1652_001:0483b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二種差別，　　善淨說為空，",
        "from": "T32n1652_001:0483b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣生分力故，　　應知十二法。",
        "from": "T32n1652_001:0483b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二種差別善淨說為空者，此無智等各別不雜，有十二分。",
        "from": "T32n1652_001:0483b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又彼皆自性空，應當正見如此所說唯是空法、自生空法。",
        "from": "T32n1652_001:0483b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣生分力故應知十二法者，若以次第生分力故，彼十二法如是應知，彼中迷惑相者是無明；",
        "from": "T32n1652_001:0483b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼行句處，積集當有相者是行；",
        "from": "T32n1652_001:0483b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼識句處，次受生分轉出相者是識；",
        "from": "T32n1652_001:0483b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼名色句處，名身色身和合相者是名色；",
        "from": "T32n1652_001:0483b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼六入句處，相安置相者是六入；",
        "from": "T32n1652_001:0483b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼觸句處，眼、色、識共聚相者是觸；",
        "from": "T32n1652_001:0483b21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼受句處，愛不愛受用相者是受；",
        "from": "T32n1652_001:0483b22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼渴愛句處，無厭足相者是渴愛；",
        "from": "T32n1652_001:0483b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼取句處，執持攝取相者是取；",
        "from": "T32n1652_001:0483b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼有句處，名身色身相者是有；",
        "from": "T32n1652_001:0483b24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼生句處，五眾出生相者是生；",
        "from": "T32n1652_001:0483b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼老句處，成熟相者是老；",
        "from": "T32n1652_001:0483b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼死句處，命根斷者是死；",
        "from": "T32n1652_001:0483b26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼憂句處，高舉相者是憂；",
        "from": "T32n1652_001:0483b27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼悲句處，哭聲者是悲；",
        "from": "T32n1652_001:0483b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼苦句處，身燒熱相者是苦；",
        "from": "T32n1652_001:0483b28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼惱句處，心逼惱相者是惱；",
        "from": "T32n1652_001:0483b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼困句處，極疲乏相者是困。",
        "from": "T32n1652_001:0483b29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無智與業識，　　名色根三和，",
        "from": "T32n1652_001:0483c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知渴及以取，　　集出熟後邊。",
        "from": "T32n1652_001:0483c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中無智者是無明。",
        "from": "T32n1652_001:0483c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "業者是行。",
        "from": "T32n1652_001:0483c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識者是解。",
        "from": "T32n1652_001:0483c03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名色者是五眾總聚。",
        "from": "T32n1652_001:0483c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "根者是入。",
        "from": "T32n1652_001:0483c04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三和者是觸。",
        "from": "T32n1652_001:0483c04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知者是受。",
        "from": "T32n1652_001:0483c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "渴者是渴愛。",
        "from": "T32n1652_001:0483c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取者是執持。",
        "from": "T32n1652_001:0483c05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "集者是有。",
        "from": "T32n1652_001:0483c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出者是生。",
        "from": "T32n1652_001:0483c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "熟者是老。",
        "from": "T32n1652_001:0483c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後邊者是死。",
        "from": "T32n1652_001:0483c06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此等差別相攝，我當次第說之。",
        "from": "T32n1652_001:0483c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中業、煩惱、報差別：",
        "from": "T32n1652_001:0483c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初八九煩惱，　　第二第十業，",
        "from": "T32n1652_001:0483c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘七皆是苦，　　三攝十二法。",
        "from": "T32n1652_001:0483c10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三煩惱者，無明、渴愛、取。",
        "from": "T32n1652_001:0483c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二業者，行、有。",
        "from": "T32n1652_001:0483c11_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七報者，識、名色、六入、觸、受、生、老死等。",
        "from": "T32n1652_001:0483c12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此十二法，三種所攝。",
        "from": "T32n1652_001:0483c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又時差別：",
        "from": "T32n1652_001:0483c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初二是過去，　　後二未來時，",
        "from": "T32n1652_001:0483c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘八是現在，　　此謂三時法。",
        "from": "T32n1652_001:0483c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無明、行初二種，過去時中。",
        "from": "T32n1652_001:0483c17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生、老死後二種，未來時中。",
        "from": "T32n1652_001:0483c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識、名色、六入、觸、受、渴愛、取、有八種，現在時中。",
        "from": "T32n1652_001:0483c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此等各各次第相生。",
        "from": "T32n1652_001:0483c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惱起業感報，　　報還生煩惱，",
        "from": "T32n1652_001:0483c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煩惱復生業，　　亦由業有報。",
        "from": "T32n1652_001:0483c22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煩惱、業、報三種如前所說，由彼煩惱故有業，由業故有報，還由報故有煩惱，由煩惱故有業，由業故有報。",
        "from": "T32n1652_001:0483c25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰：",
        "from": "T32n1652_001:0483c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由煩惱盡各各寂滅。",
        "from": "T32n1652_001:0483c26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其義云何？",
        "from": "T32n1652_001:0483c26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰：",
        "from": "T32n1652_001:0483c26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離惱何有業，　　業壞亦無報，",
        "from": "T32n1652_001:0483c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無報則離惱，　　此三各自滅。",
        "from": "T32n1652_001:0483c28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若其此心無煩惱染則不集業，若不作業則不受報，若滅報者亦不生煩惱。",
        "from": "T32n1652_001:0484a01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是此三各各自滅。",
        "from": "T32n1652_001:0484a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此等有因果分。",
        "from": "T32n1652_001:0484a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五分因生果，　　名為煩惱業，",
        "from": "T32n1652_001:0484a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七分以為果，　　七種苦應念。",
        "from": "T32n1652_001:0484a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五種因名為煩惱業者，如前所說無明、行、渴愛、取、有是也。",
        "from": "T32n1652_001:0484a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七種果轉生者，亦如前所說七種苦，所謂識、名色、六入、觸、受、生、老死是也。",
        "from": "T32n1652_001:0484a08_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此因果二種空。",
        "from": "T32n1652_001:0484a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因中空無果，　　因中亦無因，",
        "from": "T32n1652_001:0484a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果中空無因，　　果中亦無果，",
        "from": "T32n1652_001:0484a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因果二俱空，　　智者與相應。",
        "from": "T32n1652_001:0484a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵本本一偈，今為一偈半。",
        "from": "T32n1652_001:0484a12_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若此所說因果二種，於中若因空果，果亦空因；",
        "from": "T32n1652_001:0484a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是因亦空因，果亦空果。",
        "from": "T32n1652_001:0484a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此四際，當與相應。",
        "from": "T32n1652_001:0484a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此更有別分。",
        "from": "T32n1652_001:0484a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世中四種分，　　因果合故有，",
        "from": "T32n1652_001:0484a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煩惱業果合，　　念欲為六分。",
        "from": "T32n1652_001:0484a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世中四種分因果合故有者，道中五種因及七種果和合，故總略為四種，次第有四種分。",
        "from": "T32n1652_001:0484a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中無明、行，過去時中，二法為第一分。",
        "from": "T32n1652_001:0484a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識、名色、六入、觸、受，現在時中，為第二分。",
        "from": "T32n1652_001:0484a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "渴、愛、取、有亦是現在時中，為第三分。",
        "from": "T32n1652_001:0484a23_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生、老死，未來時中，二法為第四分。",
        "from": "T32n1652_001:0484a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此謂四種分也。",
        "from": "T32n1652_001:0484a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煩惱業果合念欲為六分者，煩惱、業、報三種和合，以次第故，於二根本有六種分。",
        "from": "T32n1652_001:0484a26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中無明所攝乃至受，以無明為根。",
        "from": "T32n1652_001:0484a27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "渴愛所攝乃至老死，以渴愛為根。",
        "from": "T32n1652_001:0484a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無明根中，無明是煩惱分，行是業分，識、名色、六入、觸、受是報分。",
        "from": "T32n1652_001:0484a29_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "渴愛根中，渴愛、取是煩惱分，有是業分，生，老死是報分。",
        "from": "T32n1652_001:0484b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又節分總略。",
        "from": "T32n1652_001:0484b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有節所攝故，　　二節及三略，",
        "from": "T32n1652_001:0484b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因果雜為節，　　三四節總略。",
        "from": "T32n1652_001:0484b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有節為本，發起二節，所謂有、生兩間為第一節，行、識兩間是第二節，此二並為業果節。",
        "from": "T32n1652_001:0484b07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受、渴愛中因果共雜是第三節。",
        "from": "T32n1652_001:0484b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此之三節復為四種總略。",
        "from": "T32n1652_001:0484b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無明、行二種是第一總略，識、名色、六入、觸、受五種是第二總略，渴愛、取、有三種是第三總略，生、老死二種是第四總略。",
        "from": "T32n1652_001:0484b11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此謂三節及四總略。",
        "from": "T32n1652_001:0484b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此等法中住時差別。",
        "from": "T32n1652_001:0484b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二二三三二，　　苦時有五法，",
        "from": "T32n1652_001:0484b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作者胎境界，　　發轉生流行。",
        "from": "T32n1652_001:0484b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二二法者，無明、行說為二種，識、名色說為二種。",
        "from": "T32n1652_001:0484b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三三者，六入、觸、受說為三種，渴愛、取、有說為三種。",
        "from": "T32n1652_001:0484b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又二者，生、老死說為二種。",
        "from": "T32n1652_001:0484b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此等五法，是苦時中作者、胎藏、境界、發轉、出生，於中流行，如數當知。",
        "from": "T32n1652_001:0484b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中無明、行二種，說為苦時中作者，應如是見。",
        "from": "T32n1652_001:0484b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識、名色二種，說為苦時中胎藏，應如是見。",
        "from": "T32n1652_001:0484b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六入、觸、受三種，說為苦時中境界。",
        "from": "T32n1652_001:0484b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "渴愛、取、有三種，說為苦時中發轉。",
        "from": "T32n1652_001:0484b23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生、老死二種，說為苦時中出生。",
        "from": "T32n1652_001:0484b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又果差別。",
        "from": "T32n1652_001:0484b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迷惑發起果，　　報流果為二，",
        "from": "T32n1652_001:0484b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相應根分中，　　一一三二分。",
        "from": "T32n1652_001:0484b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前所說，此無明根及渴愛根，於無明根第一分中迷惑，發起報流，名四種果。",
        "from": "T32n1652_001:0484b29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一三二數分之中，隨其次第當與相應。",
        "from": "T32n1652_001:0484c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中無明是迷惑果，行是發起果，識、名色、六入是報果，觸、受是津流果。",
        "from": "T32n1652_001:0484c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有餘殘果。",
        "from": "T32n1652_001:0484c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "熱惱貧乏果，　　轉出津流果，",
        "from": "T32n1652_001:0484c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相應他分中，　　二一一一法。",
        "from": "T32n1652_001:0484c05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前所說第二渴愛根分中，熱惱貧乏轉生津流果等，隨其數分二一一一，於此法中當與相應。",
        "from": "T32n1652_001:0484c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中渴愛、取是熱惱果，有是貧乏果，生是轉出果，老死是津流果。",
        "from": "T32n1652_001:0484c09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是此等則有八果。",
        "from": "T32n1652_001:0484c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此有十二種，　　等力緣自生，",
        "from": "T32n1652_001:0484c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無眾生無命，　　無動以慧知。",
        "from": "T32n1652_001:0484c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是無明為初、老死為邊，有十二分，平等勝故各各緣生，而無眾生、無壽命、無動作，以慧應知。",
        "from": "T32n1652_001:0484c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中無眾生者，以不牢固故。",
        "from": "T32n1652_001:0484c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無壽命者，以無我故。",
        "from": "T32n1652_001:0484c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無動作者，以無作者故。",
        "from": "T32n1652_001:0484c16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無我無我所，　　無我無我因，",
        "from": "T32n1652_001:0484c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四種無智空，　　餘分亦如是。",
        "from": "T32n1652_001:0484c18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於無我中無智、無我所中無智、無我中無智、無我因中無智，此中以無我故，四種無智空。",
        "from": "T32n1652_001:0484c20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如四種次第無智空，如是行等餘分亦皆是空，應當知之。",
        "from": "T32n1652_001:0484c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷常二邊離，　　此即是中道，",
        "from": "T32n1652_001:0484c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若覺已成就，　　覺體是諸佛。",
        "from": "T32n1652_001:0484c24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有是常攝、無是斷攝，如此二邊彼作緣，彼處，彼處諸有轉生。",
        "from": "T32n1652_001:0484c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離此二邊即是中道，若無智諸外道則墮於邊，若已覺悟諸佛世尊，則為覺體。",
        "from": "T32n1652_001:0484c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此世間獨能成就，餘人悉無此義。",
        "from": "T32n1652_001:0484c28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覺已於眾中，　　仙聖說無我，",
        "from": "T32n1652_001:0484c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曾於城喻經，　　導師說此義。",
        "from": "T32n1652_001:0485a01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼亦是此中道覺已，於諸眾中佛說無我。",
        "from": "T32n1652_001:0485a02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂比丘！",
        "from": "T32n1652_001:0485a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有我、我所，小兒凡夫無聞之類，順墮假名；",
        "from": "T32n1652_001:0485a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中無復我及我所。",
        "from": "T32n1652_001:0485a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比丘！",
        "from": "T32n1652_001:0485a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生時但苦生，滅時但苦滅。",
        "from": "T32n1652_001:0485a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如《城喻經》中導師已說此義。",
        "from": "T32n1652_001:0485a05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又：",
        "from": "T32n1652_001:0485a05_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦栴延經說，　　正見及空見，",
        "from": "T32n1652_001:0485a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破邏具膩經張宿名也，　　亦說殊勝空。",
        "from": "T32n1652_001:0485a07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此等三經及以餘處，如是之相世尊已說彼亦是此。",
        "from": "T32n1652_001:0485a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣生若正知，　　彼知空相應，",
        "from": "T32n1652_001:0485a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣生若不知，　　亦不知彼空。",
        "from": "T32n1652_001:0485a11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於前所說緣生，若有正知，彼知無異。",
        "from": "T32n1652_001:0485a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼復何知？",
        "from": "T32n1652_001:0485a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂知於空。",
        "from": "T32n1652_001:0485a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣生若不知亦不知彼空者，於此緣生若其不知，亦於彼空不能解入。",
        "from": "T32n1652_001:0485a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當知之。",
        "from": "T32n1652_001:0485a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於空若起慢，　　則不厭受眾，",
        "from": "T32n1652_001:0485a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有彼無見，　　則迷緣生義。",
        "from": "T32n1652_001:0485a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於空若起慢則不厭受眾者，若起空慢，則於五受眾中不生厭離。",
        "from": "T32n1652_001:0485a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有彼無見則迷緣生義者，若復由於無見迷此緣生義故，則於四種見中隨取何見，一者斷見、二者常見、三者自在化語、四者唯依宿世所作。",
        "from": "T32n1652_001:0485a22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣生不迷故，　　離慢彼知空，",
        "from": "T32n1652_001:0485a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及厭受眾故，　　不迷業果合。",
        "from": "T32n1652_001:0485a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣生不迷故離慢彼知空者，於前所說各各緣生中若無迷心，及於攝取我我所中若得離慢，彼則如法能入於空。",
        "from": "T32n1652_001:0485a27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及厭受眾故不迷業果合者，五受眾中攝取我我所故，則遍世間輪轉不息；",
        "from": "T32n1652_001:0485a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彼受眾起厭離故，於此業果相續則無顛倒亦不迷惑。",
        "from": "T32n1652_001:0485b01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又問：",
        "from": "T32n1652_001:0485b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此義云何？",
        "from": "T32n1652_001:0485b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "業作緣續生，　　亦非不緣此，",
        "from": "T32n1652_001:0485b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空緣當有此，　　業報受用具。",
        "from": "T32n1652_001:0485b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "業作緣續生亦非不緣此者，煩惱業染如前所說，彼以如是淨不淨業，推遣眾生傍及上下相續而生。",
        "from": "T32n1652_001:0485b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若非此業，則不作緣。",
        "from": "T32n1652_001:0485b07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不然者，則未作竟而來，及已作竟而失來謂業未作竟其果即來，失謂業已作竟其果便失。",
        "from": "T32n1652_001:0485b09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空緣當有此業報受用具者，若由此等淨不淨業有報受用，則自性是空。",
        "from": "T32n1652_001:0485b10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本無有我、作緣發生，彼自性空亦應當知。",
        "from": "T32n1652_001:0485b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼義今更略說。",
        "from": "T32n1652_001:0485b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二分差別，　　前已說緣生，",
        "from": "T32n1652_001:0485b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼煩惱業苦，　　三中如法攝。",
        "from": "T32n1652_001:0485b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無明為初、老死為邊，是十二分緣生差別，如前所說。",
        "from": "T32n1652_001:0485b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼中三是煩惱、二是業、七是苦，皆已攝入。",
        "from": "T32n1652_001:0485b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三中生於二，　　二中生於七，",
        "from": "T32n1652_001:0485b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七中復生三，　　有輪如是轉。",
        "from": "T32n1652_001:0485b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無明、渴愛、取三種所生行有二種，彼二所生識、名色、六入、觸、受、生、老死七分。",
        "from": "T32n1652_001:0485b21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼七分中如前所說還生三種，彼三復二，其二更七。",
        "from": "T32n1652_001:0485b22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故二種次第不斷，此之有輪如是普轉。",
        "from": "T32n1652_001:0485b23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因果諸生世，　　無別有眾生，",
        "from": "T32n1652_001:0485b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯是於空法，　　還自生空法。",
        "from": "T32n1652_001:0485b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因果諸生世無別有眾生者，無明、行、渴愛、取、有五種名因，識、名色、六入、觸、受、生、老死七種名果，此等所有普遍世間。",
        "from": "T32n1652_001:0485b28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若我、若眾生、若壽、若生者、若丈夫、若人、若作者，是等為首，次第分別，其唯虛誑，應當知之。",
        "from": "T32n1652_001:0485c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼云何生？",
        "from": "T32n1652_001:0485c01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯是於空法還自生空法，謂自性空中假名煩惱、業、果，唯有別空假名煩惱、業、果法生。",
        "from": "T32n1652_001:0485c03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是其義。",
        "from": "T32n1652_001:0485c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藉緣生煩惱，　　藉緣亦生業，",
        "from": "T32n1652_001:0485c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藉緣亦生報，　　無一不有緣。",
        "from": "T32n1652_001:0485c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有煩惱，則有種種無量種業，及種種業所生果報。",
        "from": "T32n1652_001:0485c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼皆共因共緣，應當知之。",
        "from": "T32n1652_001:0485c08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有一處無因緣者。",
        "from": "T32n1652_001:0485c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又為明彼義，今更說譬喻。",
        "from": "T32n1652_001:0485c10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦燈印鏡音，　　日光種子酢，",
        "from": "T32n1652_001:0485c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾續不超到，　　智應觀彼二。",
        "from": "T32n1652_001:0485c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如誦，有教誦者、受誦者，所有教誦不到受誦。",
        "from": "T32n1652_001:0485c13_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故？",
        "from": "T32n1652_001:0485c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教誦者仍安住故。",
        "from": "T32n1652_001:0485c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其教誦者亦不相續。",
        "from": "T32n1652_001:0485c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故？",
        "from": "T32n1652_001:0485c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自不生故。",
        "from": "T32n1652_001:0485c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如燈次第生，非是初燈超到，亦非第二無因而生。",
        "from": "T32n1652_001:0485c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是印與像二種、面與鏡二種、音與響二種、日與火二種、種子與芽二種、酢與舌涎二種，此等所有皆不超到，亦非不生，亦非無因而生。",
        "from": "T32n1652_001:0485c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼二種五受眾相續到時，非是初受眾超到，而第二受眾亦非不生亦非無因而生。",
        "from": "T32n1652_001:0485c21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智者於此受眾相續不超而次第到，應當正觀。",
        "from": "T32n1652_001:0485c22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又外內受用俱有十種，皆應當見。",
        "from": "T32n1652_001:0485c23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中外十種者，一者非常故、二者非斷故、三者不超到故、四者無中間故、五者非彼體故、六者非別異故、七者無作者故、八者非無因故、九者念念空故、十者同類果相繫故。",
        "from": "T32n1652_001:0485c27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼外所有種子滅無餘故、非常芽出生故、非斷種子滅無餘已，其芽本無今有生故不超到。",
        "from": "T32n1652_001:0485c29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼所相續無有斷絕，因果相繫故無中間。",
        "from": "T32n1652_001:0486a01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種子芽差別故非彼體，從彼出生故非別異，因緣和合故無作者，種子為因故非無因。",
        "from": "T32n1652_001:0486a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種子芽莖枝葉花果等傳傳相生故念念空。",
        "from": "T32n1652_001:0486a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甜酢醎苦辛澁隨因差別果轉出，故同類果相繫於中。",
        "from": "T32n1652_001:0486a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內十種者，一者死邊受眾滅無餘故非常；",
        "from": "T32n1652_001:0486a06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者得次生分受眾故非斷；",
        "from": "T32n1652_001:0486a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者死邊受眾滅無餘已，次生分受眾，本無今有生，故不超到；",
        "from": "T32n1652_001:0486a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者受眾相續無有斷絕，因果相繫故無中間；",
        "from": "T32n1652_001:0486a09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者死邊次生分受眾差別故非彼體；",
        "from": "T32n1652_001:0486a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六者從彼出生故非別異；",
        "from": "T32n1652_001:0486a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七者因緣和合故無作者；",
        "from": "T32n1652_001:0486a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八者煩惱業為因故非無因；",
        "from": "T32n1652_001:0486a12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九者迦羅邏、頞浮陀、箄尸、伽那奢佉、出胎、嬰孩、童子、少年、長宿等傳傳相生，故念念空；",
        "from": "T32n1652_001:0486a14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十者善不善熏，隨因差別果轉出，故同類果相繫。",
        "from": "T32n1652_001:0486a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又有三偈：",
        "from": "T32n1652_001:0486a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如燈炎轉生，　　識身亦如是，",
        "from": "T32n1652_001:0486a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先際與後際，　　亦無有聚集。",
        "from": "T32n1652_001:0486a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無生亦有生，　　破壞無相著，",
        "from": "T32n1652_001:0486a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生已亦無住，　　而此作業轉。",
        "from": "T32n1652_001:0486a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於彼緣生，　　而能觀知空，",
        "from": "T32n1652_001:0486a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為其施設教，　　彼亦是中道。",
        "from": "T32n1652_001:0486a22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中無明、行、渴愛、取、有，是為集諦。",
        "from": "T32n1652_001:0486a23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識、名色、六入、觸、受、生、老死，是為苦諦。",
        "from": "T32n1652_001:0486a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼等十二分滅，是為滅諦。",
        "from": "T32n1652_001:0486a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於緣生如實能知，是為道諦。",
        "from": "T32n1652_001:0486a25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣生論",
        "from": "T32n1652_001:0486a26_100",
        "context_with_next_sentence": false
    }
]