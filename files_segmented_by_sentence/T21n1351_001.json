[
    {
        "source_sentence": "佛說持句神呪經",
        "from": "T21n1351_001:0864c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吳月支優婆塞支謙譯",
        "from": "T21n1351_001:0864c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞如是。一時佛在舍衛國祇樹給孤獨園。與",
        "from": "T21n1351_001:0864c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大比丘眾千二百五十人菩薩萬人。",
        "from": "T21n1351_001:0864c13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時從是佛土過億百千佛國。",
        "from": "T21n1351_001:0864c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有世界名無量華。",
        "from": "T21n1351_001:0864c14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其佛號鄧尊王如來無所著至真等正覺。",
        "from": "T21n1351_001:0864c15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今現在說法。",
        "from": "T21n1351_001:0864c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼佛遣二菩薩。",
        "from": "T21n1351_001:0864c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名無量光明二名大光明。",
        "from": "T21n1351_001:0864c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時二菩薩來至釋迦文佛所。",
        "from": "T21n1351_001:0864c17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稽首佛足却住一面。",
        "from": "T21n1351_001:0864c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前白佛言。",
        "from": "T21n1351_001:0864c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯然世尊從是佛土過億百千佛國。",
        "from": "T21n1351_001:0864c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有世界名無量華。",
        "from": "T21n1351_001:0864c19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其佛號鄧尊王如來。",
        "from": "T21n1351_001:0864c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遣我等來。",
        "from": "T21n1351_001:0864c20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊如常不其眾安隱無他耶。",
        "from": "T21n1351_001:0864c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天龍鬼神餓鬼女鬼鳩桓。",
        "from": "T21n1351_001:0864c21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人非人虎狼毒獸。",
        "from": "T21n1351_001:0864c22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得無嬈亂人民。",
        "from": "T21n1351_001:0864c22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用是故佛遣是持句呪來。",
        "from": "T21n1351_001:0864c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "哀念眾生令安。",
        "from": "T21n1351_001:0864c23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉善名聞威神得力。",
        "from": "T21n1351_001:0864c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是。",
        "from": "T21n1351_001:0864c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闍梨摩訶　闍梨羅尼　優佉目佉　沙波提　摩訶沙波提佛告阿難。",
        "from": "T21n1351_001:0864c27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受是持句呪執持誦說。",
        "from": "T21n1351_001:0864c27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛世難值。",
        "from": "T21n1351_001:0864c27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持句呪亦為難遇。",
        "from": "T21n1351_001:0864c28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子善女人。",
        "from": "T21n1351_001:0864c28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受是持句呪。",
        "from": "T21n1351_001:0864c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諷誦讀識七世宿命。",
        "from": "T21n1351_001:0864c29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持者一切浮陀鬼神若人非人。",
        "from": "T21n1351_001:0865a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得嬈近。",
        "from": "T21n1351_001:0865a01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒蛇不敢螫。",
        "from": "T21n1351_001:0865a01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒藥自然除。",
        "from": "T21n1351_001:0865a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀亦不能傷。",
        "from": "T21n1351_001:0865a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王亦不能害。",
        "from": "T21n1351_001:0865a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵亦不瞋之。",
        "from": "T21n1351_001:0865a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是持句呪。",
        "from": "T21n1351_001:0865a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七十七億佛所說。",
        "from": "T21n1351_001:0865a03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "犯是呪者當獲重罪。",
        "from": "T21n1351_001:0865a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有菩薩彌勒等八十人。",
        "from": "T21n1351_001:0865a04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告賢者阿難。",
        "from": "T21n1351_001:0865a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦當說持句呪。",
        "from": "T21n1351_001:0865a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "哀念眾生令安。",
        "from": "T21n1351_001:0865a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉善名聞威神得力。",
        "from": "T21n1351_001:0865a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是。",
        "from": "T21n1351_001:0865a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿知和知　吒佉羅羅　里彌喜利　彌利提盧　留彌勒佛告阿難。",
        "from": "T21n1351_001:0865a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受是持句呪執持誦說。",
        "from": "T21n1351_001:0865a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛世難值。",
        "from": "T21n1351_001:0865a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持句呪諷誦讀識。",
        "from": "T21n1351_001:0865a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十四世宿命。",
        "from": "T21n1351_001:0865a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持者一切浮陀鬼神。",
        "from": "T21n1351_001:0865a10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人非人不得嬈近。",
        "from": "T21n1351_001:0865a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒蛇不敢螫。",
        "from": "T21n1351_001:0865a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒藥自然除。",
        "from": "T21n1351_001:0865a11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀亦不能傷。",
        "from": "T21n1351_001:0865a11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王亦不敢害。",
        "from": "T21n1351_001:0865a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵亦不瞋之。",
        "from": "T21n1351_001:0865a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是持句呪八十四億佛所說。",
        "from": "T21n1351_001:0865a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謗是呪者當獲重罪。",
        "from": "T21n1351_001:0865a13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告阿難。",
        "from": "T21n1351_001:0865a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦當說持句呪。",
        "from": "T21n1351_001:0865a14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "哀念眾生令安。",
        "from": "T21n1351_001:0865a14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉善名聞威神得力。",
        "from": "T21n1351_001:0865a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是。",
        "from": "T21n1351_001:0865a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿知和知　那知鳩那知　提梨　沙羅波提　阿那波提　波那提　波那迦和尼　摩訶迦和尼佛告阿難。",
        "from": "T21n1351_001:0865a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受是持句呪執持誦說。",
        "from": "T21n1351_001:0865a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛世難值。",
        "from": "T21n1351_001:0865a19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持句呪亦為難遇。",
        "from": "T21n1351_001:0865a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子善女人。",
        "from": "T21n1351_001:0865a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受是持句呪諷誦讀。",
        "from": "T21n1351_001:0865a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識無數世宿命。",
        "from": "T21n1351_001:0865a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持者一切浮陀鬼神。",
        "from": "T21n1351_001:0865a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人非人不敢嬈近。",
        "from": "T21n1351_001:0865a22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒蛇不敢螫。",
        "from": "T21n1351_001:0865a22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒藥自然除。",
        "from": "T21n1351_001:0865a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀亦不敢傷。",
        "from": "T21n1351_001:0865a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王亦不敢害。",
        "from": "T21n1351_001:0865a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵亦不瞋之。",
        "from": "T21n1351_001:0865a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是持句呪無數億佛所說。",
        "from": "T21n1351_001:0865a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "犯是呪者當獲重罪。",
        "from": "T21n1351_001:0865a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告阿難。",
        "from": "T21n1351_001:0865a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是持句呪。",
        "from": "T21n1351_001:0865a25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行道中當念之。",
        "from": "T21n1351_001:0865a25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若至縣官若行賊中。",
        "from": "T21n1351_001:0865a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若蠱道若毒若刀刃中。",
        "from": "T21n1351_001:0865a26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人非人中當念之。",
        "from": "T21n1351_001:0865a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是持句呪於枯樹令生葉華實。",
        "from": "T21n1351_001:0865a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況為人結縷也。",
        "from": "T21n1351_001:0865a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當使吉百病消除。",
        "from": "T21n1351_001:0865a28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然安隱辟除凶害。",
        "from": "T21n1351_001:0865a29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無佛令呪皆得從願。",
        "from": "T21n1351_001:0865a29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說經已皆大歡喜。",
        "from": "T21n1351_001:0865b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說持句神呪經",
        "from": "T21n1351_001:0865b02_100",
        "context_with_next_sentence": false
    }
]