[
    {
        "source_sentence": "速疾立驗魔醯首羅天說阿尾",
        "from": "T21n1277_001:0329b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奢法一卷",
        "from": "T21n1277_001:0329b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T21n1277_001:0329b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空諡大鑒正號大廣智大興善寺三藏沙門不空奉　詔譯爾時那羅延天。",
        "from": "T21n1277_001:0329b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在於香醉山頂。",
        "from": "T21n1277_001:0329b10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請魔醯首羅於自在宮中。",
        "from": "T21n1277_001:0329b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養頭面禮足。",
        "from": "T21n1277_001:0329b11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白魔醯首羅言。",
        "from": "T21n1277_001:0329b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我所乘迦樓羅使者。",
        "from": "T21n1277_001:0329b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能成辦世間所求事要。",
        "from": "T21n1277_001:0329b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且不能速疾。",
        "from": "T21n1277_001:0329b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願大天為未來有情。",
        "from": "T21n1277_001:0329b13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說速疾立驗阿尾奢法。",
        "from": "T21n1277_001:0329b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時魔醯首羅告那羅延言。",
        "from": "T21n1277_001:0329b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當諦聽。",
        "from": "T21n1277_001:0329b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為汝宣說速疾成辦使者之法。",
        "from": "T21n1277_001:0329b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能作息災增益降伏敬愛。",
        "from": "T21n1277_001:0329b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能於夜摩界往來使役。",
        "from": "T21n1277_001:0329b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能知未來善惡吉凶成敗。",
        "from": "T21n1277_001:0329b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旱澇不調隣國侵擾。",
        "from": "T21n1277_001:0329b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡人叛亂種種災祥。",
        "from": "T21n1277_001:0329b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲知未來事者。",
        "from": "T21n1277_001:0329b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當簡擇四五童男或童女。",
        "from": "T21n1277_001:0329b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可年七八歲。",
        "from": "T21n1277_001:0329b20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身上無瘢痕𭼩記。",
        "from": "T21n1277_001:0329b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聰慧靈利。",
        "from": "T21n1277_001:0329b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先令一七日服素食或三日食。",
        "from": "T21n1277_001:0329b21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡欲作法。",
        "from": "T21n1277_001:0329b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要須吉日或鬼宿或歲宿直。",
        "from": "T21n1277_001:0329b22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甘露直直日最勝。",
        "from": "T21n1277_001:0329b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沐浴遍身塗香。",
        "from": "T21n1277_001:0329b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著淨衣口含龍腦豆䓻。",
        "from": "T21n1277_001:0329b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持誦者面向東坐。",
        "from": "T21n1277_001:0329b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身前以白檀香塗一小壇。",
        "from": "T21n1277_001:0329b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可一肘量。",
        "from": "T21n1277_001:0329b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令童女等立於壇上。",
        "from": "T21n1277_001:0329b25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散花於童女前。",
        "from": "T21n1277_001:0329b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置一閼伽爐取安息香。",
        "from": "T21n1277_001:0329b26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大印真言加持七遍。",
        "from": "T21n1277_001:0329b27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒令童女熏手。",
        "from": "T21n1277_001:0329b27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又取赤花加持七遍。",
        "from": "T21n1277_001:0329b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安童女掌中。",
        "from": "T21n1277_001:0329b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便以手掩面。",
        "from": "T21n1277_001:0329b28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則持誦者結大印。",
        "from": "T21n1277_001:0329b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手合掌外相交。",
        "from": "T21n1277_001:0329b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左押右虛其掌即成。",
        "from": "T21n1277_001:0329c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此印加持自身五處。",
        "from": "T21n1277_001:0329c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂額右肩左肩心喉。",
        "from": "T21n1277_001:0329c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂上散印即誦真言曰。",
        "from": "T21n1277_001:0329c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨婆誐嚩底摩賀引母捺㘈二合迦吒攞施棄底哩二合施㘈底哩二合路左儞伊捨引儞跛輸跛帝娑嚩二合引賀引則以此印按其童女頂。",
        "from": "T21n1277_001:0329c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則想於頭上。",
        "from": "T21n1277_001:0329c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三角赤色熾盛火輪。",
        "from": "T21n1277_001:0329c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光誦真言七遍。",
        "from": "T21n1277_001:0329c07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火輪真言曰。",
        "from": "T21n1277_001:0329c07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿儗儞二合施棄娑嚩二合引賀則以此印按童女口上。",
        "from": "T21n1277_001:0329c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彼口中想水輪。",
        "from": "T21n1277_001:0329c09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白色半月形。",
        "from": "T21n1277_001:0329c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言七遍。",
        "from": "T21n1277_001:0329c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1277_001:0329c10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵惹攞祖哩二合拏引摩抳娑嚩二合引賀次應移印按彼心中。",
        "from": "T21n1277_001:0329c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想地輪形方黃色。",
        "from": "T21n1277_001:0329c12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦七遍真言曰。",
        "from": "T21n1277_001:0329c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵摩賀麼攞跛囉訖囉二合摩娑嚩二合引賀次應移印按彼臍中。",
        "from": "T21n1277_001:0329c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想風輪其形圓黑色。",
        "from": "T21n1277_001:0329c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦七遍真言曰。",
        "from": "T21n1277_001:0329c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵尾曩多引句攞曩那娑嚩二合引賀引次應以大印加持彼兩脚。",
        "from": "T21n1277_001:0329c19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想迦樓羅誦真言曰。",
        "from": "T21n1277_001:0329c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵跛乞史二合囉引惹跛那娑嚩二合引賀引次應以大印誦甲冑真言。",
        "from": "T21n1277_001:0329c22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持童女遍身旋轉。",
        "from": "T21n1277_001:0330a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1277_001:0330a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵迦嚩左摩部引多引地跛帝娑嚩二合引賀行者次應自身為魔醯首羅天。",
        "from": "T21n1277_001:0330a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三目頭冠瓔珞莊嚴。",
        "from": "T21n1277_001:0330a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭冠上有佛半月。",
        "from": "T21n1277_001:0330a05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "項上青。",
        "from": "T21n1277_001:0330a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十八臂。",
        "from": "T21n1277_001:0330a05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手持種種器仗。",
        "from": "T21n1277_001:0330a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以龍為紳線。",
        "from": "T21n1277_001:0330a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "角絡繫。",
        "from": "T21n1277_001:0330a06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又彼塗血寫及。",
        "from": "T21n1277_001:0330a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須臾頃觀自身已。",
        "from": "T21n1277_001:0330a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應以大印護彼童女一百八命節。",
        "from": "T21n1277_001:0330a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1277_001:0330a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一蜜哩二合體尾野二合吒多二惹縛引揄囉迦苫結其大印及誦真言。",
        "from": "T21n1277_001:0330a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍身旋繞加持。",
        "from": "T21n1277_001:0330a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則護一百八種命節。",
        "from": "T21n1277_001:0330a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次又以大印。",
        "from": "T21n1277_001:0330a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言加持花香及閼伽等。",
        "from": "T21n1277_001:0330a13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次又以大印真言結十方界。",
        "from": "T21n1277_001:0330a14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則應對此童女前。",
        "from": "T21n1277_001:0330a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦魔醯首羅使者真言曰。",
        "from": "T21n1277_001:0330a15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唎躋吒迦一麼多那引誐二尾灑娜跛拏三薩摩那誐囉四尾訖囉二合摩五尾羅引薩誐底六𠼝濟吒迦七布囉二合哩跛引擔囕二合八左羅左羅九左里左里十跛拏跛拏十一跛尼跛尼十二伴上爾伴爾十三羯恥羯恥十四阿尾捨阿尾捨十五𠼝躋吒迦十六嚕奴噌二合抧孃二合跋野底娑嚩二合引賀十七引此真言應誦七遍。",
        "from": "T21n1277_001:0330a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則彼童女戰動。",
        "from": "T21n1277_001:0330a23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知聖者入身。",
        "from": "T21n1277_001:0330a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則更彈指誦真言。",
        "from": "T21n1277_001:0330a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無現驗。",
        "from": "T21n1277_001:0330a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次誦摧迫使者真言曰。",
        "from": "T21n1277_001:0330a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "𠼝𠼝耶一摩魯呬多二引素囉素囉三引布爾多賀那賀那四波囉二合憾麼二合引那尼五那覩嚕二合尼都嚕二合尼六謨尼謨尼七伴上爾伴上爾八羯恥羯恥九阿尾捨阿尾捨十𠼝躋吒迦十一嚕奴噌二合枳孃二合跛野帝十二娑嚩二合引訶十三引誦此真言必速應驗。",
        "from": "T21n1277_001:0330b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問未來善惡一切災祥。",
        "from": "T21n1277_001:0330b03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不語或語遲。",
        "from": "T21n1277_001:0330b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則結棒印。",
        "from": "T21n1277_001:0330b04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手合掌。",
        "from": "T21n1277_001:0330b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二無名指外支。",
        "from": "T21n1277_001:0330b05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指並立。",
        "from": "T21n1277_001:0330b05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指各鉤無名指頭。",
        "from": "T21n1277_001:0330b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指各令押中交。",
        "from": "T21n1277_001:0330b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言曰。",
        "from": "T21n1277_001:0330b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵一母那誐二合囉二都嚕都嚕三娑嚩二合引賀引四結此棒印。",
        "from": "T21n1277_001:0330b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則語問種種事已。",
        "from": "T21n1277_001:0330b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大印真言加持閼伽。",
        "from": "T21n1277_001:0330b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三灑童女面。",
        "from": "T21n1277_001:0330b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結解。",
        "from": "T21n1277_001:0330b11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此使者真言先應誦一萬遍則法成。",
        "from": "T21n1277_001:0330b12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則見身來。",
        "from": "T21n1277_001:0330b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須獻閼伽乞願。",
        "from": "T21n1277_001:0330b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願聖者一切處一切時使用皆辦。",
        "from": "T21n1277_001:0330b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則隱不現。",
        "from": "T21n1277_001:0330b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已後欲使。",
        "from": "T21n1277_001:0330b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗一小壇著香花飲食。",
        "from": "T21n1277_001:0330b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言一百八遍。",
        "from": "T21n1277_001:0330b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則現身。",
        "from": "T21n1277_001:0330b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則言龍宮中取長年藥如意寶珠。",
        "from": "T21n1277_001:0330b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或使夜摩王處。",
        "from": "T21n1277_001:0330b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "延命增益壽命。",
        "from": "T21n1277_001:0330b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或使上取妙甘露。",
        "from": "T21n1277_001:0330b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或使他國問其善惡。",
        "from": "T21n1277_001:0330b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能助軍陣摧破他敵。",
        "from": "T21n1277_001:0330b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種使用悉能成辦。",
        "from": "T21n1277_001:0330b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此法一切迦樓羅法中最殊勝。",
        "from": "T21n1277_001:0330b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祕密難得。",
        "from": "T21n1277_001:0330b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當揀擇法器堪傳授者而傳與。",
        "from": "T21n1277_001:0330b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非人器人即損他。",
        "from": "T21n1277_001:0330b20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已後此法不成。",
        "from": "T21n1277_001:0330b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故應極祕密勿妄傳受之。",
        "from": "T21n1277_001:0330b21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大自在天迦樓羅陀羅尼曰。",
        "from": "T21n1277_001:0330b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那謨婆伽嚩底一嚕馱囉野二瞋那劫波囉野三薩嚩微那延迦囉野四薩嚩羯摩莎馱那野五薩嚩嚩尸迦羅拏野六薩嚩設覩嚕二合尾那捨那野七唵八迦波羅質擔瞋那迦波羅部擔九嚕訥嚕二合枳孃二合跛野帝十娑嚩二合引賀此陀羅尼。",
        "from": "T21n1277_001:0330b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "調伏人取赤芭蕉葉畫彼人。",
        "from": "T21n1277_001:0330b29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心上書名字。",
        "from": "T21n1277_001:0330c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦陀羅尼一百八遍。",
        "from": "T21n1277_001:0330c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即埋牛糞中即調伏。",
        "from": "T21n1277_001:0330c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲令竪子對相憎。",
        "from": "T21n1277_001:0330c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多羅葉畫彼男女形。",
        "from": "T21n1277_001:0330c02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書名相背。",
        "from": "T21n1277_001:0330c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取線纏。",
        "from": "T21n1277_001:0330c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取鼠狼毛山鷄毛蛇退皮燒熏。",
        "from": "T21n1277_001:0330c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦陀羅尼一百八遍。",
        "from": "T21n1277_001:0330c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安劫波羅中。",
        "from": "T21n1277_001:0330c04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於屍林中埋著。",
        "from": "T21n1277_001:0330c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便相憎。",
        "from": "T21n1277_001:0330c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除却依舊。",
        "from": "T21n1277_001:0330c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若令相憐者。",
        "from": "T21n1277_001:0330c05_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還於多羅葉上畫彼夫形。",
        "from": "T21n1277_001:0330c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書名相合。",
        "from": "T21n1277_001:0330c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取白線繫。",
        "from": "T21n1277_001:0330c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取雀兒毛蛇皮燒熏。",
        "from": "T21n1277_001:0330c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦陀羅尼一百八遍。",
        "from": "T21n1277_001:0330c07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即相憐。",
        "from": "T21n1277_001:0330c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法欲令人相打。",
        "from": "T21n1277_001:0330c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於大虫皮或於牛皮。",
        "from": "T21n1277_001:0330c09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫二人相把頭髻。",
        "from": "T21n1277_001:0330c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書名著。",
        "from": "T21n1277_001:0330c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取線纏著。",
        "from": "T21n1277_001:0330c10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於火上重。",
        "from": "T21n1277_001:0330c10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "埋碓臼下。",
        "from": "T21n1277_001:0330c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得日日相打。",
        "from": "T21n1277_001:0330c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除劫止。",
        "from": "T21n1277_001:0330c11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲調伏人者。",
        "from": "T21n1277_001:0330c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取貝多葉上畫人形。",
        "from": "T21n1277_001:0330c12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書名誦陀羅尼一百八遍。",
        "from": "T21n1277_001:0330c13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於床下埋。",
        "from": "T21n1277_001:0330c13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得調伏。",
        "from": "T21n1277_001:0330c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法令尿血者。",
        "from": "T21n1277_001:0330c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取多羅葉畫彼人。",
        "from": "T21n1277_001:0330c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書姓名。",
        "from": "T21n1277_001:0330c15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取釘誦陀羅尼一百八遍。",
        "from": "T21n1277_001:0330c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釘上七七日。",
        "from": "T21n1277_001:0330c16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得除去。",
        "from": "T21n1277_001:0330c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說眼藥法。",
        "from": "T21n1277_001:0330c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若調伏竪子者。",
        "from": "T21n1277_001:0330c18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取蛇頭竭羅安善那青木香寫甲蜂二介。",
        "from": "T21n1277_001:0330c19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於黑月十四日擣作抹。",
        "from": "T21n1277_001:0330c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和肉點眼角。",
        "from": "T21n1277_001:0330c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切竪子隨逐。",
        "from": "T21n1277_001:0330c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天上亦來非但人間也。",
        "from": "T21n1277_001:0330c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速疾立驗魔醯首羅天說迦婁羅阿尾奢法一卷",
        "from": "T21n1277_001:0331a02_100",
        "context_with_next_sentence": false
    }
]