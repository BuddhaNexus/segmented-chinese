    },
    {
        "source_sentence": "李卓吾。",
        "translation": "LEE Cheuk-go.",
        "source": "X88n1646_043:0260a04_0",
        "is_bad_quality": false
    },
    {
        "source_sentence": "名贄。",
        "translation": "His name was Leyuan.",
