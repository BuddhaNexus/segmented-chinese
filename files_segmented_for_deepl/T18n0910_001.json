[
    {
        "source_sentence": "梵天擇地法",
        "from": "T18n0910_001:0924c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謹案大梵天王內祕密經云。",
        "from": "T18n0910_001:0924c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來因地時五百萬生中。",
        "from": "T18n0910_001:0924c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作持呪仙人。",
        "from": "T18n0910_001:0924c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每遊歷諸山隱居淨室。",
        "from": "T18n0910_001:0924c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常感諸梵天而來侍衛。",
        "from": "T18n0910_001:0924c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有天龍八部而來現身。",
        "from": "T18n0910_001:0924c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖有善相仍被魔惱法事不成。",
        "from": "T18n0910_001:0924c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或為誦呪依不梵回。",
        "from": "T18n0910_001:0924c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或為姥陀法行。",
        "from": "T18n0910_001:0924c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或為曼荼羅不得其地。",
        "from": "T18n0910_001:0924c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此法多不成。",
        "from": "T18n0910_001:0924c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今案經有四十二種擇地法。",
        "from": "T18n0910_001:0924c20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堪作曼荼羅。",
        "from": "T18n0910_001:0924c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使持呪者如意法成。",
        "from": "T18n0910_001:0924c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得此法者徒消日月。",
        "from": "T18n0910_001:0924c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一入山石窟中深。",
        "from": "T18n0910_001:0924c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四面有石中心有土。",
        "from": "T18n0910_001:0924c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如人掃地處。",
        "from": "T18n0910_001:0924c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是聖人成道之處。",
        "from": "T18n0910_001:0924c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地中心有異花出。",
        "from": "T18n0910_001:0924c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其地第一。",
        "from": "T18n0910_001:0924c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此作壇持呪。",
        "from": "T18n0910_001:0924c25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定得感諸如來。",
        "from": "T18n0910_001:0924c25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應持戒比丘得此地法。",
        "from": "T18n0910_001:0924c26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非持戒不得於此地慎之。",
        "from": "T18n0910_001:0924c26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二深山有大蟲及師子猛獸坐處。",
        "from": "T18n0910_001:0924c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其地無草生處。",
        "from": "T18n0910_001:0924c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必是聖人坐處。",
        "from": "T18n0910_001:0924c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸獸並是護塔之神。",
        "from": "T18n0910_001:0924c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此地作壇法事立成。",
        "from": "T18n0910_001:0925a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貧道開黃五年入廣州深山有此地。",
        "from": "T18n0910_001:0925a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前法受持。",
        "from": "T18n0910_001:0925a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃有一人。",
        "from": "T18n0910_001:0925a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身作金色著白衣。",
        "from": "T18n0910_001:0925a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大神身長三丈已來。",
        "from": "T18n0910_001:0925a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貧道驚怕其人報云。",
        "from": "T18n0910_001:0925a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾是大佛頂之神。",
        "from": "T18n0910_001:0925a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今見汝坐福地。",
        "from": "T18n0910_001:0925a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "感吾身現來受汝法。",
        "from": "T18n0910_001:0925a05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三入山高頂上。",
        "from": "T18n0910_001:0925a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見有大石其清如磨處。",
        "from": "T18n0910_001:0925a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見人影現。",
        "from": "T18n0910_001:0925a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此石聖人吉祥之石。",
        "from": "T18n0910_001:0925a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取白茅為席。",
        "from": "T18n0910_001:0925a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐西北角誦呪作法。",
        "from": "T18n0910_001:0925a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剋成就。",
        "from": "T18n0910_001:0925a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非持戒比丘不得坐。",
        "from": "T18n0910_001:0925a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四山中四面有草木樹林。",
        "from": "T18n0910_001:0925a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中心無物。",
        "from": "T18n0910_001:0925a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨如人掃地。",
        "from": "T18n0910_001:0925a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作壇必成就。",
        "from": "T18n0910_001:0925a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五山中見有五色石。",
        "from": "T18n0910_001:0925a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "青黃赤白黑各在本方。",
        "from": "T18n0910_001:0925a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是聖人學道處。",
        "from": "T18n0910_001:0925a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此地必是勝。",
        "from": "T18n0910_001:0925a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作壇四天王自至。",
        "from": "T18n0910_001:0925a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六入山中見蛇頭上有角盤石。",
        "from": "T18n0910_001:0925a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蛇臥處無草。",
        "from": "T18n0910_001:0925a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見人來即起去。",
        "from": "T18n0910_001:0925a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此作壇最妙。",
        "from": "T18n0910_001:0925a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第七山中見有白鹿臥在地。",
        "from": "T18n0910_001:0925a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四面一丈已來無草木。",
        "from": "T18n0910_001:0925a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此地亦堪作壇。",
        "from": "T18n0910_001:0925a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八山中見有白鹿。",
        "from": "T18n0910_001:0925a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臥處四面無草木。",
        "from": "T18n0910_001:0925a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見此相貌並是作壇之地。",
        "from": "T18n0910_001:0925a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第九山中有人身長一丈餘。",
        "from": "T18n0910_001:0925a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著黑色衣面有四目。",
        "from": "T18n0910_001:0925a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐於石上。",
        "from": "T18n0910_001:0925a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其石上清明見人面像。",
        "from": "T18n0910_001:0925a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比丘上必成。",
        "from": "T18n0910_001:0925a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十山中有白頭老翁。",
        "from": "T18n0910_001:0925a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眉鬢皆白。",
        "from": "T18n0910_001:0925a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目送其行處。",
        "from": "T18n0910_001:0925a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地皆不生草木。",
        "from": "T18n0910_001:0925a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此地堪作壇。",
        "from": "T18n0910_001:0925a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有草木不堪。",
        "from": "T18n0910_001:0925a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十一山中見有石。",
        "from": "T18n0910_001:0925a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或土裏有五色雲出。",
        "from": "T18n0910_001:0925a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非陰雨時見。",
        "from": "T18n0910_001:0925a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此處必是聖人坐處。",
        "from": "T18n0910_001:0925a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作壇妙。",
        "from": "T18n0910_001:0925a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十二山中有石室內。",
        "from": "T18n0910_001:0925a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師子及猛獸居。",
        "from": "T18n0910_001:0925a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無草木生者。",
        "from": "T18n0910_001:0925a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此地堪作壇。",
        "from": "T18n0910_001:0925a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十三山中有平處無草。",
        "from": "T18n0910_001:0925b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其地有人脚跡一尺已來。",
        "from": "T18n0910_001:0925b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堪作壇。",
        "from": "T18n0910_001:0925b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十四入山谷。",
        "from": "T18n0910_001:0925b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見有草在地生。",
        "from": "T18n0910_001:0925b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每似人坐上。",
        "from": "T18n0910_001:0925b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其草頭低屈者。",
        "from": "T18n0910_001:0925b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其堪。",
        "from": "T18n0910_001:0925b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十五入山頂上及平下。",
        "from": "T18n0910_001:0925b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見異花出於四面。",
        "from": "T18n0910_001:0925b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中心可有八尺已來空地。",
        "from": "T18n0910_001:0925b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其處妙。",
        "from": "T18n0910_001:0925b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十六山中見有花出處。",
        "from": "T18n0910_001:0925b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但是聖人坐處。",
        "from": "T18n0910_001:0925b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無間春夏秋冬。",
        "from": "T18n0910_001:0925b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆有異花出現。",
        "from": "T18n0910_001:0925b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但有出華異於眾花。",
        "from": "T18n0910_001:0925b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其地妙。",
        "from": "T18n0910_001:0925b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十七山中持呪。",
        "from": "T18n0910_001:0925b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見地上有白光出。",
        "from": "T18n0910_001:0925b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四面無草。",
        "from": "T18n0910_001:0925b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此地妙。",
        "from": "T18n0910_001:0925b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十八山中非時見有香氣。",
        "from": "T18n0910_001:0925b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地亦似有煙出。",
        "from": "T18n0910_001:0925b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此地妙。",
        "from": "T18n0910_001:0925b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十九山中有五色光。",
        "from": "T18n0910_001:0925b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從地出或從石出。",
        "from": "T18n0910_001:0925b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "視其處。",
        "from": "T18n0910_001:0925b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出處妙。",
        "from": "T18n0910_001:0925b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二十山中見有孔雀鳥或諸鳥。",
        "from": "T18n0910_001:0925b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[銜金+缶]草及雜花來投著。",
        "from": "T18n0910_001:0925b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其地堪。",
        "from": "T18n0910_001:0925b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是聖人所出處妙。",
        "from": "T18n0910_001:0925b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二十一山中有五色土處。",
        "from": "T18n0910_001:0925b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至明日平旦。",
        "from": "T18n0910_001:0925b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "看五色光出。",
        "from": "T18n0910_001:0925b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此地堪。",
        "from": "T18n0910_001:0925b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二十二山中見有白鶴四箇。",
        "from": "T18n0910_001:0925b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對坐處無草。",
        "from": "T18n0910_001:0925b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是結壇地。",
        "from": "T18n0910_001:0925b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二十三山中見雙樹相對。",
        "from": "T18n0910_001:0925b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中間有狀如人形。",
        "from": "T18n0910_001:0925b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其地堪。",
        "from": "T18n0910_001:0925b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二十四山中見黃牛或白牛在地臥。",
        "from": "T18n0910_001:0925b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其地無草生似人掃處。",
        "from": "T18n0910_001:0925b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其地堪。",
        "from": "T18n0910_001:0925b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二十五山中有石打作鐘聲。",
        "from": "T18n0910_001:0925b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其處堪。",
        "from": "T18n0910_001:0925b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二十六山見有窟。",
        "from": "T18n0910_001:0925b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中相若喚無響者妙。",
        "from": "T18n0910_001:0925b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即有響者非。",
        "from": "T18n0910_001:0925b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二十七山中有寺觀。",
        "from": "T18n0910_001:0925b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其地平旦。",
        "from": "T18n0910_001:0925b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日未出時看五色光者堪。",
        "from": "T18n0910_001:0925b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二十八山中見有神人著天衣。",
        "from": "T18n0910_001:0925c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當所出處堪。",
        "from": "T18n0910_001:0925c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二十九山中有土。",
        "from": "T18n0910_001:0925c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如蟻子聚高五尺以來。",
        "from": "T18n0910_001:0925c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其堪。",
        "from": "T18n0910_001:0925c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三十山中見石或土如龍形狀。",
        "from": "T18n0910_001:0925c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或地上似如畫處。",
        "from": "T18n0910_001:0925c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堪。",
        "from": "T18n0910_001:0925c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三十一山中見有仙人或帶角之人獸。",
        "from": "T18n0910_001:0925c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "視其所出之地堪。",
        "from": "T18n0910_001:0925c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三十二非時非處。",
        "from": "T18n0910_001:0925c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見有光忽然出。",
        "from": "T18n0910_001:0925c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有犯其火。",
        "from": "T18n0910_001:0925c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有香烟。",
        "from": "T18n0910_001:0925c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是羅漢得道之地堪。",
        "from": "T18n0910_001:0925c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三十三若入寺舍。",
        "from": "T18n0910_001:0925c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及在俗人家作壇。",
        "from": "T18n0910_001:0925c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百倍千倍不及山中。",
        "from": "T18n0910_001:0925c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲救眾生終須擇地。",
        "from": "T18n0910_001:0925c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寺上伽藍穿地深二尺。",
        "from": "T18n0910_001:0925c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其內得骨者必不堪。",
        "from": "T18n0910_001:0925c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得瓦石者仍。",
        "from": "T18n0910_001:0925c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "埋七寶於地內。",
        "from": "T18n0910_001:0925c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然始得用。",
        "from": "T18n0910_001:0925c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三十四穿地深三尺。",
        "from": "T18n0910_001:0925c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得少許異物或寶。",
        "from": "T18n0910_001:0925c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即名上地堪。",
        "from": "T18n0910_001:0925c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三十五穿地深二尺。",
        "from": "T18n0910_001:0925c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地內無物者作壇。",
        "from": "T18n0910_001:0925c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其地內所取之土。",
        "from": "T18n0910_001:0925c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨擇去。",
        "from": "T18n0910_001:0925c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和香末實築。",
        "from": "T18n0910_001:0925c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三十六地內不曾有人居止眠臥處。",
        "from": "T18n0910_001:0925c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此地堪。",
        "from": "T18n0910_001:0925c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三十七地內未曾安窟窖竈。",
        "from": "T18n0910_001:0925c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及雞犬臥處堪。",
        "from": "T18n0910_001:0925c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三十八五更看地內氣候。",
        "from": "T18n0910_001:0925c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有黑光不堪。",
        "from": "T18n0910_001:0925c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自外光出者堪。",
        "from": "T18n0910_001:0925c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三十九若在俗家作壇。",
        "from": "T18n0910_001:0925c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多不如法致有損傷。",
        "from": "T18n0910_001:0925c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪師彼此皆無利益。",
        "from": "T18n0910_001:0925c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直須結界不須作壇。",
        "from": "T18n0910_001:0925c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貧道驗之曾有損失。",
        "from": "T18n0910_001:0925c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四十若是須結壇。",
        "from": "T18n0910_001:0925c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令病人清淨。",
        "from": "T18n0910_001:0925c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每日澡浴著新淨衣。",
        "from": "T18n0910_001:0925c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別在一室內坐。",
        "from": "T18n0910_001:0926a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪師依前擇地作壇。",
        "from": "T18n0910_001:0926a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "專心稱名為誦法。",
        "from": "T18n0910_001:0926a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "病人不令壇內得見。",
        "from": "T18n0910_001:0926a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若將病人於壇內。",
        "from": "T18n0910_001:0926a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並損呪師。",
        "from": "T18n0910_001:0926a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四十一若是轉持戒比丘誦呪作壇。",
        "from": "T18n0910_001:0926a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百千勝於俗人。",
        "from": "T18n0910_001:0926a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若擇地得一作壇之後。",
        "from": "T18n0910_001:0926a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其地劫災起時其地不壞。",
        "from": "T18n0910_001:0926a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四十二若作水壇。",
        "from": "T18n0910_001:0926a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不須作蕳擇得上勝地。",
        "from": "T18n0910_001:0926a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不得其地終無成益。",
        "from": "T18n0910_001:0926a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵天擇地法終",
        "from": "T18n0910_001:0926a09_0",
        "context_with_next_sentence": false
    }
]