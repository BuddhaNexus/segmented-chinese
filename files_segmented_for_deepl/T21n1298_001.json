[
    {
        "source_sentence": "十二天供儀軌(合兩所文為軌已出護摩軌)於道場八方敷茅草。",
        "from": "T21n1298_001:0385b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或蓮荷葉或餘青草。",
        "from": "T21n1298_001:0385b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或塗圓壇合為十位。",
        "from": "T21n1298_001:0385b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於帝釋左右置梵天地天位。",
        "from": "T21n1298_001:0385b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與八方而十。",
        "from": "T21n1298_001:0385b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若道場內無置位處。",
        "from": "T21n1298_001:0385b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於道場前用淨處別為方界。",
        "from": "T21n1298_001:0385b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中布八方。",
        "from": "T21n1298_001:0385b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中央作兩位。",
        "from": "T21n1298_001:0385b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置梵天地天。",
        "from": "T21n1298_001:0385b12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以施十方天。",
        "from": "T21n1298_001:0385b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食應用雜粥。",
        "from": "T21n1298_001:0385b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂梗米油麻菉豆相和煮。",
        "from": "T21n1298_001:0385b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令極清淨。",
        "from": "T21n1298_001:0385b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香義盛器中。",
        "from": "T21n1298_001:0385b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每座先置一淨葉。",
        "from": "T21n1298_001:0385b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "巡環卒置葉上。",
        "from": "T21n1298_001:0385b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以淨瓶盛水。",
        "from": "T21n1298_001:0385b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即寫少香水於葉上以獻。",
        "from": "T21n1298_001:0385b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以右手中無名二指彈少塗香。",
        "from": "T21n1298_001:0385b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次獻一花置之於座次獻燒香。",
        "from": "T21n1298_001:0385b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次爐梵香。",
        "from": "T21n1298_001:0385b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於座前獻諸座。",
        "from": "T21n1298_001:0385b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同此一爐。",
        "from": "T21n1298_001:0385b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次卒一杓粥置葉上以獻。",
        "from": "T21n1298_001:0385b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次用小蠟燭或紙燭以獻。",
        "from": "T21n1298_001:0385b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便插粥上。",
        "from": "T21n1298_001:0385b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從香水至燭。",
        "from": "T21n1298_001:0385b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各以本真言加持三遍。",
        "from": "T21n1298_001:0385b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每位從水至燭獻畢。",
        "from": "T21n1298_001:0385b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然向。",
        "from": "T21n1298_001:0385b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次其燭作意獻諸位。",
        "from": "T21n1298_001:0385b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未遍已來不用令滅。",
        "from": "T21n1298_001:0385b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須助伴或驅使數人。",
        "from": "T21n1298_001:0385b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各執一物以供事。",
        "from": "T21n1298_001:0385b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一一自取即燭必不終事。",
        "from": "T21n1298_001:0385b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每位於薩嚩訶上加所求願語　東方帝釋乘白象王。",
        "from": "T21n1298_001:0385b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住五色雲中。",
        "from": "T21n1298_001:0385b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身作金色。",
        "from": "T21n1298_001:0385b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持三股當心。",
        "from": "T21n1298_001:0385b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手托左胯。",
        "from": "T21n1298_001:0385b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左脚垂下。",
        "from": "T21n1298_001:0385b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三天女各手持蓮花。",
        "from": "T21n1298_001:0385b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或以盤盛雜花等。",
        "from": "T21n1298_001:0385b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盤盛青蓮花。",
        "from": "T21n1298_001:0385b27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印相者右手作拳安腰。",
        "from": "T21n1298_001:0385b27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手五指直竪相著。",
        "from": "T21n1298_001:0385b28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地水二指屈中節。",
        "from": "T21n1298_001:0385b28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以風著火背。",
        "from": "T21n1298_001:0385b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空屈中節。",
        "from": "T21n1298_001:0385b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰　曩莫三曼多沒馱南因捺羅耶娑嚩(二合)賀　東南方火天乘青羊。",
        "from": "T21n1298_001:0385b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "赤肉色遍身火焰。",
        "from": "T21n1298_001:0385c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右二手一持青竹一持軍持。",
        "from": "T21n1298_001:0385c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左二手一楊掌一持念珠。",
        "from": "T21n1298_001:0385c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有二天女持天花。",
        "from": "T21n1298_001:0385c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左右置苦行仙。",
        "from": "T21n1298_001:0385c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "垂左脚蹉右足。",
        "from": "T21n1298_001:0385c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印相者左手作拳安腰左。",
        "from": "T21n1298_001:0385c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手五指直竪相著。",
        "from": "T21n1298_001:0385c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈空納著掌中。",
        "from": "T21n1298_001:0385c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風屈中節。",
        "from": "T21n1298_001:0385c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與娑婆訶相招。",
        "from": "T21n1298_001:0385c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1298_001:0385c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三滿多沒馱南．阿哦那(二合)曳娑嚩(二合引)訶(引)南方焰摩天乘水牛。",
        "from": "T21n1298_001:0385c07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手執人頭幢左手仰掌。",
        "from": "T21n1298_001:0385c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有二天女侍。",
        "from": "T21n1298_001:0385c09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二鬼使者持刀捧戟。",
        "from": "T21n1298_001:0385c09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "赤黑色垂右脚。",
        "from": "T21n1298_001:0385c10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印相者先合掌。",
        "from": "T21n1298_001:0385c10_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二地屈中節背相著。",
        "from": "T21n1298_001:0385c11_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二風亦屈背著。",
        "from": "T21n1298_001:0385c11_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大指各押風中節即成。",
        "from": "T21n1298_001:0385c11_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1298_001:0385c12_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三滿多沒馱南焰摩耶娑嚩(二合)訶西南方羅剎主天乘白師子。",
        "from": "T21n1298_001:0385c12_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身著甲冑。",
        "from": "T21n1298_001:0385c14_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持刀令竪。",
        "from": "T21n1298_001:0385c14_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手大指押中小二指。",
        "from": "T21n1298_001:0385c14_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "赤肉色。",
        "from": "T21n1298_001:0385c15_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二天女侍左右。",
        "from": "T21n1298_001:0385c15_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二羅剎鬼。",
        "from": "T21n1298_001:0385c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持三股戟。",
        "from": "T21n1298_001:0385c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印相者右手作拳安腰右。",
        "from": "T21n1298_001:0385c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手五指竪相著屈地水中節。",
        "from": "T21n1298_001:0385c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以空端押地水甲即成。",
        "from": "T21n1298_001:0385c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1298_001:0385c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三滿多沒馱南乃哩底曳(二合)娑嚩(二合)賀西方水天住於水中乘龜。",
        "from": "T21n1298_001:0385c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淺綠色。",
        "from": "T21n1298_001:0385c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手取刀。",
        "from": "T21n1298_001:0385c20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持龍索。",
        "from": "T21n1298_001:0385c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冠上有五龍。",
        "from": "T21n1298_001:0385c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四天女持妙花。",
        "from": "T21n1298_001:0385c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印相者右手如前。",
        "from": "T21n1298_001:0385c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手握拳。",
        "from": "T21n1298_001:0385c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空勿入掌中。",
        "from": "T21n1298_001:0385c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風即直竪。",
        "from": "T21n1298_001:0385c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中節少屈即成。",
        "from": "T21n1298_001:0385c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1298_001:0385c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三滿多沒馱南嚩嚕拏野娑嚩(二合)訶西北方風天雲中乘麞著甲冑。",
        "from": "T21n1298_001:0385c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手托胯。",
        "from": "T21n1298_001:0385c25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手執獨股頭創。",
        "from": "T21n1298_001:0385c25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "創上有緋幡。",
        "from": "T21n1298_001:0385c25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二天女侍之。",
        "from": "T21n1298_001:0385c26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并藥叉眾印相者右手如前。",
        "from": "T21n1298_001:0385c26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手五指直竪相著。",
        "from": "T21n1298_001:0385c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地水屈中節即成。",
        "from": "T21n1298_001:0385c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1298_001:0385c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三滿多沒馱南嚩耶吠娑嚩(二合)訶北方毘沙門天坐二鬼上。",
        "from": "T21n1298_001:0385c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身著甲冑。",
        "from": "T21n1298_001:0385c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手掌捧塔。",
        "from": "T21n1298_001:0385c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手執寶捧。",
        "from": "T21n1298_001:0386a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身金色。",
        "from": "T21n1298_001:0386a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二天女持寶花等。",
        "from": "T21n1298_001:0386a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印相者先合掌。",
        "from": "T21n1298_001:0386a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十指內相叉。",
        "from": "T21n1298_001:0386a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二水竪頭相跓。",
        "from": "T21n1298_001:0386a02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二風竪微屈。",
        "from": "T21n1298_001:0386a03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左空入右掌中。",
        "from": "T21n1298_001:0386a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "押左火甲。",
        "from": "T21n1298_001:0386a03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右空越左空背入左掌中。",
        "from": "T21n1298_001:0386a04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "押右火甲。",
        "from": "T21n1298_001:0386a04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二風與訶相招。",
        "from": "T21n1298_001:0386a04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1298_001:0386a05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三滿多沒馱南吠室囉(二合)嚩拏野娑嚩訶東北方伊舍那天舊云魔醯首羅天。",
        "from": "T21n1298_001:0386a05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦云大自在天。",
        "from": "T21n1298_001:0386a07_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘黃豐牛。",
        "from": "T21n1298_001:0386a07_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持劫波杯盛血。",
        "from": "T21n1298_001:0386a08_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持三戟創。",
        "from": "T21n1298_001:0386a08_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淺青肉色三目忿怒。",
        "from": "T21n1298_001:0386a08_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二牙上出。",
        "from": "T21n1298_001:0386a09_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髑髏為瓔珞。",
        "from": "T21n1298_001:0386a09_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭冠中有二仰月。",
        "from": "T21n1298_001:0386a09_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二天女持花。",
        "from": "T21n1298_001:0386a10_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印相者右手作拳安腰右。",
        "from": "T21n1298_001:0386a10_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手五指直竪相著。",
        "from": "T21n1298_001:0386a11_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地水二指屈中節。",
        "from": "T21n1298_001:0386a11_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火風空三指各少相去即成。",
        "from": "T21n1298_001:0386a12_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1298_001:0386a12_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三漫多馱南伊舍曩曳娑嚩(二合)訶上方梵天印相者右手作拳安右腰。",
        "from": "T21n1298_001:0386a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手五指相著竪之少屈。",
        "from": "T21n1298_001:0386a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其高少許過肩。",
        "from": "T21n1298_001:0386a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成蓮花想。",
        "from": "T21n1298_001:0386a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1298_001:0386a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三滿多沒馱南沒羅(二合)訶摩(二合)寧娑嚩(二合)訶下方地天印相者先合掌。",
        "from": "T21n1298_001:0386a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風以下四指頭相踁。",
        "from": "T21n1298_001:0386a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前方大開。",
        "from": "T21n1298_001:0386a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二空各著風側。",
        "from": "T21n1298_001:0386a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1298_001:0386a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三滿多沒馱南畢哩(二合)體微曳(二合)娑嚩(二合)訶日天印相者不異前地天印。",
        "from": "T21n1298_001:0386a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但以二空頭。",
        "from": "T21n1298_001:0386a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各跓水下節即成。",
        "from": "T21n1298_001:0386a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1298_001:0386a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三滿多沒馱南阿(去)儞底也(二合)娑嚩(二合)訶月天印相者如前梵天印。",
        "from": "T21n1298_001:0386a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但仰掌。",
        "from": "T21n1298_001:0386a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中有月潔白相。",
        "from": "T21n1298_001:0386a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1298_001:0386a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三曼多沒馱南戰捺羅(二合去)也娑嚩(二合)訶七曜印相者先合掌十指相著。",
        "from": "T21n1298_001:0386a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二風與二空極相開。",
        "from": "T21n1298_001:0386a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1298_001:0386a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三曼多沒馱南蘖羅(二合)醯濕嚩(二合)哩耶鉢囉(二合)跛多而偷(二合)底羅摩耶娑嚩(二合)訶二十八宿印相者先虛心合掌。",
        "from": "T21n1298_001:0386a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二火外相叉。",
        "from": "T21n1298_001:0386b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二空亦相叉。",
        "from": "T21n1298_001:0386b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1298_001:0386b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南莫三曼多沒馱南諾乞灑(二合)怛羅(二合)涅蘇那儞曳娑嚩訶於八方中加兩位。",
        "from": "T21n1298_001:0386b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上下天對。",
        "from": "T21n1298_001:0386b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曜東宿西。",
        "from": "T21n1298_001:0386b04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸獻並同。",
        "from": "T21n1298_001:0386b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若別有所願。",
        "from": "T21n1298_001:0386b04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獨用亦得。",
        "from": "T21n1298_001:0386b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若護摩壇中。",
        "from": "T21n1298_001:0386b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各依本方標心令住亦設位(云云)。",
        "from": "T21n1298_001:0386b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壇中心置四臂不動尊。",
        "from": "T21n1298_001:0386b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "青肉色。",
        "from": "T21n1298_001:0386b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手金剛拳。",
        "from": "T21n1298_001:0386b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭指小指各曲如鉤形。",
        "from": "T21n1298_001:0386b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安口兩角。",
        "from": "T21n1298_001:0386b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牙相如牙。",
        "from": "T21n1298_001:0386b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持刀令竪。",
        "from": "T21n1298_001:0386b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左持索半跏右押左。",
        "from": "T21n1298_001:0386b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐盤石上。",
        "from": "T21n1298_001:0386b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "威焰光明遍身如火(云云)。",
        "from": "T21n1298_001:0386b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今作圖位。",
        "from": "T21n1298_001:0386b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但文立圓壇(云云)。",
        "from": "T21n1298_001:0386b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨時可作之耳。",
        "from": "T21n1298_001:0386b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北面而可修之。",
        "from": "T21n1298_001:0386b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或圖　中台五大明王",
        "from": "T21n1298_001:0386c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二天次第以不動五大明王為十二天主之十二天之次可供諸天妃次可供當土諸名神。",
        "from": "T21n1298_001:0386c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求覓洛東智積院動潮僧正之御本。",
        "from": "T21n1298_001:0386c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令寫之而挍。",
        "from": "T21n1298_001:0386c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以上木。",
        "from": "T21n1298_001:0386c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此軌薄草決及金寶集等引以證。",
        "from": "T21n1298_001:0386c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時惟享和元年酉八月",
        "from": "T21n1298_001:0386c17_0",
        "context_with_next_sentence": false
    }
]