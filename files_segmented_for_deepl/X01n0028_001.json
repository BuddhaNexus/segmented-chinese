[
    {
        "source_sentence": "佛說造像量度經",
        "from": "X01n0028_001:0454c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大清內閣掌譯番蒙諸文西番學總管儀賓　工布查布　譯如是我聞。",
        "from": "X01n0028_001:0454c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在舍衛國。",
        "from": "X01n0028_001:0454c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祇樹給孤獨園。",
        "from": "X01n0028_001:0454c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與諸菩薩聲聞弟子。",
        "from": "X01n0028_001:0454c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切人天龍神。",
        "from": "X01n0028_001:0454c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量眷屬大眾俱。",
        "from": "X01n0028_001:0454c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正乃世尊。",
        "from": "X01n0028_001:0454c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因為母說法。",
        "from": "X01n0028_001:0454c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將升忉利天土時也。",
        "from": "X01n0028_001:0454c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時賢者舍利弗。",
        "from": "X01n0028_001:0454c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向佛敬禮而作是言。",
        "from": "X01n0028_001:0454c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊不住斯間。",
        "from": "X01n0028_001:0454c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善人不勝懷慕。",
        "from": "X01n0028_001:0454c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思覩世尊。",
        "from": "X01n0028_001:0454c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願造容像者。",
        "from": "X01n0028_001:0454c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則其法如何為之。",
        "from": "X01n0028_001:0454c14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言。",
        "from": "X01n0028_001:0454c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉舍利弗。",
        "from": "X01n0028_001:0454c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今暫升天土。",
        "from": "X01n0028_001:0454c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未旋斯間。",
        "from": "X01n0028_001:0454c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或示無餘涅槃之後。",
        "from": "X01n0028_001:0454c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善人。",
        "from": "X01n0028_001:0454c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思覩瞻仰。",
        "from": "X01n0028_001:0454c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及為自他利益作福田故。",
        "from": "X01n0028_001:0454c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願造容像者。",
        "from": "X01n0028_001:0454c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則須遵準量度法為之。",
        "from": "X01n0028_001:0454c17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來身量。",
        "from": "X01n0028_001:0454c17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縱廣相稱。",
        "from": "X01n0028_001:0454c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如尼拘[口*落](二合)陀樹。",
        "from": "X01n0028_001:0454c18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿自一尋。",
        "from": "X01n0028_001:0454c18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今其體肢大小節分。",
        "from": "X01n0028_001:0454c18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豎橫制度。",
        "from": "X01n0028_001:0454c19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起從頂髻。",
        "from": "X01n0028_001:0454c19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略說於汝。",
        "from": "X01n0028_001:0454c19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諦聽。",
        "from": "X01n0028_001:0454c19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善思念之。",
        "from": "X01n0028_001:0454c19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是世尊即說伽陀曰。",
        "from": "X01n0028_001:0454c20_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以自手指量。",
        "from": "X01n0028_001:0454c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百有二十指。",
        "from": "X01n0028_001:0454c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "肉髻崇四指。",
        "from": "X01n0028_001:0454c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髮際亦如此。",
        "from": "X01n0028_001:0454c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面輪豎縱度。",
        "from": "X01n0028_001:0454c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶半十二指。",
        "from": "X01n0028_001:0454c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分三為額鼻。",
        "from": "X01n0028_001:0454c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及頦俱得一。",
        "from": "X01n0028_001:0454c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下分四指半。",
        "from": "X01n0028_001:0454c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頦身只二指。",
        "from": "X01n0028_001:0454c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣向十六足。",
        "from": "X01n0028_001:0454c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深分逕四指。",
        "from": "X01n0028_001:0454c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上唇長二指。",
        "from": "X01n0028_001:0454c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寬有其半矣。",
        "from": "X01n0028_001:0454c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中顯頻婆形。",
        "from": "X01n0028_001:0454c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "邊角各一指。",
        "from": "X01n0028_001:0454c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口長度四指。",
        "from": "X01n0028_001:0455a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢者須要知。",
        "from": "X01n0028_001:0455a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牙齒數四十。",
        "from": "X01n0028_001:0455a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅密白淨齊。",
        "from": "X01n0028_001:0455a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下唇長六足。",
        "from": "X01n0028_001:0455a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寬深亦如是。",
        "from": "X01n0028_001:0455a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人中凹槽闊。",
        "from": "X01n0028_001:0455a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三分指之一。",
        "from": "X01n0028_001:0455a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鼻寬二指量。",
        "from": "X01n0028_001:0455a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "準高指半矣。",
        "from": "X01n0028_001:0455a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鼻孔圓且藏。",
        "from": "X01n0028_001:0455a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竅闊有半指。",
        "from": "X01n0028_001:0455a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雙翅勻真圓。",
        "from": "X01n0028_001:0455a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鼻柱橫半指。",
        "from": "X01n0028_001:0455a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目間曠八足。",
        "from": "X01n0028_001:0455a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長分應四指。",
        "from": "X01n0028_001:0455a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白黑睛三分。",
        "from": "X01n0028_001:0455a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各分得滿指。",
        "from": "X01n0028_001:0455a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黑珠作五分。",
        "from": "X01n0028_001:0455a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正中是眸子。",
        "from": "X01n0028_001:0455a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼寬只一足。",
        "from": "X01n0028_001:0455a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其胞有三指。",
        "from": "X01n0028_001:0455a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "式如蓮花瓣。",
        "from": "X01n0028_001:0455a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清瑩金精色。",
        "from": "X01n0028_001:0455a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印堂白毫地。",
        "from": "X01n0028_001:0455a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣帶半一指。",
        "from": "X01n0028_001:0455a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眉如初月牙。",
        "from": "X01n0028_001:0455a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中高長四指。",
        "from": "X01n0028_001:0455a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耳廣有二指。",
        "from": "X01n0028_001:0455a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尖等眉中齊。",
        "from": "X01n0028_001:0455a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洞明寬四足。",
        "from": "X01n0028_001:0455a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竅孔得半指。",
        "from": "X01n0028_001:0455a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耳朵高四麥。",
        "from": "X01n0028_001:0455a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "橫分應滿指。",
        "from": "X01n0028_001:0455a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耳內上下略。",
        "from": "X01n0028_001:0455a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四分指之一。",
        "from": "X01n0028_001:0455a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "連槽深分總。",
        "from": "X01n0028_001:0455a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二指加半指。",
        "from": "X01n0028_001:0455a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耳葉四指半。",
        "from": "X01n0028_001:0455a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耳垂長五指。",
        "from": "X01n0028_001:0455a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輪郭髮際邊。",
        "from": "X01n0028_001:0455a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可愛盡難比。",
        "from": "X01n0028_001:0455a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "首圍面三倍。",
        "from": "X01n0028_001:0455a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如寶葢適意。",
        "from": "X01n0028_001:0455a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩耳面相去。",
        "from": "X01n0028_001:0455a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十有八布指。",
        "from": "X01n0028_001:0455a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復其背後間。",
        "from": "X01n0028_001:0455a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相去十四指。",
        "from": "X01n0028_001:0455a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合較週匝度。",
        "from": "X01n0028_001:0455a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十有六指。",
        "from": "X01n0028_001:0455a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頸瓶廣八指。",
        "from": "X01n0028_001:0455a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓二十四指。",
        "from": "X01n0028_001:0455a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頸邊至肩甲。",
        "from": "X01n0028_001:0455a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平量十二指。",
        "from": "X01n0028_001:0455a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手長總四搩。",
        "from": "X01n0028_001:0455a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臑長二十指。",
        "from": "X01n0028_001:0455a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臂有十六指。",
        "from": "X01n0028_001:0455a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "巨週亦如此。",
        "from": "X01n0028_001:0455a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "肩尖圓且滿。",
        "from": "X01n0028_001:0455a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "根圍二十四。",
        "from": "X01n0028_001:0455a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從起中指尖。",
        "from": "X01n0028_001:0455a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手頭正一搩。",
        "from": "X01n0028_001:0455a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掌縱應七指。",
        "from": "X01n0028_001:0455a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣分是五指。",
        "from": "X01n0028_001:0455a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掌肉平飽滿。",
        "from": "X01n0028_001:0455a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滋潤光滑赤。",
        "from": "X01n0028_001:0455a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯諸吉祥紋。",
        "from": "X01n0028_001:0455a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "螺輪華鈎飾。",
        "from": "X01n0028_001:0455a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將指之長分。",
        "from": "X01n0028_001:0455a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前面得五指。",
        "from": "X01n0028_001:0455a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此指梢節中。",
        "from": "X01n0028_001:0455a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食指之尖至。",
        "from": "X01n0028_001:0455a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈指之長分。",
        "from": "X01n0028_001:0455a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比將矮半指。",
        "from": "X01n0028_001:0455a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小梅指頭尖。",
        "from": "X01n0028_001:0455a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至屈末半節。",
        "from": "X01n0028_001:0455a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四皆具三節。",
        "from": "X01n0028_001:0455a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甲葢半節矣。",
        "from": "X01n0028_001:0455a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "巨指長四指。",
        "from": "X01n0028_001:0455a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其週亦如是。",
        "from": "X01n0028_001:0455a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此指只兩節。",
        "from": "X01n0028_001:0455a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甲遮如前矣。",
        "from": "X01n0028_001:0455a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "巨指食指根。",
        "from": "X01n0028_001:0455a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相去應三指。",
        "from": "X01n0028_001:0455a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小指根以下。",
        "from": "X01n0028_001:0455a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四指半至基。",
        "from": "X01n0028_001:0455a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "項心臍乳間。",
        "from": "X01n0028_001:0455a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶半十二指。",
        "from": "X01n0028_001:0455a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自乳尖至腋。",
        "from": "X01n0028_001:0455a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平量六指矣。",
        "from": "X01n0028_001:0455a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩腋相去度。",
        "from": "X01n0028_001:0455a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十有五指。",
        "from": "X01n0028_001:0455a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "胸堂週圍繞。",
        "from": "X01n0028_001:0455a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正五十六指。",
        "from": "X01n0028_001:0455a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自從雙乳絣。",
        "from": "X01n0028_001:0455a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十六指至臍。",
        "from": "X01n0028_001:0455a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臍圓有摺旋。",
        "from": "X01n0028_001:0455b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深闊皆一指。",
        "from": "X01n0028_001:0455b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此處腰圍繞。",
        "from": "X01n0028_001:0455b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四十有八指。",
        "from": "X01n0028_001:0455b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髀樞邊向裏。",
        "from": "X01n0028_001:0455b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平量廿四指。",
        "from": "X01n0028_001:0455b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從臍至陰藏。",
        "from": "X01n0028_001:0455b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿搩加半指。",
        "from": "X01n0028_001:0455b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陰藏如馬王。",
        "from": "X01n0028_001:0455b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密囊有四指。",
        "from": "X01n0028_001:0455b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "股奘三十二。",
        "from": "X01n0028_001:0455b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長二十五指。",
        "from": "X01n0028_001:0455b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "近膝圍繞度。",
        "from": "X01n0028_001:0455b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十有八指。",
        "from": "X01n0028_001:0455b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "連節膝四指。",
        "from": "X01n0028_001:0455b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "踝骨縱三指。",
        "from": "X01n0028_001:0455b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鹿腨纖圓直。",
        "from": "X01n0028_001:0455b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長二十五指。",
        "from": "X01n0028_001:0455b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中間週圍繞。",
        "from": "X01n0028_001:0455b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十有一指。",
        "from": "X01n0028_001:0455b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "踝圍十四指。",
        "from": "X01n0028_001:0455b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其邊寬二指。",
        "from": "X01n0028_001:0455b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以下四指踵。",
        "from": "X01n0028_001:0455b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凸闊得三指。",
        "from": "X01n0028_001:0455b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "足底豎一搩。",
        "from": "X01n0028_001:0455b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "厚分有二指。",
        "from": "X01n0028_001:0455b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四指俱三節。",
        "from": "X01n0028_001:0455b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甲遮末半矣。",
        "from": "X01n0028_001:0455b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將指惟二節。",
        "from": "X01n0028_001:0455b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圍繞五指是。",
        "from": "X01n0028_001:0455b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長度滿三指。",
        "from": "X01n0028_001:0455b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食指亦如是。",
        "from": "X01n0028_001:0455b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十六次八分。",
        "from": "X01n0028_001:0455b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中屈及小指。",
        "from": "X01n0028_001:0455b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指厚六足。",
        "from": "X01n0028_001:0455b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘漸止滿指。",
        "from": "X01n0028_001:0455b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "指根連縵網。",
        "from": "X01n0028_001:0455b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "趺高如龜背。",
        "from": "X01n0028_001:0455b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掌平滿柔輭。",
        "from": "X01n0028_001:0455b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滋潤色丹赤。",
        "from": "X01n0028_001:0455b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輪螺吉祥字。",
        "from": "X01n0028_001:0455b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸妙相深微。",
        "from": "X01n0028_001:0455b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是如來相。",
        "from": "X01n0028_001:0455b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切福德備。",
        "from": "X01n0028_001:0455b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說此經已。",
        "from": "X01n0028_001:0455b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢者舍利弗。",
        "from": "X01n0028_001:0455b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸弟子。",
        "from": "X01n0028_001:0455b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切大眾。",
        "from": "X01n0028_001:0455b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛所說。",
        "from": "X01n0028_001:0455b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆大歡喜。",
        "from": "X01n0028_001:0455b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信受奉行。",
        "from": "X01n0028_001:0455b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說造像量度經",
        "from": "X01n0028_001:0455b15_0",
        "context_with_next_sentence": false
    }
]