[
    {
        "source_sentence": "使呪法經",
        "from": "T21n1267_001:0297b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南天竺三藏菩提留支奉　詔譯",
        "from": "T21n1267_001:0297b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時毘那夜迦。",
        "from": "T21n1267_001:0297b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於鷄羅山。",
        "from": "T21n1267_001:0297b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "集諸大眾梵天自在天釋提桓因等。",
        "from": "T21n1267_001:0297b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及無量億數鬼神等。",
        "from": "T21n1267_001:0297b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從座而起。",
        "from": "T21n1267_001:0297b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稽首作禮於大自在天請言。",
        "from": "T21n1267_001:0297b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今欲說一字呪饒益眾生。",
        "from": "T21n1267_001:0297b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願印可聽我所說。",
        "from": "T21n1267_001:0297b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天言善哉如汝所說。",
        "from": "T21n1267_001:0297b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘那夜迦得說歡喜踊躍。",
        "from": "T21n1267_001:0297b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說毘那夜迦一字呪曰。",
        "from": "T21n1267_001:0297b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　虐　伽　頡里　唵　訶　[合*牛]　泮　吒欲作此法先須造像。",
        "from": "T21n1267_001:0297b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或用白鑞及金銀銅樺木等。",
        "from": "T21n1267_001:0297b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各刻作其形像。",
        "from": "T21n1267_001:0297b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫婦二身和合相抱立。",
        "from": "T21n1267_001:0297b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並作象頭人身。",
        "from": "T21n1267_001:0297b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其造像直不得還價。",
        "from": "T21n1267_001:0297b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造其像已。",
        "from": "T21n1267_001:0297b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白月一日。",
        "from": "T21n1267_001:0297b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於淨室內用淨牛糞磨作圓壇。",
        "from": "T21n1267_001:0297b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意大小。",
        "from": "T21n1267_001:0297b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當取一升胡麻油。",
        "from": "T21n1267_001:0297b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用上呪呪其淨油。",
        "from": "T21n1267_001:0297b18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一百八遍煖其油。",
        "from": "T21n1267_001:0297b19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以淨銅器盛著煖油。",
        "from": "T21n1267_001:0297b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後將像放著銅盤油中。",
        "from": "T21n1267_001:0297b20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安置壇內。",
        "from": "T21n1267_001:0297b20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用淨銅匙若銅杓等攣油。",
        "from": "T21n1267_001:0297b21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灌其二像身頂一百八遍。",
        "from": "T21n1267_001:0297b21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以後日日更呪舊油一百八遍。",
        "from": "T21n1267_001:0297b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日之中七遍灌之。",
        "from": "T21n1267_001:0297b22_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平旦四遍日午三遍共成七遍。",
        "from": "T21n1267_001:0297b23_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是作法乃至七日。",
        "from": "T21n1267_001:0297b24_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨心所願成即得稱意。",
        "from": "T21n1267_001:0297b24_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正灌油時數數發願。",
        "from": "T21n1267_001:0297b25_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用酥蜜和麨作團。",
        "from": "T21n1267_001:0297b25_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘿蔔根并盞酢酒漿。",
        "from": "T21n1267_001:0297b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是日成獻食。",
        "from": "T21n1267_001:0297b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必須自食方得氣力。",
        "from": "T21n1267_001:0297b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時毘那羅曩伽。",
        "from": "T21n1267_001:0297b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將領九千八百諸大鬼王。",
        "from": "T21n1267_001:0297b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遊行三千世界。",
        "from": "T21n1267_001:0297b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等所為神力自在。",
        "from": "T21n1267_001:0297b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍歷諸方奉衛三寶。",
        "from": "T21n1267_001:0297b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已大慈悲利益眾生。",
        "from": "T21n1267_001:0297c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向於世尊俱發聲言。",
        "from": "T21n1267_001:0297c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我以自在神通故。",
        "from": "T21n1267_001:0297c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號毘那羅曩伽。",
        "from": "T21n1267_001:0297c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名毘那夜迦。",
        "from": "T21n1267_001:0297c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名毘微那曩伽。",
        "from": "T21n1267_001:0297c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦曰摩訶毘那夜伽。",
        "from": "T21n1267_001:0297c03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是四天下稱皆不同。",
        "from": "T21n1267_001:0297c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我於出世復有別名。",
        "from": "T21n1267_001:0297c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以神變昇虛空而說偈言。",
        "from": "T21n1267_001:0297c05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　我有微妙法　　世間甚希有",
        "from": "T21n1267_001:0297c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　眾生受持者　　皆與願滿足",
        "from": "T21n1267_001:0297c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　我行順世法　　世示希有事",
        "from": "T21n1267_001:0297c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　我能隨其願　　有求名遷官",
        "from": "T21n1267_001:0297c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　我使國王召　　有求世異寶",
        "from": "T21n1267_001:0297c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　使世積珍利　　家豐足七珍",
        "from": "T21n1267_001:0297c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　世皆所希有　　有求色美者",
        "from": "T21n1267_001:0297c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　發願宛然至　　莫須言遠近",
        "from": "T21n1267_001:0297c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　高貴及難易　　志心於我者",
        "from": "T21n1267_001:0297c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　我使須臾間　　有眾生疾苦",
        "from": "T21n1267_001:0297c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　神狂及疥癩　　疾毒眾不利",
        "from": "T21n1267_001:0297c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　百種害加惱　　誦我陀羅尼",
        "from": "T21n1267_001:0297c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無不解脫者　　獨行暗冥處",
        "from": "T21n1267_001:0297c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　依我皆無畏　　劫賊忽然侵",
        "from": "T21n1267_001:0297c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　我皆令自縛　　若欲自然福",
        "from": "T21n1267_001:0298a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若有求女人　　夫心令得女",
        "from": "T21n1267_001:0298a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　我必令相愛　　世間陵突者",
        "from": "T21n1267_001:0298a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　我悉令摧伏　　逍遙自快樂",
        "from": "T21n1267_001:0298a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　宛然無所乏　　有念皆稱遂",
        "from": "T21n1267_001:0298a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　隨有咸滿足　　設眾惡來侵",
        "from": "T21n1267_001:0298a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　我使如其意　　我悉能加護",
        "from": "T21n1267_001:0298a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　住居皆吉慶　　宅舍悉清寧",
        "from": "T21n1267_001:0298a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　男女得英名　　夫妻順和合",
        "from": "T21n1267_001:0298a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　上品持我者　　我與人中王",
        "from": "T21n1267_001:0298a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　中品持我者　　我與為帝師",
        "from": "T21n1267_001:0298a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　下品持我者　　富貴無窮已",
        "from": "T21n1267_001:0298a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　恒欲相娛樂　　無不充滿足",
        "from": "T21n1267_001:0298a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　奴婢列成群　　美女滿衢庭",
        "from": "T21n1267_001:0298a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　遊行得自在　　隱顯能隨念",
        "from": "T21n1267_001:0298a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　出入無所礙　　無能測量者",
        "from": "T21n1267_001:0298a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　我於三界中　　神力得自在",
        "from": "T21n1267_001:0298a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　降世希有事　　我皆悉所為",
        "from": "T21n1267_001:0298a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若說我所能　　窮劫不能盡",
        "from": "T21n1267_001:0298a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　持我陀羅尼　　我皆現其前",
        "from": "T21n1267_001:0298a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　夫妻及眷屬　　當隨得衛護",
        "from": "T21n1267_001:0298a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　我有遊行時　　誦我即時至",
        "from": "T21n1267_001:0298a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　過於險難處　　大海及江河",
        "from": "T21n1267_001:0298a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　深山險隘處　　師子象虎狼",
        "from": "T21n1267_001:0298a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　毒蟲諸神難　　持我皆安穩",
        "from": "T21n1267_001:0298a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若有侵嬈者　　頭破作七分",
        "from": "T21n1267_001:0298a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　壽命悉長遠　　福祿自遷至",
        "from": "T21n1267_001:0298a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時毘那夜羅迦說是偈已。",
        "from": "T21n1267_001:0298a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告世人言。",
        "from": "T21n1267_001:0298a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說處世陀羅尼法。",
        "from": "T21n1267_001:0298a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最護眾生。",
        "from": "T21n1267_001:0298a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨其所願皆得滿足。",
        "from": "T21n1267_001:0298a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當須日夜誦持滿十萬遍乃至二十萬遍。",
        "from": "T21n1267_001:0298b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆得如所說即昇虛空。",
        "from": "T21n1267_001:0298b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1267_001:0298b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩　牟　毘　那　夜　迦(上音)　寫　阿　悉知　目佉(上音)　寫　怛　姪　佗(三)　阿　智耶　那　智耶(二合)(五)殊　皤　帝　耶(六)　烏　悉曇(二合)　迦耶(七)　悉婆(二合)　拖　鉢　耶(八)　婆　達　薩寫　耶(九)　婆　唎　跛　遲(十)　莎　訶使呪法經",
        "from": "T21n1267_001:0298b04_0",
        "context_with_next_sentence": false
    }
]