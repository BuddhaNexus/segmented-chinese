[
    {
        "source_sentence": "因明入正理論一卷",
        "from": "T32n1630_001:0011a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "商羯羅主菩薩造",
        "from": "T32n1630_001:0011a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏法師玄奘奉　詔譯",
        "from": "T32n1630_001:0011a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　能立與能破　　及似唯悟他",
        "from": "T32n1630_001:0011a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　現量與比量　　及似唯自悟",
        "from": "T32n1630_001:0011a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是總攝諸論要義。",
        "from": "T32n1630_001:0011b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中宗等多言名為能立。",
        "from": "T32n1630_001:0011b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由宗因喻多言。",
        "from": "T32n1630_001:0011b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開示諸有問者未了義故。",
        "from": "T32n1630_001:0011b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中宗者。",
        "from": "T32n1630_001:0011b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂極成有法極成能別差別為性。",
        "from": "T32n1630_001:0011b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨自樂為所成立性。",
        "from": "T32n1630_001:0011b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為宗。",
        "from": "T32n1630_001:0011b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如有成立聲是無常。",
        "from": "T32n1630_001:0011b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因有三相。",
        "from": "T32n1630_001:0011b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為三。",
        "from": "T32n1630_001:0011b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂遍是宗法性。",
        "from": "T32n1630_001:0011b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同品定有性。",
        "from": "T32n1630_001:0011b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "異品遍無性。",
        "from": "T32n1630_001:0011b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名為同品異品。",
        "from": "T32n1630_001:0011b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂所立法均等義品。",
        "from": "T32n1630_001:0011b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說名同品。",
        "from": "T32n1630_001:0011b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如立無常瓶等無常。",
        "from": "T32n1630_001:0011b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名同品。",
        "from": "T32n1630_001:0011b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "異品者。",
        "from": "T32n1630_001:0011b09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於是處無其所立。",
        "from": "T32n1630_001:0011b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有是常見非所作。",
        "from": "T32n1630_001:0011b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如虛空等。",
        "from": "T32n1630_001:0011b10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中所作性或勤勇無間所發性遍是宗法性。",
        "from": "T32n1630_001:0011b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同品定有性。",
        "from": "T32n1630_001:0011b11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "異品遍無性。",
        "from": "T32n1630_001:0011b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是無常等因。",
        "from": "T32n1630_001:0011b12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喻有二種。",
        "from": "T32n1630_001:0011b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者同法。",
        "from": "T32n1630_001:0011b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者異法。",
        "from": "T32n1630_001:0011b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同法者若於是處顯因同品決定有性。",
        "from": "T32n1630_001:0011b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂若所作見彼無常。",
        "from": "T32n1630_001:0011b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如瓶等。",
        "from": "T32n1630_001:0011b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "異法者若於是處說所立無。",
        "from": "T32n1630_001:0011b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因遍非有。",
        "from": "T32n1630_001:0011b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂若是常見。",
        "from": "T32n1630_001:0011b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非所作如虛空等。",
        "from": "T32n1630_001:0011b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中常言表非無常。",
        "from": "T32n1630_001:0011b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非所作言表無所作。",
        "from": "T32n1630_001:0011b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如有非有說名非有。",
        "from": "T32n1630_001:0011b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已說宗等如是多言。",
        "from": "T32n1630_001:0011b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開悟他時。",
        "from": "T32n1630_001:0011b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說名能立。",
        "from": "T32n1630_001:0011b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如說聲無常者是立宗言。",
        "from": "T32n1630_001:0011b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所作性故者是宗法言。",
        "from": "T32n1630_001:0011b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若是所作見彼無常。",
        "from": "T32n1630_001:0011b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如瓶等者是隨同品言。",
        "from": "T32n1630_001:0011b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若是其常見非所作。",
        "from": "T32n1630_001:0011b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如虛空者是遠離言。",
        "from": "T32n1630_001:0011b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯此三分說名能立。",
        "from": "T32n1630_001:0011b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖樂成立由與現量等相違故。",
        "from": "T32n1630_001:0011b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名似立宗。",
        "from": "T32n1630_001:0011b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂現量相違。",
        "from": "T32n1630_001:0011b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比量相違。",
        "from": "T32n1630_001:0011b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自教相違。",
        "from": "T32n1630_001:0011b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間相違。",
        "from": "T32n1630_001:0011b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自語相違。",
        "from": "T32n1630_001:0011b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能別不極成。",
        "from": "T32n1630_001:0011b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所別不極成。",
        "from": "T32n1630_001:0011b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱不極成。",
        "from": "T32n1630_001:0011b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相符極成。",
        "from": "T32n1630_001:0011b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中現量相違者。",
        "from": "T32n1630_001:0011b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如說聲非所聞。",
        "from": "T32n1630_001:0011b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比量相違者。",
        "from": "T32n1630_001:0011b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如說瓶等是常。",
        "from": "T32n1630_001:0011b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自教相違者。",
        "from": "T32n1630_001:0011b28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如勝論師立聲為常。",
        "from": "T32n1630_001:0011b29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間相違者。",
        "from": "T32n1630_001:0011b29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如說懷兔非月有故。",
        "from": "T32n1630_001:0011b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又如說言人頂骨淨。",
        "from": "T32n1630_001:0011c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生分故。",
        "from": "T32n1630_001:0011c01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如螺貝。",
        "from": "T32n1630_001:0011c02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自語相違者。",
        "from": "T32n1630_001:0011c02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如言我母是其石女。",
        "from": "T32n1630_001:0011c02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能別不極成者。",
        "from": "T32n1630_001:0011c03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如佛弟子對數論師立聲滅壞。",
        "from": "T32n1630_001:0011c03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所別不極成者。",
        "from": "T32n1630_001:0011c04_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如數論師對佛弟子說我是思。",
        "from": "T32n1630_001:0011c04_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱不極成者。",
        "from": "T32n1630_001:0011c05_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如勝論師對佛弟子立我以為和合因緣。",
        "from": "T32n1630_001:0011c05_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相符極成者。",
        "from": "T32n1630_001:0011c06_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如說聲是所聞。",
        "from": "T32n1630_001:0011c07_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是多言。",
        "from": "T32n1630_001:0011c07_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是遣諸法自相門故。",
        "from": "T32n1630_001:0011c07_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不容成故。",
        "from": "T32n1630_001:0011c08_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立無果故。",
        "from": "T32n1630_001:0011c08_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名似立宗過已說似宗。",
        "from": "T32n1630_001:0011c08_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當說似因。",
        "from": "T32n1630_001:0011c09_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不成不定及與相違。",
        "from": "T32n1630_001:0011c09_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名似因。",
        "from": "T32n1630_001:0011c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不成有四。",
        "from": "T32n1630_001:0011c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一兩俱不成。",
        "from": "T32n1630_001:0011c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二隨一不成。",
        "from": "T32n1630_001:0011c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三猶豫不成。",
        "from": "T32n1630_001:0011c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四所依不成。",
        "from": "T32n1630_001:0011c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如成立聲為無常等若言是眼所見性故。",
        "from": "T32n1630_001:0011c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩俱不成。",
        "from": "T32n1630_001:0011c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所作性故對聲顯論。",
        "from": "T32n1630_001:0011c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨一不成。",
        "from": "T32n1630_001:0011c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於霧等性起疑惑時。",
        "from": "T32n1630_001:0011c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為成大種和合火有而有所說。",
        "from": "T32n1630_001:0011c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶豫不成。",
        "from": "T32n1630_001:0011c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空實有。",
        "from": "T32n1630_001:0011c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "德所依故。",
        "from": "T32n1630_001:0011c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對無空論所依不成。",
        "from": "T32n1630_001:0011c16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不定有六。",
        "from": "T32n1630_001:0011c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一共。",
        "from": "T32n1630_001:0011c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二不共。",
        "from": "T32n1630_001:0011c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三同品一分轉異品遍轉。",
        "from": "T32n1630_001:0011c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四異品一分轉同品遍轉。",
        "from": "T32n1630_001:0011c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五俱品一分轉。",
        "from": "T32n1630_001:0011c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六相違決定。",
        "from": "T32n1630_001:0011c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中共者。",
        "from": "T32n1630_001:0011c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如言聲常所量性故。",
        "from": "T32n1630_001:0011c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常無常品皆共此因。",
        "from": "T32n1630_001:0011c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故不定。",
        "from": "T32n1630_001:0011c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為如瓶等所量性故聲是無常。",
        "from": "T32n1630_001:0011c20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為如空等所量性故聲是其常。",
        "from": "T32n1630_001:0011c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言不共者。",
        "from": "T32n1630_001:0011c22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如說聲常。",
        "from": "T32n1630_001:0011c22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所聞性故。",
        "from": "T32n1630_001:0011c22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常無常品皆離此因。",
        "from": "T32n1630_001:0011c23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常無常外餘非有故。",
        "from": "T32n1630_001:0011c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是猶豫因。",
        "from": "T32n1630_001:0011c24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此所聞性其猶何等。",
        "from": "T32n1630_001:0011c24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同品一分轉異品遍轉者。",
        "from": "T32n1630_001:0011c24_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如說聲非勤勇無間所發無常性故。",
        "from": "T32n1630_001:0011c25_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中非勤勇無間所發宗。",
        "from": "T32n1630_001:0011c26_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以電空等為其同品。",
        "from": "T32n1630_001:0011c26_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此無常性於電等有。",
        "from": "T32n1630_001:0011c27_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於空等無。",
        "from": "T32n1630_001:0011c27_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非勤勇無間所發宗。",
        "from": "T32n1630_001:0011c28_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以瓶等為異品。",
        "from": "T32n1630_001:0011c28_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彼遍有。",
        "from": "T32n1630_001:0011c28_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此因以電瓶等為同法故。",
        "from": "T32n1630_001:0011c29_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦是不定。",
        "from": "T32n1630_001:0011c29_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為如瓶等無常性故。",
        "from": "T32n1630_001:0012a01_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼是勤勇無間所發。",
        "from": "T32n1630_001:0012a01_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為如電等無常性故。",
        "from": "T32n1630_001:0012a02_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼非勤勇無間所發。",
        "from": "T32n1630_001:0012a02_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "異品一分轉同品遍轉者。",
        "from": "T32n1630_001:0012a03_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如立宗言聲是勤勇無間所發。",
        "from": "T32n1630_001:0012a03_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無常性故。",
        "from": "T32n1630_001:0012a04_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勤勇無間所發宗。",
        "from": "T32n1630_001:0012a04_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以瓶等為同品。",
        "from": "T32n1630_001:0012a05_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其無常性於此遍有。",
        "from": "T32n1630_001:0012a05_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以電空等為異品。",
        "from": "T32n1630_001:0012a05_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彼一分電等是有。",
        "from": "T32n1630_001:0012a06_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空等是無。",
        "from": "T32n1630_001:0012a06_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故如前亦為不定。",
        "from": "T32n1630_001:0012a07_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱品一分轉者。",
        "from": "T32n1630_001:0012a07_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如說聲常。",
        "from": "T32n1630_001:0012a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無質礙故。",
        "from": "T32n1630_001:0012a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中常宗。",
        "from": "T32n1630_001:0012a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以虛空極微等為同品。",
        "from": "T32n1630_001:0012a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無質礙性於虛空等有。",
        "from": "T32n1630_001:0012a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於極微等無。",
        "from": "T32n1630_001:0012a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以瓶樂等為異品。",
        "from": "T32n1630_001:0012a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於樂等有。",
        "from": "T32n1630_001:0012a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於瓶等無。",
        "from": "T32n1630_001:0012a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故此因以樂以空為同法故。",
        "from": "T32n1630_001:0012a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名不定。",
        "from": "T32n1630_001:0012a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相違決定者。",
        "from": "T32n1630_001:0012a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如立宗言聲是無常。",
        "from": "T32n1630_001:0012a12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所作性故。",
        "from": "T32n1630_001:0012a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如瓶等。",
        "from": "T32n1630_001:0012a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有立聲常。",
        "from": "T32n1630_001:0012a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所聞性故。",
        "from": "T32n1630_001:0012a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如聲性。",
        "from": "T32n1630_001:0012a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此二皆是猶豫因。",
        "from": "T32n1630_001:0012a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故俱名不定。",
        "from": "T32n1630_001:0012a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相違有四。",
        "from": "T32n1630_001:0012a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂法自性相違因。",
        "from": "T32n1630_001:0012a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法差別相違因。",
        "from": "T32n1630_001:0012a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有法自相相違因。",
        "from": "T32n1630_001:0012a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有法差別相違因等。",
        "from": "T32n1630_001:0012a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中。",
        "from": "T32n1630_001:0012a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法自相相違因者。",
        "from": "T32n1630_001:0012a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如說聲常。",
        "from": "T32n1630_001:0012a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所作性故。",
        "from": "T32n1630_001:0012a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或勤勇無間所發性故。",
        "from": "T32n1630_001:0012a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此因唯於異品中有。",
        "from": "T32n1630_001:0012a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故相違。",
        "from": "T32n1630_001:0012a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法差別相違因者。",
        "from": "T32n1630_001:0012a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如說眼等必為他用。",
        "from": "T32n1630_001:0012a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "積聚性故。",
        "from": "T32n1630_001:0012a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如臥具等。",
        "from": "T32n1630_001:0012a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此因如能成立眼等必為他用。",
        "from": "T32n1630_001:0012a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是亦能成立所立法差別相違積聚他用。",
        "from": "T32n1630_001:0012a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸臥具等為積聚他所受用故。",
        "from": "T32n1630_001:0012a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有法自相相違因者。",
        "from": "T32n1630_001:0012a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如說有性非實非德非業。",
        "from": "T32n1630_001:0012a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有一實故。",
        "from": "T32n1630_001:0012a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有德業故。",
        "from": "T32n1630_001:0012a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如同異性。",
        "from": "T32n1630_001:0012a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此因如能成遮實等。",
        "from": "T32n1630_001:0012a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是亦能成遮有性。",
        "from": "T32n1630_001:0012a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱決定故。",
        "from": "T32n1630_001:0012a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有法差別相違因者。",
        "from": "T32n1630_001:0012a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如即此因即於前宗有法差別作有緣性。",
        "from": "T32n1630_001:0012a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能成立與此相違作非有緣性。",
        "from": "T32n1630_001:0012a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如遮實等。",
        "from": "T32n1630_001:0012a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱決定故。",
        "from": "T32n1630_001:0012a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已說似因。",
        "from": "T32n1630_001:0012a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當說似喻。",
        "from": "T32n1630_001:0012a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "似同法喻有其五種。",
        "from": "T32n1630_001:0012b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一能立法不成。",
        "from": "T32n1630_001:0012b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二所立法不成。",
        "from": "T32n1630_001:0012b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三俱不成。",
        "from": "T32n1630_001:0012b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四無合。",
        "from": "T32n1630_001:0012b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五倒合。",
        "from": "T32n1630_001:0012b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "似異法喻亦有五種。",
        "from": "T32n1630_001:0012b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一所立不遣。",
        "from": "T32n1630_001:0012b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二能立不遣。",
        "from": "T32n1630_001:0012b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三俱不遣。",
        "from": "T32n1630_001:0012b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四不離。",
        "from": "T32n1630_001:0012b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五倒離。",
        "from": "T32n1630_001:0012b04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能立法不成者。",
        "from": "T32n1630_001:0012b04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如說聲常。",
        "from": "T32n1630_001:0012b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無質礙故。",
        "from": "T32n1630_001:0012b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸無質礙見。",
        "from": "T32n1630_001:0012b05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼是常猶如極微。",
        "from": "T32n1630_001:0012b05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然彼極微所成立法常性是有。",
        "from": "T32n1630_001:0012b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能成立法無質礙無。",
        "from": "T32n1630_001:0012b06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以諸極微質礙性故。",
        "from": "T32n1630_001:0012b07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所立法不成者。",
        "from": "T32n1630_001:0012b07_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂說如覺。",
        "from": "T32n1630_001:0012b08_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然一切覺能成立法無質礙有。",
        "from": "T32n1630_001:0012b08_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所成立法常住性無。",
        "from": "T32n1630_001:0012b09_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以一切覺皆無常故。",
        "from": "T32n1630_001:0012b09_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱不成者。",
        "from": "T32n1630_001:0012b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有二種。",
        "from": "T32n1630_001:0012b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有及非有。",
        "from": "T32n1630_001:0012b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若言如瓶。",
        "from": "T32n1630_001:0012b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有俱不成。",
        "from": "T32n1630_001:0012b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若說如空。",
        "from": "T32n1630_001:0012b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對無空論。",
        "from": "T32n1630_001:0012b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無俱不成。",
        "from": "T32n1630_001:0012b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無合者。",
        "from": "T32n1630_001:0012b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於是處無有配合。",
        "from": "T32n1630_001:0012b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但於瓶等。",
        "from": "T32n1630_001:0012b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雙現能立所立二法。",
        "from": "T32n1630_001:0012b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如言於瓶見所作性及無常性。",
        "from": "T32n1630_001:0012b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倒合者。",
        "from": "T32n1630_001:0012b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂應說言諸所作者皆是無常。",
        "from": "T32n1630_001:0012b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而倒說言諸無常者皆是所作。",
        "from": "T32n1630_001:0012b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是名似同法喻品。",
        "from": "T32n1630_001:0012b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "似異法中。",
        "from": "T32n1630_001:0012b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所立不遣者。",
        "from": "T32n1630_001:0012b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且如有言諸無常者見彼質礙。",
        "from": "T32n1630_001:0012b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如極微。",
        "from": "T32n1630_001:0012b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由於極微所成立法常性不遣。",
        "from": "T32n1630_001:0012b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼立極微是常性故。",
        "from": "T32n1630_001:0012b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能成立法無質礙無。",
        "from": "T32n1630_001:0012b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能立不遣者。",
        "from": "T32n1630_001:0012b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂說如業。",
        "from": "T32n1630_001:0012b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但遣所立。",
        "from": "T32n1630_001:0012b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不遣能立。",
        "from": "T32n1630_001:0012b21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼說諸業無質礙故。",
        "from": "T32n1630_001:0012b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱不遣者。",
        "from": "T32n1630_001:0012b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對彼有論。",
        "from": "T32n1630_001:0012b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說如虛空。",
        "from": "T32n1630_001:0012b22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由彼虛空不遣常性無質礙性。",
        "from": "T32n1630_001:0012b22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以說虛空是常性故。",
        "from": "T32n1630_001:0012b23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無質礙故。",
        "from": "T32n1630_001:0012b23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不離者。",
        "from": "T32n1630_001:0012b24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂說如瓶見無常性。",
        "from": "T32n1630_001:0012b24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有質礙性。",
        "from": "T32n1630_001:0012b24_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倒離者。",
        "from": "T32n1630_001:0012b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂如說言諸質礙者皆是無常。",
        "from": "T32n1630_001:0012b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等似宗因喻言。",
        "from": "T32n1630_001:0012b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非正能立。",
        "from": "T32n1630_001:0012b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次為自開悟。",
        "from": "T32n1630_001:0012b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知唯有現比二量。",
        "from": "T32n1630_001:0012b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中。",
        "from": "T32n1630_001:0012b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現量謂無分別。",
        "from": "T32n1630_001:0012b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有正智於色等義離名種等所有分別。",
        "from": "T32n1630_001:0012b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現現別轉。",
        "from": "T32n1630_001:0012b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名現量。",
        "from": "T32n1630_001:0012b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言比量者。",
        "from": "T32n1630_001:0012b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂藉眾相而觀於義。",
        "from": "T32n1630_001:0012c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相有三種。",
        "from": "T32n1630_001:0012c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前已說。",
        "from": "T32n1630_001:0012c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由彼為因。",
        "from": "T32n1630_001:0012c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於所比義有正智生。",
        "from": "T32n1630_001:0012c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了知有火或無常等。",
        "from": "T32n1630_001:0012c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名比量。",
        "from": "T32n1630_001:0012c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於二量中即智名果。",
        "from": "T32n1630_001:0012c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是證相故。",
        "from": "T32n1630_001:0012c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如有作用而顯現故。",
        "from": "T32n1630_001:0012c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名為量。",
        "from": "T32n1630_001:0012c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有分別智。",
        "from": "T32n1630_001:0012c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於義異轉。",
        "from": "T32n1630_001:0012c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名似現量。",
        "from": "T32n1630_001:0012c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂諸有智了瓶衣等分別而生。",
        "from": "T32n1630_001:0012c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由彼於義不以自相為境界故。",
        "from": "T32n1630_001:0012c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名似現量。",
        "from": "T32n1630_001:0012c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若似因智為先所起諸似義智。",
        "from": "T32n1630_001:0012c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名似比量。",
        "from": "T32n1630_001:0012c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "似因多種。",
        "from": "T32n1630_001:0012c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如先已說。",
        "from": "T32n1630_001:0012c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用彼為因。",
        "from": "T32n1630_001:0012c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於似所比諸有智生。",
        "from": "T32n1630_001:0012c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能正解名似比量。",
        "from": "T32n1630_001:0012c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次若正顯示能立過失。",
        "from": "T32n1630_001:0012c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說名能破。",
        "from": "T32n1630_001:0012c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂初能立缺減過性。",
        "from": "T32n1630_001:0012c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立宗過性。",
        "from": "T32n1630_001:0012c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不成立性。",
        "from": "T32n1630_001:0012c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不定因性。",
        "from": "T32n1630_001:0012c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相違因性及喻過性。",
        "from": "T32n1630_001:0012c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯示此言開曉問者。",
        "from": "T32n1630_001:0012c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名能破。",
        "from": "T32n1630_001:0012c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不實顯能立過言名似能破。",
        "from": "T32n1630_001:0012c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於圓滿能立顯示缺減性言。",
        "from": "T32n1630_001:0012c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於無過宗有過宗言。",
        "from": "T32n1630_001:0012c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於成就因不成因言。",
        "from": "T32n1630_001:0012c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於決定因不定因言。",
        "from": "T32n1630_001:0012c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於不相違因相違因言。",
        "from": "T32n1630_001:0012c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於無過喻有過喻言。",
        "from": "T32n1630_001:0012c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是言說名似能破。",
        "from": "T32n1630_001:0012c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以不能顯他宗過失。",
        "from": "T32n1630_001:0012c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼無過故。",
        "from": "T32n1630_001:0012c20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且止斯事。",
        "from": "T32n1630_001:0012c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　已宣少句義　　為始立方隅",
        "from": "T32n1630_001:0012c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　其間理非理　　妙辯於餘處",
        "from": "T32n1630_001:0012c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因明入正理論一卷",
        "from": "T32n1630_001:0012c23_0",
        "context_with_next_sentence": false
    }
]