[
    {
        "source_sentence": "除一切疾病陀羅尼經",
        "from": "T21n1323_001:0489c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T21n1323_001:0489c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空諡大鑑",
        "from": "T21n1323_001:0489c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正號大廣智大興善寺三藏沙門",
        "from": "T21n1323_001:0489c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空奉　詔譯",
        "from": "T21n1323_001:0489c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T21n1323_001:0489c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時薄伽梵。",
        "from": "T21n1323_001:0489c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住室羅伐城逝多林給孤長者園。",
        "from": "T21n1323_001:0489c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大苾芻眾千二百五十人俱。",
        "from": "T21n1323_001:0489c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾多諸大菩薩摩訶薩。",
        "from": "T21n1323_001:0489c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告阿難陀言。",
        "from": "T21n1323_001:0489c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難陀有陀羅尼。",
        "from": "T21n1323_001:0489c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除世間一切疾病。",
        "from": "T21n1323_001:0489c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當受持讀誦通利如理作意。",
        "from": "T21n1323_001:0489c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說密言曰。",
        "from": "T21n1323_001:0489c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也(二合)他(一)尾摩黎尾摩黎(二)嚩曩俱枳黎(三)室唎(二合)末底(丁以反四)軍拏黎(五)嫩奴鼻(六)印捺囉(二合)儗[寧*頁](二合七)母隷娑嚩(二合)訶佛告阿難陀。",
        "from": "T21n1323_001:0489c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此陀羅尼若誦持者。",
        "from": "T21n1323_001:0489c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宿食不消癨亂風黃痰癊。",
        "from": "T21n1323_001:0489c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "患痔瘻淋上氣。",
        "from": "T21n1323_001:0489c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嗽虐寒熱頭痛半痛。",
        "from": "T21n1323_001:0489c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著鬼魅者悉得除差。",
        "from": "T21n1323_001:0489c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我以佛眼觀見彼人。",
        "from": "T21n1323_001:0489c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天魔梵沙門婆羅門能作障難。",
        "from": "T21n1323_001:0489c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除非決定業報盡者。",
        "from": "T21n1323_001:0489c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘無能違越作其障難。",
        "from": "T21n1323_001:0489c24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來應供正遍知說。",
        "from": "T21n1323_001:0489c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切有情中如來為尊勝。",
        "from": "T21n1323_001:0489c25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切法中離欲法尊。",
        "from": "T21n1323_001:0489c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾中僧伽為尊。",
        "from": "T21n1323_001:0489c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此誠實言願我及一切有情。",
        "from": "T21n1323_001:0489c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食飲喫啖入腹消化得正安樂。",
        "from": "T21n1323_001:0489c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娑嚩(二合引)訶(引)。",
        "from": "T21n1323_001:0489c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊說是經已。",
        "from": "T21n1323_001:0489c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸苾芻僧并諸菩薩摩訶薩。",
        "from": "T21n1323_001:0489c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切大眾天龍八部。",
        "from": "T21n1323_001:0490a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持佛語歡喜奉行。",
        "from": "T21n1323_001:0490a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除一切疾病陀羅尼經",
        "from": "T21n1323_001:0490a03_0",
        "context_with_next_sentence": false
    }
]