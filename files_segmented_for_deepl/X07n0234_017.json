        "from": "X07n0234_017:0734c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所管但列九海。",
        "from": "X07n0234_017:0734c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而無結文。",
        "from": "X07n0234_017:0734c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及最近輪圍之海。",
        "from": "X07n0234_017:0734c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九行許經。",
        "from": "X07n0234_017:0734c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸梵本中。",
        "from": "X07n0234_017:0734c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆同此闕。",
        "from": "X07n0234_017:0734c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "準前後例。",
        "from": "X07n0234_017:0734c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此必定有。",
        "from": "X07n0234_017:0734c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大方廣佛華嚴經疏卷第十七",
        "from": "X07n0234_017:0734c15_0",
