[
    {
        "source_sentence": "大方廣佛華嚴經疏卷第十七(入第九卷經第四紙)清涼山沙門　澄觀　述晉水沙門　(淨源)　錄疏注經△二無盡光明輪海。",
        "from": "X07n0234_017:0732c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此下九海文皆有二。",
        "from": "X07n0234_017:0732c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初牒前海為所依。",
        "from": "X07n0234_017:0732c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛子此離垢燄藏香水海南。",
        "from": "X07n0234_017:0732c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆不牒中。",
        "from": "X07n0234_017:0732c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "海為所繞。",
        "from": "X07n0234_017:0732c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故云南也。",
        "from": "X07n0234_017:0732c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三海去。",
        "from": "X07n0234_017:0732c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但言右旋。",
        "from": "X07n0234_017:0732c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不云南者。",
        "from": "X07n0234_017:0732c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十海如環。",
        "from": "X07n0234_017:0732c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繞於中海。",
        "from": "X07n0234_017:0732c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故不正南也。",
        "from": "X07n0234_017:0732c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如以十疊繞於一盤。",
        "from": "X07n0234_017:0732c08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方所可見。",
        "from": "X07n0234_017:0732c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又第二已去。",
        "from": "X07n0234_017:0732c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或無蓮華者。",
        "from": "X07n0234_017:0732c09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前總釋種中云。",
        "from": "X07n0234_017:0732c10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有依蓮華住。",
        "from": "X07n0234_017:0732c10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有依海。",
        "from": "X07n0234_017:0732c10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故或無也。",
        "from": "X07n0234_017:0732c10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二明能依之海二。",
        "from": "X07n0234_017:0732c11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初明海華剎種。",
        "from": "X07n0234_017:0732c11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次有香水海(至)以一切佛功德海音聲為體。",
        "from": "X07n0234_017:0732c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二明所持剎二十重。",
        "from": "X07n0234_017:0732c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中最下方有世界(至)佛號蓮華光歡喜面。",
        "from": "X07n0234_017:0732c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上一世界。",
        "from": "X07n0234_017:0732c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文即有七。",
        "from": "X07n0234_017:0732c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後一文八。",
        "from": "X07n0234_017:0732c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加純淨故。",
        "from": "X07n0234_017:0732c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中間唯三。",
        "from": "X07n0234_017:0732c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂相去數量剎名佛號。",
        "from": "X07n0234_017:0732c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘八海例然。",
        "from": "X07n0234_017:0732c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此上過佛剎微塵數世界(至)佛號法界淨光明。",
        "from": "X07n0234_017:0732c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△三金剛寶燄光明海二。",
        "from": "X07n0234_017:0732c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初牒前海為所依。",
        "from": "X07n0234_017:0732c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛子此無盡光明輪香水海右旋。",
        "from": "X07n0234_017:0732c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二明能依之海二。",
        "from": "X07n0234_017:0732c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初海華剎種。",
        "from": "X07n0234_017:0732c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次有香水海名金剛寶燄(至)一切如來名音聲為體。",
        "from": "X07n0234_017:0732c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二明所持之剎二十重。",
        "from": "X07n0234_017:0732c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中最下方有世界(至)佛號大變化光明網。",
        "from": "X07n0234_017:0732c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△四帝青寶莊嚴海二。",
        "from": "X07n0234_017:0732c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初牒前海為所依。",
        "from": "X07n0234_017:0732c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛子此金剛寶燄香水海右旋。",
        "from": "X07n0234_017:0733a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二明能依之海二。",
        "from": "X07n0234_017:0733a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初海華剎種。",
        "from": "X07n0234_017:0733a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次有香水海名帝青寶莊嚴(至)無邊佛音聲為體。",
        "from": "X07n0234_017:0733a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二明所持之剎二十重。",
        "from": "X07n0234_017:0733a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此最下方有世界(至)佛號解脫精進日。",
        "from": "X07n0234_017:0733a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△五金剛輪莊嚴海二。",
        "from": "X07n0234_017:0733a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初牒前海為所依。",
        "from": "X07n0234_017:0733a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛子此帝青寶莊嚴香水海右旋。",
        "from": "X07n0234_017:0733a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二明能依之海二。",
        "from": "X07n0234_017:0733a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初海華剎種。",
        "from": "X07n0234_017:0733a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次有香水海名金剛輪(至)普賢智所生音聲為體。",
        "from": "X07n0234_017:0733a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二明能依之剎二十重。",
        "from": "X07n0234_017:0733a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中最下方有世界(至)佛號最勝覺神通王。",
        "from": "X07n0234_017:0733a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△六蓮華因陀羅海二。",
        "from": "X07n0234_017:0733a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初牒前海為所依。",
        "from": "X07n0234_017:0733a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛子此金剛輪莊嚴底香水海右旋。",
        "from": "X07n0234_017:0733a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二明能依之海二。",
        "from": "X07n0234_017:0733a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初海華剎種。",
        "from": "X07n0234_017:0733a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次有香水海名蓮華(至)一切佛智光音聲為體。",
        "from": "X07n0234_017:0733a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二明所持之剎二十重。",
        "from": "X07n0234_017:0733a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中最下方有世界(至)有世界名真珠華藏。",
        "from": "X07n0234_017:0733a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真珠藏者。",
        "from": "X07n0234_017:0733a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即蜯類也。",
        "from": "X07n0234_017:0733a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛號法界寶光明(至)佛號徧照世間最勝音。",
        "from": "X07n0234_017:0733a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△七積集寶香藏海二。",
        "from": "X07n0234_017:0733a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初牒前海為所依。",
        "from": "X07n0234_017:0733a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛子此華因陀羅網香水海右旋。",
        "from": "X07n0234_017:0733a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二明能依之海二。",
        "from": "X07n0234_017:0733a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初海華剎種。",
        "from": "X07n0234_017:0733a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次有香水海名積集寶(至)以一切佛法輪音聲為體。",
        "from": "X07n0234_017:0733a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二明所持之剎二十重。",
        "from": "X07n0234_017:0733a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中最下方有世界(至)佛號示現無畏雲。",
        "from": "X07n0234_017:0733b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△八寶莊嚴海二。",
        "from": "X07n0234_017:0733b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初牒前海為所依。",
        "from": "X07n0234_017:0733b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛子此積集寶香藏香水海右旋。",
        "from": "X07n0234_017:0733b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二明能依之海二。",
        "from": "X07n0234_017:0733b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初海華剎種。",
        "from": "X07n0234_017:0733b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次有香水海名寶莊嚴(至)佛剎神變聲為體。",
        "from": "X07n0234_017:0733b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二明所持之剎二十重。",
        "from": "X07n0234_017:0733b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中最下方有世界(至)佛號普眼大明燈。",
        "from": "X07n0234_017:0733b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△九金剛寶聚海二。",
        "from": "X07n0234_017:0733b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初牒前海為所依。",
        "from": "X07n0234_017:0733b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛子此寶莊嚴香水海右旋。",
        "from": "X07n0234_017:0733b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二明能依之海二。",
        "from": "X07n0234_017:0733b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初海華剎種。",
        "from": "X07n0234_017:0733b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次有香水海名金剛寶聚(至)方便法音聲為體。",
        "from": "X07n0234_017:0733b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二明所持之剎二十重。",
        "from": "X07n0234_017:0733b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中最下方有世界(至)佛號清淨眼。",
        "from": "X07n0234_017:0733b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△十天城寶牒海二。",
        "from": "X07n0234_017:0733b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初牒前海為所依。",
        "from": "X07n0234_017:0733b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛子此金剛寶聚香水海右旋。",
        "from": "X07n0234_017:0733b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二明能依之海二。",
        "from": "X07n0234_017:0733b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初海華剎種。",
        "from": "X07n0234_017:0733b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次有香水海名天城寶堞(至)平等法輪音為體。",
        "from": "X07n0234_017:0733b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二明所持之剎二十重。",
        "from": "X07n0234_017:0733b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中最下方有世界(至)佛號普照法界無礙光。",
        "from": "X07n0234_017:0733b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文並可知。",
        "from": "X07n0234_017:0733b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有欲解釋剎中佛名。",
        "from": "X07n0234_017:0733b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "足可留思。",
        "from": "X07n0234_017:0733b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(入第十經)。",
        "from": "X07n0234_017:0733b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△三明十海所管之海。",
        "from": "X07n0234_017:0733b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一海各管不可說佛剎塵數。",
        "from": "X07n0234_017:0733b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現文但各說十。",
        "from": "X07n0234_017:0733b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即為百海。",
        "from": "X07n0234_017:0733b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有剎種。",
        "from": "X07n0234_017:0733b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及所持剎十海。",
        "from": "X07n0234_017:0733b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即為十段。",
        "from": "X07n0234_017:0733b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但記次前十海之名。",
        "from": "X07n0234_017:0733b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此文居然易了。",
        "from": "X07n0234_017:0733b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十段中一一有二。",
        "from": "X07n0234_017:0733b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初標能管之海以定方。",
        "from": "X07n0234_017:0733b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前右旋初海。",
        "from": "X07n0234_017:0733c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時普賢菩薩復告大眾(至)彼離垢燄藏香水海東。",
        "from": "X07n0234_017:0733c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二列所管之海次第。",
        "from": "X07n0234_017:0733c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於所管中各有三節。",
        "from": "X07n0234_017:0733c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初能管海邊。",
        "from": "X07n0234_017:0733c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鄰次列九。",
        "from": "X07n0234_017:0733c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次有香水海名變化微妙身(至)世界種名寶燄燈。",
        "from": "X07n0234_017:0733c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯此一段九中闕一。",
        "from": "X07n0234_017:0733c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二總結一海所管之大數。",
        "from": "X07n0234_017:0733c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等不可說佛剎微塵數香水海。",
        "from": "X07n0234_017:0733c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△三廣最近輪圍一海。",
        "from": "X07n0234_017:0733c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中文皆有二。",
        "from": "X07n0234_017:0733c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初舉海種名體。",
        "from": "X07n0234_017:0733c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其最近輪圍山香水海(至)世界海清淨劫音聲為體。",
        "from": "X07n0234_017:0733c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二所持之剎亦皆二十重。",
        "from": "X07n0234_017:0733c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中超間。",
        "from": "X07n0234_017:0733c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文皆四節。",
        "from": "X07n0234_017:0733c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初舉下層。",
        "from": "X07n0234_017:0733c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中最下方有世界(至)佛號最勝三昧精進慧。",
        "from": "X07n0234_017:0733c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二超至第十。",
        "from": "X07n0234_017:0733c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此上過十佛剎微塵數世界(至)佛號無障礙法界燈。",
        "from": "X07n0234_017:0733c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛幢者。",
        "from": "X07n0234_017:0733c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即中央香海剎種中第十重剎。",
        "from": "X07n0234_017:0733c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△三超至第十三。",
        "from": "X07n0234_017:0733c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此上過三佛剎微塵數世界與娑婆世界齊等。",
        "from": "X07n0234_017:0733c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以等此中央娑婆故。",
        "from": "X07n0234_017:0733c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有世界名放光藏佛號徧法界無障礙慧明。",
        "from": "X07n0234_017:0733c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△四超至第二十。",
        "from": "X07n0234_017:0733c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此上過七佛剎微塵數世界(至)佛號覺分華。",
        "from": "X07n0234_017:0733c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更至第二十重者。",
        "from": "X07n0234_017:0733c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以最上故。",
        "from": "X07n0234_017:0733c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言最上者。",
        "from": "X07n0234_017:0733c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剎種最上。",
        "from": "X07n0234_017:0733c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若二十重最上。",
        "from": "X07n0234_017:0733c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以得此最上之名。",
        "from": "X07n0234_017:0733c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設不欲繁文。",
        "from": "X07n0234_017:0733c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以不加乃至最上耶。",
        "from": "X07n0234_017:0734a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然超間者。",
        "from": "X07n0234_017:0734a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "意存略故。",
        "from": "X07n0234_017:0734a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云齊等者。",
        "from": "X07n0234_017:0734a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恐失次故。",
        "from": "X07n0234_017:0734a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又上下橫豎。",
        "from": "X07n0234_017:0734a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆相當故。",
        "from": "X07n0234_017:0734a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此隨所管海有不可說。",
