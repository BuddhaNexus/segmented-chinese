        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故下但云。",
        "from": "X07n0234_017:0734a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此海之外。",
        "from": "X07n0234_017:0734a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不言右旋等。",
