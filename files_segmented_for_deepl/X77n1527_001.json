[
    {
        "source_sentence": "續武林西湖高僧事略",
        "from": "X77n1527_001:0587a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正集起於晉。",
        "from": "X77n1527_001:0587a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "止於宋元。",
        "from": "X77n1527_001:0587a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自後無續之者。",
        "from": "X77n1527_001:0587a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃補入元季　國初得五師焉。",
        "from": "X77n1527_001:0587a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而中峯本禪師。",
        "from": "X77n1527_001:0587a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相傳今吳山雲居菴有師所著麻履尚存。",
        "from": "X77n1527_001:0587a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然考之行狀誌銘。",
        "from": "X77n1527_001:0587a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及與廣錄。",
        "from": "X77n1527_001:0587a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曾無住持雲居之事。",
        "from": "X77n1527_001:0587a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不敢徇俗增補。",
        "from": "X77n1527_001:0587a10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又正集中。",
        "from": "X77n1527_001:0587a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國一欽禪師始末皆在雙徑。",
        "from": "X77n1527_001:0587a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未見住武林西湖實跡。",
        "from": "X77n1527_001:0587a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "似為可刪。",
        "from": "X77n1527_001:0587a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姑存之以俟再考。",
        "from": "X77n1527_001:0587a12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所補五師。",
        "from": "X77n1527_001:0587a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖一二誕生於元。",
        "from": "X77n1527_001:0587a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而出世涅槃大都在洪永間。",
        "from": "X77n1527_001:0587a13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故獨以　國朝標之。",
        "from": "X77n1527_001:0587a13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嗣有作者。",
        "from": "X77n1527_001:0587a14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可更續云。",
        "from": "X77n1527_001:0587a14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雲棲沙門武林袾宏識續補諸師目錄大明性原朗禪師大明楚石琦禪師大明空谷隆禪師大明毒峯善禪師大明雪庭某禪師續武林西湖高僧事略明雲棲　袾宏　輯大明性原朗禪師師諱慧朗。",
        "from": "X77n1527_001:0587a14_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "字性原。",
        "from": "X77n1527_001:0587b10_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別號幻隱。",
        "from": "X77n1527_001:0587b10_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生於元世台州府之黃巖縣項宅。",
        "from": "X77n1527_001:0587b10_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母陳氏。",
        "from": "X77n1527_001:0587b11_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七歲患疽幾死。",
        "from": "X77n1527_001:0587b11_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置牀上。",
        "from": "X77n1527_001:0587b11_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忽不見。",
        "from": "X77n1527_001:0587b11_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已而求得之。",
        "from": "X77n1527_001:0587b11_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問其故。",
        "from": "X77n1527_001:0587b12_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰。",
        "from": "X77n1527_001:0587b12_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頃睡中有四童子舁至此。",
        "from": "X77n1527_001:0587b12_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無他也。",
        "from": "X77n1527_001:0587b12_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翌日疽愈。",
        "from": "X77n1527_001:0587b13_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識者已知師為天神所祐矣。",
        "from": "X77n1527_001:0587b13_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既長。",
        "from": "X77n1527_001:0587b13_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不甘處俗。",
        "from": "X77n1527_001:0587b13_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃往依永嘉樂清寶冠寺魯山東公出家。",
        "from": "X77n1527_001:0587b14_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後詣徑山。",
        "from": "X77n1527_001:0587b14_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "參元叟端公。",
        "from": "X77n1527_001:0587b15_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端云。",
        "from": "X77n1527_001:0587b15_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東嶺來。",
        "from": "X77n1527_001:0587b15_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西嶺來。",
        "from": "X77n1527_001:0587b15_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師指草鞋云。",
        "from": "X77n1527_001:0587b15_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是三文錢買得。",
        "from": "X77n1527_001:0587b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端云。",
        "from": "X77n1527_001:0587b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未在。",
        "from": "X77n1527_001:0587b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更道。",
        "from": "X77n1527_001:0587b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師云。",
        "from": "X77n1527_001:0587b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "某甲只恁麼。",
        "from": "X77n1527_001:0587b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未審和尚作麼生。",
        "from": "X77n1527_001:0587b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端云。",
        "from": "X77n1527_001:0587b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念汝遠來。",
        "from": "X77n1527_001:0587b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放汝三十棒。",
        "from": "X77n1527_001:0587b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師乃悟其旨。",
        "from": "X77n1527_001:0587b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "久之究盡底蘊。",
        "from": "X77n1527_001:0587b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既而出世鄞之五峯。",
        "from": "X77n1527_001:0587b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　皇明洪武五年。",
        "from": "X77n1527_001:0587b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　詔建佛會於鍾山。",
        "from": "X77n1527_001:0587b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "召高行沙門。",
        "from": "X77n1527_001:0587b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師與季潭泐公與焉。",
        "from": "X77n1527_001:0587b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後住靈隱。",
        "from": "X77n1527_001:0587b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逮十寒暑。",
        "from": "X77n1527_001:0587b20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無何。",
        "from": "X77n1527_001:0587b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遭無妄之災。",
        "from": "X77n1527_001:0587b20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逮捕。",
        "from": "X77n1527_001:0587b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或勸師早自為計。",
        "from": "X77n1527_001:0587b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師不顧。",
        "from": "X77n1527_001:0587b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怡然詣所司。",
        "from": "X77n1527_001:0587b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未鞠即廡下說偈。",
        "from": "X77n1527_001:0587b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端坐而逝。",
        "from": "X77n1527_001:0587b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大明楚石琦禪師",
        "from": "X77n1527_001:0587b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師諱梵琦。",
        "from": "X77n1527_001:0587b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "字楚石。",
        "from": "X77n1527_001:0587b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明州象山人。",
        "from": "X77n1527_001:0587b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朱姓。",
        "from": "X77n1527_001:0587b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母張氏。",
        "from": "X77n1527_001:0587b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元元貞二年丙申六月丁巳。",
        "from": "X77n1527_001:0587b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生在襁褓。",
        "from": "X77n1527_001:0587c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有僧見而謂其父曰。",
        "from": "X77n1527_001:0587c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此兒必當振佛法。",
        "from": "X77n1527_001:0587c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照耀濁世。",
        "from": "X77n1527_001:0587c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九歲從海鹽天寧訥公受業。",
        "from": "X77n1527_001:0587c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十六為僧。",
        "from": "X77n1527_001:0587c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十侍族祖晉翁洵公。",
        "from": "X77n1527_001:0587c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因閱楞嚴至緣見因明。",
        "from": "X77n1527_001:0587c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "暗成無見。",
        "from": "X77n1527_001:0587c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不明自發。",
        "from": "X77n1527_001:0587c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則諸暗相永不能昏。",
        "from": "X77n1527_001:0587c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此有省。",
        "from": "X77n1527_001:0587c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然於向上一著尚有滯礙。",
        "from": "X77n1527_001:0587c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元叟端和尚主徑山。",
        "from": "X77n1527_001:0587c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往參謁。",
        "from": "X77n1527_001:0587c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "X77n1527_001:0587c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如何是言發非聲。",
        "from": "X77n1527_001:0587c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色前不物。",
        "from": "X77n1527_001:0587c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "叟遽云。",
        "from": "X77n1527_001:0587c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言發非聲。",
        "from": "X77n1527_001:0587c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色前不物。",
        "from": "X77n1527_001:0587c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速道速道。",
        "from": "X77n1527_001:0587c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師擬進語。",
        "from": "X77n1527_001:0587c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "叟震威一喝。",
        "from": "X77n1527_001:0587c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師乃錯愕而退。",
        "from": "X77n1527_001:0587c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已而應書寫大藏之選。",
        "from": "X77n1527_001:0587c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至京師崇天門。",
        "from": "X77n1527_001:0587c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一夕聞綵樓上鼓聲。",
        "from": "X77n1527_001:0587c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豁然大悟。",
        "from": "X77n1527_001:0587c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東歸。",
        "from": "X77n1527_001:0587c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "再參元叟。",
        "from": "X77n1527_001:0587c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "叟笑迎曰。",
        "from": "X77n1527_001:0587c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且喜大事了畢。",
        "from": "X77n1527_001:0587c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未幾。",
        "from": "X77n1527_001:0587c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出世海鹽之福臻。",
        "from": "X77n1527_001:0587c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至元乙亥遷杭之報國。",
        "from": "X77n1527_001:0587c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洪武元年九月十一日。",
        "from": "X77n1527_001:0587c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徵師說法於蔣山。",
        "from": "X77n1527_001:0587c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　上大悅。",
        "from": "X77n1527_001:0587c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又明年七月復被召。",
        "from": "X77n1527_001:0587c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十六日偈曰。",
        "from": "X77n1527_001:0587c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真性圓明。",
        "from": "X77n1527_001:0587c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本無生滅。",
        "from": "X77n1527_001:0587c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "木馬夜鳴。",
        "from": "X77n1527_001:0587c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方日出。",
        "from": "X77n1527_001:0587c13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端坐而逝。",
        "from": "X77n1527_001:0587c13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闍維。",
        "from": "X77n1527_001:0587c14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "齒舌數珠不壞。",
        "from": "X77n1527_001:0587c14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有語錄二十卷及西齋淨土詩行世。",
        "from": "X77n1527_001:0587c14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大明空谷隆禪師",
        "from": "X77n1527_001:0587c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師諱景隆。",
        "from": "X77n1527_001:0587c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "字祖庭。",
        "from": "X77n1527_001:0587c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號空谷。",
        "from": "X77n1527_001:0587c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姑蘇洞庭[元/?]山陳氏子。",
        "from": "X77n1527_001:0587c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父月潭居士。",
        "from": "X77n1527_001:0587c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母金氏。",
        "from": "X77n1527_001:0587c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生於洪武癸酉七月十二日。",
        "from": "X77n1527_001:0587c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為童時不茹葷。",
        "from": "X77n1527_001:0587c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "趺坐若禪定。",
        "from": "X77n1527_001:0587c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永樂壬辰。",
        "from": "X77n1527_001:0587c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從弁山白蓮懶雲和尚受學參禪。",
        "from": "X77n1527_001:0587c19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖家居。",
        "from": "X77n1527_001:0587c20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而湖海禪伯如古拙和尚輩靡不參謁。",
        "from": "X77n1527_001:0587c20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庚子出家虎丘。",
        "from": "X77n1527_001:0587c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宣德二年詣杭州昭慶受戒。",
        "from": "X77n1527_001:0587c21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂依師住靈隱七年。",
        "from": "X77n1527_001:0587c22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往天目禮祖塔。",
        "from": "X77n1527_001:0587c22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憩錫一載。",
        "from": "X77n1527_001:0587c22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刻苦參究。",
        "from": "X77n1527_001:0587c22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忽有省。",
        "from": "X77n1527_001:0587c23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因造懶雲剖露。",
        "from": "X77n1527_001:0587c23_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雲印可之。",
        "from": "X77n1527_001:0587c23_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懶雲。",
        "from": "X77n1527_001:0587c23_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨濟下二十四世也。",
        "from": "X77n1527_001:0587c23_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師所著有空谷集三十卷。",
        "from": "X77n1527_001:0587c24_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心宗洞達。",
        "from": "X77n1527_001:0587c24_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "機辨峻㧞。",
        "from": "X77n1527_001:0588a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儒釋通貫。",
        "from": "X77n1527_001:0588a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "事理交融。",
        "from": "X77n1527_001:0588a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大理卿吳公誌之。",
        "from": "X77n1527_001:0588a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師存年五十二時。",
        "from": "X77n1527_001:0588a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自作塔銘於武林西湖之修吉山。",
        "from": "X77n1527_001:0588a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "院名正傳。",
        "from": "X77n1527_001:0588a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有正傳十景之詠在集。",
        "from": "X77n1527_001:0588a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大明毒峯善禪師",
        "from": "X77n1527_001:0588a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師諱本善。",
        "from": "X77n1527_001:0588a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祖鳳陽人。",
        "from": "X77n1527_001:0588a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨任生於廣東之雷陽。",
        "from": "X77n1527_001:0588a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父姓吳。",
        "from": "X77n1527_001:0588a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母鍾氏。",
        "from": "X77n1527_001:0588a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稚小以佛事為兒戲。",
        "from": "X77n1527_001:0588a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十七歲出家。",
        "from": "X77n1527_001:0588a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初投源明和尚請益。",
        "from": "X77n1527_001:0588a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明示以無字公案。",
        "from": "X77n1527_001:0588a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正統八年入閩參無際師。",
        "from": "X77n1527_001:0588a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞鐘聲有省。",
        "from": "X77n1527_001:0588a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次見蒙隱。",
        "from": "X77n1527_001:0588a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次見楚山。",
        "from": "X77n1527_001:0588a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次見月溪。",
        "from": "X77n1527_001:0588a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "溪印可之。",
        "from": "X77n1527_001:0588a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從前打七掩關。",
        "from": "X77n1527_001:0588a09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曉夜無怠。",
        "from": "X77n1527_001:0588a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歷盡百倍苦功。",
        "from": "X77n1527_001:0588a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃得大徹。",
        "from": "X77n1527_001:0588a09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復蒙古庭重授衣拂。",
        "from": "X77n1527_001:0588a10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天順庚辰。",
        "from": "X77n1527_001:0588a10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "趙氏建西湖三塔寺。",
        "from": "X77n1527_001:0588a10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請師開山。",
        "from": "X77n1527_001:0588a11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繼興天目昭明。",
        "from": "X77n1527_001:0588a11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繼興吳山寶蓮。",
        "from": "X77n1527_001:0588a11_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繼興南山甘露。",
        "from": "X77n1527_001:0588a11_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成化庚子。",
        "from": "X77n1527_001:0588a12_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掩關石屋寺。",
        "from": "X77n1527_001:0588a12_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壬寅。",
        "from": "X77n1527_001:0588a12_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈雲嶺有山曰登雲。",
        "from": "X77n1527_001:0588a12_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寺曰天真。",
        "from": "X77n1527_001:0588a13_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗綱請師興建。",
        "from": "X77n1527_001:0588a13_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "事峻。",
        "from": "X77n1527_001:0588a13_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即掩關杜人事。",
        "from": "X77n1527_001:0588a13_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "涵養陶汰。",
        "from": "X77n1527_001:0588a14_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有四十餘年祇掩關之句。",
        "from": "X77n1527_001:0588a14_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "示寂後。",
        "from": "X77n1527_001:0588a14_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "門人悟深戒新等。",
        "from": "X77n1527_001:0588a14_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輯師言論行實名昭明甘露天真三處語要錄云。",
        "from": "X77n1527_001:0588a15_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大明雪庭某禪師",
        "from": "X77n1527_001:0588a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師號雪庭。",
        "from": "X77n1527_001:0588a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一號梅雪隱人。",
        "from": "X77n1527_001:0588a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "籍貫浙省杭州府仁和。",
        "from": "X77n1527_001:0588a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父姓桂。",
        "from": "X77n1527_001:0588a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名徵。",
        "from": "X77n1527_001:0588a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母徐氏。",
        "from": "X77n1527_001:0588a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昆仲三人。",
        "from": "X77n1527_001:0588a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師最少。",
        "from": "X77n1527_001:0588a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以景泰丙子生。",
        "from": "X77n1527_001:0588a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毀齒喪父。",
        "from": "X77n1527_001:0588a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "患痘風。",
        "from": "X77n1527_001:0588a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因得目疾。",
        "from": "X77n1527_001:0588a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數求出家。",
        "from": "X77n1527_001:0588a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十五尋師。",
        "from": "X77n1527_001:0588a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯益邪解。",
        "from": "X77n1527_001:0588a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成化癸巳。",
        "from": "X77n1527_001:0588a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "參休休翁於郡城仙林寺。",
        "from": "X77n1527_001:0588a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一見契合。",
        "from": "X77n1527_001:0588a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受無字公案。",
        "from": "X77n1527_001:0588a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十七祝髮。",
        "from": "X77n1527_001:0588a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日夜研究。",
        "from": "X77n1527_001:0588a22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滯沈寂之境。",
        "from": "X77n1527_001:0588a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "座元某勉使看教。",
        "from": "X77n1527_001:0588a22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因閱楞嚴至一毫端上現寶王剎有疑。",
        "from": "X77n1527_001:0588a23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後在江陰乾明寺。",
        "from": "X77n1527_001:0588a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忽覩萬佛閣金碧崢嶸於眉宇間。",
        "from": "X77n1527_001:0588a24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偶會得毛端現剎之句。",
        "from": "X77n1527_001:0588b01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弘治改元除夕。",
        "from": "X77n1527_001:0588b01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞鐘聲。",
        "from": "X77n1527_001:0588b01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數年行履忽爾活脫。",
        "from": "X77n1527_001:0588b01_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信口偈云。",
        "from": "X77n1527_001:0588b02_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓響心非聞。",
        "from": "X77n1527_001:0588b02_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大千同一照。",
        "from": "X77n1527_001:0588b02_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "抹過上頭關。",
        "from": "X77n1527_001:0588b03_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更不存玄妙。",
        "from": "X77n1527_001:0588b03_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乙卯。",
        "from": "X77n1527_001:0588b03_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又隨休休翁於淨慈寺。",
        "from": "X77n1527_001:0588b03_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃蒙印可。",
        "from": "X77n1527_001:0588b04_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所著有請益警進拈古頌古擬寒山和永明詩偈等。",
        "from": "X77n1527_001:0588b04_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡二十卷。",
        "from": "X77n1527_001:0588b05_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號幻寄集行世。",
        "from": "X77n1527_001:0588b05_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "續武林西湖高僧事略",
        "from": "X77n1527_001:0588b07_0",
        "context_with_next_sentence": false
    }
]