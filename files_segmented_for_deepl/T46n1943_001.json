[
    {
        "source_sentence": "略法華三昧補助儀并序",
        "from": "T46n1943_001:0956c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(由此懺法隨事觀想。",
        "from": "T46n1943_001:0956c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並指法華三昧補助儀文。",
        "from": "T46n1943_001:0956c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今恐各行無處尋。",
        "from": "T46n1943_001:0956c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "檢翻成所闕故。",
        "from": "T46n1943_001:0956c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從要急略彼辭句。",
        "from": "T46n1943_001:0956c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "附此文後以備承用。",
        "from": "T46n1943_001:0956c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請對前事儀之下。",
        "from": "T46n1943_001:0956c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運此觀想故。",
        "from": "T46n1943_001:0956c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此但令運想。",
        "from": "T46n1943_001:0956c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不錄事相。",
        "from": "T46n1943_001:0956c07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用者應知)初運香華(運想曰。",
        "from": "T46n1943_001:0956c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願此香華遍十方。",
        "from": "T46n1943_001:0956c08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為微妙光明臺。",
        "from": "T46n1943_001:0956c08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天音樂天寶香。",
        "from": "T46n1943_001:0956c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天餚饍天寶衣。",
        "from": "T46n1943_001:0956c08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可思議妙法塵。",
        "from": "T46n1943_001:0956c08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一塵出一切塵。",
        "from": "T46n1943_001:0956c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一塵出一切法。",
        "from": "T46n1943_001:0956c09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旋轉無礙互莊嚴。",
        "from": "T46n1943_001:0956c09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍至一切三寶前。",
        "from": "T46n1943_001:0956c09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方法界三寶前。",
        "from": "T46n1943_001:0956c09_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆有我身修供養。",
        "from": "T46n1943_001:0956c09_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一皆悉遍法界。",
        "from": "T46n1943_001:0956c09_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼彼無雜無障閡。",
        "from": "T46n1943_001:0956c10_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡未來際作佛事普熏法界。",
        "from": "T46n1943_001:0956c10_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸眾生蒙熏皆發菩提心。",
        "from": "T46n1943_001:0956c10_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同入無生證佛智)請佛(我三業性如虛空。",
        "from": "T46n1943_001:0956c10_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋迦牟尼亦如是。",
        "from": "T46n1943_001:0956c11_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不起真際為眾生與眾俱來受供養)諸佛菩薩(但改牟尼及形相來處)請法(法性如空不可見。",
        "from": "T46n1943_001:0956c11_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常住法寶難思議。",
        "from": "T46n1943_001:0956c12_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今三業如法請。",
        "from": "T46n1943_001:0956c12_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願影現受供養)禮佛(能禮所禮性空寂。",
        "from": "T46n1943_001:0956c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "感應道交難思議。",
        "from": "T46n1943_001:0956c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我此道場如帝珠。",
        "from": "T46n1943_001:0956c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋迦牟尼影現中。",
        "from": "T46n1943_001:0956c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今影現牟尼前。",
        "from": "T46n1943_001:0956c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭面接足歸命禮)餘佛菩薩(但改牟尼為異)禮法(真空法性如虛空。",
        "from": "T46n1943_001:0956c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常住法寶難思議。",
        "from": "T46n1943_001:0956c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我身影現法寶前。",
        "from": "T46n1943_001:0956c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫不皆悉歸命禮)普為(我及眾生云云)懺悔前運逆順十心(我與眾生云云)。",
        "from": "T46n1943_001:0956c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略法華三昧補助儀",
        "from": "T46n1943_001:0956c17_0",
        "context_with_next_sentence": false
    }
]