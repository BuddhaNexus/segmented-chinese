[
    {
        "source_sentence": "大方廣菩薩藏文殊師利根本儀",
        "from": "T20n1191_004:0848c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "軌經卷第四",
        "from": "T20n1191_004:0848c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試鴻臚少卿",
        "from": "T20n1191_004:0848c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明教大師臣天息災奉　詔譯",
        "from": "T20n1191_004:0848c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩變化儀軌品第二之一",
        "from": "T20n1191_004:0848c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時妙吉祥童子。",
        "from": "T20n1191_004:0848c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀察會中一切大眾。",
        "from": "T20n1191_004:0848c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是妙吉祥童子。",
        "from": "T20n1191_004:0848c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入觀照三摩地。",
        "from": "T20n1191_004:0848c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入此定已。",
        "from": "T20n1191_004:0848c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從其臍輪出大光明。",
        "from": "T20n1191_004:0848c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有無數百千那由他俱胝光明以為眷屬。",
        "from": "T20n1191_004:0848c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普遍照耀一切眾生界及淨光天。",
        "from": "T20n1191_004:0848c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時金剛手菩薩摩訶薩。",
        "from": "T20n1191_004:0848c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告妙吉祥童子言。",
        "from": "T20n1191_004:0848c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今宣說曼拏羅法品儀軌。",
        "from": "T20n1191_004:0848c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸菩薩及一切眾生得入三昧。",
        "from": "T20n1191_004:0848c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復令一切眾生平等悟解真言句。",
        "from": "T20n1191_004:0848c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有世間出世間一切真言行皆得成就。",
        "from": "T20n1191_004:0848c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛手祕密主如是說已。",
        "from": "T20n1191_004:0848c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時妙吉祥童子。",
        "from": "T20n1191_004:0848c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲說最上祕密曼拏羅儀軌。",
        "from": "T20n1191_004:0848c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明照一切以神變力。",
        "from": "T20n1191_004:0848c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其右手指端之上。",
        "from": "T20n1191_004:0848c26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆現起一切大眾。",
        "from": "T20n1191_004:0848c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復出無數百千那由他俱胝光明。",
        "from": "T20n1191_004:0848c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照彼淨光天上一切眾會。",
        "from": "T20n1191_004:0848c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時妙吉祥童子。",
        "from": "T20n1191_004:0848c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略說焰曼德迦忿怒明王心真言具大無畏。",
        "from": "T20n1191_004:0848c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切所作請召發遣。",
        "from": "T20n1191_004:0849a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "息除災害增長吉祥。",
        "from": "T20n1191_004:0849a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降伏諸魔隱沒身形。",
        "from": "T20n1191_004:0849a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行坐虛空。",
        "from": "T20n1191_004:0849a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或行地上足不履地。",
        "from": "T20n1191_004:0849a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令他愛敬等皆得成就。",
        "from": "T20n1191_004:0849a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破一切暗如廣大明燈。",
        "from": "T20n1191_004:0849a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大無畏三字真言。",
        "from": "T20n1191_004:0849a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儀行法力最為第一。",
        "from": "T20n1191_004:0849a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求所作一切真言義悉得成就。",
        "from": "T20n1191_004:0849a05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說大忿怒明王心真言。",
        "from": "T20n1191_004:0849a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)惡吽(引)",
        "from": "T20n1191_004:0849a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大忿怒明王心真言。",
        "from": "T20n1191_004:0849a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大菩薩妙吉祥善說曼拏羅真言行。",
        "from": "T20n1191_004:0849a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能作一切事破一切障礙。",
        "from": "T20n1191_004:0849a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時妙吉祥童子。",
        "from": "T20n1191_004:0849a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉其右手摩忿怒明王頭。",
        "from": "T20n1191_004:0849a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱如是言曩摩一切佛。",
        "from": "T20n1191_004:0849a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是說已。",
        "from": "T20n1191_004:0849a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有十方世界諸佛世尊無量菩薩。",
        "from": "T20n1191_004:0849a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具大福德三昧來住會中。",
        "from": "T20n1191_004:0849a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時忿怒明王復現大忿怒相。",
        "from": "T20n1191_004:0849a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即往一切世界剎土之中。",
        "from": "T20n1191_004:0849a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有惡心大力眾生。",
        "from": "T20n1191_004:0849a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尋遣來集淨光天上大眾之中。",
        "from": "T20n1191_004:0849a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依位安住。",
        "from": "T20n1191_004:0849a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此眾生頂上各出熾盛光焰。",
        "from": "T20n1191_004:0849a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時妙吉祥童子。",
        "from": "T20n1191_004:0849a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀彼大眾心淨。",
        "from": "T20n1191_004:0849a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞻仰而復告言。",
        "from": "T20n1191_004:0849a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此諸大眾汝當諦聽。",
        "from": "T20n1191_004:0849a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我此三昧若不依行如有違犯。",
        "from": "T20n1191_004:0849a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令此忿怒明王現大威力而自調伏。",
        "from": "T20n1191_004:0849a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何為不得違犯。",
        "from": "T20n1191_004:0849a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣此祕密三昧真言行義諸佛世尊菩薩大德平等法門。",
        "from": "T20n1191_004:0849a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今諦聽善思念之。",
        "from": "T20n1191_004:0849a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當為汝說。",
        "from": "T20n1191_004:0849a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱(引)喃(引)唵(引)囉囉三麼(二合)囉阿鉢囉(二合)底賀多舍(引)娑曩俱摩囉嚕波馱哩抳吽吽[口*癹]吒[口*癹]吒薩嚩(二合引)賀聖者此是我根本真言聖妙吉祥印。",
        "from": "T20n1191_004:0849a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為五髻大印。",
        "from": "T20n1191_004:0849a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若用此印持誦根本真言。",
        "from": "T20n1191_004:0849a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切所求皆得成就。",
        "from": "T20n1191_004:0849a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說心真言。",
        "from": "T20n1191_004:0849a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作一切善事令心寂靜。",
        "from": "T20n1191_004:0849a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩枳也(二合)捺曩莫(入)",
        "from": "T20n1191_004:0849b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此印法名為三髻。",
        "from": "T20n1191_004:0849b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增長一切福德。",
        "from": "T20n1191_004:0849b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說外心真言。",
        "from": "T20n1191_004:0849b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚩枳也(二合)吽",
        "from": "T20n1191_004:0849b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此印法亦名三髻。",
        "from": "T20n1191_004:0849b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能降伏一切眾生。",
        "from": "T20n1191_004:0849b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說內心微妙真言。",
        "from": "T20n1191_004:0849b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱喃曼",
        "from": "T20n1191_004:0849b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此印法名孔雀座。",
        "from": "T20n1191_004:0849b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬愛一切眾生。",
        "from": "T20n1191_004:0849b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有一切佛心大無畏八字真言。",
        "from": "T20n1191_004:0849b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名最上增益大吉祥。",
        "from": "T20n1191_004:0849b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷三界生死。",
        "from": "T20n1191_004:0849b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "消除一切惡趣。",
        "from": "T20n1191_004:0849b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能滅一切災害。",
        "from": "T20n1191_004:0849b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作一切事皆得安樂。",
        "from": "T20n1191_004:0849b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寂靜如現在見佛。",
        "from": "T20n1191_004:0849b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此妙吉祥菩薩。",
        "from": "T20n1191_004:0849b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宣布最上祕密真言相。",
        "from": "T20n1191_004:0849b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為一切眾生。",
        "from": "T20n1191_004:0849b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有憶念一切所願皆得圓滿。",
        "from": "T20n1191_004:0849b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有持誦之者。",
        "from": "T20n1191_004:0849b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有五逆重罪皆得清淨。",
        "from": "T20n1191_004:0849b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說真言曰。",
        "from": "T20n1191_004:0849b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿尾囉吽佉左洛",
        "from": "T20n1191_004:0849b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖者有此八字大無畏最上祕密心真言。",
        "from": "T20n1191_004:0849b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與佛住世而無有異。",
        "from": "T20n1191_004:0849b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能作一切吉祥之事。",
        "from": "T20n1191_004:0849b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大功德我今為汝略而宣說。",
        "from": "T20n1191_004:0849b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲廣陳。",
        "from": "T20n1191_004:0849b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經無數百千那由他俱胝劫。",
        "from": "T20n1191_004:0849b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "校量功德說不能盡。",
        "from": "T20n1191_004:0849b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若說此印法。",
        "from": "T20n1191_004:0849b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為大精進能圓滿一切願。",
        "from": "T20n1191_004:0849b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說召請眾聖真言。",
        "from": "T20n1191_004:0849b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)呬(引)呬(引)俱摩囉尾濕嚩(二合)嚕閉尼薩哩嚩(二合)嚩(引)羅婆史哆鉢囉(二合)冒馱儞(引)阿野(引)呬婆誐挽曩(引)野呬俱摩囉吉哩(二合)拏怛婆(二合)羅馱哩尼曼拏羅末地野(二合)底瑟姹(二合)底瑟姹(二合)三摩野摩拏娑摩(二合)囉阿鉢囉(二合)底賀哆舍娑曩吽摩(引)尾羅嚩嚕嚕頗吒娑嚩(二合)賀此真言是妙吉祥菩薩。",
        "from": "T20n1191_004:0849b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "召請一切諸佛一切菩薩。",
        "from": "T20n1191_004:0849c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切辟支佛聲聞。",
        "from": "T20n1191_004:0849c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天龍夜叉乾闥婆阿修羅誐嚕拏緊那羅摩睺羅伽。",
        "from": "T20n1191_004:0849c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘舍左羅剎娑一切部多等。",
        "from": "T20n1191_004:0849c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫欲召請。",
        "from": "T20n1191_004:0849c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以香水加持七遍。",
        "from": "T20n1191_004:0849c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灑淨一切廣闊四維上下。",
        "from": "T20n1191_004:0849c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸佛菩薩妙吉祥。",
        "from": "T20n1191_004:0849c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并及眷屬一切世間出世間真言一切部多眾。",
        "from": "T20n1191_004:0849c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生皆赴道場。",
        "from": "T20n1191_004:0849c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱(引)喃(引)阿鉢囉(二合)底賀哆舍(引)娑曩喃(引)唵度度囉度囉度波嚩(引)悉儞度波(引)哩唧(二合)史吽底瑟姹(二合)三摩野摩努娑摩(二合)囉娑嚩(二合引)賀此是獻香真言。",
        "from": "T20n1191_004:0849c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若以白栴檀龍腦供俱摩香和合等。",
        "from": "T20n1191_004:0849c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒此香時誦此真言。",
        "from": "T20n1191_004:0849c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來及諸菩薩。",
        "from": "T20n1191_004:0849c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切聖眾皆受供養。",
        "from": "T20n1191_004:0849c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前召請印名最上蓮華鬘。",
        "from": "T20n1191_004:0849c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能與一切眾生作大吉祥之事。",
        "from": "T20n1191_004:0849c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於諸佛如來一切菩薩及聖眾等。",
        "from": "T20n1191_004:0849c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獻閼伽水。",
        "from": "T20n1191_004:0849c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所用龍腦白檀供俱摩等香。",
        "from": "T20n1191_004:0849c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復用惹帝華適意華摩梨迦華。",
        "from": "T20n1191_004:0849c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "末哩師迦華龍華。",
        "from": "T20n1191_004:0849c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "末俱羅華。",
        "from": "T20n1191_004:0849c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶尼哆誐囉華。",
        "from": "T20n1191_004:0849c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等香華用淹水內。",
        "from": "T20n1191_004:0849c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此名獻閼伽水。",
        "from": "T20n1191_004:0849c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獻水真言。",
        "from": "T20n1191_004:0849c21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱(引)喃(引)摩鉢囉(二合)底賀哆舍(引)娑曩(引)喃(引)怛儞也(二合)他(引)呬(引)呬(引)摩賀(引)迦(引)嚕尼迦尾濕嚩(二合)嚕波馱哩尼(引)阿哩凝伽(二合)鉢囉(二合)底蹉鉢囉(二合)底蹉波野三摩野摩努娑摩(二合)囉底瑟姹(二合)底瑟姹(二合)曼拏羅末地也(二合)鉢囉(二合)吠舍野薩哩嚩(二合)部哆(引)努波迦仡哩(二合)恨拏(二合)吽阿蘇囉尾左(引)哩尼娑嚩(二合)賀(引)此印名圓滿印。",
        "from": "T20n1191_004:0850a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能為一切眾生作圓滿事。",
        "from": "T20n1191_004:0850a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說焚香真言。",
        "from": "T20n1191_004:0850a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若彼獻香如前真言。",
        "from": "T20n1191_004:0850a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱(引)喃(引)曩謨三滿哆巘馱(引)嚩娑(引)娑室哩(二合)夜(引)野怛他(引)誐哆(引)野怛儞也(二合)巘第巘第(引)巘第(引)巘馱(引)巘馱摩拏囉弭(引)鉢囉(二合)底蹉鉢囉(二合)底砌(引)難巘淡三滿哆(引)努左(引)哩尼(引)娑嚩(二合)賀(引)此印名波羅嚩。",
        "from": "T20n1191_004:0850a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓滿一切願。",
        "from": "T20n1191_004:0850a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說獻華真言。",
        "from": "T20n1191_004:0850a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱(引)喃(引)摩鉢囉(二合)底賀哆舍(引)娑曩喃(引)曩謨三俱蘇弭哆囉(引)惹寫怛他(引)誐哆寫怛儞也(二合)他(引)俱蘇弭(引)俱蘇摩(引)儞曳(二合)俱蘇摩布囉嚩(引)悉儞俱蘇摩(引)嚩底娑嚩(二合)賀(引)夫欲獻食。",
        "from": "T20n1191_004:0850a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先頂禮一切諸佛賢聖。",
        "from": "T20n1191_004:0850a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生不思議未曾有想。",
        "from": "T20n1191_004:0850a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此真言正覺正說。",
        "from": "T20n1191_004:0850a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後所有供獻。",
        "from": "T20n1191_004:0850a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆依此儀念此真言。",
        "from": "T20n1191_004:0850a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱(引)喃(引)摩鉢囉(二合)底賀哆舍娑曩(引)喃(引)怛儞也(二合)他(引)呬(引)呬(引)婆誐嚩摩賀薩哩嚩(二合)沒馱(引)嚩路(引)吉哆摩(引)尾楞嚩伊難嚩隷誐哩(二合)恨拏(二合引)播野誐哩(二合)恨拏(二合)誐哩(二合)吽薩哩嚩(二合)尾濕嚩(二合)囉囉吒吒薩頗(二合)吒娑嚩(二合引)賀(引)今此真言印名為大力。",
        "from": "T20n1191_004:0850a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "消除一切諸惡。",
        "from": "T20n1191_004:0850b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燃燈真言",
        "from": "T20n1191_004:0850b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱(引)喃(引)摩鉢囉(二合)底賀哆舍(引)娑曩(引)喃(引)薩哩嚩(二合)怛夢(引)馱迦(引)囉尾特鑁(二合)悉喃(引)曩謨三滿哆祖底巘馱(引)嚩婆(引)娑室哩(二合)夜(引)野怛他(引)誐哆(引)野怛儞也(二合)他(引)呬(引)呬(引)婆誐鑁祖底囉舍弭(二合)設哆娑賀娑囉(二合)鉢囉(二合)底曼尼哆舍哩囉尾俱哩嚩(二合)摩賀(引)冒地薩怛嚩(二合)三滿哆入嚩(二合)囉(引)儞喻(二合)底哆沒哩底(二合)具哩那(二合)具哩那(二合)阿嚩路(引)迦野阿嚩路迦野曼拏羅薩哩嚩(二合)薩怛嚩(二合)難左此是燃燈真言印名曰廣開。",
        "from": "T20n1191_004:0850b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀照一切眾生。",
        "from": "T20n1191_004:0850b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱(引)喃(引)摩鉢囉(二合)底賀哆舍(引)娑曩(引)喃(引)怛儞也(二合)他(引)入嚩(二合)入嚩(二合)羅入嚩(二合)羅入嚩(二合)羅野入嚩(二合)羅野吽尾冒馱迦賀哩訖哩(二合)瑟拏(二合)賓誐羅此是燃火真言印。",
        "from": "T20n1191_004:0850b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名合掌光照一切眾生過去諸佛菩薩所說。",
        "from": "T20n1191_004:0850b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時妙吉祥童子。",
        "from": "T20n1191_004:0850b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告祕密主金剛手菩薩言。",
        "from": "T20n1191_004:0850b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此微妙祕密真言。",
        "from": "T20n1191_004:0850b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等明王族部。",
        "from": "T20n1191_004:0850b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外現忿怒內含慈忍。",
        "from": "T20n1191_004:0850b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有智者求一切真言皆得成就。",
        "from": "T20n1191_004:0850b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若彼金剛蓮華族等障礙之時。",
        "from": "T20n1191_004:0850b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說此明令彼降伏。",
        "from": "T20n1191_004:0850b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨薩哩嚩(二合)沒馱(引)喃(引)摩鉢囉(二合)底賀哆舍(引)娑曩(引)喃(引)怛儞也(二合)他(引)唵迦囉迦囉俱嚕俱嚕摩摩(引)迦(引)哩焰(二合)伴惹伴惹薩哩嚩(二合)尾近難(二合引)那賀那賀薩哩嚩(二合)嚩日羅(二合)尾那野劍(引)布哩嚩(二合)吒迦[口*爾]尾旦哆迦囉摩賀(引)尾訖哩(二合)哆嚕波馱哩尼鉢左鉢左薩哩嚩(二合)努瑟吒(二合)摩賀(引)誐拏鉢底[口*爾]尾旦哆迦囉滿馱滿馱薩哩嚩(二合)誐囉(二合)賀沙目佉沙部惹沙左囉拏嚕捺囉(二合)摩(引)曩野尾瑟拏(二合)摩(引)曩野沒囉(二合)憾摩(二合)儞也(二合)禰嚩(引)曩曩野摩(引)尾楞嚩尾楞嚩羅護羅護曼拏羅末他也(二合)鉢囉(二合)吠舍野三摩野摩努娑摩(二合)囉吽吽吽吽吽吽頗吒頗吒說此真言已告祕密主言。",
        "from": "T20n1191_004:0850b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是大精進最上祕密。",
        "from": "T20n1191_004:0851a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名六面大忿怒明王。",
        "from": "T20n1191_004:0851a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能破障礙。",
        "from": "T20n1191_004:0851a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若持誦此明得自在。",
        "from": "T20n1191_004:0851a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十地菩薩猶可降伏。",
        "from": "T20n1191_004:0851a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況諸惡障礙。",
        "from": "T20n1191_004:0851a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有持誦供養大作擁護。",
        "from": "T20n1191_004:0851a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此印法名為大叉。",
        "from": "T20n1191_004:0851a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破滅一切障礙。",
        "from": "T20n1191_004:0851a06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說忿怒明王心真言。",
        "from": "T20n1191_004:0851a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)仡哩(二合)瑟致哩(二合)哆(引)曩曩吽薩哩嚩(二合)設怛嚕(二合)曩舍野薩旦(二合)婆野頗吒頗吒若有一切冤家逼害惱亂之時。",
        "from": "T20n1191_004:0851a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依法念此真言。",
        "from": "T20n1191_004:0851a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼冤家受隔四日瘧病得大苦惱。",
        "from": "T20n1191_004:0851a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人恒常誦持。",
        "from": "T20n1191_004:0851a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無慈悲心所求之事不得成就。",
        "from": "T20n1191_004:0851a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人不信三寶。",
        "from": "T20n1191_004:0851a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此呪法令彼生信定得成就。",
        "from": "T20n1191_004:0851a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印名大叉。",
        "from": "T20n1191_004:0851a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與呪同用其驗剋成。",
        "from": "T20n1191_004:0851a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說外心真言。",
        "from": "T20n1191_004:0851a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵仡哩(二合)迦羅嚕波吽欠",
        "from": "T20n1191_004:0851a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印名大叉若此明同用。",
        "from": "T20n1191_004:0851a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能調伏一切惡者。",
        "from": "T20n1191_004:0851a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說內心真言名為一字。",
        "from": "T20n1191_004:0851a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切佛說。",
        "from": "T20n1191_004:0851a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印名大叉與明同用。",
        "from": "T20n1191_004:0851a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能消除一切惡事。",
        "from": "T20n1191_004:0851a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降伏一切部多。",
        "from": "T20n1191_004:0851a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於曼拏羅中呪法成就之時。",
        "from": "T20n1191_004:0851a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定見忿怒明王靈驗。",
        "from": "T20n1191_004:0851a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說發遣賢聖等真言。",
        "from": "T20n1191_004:0851a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨薩哩嚩沒馱(引)喃(引)摩鉢囉(二合)底賀哆舍娑曩(引)喃(引)怛儞也(二合)他(引)惹焰惹野蘇惹野摩賀迦(引)嚕尼迦尾室左(二合)嚕閉尼(引)誐蹉誐蹉娑嚩(二合)婆嚩喃(引)薩哩嚩(二合)沒淡(引)室左(二合)尾娑哩惹(二合)野薩波哩嚩(引)囉(引)娑嚩(二合)婆嚩喃左怛囉(二合)鉢囉(二合)吠(引)舍野滿怛囉(二合)娑摩(二合)囉薩哩嚩(二合)室左(二合)弭悉殿覩滿怛囉(二合)播那(引)摩努囉淡左弭波哩布囉野此發遣真言印。",
        "from": "T20n1191_004:0851a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名寶座亦名獻座。",
        "from": "T20n1191_004:0851b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲發遣賢聖等。",
        "from": "T20n1191_004:0851b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "專心志意念一七遍依法加持。",
        "from": "T20n1191_004:0851b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有一切世間出世間曼拏羅真言行。",
        "from": "T20n1191_004:0851b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆得三昧成就。",
        "from": "T20n1191_004:0851b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸聖眾等歡喜而退。",
        "from": "T20n1191_004:0851b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時妙吉祥童子。",
        "from": "T20n1191_004:0851b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復觀淨光天內一切大眾。",
        "from": "T20n1191_004:0851b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說自己明王真言。",
        "from": "T20n1191_004:0851b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨薩哩嚩(二合)沒馱(引)喃(引)摩鉢囉(二合)底賀哆誐底喃(引)唵儞哩致妙吉祥說此真言已。",
        "from": "T20n1191_004:0851b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化為明王名枳世儞。",
        "from": "T20n1191_004:0851b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能作一切之事。",
        "from": "T20n1191_004:0851b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印名大五髻。",
        "from": "T20n1191_004:0851b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若用此印法。",
        "from": "T20n1191_004:0851b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切難作之事皆得成就。",
        "from": "T20n1191_004:0851b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱(引)喃(引)摩鉢囉(二合)底賀哆舍娑曩(引)喃(引)唵(引)儞致此真言名塢波枳世儞。",
        "from": "T20n1191_004:0851b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印名廣開。",
        "from": "T20n1191_004:0851b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切之事所求皆成。",
        "from": "T20n1191_004:0851b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有一切凶惡宿耀自然退散。",
        "from": "T20n1191_004:0851b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱(引)喃(引)摩鉢囉(二合)底賀哆誐底喃(引)唵(引)儞(入)此真言名曩隷儞。",
        "from": "T20n1191_004:0851b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印名寶座。",
        "from": "T20n1191_004:0851b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能成一切之事。",
        "from": "T20n1191_004:0851b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有夜叉之眾亦能遣來。",
        "from": "T20n1191_004:0851b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱(引)喃(引)摩進部哆嚕閉赧(引)唵(引)稍罩嚕(二合)此真言名波隷。",
        "from": "T20n1191_004:0851b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儞印名大力。",
        "from": "T20n1191_004:0851b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能調伏一切拏枳儞鬼。",
        "from": "T20n1191_004:0851b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙吉祥所說一切諸佛同說。",
        "from": "T20n1191_004:0851b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱(引)喃(引)摩鉢囉(二合)底賀哆誐底鉢囉(二合)左(引)哩赧(引)怛儞也(二合)他(引)唵(引)嚩囉禰(引)此明名增長化相。",
        "from": "T20n1191_004:0851b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印名三髻。",
        "from": "T20n1191_004:0851c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持誦同用速得富貴。",
        "from": "T20n1191_004:0851c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱(引)喃(引)摩進底也(二合引)部哆嚕閉拏(引)唵(引)部哩印名叉。",
        "from": "T20n1191_004:0851c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同用消除一切瘧病。",
        "from": "T20n1191_004:0851c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱(引)喃(引)摩進底也(二合引)部哆嚕閉赧(引)唵(引)怛囉(二合引)哩此明名哆囉。",
        "from": "T20n1191_004:0851c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印名大力。",
        "from": "T20n1191_004:0851c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能成一切事復能滅除障礙。",
        "from": "T20n1191_004:0851c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱(引)喃(引)摩進底也(二合引)部哆嚕閉赧(引)怛儞也(二合)他(引)唵尾路枳儞此明名大世。",
        "from": "T20n1191_004:0851c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印名嚩訖怛囉。",
        "from": "T20n1191_004:0851c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降伏一切世間皆得快樂。",
        "from": "T20n1191_004:0851c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱(引)喃(引)摩進底也(二合)部哆嚕閉赧(引)怛儞也(二合)他(引)唵尾濕吠(二合引)尾濕嚩(二合)三婆吠(引)尾濕嚩(二合)嚕閉尼迦賀迦賀阿尾舍(引)尾舍三摩野摩努此明名大精進。",
        "from": "T20n1191_004:0851c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印名能瑟吒囉。",
        "from": "T20n1191_004:0852a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名變化。",
        "from": "T20n1191_004:0852a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令一切眾生起大施願。",
        "from": "T20n1191_004:0852a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惠施一切有情。",
        "from": "T20n1191_004:0852a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱(引)喃(引)摩進底也(二合引)部哆嚕閉赧(引)怛儞也(二合)他(引)唵(引)濕吠(二合)諦(引)室哩(二合)嚩布(入)此明名為大化。",
        "from": "T20n1191_004:0852a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印名孔雀座。",
        "from": "T20n1191_004:0852a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具不思議未曾有相。",
        "from": "T20n1191_004:0852a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作一切所欲之事無不成就。",
        "from": "T20n1191_004:0852a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能降伏世間。",
        "from": "T20n1191_004:0852a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "童男童女生愛敬心。",
        "from": "T20n1191_004:0852a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱(引)喃(引)摩進底也(二合引)部哆嚕閉赧(引)怛儞也(二合)他唵(引)契契哩婆虞哩薩哩嚩(二合)設怛嚕(二合)薩旦(二合)婆野咎婆野謨(引)賀野嚩舍摩(引)曩藥此明名相應大明。",
        "from": "T20n1191_004:0852a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印名嚩訖怛囉。",
        "from": "T20n1191_004:0852a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能調伏造惡眾生。",
        "from": "T20n1191_004:0852a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱(引)喃(引)摩鉢底(二合)賀哆誐底鉢囉(二合)左(引)哩拏(引)怛儞也(二合)他(引)唵(引)室哩(二合入)此明名大福德。",
        "from": "T20n1191_004:0852a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印名合掌。",
        "from": "T20n1191_004:0852a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛自宣說。",
        "from": "T20n1191_004:0852a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令有情得國王位。",
        "from": "T20n1191_004:0852a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱(引)喃(引)薩哩嚩(二合)薩怛嚩(二合)婆野鉢囉(二合)捺(引)曳喃(引)怛儞也(二合)他唵(引)阿[口*爾]諦俱摩(引)哩嚕閉尼[目*壹]呬阿(引)誐蹉摩摩迦(引)哩焰(二合)俱嚕(入)此名無能勝現童女身說甘露句。",
        "from": "T20n1191_004:0852a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印名合掌。",
        "from": "T20n1191_004:0852a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與此真言同用。",
        "from": "T20n1191_004:0852a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離一切冤家。",
        "from": "T20n1191_004:0852a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱(引)喃(引)摩進底也(二合引)部哆嚕閉赧(引)怛儞也(二合)他(引)唵(引)惹曳(引)唵(引)尾惹曳(引)唵(引)阿[口*爾]諦(引)唵(引)阿波囉(引)[口*爾]諦(引)此真言內有四姊妹親近菩薩。",
        "from": "T20n1191_004:0852a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經行大地救度眾生。",
        "from": "T20n1191_004:0852b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸有情依行真言行。",
        "from": "T20n1191_004:0852b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如度得舡所作圓滿。",
        "from": "T20n1191_004:0852b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印名祕密。",
        "from": "T20n1191_004:0852b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱(引)喃(引)路迦(引)誐囉(二合引)地鉢底喃(引)怛儞也(二合)他唵(引)俱摩囉摩賀(引)俱摩(引)囉吉哩(二合)拏釤摩佉冐地薩怛嚩(二合引)覩惹拏(二合)哆摩布囉(引)摩布囉(引)娑曩設吉曳(二合引)覩(引)儞也(二合)哆波(引)尼囉訖旦(二合引)誐囉訖哆(二合)巘馱(引)覩隷(引)鉢曩必哩(二合)野佉佉佉呬佉呬吽吽涅哩(二合)怛也(二合)涅哩(二合)怛也(二合)囉訖哆(二合)補瑟波(二合引)唧哆沒哩底(二合)娑摩野摩拏娑摩(二合)囉部囉(二合)摩部囉(二合)摩部囉(二合)摩野部囉(二合引)摩野羅護羅護摩(引)尾覽摩薩哩嚩(二合)迦(引)哩野(二合引)尼弭俱嚕尾唧怛囉(二合)嚕波馱(引)哩尼底瑟吒(二合)底瑟吒(二合)吽吽薩哩嚩(二合引)沒馱(引)努惹拏(二合)諦(引)娑嚩(二合)賀(引)爾時妙吉祥菩薩。",
        "from": "T20n1191_004:0852b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此真言之時。",
        "from": "T20n1191_004:0852b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大地六種振動。",
        "from": "T20n1191_004:0852b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大自在天現極惡相。",
        "from": "T20n1191_004:0852b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "童子天具火曜幖幟。",
        "from": "T20n1191_004:0852b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱來教化調伏一切惡業眾生。",
        "from": "T20n1191_004:0852b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙吉祥所說大權菩薩。",
        "from": "T20n1191_004:0852b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起慈愍心行菩薩行。",
        "from": "T20n1191_004:0852b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利樂一切愚癡眾生。",
        "from": "T20n1191_004:0852b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印名大力。",
        "from": "T20n1191_004:0852b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與真言同用。",
        "from": "T20n1191_004:0852b23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令得梵天等大福德果。",
        "from": "T20n1191_004:0852b23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況人身。",
        "from": "T20n1191_004:0852b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時妙吉祥菩薩。",
        "from": "T20n1191_004:0852b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說真言名為三字。",
        "from": "T20n1191_004:0852b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為悲愍眾生。",
        "from": "T20n1191_004:0852b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印名大力。",
        "from": "T20n1191_004:0852b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與真言同用。",
        "from": "T20n1191_004:0852b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令一切眾生得大福德。",
        "from": "T20n1191_004:0852b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)吽[口*弱]",
        "from": "T20n1191_004:0852b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是三字真言。",
        "from": "T20n1191_004:0852b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依法持誦所求皆成。",
        "from": "T20n1191_004:0852b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說外心真言。",
        "from": "T20n1191_004:0852c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱(引)喃(引)三滿覩(引)儞喻(二合引)底哆沒哩底喃(引)唵(引)尾訖哩(二合)哆誐囉(二合)賀吽頗吒妙吉祥菩薩為親近者童子天。",
        "from": "T20n1191_004:0852c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此真言。",
        "from": "T20n1191_004:0852c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能降伏一切部哆摩多羅等。",
        "from": "T20n1191_004:0852c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及一切諸惡星曜。",
        "from": "T20n1191_004:0852c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如有部多等所著所魅及惡曜照臨。",
        "from": "T20n1191_004:0852c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依法持誦彼等悉皆恐怖。",
        "from": "T20n1191_004:0852c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而自退散即得解脫。",
        "from": "T20n1191_004:0852c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至求生天界一切皆得。",
        "from": "T20n1191_004:0852c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印名大力與彼同用。",
        "from": "T20n1191_004:0852c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱(引)喃(引)摩鉢囉(二合)底賀哆舍(引)娑曩(引)喃(引)怛儞也(二合)他(引)唵(引)沒囉(二合)憾摩(二合)蘇沒囉(二合)憾摩(二合)沒囉(二合)憾摩(二合)嚩哩左(二合)細(引)扇(引)底俱嚕此真言大梵天菩薩所說。",
        "from": "T20n1191_004:0852c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令息災害。",
        "from": "T20n1191_004:0852c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若部多惱亂之時。",
        "from": "T20n1191_004:0852c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言剎那退散速得安樂。",
        "from": "T20n1191_004:0852c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印名五髻。",
        "from": "T20n1191_004:0852c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若說大梵天調伏儀軌。",
        "from": "T20n1191_004:0852c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如四圍陀論。",
        "from": "T20n1191_004:0852c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱(引)喃(引)摩鉢囉(二合)底賀哆舍(引)娑曩(引)喃(引)怛儞也(二合)他唵(引)誐嚕拏嚩(引)賀曩作羯囉(二合)波(引)尼左覩哩部(二合)惹吽吽三摩努娑摩(二合)囉冐地薩怛嚩(二合)阿惹拏(二合)波野底妙吉祥說此真言。",
        "from": "T20n1191_004:0853a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速能作於吉祥之事。",
        "from": "T20n1191_004:0853a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印名三髻。",
        "from": "T20n1191_004:0853a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能破壞部哆及那羅延。",
        "from": "T20n1191_004:0853a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此則皆是方便攝化眾生。",
        "from": "T20n1191_004:0853a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿哆沒馱(引)喃(引)摩鉢囉(二合)底賀哆舍(引)娑曩(引)喃(引)怛儞也(二合)他唵(引)摩賀(引)摩呬(引)室左(二合)囉部哆(引)地波底沒哩(二合)沙特嚩(二合)惹鉢囉(二合)覽嚩惹吒摩俱吒馱(引)哩尼悉哆婆娑摩(引)度娑哩哆沒哩底(二合)吽頗吒頗吒冐地薩怛冐(二合引)惹拏(二合)波野底(入)此真言我自宣說。",
        "from": "T20n1191_004:0853a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為慈愍眾生。",
        "from": "T20n1191_004:0853a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印名大叉。",
        "from": "T20n1191_004:0853a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與明同用亦破部多之眾。",
        "from": "T20n1191_004:0853a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大方廣菩薩藏文殊師利根本儀軌經卷第四",
        "from": "T20n1191_004:0853a18_0",
        "context_with_next_sentence": false
    }
]