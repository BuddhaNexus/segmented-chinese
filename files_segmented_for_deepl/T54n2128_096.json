[
    {
        "source_sentence": "一切經音義卷第九十六",
        "from": "T54n2128_096:0904c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翻經沙門慧琳撰",
        "from": "T54n2128_096:0904c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從第五盡十四計十卷。",
        "from": "T54n2128_096:0904c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "音弘明集第五卷",
        "from": "T54n2128_096:0904c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "混淆(効交反考聲云淆雜亂也字書混淆濁水也古今正字從水肴聲亦作殽也)。",
        "from": "T54n2128_096:0904c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七竅(啟弔反字書云孔也說文云空也從穴敫聲敫音[唎禾])。",
        "from": "T54n2128_096:0904c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅毳(齒芮反鄭注周禮云毳毛之細縟者也說文獸細毛也從三毛)。",
        "from": "T54n2128_096:0904c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灺垂(上邪野反廣雅云炭[火*節][盡((烈列)/皿)+火][火*曹]炨也說文燭炭也從火也聲亦作炨[火*節]音子慄反[盡((烈列)/皿)+火]辭刃反[火*曹]子勞反集本作[火*子]非也)。",
        "from": "T54n2128_096:0904c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "枯腊(星亦反鄭注周禮云乾肉也古今正字脩腊小物也全乾也從肉昔聲)。",
        "from": "T54n2128_096:0904c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "之[難/火](熱蟬反陸佐公闕銘云刑酷[難/火]炭說文云燒也作然字義同諸字書撿並無此字亦有從艸作[卄/難]其義亦同也)。",
        "from": "T54n2128_096:0904c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而炷(音注)。",
        "from": "T54n2128_096:0904c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "桎梏(上真曰反下公屋反義已釋高僧傳也)。",
        "from": "T54n2128_096:0904c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大塊(枯潰反莊子云大塊噫氣其名為風司馬彪云大塊謂天也說文從土鬼聲也)。",
        "from": "T54n2128_096:0904c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人[革*奇](几冝反與羇同義古字也考聲云勒也繫也玉篇從革奇聲也)。",
        "from": "T54n2128_096:0904c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緬素(上綿褊反賈注國語云緬思貌也說文從糸面聲也)。",
        "from": "T54n2128_096:0905a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輿櫬(上與諸反蘇林云輿猶載之意說文從車[臼/丌]聲下初恡反說文云襯棺也從木親聲)。",
        "from": "T54n2128_096:0905a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讎隙(上受尤反下卿逆反賈注國語云隙舋也顧野王云隙猶間隙也亦云有怨憾也說文從阝[少/日/小]音同上集本作郄人姓也非雔義也)。",
        "from": "T54n2128_096:0905a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遺[怡台+咸](綺淹反考聲云憾恃險意也說文不安貌也從心咸聲也)。",
        "from": "T54n2128_096:0905a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六卷",
        "from": "T54n2128_096:0905a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋剝論(中邦角反考聲云剝取也凡革用總稱也集本作此駮字是獸名也非釋剝之義)。",
        "from": "T54n2128_096:0905a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鑪錘(住韋反廣疋云錘謂稱錘也古今正字從金垂聲案鑪錘之間是鍜者合從追作鎚於義為得)。",
        "from": "T54n2128_096:0905a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孜汲(上子斯反下金岌反孔注尚書云孜孜不怠也說文汲汲也從文子聲作此資給亦通)。",
        "from": "T54n2128_096:0905a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憮然(上音武考聲云憮失意皃也何注論語為其不達已意而非之也說文愛也一曰不動也從心無聲也)。",
        "from": "T54n2128_096:0905a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咄異哉(上都訥反說文云咄相謂也從口出聲)。",
        "from": "T54n2128_096:0905a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皦潔(上澆了反毛詩傳云皦白也說文玉石之白也從白敫聲敫音叶下音結也)。",
        "from": "T54n2128_096:0905a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婉孌(上冤阮反毛詩傳云婉從也亦美皃也杜注左傳云約也說文從女宛聲下劣囀反字書云孌婉順皃說文慕也從女[戀心]聲[戀心]音劣專反)。",
        "from": "T54n2128_096:0905a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凶愎(憑逼反杜注左傳云愎戾也佷也不仁也古今正字從心复聲复音復)。",
        "from": "T54n2128_096:0905a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖騂(恤營反毛詩傳云赤黃色曰騂古今正字從馬從辛聲亦作[打丁+辛]觪也)。",
        "from": "T54n2128_096:0905a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "犂色(上履脂反何注論語云犂雜文也古今正字從牛[禾*刃]聲[禾*刃]音利也)。",
        "from": "T54n2128_096:0905a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "騁功(上勅領反毛詩傳云騁施也杜注左傳云馳也說文從馬[叟又+ㄎ]聲[叟又+ㄎ]音匹盈反)。",
        "from": "T54n2128_096:0905a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[歹*畏]壘(上烏賄反字書云[歹*畏]不智人也古今正字從反畏聲集作揋通下雷猥反考聲云壘頭禿皃左傳有[鄀右+?]魁壘也說文從土靁省聲靁音雷也)。",
        "from": "T54n2128_096:0905a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爽塏(上霜兩反說文云爽明也從[爻*爻]從大篆文作爽下開亥反杜注左傳云塏燥也說文高燥也從土豈聲豈音愷也)。",
        "from": "T54n2128_096:0905a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "峭峻(上鍫醮反許叔重云峭亦峻也說文判也義同從阝肖聲)。",
        "from": "T54n2128_096:0905a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剖腋(上普厚反下征亦反顧野王云剖破也說文判也從刀咅音偷豆反)。",
        "from": "T54n2128_096:0905a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羽[蚡刀+允](始銳反說文云蛻蟬解皮也從虫兌聲下文同)。",
        "from": "T54n2128_096:0905b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "橫馗(匱龜反說文云馗九達道似龜背故謂之馗從九首聲亦作逵也)。",
        "from": "T54n2128_096:0905b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捫珠(上沒奔反聲類云捫摸也說文撫持也從手門聲摸音門博反)。",
        "from": "T54n2128_096:0905b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有埤(婢彌反賈逵云埤猶補也說文增也從土卑聲也)。",
        "from": "T54n2128_096:0905b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斐暐(上孚尾反毛詩傳云斐文皃也說文分別文也從文非聲易曰君子豹變其文蔚也下于鬼反說文云暐盛明皃也從日韋聲是也)。",
        "from": "T54n2128_096:0905b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[斖且+(烈列+酉)][斖且+(烈列+酉)](微匪反已具釋續高僧傳中也)。",
        "from": "T54n2128_096:0905b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豪[禾*會](口外反字書蒼頡篇說文並云糠也從禾會聲集本作糩誤也)。",
        "from": "T54n2128_096:0905b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳥聒(官活反杜注左傳云聒讙也蒼頡篇擾耳也說文讙語也從耳舌聲集本作[曷*舌]之甚也舌音滑)。",
        "from": "T54n2128_096:0905b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獸呴(呼垢反廣疋云呴鳴也顧野王云獸聲也賈逵呼呺也聲類噑也古今正字作[牛*句]義同亦作吽從牛句聲集作呴通也)。",
        "from": "T54n2128_096:0905b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "醍醐(上第奚反下戶孤反考聲云醍醐蘇中不疑者也集本從水作湜湖字非也)。",
        "from": "T54n2128_096:0905b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飾繢(上音式下迴內反古今正字云似綦赤色從糸貴聲也)。",
        "from": "T54n2128_096:0905b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[(夕*(ㄗ@、))/貝]珍(上矛候反顧野王云[夗/貝]猶交易也尒疋云市也賣也說文易財也從貝夘聲夘音古下正[玝十+小]字也)。",
        "from": "T54n2128_096:0905b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第七卷",
        "from": "T54n2128_096:0905b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "簪佩(下陪昧反鄭注周禮云珮玉所帶玉也大戴禮珮上有葱衡下雙璜衡牙璸珠以納其間天子珮白玉公侯玄玉大夫水蒼玉世子瑜玉士瓀玫孔子珮象環五寸顧野王凡帶物在身皆謂珮說文大帶也從人几聲佩必有巾巾謂之帀亦從玉作珮也)。",
        "from": "T54n2128_096:0905b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擎跽(上競迎反亦作[敬/廾]下奇几反莊子云擎跽曲拳人[耜耒]之禮也說文長跽也從足忌聲也)。",
        "from": "T54n2128_096:0905b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[大/(舛夕)]強(上跨花反考聲云[大/(舛夕)]大也說文奢也從大[舛夕]聲[舛夕]音口寡反)。",
        "from": "T54n2128_096:0905b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梟鴆(沈賃反考聲毒鳥也山海經云女几之山多鴆郭注云大如鵰紫綠色長頸赤喙食蛇杜注左傳鴆羽有毒以畫酒飲之則死說文從鳥宂聲冘音淫)。",
        "from": "T54n2128_096:0905b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬱柲(上正鬱字下悲媚反梵語也)。",
        "from": "T54n2128_096:0905b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鉾[矛*(替曰+貝)](上謨侯反下倉亂反考聲云遙投矛也廣疋云[矛*(替曰+貝)]謂之鋋古今正字義同從矛贊聲下文同也)。",
        "from": "T54n2128_096:0905b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詣摽(必遙反顧野王云摽謂揭表以識之也說文亦表也從木票聲集作檦俗字也票同音也)。",
        "from": "T54n2128_096:0905c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蹙之(上酒育反廣疋云蹙迫也鄭云促也案以手摵之合從手作戚考聲云推逼也古今正字從手戚聲)。",
        "from": "T54n2128_096:0905c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八卷",
        "from": "T54n2128_096:0905c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劉勰(嫌頰反人姓名也晉桓玄記室參軍)。",
        "from": "T54n2128_096:0905c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增靄(埃葢反考聲云靄氣增靄則昏昧不明也集本作靄恐失諸字書作靉也)。",
        "from": "T54n2128_096:0905c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[火*矣]燼(上烏垓反廣疋云[火*矣]熱也熾也古今正字從火矣聲下辭胤反正作[盡皿+火]鄭箋毛詩云火餘也古今正字從火盡聲)。",
        "from": "T54n2128_096:0905c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "閩藪(上密斌反周禮云掌方氏掌七閩之民七者周之所般國數也山海經閩在海中也說文南越蛇種也從虫門聲下蘇口反)。",
        "from": "T54n2128_096:0905c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "河鈎(狗侯反郭注尒疋云水曲如鈎流盤桓也般鈎者黃河曲名文字典說從金句聲集本作鍭音侯非其義也)。",
        "from": "T54n2128_096:0905c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "査[槮(彰章)+小](上乍加反毛詩傳云查水中浮草木也古今正字從木且聲亦作槎集本作渣非也下所錦反尒疋云[槮(彰章)+小]謂之涔郭注云今之作[槮(彰章)+小]叢積此積柴於水中魚得而入其裏藏因捕取之說文積柴捕魚名[栗木+林]音力今反)。",
        "from": "T54n2128_096:0905c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禁錮(古護反說文云錮鑄塞也從金固聲也)。",
        "from": "T54n2128_096:0905c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嗑齒(上胡臘反王弼注周易云嗑合也說文從口盍音同上後有者並同)。",
        "from": "T54n2128_096:0905c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[卄/碭]花(上黨浪反埤蒼云[菛@良][卄/碭]草名也說文從艸碭聲[菛@良]音浪碭音蕩)。",
        "from": "T54n2128_096:0905c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觵魅(虢橫反說文兕牛角可以飲者從角黃聲或作觥與集本同下音媚)。",
        "from": "T54n2128_096:0905c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "溷漫(上魂困反古今正字云溷亂也從水圂聲圂音同上下蠻諫反)。",
        "from": "T54n2128_096:0905c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "浹辰(上尖葉反鄭注周禮云從甲至癸謂之浹曰韓詩通也說文從水夾聲也)。",
        "from": "T54n2128_096:0905c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姱僈(上跨瓜反古今正字云奢也從女夸聲下蠻襻反考聲僈狡也字書作嫚說文從人曼聲)。",
        "from": "T54n2128_096:0905c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "硠[尸@技](上朗當反廣疋云硠堅也石聲也說文從石良聲下渠戟反說文屩也從尸[敲高+ㄐ]省聲屩音脚也)。",
        "from": "T54n2128_096:0905c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劈裂(上[叟又+ㄎ]覓反廣疋云割也裂也說文破也從刀辟聲)。",
        "from": "T54n2128_096:0905c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "犎牛(上福逢反考聲云野牛也領有隆肉者也出蜀中山海經云禱過之山多犎牛埤蒼野牛也說文從牛封聲)。",
        "from": "T54n2128_096:0905c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凶獷(瓜猛反說文云犬獷獷不可附也從犬廣聲)。",
        "from": "T54n2128_096:0905c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第九卷",
        "from": "T54n2128_096:0905c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱倦(權院反顧野王云倦疲也廣疋極也說文云疲也從人卷聲集作勌恐誤也)。",
        "from": "T54n2128_096:0906a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "砥礪(上之履反下離[利禾+又]反)。",
        "from": "T54n2128_096:0906a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蛟螭(上音交下恥離反山海經云限水有蛟其狀魚身而蛇尾鴛鴦食之可以為痔又況水多蛟郭云似蛇四脚小頭細頸頸有白纓大者十數圍夘生子如一二斛甖能吞人說文龍屬也沱魚滿三千六百蛟來為之長率魚而飛置苛韭水中即蛟去也從虫交聲也)。",
        "from": "T54n2128_096:0906a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鋒[金*(遻(這言))](上捧容反下昂各反釤也)。",
        "from": "T54n2128_096:0906a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鈍刃(上豚困反)。",
        "from": "T54n2128_096:0906a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "骨骼(皆頟反鄭注禮記云骨枯曰骼說文禽獸之骨曰骼從骨各聲集本作髂音客亞反誤)。",
        "from": "T54n2128_096:0906a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "涼燠(憂六反說文燠熱也從火奧聲)。",
        "from": "T54n2128_096:0906a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "痛痒(羊蔣反考聲云痒痛之微者也說文作蛘義同從虫羊聲下文並同)。",
        "from": "T54n2128_096:0906a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孫臏(頻泯反人姓名)。",
        "from": "T54n2128_096:0906a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西櫺(歷丁反方言云屋梠謂之櫺蒼頡篇櫺檻楯間也說文楯間子也從木霝聲梠音呂楯音順也)。",
        "from": "T54n2128_096:0906a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忖度(上村損反下宕各反)。",
        "from": "T54n2128_096:0906a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[日*昏]似(上敏斌反鄭注禮記云琘美石次玉也山海經岐山多琘說文從玉昏聲集本作碈通俗字昏音敏)。",
        "from": "T54n2128_096:0906a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "驎[馬*卬](上栗珍反郭璞云色有淺深斑駮隱驕今謂連錢[髟/匆/心]也字書曰馬黑脣也古今正字從馬粦聲下吾剛反楚辭云[馬*卬][馬*卬]若千里之駒埤蒼云馬搖頭也古今正字馬怒皃從馬卬聲卬音昂也)。",
        "from": "T54n2128_096:0906a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "女媧(寡花反古帝号也)。",
        "from": "T54n2128_096:0906a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多稌(兔魯反尒疋云稌稻也鄭眾云稉也說文牛冝稔從禾余聲)。",
        "from": "T54n2128_096:0906a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無垠(獃根反考聲云涯畔也古今正字從土艮聲獃音吾衰反)。",
        "from": "T54n2128_096:0906a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "播殖(上波磨反下時力反鄭箋毛詩云播猶種也說文亦種也一云布也從手番聲集本作簸是箕屬非播種字也)。",
        "from": "T54n2128_096:0906a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迷瞀(矛候反鄭注禮記云瞀瞀目不明也說文低目謹也從目敄聲敄音武也)。",
        "from": "T54n2128_096:0906a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[(工*几)/虫]螶(上[尸@句]顒反。",
        "from": "T54n2128_096:0906a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下強魚反。",
        "from": "T54n2128_096:0906a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尒疋云西方比肩獸焉與蛩蛩蛩螶虛為比螶虛齧甘草即有難蛩蛩螶虛負而走其各謂之蟨。",
        "from": "T54n2128_096:0906a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呂氏春秋云北方有獸其名蟨。",
        "from": "T54n2128_096:0906a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前足似兔趍則頓走則顛然則蛩蛩螶虛亦宜鼠後而兔前前高不得甘草故須蟨食之。",
        "from": "T54n2128_096:0906a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今鴈門廣武縣夏屋山有獸形如兔而大。",
        "from": "T54n2128_096:0906a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相負共行土俗名之為蟨鼠。",
        "from": "T54n2128_096:0906a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說文[(工*几)/虫]獸也從虫卭聲螶從[虫*虫]巨聲集本作[馬*卬]駏如馬非此獸也。",
        "from": "T54n2128_096:0906a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "失之甚矣。",
        "from": "T54n2128_096:0906b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蟨音居衛反卭音同上[虫*虫]音昆下文同也)。",
        "from": "T54n2128_096:0906b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廟祧(他彫反禮記云遠廟為祧鄭云祧之言超也上去意也遷廟之主以昭穆合藏二祧之中古今正字從示兆聲)。",
        "from": "T54n2128_096:0906b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壇墠(上堂丹反下蟬闡反孔注尚書云築土為壇除地為墠說文云壇祭場也墠野也二字竝從土亶單皆聲後有準此也)。",
        "from": "T54n2128_096:0906b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十卷",
        "from": "T54n2128_096:0906b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瘡痏(為軌反蒼頡篇云歐傷也說文[病丙+共]痏也從疒有聲疾音施也)。",
        "from": "T54n2128_096:0906b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滯甃(皺瘦反說文甃井甓也從瓦秋聲甓音鼙覓反)。",
        "from": "T54n2128_096:0906b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "之鼃(烏媧反考工記云所謂以脰鳴者也說文蝦蟆也從黽圭聲黽音猛亦作蛙也)。",
        "from": "T54n2128_096:0906b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舟航(鶴郎反毛詩傳云航渡也顧野王云今謂所以濟渡之舟為航古今正字義同從舟亢聲亦作斻音[利禾+(罩卓+止)]也)。",
        "from": "T54n2128_096:0906b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嘉抃(別變反考聲云抃撫掌也說文作[木*弁]云拊手也從手弁聲弁音同集本作抃通也)。",
        "from": "T54n2128_096:0906b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無[敲高+睪](盈迹反孔注尚書云[斀(句口+虫)+(羍干+十)]鬲也說文懈也古今正字終也從支睪聲睪亦集本作歝誤下文同也)。",
        "from": "T54n2128_096:0906b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "踳淪(上春準反司馬彪注莊子云踳雜不同也許叔重相背也古今正字差也從足春聲亦作舛偆淮南子作僢音義並同也)。",
        "from": "T54n2128_096:0906b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摑[絅口+又](上居碧反諸字書並無此字意謂俗字也)。",
        "from": "T54n2128_096:0906b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "招[保木+(巢果+言)](朅焉反孔注尚書云諐過也杜注左傳云失也說文云此[簃多+留]文諐字正作愆義同從心衍聲或作[塞土+心][(彳*言*亍)/心][羊*(巟亡+口)][起巳+(侃(仁二))]並同也)。",
        "from": "T54n2128_096:0906b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[彳*亶]徊(上展連反下音迴考聲云儃轉也徊施也古今正字作邅義同從辵亶聲徊從彳回聲集本作儃佪誤也辵音丑略反彳音勅也)。",
        "from": "T54n2128_096:0906b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擠其(上齊系反字書云擠墜也方言滅也廣疋推也說文排也從手齊聲)。",
        "from": "T54n2128_096:0906b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徐緄(昆本反人姓名梁黃門侍郎也)。",
        "from": "T54n2128_096:0906b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王暕(姦限反人姓名)。",
        "from": "T54n2128_096:0906b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "柳惲(上正柳字下鬱吻反人姓名也吻音聞粉反)。",
        "from": "T54n2128_096:0906b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "柳憕(音澄人名)。",
        "from": "T54n2128_096:0906b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摛機(上勅知反考聲云摛陳布也左思蜀都賦摛藻掞天說文舒也從手离聲离音同上)。",
        "from": "T54n2128_096:0906b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惵惵(恬叶反顧野王云惵猶懼也東京賦云惵惵黔首是也古今正字義同從心枼聲枼音葉)。",
        "from": "T54n2128_096:0906c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "碌碌(盧谷反埤蒼云碌碌多砂石也古今正字義同從石录聲录音祿)。",
        "from": "T54n2128_096:0906c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淲泉(上皮彪反毛詩傳云淲流皃也廣疋義同說文從水彪省聲彪音彼尤反)。",
        "from": "T54n2128_096:0906c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鈎深(上苟侯反下審株反周易繫辭云鈎深致遠考聲云鈎求也取也引也字書云深[遽虍+穴]也測也今集本作拘[栥欠+ㄆ]於義乖舛恐傳寫訛謬也改從鈎深於義為得亦請詳焉)。",
        "from": "T54n2128_096:0906c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[諒小+(日/羊)][諒小+(日/羊)](準純反鄭注禮記云誠懇皃謂告之丁寧也說文告曉之熟也從言[亨了+(日/羊)]聲集本作諄諄通俗字也[亨了+(日/羊)]音紀純也)。",
        "from": "T54n2128_096:0906c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終懵(墨堋反堋音崩懵也)。",
        "from": "T54n2128_096:0906c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蠡度(上盧戈反下堂洛反)。",
        "from": "T54n2128_096:0906c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何炟(丹達反人名集作[火*目]誤也)。",
        "from": "T54n2128_096:0906c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僾然(上哀岱反毛詩傳云僾[咒几+巴]也鄭箋云使人[咒几+巴]然也說文髣髴也從人愛聲[咒几+巴]合作悒音邑)。",
        "from": "T54n2128_096:0906c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嫗煦(上紆愚反下[口*(百日+ㄎ)]句反顧野王云嫗謂俛伏使溫也韓詩傳云煦暖也鄭注禮記以氣曰煦以體曰嫗說文作昫日出溫也從日句聲嫗從女區聲區音驅)。",
        "from": "T54n2128_096:0906c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蚑行(上詰以反顧野王云謂麕鹿之類蚑踵行者也亦作跛云翹足也古今正字云蟲行從虫支聲)。",
        "from": "T54n2128_096:0906c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翾飛(上血緣反考聲云翾小飛皃也古今正字從羽睘省聲也)。",
        "from": "T54n2128_096:0906c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蕭眕(真忍反人名也)。",
        "from": "T54n2128_096:0906c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏[怡台+(晅旦+且)](絙鄧反人名也絙音古登反)。",
        "from": "T54n2128_096:0906c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "侄行(上珍栗反考聲云侄仡不前貌也亦癡皃也文字典說從人至聲)。",
        "from": "T54n2128_096:0906c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲憓(下檇銳反鄭注周禮云賙衣食曰惠說文惠仁也從心叀音衛集本作憓同也)。",
        "from": "T54n2128_096:0906c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[冘*頁]首(上針稔反蒼頡篇[冘*頁]垂頭皃也聲類云項中有所枕也考聲腦後骨也今謂之玉煩說文項枕也從頁冘聲案枕支之枕即從木作枕為得也冘音[泳永+(瑤王)]也)。",
        "from": "T54n2128_096:0906c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豁然(上歡栝反廣疋云豁空也顧野王云豁達大度量也亦開皃也古今正字從谷害聲集作[(沉几+(丰/口))/土]字誤也)。",
        "from": "T54n2128_096:0906c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愾然(上希既反鄭箋詩云愾歎息也廣疋云懣也說文大息也從心氣聲懣音悶)。",
        "from": "T54n2128_096:0906c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相舛(川臠反廣疋云舛相背也說文對臥也從夕[(巨匚)*(十/十)]聲夕音雖[(巨匚)*(十/十)]音跨)。",
        "from": "T54n2128_096:0906c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蟪蛄(上携桂反下古吳反方言云劫蟧為蟪蛄也莊子云蟪蛄不知春秋古今正字並從虫惠古皆聲虭音刀蟧音勞)。",
        "from": "T54n2128_096:0906c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重雰(孚云反蒼頡篇雰霧也說文作氛義同從气分聲也)。",
        "from": "T54n2128_096:0907a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "躧足(上師滓反蒼頡篇云躧徐行皃古今正字云從足麗聲亦作屣)。",
        "from": "T54n2128_096:0907a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天璪(音早鄭注周禮云皆五釆曰璪雜文之名也說文飾如水藻也從玉喿聲喿音蘇到反)。",
        "from": "T54n2128_096:0907a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "望覘(諂廉反說文云覘闚也視也從見占聲也闚音誥規反也)。",
        "from": "T54n2128_096:0907a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還晣(之折反毛詩傳云晣明也說文昭晣亦明也從日折聲也)。",
        "from": "T54n2128_096:0907a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "齊[角*麃](下表驕反說文鑣馬銜也從金麃聲或作[角*麃]麃音薄交反也)。",
        "from": "T54n2128_096:0907a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十一卷",
        "from": "T54n2128_096:0907a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詆呵(上低禮反蒼頡篇云欺也廣疋毀也聲類呰也說文訶也從言氐聲氐音低也)。",
        "from": "T54n2128_096:0907a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周顗(冝豈反人名也)。",
        "from": "T54n2128_096:0907a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殷覬(音冀人名也)。",
        "from": "T54n2128_096:0907a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "較談(上江岳反考聲云較略也廣疋云明也漢書云較然易知說文從車交聲也)。",
        "from": "T54n2128_096:0907a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毀讟(同鹿反杜注左傳云讟誹也廣疋惡也郭璞云謗毀怨痛也說文痛[死/心]也從誩賣聲誩音競集本作讀通俗字下第十卷內同也)。",
        "from": "T54n2128_096:0907a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "李淼(彌褾反人名也)。",
        "from": "T54n2128_096:0907a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蠰佉(上而章反下欺迦反)。",
        "from": "T54n2128_096:0907a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鄒魯(上經蒐反下正魯字考聲云鄒魯邑古邾國魯穆公改為鄒夫子之父為鄒大夫故云集本作[阿可+聚]俗字也)。",
        "from": "T54n2128_096:0907a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨淄(滓師反郡名在齊集本作[蕢貝+(面(百日))]非)。",
        "from": "T54n2128_096:0907a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞥想(上片蔑反說文云瞥纔見從目敝聲也)。",
        "from": "T54n2128_096:0907a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "絓諸(上瓜華反音卦廣疋云絓止也聲類有所礙也方言持也說文[茼(一/口)+(綴糸)]滓絓頭伯囊絮從糸圭聲)。",
        "from": "T54n2128_096:0907a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孱然(上棧焉反考聲云孱顏不齊皃也[仁二+(而/火)]弱也說文穿也一云呻吟也從孨在尸下孨音剪)。",
        "from": "T54n2128_096:0907a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[(丰*力)/石]統(上音略僧名也)。",
        "from": "T54n2128_096:0907a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頡頏(上賢結反下鶴郎反考聲云頡頏乍高乍下也說文頡直項也從頁吉聲古今正字頏咽也從頁亢聲也)。",
        "from": "T54n2128_096:0907a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "邯鄲(上汗蘭反下怛蘭反漢書云縣名屬趙杜注左傳廣平縣也)。",
        "from": "T54n2128_096:0907a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "匍匐(上步摸反下朋北反顧野王云手據地伏行也說文匍手行也匐伏也二字並從勹甫畐聲勹音包畐音丕逼反)。",
        "from": "T54n2128_096:0907b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "暴鰓(上袍報反下賽才反)。",
        "from": "T54n2128_096:0907b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鰥絕(上慣還反顧野王云老而無妻曰鰥賈逵注國語嫁娶不時曰鱞釋名言愁悒不能寐目常鰥鰥然故其字從魚魚愐不閉者也說文從魚環省聲集本作[環王+角]誤之矣)。",
        "from": "T54n2128_096:0907b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耄耋(上毛報反下田結反杜注春秋云耄[辭辛+(敲高)]也鄭注禮記云惛忘也尒疋云耋老也孫炎云老人面色如鐵者也說文作[葶丁+老]年九十也從老蒿省聲耋年八十也從老省聲亦作[老/鬼])。",
        "from": "T54n2128_096:0907b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "崦嵫(上猗廉反下子斯反山海經云鳥鼠同穴山西南三百六十里曰[山*弇]嵫之山王逸注楚辭云下有濛水水中虞淵曰所入也古今正字義同二字並從山弇茲皆聲亦作崦)。",
        "from": "T54n2128_096:0907b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "濛汜(上音蒙下音似已具釋第二卷中也)。",
        "from": "T54n2128_096:0907b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鸑鷟(上顏角反下崇學反廣疋云鳳屬神鳥也說文義同又云周之興也鳴於歧山江中有鸑鷟似鳥而大赤目二字並從鳥獄族皆聲)。",
        "from": "T54n2128_096:0907b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徽牘(上音暉下徒鹿反說文云牘書版也從片賣聲)。",
        "from": "T54n2128_096:0907b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十二卷",
        "from": "T54n2128_096:0907b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "閬風(上音浪廣疋云崑崙之墟有三山閬風板桐玄圃也楚辭云望閬風之板桐也說文從門良聲)。",
        "from": "T54n2128_096:0907b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僉悟(上妾閻反廣疋云僉多也孔注尚書皆也郭璞劇也說文亦皆也從亼從叩從从亽音才入反叩音喧从音慈容反)。",
        "from": "T54n2128_096:0907b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬竅(詩弔反已釋第五卷)。",
        "from": "T54n2128_096:0907b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怒唬(号刀反考聲云唬諕從也說文從口虎聲亦作噑集本作[目*(百日+ㄎ)]誤也諕音呼故反)。",
        "from": "T54n2128_096:0907b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "肥腯(屯訥反鄭注禮記云腯充皃也方言盛也說文牛羊曰肥豕曰腯從月盾聲也)。",
        "from": "T54n2128_096:0907b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覜仰(上[袖由+兆]弔反黃疋云覜視也郭璞云察視也說文義同從見兆聲集作覜誤也)。",
        "from": "T54n2128_096:0907b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "之轍(纏列反杜注左傳云轍迹也古今正字從車[徹彳]省聲集作[跳兆+(轍車)]俗字也下文宗[跳兆+(轍車)]同此也)。",
        "from": "T54n2128_096:0907b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[黃*軍]熒(上字檢諸字書並不見恐傳寫錯誤唯有從車作[車*黃]音光庶幾相近於義或未為得又從圭作黊音胡寡反說文鮮明皃也稍近義焉未知通否)。",
        "from": "T54n2128_096:0907b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱躓(知利反顧野王云躓頓也說文從足質聲)。",
        "from": "T54n2128_096:0907c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不韙(違鬼反杜注左傳云韙是也說文義同從是韋聲)。",
        "from": "T54n2128_096:0907c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欬嚏(上開愛反顧野王云欬嗽也逆氣也從欠亥聲集本作咳音亥哀反恐誤下丁計反蒼頡篇云啑噴鼻也說文悟解氣也從口疐聲疐音竹利反)。",
        "from": "T54n2128_096:0907c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若猥(隈隗反許叔重云猥[撼咸+(句口+夕)]凡也蒼頡篇頓也廣疋眾也說文云從犬畏聲)。",
        "from": "T54n2128_096:0907c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "裨諶(上婢彌反下甚王反鄭大夫)。",
        "from": "T54n2128_096:0907c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冕旒(上音免下音流已具釋高僧傳中也)。",
        "from": "T54n2128_096:0907c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "褚翜(衫甲反人名也集本作翜誤也下文同)。",
        "from": "T54n2128_096:0907c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "炤炤(之曜反蒼頡篇云炤燭也廣疋云明也古今正字從火召聲亦作照)。",
        "from": "T54n2128_096:0907c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慺慺(漏頭反字書云慺謹敬皃也古今正字從心婁聲也)。",
        "from": "T54n2128_096:0907c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "張敞(昌兩反人名也)。",
        "from": "T54n2128_096:0907c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[(剌口+(俎且+(俎且)))木+夫]戶(上瘡[(木(企止)+((人/人)*(人/人)))/大]反字書云[(剌口+(俎且+(俎且)))木+夫][刷巾+羊]也古今正字從刀[(木(企止)+((人/人)*(人/人)))/大]聲集作[利禾+垂]撿字書並無此字雖有[(剌口+(俎且+(俎且)))木+夫]字於義猶未得通故兩出之也)。",
        "from": "T54n2128_096:0907c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "濠上(上号高反顧野王云濠水名莊子與惠子遊於濠梁之上也古今正字從水豪聲)。",
        "from": "T54n2128_096:0907c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餼羊(希既反古今正字云生曰餼熟曰饔從食氣聲饔音於容反)。",
        "from": "T54n2128_096:0907c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僚屬(上了彫反下正屬字孔注尚書云僚官也又左傳云大夫臣士土臣皁卓臣輿輿臣隷隷臣僚顧野王第九品人也說文從人尞聲亦作寮)。",
        "from": "T54n2128_096:0907c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "穢黷(上紆廢反下同鹿反賈逵云黷媟也聲類黑也廣疋蒙也說文握持垢也從黑賣聲)。",
        "from": "T54n2128_096:0907c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流漣(列纏反顧野王云漣漣淚下之皃也說文從水連聲)。",
        "from": "T54n2128_096:0907c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十三卷",
        "from": "T54n2128_096:0907c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "郗嘉賓(上恥知反人姓也)。",
        "from": "T54n2128_096:0907c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王該(改哀反人名也)。",
        "from": "T54n2128_096:0907c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蝡動(上而允反)。",
        "from": "T54n2128_096:0907c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倏忽(上詩陸反集本作儵俗用字)。",
        "from": "T54n2128_096:0907c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愆著(上起焉反已具前釋訖集作[攸ㄆ+(品/言)]古字下豬慮反)。",
        "from": "T54n2128_096:0908a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盻鮒(扶務反廣疋云鮒魚也周易井谷射鮒是也說文亦魚也從魚付聲也)。",
        "from": "T54n2128_096:0908a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "韞韣(上鬱吻反下音獨考聲云韞包藏也鄭注禮記云韞鞱也蒼頡篇束縛也古今正字義同二字並從韋昷蜀聲昷音溫也)。",
        "from": "T54n2128_096:0908a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過棰(上鵽瓜反下佳累反古今正字云檛棰也棰擊也二字並從木過垂皆聲)。",
        "from": "T54n2128_096:0908a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繾綣(上牽善反下闕阮反毛詩傳云繾綣反覆也杜注左傳云不離散也古今正字義同二字並從糸遣卷聲也)。",
        "from": "T54n2128_096:0908a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祉緣(上抽里反毛詩云祉福也說文義同從示止聲)。",
        "from": "T54n2128_096:0908a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "閘愚(上烏甲反說文云閘開閉門也從門甲聲也)。",
        "from": "T54n2128_096:0908a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苲[遻(這言)](上藏各反考聲云西南夷号亦作莋下卭各反花趺也集云苲蕚之檻其義未詳也)。",
        "from": "T54n2128_096:0908a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "巨鑊(黃郭反鄭注周禮云鑊鼎也又云煑肉器也說文云鑴也從金蒦聲音紆縛反雟音胡圭反)。",
        "from": "T54n2128_096:0908a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三朸(凌軄反說文云朸木理也從木力聲亦未詳此義也)。",
        "from": "T54n2128_096:0908a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利[此/朿](醉髓反考聲云鳥口也說文從此朿聲亦作觜集本作呰是毀呰字非也呰音子朿音七四反)。",
        "from": "T54n2128_096:0908a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攙槍(上仕銜反下鵲養反考聲云逆刺也古今正字並從木毚倉皆聲毚音同上)。",
        "from": "T54n2128_096:0908a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "狡狗(上交夘反山海經云有獸狀如犬而豹文角如牛名狡音如犬吠匃奴地有狡犬巨口黑身杜注左傳云狡滑也廣疋健也說文少狗也從犬交聲也)。",
        "from": "T54n2128_096:0908a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斐[斖且+(烈列+酉)](上妃尾反下微匪反已與釋第六卷中)。",
        "from": "T54n2128_096:0908a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苾芳(上頻蜜反字林云苾大香也從廾泌聲亦作馝字)。",
        "from": "T54n2128_096:0908a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爍爍(商灼反顧野王云銷也古今正字云人民[泳永+(瑤王)]爍不固也從火樂聲)。",
        "from": "T54n2128_096:0908a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遐[這言+正](紙盈反顧野王云遠也說文行也從辵正聲辵音丑略反亦作征也)。",
        "from": "T54n2128_096:0908a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "椿菌(上黜屯反莊子云有大椿者以八百歲為春八百歲為秋也古今正字從木春聲下逵殞反郭璞云菌地蕈也似蓋莊子云朝菌不知晦朔司馬彪云夭芝也天陰生糞上說文義與郭同從廾囷省聲囷音屈倫反蕈音深荏反)。",
        "from": "T54n2128_096:0908a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "駉末(上古熒反說文云駉牧馬苑也從馬冋聲冋音同也)。",
        "from": "T54n2128_096:0908a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愚黠(閑軋反方言云自關而東趙魏之間謂慧為黠郭璞云慧了也說文堅黑也從黑吉聲軋音烏黠反)。",
        "from": "T54n2128_096:0908a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喁喁(虞恭反淮南子云群生莫不隅然仰其德也說文眾口上見也從口禺聲音禺)。",
        "from": "T54n2128_096:0908a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓼蘇(上音了說文云蓼辛菜也從廾翏聲下素胡反尒疋云蘇桂荏也說文義同從廾穌聲翏音力救反蘇音同上)。",
        "from": "T54n2128_096:0908b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "熠燿(上[泳永+(瑤王)]挹反下怡照反毛詩傳云熠燿燐也即熒火也說文義同並從火習翟聲也亦作煜燿音義並同燐音[希巾+ㄙ]也)。",
        "from": "T54n2128_096:0908b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高翬(毀違反說文云翬大飛也從羽軍聲也)。",
        "from": "T54n2128_096:0908b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "匠椹(縶林反已具釋雜事律也)。",
        "from": "T54n2128_096:0908b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "睒倏(上苫染反考聲云目不定也古今正字從目炎聲下舒音反已具釋訖)。",
        "from": "T54n2128_096:0908b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞥若(普蔑反已具釋雜事律中)。",
        "from": "T54n2128_096:0908b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眴目(上玄絹反王逸云眴視皃也顧野王云如今人動目密相試語也說文目捶也從目[句口+目]聲也)。",
        "from": "T54n2128_096:0908b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "炯電(上公迥反蒼頡篇云炯明也說文光也從火冋聲冋音扃下文炯烔音並同此)。",
        "from": "T54n2128_096:0908b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "石飆(婢姚反尒疋云扶搖謂之飆郭云暴風從上下者也說文義同從風猋聲猋音同上也)。",
        "from": "T54n2128_096:0908b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鵬鵾(上音朋下音昆案莊子云鵬鯤魚所化其背不知幾千里搏扶搖而上九万里楚辭云鵾鷄嘲哳而悲鳴顧野王云狀如鶴而大說文云二字並從鳥用昆皆聲亦作鶤)。",
        "from": "T54n2128_096:0908b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十四卷",
        "from": "T54n2128_096:0908b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[王*(虍七+(一/八/八/目))]璣(上似緣反下音機尚書云[王*(虍七+(一/八/八/目))]璣玉衡以齊七政孔注云正天文之器運轉者也說文義同二字並從玉睿聲集作珽俗字也)。",
        "from": "T54n2128_096:0908b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翱翔(上敖高反下象羊反毛詩傳云翱翔猶仿佯也鄭云逍遙也郭璞云鳥飛布翅也說文迴飛也二字並從羽皐羊皆聲集作[睪*羽]俗也)。",
        "from": "T54n2128_096:0908b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[山/毒]气(上音獨下欺既反考聲云陰陽之所生[黍/日]殠之所聞也說文雲气也象形亦作氣集本作炁古文字下文同此也)。",
        "from": "T54n2128_096:0908b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黿鼉(上阮袁反下度河反說文云黿大鼈從黽元聲山海經云江水出鼉郭璞云似蜥蜴大者長文有鱗釆皮可以為鼓說文水蟲也餘義同郭從黽單聲也集本作魭[施方+魚]俗字也鼈音卑滅反黽音萌梗反)。",
        "from": "T54n2128_096:0908b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "狙玃(上七餘反廣疋云狙獼猴類也說文玃屬從犬且聲下居籰反郭注尒疋云似獼猴而大蒼黑色能攫持人好顧[目*丐][目*丐]說文云母猴也從犬矍聲攫音俱縛反籰音王矍反矍音吁籰反)。",
        "from": "T54n2128_096:0908b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惒羅子(上音禾梵語也)。",
        "from": "T54n2128_096:0908b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曀染(上伊計反毛詩傳云陰而風日曀說文從日壹聲也)。",
        "from": "T54n2128_096:0908b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸慪(緱侯反字書云慪歌也喜也吟也古今正字義同從心區聲或作謳亦作嘔音義並同也)。",
        "from": "T54n2128_096:0908c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後隊(徒對反杜注左傳云隊部也百人為一隊說文作隊從阝遂聲集作[阿可+匱]於義乖失非也)。",
        "from": "T54n2128_096:0908c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蕛稗(上弟泥反郭云[竺二+稊]似稗布也而生穢草也說文從草稊聲下排懈反杜注左傳云草之似穀者也古今正字義同也從禾卑聲也)。",
        "from": "T54n2128_096:0908c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "跋扈(上槃末反下胡古反漢書音義云跋扈自縱恣皃也[薩產+辛]琮云勇健皃也跋字已釋訖說文扈從邑戶聲也)。",
        "from": "T54n2128_096:0908c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高驤(想羊反說文云馬低仰也從馬襄聲下文同也)。",
        "from": "T54n2128_096:0908c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嵯峨(上昨何反下我哥反廣疋云高皃也王逸云山高峻蔽曰也古今正字並從山差我皆聲集嗟俗字也)。",
        "from": "T54n2128_096:0908c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "熯晨(上然善反亦音漢蒼頡篇云火乾也說文云亦乾皃也從火漢省聲也)。",
        "from": "T54n2128_096:0908c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祖禰(泥禮反鄭眾云禰父廟也古今正字義同從示爾聲)。",
        "from": "T54n2128_096:0908c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畷[示*昔](上知衛反。",
        "from": "T54n2128_096:0908c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毛詩傳云畷表也。",
        "from": "T54n2128_096:0908c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鄭注禮記云田畯所督約百姓於井間之處也所以表行列。",
        "from": "T54n2128_096:0908c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說文兩陌間道也。",
        "from": "T54n2128_096:0908c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣六尺從田叕聲。",
        "from": "T54n2128_096:0908c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下查駕反。",
        "from": "T54n2128_096:0908c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮記云天子大[示*昔]八伊耆氏始為[示*昔][示*昔]也者索也歲十二月而合聚萬物而索饗之[示*昔]之祭也主先嗇而祭司嗇也祭百種以報嗇也仁之至也義之盡也。",
        "from": "T54n2128_096:0908c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黃衣黃冠而祭息田夫也。",
        "from": "T54n2128_096:0908c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鄭玄云祭謂既[示*昔]臘先祖五祀於是勞農以休息之有八者。",
        "from": "T54n2128_096:0908c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一先嗇二司嗇三農四表畷五猫虎六防七水滽八昆虫。",
        "from": "T54n2128_096:0908c13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣疋云祭也夏曰清祀殷曰嘉平周曰大[示*昔]秦曰臘。",
        "from": "T54n2128_096:0908c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "古今正字從衣昔聲集作蜡亦通)。",
        "from": "T54n2128_096:0908c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "基址(之耳反說文云址基也從土止聲集本作墌恐誤也)。",
        "from": "T54n2128_096:0908c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耼適(菼甘反老君也)。",
        "from": "T54n2128_096:0908c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燔燼(上伐袁反說文云燔燒也從火番聲下似進反鄭箋詩云火餘曰燼說文作[盡((烈列)/皿)+火]亦云火餘也從火聿聲)。",
        "from": "T54n2128_096:0908c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切經音義卷第九十六",
        "from": "T54n2128_096:0908c18_0",
        "context_with_next_sentence": false
    }
]