[
    {
        "source_sentence": "佛說呪時氣病經",
        "from": "T21n1326_001:0491a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無佛南無法南無比丘僧。",
        "from": "T21n1326_001:0491a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無過去七佛南無現在諸佛南無未來諸佛。",
        "from": "T21n1326_001:0491a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無諸佛弟子。",
        "from": "T21n1326_001:0491a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令我所呪即從如願。",
        "from": "T21n1326_001:0491a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿佉尼尼佉尼阿佉耶尼佉尼阿毘羅慢多利波池尼波提梨南無佛南無法南無比丘僧。",
        "from": "T21n1326_001:0491a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無過去七佛南無現在諸佛南無未來諸佛。",
        "from": "T21n1326_001:0491a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無諸佛弟子南無諸師南無諸師弟子。",
        "from": "T21n1326_001:0491a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令我所呪即從如願。",
        "from": "T21n1326_001:0491a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人得時氣病。",
        "from": "T21n1326_001:0491a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結縷七過呪之。",
        "from": "T21n1326_001:0491a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并書此上鬼神名字。",
        "from": "T21n1326_001:0491a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若紙槐皮上。",
        "from": "T21n1326_001:0491a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫著縷頭。",
        "from": "T21n1326_001:0491a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讀是呪時。",
        "from": "T21n1326_001:0491a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當齋戒清淨澡漱燒香。",
        "from": "T21n1326_001:0491a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正心乃說之。",
        "from": "T21n1326_001:0491a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說呪時氣病經",
        "from": "T21n1326_001:0491a22_0",
        "context_with_next_sentence": false
    }
]