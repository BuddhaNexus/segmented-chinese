[
    {
        "source_sentence": "判比量論",
        "from": "X53n0860_001:0951a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定過亦能破彼、是等難故。",
        "from": "X53n0860_001:0951a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又應定問、汝言非顯淨土言中、淨土之慧為舉、淨土之體為不舉□。",
        "from": "X53n0860_001:0951a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若言舉者、則違自宗、此淨土教能顯淨土故。",
        "from": "X53n0860_001:0951a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不舉者、不違他宗、非顯之言不□淨土故。",
        "from": "X53n0860_001:0951a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此兩兩關心、辨彼意在前、則墮自語相違過失。",
        "from": "X53n0860_001:0951a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若彼救言、此淨土忽舉淨土體、而云忽不入淨土之教。",
        "from": "X53n0860_001:0951a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故無自語相違過者、則以此忽亦成不定。",
        "from": "X53n0860_001:0951a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是進退皆不應理。",
        "from": "X53n0860_001:0951a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　　二量。",
        "from": "X53n0860_001:0951a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八執四分者為破三分立比量云、自證必有即體能證。",
        "from": "X53n0860_001:0951a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心分攝故。",
        "from": "X53n0860_001:0951a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如相分。",
        "from": "X53n0860_001:0951a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自證應非心分所攝。",
        "from": "X53n0860_001:0951a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以無即體之能證故。",
        "from": "X53n0860_001:0951a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如兔角等。",
        "from": "X53n0860_001:0951a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "判云、此二比量是似非真。",
        "from": "X53n0860_001:0951a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆不能離不定過故。",
        "from": "X53n0860_001:0951a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂、自證分、為如相分、心分攝故、有即體能證。",
        "from": "X53n0860_001:0951a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為如眼識生相、心分攝故、無即體能證。",
        "from": "X53n0860_001:0951a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是前因有不定過。",
        "from": "X53n0860_001:0951a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又自證分、為如兔角、無即體能證故、非心分攝。",
        "from": "X53n0860_001:0951a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為如耳識相分三相、無即體能證故、是心分所攝。",
        "from": "X53n0860_001:0951a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是後因亦有不定。",
        "from": "X53n0860_001:0951a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若彼救言、五識三相不離體故、是其自證之所緣境、理亦不然。",
        "from": "X53n0860_001:0951b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相分三相不離相故。",
        "from": "X53n0860_001:0951b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五識見分亦得緣故。",
        "from": "X53n0860_001:0951b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此若不許、彼何得然。",
        "from": "X53n0860_001:0951b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設許彼前、此必不許、五識能緣法界諸處、法相雜亂。",
        "from": "X53n0860_001:0951b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "違理教故。",
        "from": "X53n0860_001:0951b05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "只由如是相分三相、於彼二因、足作不定。",
        "from": "X53n0860_001:0951b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設彼救言、相分三相非心分攝、則有比量相違過失。",
        "from": "X53n0860_001:0951b06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知、第四分、有言而無義。",
        "from": "X53n0860_001:0951b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　二量。",
        "from": "X53n0860_001:0951b08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九無性攝論、為成第八、對彼小乘立二比量。",
        "from": "X53n0860_001:0951b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂八識教是聖言攝。",
        "from": "X53n0860_001:0951b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "似無我故。",
        "from": "X53n0860_001:0951b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如四阿含。",
        "from": "X53n0860_001:0951b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又八識教契當道理。",
        "from": "X53n0860_001:0951b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是聖教故。",
        "from": "X53n0860_001:0951b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如六識教。",
        "from": "X53n0860_001:0951b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是展轉證有八識。",
        "from": "X53n0860_001:0951b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今於此中、直就所詮而立比量證□□□謂眼耳鼻識必有舌身意識不攝餘別識。",
        "from": "X53n0860_001:0951b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非二六門中三識攝故。",
        "from": "X53n0860_001:0951b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如舌身意識。",
        "from": "X53n0860_001:0951b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中極成六識為他異品、自許八識為自異品、三識攝因於彼不轉。",
        "from": "X53n0860_001:0951b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故此因決定能立。",
        "from": "X53n0860_001:0951b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若以轉識攝故為因、則於他異轉。",
        "from": "X53n0860_001:0951b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設以是識性故為因、亦於自異品轉、皆不能離不定過也。",
        "from": "X53n0860_001:0951b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　　　　　　　　三量。",
        "from": "X53n0860_001:0951b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十成唯識論立比量言、第八必有俱有所依。",
        "from": "X53n0860_001:0951b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是識性故。",
        "from": "X53n0860_001:0951b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如六識等。",
        "from": "X53n0860_001:0951b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此因不定、有等難故。",
        "from": "X53n0860_001:0951b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂有立言、第八必無俱有所依。",
        "from": "X53n0860_001:0951b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是根本故。",
        "from": "X53n0860_001:0951b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如真如。",
        "from": "X53n0860_001:0951b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若言此有有法差別相違過失、能成第八是無為故、是則前因亦有是過、能成第八是轉識故。",
        "from": "X53n0860_001:0951b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若言自□故不成難、彼亦違自故非難也。",
        "from": "X53n0860_001:0951c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今者別立、賴耶末那必無俱有所依之根。",
        "from": "X53n0860_001:0951c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非六識性之所攝故。",
        "from": "X53n0860_001:0951c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如眼根等。",
        "from": "X53n0860_001:0951c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若難此因有相違過、能成七八非能緣性、如眼根等、此亦不然。",
        "from": "X53n0860_001:0951c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由心所法成不定故。",
        "from": "X53n0860_001:0951c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若言望前亦有不定、以心所法非六識性有所依故、此非不定。",
        "from": "X53n0860_001:0951c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以前立言所依根故。",
        "from": "X53n0860_001:0952a01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若望心所、但是所依非所依根、法處所攝不待根故。",
        "from": "X53n0860_001:0952a01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故彼宗、雖知依與所依差別、未解所依與根有異。",
        "from": "X53n0860_001:0952a02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若論所依、通於八識及與心所。",
        "from": "X53n0860_001:0952a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其所依根不通心所及於七八。",
        "from": "X53n0860_001:0952a04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有破此宗立比量云、意識俱有根定非能能緣性。",
        "from": "X53n0860_001:0952a05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六識心心所之所不攝故。",
        "from": "X53n0860_001:0952a06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六識俱有根隨一所攝故、如眼根等彼宗反以法處所攝色法為意、故作是難。",
        "from": "X53n0860_001:0952a07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此塵通破大乘諸宗。",
        "from": "X53n0860_001:0952a08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然有相違決定過生。",
        "from": "X53n0860_001:0952a09_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂立意根必非色性。",
        "from": "X53n0860_001:0952a09_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有分別識不共依故。",
        "from": "X53n0860_001:0952a10_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如第六識俱有作意。",
        "from": "X53n0860_001:0952a10_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此等難彼因不定。",
        "from": "X53n0860_001:0952a11_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　五量。",
        "from": "X53n0860_001:0952a11_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十一如聲論師立聲為常所聞性故、若對勝論相違決定、對佛弟子不共不定、以無共許同品法故。",
        "from": "X53n0860_001:0952a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有難此因立比量言、所聞性因應非疑因。",
        "from": "X53n0860_001:0952a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同品無故。",
        "from": "X53n0860_001:0952a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如相違因。",
        "from": "X53n0860_001:0952a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又立、此因應非不定。",
        "from": "X53n0860_001:0952a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "異品無故。",
        "from": "X53n0860_001:0952a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如正因。",
        "from": "X53n0860_001:0952a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "備法師云、理門論言、一向離故、是通彼難。",
        "from": "X53n0860_001:0952a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂立宗言、所聞性因是不定攝。",
        "from": "X53n0860_001:0952a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一向離故。",
        "from": "X53n0860_001:0952a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如共不定。",
        "from": "X53n0860_001:0952a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一向離言闕一相也。",
        "from": "X53n0860_001:0952a18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "判云、此因有不定過。",
        "from": "X53n0860_001:0952a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以所見性雖闕一相而非不定、是不成故。",
        "from": "X53n0860_001:0952a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂立聲無常、所見性故、此因同有異無、唯闕初相。",
        "from": "X53n0860_001:0952a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故亦為闕一相也。",
        "from": "X53n0860_001:0952a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若為避此不定過故、更立因言、後二相中闕一相故。",
        "from": "X53n0860_001:0952a22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如共等四不定因。",
        "from": "X53n0860_001:0952a23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此因亦有餘不定過。",
        "from": "X53n0860_001:0952a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如於空宗緣生故因。",
        "from": "X53n0860_001:0952a24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖於後二相中闕一　而是真因、非不定故、故不能作相違決定。",
        "from": "X53n0860_001:0952b01_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又前所立異品無故非疑因者、亦有不定。",
        "from": "X53n0860_001:0952b02_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如諸相違決定之因。",
        "from": "X53n0860_001:0952b03_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖異品無是疑因。",
        "from": "X53n0860_001:0952b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故唯有同品無故之因、且離不定、立非疑因。",
        "from": "X53n0860_001:0952b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中應立相違比量。",
        "from": "X53n0860_001:0952b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂、所聞性不定因攝。",
        "from": "X53n0860_001:0952b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "等立相違宗故。",
        "from": "X53n0860_001:0952b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如共不定因。",
        "from": "X53n0860_001:0952b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如理門論顯此因云、以若不共所成立法、立所有差別遍攝一切、皆是疑因。",
        "from": "X53n0860_001:0952b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯彼有性彼所攝故、一向離故。",
        "from": "X53n0860_001:0952b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案云、不共所成立者、如立聲常所聞性故、或立無常所聞性故、如是一切無不等立。",
        "from": "X53n0860_001:0952b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故言所有遍攝一切。",
        "from": "X53n0860_001:0952b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由是道理、所聞性因望彼一切皆是疑因。",
        "from": "X53n0860_001:0952b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一向離故者、轉成等立諸宗之義。",
        "from": "X53n0860_001:0952b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以望諸宗皆同不共。",
        "from": "X53n0860_001:0952b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆同是一向義。",
        "from": "X53n0860_001:0952b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不共是其離義。",
        "from": "X53n0860_001:0952b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由一向離故、等立於諸々宗々相違故、其因是不定。",
        "from": "X53n0860_001:0952b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　　　五量。",
        "from": "X53n0860_001:0952b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二相違決定立二比量。",
        "from": "X53n0860_001:0952b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文軌法師自作問答。",
        "from": "X53n0860_001:0952b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "X53n0860_001:0952b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足三相應是正因、何故此中而言不定。",
        "from": "X53n0860_001:0952b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "X53n0860_001:0952b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此疑未決、不敢解之。",
        "from": "X53n0860_001:0952b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有通釋者、隨而為注此中問意、立比量云、違決中因應是正因。",
        "from": "X53n0860_001:0952b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具三相故。",
        "from": "X53n0860_001:0952b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如餘真因。",
        "from": "X53n0860_001:0952b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今者通曰、違決之因非正因攝。",
        "from": "X53n0860_001:0952b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有等難故。",
        "from": "X53n0860_001:0952b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如相違因。",
        "from": "X53n0860_001:0952b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此顯彼有不定過。",
        "from": "X53n0860_001:0952b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此二因非相違攝。",
        "from": "X53n0860_001:0952b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同品有故。",
        "from": "X53n0860_001:0952b23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如正因。",
        "from": "X53n0860_001:0952b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此二因非不成攝。",
        "from": "X53n0860_001:0952b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是共許故。",
        "from": "X53n0860_001:0952b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如不共因。",
        "from": "X53n0860_001:0952c01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是二因不定因攝。",
        "from": "X53n0860_001:0952c01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非正、非違、非不成故。",
        "from": "X53n0860_001:0952c01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如餘五種不定因也。",
        "from": "X53n0860_001:0952c02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　六量。",
        "from": "X53n0860_001:0952c02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十三或有為難五種之性、立比量言、無性有情必當作佛。",
        "from": "X53n0860_001:0952c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以有心故。",
        "from": "X53n0860_001:0952c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如有性者。",
        "from": "X53n0860_001:0952c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此因不定、故成不難。",
        "from": "X53n0860_001:0952c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為如諸佛、以有心故、非當作佛。",
        "from": "X53n0860_001:0952c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為如菩薩、以有心故、必當作佛。",
        "from": "X53n0860_001:0952c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前所立因、言以未成佛之有情故、此因亦有他不定過。",
        "from": "X53n0860_001:0952c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為如菩薩種性、為如決定二乘。",
        "from": "X53n0860_001:0952c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若為避此、更立宗言、無性性有情決定二乘皆當作佛。",
        "from": "X53n0860_001:0952c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以未成佛有情攝故。",
        "from": "X53n0860_001:0952c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如菩薩。",
        "from": "X53n0860_001:0952c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此有等難、故成不定。",
        "from": "X53n0860_001:0952c10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是三人非當作佛。",
        "from": "X53n0860_001:0952c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以無大乘無漏種子而非菩薩種性攝故。",
        "from": "X53n0860_001:0952c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如木石等諸無情物。",
        "from": "X53n0860_001:0952c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又有比量相違過失。",
        "from": "X53n0860_001:0953a01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂、五種姓中、餘四種姓墮地獄時、應有四德、許作佛故、如菩薩姓。",
        "from": "X53n0860_001:0953a01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "許則違教、不許違理。",
        "from": "X53n0860_001:0953a03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此違自悟比量過也。",
        "from": "X53n0860_001:0953a03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　　　五量。",
        "from": "X53n0860_001:0953a04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十四成唯識論、為破我法立比量言、凡諸我見不緣實我。",
        "from": "X53n0860_001:0953a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有所緣故。",
        "from": "X53n0860_001:0953a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如緣餘心。",
        "from": "X53n0860_001:0953a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我見所緣定非實我。",
        "from": "X53n0860_001:0953a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是所緣故。",
        "from": "X53n0860_001:0953a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如所餘法。",
        "from": "X53n0860_001:0953a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又言、外道餘乘所執諸法、異心心所、非實有性。",
        "from": "X53n0860_001:0953a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是所取故。",
        "from": "X53n0860_001:0953a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如心心所。",
        "from": "X53n0860_001:0953a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能取彼覺亦不緣彼。",
        "from": "X53n0860_001:0953a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是能取故。",
        "from": "X53n0860_001:0953a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如緣此覺。",
        "from": "X53n0860_001:0953a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "判云、此中有四比量。",
        "from": "X53n0860_001:0953a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是真能破。",
        "from": "X53n0860_001:0953a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破我法故。",
        "from": "X53n0860_001:0953a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無過生故。",
        "from": "X53n0860_001:0953a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或因此破、破大乘云、諸緣第八識見、不緣阿賴耶相。",
        "from": "X53n0860_001:0953a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有所緣故。",
        "from": "X53n0860_001:0953a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如緣。",
        "from": "X53n0860_001:0953a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　證成道理甚難思　　自非笑却微易解　今依聖典舉一隅　　願通佛道流三世判比量論一卷　釋元曉述",
        "from": "X53n0860_001:0953a14_0",
        "context_with_next_sentence": false
    }
]