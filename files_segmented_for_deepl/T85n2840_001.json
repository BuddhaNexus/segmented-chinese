[
    {
        "source_sentence": "三界圖",
        "from": "T85n2840_001:1293a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無色界&;",
        "from": "T85n2840_001:1293a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無明&;慢&;&;貪&;&;&;&;&;&;&;&;修&;無明&;慢&;&;貪&;疑&;戒禁&;見取&;邪見&;&;&;&;道&;無明&;慢&;&;貪&;疑&;&;見取&;邪見&;&;&;&;滅&;無明&;慢&;&;貪&;疑&;&;見取&;邪見&;&;&;&;集&;無明&;慢&;&;貪&;疑&;戒禁&;見取&;邪見&;邊見&;身見&;&;苦&;色界&;無明&;慢&;&;貪&;&;&;&;&;&;&;&;修&;無明&;慢&;&;貪&;疑&;戒禁&;見取&;邪見&;&;&;&;道&;無明&;慢&;&;貪&;疑&;&;見取&;邪見&;&;&;&;滅&;無明&;慢&;&;貪&;疑&;&;見取&;邪見&;&;&;&;集&;無明&;慢&;&;貪&;疑&;戒禁&;見取&;邪見&;邊見&;身見&;&;苦&;欲界&;無明&;慢&;瞋&;貪&;&;&;&;&;&;&;&;修&;無明&;慢&;瞋&;貪&;疑&;戒禁&;見取&;邪見&;&;&;&;道&;無明&;慢&;瞋&;貪&;疑&;&;見取&;邪見&;&;&;&;滅&;無明&;慢&;瞋&;貪&;疑&;&;見取&;邪見&;&;&;&;集&;無明&;慢&;瞋&;貪&;疑&;戒禁&;見取&;邪見&;邊見&;身見&;&;苦&;欲界見斷煩惱三十二修斷煩惱有四。",
        "from": "T85n2840_001:1293a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色界見斷二十八修斷有三。",
        "from": "T85n2840_001:1293b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無色界亦然。",
        "from": "T85n2840_001:1293b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三界共計有九十八。",
        "from": "T85n2840_001:1293b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂彼四諦十六門差別違立名安諦。",
        "from": "T85n2840_001:1293b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十六門者。",
        "from": "T85n2840_001:1293b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即四諦下各四行相。",
        "from": "T85n2840_001:1293b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦諦四者謂即非常苦空無我。",
        "from": "T85n2840_001:1293b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "集諦四者因集生緣。",
        "from": "T85n2840_001:1293b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅諦四者滅淨妙離。",
        "from": "T85n2840_001:1293b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道諦四者道如行出。",
        "from": "T85n2840_001:1293b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋云。",
        "from": "T85n2840_001:1293b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有生滅故非常。",
        "from": "T85n2840_001:1293b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逼迫性故苦。",
        "from": "T85n2840_001:1293b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "違我所見故空。",
        "from": "T85n2840_001:1293b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "違我見故非我。",
        "from": "T85n2840_001:1293b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能生法故因。",
        "from": "T85n2840_001:1293b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有多種故集。",
        "from": "T85n2840_001:1293b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒資產故生。",
        "from": "T85n2840_001:1293b16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各別助故緣。",
        "from": "T85n2840_001:1293b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸蘊盡故滅。",
        "from": "T85n2840_001:1293b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三相息故淨。",
        "from": "T85n2840_001:1293b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無眾患故妙。",
        "from": "T85n2840_001:1293b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脫眾災故離。",
        "from": "T85n2840_001:1293b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通行義故道。",
        "from": "T85n2840_001:1293b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "契正理故如。",
        "from": "T85n2840_001:1293b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正趣向故行。",
        "from": "T85n2840_001:1293b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能永超故出。",
        "from": "T85n2840_001:1293b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦諦有四智。",
        "from": "T85n2840_001:1293b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一苦法忍。",
        "from": "T85n2840_001:1293b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二苦法智。",
        "from": "T85n2840_001:1293b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三苦類忍。",
        "from": "T85n2840_001:1293b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四苦類智。",
        "from": "T85n2840_001:1293b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對治身見等十種煩惱。",
        "from": "T85n2840_001:1293b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘三諦亦然。",
        "from": "T85n2840_001:1293b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "集法忍集法智集類忍集類智。",
        "from": "T85n2840_001:1293b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對治邪見等七使。",
        "from": "T85n2840_001:1293b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅法忍滅法智滅類忍滅類智。",
        "from": "T85n2840_001:1293b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對治滅諦煩惱。",
        "from": "T85n2840_001:1293b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道法忍道法智道類忍道類智。",
        "from": "T85n2840_001:1293b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對治道諦煩惱。",
        "from": "T85n2840_001:1293b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修斷者。",
        "from": "T85n2840_001:1293b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於四諦上各各比忍比智。",
        "from": "T85n2840_001:1293b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對治欲界貪瞋慢無明四箇煩惱。",
        "from": "T85n2840_001:1293b26_11",
        "context_with_next_sentence": false
    }
]