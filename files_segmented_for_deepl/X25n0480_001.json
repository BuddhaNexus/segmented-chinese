[
    {
        "source_sentence": "金剛般若波羅蜜經觀心釋",
        "from": "X25n0480_001:0149a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩沙彌　智旭際明　述",
        "from": "X25n0480_001:0149a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　已申如實不空義　　如如演說不取著　攝彼名句及文身　　融歸一念成觀行金剛者。",
        "from": "X25n0480_001:0149a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喻現前一念心也。",
        "from": "X25n0480_001:0149a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如金剛。",
        "from": "X25n0480_001:0149a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體則無上至寶。",
        "from": "X25n0480_001:0149a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相則純淨無雜。",
        "from": "X25n0480_001:0149a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用則廣能利益。",
        "from": "X25n0480_001:0149a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現前一念。",
        "from": "X25n0480_001:0149a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦復如是。",
        "from": "X25n0480_001:0149a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體即法界。",
        "from": "X25n0480_001:0149a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相無塵染。",
        "from": "X25n0480_001:0149a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用徧一切。",
        "from": "X25n0480_001:0149a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次現前一念。",
        "from": "X25n0480_001:0149a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是金剛。",
        "from": "X25n0480_001:0149a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間金剛之寶。",
        "from": "X25n0480_001:0149a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "假心為喻。",
        "from": "X25n0480_001:0149a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃世寶似心。",
        "from": "X25n0480_001:0149a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非心似世寶也。",
        "from": "X25n0480_001:0149a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此現前一念金剛之心。",
        "from": "X25n0480_001:0149a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即三般若。",
        "from": "X25n0480_001:0149a15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法界真體。",
        "from": "X25n0480_001:0149a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為實相。",
        "from": "X25n0480_001:0149a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無塵染相。",
        "from": "X25n0480_001:0149a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為觀照。",
        "from": "X25n0480_001:0149a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徧一切用。",
        "from": "X25n0480_001:0149a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為文字。",
        "from": "X25n0480_001:0149a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "達此一念三般若者。",
        "from": "X25n0480_001:0149a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名波羅蜜。",
        "from": "X25n0480_001:0149a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以即三德究竟之彼岸故。",
        "from": "X25n0480_001:0149a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此一念性。",
        "from": "X25n0480_001:0149a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亘古亘今常不變故。",
        "from": "X25n0480_001:0149a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可軌持故。",
        "from": "X25n0480_001:0149a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切聖賢由此得成諸道果故。",
        "from": "X25n0480_001:0149a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即名為經。",
        "from": "X25n0480_001:0149a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次。",
        "from": "X25n0480_001:0149a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現前一念之性。",
        "from": "X25n0480_001:0149a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從來不異。",
        "from": "X25n0480_001:0149a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名如。",
        "from": "X25n0480_001:0149a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此外更無別法。",
        "from": "X25n0480_001:0149a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名是。",
        "from": "X25n0480_001:0149a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "統攝自在。",
        "from": "X25n0480_001:0149a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名我。",
        "from": "X25n0480_001:0149a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了了印境。",
        "from": "X25n0480_001:0149a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名聞。",
        "from": "X25n0480_001:0149a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十世古今不離當念。",
        "from": "X25n0480_001:0149a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名一時。",
        "from": "X25n0480_001:0149a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虗靈絕待。",
        "from": "X25n0480_001:0149a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名佛。",
        "from": "X25n0480_001:0149a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豎窮橫徧。",
        "from": "X25n0480_001:0149a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名在。",
        "from": "X25n0480_001:0149a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "含育出生一切諸法。",
        "from": "X25n0480_001:0149a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名舍衛國。",
        "from": "X25n0480_001:0149a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但悟惟心。",
        "from": "X25n0480_001:0149a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能降魔外。",
        "from": "X25n0480_001:0149a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名戰勝。",
        "from": "X25n0480_001:0149a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功德之林。",
        "from": "X25n0480_001:0149a23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名樹。",
        "from": "X25n0480_001:0149a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性具功德。",
        "from": "X25n0480_001:0149a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周給一切。",
        "from": "X25n0480_001:0149a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名給孤獨。",
        "from": "X25n0480_001:0149a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬善所集。",
        "from": "X25n0480_001:0149a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名園。",
        "from": "X25n0480_001:0149a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次。",
        "from": "X25n0480_001:0149a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現前一念。",
        "from": "X25n0480_001:0149b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當體常徧。",
        "from": "X25n0480_001:0149b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名大。",
        "from": "X25n0480_001:0149b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出生福慧。",
        "from": "X25n0480_001:0149b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名比丘。",
        "from": "X25n0480_001:0149b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸心心所相應。",
        "from": "X25n0480_001:0149b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名眾。",
        "from": "X25n0480_001:0149b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一念具足百界十如。",
        "from": "X25n0480_001:0149b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為千。",
        "from": "X25n0480_001:0149b02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自行化他。",
        "from": "X25n0480_001:0149b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十善各各互具。",
        "from": "X25n0480_001:0149b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為二百。",
        "from": "X25n0480_001:0149b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五根。",
        "from": "X25n0480_001:0149b03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五力各各互具。",
        "from": "X25n0480_001:0149b03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為五十。",
        "from": "X25n0480_001:0149b03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如海印炳現。",
        "from": "X25n0480_001:0149b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無前無後。",
        "from": "X25n0480_001:0149b04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名俱。",
        "from": "X25n0480_001:0149b04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次。",
        "from": "X25n0480_001:0149b04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有一法出過於心。",
        "from": "X25n0480_001:0149b04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為世尊。",
        "from": "X25n0480_001:0149b05_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切時中。",
        "from": "X25n0480_001:0149b05_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常應觀察自心。",
        "from": "X25n0480_001:0149b05_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名食時。",
        "from": "X25n0480_001:0149b05_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自心本具慚愧法忍。",
        "from": "X25n0480_001:0149b05_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名衣。",
        "from": "X25n0480_001:0149b06_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自心事理和合相應。",
        "from": "X25n0480_001:0149b06_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名鉢。",
        "from": "X25n0480_001:0149b06_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歷陰界處觀察理性。",
        "from": "X25n0480_001:0149b06_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名入城乞食。",
        "from": "X25n0480_001:0149b07_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一一法得見理性。",
        "from": "X25n0480_001:0149b07_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名次第乞已。",
        "from": "X25n0480_001:0149b07_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "會事歸理。",
        "from": "X25n0480_001:0149b08_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名還本處。",
        "from": "X25n0480_001:0149b08_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀理究竟。",
        "from": "X25n0480_001:0149b08_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名飯食訖。",
        "from": "X25n0480_001:0149b08_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "全修在性。",
        "from": "X25n0480_001:0149b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名收衣鉢。",
        "from": "X25n0480_001:0149b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福慧明顯。",
        "from": "X25n0480_001:0149b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名洗足。",
        "from": "X25n0480_001:0149b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證於第一義空。",
        "from": "X25n0480_001:0149b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名敷座而坐。",
        "from": "X25n0480_001:0149b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次。",
        "from": "X25n0480_001:0149b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相應解心所。",
        "from": "X25n0480_001:0149b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名須菩提。",
        "from": "X25n0480_001:0149b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此解有殊勝力。",
        "from": "X25n0480_001:0149b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為長老。",
        "from": "X25n0480_001:0149b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸心所中能為導首。",
        "from": "X25n0480_001:0149b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為在大眾中。",
        "from": "X25n0480_001:0149b11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不取空證。",
        "from": "X25n0480_001:0149b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名從座起。",
        "from": "X25n0480_001:0149b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "順方便道。",
        "from": "X25n0480_001:0149b12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名偏袒右肩。",
        "from": "X25n0480_001:0149b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右膝著地。",
        "from": "X25n0480_001:0149b12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "權不異實。",
        "from": "X25n0480_001:0149b13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為合掌。",
        "from": "X25n0480_001:0149b13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如理觀察。",
        "from": "X25n0480_001:0149b13_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自覺聖智境界。",
        "from": "X25n0480_001:0149b13_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名恭敬白佛。",
        "from": "X25n0480_001:0149b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心性不可思議。",
        "from": "X25n0480_001:0149b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名希有。",
        "from": "X25n0480_001:0149b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量眾善。",
        "from": "X25n0480_001:0149b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名諸菩薩。",
        "from": "X25n0480_001:0149b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾善不離一心。",
        "from": "X25n0480_001:0149b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名善護念。",
        "from": "X25n0480_001:0149b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心具行眾善。",
        "from": "X25n0480_001:0149b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名善付囑。",
        "from": "X25n0480_001:0149b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一念照性。",
        "from": "X25n0480_001:0149b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名善男子。",
        "from": "X25n0480_001:0149b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一念寂性。",
        "from": "X25n0480_001:0149b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名善女人。",
        "from": "X25n0480_001:0149b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依寂照體。",
        "from": "X25n0480_001:0149b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起寂照用。",
        "from": "X25n0480_001:0149b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為發菩提心。",
        "from": "X25n0480_001:0149b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用如其體。",
        "from": "X25n0480_001:0149b17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為應住。",
        "from": "X25n0480_001:0149b17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永離昏擾。",
        "from": "X25n0480_001:0149b18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為降伏其心。",
        "from": "X25n0480_001:0149b18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心之理性隨解現故。",
        "from": "X25n0480_001:0149b18_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名佛告須菩提。",
        "from": "X25n0480_001:0149b18_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如理而觀。",
        "from": "X25n0480_001:0149b19_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀即如理。",
        "from": "X25n0480_001:0149b19_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名善哉善哉。",
        "from": "X25n0480_001:0149b19_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次。",
        "from": "X25n0480_001:0149b19_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現前一念。",
        "from": "X25n0480_001:0149b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足無量染淨種子及現行故。",
        "from": "X25n0480_001:0149b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為一切眾生之類。",
        "from": "X25n0480_001:0149b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無明覆蔽。",
        "from": "X25n0480_001:0149b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為卵生。",
        "from": "X25n0480_001:0149b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隱覆含藏。",
        "from": "X25n0480_001:0149b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為胎生。",
        "from": "X25n0480_001:0149b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛取所潤。",
        "from": "X25n0480_001:0149b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為濕生。",
        "from": "X25n0480_001:0149b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非有現有。",
        "from": "X25n0480_001:0149b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為化生。",
        "from": "X25n0480_001:0149b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心生像生。",
        "from": "X25n0480_001:0149b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為有色。",
        "from": "X25n0480_001:0149b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覓心無像。",
        "from": "X25n0480_001:0149b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為無色。",
        "from": "X25n0480_001:0149b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種攀緣。",
        "from": "X25n0480_001:0149b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為有想。",
        "from": "X25n0480_001:0149b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攀緣無性。",
        "from": "X25n0480_001:0149b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為無想。",
        "from": "X25n0480_001:0149b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所緣不實。",
        "from": "X25n0480_001:0149b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名非有想。",
        "from": "X25n0480_001:0149b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能緣不斷。",
        "from": "X25n0480_001:0149b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名非無想。",
        "from": "X25n0480_001:0149c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無明妄想本性空寂。",
        "from": "X25n0480_001:0149c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為無餘涅槃。",
        "from": "X25n0480_001:0149c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了妄不生。",
        "from": "X25n0480_001:0149c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為令入滅度。",
        "from": "X25n0480_001:0149c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心不生。",
        "from": "X25n0480_001:0149c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬法無咎。",
        "from": "X25n0480_001:0149c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如翳除。",
        "from": "X25n0480_001:0149c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空華悉殞。",
        "from": "X25n0480_001:0149c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為滅度無量無數無邊眾生。",
        "from": "X25n0480_001:0149c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華性本空。",
        "from": "X25n0480_001:0149c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非今始滅。",
        "from": "X25n0480_001:0149c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為實無眾生得滅度者。",
        "from": "X25n0480_001:0149c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一念中。",
        "from": "X25n0480_001:0149c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妄計宰主形相眾緣相續。",
        "from": "X25n0480_001:0149c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名我等四相。",
        "from": "X25n0480_001:0149c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迷空計華。",
        "from": "X25n0480_001:0149c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名非菩薩。",
        "from": "X25n0480_001:0149c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次。",
        "from": "X25n0480_001:0149c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心無心相。",
        "from": "X25n0480_001:0149c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名無所住。",
        "from": "X25n0480_001:0149c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念念出生。",
        "from": "X25n0480_001:0149c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名之為行。",
        "from": "X25n0480_001:0149c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不取一法。",
        "from": "X25n0480_001:0149c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為布施。",
        "from": "X25n0480_001:0149c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不染一法。",
        "from": "X25n0480_001:0149c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為持戒。",
        "from": "X25n0480_001:0149c07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不礙一法。",
        "from": "X25n0480_001:0149c07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為忍辱。",
        "from": "X25n0480_001:0149c07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不捨一法。",
        "from": "X25n0480_001:0149c08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為精進。",
        "from": "X25n0480_001:0149c08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不動一法。",
        "from": "X25n0480_001:0149c08_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為禪定。",
        "from": "X25n0480_001:0149c08_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不昧一法。",
        "from": "X25n0480_001:0149c08_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為智慧。",
        "from": "X25n0480_001:0149c09_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不壞一法。",
        "from": "X25n0480_001:0149c09_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為福德。",
        "from": "X25n0480_001:0149c09_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不見一法有分劑相。",
        "from": "X25n0480_001:0149c09_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名如虗空。",
        "from": "X25n0480_001:0149c10_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不計一法異於般若。",
        "from": "X25n0480_001:0149c10_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名如教住。",
        "from": "X25n0480_001:0149c10_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次。",
        "from": "X25n0480_001:0149c10_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現前一念。",
        "from": "X25n0480_001:0149c11_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體即法界。",
        "from": "X25n0480_001:0149c11_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名非身相。",
        "from": "X25n0480_001:0149c11_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本原真如。",
        "from": "X25n0480_001:0149c11_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為如來。",
        "from": "X25n0480_001:0149c11_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由觀心故。",
        "from": "X25n0480_001:0149c12_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不毀三業。",
        "from": "X25n0480_001:0149c12_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為持戒。",
        "from": "X25n0480_001:0149c12_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "質直柔和。",
        "from": "X25n0480_001:0149c12_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為修福。",
        "from": "X25n0480_001:0149c12_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一念覺故。",
        "from": "X25n0480_001:0149c13_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為一佛。",
        "from": "X25n0480_001:0149c13_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念念相續。",
        "from": "X25n0480_001:0149c13_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至無量念。",
        "from": "X25n0480_001:0149c13_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善覺悟故。",
        "from": "X25n0480_001:0149c13_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為無量佛所種諸善根。",
        "from": "X25n0480_001:0149c14_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本覺之性。",
        "from": "X25n0480_001:0149c14_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印持攝受諸修德故。",
        "from": "X25n0480_001:0149c14_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為如來悉知悉見無量福德。",
        "from": "X25n0480_001:0149c15_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了心無相。",
        "from": "X25n0480_001:0149c15_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名無四相。",
        "from": "X25n0480_001:0149c15_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行無相善。",
        "from": "X25n0480_001:0149c16_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名無法相。",
        "from": "X25n0480_001:0149c16_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離一切惡。",
        "from": "X25n0480_001:0149c16_54",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為無非法相。",
        "from": "X25n0480_001:0149c16_55",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諦喻彼岸。",
        "from": "X25n0480_001:0149c17_56",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀智喻筏。",
        "from": "X25n0480_001:0149c17_57",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安住實諦。",
        "from": "X25n0480_001:0149c17_58",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀解亦絕。",
        "from": "X25n0480_001:0149c17_59",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為法尚應捨。",
        "from": "X25n0480_001:0149c17_60",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次。",
        "from": "X25n0480_001:0149c18_61",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現前一念。",
        "from": "X25n0480_001:0149c18_62",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即如來故。",
        "from": "X25n0480_001:0149c18_63",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦即菩提。",
        "from": "X25n0480_001:0149c18_64",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應如來得於如來。",
        "from": "X25n0480_001:0149c18_65",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應菩提得於菩提。",
        "from": "X25n0480_001:0149c19_66",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以能得所得。",
        "from": "X25n0480_001:0149c19_67",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能說所說。",
        "from": "X25n0480_001:0149c19_68",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆是即心自性。",
        "from": "X25n0480_001:0149c20_69",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無能所故。",
        "from": "X25n0480_001:0149c20_70",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此心之性。",
        "from": "X25n0480_001:0149c20_71",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即無為法。",
        "from": "X25n0480_001:0149c20_72",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切聖賢。",
        "from": "X25n0480_001:0149c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不離心性有修證故。",
        "from": "X25n0480_001:0149c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現前心性。",
        "from": "X25n0480_001:0149c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即名為經。",
        "from": "X25n0480_001:0149c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是法是常。",
        "from": "X25n0480_001:0149c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無變壞故。",
        "from": "X25n0480_001:0149c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸佛及菩提法。",
        "from": "X25n0480_001:0149c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無不依於心性而施設故。",
        "from": "X25n0480_001:0149c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若達心法。",
        "from": "X25n0480_001:0149c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即知佛法。",
        "from": "X25n0480_001:0149c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦知一切眾生之法。",
        "from": "X25n0480_001:0149c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三無差別。",
        "from": "X25n0480_001:0149c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非別更有諸佛法故。",
        "from": "X25n0480_001:0149c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次。",
        "from": "X25n0480_001:0149c24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現前一念。",
        "from": "X25n0480_001:0149c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是法界。",
        "from": "X25n0480_001:0150a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無內外故。",
        "from": "X25n0480_001:0150a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故無出入。",
        "from": "X25n0480_001:0150a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦無往來。",
        "from": "X25n0480_001:0150a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無道無果。",
        "from": "X25n0480_001:0150a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無欲無離。",
        "from": "X25n0480_001:0150a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無喧無寂。",
        "from": "X25n0480_001:0150a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為四果阿蘭那行。",
        "from": "X25n0480_001:0150a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次。",
        "from": "X25n0480_001:0150a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現前一念。",
        "from": "X25n0480_001:0150a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光光相續。",
        "from": "X25n0480_001:0150a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為然燈。",
        "from": "X25n0480_001:0150a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬法所依。",
        "from": "X25n0480_001:0150a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為佛土。",
        "from": "X25n0480_001:0150a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徧滿自在。",
        "from": "X25n0480_001:0150a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為大身。",
        "from": "X25n0480_001:0150a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福田所宗。",
        "from": "X25n0480_001:0150a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為塔廟。",
        "from": "X25n0480_001:0150a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一體三義。",
        "from": "X25n0480_001:0150a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名金剛般若。",
        "from": "X25n0480_001:0150a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "究竟秘藏。",
        "from": "X25n0480_001:0150a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名波羅蜜。",
        "from": "X25n0480_001:0150a05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如理觀心。",
        "from": "X25n0480_001:0150a05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名奉。",
        "from": "X25n0480_001:0150a05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀成不失。",
        "from": "X25n0480_001:0150a05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名持。",
        "from": "X25n0480_001:0150a06_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自此以後。",
        "from": "X25n0480_001:0150a06_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖有種種名句文身。",
        "from": "X25n0480_001:0150a06_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以義攝取。",
        "from": "X25n0480_001:0150a06_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦復不離現前一念。",
        "from": "X25n0480_001:0150a06_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可以意知。",
        "from": "X25n0480_001:0150a07_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不復委釋。",
        "from": "X25n0480_001:0150a07_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如是微妙金剛義　　不離現前一念心　觀心具足般若經　　亦復具足餘妙典　十二分教二種藏　　乃至量等大千經　於一念中悉剖出　　一切諸念亦復然　如於念念剖大經　　一切根塵亦如是　乃至無有少許塵　　而不具足法界藏　如是正慧應了知　　一切惟心心一切　若不觀心類說藥　　真藥現前亦不知　是故我今稽首禮　　性具三德無遺者　一一啟請勸觀心　　速登般若金剛岸金剛般若波羅蜜經觀心釋(終)",
        "from": "X25n0480_001:0150a08_0",
        "context_with_next_sentence": false
    }
]