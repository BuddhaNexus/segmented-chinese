[
    {
        "source_sentence": "三萬佛同根本神秘之印並法龍",
        "from": "T85n2906_001:1449b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種上尊王佛法",
        "from": "T85n2906_001:1449b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光紫色而□□□□□□□□□□□□□□□□□□□□年化為流星。",
        "from": "T85n2906_001:1449b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流星千年□□□□□□□□□□□□□□□□□為浮水。",
        "from": "T85n2906_001:1449b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金入水不耗。",
        "from": "T85n2906_001:1449c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "反益□□□□□□□□□□□□□□□太清此飛仙之法。",
        "from": "T85n2906_001:1449c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿□□□□□□□□□□□□□□□□□□□酒二升與茯苓合□□□□□□□□□□□□□□□□□□絕穀仙方　胡麻之法□□□□□□□□□□□□□□□□□如一名三光之貴榮。",
        "from": "T85n2906_001:1449c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名□□□□□□□□□□□□□□□昌。",
        "from": "T85n2906_001:1449c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名含映。",
        "from": "T85n2906_001:1449c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名青襄。",
        "from": "T85n2906_001:1449c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是其葉食□□□□□□□□□□□□好成熟者搗持土穢。",
        "from": "T85n2906_001:1449c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意多少湯□□□□□□□□□□□□乾後蒸之。",
        "from": "T85n2906_001:1449c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使微氣出極溜通止。",
        "from": "T85n2906_001:1449c12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更□□□□□□□□□□□□曝每至蒸時。",
        "from": "T85n2906_001:1449c12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要須快日天陰。",
        "from": "T85n2906_001:1449c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得□□□□□□□□□□□□復溫而復蒸。",
        "from": "T85n2906_001:1449c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "都[暻京+恭]訖而後搗之。",
        "from": "T85n2906_001:1449c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和以白蜜。",
        "from": "T85n2906_001:1449c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□□□□□□□至服時。",
        "from": "T85n2906_001:1449c15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一服一枚。",
        "from": "T85n2906_001:1449c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以日三時服之。",
        "from": "T85n2906_001:1449c16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可長生不死□年□□□□□十一月上亥日。",
        "from": "T85n2906_001:1449c17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "收槐子熟好者。",
        "from": "T85n2906_001:1449c18_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□著黃□□□□□□□□□服之。",
        "from": "T85n2906_001:1449c18_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百日上見天文。",
        "from": "T85n2906_001:1449c19_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二百□□□□初服一枚。",
        "from": "T85n2906_001:1449c19_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□□□□□□又十月上建收槐子三升□□□□枚一年夜□□□□□□□□二年萬病皆除。",
        "from": "T85n2906_001:1449c20_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "服之三□□□□聰□□□□□□□□□□取章陸根三十斤去惡□□□□□□□□□□□□□□□□□□陰乾六十日。",
        "from": "T85n2906_001:1449c22_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "末篩方寸七□□□□□□□□□□□□□□□取寶立得不難日行千□□□□□□□□□□□□□□□金不傳　去三尸方　恒以□□□□□□□□□□□□□□黃。",
        "from": "T85n2906_001:1449c25_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雌黃。",
        "from": "T85n2906_001:1449c29_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "等分末之。",
        "from": "T85n2906_001:1449c29_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以綿裹□□□□□□□□□□□□□□□死出去壽五百年活　又□□□□□□□□□□□□□□□以酒和。",
        "from": "T85n2906_001:1449c29_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取八月破除日。",
        "from": "T85n2906_001:1450a03_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和藥□□□□□□□□□□□□□□□百病愈。",
        "from": "T85n2906_001:1450a03_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "服三節耳目聰明□□□□□□□□□□□□□□□同光。",
        "from": "T85n2906_001:1450a04_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上尸百日中尸。",
        "from": "T85n2906_001:1450a06_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六十日下□□□□□□□□□□□□□□□鷄子。",
        "from": "T85n2906_001:1450a06_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上尸。",
        "from": "T85n2906_001:1450a07_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黑。",
        "from": "T85n2906_001:1450a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中尸青。",
        "from": "T85n2906_001:1450a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下尸□□□□□□□□□□□□□□伺人罪過。",
        "from": "T85n2906_001:1450a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上奏天翁。",
        "from": "T85n2906_001:1450a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間□□□□□□□□□□□□□□□洇其藥力令人百病不愈□□□□□□□□□□□□□□□五色若不去三尸但□□□□□□□□□□□□□□□仙人與日月同光莫視。",
        "from": "T85n2906_001:1450a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡下□□□□□□□□□□□□□□□又方。",
        "from": "T85n2906_001:1450a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七月十七日。",
        "from": "T85n2906_001:1450a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "去手爪足爪吞。",
        "from": "T85n2906_001:1450a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "腹中去三尸□□□□□□□　又方。",
        "from": "T85n2906_001:1450a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取蕪荑五升。",
        "from": "T85n2906_001:1450a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乾柴四兩。",
        "from": "T85n2906_001:1450a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苟杞根四□□□□□□□華水服日一食。",
        "from": "T85n2906_001:1450a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五日三尸。",
        "from": "T85n2906_001:1450a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虫於下部中出□□□□□□□□防護之。",
        "from": "T85n2906_001:1450a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三日後自好不須防。",
        "from": "T85n2906_001:1450a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀世音菩薩□□□□□□□□傍蟹八枚。",
        "from": "T85n2906_001:1450a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上好酒半升。",
        "from": "T85n2906_001:1450a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□□□末和□□□□□□肉身日行萬里。",
        "from": "T85n2906_001:1450a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奔馬趁不及□□□□□□□□□□□□□□從經劫數已來不曾見之。",
        "from": "T85n2906_001:1450a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念□□□□□□□□□□□□□□□□說令人狐疑顛倒　採黃精□□□□□□□□□□□□□□□□□洗却惡皮毛細切一石水煮之至□□□□□□□□□□□□□□黃末和作餅日服如鷄子□□□□□□□□□□□□□□□不老。",
        "from": "T85n2906_001:1450a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聰明萬倍。",
        "from": "T85n2906_001:1450a29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與天地相□□□□□□□□□□□□□□□搗熟以水一石煮。",
        "from": "T85n2906_001:1450a29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復內麻□□□□□□□□□□□□□□□可丸如鷄子大。",
        "from": "T85n2906_001:1450b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日服三丸。",
        "from": "T85n2906_001:1450b03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十日已後□□□□□□□□□□□□□服之百日。",
        "from": "T85n2906_001:1450b03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬病皆除。",
        "from": "T85n2906_001:1450b04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三二間即昇天矣□□□□□□□□□□□洗生搗以生大豆黃末一升和合作餅如常。",
        "from": "T85n2906_001:1450b05_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以爐□□□□□□□服一餅。",
        "from": "T85n2906_001:1450b07_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後日三餅漸自不飢。",
        "from": "T85n2906_001:1450b07_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至十日□□□□□□□□□□□□□睡眠自然不須食。",
        "from": "T85n2906_001:1450b08_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一年已後昇□□□□□□□□□□□□□黃精　二名重樓　三名仙人。",
        "from": "T85n2906_001:1450b09_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘糧□□□□□□□□□□□□□天老日天地所生草有食之令□□□□□□□□□□□□□名曰黃精餌而食之。",
        "from": "T85n2906_001:1450b11_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以度世□□□□□□□□□□□□□之入口即死人若信呴吻傷□□□□□□□□□□□□□或乎其草精者葉似竹□□□□□□□□□□□□□也　夫採以二月八月三月□□□□□□□□□□□□□禁忌食梅。",
        "from": "T85n2906_001:1450b14_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "絕穀不飢□□□□□□□□□□□□□大豆熬之。",
        "from": "T85n2906_001:1450b19_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以上六味各五合搗篩為末。",
        "from": "T85n2906_001:1450b20_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白蜜一斤□□□□□可丸如李子大。",
        "from": "T85n2906_001:1450b21_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頓吞服之。",
        "from": "T85n2906_001:1450b22_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令人終不飢渴　□□□皆須天氣晴明。",
        "from": "T85n2906_001:1450b22_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先當清心齋戒。",
        "from": "T85n2906_001:1450b23_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香湯沐浴□□□□□可合和仙藥　又治三尸法　取狼牙根□乾□□□□□服之。",
        "from": "T85n2906_001:1450b23_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一方寸七。",
        "from": "T85n2906_001:1450b25_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日三明虫下。",
        "from": "T85n2906_001:1450b25_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得□□□□□□□□□□□□□麻子二升。",
        "from": "T85n2906_001:1450b26_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大豆一升(已上冬服)麻子□□□□□□□□□□□□麻子亦炒和搗作末。",
        "from": "T85n2906_001:1450b27_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每日一抄□□□□□□□□□□□□□□服子。",
        "from": "T85n2906_001:1450b28_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若經一年日行千里。",
        "from": "T85n2906_001:1450b29_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五芝□□鳥□□□□□□□茯苓半斤。",
        "from": "T85n2906_001:1450c01_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "椒四兩。",
        "from": "T85n2906_001:1450c02_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "錫半□□□蜜二合。",
        "from": "T85n2906_001:1450c02_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令□□□□□□□日三服食之。",
        "from": "T85n2906_001:1450c02_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三丸以日出時□□□及午時申時□□□年頭白更黑力徹虛空。",
        "from": "T85n2906_001:1450c03_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□行千里　王喬□□□五斤。",
        "from": "T85n2906_001:1450c05_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水三石。",
        "from": "T85n2906_001:1450c05_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於釜口稍稍添水煮之。",
        "from": "T85n2906_001:1450c06_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可至一升□品□□□之。",
        "from": "T85n2906_001:1450c06_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又內白蜜一升重湯中更煎。",
        "from": "T85n2906_001:1450c07_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五升□□□□□□□□□□□騰躍自在。",
        "from": "T85n2906_001:1450c07_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三萬佛同根本神祕之印并法。",
        "from": "T85n2906_001:1450c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□我聞一時佛在他化自在天。",
        "from": "T85n2906_001:1450c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與諸大菩薩百□□□□□□□□□□超世間。",
        "from": "T85n2906_001:1450c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其名曰總持天菩薩。",
        "from": "T85n2906_001:1450c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空菩薩。",
        "from": "T85n2906_001:1450c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "充明□□□菩薩。",
        "from": "T85n2906_001:1450c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量□□□□□幢菩薩。",
        "from": "T85n2906_001:1450c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量身菩薩。",
        "from": "T85n2906_001:1450c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛□□□□□菩薩。",
        "from": "T85n2906_001:1450c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀世音菩薩。",
        "from": "T85n2906_001:1450c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大□□□□鳴菩薩。",
        "from": "T85n2906_001:1450c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天親菩薩。",
        "from": "T85n2906_001:1450c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天津菩薩□□□□□□□□□□菩薩。",
        "from": "T85n2906_001:1450c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "海眼菩薩。",
        "from": "T85n2906_001:1450c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□文殊師利菩薩等。",
        "from": "T85n2906_001:1450c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有□□□□□□□□□大菩薩摩訶薩。",
        "from": "T85n2906_001:1450c17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□有無量聲聞緣覺□□□□□□□□□□□□□信男□信女等。",
        "from": "T85n2906_001:1450c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繞佛百匝。",
        "from": "T85n2906_001:1450c19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "默然□□。",
        "from": "T85n2906_001:1450c19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時□□□□□□莊嚴世□龍種上尊王佛。",
        "from": "T85n2906_001:1450c20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將三十二萬億呪□□□□□□□□提桓□□等類并諸眷屬。",
        "from": "T85n2906_001:1450c21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後圍繞。",
        "from": "T85n2906_001:1450c22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□□□□□□諸□散花。",
        "from": "T85n2906_001:1450c22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香風吹林。",
        "from": "T85n2906_001:1450c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處處普遍。",
        "from": "T85n2906_001:1450c23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光明照曜。",
        "from": "T85n2906_001:1450c24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□地六種震動。",
        "from": "T85n2906_001:1450c24_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "巡諸國土來至自天上。",
        "from": "T85n2906_001:1450c24_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其諸二十八天及諸日月星辰。",
        "from": "T85n2906_001:1450c25_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並隱沒不現。",
        "from": "T85n2906_001:1450c25_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及此自在天上變為金色菩薩所威光並悉不□。",
        "from": "T85n2906_001:1450c26_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時彼龍種上尊王佛。",
        "from": "T85n2906_001:1450c27_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到如來所。",
        "from": "T85n2906_001:1450c27_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量問訊。",
        "from": "T85n2906_001:1450c28_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起□□□□面。",
        "from": "T85n2906_001:1450c28_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時□佛白言。",
        "from": "T85n2906_001:1450c28_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "T85n2906_001:1450c29_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我無量劫來修集方□□□□□□□此欲崇大法。",
        "from": "T85n2906_001:1450c29_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願佛慈悲□□□□□□□□□□□□聲□言。",
        "from": "T85n2906_001:1451a01_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉。",
        "from": "T85n2906_001:1451a02_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝能發心欲□□□□今速說。",
        "from": "T85n2906_001:1451a02_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我及十方諸大如來助汝宣化。",
        "from": "T85n2906_001:1451a03_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時上王佛復白□□□有神印。",
        "from": "T85n2906_001:1451a04_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前佛後佛三萬餘人。",
        "from": "T85n2906_001:1451a04_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆因此印得無上道究竟涅槃。",
        "from": "T85n2906_001:1451a05_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "T85n2906_001:1451a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若未得□□□阿羅漢辟支佛等。",
        "from": "T85n2906_001:1451a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并下類凡夫初發心者。",
        "from": "T85n2906_001:1451a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若求諸大方術□□自在先當取盧耶娑木方一寸八分。",
        "from": "T85n2906_001:1451a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此云赤柳木。",
        "from": "T85n2906_001:1451a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是剋印持帶□□□□千靈並顯萬術自通　龍種上尊王佛法第一　先立壇。",
        "from": "T85n2906_001:1451a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三日一上廁一浴。",
        "from": "T85n2906_001:1451a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後剋印。",
        "from": "T85n2906_001:1451a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "含香水剋印。",
        "from": "T85n2906_001:1451a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T85n2906_001:1451a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘耶　提摩辟駄兜莎訶　念龍種上尊王□一切遍發願速□┌─────┐│○　求　○││求　□　求││○　求　○│└─────┘(世尊若有諸善男子善女人。",
        "from": "T85n2906_001:1451a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲求無量百千萬□□術者。",
        "from": "T85n2906_001:1451a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即尅此印□□卑作囊盛之。",
        "from": "T85n2906_001:1451a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於膊上安之。",
        "from": "T85n2906_001:1451a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無得污淨。",
        "from": "T85n2906_001:1451a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨所住□□□坐臥百無妨礙。",
        "from": "T85n2906_001:1451a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常念我名　南無龍種□□□王佛。",
        "from": "T85n2906_001:1451a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每須念之　龍種上尊王佛尅印法第一　世尊當尅印之際□□□□默然除語尅之□□□□□發願。",
        "from": "T85n2906_001:1451a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後即受印。",
        "from": "T85n2906_001:1451a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當受印之。",
        "from": "T85n2906_001:1451a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日我及四眾□諸呪□□□□□□□□□□□□於黃昏受之)。",
        "from": "T85n2906_001:1451a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍種上尊王佛帶印試效境相第一　世尊。",
        "from": "T85n2906_001:1451a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲帶之。",
        "from": "T85n2906_001:1451a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□略云。",
        "from": "T85n2906_001:1451a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其印多聖顯少效作是言已即臥。",
        "from": "T85n2906_001:1451a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有上類利根凡夫。",
        "from": "T85n2906_001:1451a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於白頭邊即有三丸黑摩尼心寶藥。",
        "from": "T85n2906_001:1451a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有下類凡夫。",
        "from": "T85n2906_001:1451a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於中夜時有鐘鳴。",
        "from": "T85n2906_001:1451a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若□□□凡夫。",
        "from": "T85n2906_001:1451a29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即有異香來前。",
        "from": "T85n2906_001:1451a29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有利根即有此事。",
        "from": "T85n2906_001:1451a30_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若中根三日及有。",
        "from": "T85n2906_001:1451a30_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若得藥時勿令外人知覺。",
        "from": "T85n2906_001:1451a30_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便一氣吞之。",
        "from": "T85n2906_001:1451a31_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人即與陀羅尼菩薩同位也。",
        "from": "T85n2906_001:1451a31_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若得是相已後。",
        "from": "T85n2906_001:1451a32_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至日滿乃可即試印術方爾。",
        "from": "T85n2906_001:1451a32_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "試印效法第一　世尊。",
        "from": "T85n2906_001:1451b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若當帶□□□□□□□□□效力也。",
        "from": "T85n2906_001:1451b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲試之時丁香七枚。",
        "from": "T85n2906_001:1451b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印印之。",
        "from": "T85n2906_001:1451b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一下各[打丁+鳥]碎。",
        "from": "T85n2906_001:1451b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和水吞之。",
        "from": "T85n2906_001:1451b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須臾即得□□智　若欲求他心智者。",
        "from": "T85n2906_001:1451b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦取丁香二□□□□乾吞之。",
        "from": "T85n2906_001:1451b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以印印口三下。",
        "from": "T85n2906_001:1451b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立得他心智。",
        "from": "T85n2906_001:1451b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "預知三生之事　又法。",
        "from": "T85n2906_001:1451b05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取死人髑髏一枚。",
        "from": "T85n2906_001:1451b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無問以印□之七下。",
        "from": "T85n2906_001:1451b06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間一切惡鬼並悉來降伏　又法。",
        "from": "T85n2906_001:1451b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲求役使百千萬種神金剛等類。",
        "from": "T85n2906_001:1451b08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取桑根皮一斤煮令爛去惡皮。",
        "from": "T85n2906_001:1451b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印印之。",
        "from": "T85n2906_001:1451b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食次復之。",
        "from": "T85n2906_001:1451b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬鬼並來降伏□□信息並亦預　又法。",
        "from": "T85n2906_001:1451b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取清淨沙一升。",
        "from": "T85n2906_001:1451b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印印之。",
        "from": "T85n2906_001:1451b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於四方□之□□□□並悉得脫　又法。",
        "from": "T85n2906_001:1451b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取井華水三升。",
        "from": "T85n2906_001:1451b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印印之。",
        "from": "T85n2906_001:1451b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四方散一切枯□□□□□潤益　又法。",
        "from": "T85n2906_001:1451b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取黃沙三升。",
        "from": "T85n2906_001:1451b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印印之。",
        "from": "T85n2906_001:1451b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "埋於土中。",
        "from": "T85n2906_001:1451b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日變成黃金　又法。",
        "from": "T85n2906_001:1451b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取帶印人髮七枚。",
        "from": "T85n2906_001:1451b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印印之。",
        "from": "T85n2906_001:1451b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擲在地中。",
        "from": "T85n2906_001:1451b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變成黑地　又法。",
        "from": "T85n2906_001:1451b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶印人取蛇。",
        "from": "T85n2906_001:1451b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印印之。",
        "from": "T85n2906_001:1451b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各印一下。",
        "from": "T85n2906_001:1451b17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其蛇印死取之□□第一癩及瘻□無申等患。",
        "from": "T85n2906_001:1451b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以蛇少許與服□□□□差　又法。",
        "from": "T85n2906_001:1451b18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若印人求出世間者。",
        "from": "T85n2906_001:1451b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取水三□煎。",
        "from": "T85n2906_001:1451b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取□□以□□□□同洗面及脚子時之間。",
        "from": "T85n2906_001:1451b20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從地踊出。",
        "from": "T85n2906_001:1451b21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百寶蓮花承印□□□□□□人求騰空者。",
        "from": "T85n2906_001:1451b21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取水三升印之百遍。",
        "from": "T85n2906_001:1451b22_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洗自立即便騰身而去　又發廣大心求萬法術者。",
        "from": "T85n2906_001:1451b22_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取椒一兩取水三升煎之。",
        "from": "T85n2906_001:1451b24_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□□以印印之三遍。",
        "from": "T85n2906_001:1451b24_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分作三分。",
        "from": "T85n2906_001:1451b25_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平旦時服一分。",
        "from": "T85n2906_001:1451b25_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "午時服一分。",
        "from": "T85n2906_001:1451b25_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至夜間服一分。",
        "from": "T85n2906_001:1451b26_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□□□□悟相似。",
        "from": "T85n2906_001:1451b26_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後即得通達萬法。",
        "from": "T85n2906_001:1451b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上知三十三天。",
        "from": "T85n2906_001:1451b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下知九十二億世界之□□□□□苦眾生並悉能救之　又法。",
        "from": "T85n2906_001:1451b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取乳頭香三兩。",
        "from": "T85n2906_001:1451b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印印之七遍。",
        "from": "T85n2906_001:1451b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□□□□□夜叉羅剎及諸惡鬼。",
        "from": "T85n2906_001:1451b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆發善心奉事和上亦□□□□□□□□□□並□□歸依　又法。",
        "from": "T85n2906_001:1451c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取牛蘇一斤。",
        "from": "T85n2906_001:1451c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "練持白瓷椀盛。",
        "from": "T85n2906_001:1451c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無□多少。",
        "from": "T85n2906_001:1451c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印印之。",
        "from": "T85n2906_001:1451c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日服三兩。",
        "from": "T85n2906_001:1451c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "服□自厭此身。",
        "from": "T85n2906_001:1451c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為我被惡賊。",
        "from": "T85n2906_001:1451c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "侵陵捨身。",
        "from": "T85n2906_001:1451c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他世得生佛前　又法。",
        "from": "T85n2906_001:1451c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若取白石七枚。",
        "from": "T85n2906_001:1451c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印印之。",
        "from": "T85n2906_001:1451c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擲在水中。",
        "from": "T85n2906_001:1451c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "江神□□並來踊出。",
        "from": "T85n2906_001:1451c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸毒龍並皆自歸依　又法。",
        "from": "T85n2906_001:1451c07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取印□□□□□□成寶樓。",
        "from": "T85n2906_001:1451c08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "T85n2906_001:1451c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其印前後有二萬億佛。",
        "from": "T85n2906_001:1451c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆從是度。",
        "from": "T85n2906_001:1451c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若□□□□男子善女人。",
        "from": "T85n2906_001:1451c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但求得此印帶行。",
        "from": "T85n2906_001:1451c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日滿亦與佛□□□□□□□□□□□　又法。",
        "from": "T85n2906_001:1451c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但取印印隨意所須之處。",
        "from": "T85n2906_001:1451c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立即□□□□□□□□□□□為灰。",
        "from": "T85n2906_001:1451c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印與餘死人身全者。",
        "from": "T85n2906_001:1451c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口中□□□□□□□□□□□。",
        "from": "T85n2906_001:1451c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印印之。",
        "from": "T85n2906_001:1451c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變成灰土　又法。",
        "from": "T85n2906_001:1451c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取□少□□□□□□□□□□□　又法。",
        "from": "T85n2906_001:1451c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取松樹以印印之。",
        "from": "T85n2906_001:1451c16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立變成花樹　又法□□□□□□□取桃木六尺。",
        "from": "T85n2906_001:1451c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印印。",
        "from": "T85n2906_001:1451c17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將行意欲去處隨意□□□□□□□世尊。",
        "from": "T85n2906_001:1451c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其印我自化身付囑前人□□□□□□□□□又世尊。",
        "from": "T85n2906_001:1451c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此印若處世界恒被□□□□□□□□□□□□知唯有菩薩。",
        "from": "T85n2906_001:1451c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自在者即得之。",
        "from": "T85n2906_001:1451c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘者□□□見　世尊。",
        "from": "T85n2906_001:1451c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□若有持帶此印。",
        "from": "T85n2906_001:1451c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印人二十萬種福不可得□□而說之　第一從無量世以來。",
        "from": "T85n2906_001:1451c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輪迴六趣。",
        "from": "T85n2906_001:1451c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三業重障□□□□□□□□□□見此印便即除滅。",
        "from": "T85n2906_001:1451c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得清涼勝報　第二無量劫來□□□□孝。",
        "from": "T85n2906_001:1451c25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出佛身血。",
        "from": "T85n2906_001:1451c26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殺阿羅僧。",
        "from": "T85n2906_001:1451c27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誹謗三明大出長□□□□□□此印便永除斷無量重障。",
        "from": "T85n2906_001:1451c27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得清淨□□□□□□□□□□□恒在地獄餓鬼畜生邊。",
        "from": "T85n2906_001:1451c28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□□□□□□□□□□□無量百福莊嚴之身　第四□□□□□□□□□□□頭數。",
        "from": "T85n2906_001:1452a01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若聞此印便得除斷無量□□□□　第五從無量年來。",
        "from": "T85n2906_001:1452a03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "學種種法及諸方術。",
        "from": "T85n2906_001:1452a04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一圓此印□□□□□□□□□報　第六從無量時來。",
        "from": "T85n2906_001:1452a04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求緣定□□□□□□□□□□□□□此印便得通達無邊智慧之門　□□□□□□□□□□□□便成無上道法　世尊。",
        "from": "T85n2906_001:1452a06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其印□□□□□□□□□□□□□□覺者我當亦不取正覺　□□□□□□□□□□□□□□自現不假遠覓　又法。",
        "from": "T85n2906_001:1452a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取灰一升□□□□□□□□□□□□□□此世界至於他方　又法。",
        "from": "T85n2906_001:1452a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取□□□印印之。",
        "from": "T85n2906_001:1452a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "服令□□□□□□□世尊。",
        "from": "T85n2906_001:1452a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸善男子但有緣者。",
        "from": "T85n2906_001:1452a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□一人三人百人力□□□□□□□持帶我領無量呪術。",
        "from": "T85n2906_001:1452a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大仙恒□□護　□□□□□□□哉善哉。",
        "from": "T85n2906_001:1452a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大士。",
        "from": "T85n2906_001:1452a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝能有如是力不可思議。",
        "from": "T85n2906_001:1452a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此□□□□□□□彼法及物十方諸大菩薩□亦令得聞□□□□□□□□□□帶此印。",
        "from": "T85n2906_001:1452a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於五濁惡世而得清□□□□□□□□□□□□□□□眾恒常術助呪乃作印帶之者□□□□□□過□□□□□□□佛為上首。",
        "from": "T85n2906_001:1452a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆從此印得度現在□。",
        "from": "T85n2906_001:1452a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦隨此印得大□□□□□□□□□□□□□□此印而得度得　世尊。",
        "from": "T85n2906_001:1452a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但有諸善士等。",
        "from": "T85n2906_001:1452a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修□此印□□□□□□□□□□□□□□□住坐臥悉但留心令立即成就。",
        "from": "T85n2906_001:1452a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此諸時□□□□□□□□□□□□□□□□□俱從座起而白佛言。",
        "from": "T85n2906_001:1452a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今者若有諸善□□□□□□□□□□□□□□□香一兩[打丁+鳥]作末。",
        "from": "T85n2906_001:1452a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以蜜和之燒。",
        "from": "T85n2906_001:1452b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印印。",
        "from": "T85n2906_001:1452b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香□□□□□□□□□□□□□□法令得與我等生於一處□□□□□□□□□□□□□□□□令得度之。",
        "from": "T85n2906_001:1452b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量大願悉□□□□□□□□□□□□□□□□仙而為上首。",
        "from": "T85n2906_001:1452b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及獨仙人所為菴□□□□□□□□□□□□□□□阿婆仙而為上首。",
        "from": "T85n2906_001:1452b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及三明□□通神□□□□□□□□□□□□□□□龍八部諸大鬼王及諸夜叉王獸王□□□□□□□□□□□□□□□王鐵山王大鐵山王目真陀山王□陀□□□□□□□□□□□□□□□六欲天王。",
        "from": "T85n2906_001:1452b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並作是言　世尊。",
        "from": "T85n2906_001:1452b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□□□□□□□□□□□□□□□□恒常術護悉不暫捨。",
        "from": "T85n2906_001:1452b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□□□□□□□□□□□□□□□□徒眾皆大歡喜。",
        "from": "T85n2906_001:1452b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心修持。",
        "from": "T85n2906_001:1452b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各奉□□□□□□□□□□□□□□□於佛四面高聲讚佛。",
        "from": "T85n2906_001:1452b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散花供養。",
        "from": "T85n2906_001:1452b17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□□□□□□□□□□□□□。",
        "from": "T85n2906_001:1452b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍種上尊王佛消息萬神勅□法　凡欲作此印者。",
        "from": "T85n2906_001:1452b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬鬼來集。",
        "from": "T85n2906_001:1452b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千神俱□□□此印□□□□□□□□□□坐臥無量者。",
        "from": "T85n2906_001:1452b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即當怜目取定。",
        "from": "T85n2906_001:1452b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若得□□□□□□□□□□□□凡欲所作法。",
        "from": "T85n2906_001:1452b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以大母指掐離傳目□□□□□□□□□□□□不得成　又有外作法中。",
        "from": "T85n2906_001:1452b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上搯□□□□□□□□□□□□□□□若作此法更不得成別□□□□□□□□□□□□□□□□喜並能作之。",
        "from": "T85n2906_001:1452b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若修印人□□□□□□□□□□□□□□□□意所坐處",
        "from": "T85n2906_001:1452b27_1",
