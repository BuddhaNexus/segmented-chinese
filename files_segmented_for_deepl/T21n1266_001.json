[
    {
        "source_sentence": "大聖天歡喜雙身毘那夜迦法",
        "from": "T21n1266_001:0296a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一卷",
        "from": "T21n1266_001:0296a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T21n1266_001:0296a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空諡大鑑",
        "from": "T21n1266_001:0296a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正號大廣智大興善寺三藏沙門",
        "from": "T21n1266_001:0296a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空奉　詔譯",
        "from": "T21n1266_001:0296a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(出陀羅尼集經第十一諸天下卷)",
        "from": "T21n1266_001:0296a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身呪",
        "from": "T21n1266_001:0296a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨尾那翼迦(二合)瀉賀悉底(二合)母佉瀉怛儞野(二合)佗唵娜(去聲下同)翼迦(二合)娜翼迦(二合)尾娜翼迦(二合)尾娜翼迦(二合)怛囉(二合)簸哩(二合)怛囉(二合)翼迦(二合)餉(引)佉賀悉底(二合)餉(引)佉迦只多扇(上)底迦囉(二合)娑嚩(二合引)賀(引)心呪唵儗(上)哩虐心中心呪唵虐虐吽娑嚩(二合引)賀若欲作此天法。",
        "from": "T21n1266_001:0296a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先須畫像。",
        "from": "T21n1266_001:0296a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或用白鑞銅木等及花木。",
        "from": "T21n1266_001:0296a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若鑄若刻並得。",
        "from": "T21n1266_001:0296a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其像形。",
        "from": "T21n1266_001:0296a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫婦二身令相抱立。",
        "from": "T21n1266_001:0296a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各長五寸。",
        "from": "T21n1266_001:0296a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或七寸皆得。",
        "from": "T21n1266_001:0296a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並作象頭人身。",
        "from": "T21n1266_001:0296a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造像不得違價。",
        "from": "T21n1266_001:0296a25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其像成已。",
        "from": "T21n1266_001:0296a25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當以白月一日。",
        "from": "T21n1266_001:0296a25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於淨室內。",
        "from": "T21n1266_001:0296a26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用淨牛糞摩作圓壇。",
        "from": "T21n1266_001:0296a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意大小。",
        "from": "T21n1266_001:0296a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當取一升清細麻油。",
        "from": "T21n1266_001:0296a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以淨銅器盛之。",
        "from": "T21n1266_001:0296a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用上呪文呪油一百八遍。",
        "from": "T21n1266_001:0296a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即暖其油。",
        "from": "T21n1266_001:0296a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後將像放著油中。",
        "from": "T21n1266_001:0296a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安置壇內。",
        "from": "T21n1266_001:0296a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用淨銅匙銅杓等率油。",
        "from": "T21n1266_001:0296a29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灌其二像頂身一百八遍。",
        "from": "T21n1266_001:0296b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日之中七遍灌之。",
        "from": "T21n1266_001:0296b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平曉四遍日午三遍共成七遍。",
        "from": "T21n1266_001:0296b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已後每日更呪舊油以灌其像。",
        "from": "T21n1266_001:0296b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是作法乃至七日。",
        "from": "T21n1266_001:0296b03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨心所願即得稱意。",
        "from": "T21n1266_001:0296b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正灌油之時數數發願。",
        "from": "T21n1266_001:0296b04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復用酥蜜和麨作團。",
        "from": "T21n1266_001:0296b05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及蘿蔔根并一盞酒。",
        "from": "T21n1266_001:0296b05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及歡喜團時新花菓等。",
        "from": "T21n1266_001:0296b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是日別取新者供養。",
        "from": "T21n1266_001:0296b06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切善事隨意成就。",
        "from": "T21n1266_001:0296b07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切災禍悉皆銷滅。",
        "from": "T21n1266_001:0296b07_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其所獻食必須自食始得氣力。",
        "from": "T21n1266_001:0296b08_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請召印。",
        "from": "T21n1266_001:0296b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二小指二無名指。",
        "from": "T21n1266_001:0296b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相鉤向內。",
        "from": "T21n1266_001:0296b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以二中指竪相叉。",
        "from": "T21n1266_001:0296b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以二頭指。",
        "from": "T21n1266_001:0296b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各竪附中指。",
        "from": "T21n1266_001:0296b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大指亦竪。",
        "from": "T21n1266_001:0296b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "附近頭指側。",
        "from": "T21n1266_001:0296b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指來去。",
        "from": "T21n1266_001:0296b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1266_001:0296b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵簸迦囉(二合)主拏禰(去)嚩哆野(二合)誦七遍。",
        "from": "T21n1266_001:0296b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讚歎呪",
        "from": "T21n1266_001:0296b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵誐娜簸(波上下同)底扇(上聲呼)底娑嚩(二合引)悉底摩賀誐娜簸底娑嚩(二合引)賀(引)送呪唵簸迦囉(二合)主拏禰(去)嚩哆野(二合)娑嚩(二合引)賀(引)誦七遍。",
        "from": "T21n1266_001:0296b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪水護身呪",
        "from": "T21n1266_001:0296b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵吉里吉里跋折囉(二合引)吽泮吒(半音)若欲作法時。",
        "from": "T21n1266_001:0296b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以上呪呪水七遍。",
        "from": "T21n1266_001:0296b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用洗口。",
        "from": "T21n1266_001:0296b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并更取水呪七遍。",
        "from": "T21n1266_001:0296b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用楊柳散灑身上即為護身。",
        "from": "T21n1266_001:0296b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後入持誦室中作法之所。",
        "from": "T21n1266_001:0296b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洗浴呪",
        "from": "T21n1266_001:0296b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿拏婆折唎(二合)婆跛隷拏訶",
        "from": "T21n1266_001:0296b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以上呪呪水七遍當用洗浴。",
        "from": "T21n1266_001:0296c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "調和毘那夜迦法",
        "from": "T21n1266_001:0296c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請一切天作帝殊羅施印。",
        "from": "T21n1266_001:0296c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二小指無名指。",
        "from": "T21n1266_001:0296c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相叉於掌內。",
        "from": "T21n1266_001:0296c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二中指指頭相捻。",
        "from": "T21n1266_001:0296c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二頭指各加中指背第一節下半分。",
        "from": "T21n1266_001:0296c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指。",
        "from": "T21n1266_001:0296c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來去呪曰。",
        "from": "T21n1266_001:0296c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)鑠都嚕(二)波羅摩馱儞曳(三)莎訶誦七遍。",
        "from": "T21n1266_001:0297a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但是誦呪人夢中驚怖。",
        "from": "T21n1266_001:0297a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見諸畜生惡境界等。",
        "from": "T21n1266_001:0297a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知是毘那夜迦王瞋。",
        "from": "T21n1266_001:0297a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夢中覺已即慚愧乞莫瞋。",
        "from": "T21n1266_001:0297a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明日自有飲食勞謝。",
        "from": "T21n1266_001:0297a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以水摩地作二肘水圓壇。",
        "from": "T21n1266_001:0297a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如槃大亦得。",
        "from": "T21n1266_001:0297a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即取[飢几+蒸]餅五顆蘿蔔根三顆火燒熟。",
        "from": "T21n1266_001:0297a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有花著花并燒白膠薰陸等香安於壇中。",
        "from": "T21n1266_001:0297a06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪人壇西面向東坐。",
        "from": "T21n1266_001:0297a07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦大自在天呪一百八遍已。",
        "from": "T21n1266_001:0297a08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口云慚愧好去。",
        "from": "T21n1266_001:0297a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是語已。",
        "from": "T21n1266_001:0297a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壇中雜物盤盛。",
        "from": "T21n1266_001:0297a09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出門向西棄却。",
        "from": "T21n1266_001:0297a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西北亦得。",
        "from": "T21n1266_001:0297a10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口云薩婆藥叉囉闍阿藹捨訶娑鉢闍伽車。",
        "from": "T21n1266_001:0297a10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是語已棄却即歸。",
        "from": "T21n1266_001:0297a11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大自在天呪曰。",
        "from": "T21n1266_001:0297a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)毘跢囉薩尼(二)波囉末唎達尼(三)瞋達尼瞋達尼(四)頻達尼頻達尼(五)莎訶誦呪一百八遍。",
        "from": "T21n1266_001:0297a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即心歡喜非但夢中。",
        "from": "T21n1266_001:0297a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但覺有魔事即作前法定好。",
        "from": "T21n1266_001:0297a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造像法",
        "from": "T21n1266_001:0297a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其像形端立。",
        "from": "T21n1266_001:0297a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "象頭人身。",
        "from": "T21n1266_001:0297a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左牙出右牙折。",
        "from": "T21n1266_001:0297a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面少向左。",
        "from": "T21n1266_001:0297a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其鼻向外瘻。",
        "from": "T21n1266_001:0297a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有六臂。",
        "from": "T21n1266_001:0297a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其左上手把刀。",
        "from": "T21n1266_001:0297a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次手把果盤。",
        "from": "T21n1266_001:0297a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下手把輪。",
        "from": "T21n1266_001:0297a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右上手把棒。",
        "from": "T21n1266_001:0297a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次手把索。",
        "from": "T21n1266_001:0297a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下手把牙。",
        "from": "T21n1266_001:0297a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造此像不得違價。",
        "from": "T21n1266_001:0297a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "足數付之。",
        "from": "T21n1266_001:0297a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若鑄若刻若畫並得。",
        "from": "T21n1266_001:0297a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右此大聖大祕要法。",
        "from": "T21n1266_001:0297a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人間希有。",
        "from": "T21n1266_001:0297a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實勿傳之宜誡慎也。",
        "from": "T21n1266_001:0297a23_2",
