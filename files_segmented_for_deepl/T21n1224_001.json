[
    {
        "source_sentence": "金剛童子持念經",
        "from": "T21n1224_001:0133b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨三業真言",
        "from": "T21n1224_001:0133b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵秫弟(引)耨戍(引)馱曩(引)野娑嚩(二合引)賀(引)二羽蓮花合掌。",
        "from": "T21n1224_001:0133b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦前真言七遍。",
        "from": "T21n1224_001:0133b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持身心三業。",
        "from": "T21n1224_001:0133b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妄念諸過悉皆清淨。",
        "from": "T21n1224_001:0133b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結普禮一切如來印。",
        "from": "T21n1224_001:0133b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右膝著地。",
        "from": "T21n1224_001:0133b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二羽左覆右。",
        "from": "T21n1224_001:0133b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仰背相著。",
        "from": "T21n1224_001:0133b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二信二惠反相叉安於頂上。",
        "from": "T21n1224_001:0133b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口誦彼真言。",
        "from": "T21n1224_001:0133b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身住此儀相。",
        "from": "T21n1224_001:0133b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心想自身滿於法界。",
        "from": "T21n1224_001:0133b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二佛菩薩前有己身。",
        "from": "T21n1224_001:0133b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至誠作禮真言。",
        "from": "T21n1224_001:0133b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉(二合)尾(微次反短聲)多(半音)如是以三業齊運。",
        "from": "T21n1224_001:0133b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮觀一切聖眾及曼茶羅本尊像已。",
        "from": "T21n1224_001:0133b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結加趺坐或半加坐。",
        "from": "T21n1224_001:0133b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "澄定身心。",
        "from": "T21n1224_001:0133b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結佛部三昧耶印。",
        "from": "T21n1224_001:0133b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二羽芙蓉合掌。",
        "from": "T21n1224_001:0133b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開手如掬物勢。",
        "from": "T21n1224_001:0133b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二定輔於二念上節文側。",
        "from": "T21n1224_001:0133b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二惠捻二定下第一文即成。",
        "from": "T21n1224_001:0133b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即念佛部三昧耶真言。",
        "from": "T21n1224_001:0133b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵怛他(去引)[薩/子]姤(引)[口*納]婆(二合)嚩(引)野娑嚩(二合)賀(引)以印安於頂上。",
        "from": "T21n1224_001:0133b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言三遍。",
        "from": "T21n1224_001:0133b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想印如佛形像。",
        "from": "T21n1224_001:0133b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結蓮花部三昧耶印。",
        "from": "T21n1224_001:0133b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二羽如前合掌微開。",
        "from": "T21n1224_001:0133b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中間二進二念二定六指微曲。",
        "from": "T21n1224_001:0133b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭相去各一寸許。",
        "from": "T21n1224_001:0133b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二信二惠各并竪。",
        "from": "T21n1224_001:0133b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如開敷蓮花形。",
        "from": "T21n1224_001:0133b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言三遍。",
        "from": "T21n1224_001:0133b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉印安於頂右右耳之上。",
        "from": "T21n1224_001:0133b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想如觀自在菩薩。",
        "from": "T21n1224_001:0133b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮花三昧耶真言曰。",
        "from": "T21n1224_001:0133b27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵鉢納謨(二合引)納婆(二合)嚩(引)野娑嚩(二合)賀(引)次結金剛部三昧耶印。",
        "from": "T21n1224_001:0133b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前遍禮印安於心上。",
        "from": "T21n1224_001:0133b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言三遍。",
        "from": "T21n1224_001:0133c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉印安於頂左左耳之上。",
        "from": "T21n1224_001:0133c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想如金剛藏菩薩。",
        "from": "T21n1224_001:0133c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛部三昧耶真言曰。",
        "from": "T21n1224_001:0133c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩曰嚧(二合引)納婆(二合)嚩(引)野娑嚩(二合引)賀(引)以結護身印。",
        "from": "T21n1224_001:0133c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二羽右押左。",
        "from": "T21n1224_001:0133c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內相叉直竪。",
        "from": "T21n1224_001:0133c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二念頭相柱。",
        "from": "T21n1224_001:0133c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二定微曲。",
        "from": "T21n1224_001:0133c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於二念背不令相著。",
        "from": "T21n1224_001:0133c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如三股杵形。",
        "from": "T21n1224_001:0133c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二惠并竪。",
        "from": "T21n1224_001:0133c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掌中押二進二信上即成。",
        "from": "T21n1224_001:0133c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本經中或用根本印亦得。",
        "from": "T21n1224_001:0133c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今取蘇悉地經中通法真言即不[阿可+文]。",
        "from": "T21n1224_001:0133c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護身真言。",
        "from": "T21n1224_001:0133c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩嚩訥瑟吒(二合)嚩餉羯囉迦抳矩嚕(二合引)馱囉乞灑(二合)囉乞灑(二合)[牟*含](引)娑嚩(二合引)賀(引)由結此印。",
        "from": "T21n1224_001:0133c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦前真言。",
        "from": "T21n1224_001:0133c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持自身五處。",
        "from": "T21n1224_001:0133c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先印額次右肩左肩心喉。",
        "from": "T21n1224_001:0133c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為五處。",
        "from": "T21n1224_001:0133c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以真言威力加持自身。",
        "from": "T21n1224_001:0133c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成被金剛甲冑。",
        "from": "T21n1224_001:0133c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結辟除諸障繫縛毘那夜迦印。",
        "from": "T21n1224_001:0133c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二羽十度內相叉以屈二惠入掌。",
        "from": "T21n1224_001:0133c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甲相背各捻二念指面。",
        "from": "T21n1224_001:0133c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "急握拳。",
        "from": "T21n1224_001:0133c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心想毘那夜迦。",
        "from": "T21n1224_001:0133c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作障難者皆被五繫縛之。",
        "from": "T21n1224_001:0133c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辟除真言曰。",
        "from": "T21n1224_001:0133c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵度曩尾度曩怛羅(二合)娑野戍(引)灑野薩嚩尾曩(引)野迦(引)南(引)吽(引)撥(普未反下同此)吒(半)娑嚩(二合引)賀(引)次結被金剛甲冑印。",
        "from": "T21n1224_001:0133c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前印以二定直竪頭相柱。",
        "from": "T21n1224_001:0133c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二惠各捻二定下第一文即成。",
        "from": "T21n1224_001:0133c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印如前加持自身五處。",
        "from": "T21n1224_001:0133c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍身自然被金剛鎧甲光明赫奕。",
        "from": "T21n1224_001:0133c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1224_001:0133c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵紇哩(二合)迦抳野麼娜賀吽(引)撥吒(半音)娑嚩(二合)賀次結金剛牆印。",
        "from": "T21n1224_001:0133c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前甲冑印。",
        "from": "T21n1224_001:0133c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開二定頭。",
        "from": "T21n1224_001:0133c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相去一寸許。",
        "from": "T21n1224_001:0133c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開掌如牆形即成。",
        "from": "T21n1224_001:0133c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1224_001:0133c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵紇哩(二合)迦抳諾賀諾賀鉢左鉢左吽撥吒(半音)娑嚩(二合引)賀以印頂上。",
        "from": "T21n1224_001:0133c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空中右旋三匝。",
        "from": "T21n1224_001:0134a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼誦真言三遍。",
        "from": "T21n1224_001:0134a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此加持威神力故。",
        "from": "T21n1224_001:0134a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周匝四方。",
        "from": "T21n1224_001:0134a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨心遠近結成金剛牆。",
        "from": "T21n1224_001:0134a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "界界光明照曜。",
        "from": "T21n1224_001:0134a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸魔不敢輔近。",
        "from": "T21n1224_001:0134a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結迎請聖尊印。",
        "from": "T21n1224_001:0134a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二羽內相叉直竪。",
        "from": "T21n1224_001:0134a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二定頭柱。",
        "from": "T21n1224_001:0134a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二惠向身招之。",
        "from": "T21n1224_001:0134a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請尊真言曰。",
        "from": "T21n1224_001:0134a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵度[寧*頁]婆(去)誐鑁(武點)迦[寧*頁]短嚕(二合引)馱翳醯(引)呬吽(引)撥吒(半音)不改此印。",
        "from": "T21n1224_001:0134a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便誦後真言。",
        "from": "T21n1224_001:0134a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迎請聖尊者眷屬真言。",
        "from": "T21n1224_001:0134a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵娑跛哩嚩(引)囉麼(鼻音反)嚩賀野婆(去)誐鑁(武敢反)迦[寧*頁]矩嚕(二合)馱三(去)麼耶麼努娑麼(二合)囉娑嚩(二合)賀(引)次結都明王忿怒軍吒利印。",
        "from": "T21n1224_001:0134a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辟除諸魔恐隨聖眾潛隱在其中。",
        "from": "T21n1224_001:0134a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以明王威神搜棟發遣。",
        "from": "T21n1224_001:0134a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作障難者唯留聖者及自眷屬。",
        "from": "T21n1224_001:0134a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其印以二信右押左內相交。",
        "from": "T21n1224_001:0134a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二進雙屈並押二信交上。",
        "from": "T21n1224_001:0134a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二念直竪頭柱。",
        "from": "T21n1224_001:0134a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二定各微曲。",
        "from": "T21n1224_001:0134a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於二念背後不相著。",
        "from": "T21n1224_001:0134a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如三股金剛杵形。",
        "from": "T21n1224_001:0134a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二惠並竪押二進上即成。",
        "from": "T21n1224_001:0134a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨誦真言。",
        "from": "T21n1224_001:0134a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印空中左旋三匝。",
        "from": "T21n1224_001:0134a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辟除姦魔棟擇發遣。",
        "from": "T21n1224_001:0134a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便即右旋三匝結成。",
        "from": "T21n1224_001:0134a22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛光明堅固大界。",
        "from": "T21n1224_001:0134a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切凡聖不合越此界故。",
        "from": "T21n1224_001:0134a23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "違越者化三昧耶決定不安。",
        "from": "T21n1224_001:0134a23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾聖陰罰違越。",
        "from": "T21n1224_001:0134a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾聖言誓故也。",
        "from": "T21n1224_001:0134a24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦軍吒利真言曰。",
        "from": "T21n1224_001:0134a25_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨(引)囉怛曩(二合)怛囉(二合)夜(引)野娜莫室戰(二合)嚩日囉(二合)播(引)緊(奴雅反)曳摩賀藥叉細(引)曩(引)鉢多(上)曳唵阿密[口*栗](二合)多軍[奴/糸](上)里却却佉(去引)呬佉(上同)呬(聲異反上同)底瑟咤(二合)底瑟姹(二合)滿馱滿馱賀曩賀曩[薩/子]惹[薩/子]惹尾颯普(二合引)吒野尾颯普(二合)吒野薩嚩尾近曩(二合)尾曩(引)野迦摩賀誐[奴/糸](鼻)鉢底爾(引)尾亶(多懶反)得迦囉(引)野吽撥吒(半音)便結上方金剛網印。",
        "from": "T21n1224_001:0134a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前牆印。",
        "from": "T21n1224_001:0134b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二惠開散直竪即成。",
        "from": "T21n1224_001:0134b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印頂上右旋三匝。",
        "from": "T21n1224_001:0134b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言三遍即成。",
        "from": "T21n1224_001:0134b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛網遍覆其上。",
        "from": "T21n1224_001:0134b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1224_001:0134b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉(二合)半惹囉吽(引)撥吒(半音)次獻閼伽香水浴聖尊印。",
        "from": "T21n1224_001:0134b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用前迎請印。",
        "from": "T21n1224_001:0134b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二惠各捻二定根下第一文。",
        "from": "T21n1224_001:0134b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印當心。",
        "from": "T21n1224_001:0134b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平舒如羸盃形。",
        "from": "T21n1224_001:0134b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉印當額奉獻。",
        "from": "T21n1224_001:0134b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有香水器。",
        "from": "T21n1224_001:0134b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以此印按之。",
        "from": "T21n1224_001:0134b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持三遍或一遍。",
        "from": "T21n1224_001:0134b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後捧而獻之。",
        "from": "T21n1224_001:0134b12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1224_001:0134b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩曰嚕(二合)娜迦吽(引)",
        "from": "T21n1224_001:0134b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結寶山印。",
        "from": "T21n1224_001:0134b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以獨股杵印。",
        "from": "T21n1224_001:0134b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[戚小+谷]上節令平如上頂形。",
        "from": "T21n1224_001:0134b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二惠並竪押掌中諸度上即成。",
        "from": "T21n1224_001:0134b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦寶山真言曰。",
        "from": "T21n1224_001:0134b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿左羅吽(引)",
        "from": "T21n1224_001:0134b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次獻蓮花座印。",
        "from": "T21n1224_001:0134b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以前閼伽印。",
        "from": "T21n1224_001:0134b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二惠頭向外開竪。",
        "from": "T21n1224_001:0134b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相去半寸許。",
        "from": "T21n1224_001:0134b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如蓮花形。",
        "from": "T21n1224_001:0134b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1224_001:0134b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日羅(二合)尾囉(引)野娑嚩(二合引)賀(引)次獻五供養先結塗香印。",
        "from": "T21n1224_001:0134b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前獨股杵印。",
        "from": "T21n1224_001:0134b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二惠微屈。",
        "from": "T21n1224_001:0134b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捻二定下第一文。",
        "from": "T21n1224_001:0134b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相去一寸許即成。",
        "from": "T21n1224_001:0134b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗香真言曰。",
        "from": "T21n1224_001:0134b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵迦抳矩嚕(二合引)馱(一)薩嚩薩怛嚩(二合)婆(去)孕迦囉(二合)囉乞灑(二合)囉乞灑(二合三)薩嚩彥[肆聿+犬](引)[寧*頁]娑嚩(二合引)賀(引)次結獻花印。",
        "from": "T21n1224_001:0134b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前獨股杵印。",
        "from": "T21n1224_001:0134b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出右惠即成。",
        "from": "T21n1224_001:0134b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦獻花真言曰。",
        "from": "T21n1224_001:0134b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵迦抳補澁波(二合)母納婆(二合)嚩吽(引)次結燒香印。",
        "from": "T21n1224_001:0134c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前獨股印。",
        "from": "T21n1224_001:0134c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出左惠即成。",
        "from": "T21n1224_001:0134c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦燒香真言曰。",
        "from": "T21n1224_001:0134c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵迦抳矩嚕(二合)馱塢[(薩文+(立一))/木]囉(二合)能(上聲)瑟吒嚕(二合)得羯(二合)吒佩(引)囉嚩仡哩(二合)恨拏(二合)仡哩(二合)恨拏(二合)度(引)奔娑嚩(二合引)賀次結獻食印。",
        "from": "T21n1224_001:0134c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "準前獨[月*古]杵印。",
        "from": "T21n1224_001:0134c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出二惠及二信頭相柱。",
        "from": "T21n1224_001:0134c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘四度內相叉作拳即成。",
        "from": "T21n1224_001:0134c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1224_001:0134c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵迦抳吽(引)吽(引)娑嚩(二合引)賀(引)次結燈明印。",
        "from": "T21n1224_001:0134c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前獨股杵印。",
        "from": "T21n1224_001:0134c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二惠向身並竪即成。",
        "from": "T21n1224_001:0134c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦燈明真言曰。",
        "from": "T21n1224_001:0134c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵迦抳儞波儞簸野吽(引)撥吒(半音)娑嚩(二合引)賀(引)次結普供養印。",
        "from": "T21n1224_001:0134c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手合掌。",
        "from": "T21n1224_001:0134c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右押左叉交上一節。",
        "from": "T21n1224_001:0134c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是想從此印流出。",
        "from": "T21n1224_001:0134c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種供養。",
        "from": "T21n1224_001:0134c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雲海化成七寶宮殿樓閣。",
        "from": "T21n1224_001:0134c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣服飲食上好天樂充滿法界。",
        "from": "T21n1224_001:0134c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養一切聖眾。",
        "from": "T21n1224_001:0134c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今略舉一二。",
        "from": "T21n1224_001:0134c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若廣說者。",
        "from": "T21n1224_001:0134c17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一如花嚴經中所說。",
        "from": "T21n1224_001:0134c17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養即誦是供真言三遍或七遍。",
        "from": "T21n1224_001:0134c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1224_001:0134c19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿(上)謨(引)伽麼抳鉢納麼(二合)入嚩(二合)羅鉢囉(二合)襪多(上)耶吽(引)暨如上諸供養已。",
        "from": "T21n1224_001:0134c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手輪擲金剛杵。",
        "from": "T21n1224_001:0134c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦本真言。",
        "from": "T21n1224_001:0134c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於眾聖前。",
        "from": "T21n1224_001:0134c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心想如對至尊。",
        "from": "T21n1224_001:0134c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作踏舞之勢。",
        "from": "T21n1224_001:0134c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倣像金剛薩埵。",
        "from": "T21n1224_001:0134c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於如來前。",
        "from": "T21n1224_001:0134c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擲弄其杵。",
        "from": "T21n1224_001:0134c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因便加持自身五處。",
        "from": "T21n1224_001:0134c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手執取金剛鈴。",
        "from": "T21n1224_001:0134c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於額前三度搖振。",
        "from": "T21n1224_001:0134c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "表以天樂音聲供養聖眾令其悅喜。",
        "from": "T21n1224_001:0134c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦誦本真言。",
        "from": "T21n1224_001:0134c26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心住薩埵妙三昧地(此上用鈴杵二真言及印。",
        "from": "T21n1224_001:0134c27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由來師受密傳不戴於文。",
        "from": "T21n1224_001:0134c27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡智有限。",
        "from": "T21n1224_001:0134c28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或傳之不盡。",
        "from": "T21n1224_001:0134c28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或領受學者不明置有多種。",
        "from": "T21n1224_001:0134c28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先後著錯不同。",
        "from": "T21n1224_001:0134c28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今故書之以草前蔽)修行者又慮恐承事供養之中。",
        "from": "T21n1224_001:0134c28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有忘誤疎遺犯三昧耶禁。",
        "from": "T21n1224_001:0134c29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有多生宿業罪障由未消除。",
        "from": "T21n1224_001:0135a01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦金剛部百字真言。",
        "from": "T21n1224_001:0135a02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以補前潛滅除諸過。",
        "from": "T21n1224_001:0135a02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得身心清淨。",
        "from": "T21n1224_001:0135a03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與法相應。",
        "from": "T21n1224_001:0135a03_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百字真言曰。",
        "from": "T21n1224_001:0135a03_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉(二合)薩多嚩(二合一)三(去)摩野麼(鼻努鼻)播(引)攞(勒可反)野(二)嚩日囉(二合)薩多嚩(二合三)怛[口*犮](二合引)努(鼻引)跛底瑟姹(二合四)揑哩(二合)濯(引)銘婆(去)嚩(五)素姤(引)數(沙句反引)銘婆(去)嚩(六)阿努(鼻)囉訖姤(二合引)銘娑嚩(七)素布(引)數(引准前)銘婆嚩(八)薩嚩悉地淫(二合)銘鉢囉(二合)曳(延結反)蹉(倉呵反九)薩嚩羯磨素左銘(十)郎多室哩(二合)藥矩嚕(十一)吽(十二引)賀賀賀賀斛(引十三)娑誐鑁(武敢反)薩嚩怛他[(薩文+(立一))/木]多(十四)嚩日囉(二合)麼(鼻音引)[方*耳](引)悶左(十五)嚩曰哩(二合引)婆嚩(十六)摩賀(引)三(去)麼耶薩多嚩(二合)惡(引十七呵各反)其印二手右押左。",
        "from": "T21n1224_001:0135a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外相叉作拳直竪。",
        "from": "T21n1224_001:0135a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指相合。",
        "from": "T21n1224_001:0135a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此名三昧耶印。",
        "from": "T21n1224_001:0135a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由結此印。",
        "from": "T21n1224_001:0135a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加身四處。",
        "from": "T21n1224_001:0135a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心額喉頂。",
        "from": "T21n1224_001:0135a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及誦百字真言。",
        "from": "T21n1224_001:0135a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除無始已來一切重罪。",
        "from": "T21n1224_001:0135a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破四重禁五無間業罪謗方等經。",
        "from": "T21n1224_001:0135a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等罪悉皆消滅。",
        "from": "T21n1224_001:0135a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況誤失小過而不清淨。",
        "from": "T21n1224_001:0135a18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結本尊印。",
        "from": "T21n1224_001:0135a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手合掌。",
        "from": "T21n1224_001:0135a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二無名左安前居下。",
        "from": "T21n1224_001:0135a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右安後居上。",
        "from": "T21n1224_001:0135a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "押左名指背交指。",
        "from": "T21n1224_001:0135a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於二中指背後。",
        "from": "T21n1224_001:0135a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二頭指各鉤取二名指頭。",
        "from": "T21n1224_001:0135a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指並竪合。",
        "from": "T21n1224_001:0135a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指亦並竪。",
        "from": "T21n1224_001:0135a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捻二中指中節。",
        "from": "T21n1224_001:0135a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二小指合竪即成。",
        "from": "T21n1224_001:0135a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦根本真言。",
        "from": "T21n1224_001:0135a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印加身四處。",
        "from": "T21n1224_001:0135a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1224_001:0135a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨(引)囉怛曩(二)怛(二合)野(引)野曩莫室戰(二合)拏嚩日囉(二合)播(引)拏(鼻)曳摩賀(引)藥叉(二合)細曩鉢多(上)曳怛儞也(二合)他(去引)唵加抳度[寧*頁]吽撥吒(半音)娑嚩(二合)賀又結第二根本印。",
        "from": "T21n1224_001:0135a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前最初護身印。",
        "from": "T21n1224_001:0135a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是亦名獨股杵印。",
        "from": "T21n1224_001:0135a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此與多羅菩薩印同。",
        "from": "T21n1224_001:0135b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二根本真言曰。",
        "from": "T21n1224_001:0135b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨囉怛曩(二合)夜(引)野娜莫室戰(二合)拏嚩日囉(二合)播(引)拏(鼻)曳摩賀(引)藥叉細(引)曩(引)鉢多(上)曳(引)怛他也(二合)他唵度曩尾度曩(引)迦抳矩嚕(二合引)馱薩嚩演怛囉(二合)抳吽(引)次結頭印。",
        "from": "T21n1224_001:0135b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前獨股杵印。",
        "from": "T21n1224_001:0135b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大指各鉤二無名指頭。",
        "from": "T21n1224_001:0135b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於掌中捻其上節內即成。",
        "from": "T21n1224_001:0135b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1224_001:0135b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵吽(引)吽(引)麼麼撥吒",
        "from": "T21n1224_001:0135b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結頂印。",
        "from": "T21n1224_001:0135b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前獨股杵印。",
        "from": "T21n1224_001:0135b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大指直竪相柱即成。",
        "from": "T21n1224_001:0135b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1224_001:0135b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵紇哩(二合一)戰拏迦抳(二)諾賀鉢左(三)吽(引)撥吒娑嚩(二合引)賀(引四)次結心印。",
        "from": "T21n1224_001:0135b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前根本印。",
        "from": "T21n1224_001:0135b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安於心上。",
        "from": "T21n1224_001:0135b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以心真言加持一遍。",
        "from": "T21n1224_001:0135b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1224_001:0135b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵迦[寧*頁]吽撥吒",
        "from": "T21n1224_001:0135b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次捧念珠當心。",
        "from": "T21n1224_001:0135b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以隨心真言加持七遍。",
        "from": "T21n1224_001:0135b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1224_001:0135b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵迦抳度麼(鼻)吽撥吒(半音)",
        "from": "T21n1224_001:0135b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手當心。",
        "from": "T21n1224_001:0135b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持珠念誦身端正坐儼然苦思。",
        "from": "T21n1224_001:0135b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心住本尊威德妙觀。",
        "from": "T21n1224_001:0135b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即與畫像如鏡照形。",
        "from": "T21n1224_001:0135b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不一不異。",
        "from": "T21n1224_001:0135b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨無瑕穢。",
        "from": "T21n1224_001:0135b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外想威光。",
        "from": "T21n1224_001:0135b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心住慈悲妙觀。",
        "from": "T21n1224_001:0135b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愍念六道諸類四生輪迴不息。",
        "from": "T21n1224_001:0135b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今發猛利勇信之心。",
        "from": "T21n1224_001:0135b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至誠誦念神妙章句。",
        "from": "T21n1224_001:0135b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心住本尊瑜伽觀智。",
        "from": "T21n1224_001:0135b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今我身心既得清淨。",
        "from": "T21n1224_001:0135b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迴此功德神力加被遍用法界。",
        "from": "T21n1224_001:0135b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普濟有情。",
        "from": "T21n1224_001:0135b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蒙此加持。",
        "from": "T21n1224_001:0135b27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆如我身外。",
        "from": "T21n1224_001:0135b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若依本經。",
        "from": "T21n1224_001:0135b28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更有十五餘道。",
        "from": "T21n1224_001:0135b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言及以印契總合誦習。",
        "from": "T21n1224_001:0135b28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令便精熟。",
        "from": "T21n1224_001:0135b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "待至事相應時棟取用之。",
        "from": "T21n1224_001:0135b29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以備臨事急難。",
        "from": "T21n1224_001:0135c01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "補其不足。",
        "from": "T21n1224_001:0135c01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦非常用具。",
        "from": "T21n1224_001:0135c01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在大經中說。",
        "from": "T21n1224_001:0135c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此不重敘。",
        "from": "T21n1224_001:0135c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨事若要。",
        "from": "T21n1224_001:0135c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自撿取用之。",
        "from": "T21n1224_001:0135c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大忿怒金剛童子念誦供養儀軌",
        "from": "T21n1224_001:0135c04_0",
        "context_with_next_sentence": false
    }
]