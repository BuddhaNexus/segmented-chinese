[
    {
        "source_sentence": "律雜抄",
        "from": "T85n2790_001:0653a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "律一卷雜抄　律一卷雜抄　律策一卷　問。",
        "from": "T85n2790_001:0653a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比丘是何義　答。",
        "from": "T85n2790_001:0653a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□□。",
        "from": "T85n2790_001:0653a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□□□有三種義□□□□怖魔。",
        "from": "T85n2790_001:0653a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三能破煩惱故名比丘。",
        "from": "T85n2790_001:0653a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□□□　答□□□比丘。",
        "from": "T85n2790_001:0653a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二相似比丘。",
        "from": "T85n2790_001:0653a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三自稱比丘。",
        "from": "T85n2790_001:0653a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四乞求比丘。",
        "from": "T85n2790_001:0653a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五割截衣比丘。",
        "from": "T85n2790_001:0653a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六破結使比丘。",
        "from": "T85n2790_001:0653a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七善來比丘。",
        "from": "T85n2790_001:0653a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八羯磨比丘。",
        "from": "T85n2790_001:0653a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□□□種　答。",
        "from": "T85n2790_001:0653a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依四分律有四種。",
        "from": "T85n2790_001:0653a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一善來。",
        "from": "T85n2790_001:0653a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二三歸。",
        "from": "T85n2790_001:0653a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三八敬。",
        "from": "T85n2790_001:0653a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四羯磨。",
        "from": "T85n2790_001:0653a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□刑有幾種　答依俱舍論有五種。",
        "from": "T85n2790_001:0653a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一命終捨戒。",
        "from": "T85n2790_001:0653a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二斷善根捨。",
        "from": "T85n2790_001:0653a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三二形捨捨。",
        "from": "T85n2790_001:0653a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五度捨戒。",
        "from": "T85n2790_001:0653a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四作法捨。",
        "from": "T85n2790_001:0653a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□。",
        "from": "T85n2790_001:0653a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羯磨有幾種。",
        "from": "T85n2790_001:0653a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復是何義　答。",
        "from": "T85n2790_001:0653a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羯磨有三種。",
        "from": "T85n2790_001:0653a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一單白羯磨。",
        "from": "T85n2790_001:0653a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二白二羯磨。",
        "from": "T85n2790_001:0653a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三白四羯磨。",
        "from": "T85n2790_001:0653a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所言義有是辨事義。",
        "from": "T85n2790_001:0653a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然辨前法事故名辨事義也。",
        "from": "T85n2790_001:0653a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "T85n2790_001:0653a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羯磨所為事有幾種　答。",
        "from": "T85n2790_001:0653a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總束為三種。",
        "from": "T85n2790_001:0653a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一情道事為受戒等。",
        "from": "T85n2790_001:0653a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二非常事為結界等。",
        "from": "T85n2790_001:0653a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三情非情事為乞杖離衣等。",
        "from": "T85n2790_001:0653a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "T85n2790_001:0653b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比丘所食藥總有幾種　答。",
        "from": "T85n2790_001:0653b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有其四種。",
        "from": "T85n2790_001:0653b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時藥。",
        "from": "T85n2790_001:0653b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二非時藥。",
        "from": "T85n2790_001:0653b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三七日藥。",
        "from": "T85n2790_001:0653b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四盡形藥。",
        "from": "T85n2790_001:0653b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "T85n2790_001:0653b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戒與律毘尼波羅提木叉有何差別　答。",
        "from": "T85n2790_001:0653b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戒者防非止惡。",
        "from": "T85n2790_001:0653b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不造惡故名為戒。",
        "from": "T85n2790_001:0653b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "律者法也。",
        "from": "T85n2790_001:0653b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非法無以齋戒儀伏。",
        "from": "T85n2790_001:0653b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是會意形聲釋也。",
        "from": "T85n2790_001:0653b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名為律。",
        "from": "T85n2790_001:0653b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘尼者名調伏義能調身三等業。",
        "from": "T85n2790_001:0653b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "木叉者此名解脫。",
        "from": "T85n2790_001:0653b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解脫一切諸縛名為解脫。",
        "from": "T85n2790_001:0653b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "T85n2790_001:0653b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何名五毘尼者　答。",
        "from": "T85n2790_001:0653b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一謂戒由序。",
        "from": "T85n2790_001:0653b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二制比丘戒。",
        "from": "T85n2790_001:0653b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三重制比丘尼戒本。",
        "from": "T85n2790_001:0653b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四修多羅。",
        "from": "T85n2790_001:0653b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂諸軌度。",
        "from": "T85n2790_001:0653b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五隨順修多羅增一比尼等。",
        "from": "T85n2790_001:0653b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "T85n2790_001:0653b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "律儀有幾種　答。",
        "from": "T85n2790_001:0653b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有四種。",
        "from": "T85n2790_001:0653b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一淨慮律儀。",
        "from": "T85n2790_001:0653b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是色界尸羅。",
        "from": "T85n2790_001:0653b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二無漏律儀。",
        "from": "T85n2790_001:0653b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為無漏人身中所起尸羅是。",
        "from": "T85n2790_001:0653b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三斷律義。",
        "from": "T85n2790_001:0653b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為斷煩惱名斷律義。",
        "from": "T85n2790_001:0653b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四別解脫律儀。",
        "from": "T85n2790_001:0653b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲界尸羅別防非名解脫律儀。",
        "from": "T85n2790_001:0653b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "T85n2790_001:0653b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略說戒有幾種。",
        "from": "T85n2790_001:0653b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緩急如何　答有三種。",
        "from": "T85n2790_001:0653b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一羯磨說戒。",
        "from": "T85n2790_001:0653b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二對首說戒。",
        "from": "T85n2790_001:0653b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三心念說戒。",
        "from": "T85n2790_001:0653b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然略說戒有三五十五種。",
        "from": "T85n2790_001:0653b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七種從緩向急。",
        "from": "T85n2790_001:0653b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有七略。",
        "from": "T85n2790_001:0653b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從急向緩有十五種略也。",
        "from": "T85n2790_001:0653b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "T85n2790_001:0653b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安居有幾種　答。",
        "from": "T85n2790_001:0653b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有四種。",
        "from": "T85n2790_001:0653b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一忘成安居。",
        "from": "T85n2790_001:0653b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二及界安居。",
        "from": "T85n2790_001:0653b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三對首安居。",
        "from": "T85n2790_001:0653b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四心念託緣。",
        "from": "T85n2790_001:0653b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慮淨名安。",
        "from": "T85n2790_001:0653b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處名居。",
        "from": "T85n2790_001:0653b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "T85n2790_001:0653b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安居是何義　答。",
        "from": "T85n2790_001:0653b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九旬修道標心。",
        "from": "T85n2790_001:0653b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此住故曰安居。",
        "from": "T85n2790_001:0653b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又有三義故。",
        "from": "T85n2790_001:0653b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一無事遊行妨修道業故。",
        "from": "T85n2790_001:0653b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二為損傷物命違慈道故。",
        "from": "T85n2790_001:0653b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三為除招世譏嫌故。",
        "from": "T85n2790_001:0653b25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "T85n2790_001:0653b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自恣有幾種　答。",
        "from": "T85n2790_001:0653b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有三種。",
        "from": "T85n2790_001:0653b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一羯磨自恣。",
        "from": "T85n2790_001:0653b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二對首自恣。",
        "from": "T85n2790_001:0653b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三心念自恣。",
        "from": "T85n2790_001:0653b28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又有三種。",
        "from": "T85n2790_001:0653b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時自恣。",
        "from": "T85n2790_001:0653b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二增益自恣。",
        "from": "T85n2790_001:0653b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三滅劫自恣。",
        "from": "T85n2790_001:0653b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "T85n2790_001:0653c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自恣是何義　答。",
        "from": "T85n2790_001:0653c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為迷已過恣他舉見聞疑罪。",
        "from": "T85n2790_001:0653c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名為自恣義也。",
        "from": "T85n2790_001:0653c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "T85n2790_001:0653c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何名四分律。",
        "from": "T85n2790_001:0653c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "主名何　答。",
        "from": "T85n2790_001:0653c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者數也。",
        "from": "T85n2790_001:0653c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵本四夾。",
        "from": "T85n2790_001:0653c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復云四。",
        "from": "T85n2790_001:0653c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦方成故名四分。",
        "from": "T85n2790_001:0653c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即五分十誦義亦如是。",
        "from": "T85n2790_001:0653c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大僧戒本初分。",
        "from": "T85n2790_001:0653c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尼戒已下為二分。",
        "from": "T85n2790_001:0653c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安居楗度已下為二。",
        "from": "T85n2790_001:0653c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一分房舍揵度已下為四分。",
        "from": "T85n2790_001:0653c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名為四分律。",
        "from": "T85n2790_001:0653c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "律主名曇無德。",
        "from": "T85n2790_001:0653c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此翻為護法。",
        "from": "T85n2790_001:0653c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "T85n2790_001:0653c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十部小乘。",
        "from": "T85n2790_001:0653c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此律何部所攝　上座犢子。",
        "from": "T85n2790_001:0653c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "T85n2790_001:0653c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法密部所攝。",
        "from": "T85n2790_001:0653c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大眾一說說出世鷄胤多聞說假制多西山北山上座轉多雪山一切犢子法上賢胄正量密林化地法藏飲光經量。",
        "from": "T85n2790_001:0653c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "T85n2790_001:0653c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來成道後幾年制戒　答。",
        "from": "T85n2790_001:0653c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來成道後十二年前。",
        "from": "T85n2790_001:0653c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸比丘過失尠。",
        "from": "T85n2790_001:0653c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略制禁戒。",
        "from": "T85n2790_001:0653c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此則利根也。",
        "from": "T85n2790_001:0653c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二年後終至涅槃。",
        "from": "T85n2790_001:0653c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸比丘繁多。",
        "from": "T85n2790_001:0653c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣制禁戒。",
        "from": "T85n2790_001:0653c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此鈍根也。",
        "from": "T85n2790_001:0653c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "T85n2790_001:0653c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受具足戒具幾因緣　答。",
        "from": "T85n2790_001:0653c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具六因緣。",
        "from": "T85n2790_001:0653c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一受戒人身無十三難緣。",
        "from": "T85n2790_001:0653c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二年滿二十。",
        "from": "T85n2790_001:0653c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三衣鉢具足。",
        "from": "T85n2790_001:0653c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四三師數如法清淨。",
        "from": "T85n2790_001:0653c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五在結戒內不得別眾勢分相及。",
        "from": "T85n2790_001:0653c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六一白三羯磨前後方便言辭具足。",
        "from": "T85n2790_001:0653c20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又准摩夷經有十種受戒。",
        "from": "T85n2790_001:0653c21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比丘有五種。",
        "from": "T85n2790_001:0653c21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一上法。",
        "from": "T85n2790_001:0653c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二善來。",
        "from": "T85n2790_001:0653c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三法語。",
        "from": "T85n2790_001:0653c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四勅聽。",
        "from": "T85n2790_001:0653c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五羯磨。",
        "from": "T85n2790_001:0653c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尼亦有種。",
        "from": "T85n2790_001:0653c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一上法。",
        "from": "T85n2790_001:0653c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二善來。",
        "from": "T85n2790_001:0653c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三師法。",
        "from": "T85n2790_001:0653c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四遣使。",
        "from": "T85n2790_001:0653c23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五羯磨。",
        "from": "T85n2790_001:0653c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合為十種。",
        "from": "T85n2790_001:0653c24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "T85n2790_001:0653c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "律以何為宗。",
        "from": "T85n2790_001:0653c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以何體　答。",
        "from": "T85n2790_001:0653c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以戒為宗。",
        "from": "T85n2790_001:0653c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又說不同。",
        "from": "T85n2790_001:0653c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依東塔疏以戒為宗。",
        "from": "T85n2790_001:0653c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "律以何為體。",
        "from": "T85n2790_001:0653c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "T85n2790_001:0653c26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合是語業為體。",
        "from": "T85n2790_001:0653c26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "任意取捨。",
        "from": "T85n2790_001:0653c27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "T85n2790_001:0653c27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二處中以法為體。",
        "from": "T85n2790_001:0653c27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若依經部以不相應為體。",
        "from": "T85n2790_001:0653c28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依有部以色為體。",
        "from": "T85n2790_001:0653c28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱是法處攝。",
        "from": "T85n2790_001:0653c29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "T85n2790_001:0654a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "律之所防三業之中。",
        "from": "T85n2790_001:0654a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何不防意地　答。",
        "from": "T85n2790_001:0654a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所防戒不同別解脫戒。",
        "from": "T85n2790_001:0654a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次是色法但防身口。",
        "from": "T85n2790_001:0654a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故所防心者定道戒。",
        "from": "T85n2790_001:0654a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故得防心。",
        "from": "T85n2790_001:0654a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又問。",
        "from": "T85n2790_001:0654a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聲聞力劣不防意地故。",
        "from": "T85n2790_001:0654a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但身口業故。",
        "from": "T85n2790_001:0654a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "T85n2790_001:0654a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十三同篇。",
        "from": "T85n2790_001:0654a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何故九初便犯四諫方成　答。",
        "from": "T85n2790_001:0654a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸犯戒緣不同。",
        "from": "T85n2790_001:0654a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有倚傍者須僧設諫。",
        "from": "T85n2790_001:0654a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故四戒倚傍相似須僧設諫。",
        "from": "T85n2790_001:0654a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九戒無倚傍。",
        "from": "T85n2790_001:0654a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作即便犯故。",
        "from": "T85n2790_001:0654a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不須諫故。",
        "from": "T85n2790_001:0654a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "T85n2790_001:0654a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "污家犯何罪耶　答。",
        "from": "T85n2790_001:0654a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為違僧諫得殘罪。",
        "from": "T85n2790_001:0654a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直作未諫結罪不同。",
        "from": "T85n2790_001:0654a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自摘花等得提罪。",
        "from": "T85n2790_001:0654a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "污家惡行等得吉罪。",
        "from": "T85n2790_001:0654a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "T85n2790_001:0654a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "污家戒諫何事　答。",
        "from": "T85n2790_001:0654a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所諫之語是傍僧言有愛有恚。",
        "from": "T85n2790_001:0654a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故僧殘。",
        "from": "T85n2790_001:0654a13_3",
        "context_with_next_sentence": false
    }
]