[
    {
        "source_sentence": "受五戒八戒文(海)",
        "from": "T18n0916_001:0941c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八戒文",
        "from": "T18n0916_001:0941c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一稽首請證明品",
        "from": "T18n0916_001:0941c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弟子眾等。",
        "from": "T18n0916_001:0941c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稽首和南虛空法界無上種智。",
        "from": "T18n0916_001:0941c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘形紀身成正覺者。",
        "from": "T18n0916_001:0941c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩訶般若方廣一乘。",
        "from": "T18n0916_001:0941c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十地聖人三賢大士。",
        "from": "T18n0916_001:0941c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空幽顯主異諸神。",
        "from": "T18n0916_001:0941c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色無色天梵王眷屬。",
        "from": "T18n0916_001:0941c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護世帝釋輔翼諸官。",
        "from": "T18n0916_001:0941c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冥道曹寮焰魔羅主。",
        "from": "T18n0916_001:0941c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願今日。",
        "from": "T18n0916_001:0941c11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以淨天眼見於眾人。",
        "from": "T18n0916_001:0941c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虔恭志誠一心奉請。",
        "from": "T18n0916_001:0941c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二洗懺往業品",
        "from": "T18n0916_001:0941c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弟子眾等合道場人。",
        "from": "T18n0916_001:0941c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無始時來至于今日。",
        "from": "T18n0916_001:0941c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "積罪無量難測邊涯。",
        "from": "T18n0916_001:0941c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造愆累生罕究其際。",
        "from": "T18n0916_001:0941c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於三寶所起不信心。",
        "from": "T18n0916_001:0941c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對十方尊行非梵行。",
        "from": "T18n0916_001:0941c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縱身殺害偷盜邪非。",
        "from": "T18n0916_001:0941c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縱口妄言綺語惡罵。",
        "from": "T18n0916_001:0941c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縱心貪欲瞋恚愚癡。",
        "from": "T18n0916_001:0941c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從此廣生無邊惡業。",
        "from": "T18n0916_001:0941c18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "循環三有沒溺四流。",
        "from": "T18n0916_001:0941c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五蓋覆心十纏迷性。",
        "from": "T18n0916_001:0941c19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長居凡品解脫無期。",
        "from": "T18n0916_001:0941c19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵曰懺摩此云悔過。",
        "from": "T18n0916_001:0941c20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從今已後斷相續心。",
        "from": "T18n0916_001:0941c20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往昔罪愆總願消滅。",
        "from": "T18n0916_001:0941c21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三得戒三歸品",
        "from": "T18n0916_001:0941c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弟子眾等合道場人。",
        "from": "T18n0916_001:0941c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸依佛兩足尊。",
        "from": "T18n0916_001:0941c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸依法離欲尊。",
        "from": "T18n0916_001:0941c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸依僧眾中尊。",
        "from": "T18n0916_001:0941c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願三寶慈悲證明哀憐攝受(三說)弟子眾等合道場人。",
        "from": "T18n0916_001:0941c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸依佛竟。",
        "from": "T18n0916_001:0941c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸依法竟。",
        "from": "T18n0916_001:0941c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸依僧竟。",
        "from": "T18n0916_001:0941c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我受八戒近住律儀。",
        "from": "T18n0916_001:0941c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願尊憶持慈悲護念(三說)。",
        "from": "T18n0916_001:0941c26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四問持說品",
        "from": "T18n0916_001:0941c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛子等諦聽。",
        "from": "T18n0916_001:0941c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言八戒者。",
        "from": "T18n0916_001:0941c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂一不殺。",
        "from": "T18n0916_001:0941c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二不盜。",
        "from": "T18n0916_001:0941c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三不染欲。",
        "from": "T18n0916_001:0942a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四不妄語。",
        "from": "T18n0916_001:0942a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五不飲酒食肉。",
        "from": "T18n0916_001:0942a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六不香油塗身及鬘花莊飾。",
        "from": "T18n0916_001:0942a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七不作倡伎樂故往觀聽。",
        "from": "T18n0916_001:0942a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八不上高大床及過中食。",
        "from": "T18n0916_001:0942a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始於今時至明清旦。",
        "from": "T18n0916_001:0942a03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日一夜隨佛出家。",
        "from": "T18n0916_001:0942a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其中閒不得犯。",
        "from": "T18n0916_001:0942a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能持否。",
        "from": "T18n0916_001:0942a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五迴向發願品",
        "from": "T18n0916_001:0942a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弟子眾等合道場人。",
        "from": "T18n0916_001:0942a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受戒善緣盡將迴向法界含生。",
        "from": "T18n0916_001:0942a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願妄執情袪愚癡障盡。",
        "from": "T18n0916_001:0942a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無分別智明了現前。",
        "from": "T18n0916_001:0942a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有相行門熾然修學。",
        "from": "T18n0916_001:0942a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華藏世界遍歷遊從。",
        "from": "T18n0916_001:0942a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來藏心深契根本。",
        "from": "T18n0916_001:0942a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未證者證未圓者圓。",
        "from": "T18n0916_001:0942a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生海中總願成佛。",
        "from": "T18n0916_001:0942a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五戒文",
        "from": "T18n0916_001:0942a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切凡夫迷本覺性。",
        "from": "T18n0916_001:0942a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨妄流轉居三有中。",
        "from": "T18n0916_001:0942a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂六趣區分人天隔越。",
        "from": "T18n0916_001:0942a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛有五戒令人奉持。",
        "from": "T18n0916_001:0942a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從方便門入實相理。",
        "from": "T18n0916_001:0942a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一發誠稽請章",
        "from": "T18n0916_001:0942a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弟子眾等合道場人。",
        "from": "T18n0916_001:0942a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仰啟十方虛空法界成正覺者。",
        "from": "T18n0916_001:0942a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉法輪者般涅槃者。",
        "from": "T18n0916_001:0942a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是我等輩真善知識。",
        "from": "T18n0916_001:0942a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "醫王大師攝受我故。",
        "from": "T18n0916_001:0942a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無邊法藏一味醍醐。",
        "from": "T18n0916_001:0942a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "半滿圓音摩訶衍教。",
        "from": "T18n0916_001:0942a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是我良藥是我舟航。",
        "from": "T18n0916_001:0942a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於生死河濟度我故。",
        "from": "T18n0916_001:0942a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三乘聖眾八輩比丘。",
        "from": "T18n0916_001:0942a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "趣寂聲聞漏悲菩薩。",
        "from": "T18n0916_001:0942a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於我等輩生憐愍心。",
        "from": "T18n0916_001:0942a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如看病人救護我故。",
        "from": "T18n0916_001:0942a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願三寶弘誓願力不思議力。",
        "from": "T18n0916_001:0942a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於真性內起無緣慈。",
        "from": "T18n0916_001:0942a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於今日時受我等請。",
        "from": "T18n0916_001:0942a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二悔過先罪章",
        "from": "T18n0916_001:0942a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弟子眾等合道場人。",
        "from": "T18n0916_001:0942a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曠劫愚迷至于今日。",
        "from": "T18n0916_001:0942a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於第一義不明了故。",
        "from": "T18n0916_001:0942a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生諸障門煩惱所知迷事迷理。",
        "from": "T18n0916_001:0942a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觸境起業隨緣造愆身起三支口犯四過。",
        "from": "T18n0916_001:0942a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "意三業行轉轉逾深。",
        "from": "T18n0916_001:0942b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如沙如塵其數無量。",
        "from": "T18n0916_001:0942b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不覺身篋盛四毒蛇。",
        "from": "T18n0916_001:0942b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五旃陀羅六入空聚。",
        "from": "T18n0916_001:0942b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受詐親善眾多惡緣。",
        "from": "T18n0916_001:0942b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "驅馳識心五趣循轉。",
        "from": "T18n0916_001:0942b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從此積集八萬四千人間。",
        "from": "T18n0916_001:0942b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過非無惡不造。",
        "from": "T18n0916_001:0942b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始於今日省察身心。",
        "from": "T18n0916_001:0942b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "披陳佛前總願消滅。",
        "from": "T18n0916_001:0942b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三轉邪歸敬章",
        "from": "T18n0916_001:0942b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弟子眾等合道場人。",
        "from": "T18n0916_001:0942b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸依佛無上尊。",
        "from": "T18n0916_001:0942b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸依法離欲尊。",
        "from": "T18n0916_001:0942b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸依僧眾中尊自從今日以佛為師。",
        "from": "T18n0916_001:0942b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更不歸餘邪魔外道。",
        "from": "T18n0916_001:0942b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願三寶慈悲證明哀憐攝受(三說)。",
        "from": "T18n0916_001:0942b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弟子眾等合道場人。",
        "from": "T18n0916_001:0942b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸依佛竟。",
        "from": "T18n0916_001:0942b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸依法竟。",
        "from": "T18n0916_001:0942b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸依僧竟。",
        "from": "T18n0916_001:0942b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是佛婆伽婆。",
        "from": "T18n0916_001:0942b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多陀阿伽度阿羅訶三藐三佛陀。",
        "from": "T18n0916_001:0942b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是我大師。",
        "from": "T18n0916_001:0942b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今歸依願佛攝受(三說)。",
        "from": "T18n0916_001:0942b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四說相合修章",
        "from": "T18n0916_001:0942b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛子諦聽。",
        "from": "T18n0916_001:0942b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言優婆塞戒者。",
        "from": "T18n0916_001:0942b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂一不殺。",
        "from": "T18n0916_001:0942b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二不盜。",
        "from": "T18n0916_001:0942b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三不邪行。",
        "from": "T18n0916_001:0942b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四不妄語。",
        "from": "T18n0916_001:0942b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五不飲酒食肉。",
        "from": "T18n0916_001:0942b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始於今時盡未來際。",
        "from": "T18n0916_001:0942b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其中間不得犯。",
        "from": "T18n0916_001:0942b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能持否。",
        "from": "T18n0916_001:0942b17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五迴向願海章",
        "from": "T18n0916_001:0942b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弟子眾等合道場人。",
        "from": "T18n0916_001:0942b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向來受戒。",
        "from": "T18n0916_001:0942b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迴向菩提真如性海。",
        "from": "T18n0916_001:0942b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四流三有一切眾生。",
        "from": "T18n0916_001:0942b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總願發心修普賢行。",
        "from": "T18n0916_001:0942b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不斷佛種生如來家。",
        "from": "T18n0916_001:0942b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還將大悲展轉開示。",
        "from": "T18n0916_001:0942b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲髻中寶得衣裏珠。",
        "from": "T18n0916_001:0942b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當坐道場總願成佛。",
        "from": "T18n0916_001:0942b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受五戒八戒文一卷",
        "from": "T18n0916_001:0942b23_0",
        "context_with_next_sentence": false
    }
]