[
    {
        "source_sentence": "不空羂索毘盧遮那佛大灌頂光",
        "from": "T19n1002_001:0606b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言一卷",
        "from": "T19n1002_001:0606b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　(出不空羂索經二十八卷)",
        "from": "T19n1002_001:0606b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿",
        "from": "T19n1002_001:0606b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "肅國公食邑三千戶賜紫贈司",
        "from": "T19n1002_001:0606b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空諡大鑒正號大廣智大興善",
        "from": "T19n1002_001:0606b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寺三藏沙門不空奉　詔譯",
        "from": "T19n1002_001:0606b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　阿　謨　伽　尾　嚧　左　曩　摩　賀　母捺囉(二合)　麼　抳　鉢　納麼(二合)　入嚩(二合)　攞鉢囉(二合)　韈　哆　野　吽毘盧遮那如來。",
        "from": "T19n1002_001:0606b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為授母陀羅尼印三昧耶神通法品。",
        "from": "T19n1002_001:0606b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而最為第一。",
        "from": "T19n1002_001:0606b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有過去一切十惡五逆四重諸罪。",
        "from": "T19n1002_001:0606b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燼然除滅。",
        "from": "T19n1002_001:0606b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生。",
        "from": "T19n1002_001:0606b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨處得聞此大灌頂光真言。",
        "from": "T19n1002_001:0606b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二三七遍經耳根者。",
        "from": "T19n1002_001:0606b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得除滅一切罪障。",
        "from": "T19n1002_001:0606b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若諸眾生具造十惡五逆四重諸罪。",
        "from": "T19n1002_001:0606b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如微塵滿斯世界。",
        "from": "T19n1002_001:0606b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身壞命終墮諸惡道。",
        "from": "T19n1002_001:0606b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以是真言加持土沙一百八遍。",
        "from": "T19n1002_001:0606b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尸陀林中散亡者尸骸上。",
        "from": "T19n1002_001:0606b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或散墓上。",
        "from": "T19n1002_001:0606b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遇皆散之。",
        "from": "T19n1002_001:0606b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼所亡者。",
        "from": "T19n1002_001:0606b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若地獄中若餓鬼中。",
        "from": "T19n1002_001:0606b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若修羅中若傍生中。",
        "from": "T19n1002_001:0606b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以一切不空如來不空毘盧遮那如來真實本願大灌頂光真言神通威力加持沙土之力。",
        "from": "T19n1002_001:0606b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應時即得光明及身除諸罪報捨所苦身。",
        "from": "T19n1002_001:0606b28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往於西方極樂國土。",
        "from": "T19n1002_001:0606b29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮華化生乃至菩提更不墮落。",
        "from": "T19n1002_001:0606b29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有眾生連年累月。",
        "from": "T19n1002_001:0606c01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "痿黃疾惱苦楚萬端。",
        "from": "T19n1002_001:0606c01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是病人者先世業報。",
        "from": "T19n1002_001:0606c02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以是真言於病者前。",
        "from": "T19n1002_001:0606c02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一二三日每日高聲誦此真言一千八十遍。",
        "from": "T19n1002_001:0606c03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則得除滅宿業病障。",
        "from": "T19n1002_001:0606c04_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若為鬼嬈魂識悶亂失音不語。",
        "from": "T19n1002_001:0606c04_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持真言者加持手一百八遍。",
        "from": "T19n1002_001:0606c05_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩捫頭面。",
        "from": "T19n1002_001:0606c06_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以手按於心上額上。",
        "from": "T19n1002_001:0606c06_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持一千八十遍。",
        "from": "T19n1002_001:0606c06_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則得除差。",
        "from": "T19n1002_001:0606c07_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩訶迦羅神作病惱者。",
        "from": "T19n1002_001:0606c07_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能治遣。",
        "from": "T19n1002_001:0606c08_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若諸鬼神魍魎之病。",
        "from": "T19n1002_001:0606c08_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持五色線索。",
        "from": "T19n1002_001:0606c08_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一百八結。",
        "from": "T19n1002_001:0606c09_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫其病者腰臂項上。",
        "from": "T19n1002_001:0606c09_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則便除差。",
        "from": "T19n1002_001:0606c09_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若諸瘧病。",
        "from": "T19n1002_001:0606c10_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持白線索一百八結。",
        "from": "T19n1002_001:0606c10_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫頭項上。",
        "from": "T19n1002_001:0606c10_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及加持衣著。",
        "from": "T19n1002_001:0606c11_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即令除差。",
        "from": "T19n1002_001:0606c11_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若加持石菖蒲一千八十遍。",
        "from": "T19n1002_001:0606c11_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "含之與他相對談論。",
        "from": "T19n1002_001:0606c12_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則勝他伏。",
        "from": "T19n1002_001:0606c12_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若以胡椒多誐囉香。",
        "from": "T19n1002_001:0606c13_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "青木香小柏檀黃。",
        "from": "T19n1002_001:0606c13_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "囉娑惹娜(唐言小栢汁)等數末。",
        "from": "T19n1002_001:0606c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "治水丸如棗。",
        "from": "T19n1002_001:0606c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持十萬遍。",
        "from": "T19n1002_001:0606c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便當陰乾。",
        "from": "T19n1002_001:0606c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患一切鬼神病種種瘧病。",
        "from": "T19n1002_001:0606c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或毒藥中或失音者。",
        "from": "T19n1002_001:0606c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當以藥和水研之。",
        "from": "T19n1002_001:0606c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持一百八遍。",
        "from": "T19n1002_001:0606c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數點兩眼額上心上。",
        "from": "T19n1002_001:0606c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當怒加持。",
        "from": "T19n1002_001:0606c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則便除差。",
        "from": "T19n1002_001:0606c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作病鬼神若不放捨。",
        "from": "T19n1002_001:0606c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即當頭破如阿梨樹枝。",
        "from": "T19n1002_001:0606c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若諸毒蟲蛇蝎螫者。",
        "from": "T19n1002_001:0606c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以藥塗眼即便除差。",
        "from": "T19n1002_001:0606c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法以新米[亭*單]羅。",
        "from": "T19n1002_001:0606c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "澡浴清淨著淨衣服已。",
        "from": "T19n1002_001:0606c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以藥和水研。",
        "from": "T19n1002_001:0606c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持一百八遍點米[亭*單]羅眼中。",
        "from": "T19n1002_001:0606c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奮怒加持一千八十遍。",
        "from": "T19n1002_001:0606c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則便起坐所問皆答。",
        "from": "T19n1002_001:0606c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲放者。",
        "from": "T19n1002_001:0607a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持白芥子水二十一遍。",
        "from": "T19n1002_001:0607a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散米[亭*單]羅上。",
        "from": "T19n1002_001:0607a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即便如舊。",
        "from": "T19n1002_001:0607a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若為貴人相請喚者。",
        "from": "T19n1002_001:0607a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以藥點眼當往見之。",
        "from": "T19n1002_001:0607a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則相賓敬。",
        "from": "T19n1002_001:0607a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空羂索毘盧遮那佛大灌頂光真言一卷",
        "from": "T19n1002_001:0607a04_0",
        "context_with_next_sentence": false
    }
]