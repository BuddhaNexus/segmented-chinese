[
    {
        "source_sentence": "金剛祕密善門陀羅尼呪經",
        "from": "T20n1138a_001:0581c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "失譯人名今附東晉錄",
        "from": "T20n1138a_001:0581c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T20n1138a_001:0581c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在菩提樹下元吉道場。",
        "from": "T20n1138a_001:0581c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告諸比丘大德舍利弗大德阿難。",
        "from": "T20n1138a_001:0581c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等諸聲聞眾及大菩薩。",
        "from": "T20n1138a_001:0581c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其名曰金剛幢菩薩。",
        "from": "T20n1138a_001:0581c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛藏菩薩彌勒菩薩。",
        "from": "T20n1138a_001:0581c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是賢劫中諸菩薩摩訶薩等而為上首。",
        "from": "T20n1138a_001:0581c21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時金剛密迹菩薩白佛言。",
        "from": "T20n1138a_001:0581c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊多陀阿伽度阿羅呵三藐三佛陀。",
        "from": "T20n1138a_001:0581c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊說善門陀羅尼。",
        "from": "T20n1138a_001:0581c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸眾生而作照明。",
        "from": "T20n1138a_001:0581c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不令諸惡之所嬈害。",
        "from": "T20n1138a_001:0581c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若天龍夜叉若羅剎。",
        "from": "T20n1138a_001:0581c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若鳩槃茶若人非人。",
        "from": "T20n1138a_001:0581c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有奪人精氣者。",
        "from": "T20n1138a_001:0581c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若刀杖若毒藥若方道蟲治。",
        "from": "T20n1138a_001:0581c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若食若飲若行若臥若睡若夢。",
        "from": "T20n1138a_001:0581c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若怨家鬪諍共相言說。",
        "from": "T20n1138a_001:0581c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此陀羅尼。",
        "from": "T20n1138a_001:0581c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸惡不能嬈害爾時佛讚金剛密迹菩薩摩訶薩。",
        "from": "T20n1138a_001:0581c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉金剛密迹汝今善聽。",
        "from": "T20n1138a_001:0582a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當為汝說善門陀羅尼。",
        "from": "T20n1138a_001:0582a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲求救護一切眾生。",
        "from": "T20n1138a_001:0582a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T20n1138a_001:0582a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多狄吔　優久貿久　摩卑尼　曼陀尼　曼陀尼　婆囉　遮利尼休休　盧啝多　莎訶是諸如來灌頂章句之所護持。",
        "from": "T20n1138a_001:0582a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛復重說是呪。",
        "from": "T20n1138a_001:0582a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多狄咃　啝祇啝祇啝祇羅達利　啝耆羅啝帝　和耆羅脾帝　和耆羅達提　遮迦羅　啝耆利　遮迦羅達利達利達利貿利　遮利休休利多　伽脾陛利　利利尸利尸利留留之　遮利周利貿利曼茶利　薩婆波簸　脾那奢兒　鵄鵄兜毘離　摩囄呵多尼　跛離　婆離　跋提毘提毘離　抑娑離　奢羅毘　羅和泥羅　啝那茶移娑呵摩遮離拏　因陀羅和帝提提羅耶　南無摩醯首波　羅耶尼利　師梵浮尼　波波嚪婆尼迦囉啝提　浮多啝提　薩夕迦鬄　蘇摩啝帝帝　蘇摩鉢羅莎呵　絰伽由迦多耶　多狄他　呵邏呵邏呵邏唏邏伊曇摩蒲遮　波伽婆　莎呵　多狄他　伽絰多婆絰多阿逸多阿利嚪婆尼　迦伽怛尼　阿囉茶呵尼　末伽毘盧呵尼休婆啝帝　浮娑啝帝唏利唏利　耶他嗜　耶他阿斤尼　耶他波連遮　耶他婆炎　耶他唏　利多　阿炎婆嗜羅波尼。",
        "from": "T20n1138a_001:0582a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘇目佉他　羅尼鉢但尼　多咃伽帝那　婆師多薩婆薩埵那末咃耶　迦留那多耶阿唅婆沙彌　耶咃帝弗婆枳　多咃伽帝　阿羅呵三藐菩提婆師多　阿提癡多　薩夕婆羅底爾那脾跋咃卑多爾時佛告金剛密迹。",
        "from": "T20n1138a_001:0582a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾欲利益一切眾生說陀羅尼。",
        "from": "T20n1138a_001:0582b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如過去諸佛受持守護。",
        "from": "T20n1138a_001:0582b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦護持。",
        "from": "T20n1138a_001:0582b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛放頂上螺髮光明。",
        "from": "T20n1138a_001:0582b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現大人相遍照十方。",
        "from": "T20n1138a_001:0582b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛世界靡不周遍。",
        "from": "T20n1138a_001:0582b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛各各同聲讚言。",
        "from": "T20n1138a_001:0582b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉釋迦牟尼佛。",
        "from": "T20n1138a_001:0582b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "快說是善門陀羅尼。",
        "from": "T20n1138a_001:0582b05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸眾生而作照明。",
        "from": "T20n1138a_001:0582b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等亦當受持守護。",
        "from": "T20n1138a_001:0582b06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讚言善哉善哉。",
        "from": "T20n1138a_001:0582b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人受持讀誦善門陀羅尼者。",
        "from": "T20n1138a_001:0582b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當於晨朝一切諸佛常當護念。",
        "from": "T20n1138a_001:0582b08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若先所造罪報業障漸得除滅。",
        "from": "T20n1138a_001:0582b09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是善男子復得二十種功德。",
        "from": "T20n1138a_001:0582b09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者長命。",
        "from": "T20n1138a_001:0582b10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者老壽。",
        "from": "T20n1138a_001:0582b10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者名稱。",
        "from": "T20n1138a_001:0582b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者遠聞。",
        "from": "T20n1138a_001:0582b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者大富。",
        "from": "T20n1138a_001:0582b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六者大力。",
        "from": "T20n1138a_001:0582b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七者端正。",
        "from": "T20n1138a_001:0582b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八者好色。",
        "from": "T20n1138a_001:0582b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九者相好。",
        "from": "T20n1138a_001:0582b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十者嚴身。",
        "from": "T20n1138a_001:0582b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十一者無病。",
        "from": "T20n1138a_001:0582b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二者安樂。",
        "from": "T20n1138a_001:0582b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十三者諸佛護念。",
        "from": "T20n1138a_001:0582b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十四者諸天守護。",
        "from": "T20n1138a_001:0582b13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十五者善根成熟。",
        "from": "T20n1138a_001:0582b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十六者精進。",
        "from": "T20n1138a_001:0582b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十七者無厭。",
        "from": "T20n1138a_001:0582b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十八者辯才。",
        "from": "T20n1138a_001:0582b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十九者解議。",
        "from": "T20n1138a_001:0582b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十者善根成就。",
        "from": "T20n1138a_001:0582b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有受持讀誦。",
        "from": "T20n1138a_001:0582b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得如上所說二十種具足功德。",
        "from": "T20n1138a_001:0582b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛告金剛密迹。",
        "from": "T20n1138a_001:0582b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是陀羅尼皆是諸佛祕密藏法。",
        "from": "T20n1138a_001:0582b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝好受持無令漏失。",
        "from": "T20n1138a_001:0582b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆是諸佛之所住處。",
        "from": "T20n1138a_001:0582b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時金剛密迹菩薩白佛言。",
        "from": "T20n1138a_001:0582b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我亦說陀羅尼守護是人。",
        "from": "T20n1138a_001:0582b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持讀誦善門陀羅尼者。",
        "from": "T20n1138a_001:0582b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不令諸惡得其便也。",
        "from": "T20n1138a_001:0582b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T20n1138a_001:0582b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寫狄鬄曇　式棄式棄　指指指指婆婆　婆婆留留　留留啫啫啫啫伽伽伽伽啼啼啼啼　破破破破　摩摩摩摩　訶那訶那訶那訶那　陀訶陀訶鉢遮鉢鉢遮　莎呵爾時佛復讚金剛密迹菩薩。",
        "from": "T20n1138a_001:0582b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉金剛密迹。",
        "from": "T20n1138a_001:0582b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令汝呪術悉皆神驗利益眾生。",
        "from": "T20n1138a_001:0582b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有奪人精神氣者。",
        "from": "T20n1138a_001:0582b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為作守護使不得便。",
        "from": "T20n1138a_001:0582b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時梵天王從座而起。",
        "from": "T20n1138a_001:0582b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊。",
        "from": "T20n1138a_001:0582c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人受持讀誦善門陀羅尼者。",
        "from": "T20n1138a_001:0582c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦隨喜說陀羅尼守護是人。",
        "from": "T20n1138a_001:0582c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令其所呪悉得神驗。",
        "from": "T20n1138a_001:0582c03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T20n1138a_001:0582c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多狄他　啼利　彌利　至利　莎呵　婆呵　摩富利　莎呵　婆呵摩脾　莎呵　波羅　迦卑弗　波散　哆離　莎呵爾時釋提桓因。",
        "from": "T20n1138a_001:0582c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從座而起白佛言。",
        "from": "T20n1138a_001:0582c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我亦說陀羅尼守護是人。",
        "from": "T20n1138a_001:0582c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持讀誦一切諸惡不令得便。",
        "from": "T20n1138a_001:0582c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T20n1138a_001:0582c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寫狄鬄曇毘尼啝羅尼　啝陀摩邏持趾趾趾瞿利　揵陀利　波邏摩邏婆　阿那摩慎多邏尼　陀邏摩利尼　遮迦羅啝枳　簷婆利　簷婆利　莎呵爾時四天王從座而起。",
        "from": "T20n1138a_001:0582c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊。",
        "from": "T20n1138a_001:0582c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等四王亦說陀羅尼守護是人。",
        "from": "T20n1138a_001:0582c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不令諸惡得其便。",
        "from": "T20n1138a_001:0582c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T20n1138a_001:0582c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多狄他　弗卑修弗卑　豆摩簸梨　呵梨　呵梨　耶波邏奢帝　涅目帝　瞢伽梨兜　帝　兜低　莎呵爾時釋迦牟尼佛。",
        "from": "T20n1138a_001:0582c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "申其右臂摩釋梵護世諸現士頂讚言。",
        "from": "T20n1138a_001:0582c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉快說是陀羅尼守護眾生。",
        "from": "T20n1138a_001:0582c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常為諸佛之所護持。",
        "from": "T20n1138a_001:0582c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛復告金剛密迹菩薩。",
        "from": "T20n1138a_001:0582c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人。",
        "from": "T20n1138a_001:0582c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持讀誦善門陀羅尼者。",
        "from": "T20n1138a_001:0582c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當於晨朝。",
        "from": "T20n1138a_001:0582c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸佛及諸天神常隨護助。",
        "from": "T20n1138a_001:0582c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求其短者使不得便。",
        "from": "T20n1138a_001:0582c25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時金剛密迹菩薩。",
        "from": "T20n1138a_001:0582c25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切大眾諸天及人乾闥婆阿修羅等。",
        "from": "T20n1138a_001:0582c26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛所說歡喜奉行。",
        "from": "T20n1138a_001:0582c27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛祕密善門陀羅尼經(與丹本不同)",
        "from": "T20n1138a_001:0582c28_0",
        "context_with_next_sentence": false
    }
]