[
    {
        "source_sentence": "何耶揭唎婆觀世音菩薩受法",
        "from": "T20n1074_001:0171c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壇",
        "from": "T20n1074_001:0171c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有沙門。",
        "from": "T20n1074_001:0171c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若婆羅門諸善男子善女人等。",
        "from": "T20n1074_001:0171c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲受持菩薩法者。",
        "from": "T20n1074_001:0171c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應作四肘壇。",
        "from": "T20n1074_001:0172a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當覓勝地清淨之所掃灑清淨。",
        "from": "T20n1074_001:0172a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以香水牛糞埿地。",
        "from": "T20n1074_001:0172a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懸於種種雜色幡蓋寶鈴珮鏡。",
        "from": "T20n1074_001:0172a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并諸金銀種種間錯嚴錺道場。",
        "from": "T20n1074_001:0172a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其道場中立五色壇。",
        "from": "T20n1074_001:0172a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縱廣四肘。",
        "from": "T20n1074_001:0172a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先下白色次黃次赤次黑。",
        "from": "T20n1074_001:0172a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而作四門。",
        "from": "T20n1074_001:0172a05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其壇中心作蓮華座。",
        "from": "T20n1074_001:0172a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安置何耶揭利婆觀世音像。",
        "from": "T20n1074_001:0172a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東門作華座安十一面菩薩。",
        "from": "T20n1074_001:0172a06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北門作蓮華座安八臂觀世音。",
        "from": "T20n1074_001:0172a07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南方安八龍王。",
        "from": "T20n1074_001:0172a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難陀龍王。",
        "from": "T20n1074_001:0172a08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婆素難龍王。",
        "from": "T20n1074_001:0172a08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "德叉迦龍王。",
        "from": "T20n1074_001:0172a09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羯固奼龍王。",
        "from": "T20n1074_001:0172a09_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "般摩龍王。",
        "from": "T20n1074_001:0172a09_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩訶般摩龍王。",
        "from": "T20n1074_001:0172a10_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "商佉波羅龍王。",
        "from": "T20n1074_001:0172a10_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳩利迦龍王(八龍唯供粳米乳糜。",
        "from": "T20n1074_001:0172a10_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘者以種種供養亦得)燃四十五燈。",
        "from": "T20n1074_001:0172a11_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先喚八箇龍王(用何耶揭唎婆身印來去)。",
        "from": "T20n1074_001:0172a11_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵闍(去音)耶毘闍(去音)耶(二)阿瑟吒(二合)那(去聲)伽羅闍耶(三)阿羅闡都(四)莎訶(五)其壇西門近。",
        "from": "T20n1074_001:0172a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南安一火爐(以胡麻稻穀華等蘇蜜相和。",
        "from": "T20n1074_001:0172a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦心呪投火中燒之。",
        "from": "T20n1074_001:0172a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿一千八遍)次請中心何耶揭唎婆菩薩。",
        "from": "T20n1074_001:0172a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次請八臂觀音(身印來去)。",
        "from": "T20n1074_001:0172a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)夜勢夜輒擔(二)莎訶",
        "from": "T20n1074_001:0172a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次用華座印來去。",
        "from": "T20n1074_001:0172a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請十一面菩薩已。",
        "from": "T20n1074_001:0172a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散種種花燒諸名香。",
        "from": "T20n1074_001:0172a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沈水薰陸栴檀香等而供養之。",
        "from": "T20n1074_001:0172a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪師一日不食。",
        "from": "T20n1074_001:0172a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不忍飢唯得食蘇。",
        "from": "T20n1074_001:0172a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香湯洗浴著新淨衣。",
        "from": "T20n1074_001:0172a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入道場中而作供養。",
        "from": "T20n1074_001:0172a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常得觀世音菩薩憶念。",
        "from": "T20n1074_001:0172a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦兼通軍荼利法結界供養也。",
        "from": "T20n1074_001:0172a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當以真言印護身及結界。",
        "from": "T20n1074_001:0172a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪木七返或呪水七返。",
        "from": "T20n1074_001:0172a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以印呪水。",
        "from": "T20n1074_001:0172a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或呪白芥子灰等七返。",
        "from": "T20n1074_001:0172a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將木竪四角為界。",
        "from": "T20n1074_001:0172a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "芥子灰水散於十方閒成結界。",
        "from": "T20n1074_001:0172a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手外縛二食指直竪相去五分許。",
        "from": "T20n1074_001:0172a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並二大指相著。",
        "from": "T20n1074_001:0172a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各屈一節勿著食指。",
        "from": "T20n1074_001:0172a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1074_001:0172a28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)鉢羅毘迦悉跢(二)跋折羅(三)涉筏(二合)囉囉(上音)支(四)莎訶何耶揭唎婆解禁刀印(舒二手左手在外右手在內。",
        "from": "T20n1074_001:0172a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左掌著右手背。",
        "from": "T20n1074_001:0172b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二手大指)。",
        "from": "T20n1074_001:0172b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)阿娑忙義儞(二)莎呵(三句)何耶揭唎婆大法身印(二手外縛以二小指竪相合。",
        "from": "T20n1074_001:0172b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並二大指竪相著屈。",
        "from": "T20n1074_001:0172b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怒大指來去)。",
        "from": "T20n1074_001:0172b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)杜那杜那(二)摩他摩他(三)可馱可馱(四)訶耶揭唎婆(五)嗚吽泮(六)莎(去音)訶(七)次何耶揭唎婆心印(二手外縛並二大指相著。",
        "from": "T20n1074_001:0172b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各屈一節勿著食指。",
        "from": "T20n1074_001:0172b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指來去)。",
        "from": "T20n1074_001:0172b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)阿蜜唎都智(二合)婆(平音二)嗚[合*牛]泮(三)莎(去音)訶次頭印(二手外縛立二食指。",
        "from": "T20n1074_001:0172b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭相拄各屈二大指節。",
        "from": "T20n1074_001:0172b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小指頭尖也)。",
        "from": "T20n1074_001:0172b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "跢姪他(一)斫迦羅[口*例]怖(二)斫迦羅(去音)乞叉[口*例]怖(三)斫迦羅跛曇摩叉唎怖(四)阿謨迦(去音)寫(稱彼人名五)始羅(上音)枳羅(上音二合六)跛羅舍網伽車(上音)覩(七)莎訶(八叉去沙反餘處皆然)次頂印(準前頭印。",
        "from": "T20n1074_001:0172b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯改竪二食指頭相著。",
        "from": "T20n1074_001:0172b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "列二中指在二食指前頭相拄)。",
        "from": "T20n1074_001:0172b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)室唎(二合)羅唎至(二)嗚[合*牛]泮(三)莎(去)訶(四)次口印(二手小指無名指內相叉二中指頭相拄。",
        "from": "T20n1074_001:0172b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並竪二大指。",
        "from": "T20n1074_001:0172b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右風捻右大頭。",
        "from": "T20n1074_001:0172b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左頭指竪小曲在中指邊)。",
        "from": "T20n1074_001:0172b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)鶻嚕嚧(二)鳩嚧馱那(三)嗚[合*牛]泮(四)莎訶次牙印(準前觀世音不空羂索菩薩牙印。",
        "from": "T20n1074_001:0172b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上準改二中指直竪頭相著。",
        "from": "T20n1074_001:0172b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指並竪博中指。",
        "from": "T20n1074_001:0172b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以左小指握右無名指背後。",
        "from": "T20n1074_001:0172b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右小指握左小指背)。",
        "from": "T20n1074_001:0172b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南(上音)謨(上)囉跢那(二合)怛囉(二合)夜耶(一)南謨阿唎耶(二)婆盧吉帝(三)攝婆(二合)羅(去)耶(四)菩提薩埵(去)耶(五)摩訶薩埵(去)耶摩訶迦盧尼迦耶(七)跢姪他(八)跢羅(上二合)吒(九)跢羅(上二合)吒(十)末吒末吒(十一)瞋陀瞋陀(十二)頻陀頻陀(十三)嗚[合*牛](十四)泮泮(十五)莎訶何耶揭唎婆觀世音菩薩大呪。",
        "from": "T20n1074_001:0172b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又何耶揭唎婆別大呪(在別)是二呪者波帝吒悉陀波泮底(唐云隨誦成驗)雖不受持壇供養法隨誦成驗。",
        "from": "T20n1074_001:0172c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲發遣何耶揭唎婆觀世音者。",
        "from": "T20n1074_001:0172c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手執素羅毘布瑟波(二合反又云柏花)呪一遍已。",
        "from": "T20n1074_001:0172c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一打像上。",
        "from": "T20n1074_001:0172c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是七遍。",
        "from": "T20n1074_001:0172c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即歸去也真言曰。",
        "from": "T20n1074_001:0172c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "跢姪他(一)薩羅波羅薩羅(二)社耶毘社耶(三)跋折羅句羅娑三婆(去音)皤(四)莎跛那迦車目都起(六)莎(去)訶(七)",
        "from": "T20n1074_001:0172c08_0",
        "context_with_next_sentence": false
    }
]