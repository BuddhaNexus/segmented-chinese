[
    {
        "source_sentence": "金剛頂經觀自在王如來修行法",
        "from": "T19n0931_001:0072c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一卷",
        "from": "T19n0931_001:0072c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T19n0931_001:0072c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空諡大鑒",
        "from": "T19n0931_001:0072c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正號大廣智大興善寺三藏沙門",
        "from": "T19n0931_001:0072c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空奉　詔譯",
        "from": "T19n0931_001:0072c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今依金剛頂經。",
        "from": "T19n0931_001:0072c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "演金剛蓮花達摩法要。",
        "from": "T19n0931_001:0072c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先須入於灌頂三昧耶曼茶羅。",
        "from": "T19n0931_001:0072c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發大菩提心。",
        "from": "T19n0931_001:0073a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨身命財饒益一切。",
        "from": "T19n0931_001:0073a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勇猛精進隨念相應。",
        "from": "T19n0931_001:0073a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈悲喜捨無有間斷。",
        "from": "T19n0931_001:0073a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是之人方應修習。",
        "from": "T19n0931_001:0073a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其曼茶羅畫像等法廣如餘經所說。",
        "from": "T19n0931_001:0073a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡入精舍欲念誦時。",
        "from": "T19n0931_001:0073a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以五輪著地。",
        "from": "T19n0931_001:0073a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂禮本尊觀自在王如來。",
        "from": "T19n0931_001:0073a05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次禮北方不空成就如來乃至無動寶生遍照如來。",
        "from": "T19n0931_001:0073a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆依法至誠敬禮。",
        "from": "T19n0931_001:0073a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雙膝而跪蓮花合掌。",
        "from": "T19n0931_001:0073a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懺悔三業一切過咎。",
        "from": "T19n0931_001:0073a08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　我從無量劫　　淪滯生死海",
        "from": "T19n0931_001:0073a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　今以清淨心　　發露而悔過",
        "from": "T19n0931_001:0073a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如諸佛所懺　　我今亦如是",
        "from": "T19n0931_001:0073a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　願我及眾生　　一切皆清淨",
        "from": "T19n0931_001:0073a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T19n0931_001:0073a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)馺嚩(二合)婆(去)嚩秝(入)陀(二)薩嚩達摩(三)馺嚩(二合)婆(去)嚩(四)秝(入)度澸(五)次應隨喜過現未來諸佛菩薩所集福智。",
        "from": "T19n0931_001:0073a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　過去三世佛　　菩薩與眾生",
        "from": "T19n0931_001:0073a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　所集諸善根　　合掌盡隨喜",
        "from": "T19n0931_001:0073a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應右膝著地。",
        "from": "T19n0931_001:0073a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛心合掌置於頂上。",
        "from": "T19n0931_001:0073a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想禮諸佛如來及菩薩足。",
        "from": "T19n0931_001:0073a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0073a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵鉢納摩(二合上)尾多(多字半音與尾字合呼)禮諸佛已。",
        "from": "T19n0931_001:0073a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依座印而坐入定思惟。",
        "from": "T19n0931_001:0073a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀無量如來周遍法界。",
        "from": "T19n0931_001:0073a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者己身悉在彼會。",
        "from": "T19n0931_001:0073a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後結祕密三昧耶印。",
        "from": "T19n0931_001:0073a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅固縛以檀慧禪智。",
        "from": "T19n0931_001:0073a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相合而竪。",
        "from": "T19n0931_001:0073a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此驚覺聖眾。",
        "from": "T19n0931_001:0073a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0073a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)嚩日囉(二合二)跛娜莾(二合三)三昧耶(四)薩怛梵(三合五)次應結一切如來歡喜悅意三昧耶印。",
        "from": "T19n0931_001:0073a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前印唯以忍願入掌中。",
        "from": "T19n0931_001:0073a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T19n0931_001:0073a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)三莾耶呼(引二)蘇(去)囉多(三)薩怛梵(三合四)次結開心印。",
        "from": "T19n0931_001:0073a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀二乳上有怛囉(二合)吒字。",
        "from": "T19n0931_001:0073b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以金剛縛。",
        "from": "T19n0931_001:0073b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三拍開之。",
        "from": "T19n0931_001:0073b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T19n0931_001:0073b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)嚩日囉(二合二)滿馱(三)怛囉(二合)吒(四)次結入智字印。",
        "from": "T19n0931_001:0073b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於己面前。",
        "from": "T19n0931_001:0073b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀一紅蓮華上有娑字。",
        "from": "T19n0931_001:0073b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以堅固縛禪智入中。",
        "from": "T19n0931_001:0073b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力相拄。",
        "from": "T19n0931_001:0073b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想捻其字內於心中。",
        "from": "T19n0931_001:0073b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0073b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)嚩日囉(二合引)吠捨(二)惡(三)次結闔智字印。",
        "from": "T19n0931_001:0073b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不易前印。",
        "from": "T19n0931_001:0073b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力拄禪智背。",
        "from": "T19n0931_001:0073b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想闔心門。",
        "from": "T19n0931_001:0073b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T19n0931_001:0073b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)嚩日囉(二合二)母瑟致(二合三)[牟*含](四)次結辟除結界印。",
        "from": "T19n0931_001:0073b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戒方右押左外相交。",
        "from": "T19n0931_001:0073b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忍願相拄。",
        "from": "T19n0931_001:0073b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以進力鉤戒方。",
        "from": "T19n0931_001:0073b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪智捻檀慧頭。",
        "from": "T19n0931_001:0073b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T19n0931_001:0073b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)阿密栗(二合)覩娜婆(二合)嚩(二)吽(三)泮吒(吒半音呼之四)以此印左轉三匝。",
        "from": "T19n0931_001:0073b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辟除不善。",
        "from": "T19n0931_001:0073b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋三匝。",
        "from": "T19n0931_001:0073b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意遠近以為結界。",
        "from": "T19n0931_001:0073b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結三摩地印。",
        "from": "T19n0931_001:0073b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二羽仰叉。",
        "from": "T19n0931_001:0073b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進力相背而竪。",
        "from": "T19n0931_001:0073b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪智捻進力頭。",
        "from": "T19n0931_001:0073b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置於跏上。",
        "from": "T19n0931_001:0073b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0073b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)三麼(引)地(二)鉢娜名(二合三)頡哩(二合引)其出入息一一明了。",
        "from": "T19n0931_001:0073b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀虛空中無量諸佛。",
        "from": "T19n0931_001:0073b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時彈指警覺行者而告之言。",
        "from": "T19n0931_001:0073b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今云何成無上覺。",
        "from": "T19n0931_001:0073b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不知諸佛實相法要。",
        "from": "T19n0931_001:0073b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者爾時聞警覺已。",
        "from": "T19n0931_001:0073b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言云何名真實。",
        "from": "T19n0931_001:0073b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願如來為我解說。",
        "from": "T19n0931_001:0073b25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時諸佛告行者言。",
        "from": "T19n0931_001:0073b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉能作是問。",
        "from": "T19n0931_001:0073b26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝想心中所內惡字瑩徹於心。",
        "from": "T19n0931_001:0073b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0073b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)唧多鉢囉(二合)底(二)味鄧迦[口*路]弭(三)當默誦一遍。",
        "from": "T19n0931_001:0073b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便想為月輪。",
        "from": "T19n0931_001:0073b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如在輕霧。",
        "from": "T19n0931_001:0073b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲令其月輪得清淨故。",
        "from": "T19n0931_001:0073c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此密言曰。",
        "from": "T19n0931_001:0073c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)冒地唧多母怛跛(二二)娜夜弭誦已諦觀心月極令清淨。",
        "from": "T19n0931_001:0073c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如處大虛廓無瑕翳。",
        "from": "T19n0931_001:0073c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復於心月想一蓮華。",
        "from": "T19n0931_001:0073c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0073c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)底瑟吒(二合二)嚩日囉(二合三)鉢娜麼(二合四)想其蓮花與月。",
        "from": "T19n0931_001:0073c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漸漸而引周遍法界。",
        "from": "T19n0931_001:0073c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "量等虛空。",
        "from": "T19n0931_001:0073c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0073c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)薩頗(二合)囉(二)嚩日囉(二合三)鉢娜麼(二合四)其引蓮華中放無量光。",
        "from": "T19n0931_001:0073c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨光流出無量無邊際極樂世界。",
        "from": "T19n0931_001:0073c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一世界妙寶莊嚴。",
        "from": "T19n0931_001:0073c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆有觀自在王如來。",
        "from": "T19n0931_001:0073c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與聖眾前後圍遶。",
        "from": "T19n0931_001:0073c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是觀已漸斂其蓮華。",
        "from": "T19n0931_001:0073c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0073c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)僧賀囉(二合二)鉢娜麼(二合三)如是一切世界諸佛如來。",
        "from": "T19n0931_001:0073c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨華而斂量等本身。",
        "from": "T19n0931_001:0073c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即變己身為無量壽佛身。",
        "from": "T19n0931_001:0073c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "紅頗梨色放大光明。",
        "from": "T19n0931_001:0073c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結三麼地印金剛蓮華座。",
        "from": "T19n0931_001:0073c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想行者己身為如來已。",
        "from": "T19n0931_001:0073c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復恐散亂而有退失。",
        "from": "T19n0931_001:0073c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次作加持印而加持之。",
        "from": "T19n0931_001:0073c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅固縛以進力屈如蓮葉。",
        "from": "T19n0931_001:0073c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪智並竪印心額喉頂隨所印。",
        "from": "T19n0931_001:0073c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想頡哩(二合)字。",
        "from": "T19n0931_001:0073c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0073c20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)嚩日囉(二合二)達麼(三)頡哩(二合)次結灌頂印。",
        "from": "T19n0931_001:0074a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮華合掌進力相蹙如寶。",
        "from": "T19n0931_001:0074a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "檀慧竪相遠。",
        "from": "T19n0931_001:0074a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置於額上。",
        "from": "T19n0931_001:0074a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T19n0931_001:0074a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)鉢娜麼(二合二)囉怛那(二合引三)毘詵遮(四)薩嚩[牟*含](五)頡哩(二合六)怛[口*路](二合七)次結蓮華鬘印。",
        "from": "T19n0931_001:0074a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以前印從額分手。",
        "from": "T19n0931_001:0074a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繞至頂後。",
        "from": "T19n0931_001:0074a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結蓮華拳。",
        "from": "T19n0931_001:0074a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如繫華鬘勢。",
        "from": "T19n0931_001:0074a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徐徐前下散誦密言曰。",
        "from": "T19n0931_001:0074a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)鉢娜麼(二合二)囉怛那(二合三)麼(引)隷(四)[牟*含](牟含反五)次結甲冑印。",
        "from": "T19n0931_001:0074a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結蓮華拳。",
        "from": "T19n0931_001:0074a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後繞身如擐甲法。",
        "from": "T19n0931_001:0074a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)阿婆耶(二)鉢娜麼(二合三)迦嚩制(四)滿馱(五)囉乞灑(二合)[牟*含](六)吽(七)憾(八)次結歡喜印。",
        "from": "T19n0931_001:0074a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮華合掌微拍三聲。",
        "from": "T19n0931_001:0074a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)鉢娜麼(二合二)覩使野(二合)護(引三)復於面前。",
        "from": "T19n0931_001:0074a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀安樂世界。",
        "from": "T19n0931_001:0074a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "琉璃為地功德乳海。",
        "from": "T19n0931_001:0074a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其海中觀頡哩(二合)字。",
        "from": "T19n0931_001:0074a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變為微妙開敷蓮華。",
        "from": "T19n0931_001:0074a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即變其華為觀自在王如來。",
        "from": "T19n0931_001:0074a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色相莊嚴如前身觀。",
        "from": "T19n0931_001:0074a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結蓮華鉤印。",
        "from": "T19n0931_001:0074a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮華合掌進力如鉤。",
        "from": "T19n0931_001:0074a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)鉢娜懵(二合引二)句捨(三)吽(四)若(五)次結蓮華索印。",
        "from": "T19n0931_001:0074a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以前印進力如環。",
        "from": "T19n0931_001:0074a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)鉢娜麼(二合引二)母伽播捨(三)吽(四)次結蓮華鎖印。",
        "from": "T19n0931_001:0074a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "芙蓉合掌進禪相捻。",
        "from": "T19n0931_001:0074a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "力智亦爾。",
        "from": "T19n0931_001:0074a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相鉤成鎖。",
        "from": "T19n0931_001:0074a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)鉢娜麼(二合二)塞怖(二合)吒(三)[牟*含](四)次結蓮華鈴印。",
        "from": "T19n0931_001:0074b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以蓮華合掌。",
        "from": "T19n0931_001:0074b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪智入中進力如環。",
        "from": "T19n0931_001:0074b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)鉢娜麼(二合引二)吠捨惡(三)次結蓮華部百字密言。",
        "from": "T19n0931_001:0074b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捧閼伽器。",
        "from": "T19n0931_001:0074b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以欝金龍腦相和香水。",
        "from": "T19n0931_001:0074b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而為供養。",
        "from": "T19n0931_001:0074b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦百字密言曰。",
        "from": "T19n0931_001:0074b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)鉢娜麼(二合二)薩怛嚩(二合三)三麼耶(四)麼弩播攞耶(五)鉢那麼(二合六)薩怛嚩(二合七)帝尾(二合)弩波底瑟姹(二合八)涅哩(二合)擢(茶護反)茗皤嚩(九)蘇覩灑諭(二合)茗皤嚩(十)阿弩囉訖覩(二合)茗皤嚩(十一)蘇報灑諭(二合)茗皤嚩(十二)薩嚩悉簟寐鉢囉(二合)也瑳(十三)薩婆羯麼蘇者茗(十四)唧多失唎(二合)藥(十五)句嚧(十六)吽(十七)訶(十八)訶(十九)訶(二十)訶(二十一)護(二十二)薄伽梵(二十三)薩婆怛他孽多(二十四)鉢娜麼(二合二十五)莽茗悶遮(二十六)鉢娜茗(二合)皤嚩(二十七)麼訶三麼野(二十八)薩怛嚩(二合二十九)惡(引三十)次結蓮華內四供養嬉戲印。",
        "from": "T19n0931_001:0074b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "芙蓉合掌禪智並竪誦。",
        "from": "T19n0931_001:0074b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T19n0931_001:0074b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)鉢娜麼(二合二)邏細(三)呼(四引)次結蓮華鬘印。",
        "from": "T19n0931_001:0074b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以前印。",
        "from": "T19n0931_001:0074b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二臂俱申。",
        "from": "T19n0931_001:0074b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)鉢娜麼(二合二)麼隷(三)怛囉(二合)吒(半音輕呼)次結蓮華歌印。",
        "from": "T19n0931_001:0074b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以芙蓉合掌。",
        "from": "T19n0931_001:0074b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈拄諸度。",
        "from": "T19n0931_001:0074b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從臍而上。",
        "from": "T19n0931_001:0074b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至口方散。",
        "from": "T19n0931_001:0074b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074b25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)鉢娜麼(二合二)擬(妍以反)帝(三)擬(同上音四)次結蓮華舞印。",
        "from": "T19n0931_001:0074b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以芙蓉合掌。",
        "from": "T19n0931_001:0074b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左右而旋。",
        "from": "T19n0931_001:0074b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於頂上散。",
        "from": "T19n0931_001:0074b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)鉢娜麼(二合二)儞哩(二合)諦(三)訖哩(二合)吒(半聲四)次結蓮華外四供養燒香印。",
        "from": "T19n0931_001:0074c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮華合掌下散。",
        "from": "T19n0931_001:0074c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如焚香。",
        "from": "T19n0931_001:0074c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)鉢娜麼(二合二)度閉(二合三)噁(四)次結蓮華散華印。",
        "from": "T19n0931_001:0074c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮華合掌前上散。",
        "from": "T19n0931_001:0074c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如散華勢。",
        "from": "T19n0931_001:0074c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)鉢娜麼(二合二)補瑟閉(二合三)唵(四)次結蓮華燈印。",
        "from": "T19n0931_001:0074c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮華合掌禪智竪相逼。",
        "from": "T19n0931_001:0074c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)鉢娜麼(二合二)儞(引)閉(三)儞(四)次結蓮華塗香印。",
        "from": "T19n0931_001:0074c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十度作蓮華合掌。",
        "from": "T19n0931_001:0074c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當心分散。",
        "from": "T19n0931_001:0074c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如塗香勢。",
        "from": "T19n0931_001:0074c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)鉢娜麼(二合二)囐提(三)虐(四)次結根本印。",
        "from": "T19n0931_001:0074c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅固縛以忍願相拄。",
        "from": "T19n0931_001:0074c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如蓮葉形。",
        "from": "T19n0931_001:0074c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0074c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)路計什嚩(二合)囉(二)囉(引)惹(三)頡哩(二合四)結根本印。",
        "from": "T19n0931_001:0074c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦明七遍已。",
        "from": "T19n0931_001:0074c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後執蓮子珠。",
        "from": "T19n0931_001:0074c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相應念誦。",
        "from": "T19n0931_001:0074c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於四時中隨力而作。",
        "from": "T19n0931_001:0074c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲止時。",
        "from": "T19n0931_001:0074c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重結根本印復誦七遍根本陀羅尼。",
        "from": "T19n0931_001:0074c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結八供養印。",
        "from": "T19n0931_001:0075a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後發遣本尊。",
        "from": "T19n0931_001:0075a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發遣印。",
        "from": "T19n0931_001:0075a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以前根本印。",
        "from": "T19n0931_001:0075a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從臍至面方散。",
        "from": "T19n0931_001:0075a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌於頂上。",
        "from": "T19n0931_001:0075a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想觀自在王如來復還本宮。",
        "from": "T19n0931_001:0075a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言曰。",
        "from": "T19n0931_001:0075a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)嚩日囉(二合二)鉢娜麼(二合三)穆(四)發遣本尊已。",
        "from": "T19n0931_001:0075a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意發願。",
        "from": "T19n0931_001:0075a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復以甲印護身。",
        "from": "T19n0931_001:0075a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以馬頭明王結界印。",
        "from": "T19n0931_001:0075a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左旋解界。",
        "from": "T19n0931_001:0075a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意經行往諸淨處。",
        "from": "T19n0931_001:0075a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讀大乘大般若經華嚴涅槃及楞伽等經。",
        "from": "T19n0931_001:0075a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行住坐臥常念本尊無令間斷。",
        "from": "T19n0931_001:0075a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛頂經觀自在王如來修行法",
        "from": "T19n0931_001:0075a11_0",
        "context_with_next_sentence": false
    }
]