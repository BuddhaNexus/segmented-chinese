[
    {
        "source_sentence": "大乘集菩薩學論卷第十六",
        "from": "T32n1636_016:0120a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫",
        "from": "T32n1636_016:0120a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "試鴻臚少卿宣梵大師賜紫",
        "from": "T32n1636_016:0120a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙門臣日稱等奉　詔譯",
        "from": "T32n1636_016:0120a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "治心品第十二之餘",
        "from": "T32n1636_016:0120a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次所說真實行及邪妄行。",
        "from": "T32n1636_016:0120a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂無智無明。",
        "from": "T32n1636_016:0120a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由是無明起三種行。",
        "from": "T32n1636_016:0120a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂福非福不動行等。",
        "from": "T32n1636_016:0120a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此說是為無明緣行。",
        "from": "T32n1636_016:0120a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此福行非福行不動行等隨所得識之所招集此說是為行緣識(今詳此處合有識緣名色一段經文梵本元闕)。",
        "from": "T32n1636_016:0120a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是名色及名色增長於六處門造作所起。",
        "from": "T32n1636_016:0120a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此說是為名色緣六處。",
        "from": "T32n1636_016:0120a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此六處六觸身轉。",
        "from": "T32n1636_016:0120a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此說是為六處緣觸若觸。",
        "from": "T32n1636_016:0120a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生時令受所起。",
        "from": "T32n1636_016:0120a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此說是為觸緣受。",
        "from": "T32n1636_016:0120a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若受美味深喜樂著。",
        "from": "T32n1636_016:0120a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此說是為受緣愛。",
        "from": "T32n1636_016:0120a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喜耽味故若處樂色。",
        "from": "T32n1636_016:0120a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色離散時數數追求而不棄捨。",
        "from": "T32n1636_016:0120a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此說是為愛緣取。",
        "from": "T32n1636_016:0120a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是追求令後有起身語意業。",
        "from": "T32n1636_016:0120a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此說是為取緣有。",
        "from": "T32n1636_016:0120a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若業遷謝令蘊生起。",
        "from": "T32n1636_016:0120a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此說是為有緣生。",
        "from": "T32n1636_016:0120a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又若生已變異令熟及壞滅時。",
        "from": "T32n1636_016:0120a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此說是為生緣老死。",
        "from": "T32n1636_016:0120a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至是中由識等自體為種子。",
        "from": "T32n1636_016:0120b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "業自體為良田。",
        "from": "T32n1636_016:0120b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無明愛等自體為煩惱。",
        "from": "T32n1636_016:0120b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以業煩惱等令識種生者。",
        "from": "T32n1636_016:0120b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂此業為識種田事。",
        "from": "T32n1636_016:0120b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛為識種沃潤。",
        "from": "T32n1636_016:0120b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無明為識種開發。",
        "from": "T32n1636_016:0120b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然業不念我為識種田事。",
        "from": "T32n1636_016:0120b04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛亦不念我為識種沃潤。",
        "from": "T32n1636_016:0120b05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無明不念我為識種開發。",
        "from": "T32n1636_016:0120b05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是識種子亦復不念我為眾緣所生。",
        "from": "T32n1636_016:0120b06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然識種子住於業地。",
        "from": "T32n1636_016:0120b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以愛沃潤無明滋長生名色芽。",
        "from": "T32n1636_016:0120b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此名色芽非自作非他作非二俱作。",
        "from": "T32n1636_016:0120b08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非自在天所化。",
        "from": "T32n1636_016:0120b09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不由時變。",
        "from": "T32n1636_016:0120b09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非一因生非無因生。",
        "from": "T32n1636_016:0120b09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無不繫於父母因緣和合耽染相續。",
        "from": "T32n1636_016:0120b10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是識種子於母腹中生名色芽。",
        "from": "T32n1636_016:0120b11_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然此法爾無有主宰。",
        "from": "T32n1636_016:0120b11_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無我無取等如虛空。",
        "from": "T32n1636_016:0120b12_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體相幻化因緣不闕。",
        "from": "T32n1636_016:0120b12_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由五種緣發生眼識。",
        "from": "T32n1636_016:0120b13_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為五。",
        "from": "T32n1636_016:0120b13_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂眼識生時。",
        "from": "T32n1636_016:0120b13_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藉眼及色空緣明緣作意緣等。",
        "from": "T32n1636_016:0120b14_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是中眼識眼為所依色為所緣。",
        "from": "T32n1636_016:0120b14_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明為照矚。",
        "from": "T32n1636_016:0120b15_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空作無礙。",
        "from": "T32n1636_016:0120b15_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同時作意為彼警發闕是緣者眼識不生。",
        "from": "T32n1636_016:0120b16_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂若不闕內眼根處。",
        "from": "T32n1636_016:0120b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是不闕色及空明作意緣等故。",
        "from": "T32n1636_016:0120b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼一切和合則能生眼識。",
        "from": "T32n1636_016:0120b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而眼不念我與眼色為作所依。",
        "from": "T32n1636_016:0120b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色亦不念我與眼識為作所緣。",
        "from": "T32n1636_016:0120b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空亦不念我與眼識為作無礙。",
        "from": "T32n1636_016:0120b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明亦不念我與眼識為作照矚。",
        "from": "T32n1636_016:0120b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作意不念我與眼識而為警發。",
        "from": "T32n1636_016:0120b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是眼識不念我從緣生。",
        "from": "T32n1636_016:0120b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然眼識生時。",
        "from": "T32n1636_016:0120b22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實藉眾緣和合而生。",
        "from": "T32n1636_016:0120b23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如其次第諸根生識隨應為說。",
        "from": "T32n1636_016:0120b23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然無有法從於此世得至他世。",
        "from": "T32n1636_016:0120b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但由業果因緣不闕之所建立。",
        "from": "T32n1636_016:0120b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如無薪火則不生。",
        "from": "T32n1636_016:0120b25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是業煩惱所生識種亦復如是。",
        "from": "T32n1636_016:0120b26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼彼生處相續和合生名色芽。",
        "from": "T32n1636_016:0120b26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是法爾無有主宰。",
        "from": "T32n1636_016:0120b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無我無取如虛空等。",
        "from": "T32n1636_016:0120b28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體相幻化因緣不闕。",
        "from": "T32n1636_016:0120b28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應知內緣生法有其五種。",
        "from": "T32n1636_016:0120b29_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為五。",
        "from": "T32n1636_016:0120b29_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂不常不斷無所至向因少果多相似相續。",
        "from": "T32n1636_016:0120b29_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何不常。",
        "from": "T32n1636_016:0120c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂若此邊蘊死彼邊蘊生。",
        "from": "T32n1636_016:0120c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非即死邊蘊是彼生邊蘊。",
        "from": "T32n1636_016:0120c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是曰不常。",
        "from": "T32n1636_016:0120c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又不滅於死邊蘊而起於生邊蘊。",
        "from": "T32n1636_016:0120c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦非不滅於死邊蘊然起於生邊蘊。",
        "from": "T32n1636_016:0120c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬若權衡低昂時等。",
        "from": "T32n1636_016:0120c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是曰不斷。",
        "from": "T32n1636_016:0120c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又於異類眾生身處非眾同分生邊蘊起。",
        "from": "T32n1636_016:0120c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是曰無所至向。",
        "from": "T32n1636_016:0120c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又今少作善惡業因。",
        "from": "T32n1636_016:0120c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於未來世多獲果報。",
        "from": "T32n1636_016:0120c07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是曰因少果多。",
        "from": "T32n1636_016:0120c08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又若此生作業則未來受報。",
        "from": "T32n1636_016:0120c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是曰相似相續。",
        "from": "T32n1636_016:0120c09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言。",
        "from": "T32n1636_016:0120c09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利子。",
        "from": "T32n1636_016:0120c09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此緣生者以如實正惠。",
        "from": "T32n1636_016:0120c09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常修無我無人眾生壽者。",
        "from": "T32n1636_016:0120c10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如不顛倒則不生不滅。",
        "from": "T32n1636_016:0120c10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無作無為無對無礙無怖無奪無勤無懈。",
        "from": "T32n1636_016:0120c11_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀彼自性無有堅實。",
        "from": "T32n1636_016:0120c12_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如病如癰如痛如害。",
        "from": "T32n1636_016:0120c12_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是苦空無常無我等性。",
        "from": "T32n1636_016:0120c13_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼復隨觀前際不流轉者。",
        "from": "T32n1636_016:0120c13_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂我於過去世曾有耶。",
        "from": "T32n1636_016:0120c14_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誰謂我於過去世曾有耶。",
        "from": "T32n1636_016:0120c14_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我於過去世為無耶。",
        "from": "T32n1636_016:0120c15_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誰謂我於過去世為無耶。",
        "from": "T32n1636_016:0120c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復於後際不流轉者。",
        "from": "T32n1636_016:0120c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當未來世為有耶。",
        "from": "T32n1636_016:0120c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誰謂我當未來世為有耶。",
        "from": "T32n1636_016:0120c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當未來世為無耶。",
        "from": "T32n1636_016:0120c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誰謂我當未來世為無耶。",
        "from": "T32n1636_016:0120c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復於現在世不流轉者。",
        "from": "T32n1636_016:0120c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我於今有耶。",
        "from": "T32n1636_016:0120c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誰謂我今有耶。",
        "from": "T32n1636_016:0120c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為我今有耶。",
        "from": "T32n1636_016:0120c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又十地經說。",
        "from": "T32n1636_016:0120c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是中無明愛取。",
        "from": "T32n1636_016:0120c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為煩惱。",
        "from": "T32n1636_016:0120c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流轉不斷行有。",
        "from": "T32n1636_016:0120c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為業。",
        "from": "T32n1636_016:0120c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流轉不斷煩惱餘支。",
        "from": "T32n1636_016:0120c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為苦流轉不斷。",
        "from": "T32n1636_016:0120c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說無明緣行。",
        "from": "T32n1636_016:0120c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是曰前際觀待。",
        "from": "T32n1636_016:0120c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識及受等。",
        "from": "T32n1636_016:0120c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是曰現在觀待。",
        "from": "T32n1636_016:0120c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛及有等。",
        "from": "T32n1636_016:0120c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是曰後際觀待。",
        "from": "T32n1636_016:0120c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此上所說皆名流轉。",
        "from": "T32n1636_016:0120c25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至如是。",
        "from": "T32n1636_016:0120c25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由繫屬故則為流轉。",
        "from": "T32n1636_016:0120c25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若離繫屬則無流轉。",
        "from": "T32n1636_016:0120c26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由和合故則為流轉。",
        "from": "T32n1636_016:0120c26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若離和合則無流轉。",
        "from": "T32n1636_016:0120c27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由是了知諸有為中多種過失。",
        "from": "T32n1636_016:0120c27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故我今當斷繫屬及彼和合。",
        "from": "T32n1636_016:0120c28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然為化度一切眾生。",
        "from": "T32n1636_016:0120c29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畢竟不斷諸有為行。",
        "from": "T32n1636_016:0120c29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念處品第十三",
        "from": "T32n1636_016:0121a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1636_016:0121a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼治心業略明緣生對治於癡。",
        "from": "T32n1636_016:0121a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當入解不淨身念處今當說。",
        "from": "T32n1636_016:0121a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如法集經云。",
        "from": "T32n1636_016:0121a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次善男子。",
        "from": "T32n1636_016:0121a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩如是住身念處者。",
        "from": "T32n1636_016:0121a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂我此身從足足指跟踝腨脛膝髀臗骨腰脊腹肋胸脇手指肘腕肩臂頸項頭頰髑髏等狀。",
        "from": "T32n1636_016:0121a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾分積聚是即業有。",
        "from": "T32n1636_016:0121a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由斯造作種種煩惱及隨煩惱。",
        "from": "T32n1636_016:0121a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百千種類邪正分別。",
        "from": "T32n1636_016:0121a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身唯有髮毛爪齒血肉皮骨肝膽脾腎心肺腸胃生藏熟藏肪膏腦膜眵淚涎涕便利不淨多物積聚。",
        "from": "T32n1636_016:0121a09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以何為身。",
        "from": "T32n1636_016:0121a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由是觀察身如虛空。",
        "from": "T32n1636_016:0121a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則見一切諸法皆空。",
        "from": "T32n1636_016:0121a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為空念處。",
        "from": "T32n1636_016:0121a12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了知是身由二種念。",
        "from": "T32n1636_016:0121a13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂流散及不流散。",
        "from": "T32n1636_016:0121a13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身來者。",
        "from": "T32n1636_016:0121a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不從前際。",
        "from": "T32n1636_016:0121a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "去者不至後際。",
        "from": "T32n1636_016:0121a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不住二際中間。",
        "from": "T32n1636_016:0121a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯從顛倒和合造作。",
        "from": "T32n1636_016:0121a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由斯領受。",
        "from": "T32n1636_016:0121a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是中居處而為根本。",
        "from": "T32n1636_016:0121a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然實無有主宰。",
        "from": "T32n1636_016:0121a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦無攝屬。",
        "from": "T32n1636_016:0121a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但從客塵先所作事。",
        "from": "T32n1636_016:0121a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身體形貌受用依止為所恃處。",
        "from": "T32n1636_016:0121a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然此身者。",
        "from": "T32n1636_016:0121a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯有父母赤白和合不淨臭穢攬為自體。",
        "from": "T32n1636_016:0121a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三毒憂惱常為賊害。",
        "from": "T32n1636_016:0121a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是散壞法。",
        "from": "T32n1636_016:0121a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種百千病惱窟宅。",
        "from": "T32n1636_016:0121a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如寶髻經說。",
        "from": "T32n1636_016:0121a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身無常而不久住死為後際。",
        "from": "T32n1636_016:0121a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了知此已不應於身而生邪命。",
        "from": "T32n1636_016:0121a21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當以身修三種淨施。",
        "from": "T32n1636_016:0121a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為三謂身淨施。",
        "from": "T32n1636_016:0121a22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽命淨施。",
        "from": "T32n1636_016:0121a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受用淨施。",
        "from": "T32n1636_016:0121a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身無常。",
        "from": "T32n1636_016:0121a23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當往詣一切眾生所親近承事。",
        "from": "T32n1636_016:0121a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲何所作。",
        "from": "T32n1636_016:0121a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或如僕使或如弟子。",
        "from": "T32n1636_016:0121a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離諸諂誑身過失等。",
        "from": "T32n1636_016:0121a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身無常。",
        "from": "T32n1636_016:0121a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯出入息為壽命因。",
        "from": "T32n1636_016:0121a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寧造罪惡。",
        "from": "T32n1636_016:0121a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身無常。",
        "from": "T32n1636_016:0121a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應於愛樂受用不生耽著。",
        "from": "T32n1636_016:0121a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切當捨。",
        "from": "T32n1636_016:0121a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次善男子。",
        "from": "T32n1636_016:0121a27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩以身觀身念處。",
        "from": "T32n1636_016:0121a28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應觀自身及一切眾生身等同佛身。",
        "from": "T32n1636_016:0121a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "威神加持得離纏縛。",
        "from": "T32n1636_016:0121a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然觀自身及一切眾生身與如來身無漏法性彼相無異故。",
        "from": "T32n1636_016:0121b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應如是知。",
        "from": "T32n1636_016:0121b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又無畏授所問經說。",
        "from": "T32n1636_016:0121b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂我此身非先所集。",
        "from": "T32n1636_016:0121b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漸次散壞。",
        "from": "T32n1636_016:0121b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬若微塵。",
        "from": "T32n1636_016:0121b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九竅流溢如瘡疱門。",
        "from": "T32n1636_016:0121b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又諸毛孔有嚩羅弭迦。",
        "from": "T32n1636_016:0121b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如毒蛇依止而住。",
        "from": "T32n1636_016:0121b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身如聚沫體性怯弱。",
        "from": "T32n1636_016:0121b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如水上泡暫生即滅。",
        "from": "T32n1636_016:0121b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦如陽焰本無有體。",
        "from": "T32n1636_016:0121b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身如芭蕉中無有實。",
        "from": "T32n1636_016:0121b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身如幻事為他教勅。",
        "from": "T32n1636_016:0121b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身如惡友多諸諍訟。",
        "from": "T32n1636_016:0121b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身如猴性唯輕躁。",
        "from": "T32n1636_016:0121b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身如讎常伺其短。",
        "from": "T32n1636_016:0121b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身如賊機巧貪取。",
        "from": "T32n1636_016:0121b09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身如囚常被縛戮。",
        "from": "T32n1636_016:0121b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身如冤不可愛樂。",
        "from": "T32n1636_016:0121b10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦如魁膾能斷其命。",
        "from": "T32n1636_016:0121b10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復此身如空聚落中無有我。",
        "from": "T32n1636_016:0121b11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如陶家輪無暫停止。",
        "from": "T32n1636_016:0121b11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如滓穢瓶不淨充滿。",
        "from": "T32n1636_016:0121b12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身如癰疽挃唯痛楚。",
        "from": "T32n1636_016:0121b12_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身如朽宅不久遷壞。",
        "from": "T32n1636_016:0121b13_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身如漏舡無堪濟渡。",
        "from": "T32n1636_016:0121b13_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身如坏器漸當沮壞。",
        "from": "T32n1636_016:0121b14_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復此身如河岸樹必為駃流之所漂動。",
        "from": "T32n1636_016:0121b14_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身如旅泊多諸艱苦。",
        "from": "T32n1636_016:0121b15_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身如邸舍妄執主宰。",
        "from": "T32n1636_016:0121b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如伺盜者常生暴惡。",
        "from": "T32n1636_016:0121b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至是身如癡小兒要當守護。",
        "from": "T32n1636_016:0121b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼經復說。",
        "from": "T32n1636_016:0121b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是種種不淨之身。",
        "from": "T32n1636_016:0121b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由我慢愚癡妄為光潔。",
        "from": "T32n1636_016:0121b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智者所觀猶如穢瓶。",
        "from": "T32n1636_016:0121b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼鼻口等眵涕流出。",
        "from": "T32n1636_016:0121b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何於此多生貪慢。",
        "from": "T32n1636_016:0121b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又彼偈云。",
        "from": "T32n1636_016:0121b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如愚癡童子　　洗炭欲令白",
        "from": "T32n1636_016:0121b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　設盡此生中　　求白不可得",
        "from": "T32n1636_016:0121b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　亦如無智人　　洗身欲令潔",
        "from": "T32n1636_016:0121b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　屢加於澡沐　　至死不能淨",
        "from": "T32n1636_016:0121b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　菩薩觀於身　　九竅瘡疱門",
        "from": "T32n1636_016:0121b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　八十千戶虫　　依身而所住",
        "from": "T32n1636_016:0121b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　菩薩觀於身　　如刻木眾像",
        "from": "T32n1636_016:0121b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　筋骨假連持　　應知無主宰",
        "from": "T32n1636_016:0121b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　菩薩觀於身　　或受他飲食",
        "from": "T32n1636_016:0121b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　應知同狐狼　　來食自身肉",
        "from": "T32n1636_016:0121c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1636_016:0121c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由飲食資助成廣大行。",
        "from": "T32n1636_016:0121c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是處處應當了知。",
        "from": "T32n1636_016:0121c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受念處者。",
        "from": "T32n1636_016:0121c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如寶髻經說。",
        "from": "T32n1636_016:0121c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子。",
        "from": "T32n1636_016:0121c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩以受觀受念處了知眾生。",
        "from": "T32n1636_016:0121c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依止大悲得如是樂。",
        "from": "T32n1636_016:0121c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一切眾生無有解脫。",
        "from": "T32n1636_016:0121c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當修習以受觀受念處。",
        "from": "T32n1636_016:0121c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當被大悲甲攝受調伏一切眾生。",
        "from": "T32n1636_016:0121c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉以彼樂滅除眾苦。",
        "from": "T32n1636_016:0121c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我以大悲令貪行眾生了知樂受離於貪染。",
        "from": "T32n1636_016:0121c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我以大悲令瞋行眾生了知苦受離諸過失。",
        "from": "T32n1636_016:0121c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我以大悲令癡行眾生了知不苦不樂受捨俱生癡。",
        "from": "T32n1636_016:0121c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又彼樂受不壞苦受不減不苦不樂受離於對治則無無明。",
        "from": "T32n1636_016:0121c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無無明彼何名受。",
        "from": "T32n1636_016:0121c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何了知一切無常及無我等。",
        "from": "T32n1636_016:0121c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然此樂受即無常性。",
        "from": "T32n1636_016:0121c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦受即逼迫性。",
        "from": "T32n1636_016:0121c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不苦不樂受即寂靜性。",
        "from": "T32n1636_016:0121c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此了知若樂若苦無常無我等。",
        "from": "T32n1636_016:0121c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又無盡意經云。",
        "from": "T32n1636_016:0121c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若受苦時當念一切惡道眾生起大悲心。",
        "from": "T32n1636_016:0121c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝除於瞋離於顛倒分別受等。",
        "from": "T32n1636_016:0121c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法集經偈云。",
        "from": "T32n1636_016:0121c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　說受為領納　　領納復為誰",
        "from": "T32n1636_016:0121c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　受者離於受　　差別不可得",
        "from": "T32n1636_016:0121c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　智者觀於受　　住如是念處",
        "from": "T32n1636_016:0121c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　彼相如菩提　　光明遍寂靜",
        "from": "T32n1636_016:0121c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大乘集菩薩學論卷第十六",
        "from": "T32n1636_016:0121c22_0",
        "context_with_next_sentence": false
    }
]