[
    {
        "source_sentence": "大乘稻芉經隨聽疏決",
        "from": "T85n2783_001:0556b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　稽首恭敬正遍知　　及以妙法僧伽眾　所有稻芉未決義　　今當次第略解釋十六論中第一言因中有果論者。",
        "from": "T85n2783_001:0556b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂如有一若沙門若婆羅門。",
        "from": "T85n2783_001:0556b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起如是見立論。",
        "from": "T85n2783_001:0556c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常常時恒恒時。",
        "from": "T85n2783_001:0556c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸因中具有果性。",
        "from": "T85n2783_001:0556c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂雨眾外道作如是計。",
        "from": "T85n2783_001:0556c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何因緣故。",
        "from": "T85n2783_001:0556c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起如是計。",
        "from": "T85n2783_001:0556c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立如是論。",
        "from": "T85n2783_001:0556c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯示因中具有果性。",
        "from": "T85n2783_001:0556c03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由教及理故。",
        "from": "T85n2783_001:0556c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教者謂彼先師所造教藏。",
        "from": "T85n2783_001:0556c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨門傳授。",
        "from": "T85n2783_001:0556c05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳至于今。",
        "from": "T85n2783_001:0556c05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯示因中先有果性。",
        "from": "T85n2783_001:0556c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "理者謂即彼沙門若婆羅門為性。",
        "from": "T85n2783_001:0556c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尋□□□□察住即思地住自辯地住異生地住。",
        "from": "T85n2783_001:0556c06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨思惟觀察行地。",
        "from": "T85n2783_001:0556c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼作是思。",
        "from": "T85n2783_001:0556c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若從彼性此住得生。",
        "from": "T85n2783_001:0556c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切世間共知共立。",
        "from": "T85n2783_001:0556c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼為此因非餘。",
        "from": "T85n2783_001:0556c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又求果者惟所此因非餘。",
        "from": "T85n2783_001:0556c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又即於彼加功營構諸所求事非餘。",
        "from": "T85n2783_001:0556c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又若彼果即從彼生不從餘生。",
        "from": "T85n2783_001:0556c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故彼果因中已有。",
        "from": "T85n2783_001:0556c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不爾者。",
        "from": "T85n2783_001:0556c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應立一切是一切因。",
        "from": "T85n2783_001:0556c12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為求一果應所一切應於一切。",
        "from": "T85n2783_001:0556c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加功營構。",
        "from": "T85n2783_001:0556c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應從一切一切果生。",
        "from": "T85n2783_001:0556c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是由施設故。",
        "from": "T85n2783_001:0556c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求所故。",
        "from": "T85n2783_001:0556c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所作決定故。",
        "from": "T85n2783_001:0556c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生故。",
        "from": "T85n2783_001:0556c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼見因中常有果性。",
        "from": "T85n2783_001:0556c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應審問彼。",
        "from": "T85n2783_001:0556c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝何所欲。",
        "from": "T85n2783_001:0556c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何者因相。",
        "from": "T85n2783_001:0556c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何者果相。",
        "from": "T85n2783_001:0556c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因果兩相為異不異。",
        "from": "T85n2783_001:0556c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無異相。",
        "from": "T85n2783_001:0556c17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便無因果二種。",
        "from": "T85n2783_001:0556c17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "決定因果二種無差別故。",
        "from": "T85n2783_001:0556c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因中有果不應道理。",
        "from": "T85n2783_001:0556c18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有異相。",
        "from": "T85n2783_001:0556c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝意云何。",
        "from": "T85n2783_001:0556c19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因中果相為未生相。",
        "from": "T85n2783_001:0556c19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為已生相。",
        "from": "T85n2783_001:0556c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若未生相。",
        "from": "T85n2783_001:0556c20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便於因中果猶未生而說。",
        "from": "T85n2783_001:0556c20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是有不應道理。",
        "from": "T85n2783_001:0556c21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若已生相即果。",
        "from": "T85n2783_001:0556c21_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已生復從因生。",
        "from": "T85n2783_001:0556c21_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應道理。",
        "from": "T85n2783_001:0556c22_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故因中非先有果。",
        "from": "T85n2783_001:0556c22_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然要有因待緣果生。",
        "from": "T85n2783_001:0556c22_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又有相法。",
        "from": "T85n2783_001:0556c23_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於有相法中。",
        "from": "T85n2783_001:0556c23_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由五種相故。",
        "from": "T85n2783_001:0556c23_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方可了知。",
        "from": "T85n2783_001:0556c24_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一處所可得。",
        "from": "T85n2783_001:0556c24_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如瓶中水。",
        "from": "T85n2783_001:0556c24_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二於所依可得。",
        "from": "T85n2783_001:0556c25_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如眼中眼識。",
        "from": "T85n2783_001:0556c25_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三即自相可得。",
        "from": "T85n2783_001:0556c25_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如因自體不由比度。",
        "from": "T85n2783_001:0556c26_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四即由自作業可得。",
        "from": "T85n2783_001:0556c26_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五由因變異故成變異。",
        "from": "T85n2783_001:0556c27_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或由緣變異故果成變異。",
        "from": "T85n2783_001:0556c27_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故彼說常常時恒恒時因中有果。",
        "from": "T85n2783_001:0556c28_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應道理。",
        "from": "T85n2783_001:0556c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此因緣故。",
        "from": "T85n2783_001:0556c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所立論非如理說。",
        "from": "T85n2783_001:0556c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是不異相故。",
        "from": "T85n2783_001:0556c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "異相故。",
        "from": "T85n2783_001:0557a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未生相故。",
        "from": "T85n2783_001:0557a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已生相故。",
        "from": "T85n2783_001:0557a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應道理　二言從緣顯了。",
        "from": "T85n2783_001:0557a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論者。",
        "from": "T85n2783_001:0557a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即如有一若沙門若婆羅門。",
        "from": "T85n2783_001:0557a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起如是見。",
        "from": "T85n2783_001:0557a03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立如是論。",
        "from": "T85n2783_001:0557a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸法本性是有。",
        "from": "T85n2783_001:0557a03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從眾緣顯。",
        "from": "T85n2783_001:0557a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不從緣生。",
        "from": "T85n2783_001:0557a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂即因中有果論者。",
        "from": "T85n2783_001:0557a04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及聲相論者。",
        "from": "T85n2783_001:0557a05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是計。",
        "from": "T85n2783_001:0557a05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何因緣故。",
        "from": "T85n2783_001:0557a05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因中有果。",
        "from": "T85n2783_001:0557a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論者。",
        "from": "T85n2783_001:0557a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見諸因中。",
        "from": "T85n2783_001:0557a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先有果性。",
        "from": "T85n2783_001:0557a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從緣顯耶。",
        "from": "T85n2783_001:0557a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由教及理故。",
        "from": "T85n2783_001:0557a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教如前說。",
        "from": "T85n2783_001:0557a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "理者謂如有一為性。",
        "from": "T85n2783_001:0557a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即思為性觀察廣說如前。",
        "from": "T85n2783_001:0557a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼如是思。",
        "from": "T85n2783_001:0557a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果先是有。",
        "from": "T85n2783_001:0557a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復從因生。",
        "from": "T85n2783_001:0557a09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應道理。",
        "from": "T85n2783_001:0557a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然非不用功為成於果。",
        "from": "T85n2783_001:0557a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼復何緣而作功用。",
        "from": "T85n2783_001:0557a10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈非唯為顯了果耶彼作如是妄分別已。",
        "from": "T85n2783_001:0557a10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立顯了論。",
        "from": "T85n2783_001:0557a11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應審問彼。",
        "from": "T85n2783_001:0557a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝何所欲為無障緣而為障礙為有障緣耶。",
        "from": "T85n2783_001:0557a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無障緣者。",
        "from": "T85n2783_001:0557a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無障礙緣而為障礙。",
        "from": "T85n2783_001:0557a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應道理。",
        "from": "T85n2783_001:0557a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有障緣者。",
        "from": "T85n2783_001:0557a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屬果之因。",
        "from": "T85n2783_001:0557a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何故不障。",
        "from": "T85n2783_001:0557a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同是有故。",
        "from": "T85n2783_001:0557a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應道理。",
        "from": "T85n2783_001:0557a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如黑闇障盆中水。",
        "from": "T85n2783_001:0557a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能障盆。",
        "from": "T85n2783_001:0557a15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若言障因緣。",
        "from": "T85n2783_001:0557a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能障者。",
        "from": "T85n2783_001:0557a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦應顯因。",
        "from": "T85n2783_001:0557a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱被障故。",
        "from": "T85n2783_001:0557a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而言但顯因中光有果生不顯因者。",
        "from": "T85n2783_001:0557a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應道理。",
        "from": "T85n2783_001:0557a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復應問彼。",
        "from": "T85n2783_001:0557a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為有性是障緣為異耶。",
        "from": "T85n2783_001:0557a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有性是障緣者。",
        "from": "T85n2783_001:0557a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是即有性應常不顯了。",
        "from": "T85n2783_001:0557a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應道理。",
        "from": "T85n2783_001:0557a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因亦是有。",
        "from": "T85n2783_001:0557a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何不為障。",
        "from": "T85n2783_001:0557a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若言果性。",
        "from": "T85n2783_001:0557a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是障緣者。",
        "from": "T85n2783_001:0557a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是則一法亦因亦果。",
        "from": "T85n2783_001:0557a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如芽是種子。",
        "from": "T85n2783_001:0557a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果是莖等。",
        "from": "T85n2783_001:0557a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因是即一法亦顯不顯。",
        "from": "T85n2783_001:0557a22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應道理。",
        "from": "T85n2783_001:0557a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又今問汝。",
        "from": "T85n2783_001:0557a22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨汝意答。",
        "from": "T85n2783_001:0557a23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本法與顯為異不異。",
        "from": "T85n2783_001:0557a23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不異者。",
        "from": "T85n2783_001:0557a23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法應常顯。",
        "from": "T85n2783_001:0557a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一已復顯。",
        "from": "T85n2783_001:0557a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應道理。",
        "from": "T85n2783_001:0557a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若言異者。",
        "from": "T85n2783_001:0557a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼顯為無因耶。",
        "from": "T85n2783_001:0557a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為有因耶。",
        "from": "T85n2783_001:0557a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若言無因。",
        "from": "T85n2783_001:0557a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無因而顯。",
        "from": "T85n2783_001:0557a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應道理。",
        "from": "T85n2783_001:0557a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有因者。",
        "from": "T85n2783_001:0557a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果性可顯。",
        "from": "T85n2783_001:0557a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非是因性。",
        "from": "T85n2783_001:0557a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以不顯因能顯於果。",
        "from": "T85n2783_001:0557a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應道理。",
        "from": "T85n2783_001:0557a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是無障緣故。",
        "from": "T85n2783_001:0557a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有障緣故。",
        "from": "T85n2783_001:0557a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有相故。",
        "from": "T85n2783_001:0557a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果相故。",
        "from": "T85n2783_001:0557a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯不異故。",
        "from": "T85n2783_001:0557a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯異故。",
        "from": "T85n2783_001:0557a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應道理　三言去來實有論者。",
        "from": "T85n2783_001:0557a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂如有一若沙門若婆羅門。",
        "from": "T85n2783_001:0557a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若在此法者。",
        "from": "T85n2783_001:0557b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由不正思惟故。",
        "from": "T85n2783_001:0557b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起如是見立如是論。",
        "from": "T85n2783_001:0557b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有過去有未來其相成就。",
        "from": "T85n2783_001:0557b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如見在實有非假。",
        "from": "T85n2783_001:0557b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何因緣故起如見立如是論。",
        "from": "T85n2783_001:0557b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由教及理。",
        "from": "T85n2783_001:0557b04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教如前說。",
        "from": "T85n2783_001:0557b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又在此法者。",
        "from": "T85n2783_001:0557b04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於如來經。",
        "from": "T85n2783_001:0557b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不如理分別故。",
        "from": "T85n2783_001:0557b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂如經言。",
        "from": "T85n2783_001:0557b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切有者。",
        "from": "T85n2783_001:0557b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即十二處。",
        "from": "T85n2783_001:0557b06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此二十處實相是有。",
        "from": "T85n2783_001:0557b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又薄伽梵說。",
        "from": "T85n2783_001:0557b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有過去業。",
        "from": "T85n2783_001:0557b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又說。",
        "from": "T85n2783_001:0557b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有過去色。",
        "from": "T85n2783_001:0557b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有未來色。",
        "from": "T85n2783_001:0557b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣說乃至識亦如是。",
        "from": "T85n2783_001:0557b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "理者謂如有一為性尋思為性觀察。",
        "from": "T85n2783_001:0557b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣說如前。",
        "from": "T85n2783_001:0557b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼如是思。",
        "from": "T85n2783_001:0557b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若法自相安住此法。",
        "from": "T85n2783_001:0557b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真實是有。",
        "from": "T85n2783_001:0557b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此若未來無者。",
        "from": "T85n2783_001:0557b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時應未受相。",
        "from": "T85n2783_001:0557b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此若過去無者。",
        "from": "T85n2783_001:0557b11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時應失自相。",
        "from": "T85n2783_001:0557b11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若如是者。",
        "from": "T85n2783_001:0557b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法法自相應不成就。",
        "from": "T85n2783_001:0557b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由是道理亦非真實故。",
        "from": "T85n2783_001:0557b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應道理。",
        "from": "T85n2783_001:0557b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由是思惟。",
        "from": "T85n2783_001:0557b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起如是見。",
        "from": "T85n2783_001:0557b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立如是論。",
        "from": "T85n2783_001:0557b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過去未來性相實有。",
        "from": "T85n2783_001:0557b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應審問彼。",
        "from": "T85n2783_001:0557b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝何所欲。",
        "from": "T85n2783_001:0557b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "去來二相。",
        "from": "T85n2783_001:0557b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與現在相。",
        "from": "T85n2783_001:0557b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為一為異。",
        "from": "T85n2783_001:0557b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若言相一立三世相。",
        "from": "T85n2783_001:0557b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應道理。",
        "from": "T85n2783_001:0557b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若相異者。",
        "from": "T85n2783_001:0557b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性相實有。",
        "from": "T85n2783_001:0557b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應道理。",
        "from": "T85n2783_001:0557b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又汝應說。",
        "from": "T85n2783_001:0557b17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自意所欲。",
        "from": "T85n2783_001:0557b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨三世法。",
        "from": "T85n2783_001:0557b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為是常耶。",
        "from": "T85n2783_001:0557b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為無常耶。",
        "from": "T85n2783_001:0557b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若常相者。",
        "from": "T85n2783_001:0557b18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "墮在三世。",
        "from": "T85n2783_001:0557b18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應道理。",
        "from": "T85n2783_001:0557b19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無常相。",
        "from": "T85n2783_001:0557b19_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於三世中。",
        "from": "T85n2783_001:0557b19_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒是實有。",
        "from": "T85n2783_001:0557b19_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應道理。",
        "from": "T85n2783_001:0557b20_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又今問汝。",
        "from": "T85n2783_001:0557b20_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨汝意答。",
        "from": "T85n2783_001:0557b20_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為計未來法至現在世耶。",
        "from": "T85n2783_001:0557b20_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為彼死已於此生耶。",
        "from": "T85n2783_001:0557b21_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為即住未來。",
        "from": "T85n2783_001:0557b21_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為緣生現在耶。",
        "from": "T85n2783_001:0557b22_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為本無業。",
        "from": "T85n2783_001:0557b22_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今有業耶。",
        "from": "T85n2783_001:0557b22_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為本相不圓滿。",
        "from": "T85n2783_001:0557b23_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今相圓滿耶。",
        "from": "T85n2783_001:0557b23_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為本異相。",
        "from": "T85n2783_001:0557b23_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今非異相耶。",
        "from": "T85n2783_001:0557b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為於未來有現在分耶。",
        "from": "T85n2783_001:0557b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若即未法來來至現在者。",
        "from": "T85n2783_001:0557b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此便有方所。",
        "from": "T85n2783_001:0557b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後與現在應無差別。",
        "from": "T85n2783_001:0557b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復應是常。",
        "from": "T85n2783_001:0557b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應道理。",
        "from": "T85n2783_001:0557b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若言未來死已現在生者。",
        "from": "T85n2783_001:0557b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是即未來不生。",
        "from": "T85n2783_001:0557b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於今現在世法本",
        "from": "T85n2783_001:0557b27_9",
        "context_with_next_sentence": false
    }
]