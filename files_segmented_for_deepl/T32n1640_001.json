[
    {
        "source_sentence": "提婆菩薩釋楞伽經中外道小乘",
        "from": "T32n1640_001:0156c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "涅槃論",
        "from": "T32n1640_001:0156c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提波菩薩造",
        "from": "T32n1640_001:0156c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後魏北印度三藏菩提流支譯",
        "from": "T32n1640_001:0156c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1640_001:0156c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何者外道所謂涅槃。",
        "from": "T32n1640_001:0156c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1640_001:0156c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外道所說涅槃有二十種。",
        "from": "T32n1640_001:0156c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸外道等虛妄分別。",
        "from": "T32n1640_001:0156c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等因能生六道。",
        "from": "T32n1640_001:0156c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來為遮是等邪見故說涅槃因果正義。",
        "from": "T32n1640_001:0156c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等二十。",
        "from": "T32n1640_001:0156c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者小乘外道論師。",
        "from": "T32n1640_001:0156c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者方論師。",
        "from": "T32n1640_001:0156c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者風論師。",
        "from": "T32n1640_001:0156c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者違陀論師。",
        "from": "T32n1640_001:0156c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者伊賒那論師。",
        "from": "T32n1640_001:0156c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六者裸形外道論師。",
        "from": "T32n1640_001:0156c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七者毘世師論師。",
        "from": "T32n1640_001:0156c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八者苦行論師。",
        "from": "T32n1640_001:0156c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九者女人眷屬論師。",
        "from": "T32n1640_001:0156c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十者行苦行論師。",
        "from": "T32n1640_001:0156c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十一者淨眼論師。",
        "from": "T32n1640_001:0156c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二者摩陀羅論師。",
        "from": "T32n1640_001:0156c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十三者尼犍子論師。",
        "from": "T32n1640_001:0156c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十四者僧佉論師。",
        "from": "T32n1640_001:0156c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十五者摩醯首羅論師。",
        "from": "T32n1640_001:0156c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十六者無因論師。",
        "from": "T32n1640_001:0156c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十七者時論師。",
        "from": "T32n1640_001:0156c27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十八者服水論師。",
        "from": "T32n1640_001:0156c27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十九者口力論師。",
        "from": "T32n1640_001:0156c28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十者本生安荼論師。",
        "from": "T32n1640_001:0156c28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1640_001:0157a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何者外道說諸受陰盡如燈火滅種壞風止名涅槃。",
        "from": "T32n1640_001:0157a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1640_001:0157a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一小乘外道論師說。",
        "from": "T32n1640_001:0157a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1640_001:0157a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等外道說方名涅槃。",
        "from": "T32n1640_001:0157a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1640_001:0157a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二外道方論師說。",
        "from": "T32n1640_001:0157a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最初生諸方。",
        "from": "T32n1640_001:0157a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從諸方生世間人。",
        "from": "T32n1640_001:0157a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從人生天地。",
        "from": "T32n1640_001:0157a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天地滅沒還入彼處名為涅槃。",
        "from": "T32n1640_001:0157a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故方論師說方是常名涅槃因。",
        "from": "T32n1640_001:0157a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1640_001:0157a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等外道說風為涅槃因。",
        "from": "T32n1640_001:0157a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1640_001:0157a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三外道風仙論師說。",
        "from": "T32n1640_001:0157a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風能生長命物能殺命物。",
        "from": "T32n1640_001:0157a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風造萬物能壞萬物。",
        "from": "T32n1640_001:0157a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名風為涅槃。",
        "from": "T32n1640_001:0157a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故風仙論師說風為常是涅槃因。",
        "from": "T32n1640_001:0157a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1640_001:0157a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等外道說梵天是涅槃因。",
        "from": "T32n1640_001:0157a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1640_001:0157a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四外道圍陀論師說。",
        "from": "T32n1640_001:0157a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從那羅延天臍中生大蓮華。",
        "from": "T32n1640_001:0157a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從蓮華生梵天祖公。",
        "from": "T32n1640_001:0157a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼梵天作一切命。",
        "from": "T32n1640_001:0157a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無命物從梵天口中生。",
        "from": "T32n1640_001:0157a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婆羅門兩臂中生。",
        "from": "T32n1640_001:0157a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剎利兩髀中生。",
        "from": "T32n1640_001:0157a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘舍從兩脚跟生。",
        "from": "T32n1640_001:0157a15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "首陀一切大地。",
        "from": "T32n1640_001:0157a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是修福德戒場生一切華草。",
        "from": "T32n1640_001:0157a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為供養。",
        "from": "T32n1640_001:0157a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化作山野禽獸人中猪羊驢馬等。",
        "from": "T32n1640_001:0157a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於界場中殺害供養梵天。",
        "from": "T32n1640_001:0157a17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得生彼處名涅槃。",
        "from": "T32n1640_001:0157a18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故違陀論師說梵天名常是涅槃因。",
        "from": "T32n1640_001:0157a18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1640_001:0157a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等外道說不見分別見常無常是涅槃。",
        "from": "T32n1640_001:0157a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1640_001:0157a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五外道伊賒那論師眷屬作如是說。",
        "from": "T32n1640_001:0157a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伊賒那論師尊者形相不可見遍一切處。",
        "from": "T32n1640_001:0157a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以無形相而能生諸有命無命一切萬物。",
        "from": "T32n1640_001:0157a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為涅槃。",
        "from": "T32n1640_001:0157a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故伊賒那論師眷屬作如是說。",
        "from": "T32n1640_001:0157a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伊賒那是常名涅槃因。",
        "from": "T32n1640_001:0157a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1640_001:0157a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等外道。",
        "from": "T32n1640_001:0157a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分別見種種異相名涅槃。",
        "from": "T32n1640_001:0157a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1640_001:0157a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六裸形外道論師說。",
        "from": "T32n1640_001:0157a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1640_001:0157a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等外道說見一切法自相同相名涅槃。",
        "from": "T32n1640_001:0157a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1640_001:0157a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第七外道毘世師論師作如是說。",
        "from": "T32n1640_001:0157a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂地水火風虛空微塵物功德業勝等十種法常故。",
        "from": "T32n1640_001:0157a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和合而生一切世間知無知物。",
        "from": "T32n1640_001:0157b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從二微塵次第生一切法。",
        "from": "T32n1640_001:0157b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無彼者無和合者。",
        "from": "T32n1640_001:0157b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無和合者即是離散。",
        "from": "T32n1640_001:0157b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散者即是涅槃。",
        "from": "T32n1640_001:0157b04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故毘世師論師說。",
        "from": "T32n1640_001:0157b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微塵是常能生一切物。",
        "from": "T32n1640_001:0157b05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是涅槃因。",
        "from": "T32n1640_001:0157b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1640_001:0157b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等外道說身盡福德盡名為涅槃。",
        "from": "T32n1640_001:0157b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1640_001:0157b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八苦行論師說。",
        "from": "T32n1640_001:0157b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1640_001:0157b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等外道說自性人命轉變名涅槃。",
        "from": "T32n1640_001:0157b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1640_001:0157b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第九外道女人眷屬論師說。",
        "from": "T32n1640_001:0157b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩醯首羅作八女人。",
        "from": "T32n1640_001:0157b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名阿提[仁-二+致]。",
        "from": "T32n1640_001:0157b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二名提[仁-二+致]。",
        "from": "T32n1640_001:0157b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三名蘇羅婆。",
        "from": "T32n1640_001:0157b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四名毘那多。",
        "from": "T32n1640_001:0157b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五名迦毘羅。",
        "from": "T32n1640_001:0157b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六名摩[少/兔]。",
        "from": "T32n1640_001:0157b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七名伊羅。",
        "from": "T32n1640_001:0157b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八名歌頭。",
        "from": "T32n1640_001:0157b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿提[仁-二+致]生諸天。",
        "from": "T32n1640_001:0157b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提[仁-二+致]作阿脩羅。",
        "from": "T32n1640_001:0157b12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘇羅婆生諸龍。",
        "from": "T32n1640_001:0157b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘那多生諸鳥。",
        "from": "T32n1640_001:0157b13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦毘羅生四足。",
        "from": "T32n1640_001:0157b13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩[少/兔]生人。",
        "from": "T32n1640_001:0157b14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伊羅生一切穀子。",
        "from": "T32n1640_001:0157b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歌頭生一切蛇蝎蚊虻蠅蚤蚰蜒百足等。",
        "from": "T32n1640_001:0157b14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是知者名為涅槃。",
        "from": "T32n1640_001:0157b15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故女人眷屬論師說。",
        "from": "T32n1640_001:0157b16_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "女人是常名涅槃因。",
        "from": "T32n1640_001:0157b16_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1640_001:0157b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等外道說罪福盡德亦盡故名涅槃。",
        "from": "T32n1640_001:0157b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1640_001:0157b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十外道行苦行論師說。",
        "from": "T32n1640_001:0157b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1640_001:0157b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等外道說煩惱盡故依智名涅槃。",
        "from": "T32n1640_001:0157b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1640_001:0157b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十一外道淨眼論師作如是說。",
        "from": "T32n1640_001:0157b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1640_001:0157b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等外道說見自在天造作眾生名涅槃。",
        "from": "T32n1640_001:0157b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1640_001:0157b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十二外道摩陀羅論師言。",
        "from": "T32n1640_001:0157b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那羅延論師說。",
        "from": "T32n1640_001:0157b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我造一切物。",
        "from": "T32n1640_001:0157b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我於一切眾生中最勝。",
        "from": "T32n1640_001:0157b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我生一切世間有命無命物。",
        "from": "T32n1640_001:0157b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我是一切山中大須彌山王。",
        "from": "T32n1640_001:0157b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我是一切水中大海。",
        "from": "T32n1640_001:0157b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我是一切藥中穀。",
        "from": "T32n1640_001:0157b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我是一切仙人中迦毘羅牟尼。",
        "from": "T32n1640_001:0157b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人至心以水草華果供養我。",
        "from": "T32n1640_001:0157b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我不失彼人。",
        "from": "T32n1640_001:0157c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼人不失我。",
        "from": "T32n1640_001:0157c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩陀羅論師說。",
        "from": "T32n1640_001:0157c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那羅延論師言。",
        "from": "T32n1640_001:0157c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切物從我作生。",
        "from": "T32n1640_001:0157c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還沒彼處名為涅槃。",
        "from": "T32n1640_001:0157c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故名常。",
        "from": "T32n1640_001:0157c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是涅槃因。",
        "from": "T32n1640_001:0157c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1640_001:0157c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等外道說眾生遞共因生名涅槃。",
        "from": "T32n1640_001:0157c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1640_001:0157c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十三外道尼犍子論師作如是說。",
        "from": "T32n1640_001:0157c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初生一男共一女。",
        "from": "T32n1640_001:0157c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼二和合能生一切有命無命等物。",
        "from": "T32n1640_001:0157c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後時離散還沒彼處名為涅槃。",
        "from": "T32n1640_001:0157c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故尼犍子論師說。",
        "from": "T32n1640_001:0157c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "男女和合生一切物。",
        "from": "T32n1640_001:0157c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名涅槃因。",
        "from": "T32n1640_001:0157c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1640_001:0157c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等外道說證諦道名涅槃因。",
        "from": "T32n1640_001:0157c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1640_001:0157c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十四外道僧佉論師說。",
        "from": "T32n1640_001:0157c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十五諦自性因生諸眾生是涅槃因。",
        "from": "T32n1640_001:0157c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自性是常故從自性生大。",
        "from": "T32n1640_001:0157c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從大生意。",
        "from": "T32n1640_001:0157c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從意生智。",
        "from": "T32n1640_001:0157c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從智生五分。",
        "from": "T32n1640_001:0157c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從五分生五知根。",
        "from": "T32n1640_001:0157c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從五知根生五業根。",
        "from": "T32n1640_001:0157c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從五業根生五大。",
        "from": "T32n1640_001:0157c15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故論中說。",
        "from": "T32n1640_001:0157c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨何等何等性修行二十五諦。",
        "from": "T32n1640_001:0157c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如實知從自性生還入自性能離一切生死得涅槃。",
        "from": "T32n1640_001:0157c17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是從自性生一切眾生。",
        "from": "T32n1640_001:0157c18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故外道僧佉說。",
        "from": "T32n1640_001:0157c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自性是常能生諸法。",
        "from": "T32n1640_001:0157c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是涅槃因。",
        "from": "T32n1640_001:0157c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1640_001:0157c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等外道說有作所作而共和合名涅槃。",
        "from": "T32n1640_001:0157c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1640_001:0157c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十五外道摩醯首羅論師作如是說。",
        "from": "T32n1640_001:0157c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果是那羅延所作。",
        "from": "T32n1640_001:0157c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵天是因。",
        "from": "T32n1640_001:0157c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩醯首羅一體三分。",
        "from": "T32n1640_001:0157c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂梵天那羅延摩醯首羅。",
        "from": "T32n1640_001:0157c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地是依處。",
        "from": "T32n1640_001:0157c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地主是摩醯首羅天。",
        "from": "T32n1640_001:0157c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於三界中所有一切命非命物。",
        "from": "T32n1640_001:0157c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆是摩醯首羅天生。",
        "from": "T32n1640_001:0157c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩醯首羅身者。",
        "from": "T32n1640_001:0157c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空是頭。",
        "from": "T32n1640_001:0157c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地是身。",
        "from": "T32n1640_001:0157c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水是尿。",
        "from": "T32n1640_001:0157c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "山是糞。",
        "from": "T32n1640_001:0157c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生是腹中虫。",
        "from": "T32n1640_001:0157c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風是命。",
        "from": "T32n1640_001:0157c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火是煖。",
        "from": "T32n1640_001:0157c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "罪福是業。",
        "from": "T32n1640_001:0157c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是八種是摩醯首羅身。",
        "from": "T32n1640_001:0157c29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自在天是生滅因。",
        "from": "T32n1640_001:0158a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切從自在天生。",
        "from": "T32n1640_001:0158a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從自在天滅。",
        "from": "T32n1640_001:0158a02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為涅槃。",
        "from": "T32n1640_001:0158a02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故摩醯首羅論師說。",
        "from": "T32n1640_001:0158a02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自在天常生一切物。",
        "from": "T32n1640_001:0158a03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是涅槃因。",
        "from": "T32n1640_001:0158a03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1640_001:0158a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等外道說一切物自然而生名涅槃。",
        "from": "T32n1640_001:0158a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1640_001:0158a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十六外道無因論師作如是說。",
        "from": "T32n1640_001:0158a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無因無緣生一切物。",
        "from": "T32n1640_001:0158a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無染因無淨因。",
        "from": "T32n1640_001:0158a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我論中說。",
        "from": "T32n1640_001:0158a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如棘刺針無人作。",
        "from": "T32n1640_001:0158a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孔雀等種種畫色皆無人作。",
        "from": "T32n1640_001:0158a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然而有不從因生名為涅槃。",
        "from": "T32n1640_001:0158a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故無因論師說。",
        "from": "T32n1640_001:0158a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然是常生一切物。",
        "from": "T32n1640_001:0158a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是涅槃因。",
        "from": "T32n1640_001:0158a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1640_001:0158a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等外道說諸物皆是時作名涅槃。",
        "from": "T32n1640_001:0158a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1640_001:0158a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十七外道時散論師作如是說時熟一切大。",
        "from": "T32n1640_001:0158a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時作一切物。",
        "from": "T32n1640_001:0158a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時散一切物。",
        "from": "T32n1640_001:0158a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故我論中說。",
        "from": "T32n1640_001:0158a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如被百箭射時不到不死。",
        "from": "T32n1640_001:0158a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時到則小草觸即死。",
        "from": "T32n1640_001:0158a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切物時生。",
        "from": "T32n1640_001:0158a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切物時熟。",
        "from": "T32n1640_001:0158a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切物時滅。",
        "from": "T32n1640_001:0158a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時不可過。",
        "from": "T32n1640_001:0158a15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故時論師說。",
        "from": "T32n1640_001:0158a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時是常生一切物。",
        "from": "T32n1640_001:0158a15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名涅槃因。",
        "from": "T32n1640_001:0158a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1640_001:0158a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等外道說見有物名涅槃。",
        "from": "T32n1640_001:0158a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1640_001:0158a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十八外道服水論師作如是說。",
        "from": "T32n1640_001:0158a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水是萬物根本。",
        "from": "T32n1640_001:0158a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水能生天地。",
        "from": "T32n1640_001:0158a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生有命無命一切物。",
        "from": "T32n1640_001:0158a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下至阿鼻地獄上至阿迦尼吒天。",
        "from": "T32n1640_001:0158a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆水為主。",
        "from": "T32n1640_001:0158a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水能生物。",
        "from": "T32n1640_001:0158a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水能壞物。",
        "from": "T32n1640_001:0158a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為涅槃。",
        "from": "T32n1640_001:0158a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故外道服水論師說。",
        "from": "T32n1640_001:0158a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水是常名涅槃因。",
        "from": "T32n1640_001:0158a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1640_001:0158a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等外道說見無物名涅槃因。",
        "from": "T32n1640_001:0158a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1640_001:0158a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十九外道口力論師說。",
        "from": "T32n1640_001:0158a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空是萬物因。",
        "from": "T32n1640_001:0158a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最初生虛空。",
        "from": "T32n1640_001:0158a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從虛空生風。",
        "from": "T32n1640_001:0158a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從風生火。",
        "from": "T32n1640_001:0158a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從火生煖。",
        "from": "T32n1640_001:0158a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煖生水。",
        "from": "T32n1640_001:0158a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水即凍凌堅作地。",
        "from": "T32n1640_001:0158a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從地生種種藥草。",
        "from": "T32n1640_001:0158a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從種種藥草生五穀。",
        "from": "T32n1640_001:0158a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從五穀生命。",
        "from": "T32n1640_001:0158a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故我論中說。",
        "from": "T32n1640_001:0158a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命者是食後時還沒。",
        "from": "T32n1640_001:0158a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空名涅槃。",
        "from": "T32n1640_001:0158a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故外道口力論師說。",
        "from": "T32n1640_001:0158a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空是常。",
        "from": "T32n1640_001:0158a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名涅槃因。",
        "from": "T32n1640_001:0158a29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1640_001:0158b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等外道說見有無物是涅槃因。",
        "from": "T32n1640_001:0158b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1640_001:0158b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二十外道本生安荼論師說。",
        "from": "T32n1640_001:0158b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本無日月星辰虛空及地。",
        "from": "T32n1640_001:0158b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯有大水。",
        "from": "T32n1640_001:0158b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時大安荼生如雞子周匝金色時熟。",
        "from": "T32n1640_001:0158b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破為二段。",
        "from": "T32n1640_001:0158b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一段在上作天。",
        "from": "T32n1640_001:0158b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一段在下作地。",
        "from": "T32n1640_001:0158b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼二中間生梵天名一切眾生祖公。",
        "from": "T32n1640_001:0158b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作一切有命無命物。",
        "from": "T32n1640_001:0158b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是有命無命等物散沒。",
        "from": "T32n1640_001:0158b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼處名涅槃。",
        "from": "T32n1640_001:0158b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故外道安荼論師說。",
        "from": "T32n1640_001:0158b07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大安荼出生梵天是常。",
        "from": "T32n1640_001:0158b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名涅槃因。",
        "from": "T32n1640_001:0158b08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提婆菩薩釋楞伽經中外道小乘涅槃論",
        "from": "T32n1640_001:0158b09_0",
        "context_with_next_sentence": false
    }
]