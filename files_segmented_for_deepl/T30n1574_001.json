[
    {
        "source_sentence": "大乘破有論",
        "from": "T30n1574_001:0254a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍樹菩薩造",
        "from": "T30n1574_001:0254a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝奉大",
        "from": "T30n1574_001:0254a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫試光祿卿傳法大師",
        "from": "T30n1574_001:0254a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賜紫臣施護奉　詔譯",
        "from": "T30n1574_001:0254a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命一切佛諸有智者。",
        "from": "T30n1574_001:0254a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當如實了知諸法。",
        "from": "T30n1574_001:0254a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中云何。",
        "from": "T30n1574_001:0254a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂一切性從無性生。",
        "from": "T30n1574_001:0254a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦非無性生。",
        "from": "T30n1574_001:0254a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切性若有生者。",
        "from": "T30n1574_001:0254a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼性是常。",
        "from": "T30n1574_001:0254a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是性無實。",
        "from": "T30n1574_001:0254a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如空華。",
        "from": "T30n1574_001:0254a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知諸法與虛空等。",
        "from": "T30n1574_001:0254a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼諸法生亦與空等。",
        "from": "T30n1574_001:0254a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切緣法皆如虛空。",
        "from": "T30n1574_001:0254a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼無實故。",
        "from": "T30n1574_001:0254a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當云何有。",
        "from": "T30n1574_001:0254a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸法無因而復無果。",
        "from": "T30n1574_001:0254a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦無諸業自性可得。",
        "from": "T30n1574_001:0254a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中一切而無有實。",
        "from": "T30n1574_001:0254a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無世間故無出世間。",
        "from": "T30n1574_001:0254a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切無生亦無有性。",
        "from": "T30n1574_001:0254a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何諸法而有所生。",
        "from": "T30n1574_001:0254a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間親愛父子眷屬。",
        "from": "T30n1574_001:0254a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖有所生而無其實。",
        "from": "T30n1574_001:0254a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不從先世之所生故。",
        "from": "T30n1574_001:0254a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦非現世有其相故。",
        "from": "T30n1574_001:0254a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此於世間無義可轉。",
        "from": "T30n1574_001:0254a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如月中見諸影像。",
        "from": "T30n1574_001:0254a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間無實從分別起。",
        "from": "T30n1574_001:0254a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此分別故分別心生。",
        "from": "T30n1574_001:0254a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此心為因即有身生。",
        "from": "T30n1574_001:0254a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故有身行於世間。",
        "from": "T30n1574_001:0254a21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘊所成故名之為身。",
        "from": "T30n1574_001:0254a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸蘊皆空無有自性。",
        "from": "T30n1574_001:0254a22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘊無自性而亦無心。",
        "from": "T30n1574_001:0254a22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以無心故是故無身。",
        "from": "T30n1574_001:0254a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知自性離諸分別。",
        "from": "T30n1574_001:0254a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無其心亦無有法。",
        "from": "T30n1574_001:0254a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無其身亦無有界。",
        "from": "T30n1574_001:0254a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中所說是無二道。",
        "from": "T30n1574_001:0254a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此所說者是真實說。",
        "from": "T30n1574_001:0254a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中一切離諸所緣。",
        "from": "T30n1574_001:0254a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中所說離諸所緣。",
        "from": "T30n1574_001:0254a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中所作離諸所緣。",
        "from": "T30n1574_001:0254a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中所得離諸所緣。",
        "from": "T30n1574_001:0254a27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有布施持戒忍辱精進禪定智慧諸法如是常行。",
        "from": "T30n1574_001:0254a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不久時中即能證得無上菩提。",
        "from": "T30n1574_001:0254a28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以慧方便安住實際。",
        "from": "T30n1574_001:0254a29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起悲愍行廣度眾生。",
        "from": "T30n1574_001:0254b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖復如是有所得相。",
        "from": "T30n1574_001:0254b01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切智性而不可說得。",
        "from": "T30n1574_001:0254b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼一切法但有名字。",
        "from": "T30n1574_001:0254b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切但於有想中住。",
        "from": "T30n1574_001:0254b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現前無實差別所生。",
        "from": "T30n1574_001:0254b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "差別生法而無所有。",
        "from": "T30n1574_001:0254b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼一切法本無有名。",
        "from": "T30n1574_001:0254b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但以假名而表了故。",
        "from": "T30n1574_001:0254b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知諸法而無實體。",
        "from": "T30n1574_001:0254b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切皆從分別所生。",
        "from": "T30n1574_001:0254b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中若無分別者。",
        "from": "T30n1574_001:0254b06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即同虛空離諸分別。",
        "from": "T30n1574_001:0254b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如說眼者能見於色。",
        "from": "T30n1574_001:0254b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作此說者是真實語。",
        "from": "T30n1574_001:0254b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間有諸邪執心者。",
        "from": "T30n1574_001:0254b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執此所說如實而轉。",
        "from": "T30n1574_001:0254b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼一切法聚類所現。",
        "from": "T30n1574_001:0254b09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知此說是佛所說。",
        "from": "T30n1574_001:0254b09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故應知。",
        "from": "T30n1574_001:0254b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中義者眼不見色。",
        "from": "T30n1574_001:0254b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至意不知法。",
        "from": "T30n1574_001:0254b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若如是知是為智者。",
        "from": "T30n1574_001:0254b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即能通達第一義諦。",
        "from": "T30n1574_001:0254b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是乃名最上真實。",
        "from": "T30n1574_001:0254b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今依經。",
        "from": "T30n1574_001:0254b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是略說。",
        "from": "T30n1574_001:0254b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大乘破有論",
        "from": "T30n1574_001:0254b13_0",
        "context_with_next_sentence": false
    }
]