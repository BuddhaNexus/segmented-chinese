    },
    {
        "source_sentence": "大唐天后勅佛授記寺沙門明佺等撰",
        "from": "T55n2153_012:0442b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大小乘闕本經目合為一卷(四百七十八部六百七十七卷)右闕本經者。",
        "from": "T55n2153_012:0442b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自微言西絕像教東流年載。",
        "from": "T55n2153_012:0442b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既其浩然翻傳蓋亦多矣。",
        "from": "T55n2153_012:0442b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂使市朝陵谷處處遷移。",
        "from": "T55n2153_012:0442b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貝葉繩花時時散失。",
        "from": "T55n2153_012:0442b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今尋求未獲者。",
        "from": "T55n2153_012:0442b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故列於闕本焉。",
