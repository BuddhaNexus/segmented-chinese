[
    {
        "source_sentence": "聖歡喜天式法",
        "from": "T21n1275_001:0324a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏法師般若惹羯羅撰",
        "from": "T21n1275_001:0324a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於聖天法大欲開靈驗者。",
        "from": "T21n1275_001:0324a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先造立天地盤。",
        "from": "T21n1275_001:0324a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則天盤長三寸又四寸。",
        "from": "T21n1275_001:0324a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地盤長方七寸。",
        "from": "T21n1275_001:0324a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以有乳木及香木。",
        "from": "T21n1275_001:0324a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日之內造之。",
        "from": "T21n1275_001:0324a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天地共以白木作之。",
        "from": "T21n1275_001:0324a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造畫時不可見下劣之人。",
        "from": "T21n1275_001:0324a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必必精進潔齋。",
        "from": "T21n1275_001:0324a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以吉日為之。",
        "from": "T21n1275_001:0324a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依通法擇日。",
        "from": "T21n1275_001:0324a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合耳一頭天盤圓作。",
        "from": "T21n1275_001:0324a10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀東方安葉上唵字。",
        "from": "T21n1275_001:0324a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即變成日王歡喜天。",
        "from": "T21n1275_001:0324a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭上在日輪於俗形而有瞋相。",
        "from": "T21n1275_001:0324a12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面容赤色。",
        "from": "T21n1275_001:0324a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持吠瑠璃收胸上。",
        "from": "T21n1275_001:0324a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持獨股收左腰。",
        "from": "T21n1275_001:0324a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摧壞部主名無憂大將。",
        "from": "T21n1275_001:0324a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被七阿僧祇眷屬圍繞。",
        "from": "T21n1275_001:0324a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居青石上。",
        "from": "T21n1275_001:0324a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南方葉上觀爾字。",
        "from": "T21n1275_001:0324a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即變為愛王歡喜天。",
        "from": "T21n1275_001:0324a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "似法師形。",
        "from": "T21n1275_001:0324a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及其鼻胸下如面目矢。",
        "from": "T21n1275_001:0324a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持團餅安結跏上。",
        "from": "T21n1275_001:0324a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持蘿富根舉左肩間。",
        "from": "T21n1275_001:0324a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "野干部主名象頭大將。",
        "from": "T21n1275_001:0324a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十八俱胝被眷屬圍繞。",
        "from": "T21n1275_001:0324a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居赤石上。",
        "from": "T21n1275_001:0324a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方葉上觀頡哩字。",
        "from": "T21n1275_001:0324a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即變為月愛歡喜天。",
        "from": "T21n1275_001:0324a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其形美女也。",
        "from": "T21n1275_001:0324a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭有花鬘。",
        "from": "T21n1275_001:0324a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持寶鏡。",
        "from": "T21n1275_001:0324a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手支頰。",
        "from": "T21n1275_001:0324a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以寶篋為□足。",
        "from": "T21n1275_001:0324a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端正殊妙色。",
        "from": "T21n1275_001:0324a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一牙部主名嚴髻大將。",
        "from": "T21n1275_001:0324a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一百四千俱胝眷屬被圍繞。",
        "from": "T21n1275_001:0324a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "座虎皮上。",
        "from": "T21n1275_001:0324a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方葉上觀忙字。",
        "from": "T21n1275_001:0324a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即變議特歡喜天。",
        "from": "T21n1275_001:0324a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其形青色如憍尸迦天。",
        "from": "T21n1275_001:0324a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左右手各作刀印。",
        "from": "T21n1275_001:0324a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "收各方腰。",
        "from": "T21n1275_001:0324a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍象部主名頂行大將。",
        "from": "T21n1275_001:0324a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一俱胝那臾多眷屬圍繞踞黑石。",
        "from": "T21n1275_001:0324a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次明地盤眾。",
        "from": "T21n1275_001:0324a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東方安帝釋天。",
        "from": "T21n1275_001:0324a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東南方安火天。",
        "from": "T21n1275_001:0324a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南方安炎魔天。",
        "from": "T21n1275_001:0324a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西南方安羅剎天。",
        "from": "T21n1275_001:0324a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方安水天。",
        "from": "T21n1275_001:0324a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西北方安風天。",
        "from": "T21n1275_001:0324b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方安毘沙門天。",
        "from": "T21n1275_001:0324b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東北方安大自在天。",
        "from": "T21n1275_001:0324b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安廿八宿卅六禽。",
        "from": "T21n1275_001:0324b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是次第建立之矣。",
        "from": "T21n1275_001:0324b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天盤四天皆一體也。",
        "from": "T21n1275_001:0324b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非異體者。",
        "from": "T21n1275_001:0324b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若中□亦安多波天無妨礙。",
        "from": "T21n1275_001:0324b03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次召請印　以二小指二無名指相鉤向內。",
        "from": "T21n1275_001:0324b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二中指立相叉。",
        "from": "T21n1275_001:0324b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以二頭指各竪付中指。",
        "from": "T21n1275_001:0324b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大指亦竪付近頭指側。",
        "from": "T21n1275_001:0324b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指去來。",
        "from": "T21n1275_001:0324b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明曰(四天通用)。",
        "from": "T21n1275_001:0324b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵簸迦羅(二合)主拏禰(去)縛哆野(二合)次用普印伴呪曰。",
        "from": "T21n1275_001:0324b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵簸迦羅(二合)三萬眷屬從恒河沙守護自他祈祈決定急急如律令次地盤眾等召請印。",
        "from": "T21n1275_001:0324b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作帝殊羅施印。",
        "from": "T21n1275_001:0324b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二小指二無名指叉於掌內竪。",
        "from": "T21n1275_001:0324b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指頭相捻。",
        "from": "T21n1275_001:0324b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二頭指各加中指背節下半分。",
        "from": "T21n1275_001:0324b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指來去。",
        "from": "T21n1275_001:0324b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1275_001:0324b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)鑠都嚕(二)波羅摩馱儞曳(三)莎訶(通八方天)次二十八宿三十六禽召請印(用輪壇印通)呪曰。",
        "from": "T21n1275_001:0324b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵知里安疑嚕儞曳莎訶",
        "from": "T21n1275_001:0324b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次請一切諸天類等印呪。",
        "from": "T21n1275_001:0324b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛合掌立二空去來。",
        "from": "T21n1275_001:0324b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1275_001:0324b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "富順愛神相合部類宴坐引入伴族一億二億一切諸頭頭契來入入入娑婆呵次次供養契印。",
        "from": "T21n1275_001:0324b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉求他之中(云云)。",
        "from": "T21n1275_001:0324b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盛冥文符(裏某甲耳)",
        "from": "T21n1275_001:0324b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祕攝符(固對表畫斷也)",
        "from": "T21n1275_001:0324b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右欲望官位爵祿者。",
        "from": "T21n1275_001:0324c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以日輪天加帝釋天。",
        "from": "T21n1275_001:0324c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲令他熱病者。",
        "from": "T21n1275_001:0324c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以日輪天加火天王。",
        "from": "T21n1275_001:0324c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲得福德者。",
        "from": "T21n1275_001:0324c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以月愛天加毘沙門天祈之。",
        "from": "T21n1275_001:0324c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲得他愛念者。",
        "from": "T21n1275_001:0324c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以月愛天加毘沙門天使者。",
        "from": "T21n1275_001:0324c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呼二十八宿。",
        "from": "T21n1275_001:0324c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲勝兵軍者。",
        "from": "T21n1275_001:0324c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以日輪天加大自在天。",
        "from": "T21n1275_001:0324c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呼一萬眷屬。",
        "from": "T21n1275_001:0324c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲得人心遂吾思者。",
        "from": "T21n1275_001:0324c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以議特天加炎魔天。",
        "from": "T21n1275_001:0324c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一切人欲被愛敬者。",
        "from": "T21n1275_001:0324c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以愛王天加毘沙門天。",
        "from": "T21n1275_001:0324c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲得他人財寶者。",
        "from": "T21n1275_001:0324c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以月愛天加大自在天。",
        "from": "T21n1275_001:0324c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若夫妻之中欲令愛念者。",
        "from": "T21n1275_001:0324c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以月愛天加帝釋天。",
        "from": "T21n1275_001:0324c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若夫妻之中欲令相離者。",
        "from": "T21n1275_001:0324c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以議特天加水天王。",
        "from": "T21n1275_001:0324c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲忽死敵惡人怨敵者。",
        "from": "T21n1275_001:0324c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以愛王天加羅剎天。",
        "from": "T21n1275_001:0324c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若從者逃走欲令還來者。",
        "from": "T21n1275_001:0324c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以愛王天加大自在天。",
        "from": "T21n1275_001:0324c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呼其從者姓名。",
        "from": "T21n1275_001:0324c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若懷妊之女早安欲令產生者。",
        "from": "T21n1275_001:0324c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以月愛天加炎魔天好矣。",
        "from": "T21n1275_001:0324c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若被呪咀時。",
        "from": "T21n1275_001:0324c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲被還者。",
        "from": "T21n1275_001:0324c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以日輪天加羅剎天祈申之。",
        "from": "T21n1275_001:0324c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲令他人風病者。",
        "from": "T21n1275_001:0324c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以議特天加風天祈之。",
        "from": "T21n1275_001:0324c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲止病瘡癰疸一切瘡病。",
        "from": "T21n1275_001:0324c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以愛王天加炎魔天願祈之。",
        "from": "T21n1275_001:0324c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲止長血赤痢者。",
        "from": "T21n1275_001:0324c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以日輪天加帝釋天祈申。",
        "from": "T21n1275_001:0324c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲止雨降難者。",
        "from": "T21n1275_001:0324c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以愛王天加帝釋天。",
        "from": "T21n1275_001:0324c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲止旱魃降雨者。",
        "from": "T21n1275_001:0324c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以議特天加水天王。",
        "from": "T21n1275_001:0324c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲止腹病頭病者。",
        "from": "T21n1275_001:0325a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以日輪天加炎魔天。",
        "from": "T21n1275_001:0325a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲遂論議決[儷-鹿+木]者。",
        "from": "T21n1275_001:0325a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以日輪天加大自在。",
        "from": "T21n1275_001:0325a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呼二十八宿等。",
        "from": "T21n1275_001:0325a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲付公私遂所聖事者。",
        "from": "T21n1275_001:0325a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以愛王天加帝釋天。",
        "from": "T21n1275_001:0325a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲得愜年穀者。",
        "from": "T21n1275_001:0325a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以議特天加帝釋天。",
        "from": "T21n1275_001:0325a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲家內靜者。",
        "from": "T21n1275_001:0325a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以愛王天加毘沙門天。",
        "from": "T21n1275_001:0325a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲止口舌事者。",
        "from": "T21n1275_001:0325a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以愛王天加自在天。",
        "from": "T21n1275_001:0325a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲止物怪惡夢者。",
        "from": "T21n1275_001:0325a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以日輪天加炎魔天。",
        "from": "T21n1275_001:0325a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲使者遣遠早返來者。",
        "from": "T21n1275_001:0325a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以議特天加風天忽返來。",
        "from": "T21n1275_001:0325a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是一一合法對祈申。",
        "from": "T21n1275_001:0325a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所念之事必成就。",
        "from": "T21n1275_001:0325a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對時誦根本呪印(云云)。",
        "from": "T21n1275_001:0325a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結三部護身印明依諸通印。",
        "from": "T21n1275_001:0325a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "都合攝鉤印。",
        "from": "T21n1275_001:0325a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內縛置頂上散。",
        "from": "T21n1275_001:0325a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1275_001:0325a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩羅薩羅天地都合禁禁吽吽跘",
        "from": "T21n1275_001:0325a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結對印不動謁哦印。",
        "from": "T21n1275_001:0325a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1275_001:0325a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吽吽吉儞吉儞吽泮(半音)娑婆呵",
        "from": "T21n1275_001:0325a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次十方結界印　地結方牆虛空結印等真言亦爾(云云)。",
        "from": "T21n1275_001:0325a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以糸對之。",
        "from": "T21n1275_001:0325a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1275_001:0325a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵乞曳曳曳吽",
        "from": "T21n1275_001:0325a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "件法天法國於菩提寺所傳也。",
        "from": "T21n1275_001:0325a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其調度不多類。",
        "from": "T21n1275_001:0325a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時人皆號驗法焉。",
        "from": "T21n1275_001:0325a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖天式法一卷",
        "from": "T21n1275_001:0325a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(附)",
        "from": "T21n1275_001:0325a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同天次第一卷",
        "from": "T21n1275_001:0325a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先淨三業印真言　次護身真言印　次向建受三度建吞之　次天鼓三七返　次向本尊禮三返　次著座　次塗香淨手　次加持灑水散壇上并供物及室內悉令清淨　次表白事由　次神分祈願　次大金剛輪真言印　次輪壇真言印　次勝悉地真言印　次三部三昧心真言印　次三刀偈　次如來拳印　次道場觀　次天盤聖眾勸請明印　次天盤伴天印　次地盤眾勸請明印　次地盤伴天印　次二十八宿并三十六禽明印　次地申明印　次避除結界明印　次玉女真言印(但申次用之)　次示三昧耶明印　次閼伽明印　次花座明印　次善來偈　次閼伽明印　次塗香明印　次花供印　次燒香明印　次飲食燈明印　次普供養明印　次大金剛輪明印　次合掌誦天盤眾二十八宿三十六禽名號三返　次誦七星并九天名號二返　次三部三昧耶心明印　次地結明印　次方隅界明印　次上方結界明印　次對印明　若以糸對盤或不然　次護身真言印　次取珠三迴香盤置本處　次結根本印　取珠念誦任意要天呪　次念誦了去珠在本處　次左方塗香　次花塗燒飲燈　次普供養　次祈願　次閼伽　次迴向　次結界　次奉送　次三部護身印明　次出堂聖天次第一卷　　　　　　　　一交了　興然之本",
        "from": "T21n1275_001:0325a27_0",
        "context_with_next_sentence": false
    }
]