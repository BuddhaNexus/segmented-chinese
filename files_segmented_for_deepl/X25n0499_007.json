[
    {
        "source_sentence": "金剛般若波羅蜜經懸判疏鈔卷之七",
        "from": "X25n0499_007:0654a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勅建西天寺武林後學沙門性起述",
        "from": "X25n0499_007:0654a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△七處會傳譯三。",
        "from": "X25n0499_007:0654a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初標。",
        "from": "X25n0499_007:0654a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第七處會傳譯者。",
        "from": "X25n0499_007:0654a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二釋。",
        "from": "X25n0499_007:0654a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文二。",
        "from": "X25n0499_007:0654a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初明佛說二。",
        "from": "X25n0499_007:0654a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初中又三。",
        "from": "X25n0499_007:0654a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初標處會。",
        "from": "X25n0499_007:0654a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初總明佛說者。",
        "from": "X25n0499_007:0654a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大部六百卷文。",
        "from": "X25n0499_007:0654a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四處十六會。",
        "from": "X25n0499_007:0654a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二詳處會。",
        "from": "X25n0499_007:0654a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王舍城鷲峰山七會。",
        "from": "X25n0499_007:0654a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "山中三會。",
        "from": "X25n0499_007:0654a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "山頂四會。",
        "from": "X25n0499_007:0654a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二給孤獨園七會。",
        "from": "X25n0499_007:0654a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三他化自在天摩尼寶藏殿一會。",
        "from": "X25n0499_007:0654a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四王舍城白鷺池邊一會。",
        "from": "X25n0499_007:0654a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△三指本經處會。",
        "from": "X25n0499_007:0654b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經則給孤獨處第九會。",
        "from": "X25n0499_007:0654b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃第五百七十七卷之本。",
        "from": "X25n0499_007:0654b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鈔。",
        "from": "X25n0499_007:0654b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以上明佛說已竟。",
        "from": "X25n0499_007:0654b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△次明傳譯何時二。",
        "from": "X25n0499_007:0654b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初指諸佛傳譯。",
        "from": "X25n0499_007:0654b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後別明傳譯此經卷者。",
        "from": "X25n0499_007:0654b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時主前後六譯。",
        "from": "X25n0499_007:0654b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一後秦羅什。",
        "from": "X25n0499_007:0654b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鈔。",
        "from": "X25n0499_007:0654b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳩摩羅什者。",
        "from": "X25n0499_007:0654b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵語。",
        "from": "X25n0499_007:0654b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此云童壽。",
        "from": "X25n0499_007:0654b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祖印度人文以聰敏見稱。",
        "from": "X25n0499_007:0654b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母是龜茲王妹。",
        "from": "X25n0499_007:0654b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母孕什時。",
        "from": "X25n0499_007:0654b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增益慧辨。",
        "from": "X25n0499_007:0654b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七歲出家。",
        "from": "X25n0499_007:0654b07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日誦千偈義旨。",
        "from": "X25n0499_007:0654b08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通至九歲。",
        "from": "X25n0499_007:0654b08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與外道論義。",
        "from": "X25n0499_007:0654b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辨挫邪風。",
        "from": "X25n0499_007:0654b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸皆愧伏。",
        "from": "X25n0499_007:0654b09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年十二歲。",
        "from": "X25n0499_007:0654b09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有羅漢奇之。",
        "from": "X25n0499_007:0654b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂母曰。",
        "from": "X25n0499_007:0654b09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常守護之。",
        "from": "X25n0499_007:0654b09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若年三十五歲。",
        "from": "X25n0499_007:0654b10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不破戒律。",
        "from": "X25n0499_007:0654b10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當大興佛法。",
        "from": "X25n0499_007:0654b10_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "度無數人。",
        "from": "X25n0499_007:0654b10_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師又習五明論。",
        "from": "X25n0499_007:0654b11_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四韋馱典。",
        "from": "X25n0499_007:0654b11_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陰陽星算。",
        "from": "X25n0499_007:0654b11_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必窮其妙。",
        "from": "X25n0499_007:0654b11_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後轉習大乘。",
        "from": "X25n0499_007:0654b12_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數破外道。",
        "from": "X25n0499_007:0654b12_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠近諸國。",
        "from": "X25n0499_007:0654b12_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸謂神異。",
        "from": "X25n0499_007:0654b12_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母生什後。",
        "from": "X25n0499_007:0654b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦即出家。",
        "from": "X25n0499_007:0654b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聰拔眾尼。",
        "from": "X25n0499_007:0654b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得第三果。",
        "from": "X25n0499_007:0654b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "什既受具。",
        "from": "X25n0499_007:0654b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母謂什曰。",
        "from": "X25n0499_007:0654b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方等深教。",
        "from": "X25n0499_007:0654b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應闡秦都。",
        "from": "X25n0499_007:0654b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於汝無益。",
        "from": "X25n0499_007:0654b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何如。",
        "from": "X25n0499_007:0654b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "什曰。",
        "from": "X25n0499_007:0654b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩之行。",
        "from": "X25n0499_007:0654b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利物亡軀。",
        "from": "X25n0499_007:0654b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大化必行。",
        "from": "X25n0499_007:0654b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鑪鑊無恨。",
        "from": "X25n0499_007:0654b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母即告曰。",
        "from": "X25n0499_007:0654b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝立誓宏。",
        "from": "X25n0499_007:0654b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾不與止。",
        "from": "X25n0499_007:0654b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "什自是已後。",
        "from": "X25n0499_007:0654b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣習大乘。",
        "from": "X25n0499_007:0654b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必洞其奧。",
        "from": "X25n0499_007:0654b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西域諸王請什講說。",
        "from": "X25n0499_007:0654b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必長跪座側。",
        "from": "X25n0499_007:0654b17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命什蹹登。",
        "from": "X25n0499_007:0654b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苻堅建元九年。",
        "from": "X25n0499_007:0654b18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "太史奏云。",
        "from": "X25n0499_007:0654b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有德星現於外國。",
        "from": "X25n0499_007:0654b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當有大德智人。",
        "from": "X25n0499_007:0654b19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入輔中國。",
        "from": "X25n0499_007:0654b19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅曰。",
        "from": "X25n0499_007:0654b19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朕聞西域有羅什。",
        "from": "X25n0499_007:0654b19_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "襄陽有道安。",
        "from": "X25n0499_007:0654b20_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將非此耶。",
        "from": "X25n0499_007:0654b20_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後遣呂光統領七萬雄兵。",
        "from": "X25n0499_007:0654b20_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西伐龜茲。",
        "from": "X25n0499_007:0654b21_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王不能捨。",
        "from": "X25n0499_007:0654b21_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "什曰。",
        "from": "X25n0499_007:0654b21_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為我一人。",
        "from": "X25n0499_007:0654b21_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傷害眾多。",
        "from": "X25n0499_007:0654b21_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾不能忍。",
        "from": "X25n0499_007:0654b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王只得含淚而送。",
        "from": "X25n0499_007:0654b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "什雖同呂光進國。",
        "from": "X25n0499_007:0654b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呂見師幼少。",
        "from": "X25n0499_007:0654b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生慢。",
        "from": "X25n0499_007:0654b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因什善卜數類應。",
        "from": "X25n0499_007:0654b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呂又用之。",
        "from": "X25n0499_007:0654b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呂立創西凉為主。",
        "from": "X25n0499_007:0654b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦請什留。",
        "from": "X25n0499_007:0654b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至姚秦宏始三年。",
        "from": "X25n0499_007:0654b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "興滅西呂。",
        "from": "X25n0499_007:0654b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方入長安。",
        "from": "X25n0499_007:0654c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "秦主興加厚禮之敬。",
        "from": "X25n0499_007:0654c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入西明閣及逍遙園。",
        "from": "X25n0499_007:0654c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別舘安置數僧。",
        "from": "X25n0499_007:0654c02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[((素-糸)*力)/石]等八百沙門。",
        "from": "X25n0499_007:0654c02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諮受什旨。",
        "from": "X25n0499_007:0654c02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又于草堂寺。",
        "from": "X25n0499_007:0654c02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共三千僧眾。",
        "from": "X25n0499_007:0654c03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手執舊經本而參定之。",
        "from": "X25n0499_007:0654c03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫不精究洞明深旨。",
        "from": "X25n0499_007:0654c03_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時有僧叡覲什。",
        "from": "X25n0499_007:0654c04_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故什所譯經。",
        "from": "X25n0499_007:0654c04_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "叡並參正。",
        "from": "X25n0499_007:0654c04_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二後魏菩提流支。",
        "from": "X25n0499_007:0654c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鈔。",
        "from": "X25n0499_007:0654c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此云覺希。",
        "from": "X25n0499_007:0654c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北印度人。",
        "from": "X25n0499_007:0654c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徧通三藏。",
        "from": "X25n0499_007:0654c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙入總持。",
        "from": "X25n0499_007:0654c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "志在宏法。",
        "from": "X25n0499_007:0654c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣流視聽。",
        "from": "X25n0499_007:0654c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魏宣武帝。",
        "from": "X25n0499_007:0654c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洛陽翻譯。",
        "from": "X25n0499_007:0654c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經在內。",
        "from": "X25n0499_007:0654c08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三陳朝真諦。",
        "from": "X25n0499_007:0654c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鈔。",
        "from": "X25n0499_007:0654c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵語波羅末陀。",
        "from": "X25n0499_007:0654c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此云真諦。",
        "from": "X25n0499_007:0654c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西印度優禪尼國人。",
        "from": "X25n0499_007:0654c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "景行澄明。",
        "from": "X25n0499_007:0654c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "器宇清寧。",
        "from": "X25n0499_007:0654c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藝術異解。",
        "from": "X25n0499_007:0654c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "編素諳練。",
        "from": "X25n0499_007:0654c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梁武泰清。",
        "from": "X25n0499_007:0654c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於寶雲殿。",
        "from": "X25n0499_007:0654c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屬侯景紛紀。",
        "from": "X25n0499_007:0654c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃適豫章。",
        "from": "X25n0499_007:0654c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始興南康等。",
        "from": "X25n0499_007:0654c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖復恓惶。",
        "from": "X25n0499_007:0654c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譚叢無輟。",
        "from": "X25n0499_007:0654c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即汎泊西歸。",
        "from": "X25n0499_007:0654c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "業風賦命。",
        "from": "X25n0499_007:0654c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飄還康州。",
        "from": "X25n0499_007:0654c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住制止寺。",
        "from": "X25n0499_007:0654c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翻譯訖。",
        "from": "X25n0499_007:0654c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陳恭建譯五十部。",
        "from": "X25n0499_007:0654c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今經亦在其數。",
        "from": "X25n0499_007:0654c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四陳朝達摩笈多。",
        "from": "X25n0499_007:0654c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鈔。",
        "from": "X25n0499_007:0654c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隋言法密。",
        "from": "X25n0499_007:0654c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南賢豆國人。",
        "from": "X25n0499_007:0654c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開皇十年來屆見文帝。",
        "from": "X25n0499_007:0654c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請入京寺。",
        "from": "X25n0499_007:0654c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義理允正。",
        "from": "X25n0499_007:0654c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱經微旨。",
        "from": "X25n0499_007:0654c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然而慈恕立身。",
        "from": "X25n0499_007:0654c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恭和成性。",
        "from": "X25n0499_007:0654c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心非道外。",
        "from": "X25n0499_007:0654c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行在說前。",
        "from": "X25n0499_007:0654c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戒地夷而淨。",
        "from": "X25n0499_007:0654c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定水幽而潔。",
        "from": "X25n0499_007:0654c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經洞字原。",
        "from": "X25n0499_007:0654c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論探聲意。",
        "from": "X25n0499_007:0654c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至煬帝定鼎東都。",
        "from": "X25n0499_007:0654c20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置翻譯館。",
        "from": "X25n0499_007:0654c20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而有金剛文義在耳。",
        "from": "X25n0499_007:0654c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五唐初玄弉。",
        "from": "X25n0499_007:0654c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鈔。",
        "from": "X25n0499_007:0654c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師是河南洛陽人。",
        "from": "X25n0499_007:0654c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俗姓陳氏。",
        "from": "X25n0499_007:0654c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頴川仲弓之後。",
        "from": "X25n0499_007:0654c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出家徧探諸藏。",
        "from": "X25n0499_007:0654c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆有錯悞。",
        "from": "X25n0499_007:0654c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常慨教缺傳匠。",
        "from": "X25n0499_007:0654c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "理翳譯人。",
        "from": "X25n0499_007:0654c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂使如意之寶不全。",
        "from": "X25n0499_007:0655a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雪山之偈猶半。",
        "from": "X25n0499_007:0655a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是裸足涉履險若平夷。",
        "from": "X25n0499_007:0655a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既戾梵境。",
        "from": "X25n0499_007:0655a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "等諮無倦。",
        "from": "X25n0499_007:0655a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五明四含之典。",
        "from": "X25n0499_007:0655a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十二之筌。",
        "from": "X25n0499_007:0655a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡搜其微。",
        "from": "X25n0499_007:0655a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "究其妙法。",
        "from": "X25n0499_007:0655a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師討經論一十七周。",
        "from": "X25n0499_007:0655a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遊覽百有餘國。",
        "from": "X25n0499_007:0655a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貞觀十九年回歸上京。",
        "from": "X25n0499_007:0655a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勅宏福寺翻譯。",
        "from": "X25n0499_007:0655a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已上多出靜邁法師譯經圖記。",
        "from": "X25n0499_007:0655a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六大周義淨。",
        "from": "X25n0499_007:0655a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未曾詳出。",
        "from": "X25n0499_007:0655a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "另閱別章。",
        "from": "X25n0499_007:0655a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已上六人皆三藏。",
        "from": "X25n0499_007:0655a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鈔。",
        "from": "X25n0499_007:0655a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏表德。",
        "from": "X25n0499_007:0655a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法師舉名。",
        "from": "X25n0499_007:0655a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由德而顯。",
        "from": "X25n0499_007:0655a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故彰其名。",
        "from": "X25n0499_007:0655a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以莊生云。",
        "from": "X25n0499_007:0655a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名者寔之賓。",
        "from": "X25n0499_007:0655a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "德者名之主。",
        "from": "X25n0499_007:0655a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無其德。",
        "from": "X25n0499_007:0655a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苟有其名。",
        "from": "X25n0499_007:0655a10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故夫為人師者。",
        "from": "X25n0499_007:0655a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可以無德而虗當其名。",
        "from": "X25n0499_007:0655a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若虗當者。",
        "from": "X25n0499_007:0655a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則損其福耳。",
        "from": "X25n0499_007:0655a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "況未通經旨者乎。",
        "from": "X25n0499_007:0655a12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今云三藏者。",
        "from": "X25n0499_007:0655a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即通習經律論三藏。",
        "from": "X25n0499_007:0655a13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "達無相法。",
        "from": "X25n0499_007:0655a13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住於佛住。",
        "from": "X25n0499_007:0655a13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼修自利利他。",
        "from": "X25n0499_007:0655a13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而為三乘人則。",
        "from": "X25n0499_007:0655a14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以法為師也。",
        "from": "X25n0499_007:0655a14_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前之六師耳。",
        "from": "X25n0499_007:0655a14_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又六師各有其諱。",
        "from": "X25n0499_007:0655a15_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而為之別。",
        "from": "X25n0499_007:0655a15_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏等四字。",
        "from": "X25n0499_007:0655a15_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而為之通。",
        "from": "X25n0499_007:0655a15_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為通別德而彰顯也。",
        "from": "X25n0499_007:0655a16_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二指本經傳譯釋主二。",
        "from": "X25n0499_007:0655a16_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初傳譯主。",
        "from": "X25n0499_007:0655a17_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今所傳者。",
        "from": "X25n0499_007:0655a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即羅什宏始四年。",
        "from": "X25n0499_007:0655a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於長安草堂寺所譯。",
        "from": "X25n0499_007:0655a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二釋論主。",
        "from": "X25n0499_007:0655a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文二。",
        "from": "X25n0499_007:0655a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初補主傳釋。",
        "from": "X25n0499_007:0655a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天竺有無著大士入光定。",
        "from": "X25n0499_007:0655a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上昇兜率。",
        "from": "X25n0499_007:0655a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "親見彌勒。",
        "from": "X25n0499_007:0655a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稟受八十行偈。",
        "from": "X25n0499_007:0655a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鈔。",
        "from": "X25n0499_007:0655a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵語彌勒。",
        "from": "X25n0499_007:0655a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此云慈氏。",
        "from": "X25n0499_007:0655a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以多生劫來。",
        "from": "X25n0499_007:0655a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒修慈行。",
        "from": "X25n0499_007:0655a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而得慈心三昧之定。",
        "from": "X25n0499_007:0655a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故云慈氏。",
        "from": "X25n0499_007:0655a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛世時。",
        "from": "X25n0499_007:0655a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降生西天婆羅門種。",
        "from": "X25n0499_007:0655a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輔揚大化。",
        "from": "X25n0499_007:0655a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "影彰佛會。",
        "from": "X25n0499_007:0655a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現為釋迦法王之子。",
        "from": "X25n0499_007:0655a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處補當來之記。",
        "from": "X25n0499_007:0655b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已入賢劫千佛中。",
        "from": "X25n0499_007:0655b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五尊龍華教主。",
        "from": "X25n0499_007:0655b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "度無量眾。",
        "from": "X25n0499_007:0655b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣如下生經說。",
        "from": "X25n0499_007:0655b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今現在兜率天宮內院。",
        "from": "X25n0499_007:0655b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教化諸天及菩薩眾。",
        "from": "X25n0499_007:0655b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因無著大士欲習大乘真理。",
        "from": "X25n0499_007:0655b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未諳深求的旨。",
        "from": "X25n0499_007:0655b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悲嘆本師涅槃。",
        "from": "X25n0499_007:0655b04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心意思惟。",
        "from": "X25n0499_007:0655b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幸喜彌勒補處大師。",
        "from": "X25n0499_007:0655b04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而在天宮。",
        "from": "X25n0499_007:0655b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即入日光定力。",
        "from": "X25n0499_007:0655b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上昇見彌勒大士。",
        "from": "X25n0499_007:0655b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "親傳八十行偈。",
        "from": "X25n0499_007:0655b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金口解釋。",
        "from": "X25n0499_007:0655b06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通卷要旨。",
        "from": "X25n0499_007:0655b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可知此經乃佛心印耳。",
        "from": "X25n0499_007:0655b06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二論主轉授。",
        "from": "X25n0499_007:0655b07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又將此偈轉授世親。",
        "from": "X25n0499_007:0655b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世親又將偈作長行解。",
        "from": "X25n0499_007:0655b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋成三卷。",
        "from": "X25n0499_007:0655b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論約斷疑執以釋。",
        "from": "X25n0499_007:0655b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無著又造兩卷之論。",
        "from": "X25n0499_007:0655b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "約行位以釋。",
        "from": "X25n0499_007:0655b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鈔。",
        "from": "X25n0499_007:0655b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世親．無着。",
        "from": "X25n0499_007:0655b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本係親仲。",
        "from": "X25n0499_007:0655b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因世親專習小乘。",
        "from": "X25n0499_007:0655b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無著專習大乘。",
        "from": "X25n0499_007:0655b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無著未證果時。",
        "from": "X25n0499_007:0655b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "習學大乘。",
        "from": "X25n0499_007:0655b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "根鈍未悟。",
        "from": "X25n0499_007:0655b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲尋短見。",
        "from": "X25n0499_007:0655b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因有他方羅漢。",
        "from": "X25n0499_007:0655b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而化度之。",
        "from": "X25n0499_007:0655b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖入小乘果證。",
        "from": "X25n0499_007:0655b13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心本大乘之種。",
        "from": "X25n0499_007:0655b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還求大乘二空真知。",
        "from": "X25n0499_007:0655b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因見彌勒後。",
        "from": "X25n0499_007:0655b14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方悟入。",
        "from": "X25n0499_007:0655b14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無著因見親弟不好大乘。",
        "from": "X25n0499_007:0655b15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "專習小者。",
        "from": "X25n0499_007:0655b15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毀剝大乘。",
        "from": "X25n0499_007:0655b15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故無著見弟來見。",
        "from": "X25n0499_007:0655b16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告云。",
        "from": "X25n0499_007:0655b16_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾之病也。",
        "from": "X25n0499_007:0655b16_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為爾所害。",
        "from": "X25n0499_007:0655b16_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾專讚小乘。",
        "from": "X25n0499_007:0655b16_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毀說大乘。",
        "from": "X25n0499_007:0655b17_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必墮苦處。",
        "from": "X25n0499_007:0655b17_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是吾憂也。",
        "from": "X25n0499_007:0655b17_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今吾所未閱之大乘餘典。",
        "from": "X25n0499_007:0655b17_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾未觀者。",
        "from": "X25n0499_007:0655b18_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾代吾閱。",
        "from": "X25n0499_007:0655b18_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "親即生大慚愧。",
        "from": "X25n0499_007:0655b18_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速閱華嚴大乘等教。",
        "from": "X25n0499_007:0655b19_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又觀彌勒金剛偈頌畢。",
        "from": "X25n0499_007:0655b19_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即向兄求懺曰。",
        "from": "X25n0499_007:0655b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可將利劍割吾舌根。",
        "from": "X25n0499_007:0655b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾不欲命矣。",
        "from": "X25n0499_007:0655b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兄即止曰。",
        "from": "X25n0499_007:0655b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝不可為也。",
        "from": "X25n0499_007:0655b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如人從地跌倒。",
        "from": "X25n0499_007:0655b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還從地起。",
        "from": "X25n0499_007:0655b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先言毀說大乘之錯。",
        "from": "X25n0499_007:0655b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今可將舌根轉讚大乘。",
        "from": "X25n0499_007:0655b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即懺過愆。",
        "from": "X25n0499_007:0655b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而復其功也。",
        "from": "X25n0499_007:0655b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故世親依斯妙偈。",
        "from": "X25n0499_007:0655b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "裂成二十七段疑文。",
        "from": "X25n0499_007:0655b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "窮幽妙旨。",
        "from": "X25n0499_007:0655b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無不明矣。",
        "from": "X25n0499_007:0655b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無著謂此經發揮諸地位中。",
        "from": "X25n0499_007:0655b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真如意旨。",
        "from": "X25n0499_007:0655c01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故約十八行位而釋之也。",
        "from": "X25n0499_007:0655c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二結歎釋論二主。",
        "from": "X25n0499_007:0655c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是知古今解釋者。",
        "from": "X25n0499_007:0655c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不遵無著．世親二大士者。",
        "from": "X25n0499_007:0655c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱是傍求異解。",
        "from": "X25n0499_007:0655c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未合聖意耳。",
        "from": "X25n0499_007:0655c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鈔。",
        "from": "X25n0499_007:0655c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "決不可易古就今。",
        "from": "X25n0499_007:0655c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總然玄妙。",
        "from": "X25n0499_007:0655c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無非後撰之己見耳。",
        "from": "X25n0499_007:0655c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故云未合聖意也。",
        "from": "X25n0499_007:0655c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△三結。",
        "from": "X25n0499_007:0655c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以上處會傳譯一科已竟。",
        "from": "X25n0499_007:0655c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛般若波羅蜜經懸判疏鈔卷之七",
        "from": "X25n0499_007:0655c09_0",
        "context_with_next_sentence": false
    }
]