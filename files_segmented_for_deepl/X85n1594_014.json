[
    {
        "source_sentence": "佛祖綱目卷第十四(宿字號)",
        "from": "X85n1594_014:0577b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甲子(周赧王十八年起)丙午(秦昭襄王元年)壬子(秦莊襄王元年)乙卯(秦始皇元年)癸亥(秦始皇九年止)秦嬴姓。",
        "from": "X85n1594_014:0577b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起昭襄王丙午。",
        "from": "X85n1594_014:0577b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "止子嬰甲午。",
        "from": "X85n1594_014:0577b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共六主。",
        "from": "X85n1594_014:0577b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合四十九年。",
        "from": "X85n1594_014:0577b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(壬辰)十三祖迦毗摩羅傳法龍樹",
        "from": "X85n1594_014:0577b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦毗摩羅。",
        "from": "X85n1594_014:0577b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華氏國人。",
        "from": "X85n1594_014:0577b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初為外道。",
        "from": "X85n1594_014:0577b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有徒三千。",
        "from": "X85n1594_014:0577b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通諸異論。",
        "from": "X85n1594_014:0577b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後於馬鳴尊者。",
        "from": "X85n1594_014:0577b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得法領徒。",
        "from": "X85n1594_014:0577b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至西印度。",
        "from": "X85n1594_014:0577b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼有太子。",
        "from": "X85n1594_014:0577b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名雲自在。",
        "from": "X85n1594_014:0577b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仰摩羅名。",
        "from": "X85n1594_014:0577b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請於宮中供養。",
        "from": "X85n1594_014:0577b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩羅曰。",
        "from": "X85n1594_014:0577b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來有教。",
        "from": "X85n1594_014:0577b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙門不得親近國王大臣有勢之家。",
        "from": "X85n1594_014:0577b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雲自在曰。",
        "from": "X85n1594_014:0577b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今我國城之北。",
        "from": "X85n1594_014:0577b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大山焉。",
        "from": "X85n1594_014:0577b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "山中有一石窟。",
        "from": "X85n1594_014:0577b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師可禪寂於此否。",
        "from": "X85n1594_014:0577b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩羅曰諾。",
        "from": "X85n1594_014:0577b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即入彼山。",
        "from": "X85n1594_014:0577b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行數里。",
        "from": "X85n1594_014:0577b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逢一大蠎。",
        "from": "X85n1594_014:0577b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩羅直前。",
        "from": "X85n1594_014:0577b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不顧繞摩羅身。",
        "from": "X85n1594_014:0577b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩羅因與授三皈依。",
        "from": "X85n1594_014:0577b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蠎聽訖而去。",
        "from": "X85n1594_014:0577b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩羅將至石窟。",
        "from": "X85n1594_014:0577b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有一老人。",
        "from": "X85n1594_014:0577b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "素服而出。",
        "from": "X85n1594_014:0577b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌問訊。",
        "from": "X85n1594_014:0577b18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩羅曰。",
        "from": "X85n1594_014:0577b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝何所止。",
        "from": "X85n1594_014:0577b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "X85n1594_014:0577b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我昔甞為比丘。",
        "from": "X85n1594_014:0577b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多樂寂靜。",
        "from": "X85n1594_014:0577b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有初學比丘。",
        "from": "X85n1594_014:0577b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數來請益。",
        "from": "X85n1594_014:0577b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而我煩於應答。",
        "from": "X85n1594_014:0577b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起嗔恨想。",
        "from": "X85n1594_014:0577b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命終墮為蠎身。",
        "from": "X85n1594_014:0577b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住是窟中。",
        "from": "X85n1594_014:0577b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今已千載。",
        "from": "X85n1594_014:0577b21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "適遇尊者。",
        "from": "X85n1594_014:0577b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲聞戒法。",
        "from": "X85n1594_014:0577b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故來謝耳。",
        "from": "X85n1594_014:0577b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩羅曰。",
        "from": "X85n1594_014:0577b22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此山更有何人居止。",
        "from": "X85n1594_014:0577b22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰此去十里。",
        "from": "X85n1594_014:0577b22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大樹。",
        "from": "X85n1594_014:0577b22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蔭覆五百大龍。",
        "from": "X85n1594_014:0577b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其樹王名龍樹。",
        "from": "X85n1594_014:0577b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甞為龍眾說法。",
        "from": "X85n1594_014:0577b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦聽受耳。",
        "from": "X85n1594_014:0577b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩羅遂與徒眾詣彼。",
        "from": "X85n1594_014:0577b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍樹出迎曰深山孤寂。",
        "from": "X85n1594_014:0577b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍蟒所居。",
        "from": "X85n1594_014:0577c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大德至尊。",
        "from": "X85n1594_014:0577c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何枉神足。",
        "from": "X85n1594_014:0577c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰吾非至尊。",
        "from": "X85n1594_014:0577c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來訪賢者。",
        "from": "X85n1594_014:0577c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍樹默念。",
        "from": "X85n1594_014:0577c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此師得決定性。",
        "from": "X85n1594_014:0577c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明道眼否。",
        "from": "X85n1594_014:0577c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是大聖繼真乘否。",
        "from": "X85n1594_014:0577c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩羅曰。",
        "from": "X85n1594_014:0577c03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝雖心語。",
        "from": "X85n1594_014:0577c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我已意知。",
        "from": "X85n1594_014:0577c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但辦出家。",
        "from": "X85n1594_014:0577c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何慮我之不聖。",
        "from": "X85n1594_014:0577c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍樹聞已悔謝。",
        "from": "X85n1594_014:0577c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩羅即與度脫。",
        "from": "X85n1594_014:0577c04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及五百龍眾。",
        "from": "X85n1594_014:0577c04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱受具戒。",
        "from": "X85n1594_014:0577c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復告之曰。",
        "from": "X85n1594_014:0577c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今以如來大法眼藏。",
        "from": "X85n1594_014:0577c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "付囑於汝。",
        "from": "X85n1594_014:0577c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諦聽偈言。",
        "from": "X85n1594_014:0577c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非隱非顯法。",
        "from": "X85n1594_014:0577c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是真實際。",
        "from": "X85n1594_014:0577c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟此隱顯法。",
        "from": "X85n1594_014:0577c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非愚亦非智。",
        "from": "X85n1594_014:0577c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "付法已。",
        "from": "X85n1594_014:0577c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即現神變。",
        "from": "X85n1594_014:0577c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化火焚身。",
        "from": "X85n1594_014:0577c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍樹收五色舍利。",
        "from": "X85n1594_014:0577c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建塔焉當此土周赧王四十六年壬辰歲也○龍樹亦名龍勝。",
        "from": "X85n1594_014:0577c08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南天竺人。",
        "from": "X85n1594_014:0577c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乳哺中。",
        "from": "X85n1594_014:0577c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞諸梵志誦四韋馱。",
        "from": "X85n1594_014:0577c09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡四萬偈。",
        "from": "X85n1594_014:0577c10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆達句義。",
        "from": "X85n1594_014:0577c10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長而善知眾藝。",
        "from": "X85n1594_014:0577c10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天文地理星緯圖纖之屬。",
        "from": "X85n1594_014:0577c10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無不綜練。",
        "from": "X85n1594_014:0577c11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後因術罹禍。",
        "from": "X85n1594_014:0577c11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂悟慾為苦本。",
        "from": "X85n1594_014:0577c11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出家入石窟。",
        "from": "X85n1594_014:0577c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "棲止龍樹。",
        "from": "X85n1594_014:0577c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為龍眾所歸。",
        "from": "X85n1594_014:0577c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遇摩羅付法。",
        "from": "X85n1594_014:0577c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於九十日。",
        "from": "X85n1594_014:0577c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦通閻浮所有經書。",
        "from": "X85n1594_014:0577c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復念。",
        "from": "X85n1594_014:0577c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世界佛經雖妙。",
        "from": "X85n1594_014:0577c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "句義未盡。",
        "from": "X85n1594_014:0577c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當更為敷演。",
        "from": "X85n1594_014:0577c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開悟後學。",
        "from": "X85n1594_014:0577c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大龍菩薩。",
        "from": "X85n1594_014:0577c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便以神力。",
        "from": "X85n1594_014:0577c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "接入龍宮。",
        "from": "X85n1594_014:0577c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開七寶函。",
        "from": "X85n1594_014:0577c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與諸方等經典。",
        "from": "X85n1594_014:0577c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九十日中。",
        "from": "X85n1594_014:0577c15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通解甚多。",
        "from": "X85n1594_014:0577c15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍曰。",
        "from": "X85n1594_014:0577c16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今閱經。",
        "from": "X85n1594_014:0577c16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為徧未耶。",
        "from": "X85n1594_014:0577c16_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樹曰。",
        "from": "X85n1594_014:0577c16_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝經無量。",
        "from": "X85n1594_014:0577c16_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可得盡。",
        "from": "X85n1594_014:0577c16_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今所讀。",
        "from": "X85n1594_014:0577c17_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "足過閻浮十倍。",
        "from": "X85n1594_014:0577c17_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍曰。",
        "from": "X85n1594_014:0577c17_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忉利天上諸經。",
        "from": "X85n1594_014:0577c17_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復過此中。",
        "from": "X85n1594_014:0577c18_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百千萬倍。",
        "from": "X85n1594_014:0577c18_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樹於龍宮修行。",
        "from": "X85n1594_014:0577c18_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深入無生。",
        "from": "X85n1594_014:0577c18_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍送出宮。",
        "from": "X85n1594_014:0577c18_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是後大弘佛法。",
        "from": "X85n1594_014:0577c19_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造大無畏等論。",
        "from": "X85n1594_014:0577c19_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數十萬偈。",
        "from": "X85n1594_014:0577c19_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(中觀論是其一品。",
        "from": "X85n1594_014:0577c19_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即大智度論是)經云。",
        "from": "X85n1594_014:0577c20_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善逝涅槃後。",
        "from": "X85n1594_014:0577c20_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未來世當有。",
        "from": "X85n1594_014:0577c20_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南天竺比丘。",
        "from": "X85n1594_014:0577c20_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "厥號為龍樹。",
        "from": "X85n1594_014:0577c21_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能破有無宗。",
        "from": "X85n1594_014:0577c21_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯我大乘法。",
        "from": "X85n1594_014:0577c21_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得初歡喜地。",
        "from": "X85n1594_014:0577c21_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往生安養國○樹入龍宮。",
        "from": "X85n1594_014:0577c22_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "看藏。",
        "from": "X85n1594_014:0577c22_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見華嚴經。",
        "from": "X85n1594_014:0577c22_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有三本。",
        "from": "X85n1594_014:0577c22_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上本。",
        "from": "X85n1594_014:0577c23_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十三世界微塵數偈。",
        "from": "X85n1594_014:0577c23_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一四天下微塵數品。",
        "from": "X85n1594_014:0577c23_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中本。",
        "from": "X85n1594_014:0577c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四十九萬八千八百偈。",
        "from": "X85n1594_014:0577c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一千二百品。",
        "from": "X85n1594_014:0577c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下本。",
        "from": "X85n1594_014:0577c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十萬偈。",
        "from": "X85n1594_014:0577c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四十八品。",
        "from": "X85n1594_014:0578a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上本非佛不能知。",
        "from": "X85n1594_014:0578a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中本惟住地菩薩。",
        "from": "X85n1594_014:0578a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃能知之。",
        "from": "X85n1594_014:0578a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂記下本。",
        "from": "X85n1594_014:0578a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "回歸西土。",
        "from": "X85n1594_014:0578a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西土傳至震旦。",
        "from": "X85n1594_014:0578a02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八十卷經。",
        "from": "X85n1594_014:0578a02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十九品。",
        "from": "X85n1594_014:0578a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "品分為七處九會。",
        "from": "X85n1594_014:0578a03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一會菩提場。",
        "from": "X85n1594_014:0578a03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說六品十一卷。",
        "from": "X85n1594_014:0578a03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普賢為會主。",
        "from": "X85n1594_014:0578a04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二會普光明殿。",
        "from": "X85n1594_014:0578a04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說六品四卷。",
        "from": "X85n1594_014:0578a04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊為會主。",
        "from": "X85n1594_014:0578a04_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三會忉利天。",
        "from": "X85n1594_014:0578a05_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說六品三卷。",
        "from": "X85n1594_014:0578a05_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法慧為會主。",
        "from": "X85n1594_014:0578a05_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四會夜摩天。",
        "from": "X85n1594_014:0578a05_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說四品三卷。",
        "from": "X85n1594_014:0578a06_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功德林為會主。",
        "from": "X85n1594_014:0578a06_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五會兜率天。",
        "from": "X85n1594_014:0578a06_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說三品十二卷。",
        "from": "X85n1594_014:0578a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛幢為會主。",
        "from": "X85n1594_014:0578a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六會他化天。",
        "from": "X85n1594_014:0578a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說一品。",
        "from": "X85n1594_014:0578a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛藏為會主。",
        "from": "X85n1594_014:0578a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七重會普光明殿。",
        "from": "X85n1594_014:0578a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說十一品十三卷。",
        "from": "X85n1594_014:0578a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普賢與如來。",
        "from": "X85n1594_014:0578a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普賢品談平等因。",
        "from": "X85n1594_014:0578a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來出現品談平等果。",
        "from": "X85n1594_014:0578a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八三會普光明殿。",
        "from": "X85n1594_014:0578a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說一品七卷。",
        "from": "X85n1594_014:0578a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦普賢。",
        "from": "X85n1594_014:0578a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九會逝多林。",
        "from": "X85n1594_014:0578a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說一品二十一卷。",
        "from": "X85n1594_014:0578a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則如來與善友也。",
        "from": "X85n1594_014:0578a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此乃下本前分三萬六千偈。",
        "from": "X85n1594_014:0578a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尚有六萬四千偈。",
        "from": "X85n1594_014:0578a12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九品在西土。",
        "from": "X85n1594_014:0578a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛祖綱目卷第十四",
        "from": "X85n1594_014:0578a14_0",
        "context_with_next_sentence": false
    }
]