[
    {
        "source_sentence": "新菩薩經",
        "from": "T85n2917B_001:1462b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "率□□□□□□□大小念百萬口。",
        "from": "T85n2917B_001:1462b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿彌陀佛五百身中不入□□菩薩說今[(乖北+(刀*刀))/干]八[匚@出]九[匚@出]太山崩須九萬億□亦須九千億牛。",
        "from": "T85n2917B_001:1462b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一患死。",
        "from": "T85n2917B_001:1462b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二卒死。",
        "from": "T85n2917B_001:1462b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三產坐死。",
        "from": "T85n2917B_001:1462b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四不持齋死。",
        "from": "T85n2917B_001:1462b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五腸肚熱死。",
        "from": "T85n2917B_001:1462b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六自絞死。",
        "from": "T85n2917B_001:1462b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今[(乖北+(刀*刀))/干]禾荳熟恐無人無收度。",
        "from": "T85n2917B_001:1462b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今[(乖北+(刀*刀))/干]即好信者寫一通免身。",
        "from": "T85n2917B_001:1462b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寫二通免合家。",
        "from": "T85n2917B_001:1462b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寫三通免一村。",
        "from": "T85n2917B_001:1462b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若聞不寫即滅門。",
        "from": "T85n2917B_001:1462b20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有轉讀此經者。",
        "from": "T85n2917B_001:1462b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆度苦難。",
        "from": "T85n2917B_001:1462b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不信即身滅。",
        "from": "T85n2917B_001:1462b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經從西涼州縣來。",
        "from": "T85n2917B_001:1462b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在彼城東門口。",
        "from": "T85n2917B_001:1462b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四[匚@出]雷鳴雨下一石如斗。",
        "from": "T85n2917B_001:1462b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破兩片遂見此經。",
        "from": "T85n2917B_001:1462b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉示眾生得福無量。",
        "from": "T85n2917B_001:1462b23_7",
        "context_with_next_sentence": false
    }
]