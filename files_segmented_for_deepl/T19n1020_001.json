[
    {
        "source_sentence": "大方廣佛花嚴經入法界品頓證",
        "from": "T19n1020_001:0709b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘盧遮那法身字輪瑜伽儀軌",
        "from": "T19n1020_001:0709b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫欲頓入一乘修習毘盧遮那如來法身觀者。",
        "from": "T19n1020_001:0709b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先應發起普賢菩薩微妙行願。",
        "from": "T19n1020_001:0709b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復應以三密加持身心。",
        "from": "T19n1020_001:0709b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則能悟入文殊師利大智慧海。",
        "from": "T19n1020_001:0709b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然修行者最初於空閑處。",
        "from": "T19n1020_001:0709b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝念安心閉目端身結跏趺坐。",
        "from": "T19n1020_001:0709b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運心普緣無邊剎海。",
        "from": "T19n1020_001:0709b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諦觀三世一切如來遍於一一佛菩薩前。",
        "from": "T19n1020_001:0709b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慇勤恭敬禮拜旋遶。",
        "from": "T19n1020_001:0709b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以種種供具雲海。",
        "from": "T19n1020_001:0709b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉獻如是等一切眾聖。",
        "from": "T19n1020_001:0709b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣大供養已。",
        "from": "T19n1020_001:0709b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復應觀自心。",
        "from": "T19n1020_001:0709b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心本不生。",
        "from": "T19n1020_001:0709b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自性成就。",
        "from": "T19n1020_001:0709b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光明遍照猶如虛空。",
        "from": "T19n1020_001:0709b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復應深起悲念。",
        "from": "T19n1020_001:0709b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "哀愍眾生不悟自心輪迴諸趣。",
        "from": "T19n1020_001:0709b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當普化拔濟。",
        "from": "T19n1020_001:0709b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令其開悟盡無有餘。",
        "from": "T19n1020_001:0709b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復應觀察自心諸眾生心及諸佛心。",
        "from": "T19n1020_001:0709b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本無有異平等一相。",
        "from": "T19n1020_001:0709b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成大菩提心。",
        "from": "T19n1020_001:0709b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瑩徹清涼廓然周遍。",
        "from": "T19n1020_001:0709b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓明皎潔成大月輪。",
        "from": "T19n1020_001:0709b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "量等虛空無有邊際。",
        "from": "T19n1020_001:0709b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復應於月輪內。",
        "from": "T19n1020_001:0709b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋布列四十二梵字。",
        "from": "T19n1020_001:0709b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆金色放大光明。",
        "from": "T19n1020_001:0709b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照徹十方分明顯現。",
        "from": "T19n1020_001:0709b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一光中見無量剎海。",
        "from": "T19n1020_001:0709b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有無量諸佛。",
        "from": "T19n1020_001:0709b22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有無量眾前後圍遶。",
        "from": "T19n1020_001:0709b23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐菩提場成等正覺。",
        "from": "T19n1020_001:0709b23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智入三際身遍十方。",
        "from": "T19n1020_001:0709b24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉大法輪度脫群品。",
        "from": "T19n1020_001:0709b24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉令現證無住涅槃。",
        "from": "T19n1020_001:0709b25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復應悟入般若波羅蜜四十二字門。",
        "from": "T19n1020_001:0709b25_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了一切法皆無所得。",
        "from": "T19n1020_001:0709b26_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能觀正智所觀法界。",
        "from": "T19n1020_001:0709b26_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆平等無異無別。",
        "from": "T19n1020_001:0709b27_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修瑜伽者若能與是旋陀羅尼觀行相應。",
        "from": "T19n1020_001:0709b27_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即能現證毘盧遮那如來智身。",
        "from": "T19n1020_001:0709b28_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸法中得無障礙。",
        "from": "T19n1020_001:0709b29_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓明字輪",
        "from": "T19n1020_001:0709c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四十二字頌曰。",
        "from": "T19n1020_001:0709c14_0",
        "context_with_next_sentence": true
    },
