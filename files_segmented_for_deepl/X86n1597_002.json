[
    {
        "source_sentence": "布袋和尚傳",
        "from": "X86n1597_002:0048a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明州奉化布袋和尚。",
        "from": "X86n1597_002:0048a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不知何許人。",
        "from": "X86n1597_002:0048a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自稱契此。",
        "from": "X86n1597_002:0048a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未詳氏族。",
        "from": "X86n1597_002:0048a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形裁腲(烏罪切)脮(奴罪切)。",
        "from": "X86n1597_002:0048a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蹙額皤腹。",
        "from": "X86n1597_002:0048a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出語無定。",
        "from": "X86n1597_002:0048a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寢臥隨處。",
        "from": "X86n1597_002:0048a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常以杖荷一袋囊并破席。",
        "from": "X86n1597_002:0048a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡供身之具。",
        "from": "X86n1597_002:0048a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡貯囊中。",
        "from": "X86n1597_002:0048a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入廛肆聚落。",
        "from": "X86n1597_002:0048a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見物則乞。",
        "from": "X86n1597_002:0048a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或醯[醢右+(乞乙+口)]魚菹。",
        "from": "X86n1597_002:0048a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纔接入口。",
        "from": "X86n1597_002:0048a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分少許投囊中。",
        "from": "X86n1597_002:0048a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時號長汀子。",
        "from": "X86n1597_002:0048a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日。",
        "from": "X86n1597_002:0048a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有僧在師前行。",
        "from": "X86n1597_002:0048a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師乃拊其背。",
        "from": "X86n1597_002:0048a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧回首。",
        "from": "X86n1597_002:0048a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師曰。",
        "from": "X86n1597_002:0048a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乞我一文錢。",
        "from": "X86n1597_002:0048a22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰。",
        "from": "X86n1597_002:0048a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道得即與汝一文。",
        "from": "X86n1597_002:0048a22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師放下布袋。",
        "from": "X86n1597_002:0048a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "叉手而立。",
        "from": "X86n1597_002:0048a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白鹿和尚問。",
        "from": "X86n1597_002:0048a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如何是布袋。",
        "from": "X86n1597_002:0048a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師便放下布袋。",
        "from": "X86n1597_002:0048a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰。",
        "from": "X86n1597_002:0048a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如何是布袋下事。",
        "from": "X86n1597_002:0048a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師負之而去。",
        "from": "X86n1597_002:0048a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先保福和尚問。",
        "from": "X86n1597_002:0048a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如何是佛法大意。",
        "from": "X86n1597_002:0048b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師放下布袋叉手。",
        "from": "X86n1597_002:0048b01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福曰。",
        "from": "X86n1597_002:0048b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為祇如此。",
        "from": "X86n1597_002:0048b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為更有向上事。",
        "from": "X86n1597_002:0048b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師負之而去。",
        "from": "X86n1597_002:0048b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師在街衢立。",
        "from": "X86n1597_002:0048b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有僧問。",
        "from": "X86n1597_002:0048b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和尚在這裏作甚麼。",
        "from": "X86n1597_002:0048b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師曰。",
        "from": "X86n1597_002:0048b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "等箇人。",
        "from": "X86n1597_002:0048b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰。",
        "from": "X86n1597_002:0048b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來也。",
        "from": "X86n1597_002:0048b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來也。",
        "from": "X86n1597_002:0048b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸宗柔和尚別曰。",
        "from": "X86n1597_002:0048b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸去來。",
        "from": "X86n1597_002:0048b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師曰。",
        "from": "X86n1597_002:0048b04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝不是這箇人。",
        "from": "X86n1597_002:0048b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰。",
        "from": "X86n1597_002:0048b04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如何是這箇人。",
        "from": "X86n1597_002:0048b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師曰。",
        "from": "X86n1597_002:0048b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乞我一文錢。",
        "from": "X86n1597_002:0048b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師有歌曰。",
        "from": "X86n1597_002:0048b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祇箇心心心是佛。",
        "from": "X86n1597_002:0048b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方世界最靈物。",
        "from": "X86n1597_002:0048b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縱橫妙用可憐生。",
        "from": "X86n1597_002:0048b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切不如心真實。",
        "from": "X86n1597_002:0048b06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "騰騰自在無所為。",
        "from": "X86n1597_002:0048b07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "閑閑究竟出家兒。",
        "from": "X86n1597_002:0048b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若覩目前真大道。",
        "from": "X86n1597_002:0048b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不見纖毫也大奇。",
        "from": "X86n1597_002:0048b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬法何殊心何異。",
        "from": "X86n1597_002:0048b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何勞更用尋經義。",
        "from": "X86n1597_002:0048b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心王本自絕多知。",
        "from": "X86n1597_002:0048b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智者祇明無學地。",
        "from": "X86n1597_002:0048b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非聖非凡復若平。",
        "from": "X86n1597_002:0048b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不彊分別聖情孤。",
        "from": "X86n1597_002:0048b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無價心珠本圓淨。",
        "from": "X86n1597_002:0048b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡是異相妄空呼。",
        "from": "X86n1597_002:0048b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人能弘道道分明。",
        "from": "X86n1597_002:0048b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量清高稱道情。",
        "from": "X86n1597_002:0048b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "携錫若登故國路。",
        "from": "X86n1597_002:0048b12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫愁請處不聞聲。",
        "from": "X86n1597_002:0048b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又有偈曰。",
        "from": "X86n1597_002:0048b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是非憎愛世偏多。",
        "from": "X86n1597_002:0048b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "子細思量奈我何。",
        "from": "X86n1597_002:0048b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寬却肚腸須忍辱。",
        "from": "X86n1597_002:0048b13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豁開心地任從他。",
        "from": "X86n1597_002:0048b14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若逢知己須依分。",
        "from": "X86n1597_002:0048b14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縱遇冤家也共和。",
        "from": "X86n1597_002:0048b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能了此心頭事。",
        "from": "X86n1597_002:0048b15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然證得六波羅。",
        "from": "X86n1597_002:0048b15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我有一布袋。",
        "from": "X86n1597_002:0048b15_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虗空無罣礙。",
        "from": "X86n1597_002:0048b16_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "展開徧十方。",
        "from": "X86n1597_002:0048b16_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入時觀自在。",
        "from": "X86n1597_002:0048b16_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾有三寶堂。",
        "from": "X86n1597_002:0048b16_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "裏空無色相。",
        "from": "X86n1597_002:0048b17_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不高亦不低。",
        "from": "X86n1597_002:0048b17_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無遮亦無障。",
        "from": "X86n1597_002:0048b17_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "學者體不如。",
        "from": "X86n1597_002:0048b17_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來者難得樣。",
        "from": "X86n1597_002:0048b18_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智慧解安排。",
        "from": "X86n1597_002:0048b18_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千中無一匠。",
        "from": "X86n1597_002:0048b18_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四門四果生。",
        "from": "X86n1597_002:0048b18_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方盡供養。",
        "from": "X86n1597_002:0048b19_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾有一軀佛。",
        "from": "X86n1597_002:0048b19_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世人皆不識。",
        "from": "X86n1597_002:0048b19_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不塑亦不裝。",
        "from": "X86n1597_002:0048b19_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不雕亦不刻。",
        "from": "X86n1597_002:0048b20_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無一滴灰泥。",
        "from": "X86n1597_002:0048b20_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無一點彩色。",
        "from": "X86n1597_002:0048b20_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人畫畫不成。",
        "from": "X86n1597_002:0048b20_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賊偷偷不得。",
        "from": "X86n1597_002:0048b21_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體相本自然。",
        "from": "X86n1597_002:0048b21_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清淨非拂拭。",
        "from": "X86n1597_002:0048b21_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖然是一軀。",
        "from": "X86n1597_002:0048b21_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分身千百億。",
        "from": "X86n1597_002:0048b22_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又有偈曰。",
        "from": "X86n1597_002:0048b22_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一鉢千家飯。",
        "from": "X86n1597_002:0048b22_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孤身萬里遊。",
        "from": "X86n1597_002:0048b22_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "青目覩人少。",
        "from": "X86n1597_002:0048b23_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問路白雲頭。",
        "from": "X86n1597_002:0048b23_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梁貞明三年丙子三月。",
        "from": "X86n1597_002:0048b23_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師將示滅。",
        "from": "X86n1597_002:0048b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於岳林寺東廊下端坐磐石。",
        "from": "X86n1597_002:0048b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而說偈曰。",
        "from": "X86n1597_002:0048b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彌勒真彌勒。",
        "from": "X86n1597_002:0048b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分身千百億。",
        "from": "X86n1597_002:0048c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時時示時人。",
        "from": "X86n1597_002:0048c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時人自不識。",
        "from": "X86n1597_002:0048c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈畢安然而化。",
        "from": "X86n1597_002:0048c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其後復現于他州。",
        "from": "X86n1597_002:0048c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦負布袋而行。",
        "from": "X86n1597_002:0048c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四眾",
        "from": "X86n1597_002:0048c02_10",
        "context_with_next_sentence": false
    }
]