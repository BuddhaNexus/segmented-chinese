[
    {
        "source_sentence": "佛說灌頂三歸五戒帶佩護身呪",
        "from": "T21n1331_003:0501c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經卷第三",
        "from": "T21n1331_003:0501c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東晉天竺三藏帛尸梨蜜多羅譯",
        "from": "T21n1331_003:0501c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞如是。",
        "from": "T21n1331_003:0501c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在舍衛國祇樹給孤獨園時。",
        "from": "T21n1331_003:0501c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與千二百五十比丘菩薩萬人。",
        "from": "T21n1331_003:0501c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天龍八部悉來在會。",
        "from": "T21n1331_003:0501c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸然一心叉手聽法。",
        "from": "T21n1331_003:0501c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是異道有鹿頭梵志。",
        "from": "T21n1331_003:0501c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來到佛所稽首作禮。",
        "from": "T21n1331_003:0501c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "胡跪合掌白佛言。",
        "from": "T21n1331_003:0501c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "久聞瞿曇名聲遠振。",
        "from": "T21n1331_003:0501c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今欲捨置異學。",
        "from": "T21n1331_003:0501c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受三自歸并五戒法。",
        "from": "T21n1331_003:0501c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言善哉善哉梵志。",
        "from": "T21n1331_003:0501c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝能捨置餘道歸命我者。",
        "from": "T21n1331_003:0501c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當自悔過生死之罪。",
        "from": "T21n1331_003:0501c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其劫無量不可稱計。",
        "from": "T21n1331_003:0501c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵志言諾受教。",
        "from": "T21n1331_003:0501c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即淨身口意復作是念。",
        "from": "T21n1331_003:0501c16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願世尊施我法戒。",
        "from": "T21n1331_003:0501c17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終身奉行不敢毀缺。",
        "from": "T21n1331_003:0501c17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言是為如來至真等正覺三世諸佛說是戒法。",
        "from": "T21n1331_003:0501c18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言梵志諦聽諦受心持念之。",
        "from": "T21n1331_003:0501c19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又言梵志盡形壽歸命諸佛無上尊。",
        "from": "T21n1331_003:0501c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡形壽歸命法離欲尊。",
        "from": "T21n1331_003:0501c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡形壽歸命僧眾中尊。",
        "from": "T21n1331_003:0501c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言梵志以三自歸竟。",
        "from": "T21n1331_003:0501c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為真正弟子。",
        "from": "T21n1331_003:0501c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為邪惡之所忓嬈。",
        "from": "T21n1331_003:0501c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言梵志汝能一心受三自歸已。",
        "from": "T21n1331_003:0501c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當為汝及十方人。",
        "from": "T21n1331_003:0501c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勅天帝釋所遣諸鬼神。",
        "from": "T21n1331_003:0501c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以護男子女人輩受三歸者。",
        "from": "T21n1331_003:0501c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵志因問佛言。",
        "from": "T21n1331_003:0501c26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等是也願欲聞之。",
        "from": "T21n1331_003:0501c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開化十方諸受歸者。",
        "from": "T21n1331_003:0501c27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言如是灌頂善神。",
        "from": "T21n1331_003:0501c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今當為汝略說三十六。",
        "from": "T21n1331_003:0501c28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四天上遣神名彌栗頭不羅婆(漢言善光主疾病)四天上遣神名彌栗頭婆呵娑(漢言善明主頭痛)四天上遣神名彌栗頭婆邏波(漢言善力主寒熱)四天上遣神名彌栗頭栴陀羅(漢言善月主腹滿)四天上遣神名彌栗頭陀利奢(漢言善見主癰腫)四天上遣神名彌栗頭阿樓呵(漢言善供主癲狂)四天上遣神名彌栗頭伽娑婆帝(漢言善捨主愚癡)四天上遣神名彌栗頭悉坻哆(漢言善寂主瞋恚)四天上遣神名彌栗頭菩提薩(漢言善覺主婬欲)四天上遣神名彌栗頭提波羅(漢言善天主邪鬼)四天上遣神名彌栗頭呵波帝(漢言善住主傷亡)四天上遣神名彌栗頭不若羅(漢言善福主塚墓)四天上遣神名彌栗頭苾闍伽(漢言善術主四方)四天上遣神名彌栗頭伽麗娑(漢言善帝主怨家)四天上遣神名彌栗頭羅闍遮(漢言善主主偷盜)四天上遣神名彌栗頭修乾陀(漢言善香主債主)四天上遣神名彌栗頭檀那波(漢言善施主劫賊)四天上遣神名彌栗頭支多那(漢言善意主疫毒)四天上遣神名彌栗頭羅婆那(漢言善吉主五溫)四天上遣神名彌栗頭鉢婆馱(漢言善山主蜚尸)四天上遣神名彌栗頭三摩陀(漢言善調主注連)四天上遣神名彌栗頭戾禘馱(漢言善備主注腹)四天上遣神名彌栗頭波利陀(漢言善敬主相引)四天上遣神名彌栗頭波利那(漢言善淨主惡黨)四天上遣神名彌栗頭虔伽地(漢言善品主蠱毒)四天上遣神名彌栗頭毘梨馱(漢言善結主恐怖)四天上遣神名彌栗頭支陀那(漢言善壽主厄難)四天上遣神名彌栗頭伽林摩(漢言善遊主產乳)四天上遣神名彌栗頭阿留伽(漢言善願主縣官)四天上遣神名彌栗頭闍利馱(漢言善因主口舌)四天上遣神名彌栗頭阿伽馱(漢言善照主憂惱)四天上遣神名彌栗頭阿訶娑(漢言善生主不安)四天上遣神名彌栗頭娑和邏(漢言善至主百怪)四天上遣神名彌栗頭波利那(漢言善藏主嫉妬)四天上遣神名彌栗頭周陀那(漢言善音主呪咀)四天上遣神名彌栗頭韋陀羅(漢言善妙主厭禱)佛語梵志。",
        "from": "T21n1331_003:0501c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為三十六部神王。",
        "from": "T21n1331_003:0502b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此諸善神凡有萬億恒河沙鬼神。",
        "from": "T21n1331_003:0502b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為眷屬陰相番代。",
        "from": "T21n1331_003:0502b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以護男子女人等輩受三歸者。",
        "from": "T21n1331_003:0502b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當書神王名字帶在身上。",
        "from": "T21n1331_003:0502b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行來出入無所畏也。",
        "from": "T21n1331_003:0502b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辟除邪惡消滅不善。",
        "from": "T21n1331_003:0502b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵志言諾唯唯天中天。",
        "from": "T21n1331_003:0502b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵志又白佛言。",
        "from": "T21n1331_003:0502b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊以賜三自歸法。",
        "from": "T21n1331_003:0502b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天帝遣善神三十六大王護助我身。",
        "from": "T21n1331_003:0502b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已蒙世尊哀愍救度。",
        "from": "T21n1331_003:0502b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今更頂禮請受法戒。",
        "from": "T21n1331_003:0502b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言善哉梵志汝當淨身口意。",
        "from": "T21n1331_003:0502b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懇惻至心敬受法戒。",
        "from": "T21n1331_003:0502b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言十方三世如來至真等正覺。",
        "from": "T21n1331_003:0502b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆由三歸五戒得之。",
        "from": "T21n1331_003:0502b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言梵志盡形壽。",
        "from": "T21n1331_003:0502b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不殺生不教他殺。",
        "from": "T21n1331_003:0502b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是戒能持不。",
        "from": "T21n1331_003:0502b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能持有五神王隨逐護汝身。",
        "from": "T21n1331_003:0502b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不令邪神惡鬼之所得便。",
        "from": "T21n1331_003:0502b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵志盡形壽。",
        "from": "T21n1331_003:0502b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不盜他人財寶不教他行盜。",
        "from": "T21n1331_003:0502b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是戒能持不。",
        "from": "T21n1331_003:0502b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能持者。",
        "from": "T21n1331_003:0502b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有五善神王隨逐護汝身。",
        "from": "T21n1331_003:0502b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵志盡形壽不邪婬是戒能持不若能持者。",
        "from": "T21n1331_003:0502b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有五神王隨逐護汝身。",
        "from": "T21n1331_003:0502c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾魔皆不得便。",
        "from": "T21n1331_003:0502c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵志盡形壽。",
        "from": "T21n1331_003:0502c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不妄言綺語兩舌鬪亂。",
        "from": "T21n1331_003:0502c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是戒能持不。",
        "from": "T21n1331_003:0502c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能持者。",
        "from": "T21n1331_003:0502c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有五神王隨逐護汝身。",
        "from": "T21n1331_003:0502c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵志盡形壽。",
        "from": "T21n1331_003:0502c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不飲穀酒甘蔗酒葡萄酒能放逸酒。",
        "from": "T21n1331_003:0502c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等酒皆不得飲。",
        "from": "T21n1331_003:0502c04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是戒能持不。",
        "from": "T21n1331_003:0502c05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能持者。",
        "from": "T21n1331_003:0502c05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有五善神王隨逐護汝身。",
        "from": "T21n1331_003:0502c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語梵志。",
        "from": "T21n1331_003:0502c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為三歸五戒法也。",
        "from": "T21n1331_003:0502c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝善持之勿有毀犯。",
        "from": "T21n1331_003:0502c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說已。",
        "from": "T21n1331_003:0502c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵志因白佛言。",
        "from": "T21n1331_003:0502c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊說言若持五戒者。",
        "from": "T21n1331_003:0502c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有二十五善神。",
        "from": "T21n1331_003:0502c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衛護人身在人左右。",
        "from": "T21n1331_003:0502c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "守於宮宅門戶之上。",
        "from": "T21n1331_003:0502c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使萬事吉祥。",
        "from": "T21n1331_003:0502c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願世尊為我說之。",
        "from": "T21n1331_003:0502c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言梵志我今略演。",
        "from": "T21n1331_003:0502c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勅天帝釋使四天王。",
        "from": "T21n1331_003:0502c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遣諸善神營護汝身。",
        "from": "T21n1331_003:0502c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是章句善神名字。",
        "from": "T21n1331_003:0502c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十五王其名如是。",
        "from": "T21n1331_003:0502c13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神名蔡芻毘愈他尼主護人身辟除邪惡神名輸多利輸陀尼主護人六情悉令完具神名毘樓遮耶波主護人腹內五藏平調神名阿陀龍摩坻主護人血脈悉令通暢神名波羅桓尼和尼主護人爪指無所毀傷神名坻摩阿毘婆馱主護人出入行來安寧神名阿修輪婆羅陀主護人所噉飲食甘香神名婆羅摩亶雄雌主護人夢安覺歡悅神名婆羅門地鞞哆主護人不為虫毒所中神名那摩吁多耶舍主護人不為霧露惡毒所害神名佛馱仙陀樓多主護人鬪諍口舌不行神名鞞闍耶藪多娑主護人不為溫瘧鬼所持神名涅坻醯馱多耶主護人不為縣官所得神名阿邏多賴都耶主護人舍宅四方逐凶殃神名波羅那佛曇主護人平定舍宅八神神名阿提梵者珊耶主護人不為塚墓鬼所嬈神名因臺羅因臺羅主護人門戶辟除邪惡神名三摩毘羅尸陀主護人四大安隱無病(此神名丹本無)神名阿伽嵐施婆多主護人不為外氣鬼神害神名佛曇彌摩多哆主護人不為災火所近神名多賴叉三密陀主護人不為偷盜所侵神名阿摩羅斯兜嘻主護人若入山林不為虎狼所害神名那羅門闍兜帝主護人不為傷亡所嬈神名薩鞞尼乾那波主護人除諸鳥鳴狐鳴神名荼鞞闍毘舍羅主護人除犬鼠變怪神名加摩毘那闍尼佉主護人不為凶注所牽佛告梵志言。",
        "from": "T21n1331_003:0502c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若男子女人帶佩此二十五灌頂章句善神名者。",
        "from": "T21n1331_003:0503a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若入軍陣鬪諍之時。",
        "from": "T21n1331_003:0503a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀不傷身箭射不入。",
        "from": "T21n1331_003:0503a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼神羅剎終不嬈近。",
        "from": "T21n1331_003:0503a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若到蠱道家亦不能害。",
        "from": "T21n1331_003:0503a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若行來出入有小魔鬼亦不得近。",
        "from": "T21n1331_003:0503a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶佩此神王名者夜無惡夢。",
        "from": "T21n1331_003:0503a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縣官盜賊水火災怪。",
        "from": "T21n1331_003:0503a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怨家闇謀口舌鬪亂。",
        "from": "T21n1331_003:0503a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然歡喜兩作和解。",
        "from": "T21n1331_003:0503a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱生慈心惡意悉滅。",
        "from": "T21n1331_003:0503a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[(魅-未+夭)-ㄙ]魅魍魎邪忤薜荔。",
        "from": "T21n1331_003:0503a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外道符呪厭禱之者。",
        "from": "T21n1331_003:0503a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樹木精魅百蟲精魅。",
        "from": "T21n1331_003:0503a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳥狩精魅溪谷精魅。",
        "from": "T21n1331_003:0503a26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "門中鬼神戶中鬼神。",
        "from": "T21n1331_003:0503a26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "井竈鬼神洿池鬼神。",
        "from": "T21n1331_003:0503a27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廁溷中鬼一切諸鬼神。",
        "from": "T21n1331_003:0503a27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆不得留住某甲身中。",
        "from": "T21n1331_003:0503a28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若男子女人。",
        "from": "T21n1331_003:0503a28_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶此三歸五戒善神名字者。",
        "from": "T21n1331_003:0503a29_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "某甲入山陵溪谷。",
        "from": "T21n1331_003:0503a29_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曠路抄賊自然不現。",
        "from": "T21n1331_003:0503b01_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師子虎狼羆熊之屬。",
        "from": "T21n1331_003:0503b01_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉自藏縮不害人也。",
        "from": "T21n1331_003:0503b02_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告梵志。",
        "from": "T21n1331_003:0503b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔迦羅柰大國有婆羅門子名曰執持。",
        "from": "T21n1331_003:0503b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "富貴大姓不奉三寶。",
        "from": "T21n1331_003:0503b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "事九十五種之道以求福祐。",
        "from": "T21n1331_003:0503b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "久久之後聞其國中。",
        "from": "T21n1331_003:0503b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有賢善長者輩盡奉佛法聖僧化導。",
        "from": "T21n1331_003:0503b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆得富貴長壽安隱。",
        "from": "T21n1331_003:0503b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又能度脫生老病死受法無窮。",
        "from": "T21n1331_003:0503b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今世後世不入三惡道中。",
        "from": "T21n1331_003:0503b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執持長者作是念言。",
        "from": "T21n1331_003:0503b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不如捨置餘道奉敬三寶。",
        "from": "T21n1331_003:0503b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即便詣佛頭面著地。",
        "from": "T21n1331_003:0503b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為佛作禮長跪白佛言。",
        "from": "T21n1331_003:0503b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我本愚癡無所知識。",
        "from": "T21n1331_003:0503b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "久聞三寶不能奉事。",
        "from": "T21n1331_003:0503b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我於今日始得信解。",
        "from": "T21n1331_003:0503b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛法大慈普濟天下。",
        "from": "T21n1331_003:0503b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今欲捨餘道歸命於佛。",
        "from": "T21n1331_003:0503b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願天尊哀愍我等。",
        "from": "T21n1331_003:0503b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使得法戒為清信士。",
        "from": "T21n1331_003:0503b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言汝善思量之。",
        "from": "T21n1331_003:0503b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人能止惡為善者。",
        "from": "T21n1331_003:0503b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何憂不得安隱富貴。",
        "from": "T21n1331_003:0503b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽命延長解脫眾難者乎。",
        "from": "T21n1331_003:0503b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執持白佛言。",
        "from": "T21n1331_003:0503b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今我以所事非真故歸命於佛耳。",
        "from": "T21n1331_003:0503b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願哀愍去我濁穢之行。",
        "from": "T21n1331_003:0503b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受佛清淨決言。",
        "from": "T21n1331_003:0503b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語執持長者。",
        "from": "T21n1331_003:0503b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝審能爾者可禮敬三寶。",
        "from": "T21n1331_003:0503b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執持長者即便胡跪合掌禮佛。",
        "from": "T21n1331_003:0503b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是與世尊授三歸。",
        "from": "T21n1331_003:0503b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受三歸已當有三十六善神王。",
        "from": "T21n1331_003:0503b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨逐護汝身。",
        "from": "T21n1331_003:0503b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛復告言善男子汝能遠惡。",
        "from": "T21n1331_003:0503b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求善知識世之希有。",
        "from": "T21n1331_003:0503b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當更授汝五戒之法。",
        "from": "T21n1331_003:0503b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言第一不殺第二不盜第三不邪婬第四不兩舌惡口妄言綺語第五不飲酒。",
        "from": "T21n1331_003:0503b22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長者執持已受三歸及五戒竟。",
        "from": "T21n1331_003:0503b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語長者。",
        "from": "T21n1331_003:0503b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝能持是歸戒。",
        "from": "T21n1331_003:0503b25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遊行之處可無所畏。",
        "from": "T21n1331_003:0503b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戒神二十五歸神三十六。",
        "from": "T21n1331_003:0503b26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常隨護汝。",
        "from": "T21n1331_003:0503b26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外諸惡魔無敢當汝者。",
        "from": "T21n1331_003:0503b27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長者從佛受歸戒竟。",
        "from": "T21n1331_003:0503b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛為說法。",
        "from": "T21n1331_003:0503b28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡喜信解禮佛而去。",
        "from": "T21n1331_003:0503b28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是已後長者執持到他國中。",
        "from": "T21n1331_003:0503b28_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見人殺生盜人財物。",
        "from": "T21n1331_003:0503b29_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見好色女貪愛戀之。",
        "from": "T21n1331_003:0503c01_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見人好惡便論說之。",
        "from": "T21n1331_003:0503c01_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見人飲酒便欲追之。",
        "from": "T21n1331_003:0503c02_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心意如是無一時定。",
        "from": "T21n1331_003:0503c02_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便自念言。",
        "from": "T21n1331_003:0503c03_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悔從佛受三歸五戒重誓之法。",
        "from": "T21n1331_003:0503c03_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是念。",
        "from": "T21n1331_003:0503c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當還佛三歸五戒之法。",
        "from": "T21n1331_003:0503c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即詣佛所而白佛言。",
        "from": "T21n1331_003:0503c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前受三歸五戒之法。",
        "from": "T21n1331_003:0503c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多所禁制不得復從本意所作。",
        "from": "T21n1331_003:0503c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今自思惟欲罷。",
        "from": "T21n1331_003:0503c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能事佛可爾以不。",
        "from": "T21n1331_003:0503c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故佛法尊重非凡類所及。",
        "from": "T21n1331_003:0503c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可得還法戒不乎。",
        "from": "T21n1331_003:0503c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛默然不應。",
        "from": "T21n1331_003:0503c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言猶未絕口中便有自然鬼神。",
        "from": "T21n1331_003:0503c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持鐵椎拍長者頭。",
        "from": "T21n1331_003:0503c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有鬼神解脫其衣裳。",
        "from": "T21n1331_003:0503c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有鬼神以鐵鉤。",
        "from": "T21n1331_003:0503c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就其口中曳取其舌。",
        "from": "T21n1331_003:0503c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有婬女鬼神以刀割其陰。",
        "from": "T21n1331_003:0503c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有鬼神洋銅灌其口。",
        "from": "T21n1331_003:0503c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後左右有諸鬼神。",
        "from": "T21n1331_003:0503c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "競來分裂取其血肉而噉食之。",
        "from": "T21n1331_003:0503c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長者執持恐怖戰掉。",
        "from": "T21n1331_003:0503c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無所歸憑面如土色。",
        "from": "T21n1331_003:0503c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又有自然之火焚燒其身。",
        "from": "T21n1331_003:0503c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求生不得求死不得。",
        "from": "T21n1331_003:0503c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸鬼神輩急持長者不令得動。",
        "from": "T21n1331_003:0503c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛見如是哀而念之。",
        "from": "T21n1331_003:0503c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因問長者汝今當復云何。",
        "from": "T21n1331_003:0503c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長者口噤不能復言。",
        "from": "T21n1331_003:0503c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但得舉手自摶而已。",
        "from": "T21n1331_003:0503c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從佛求哀悔惡歸善。",
        "from": "T21n1331_003:0503c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛便以威神救度長者。",
        "from": "T21n1331_003:0503c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸鬼神王見佛世尊以威神力救度長者。",
        "from": "T21n1331_003:0503c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各各住立一面。",
        "from": "T21n1331_003:0503c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長者於是小得蘇息。",
        "from": "T21n1331_003:0503c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便起叩頭前白佛言。",
        "from": "T21n1331_003:0503c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我身中有是五賊。",
        "from": "T21n1331_003:0503c21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牽我入三惡道中。",
        "from": "T21n1331_003:0503c22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐欲作罪違負所受。",
        "from": "T21n1331_003:0503c22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願佛哀我受我懺悔。",
        "from": "T21n1331_003:0503c23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言汝。",
        "from": "T21n1331_003:0503c23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自心口所為當咎於誰長者白佛我從今日改往修來。",
        "from": "T21n1331_003:0503c23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉受三歸及五戒法。",
        "from": "T21n1331_003:0504a01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持月六齋年三長齋。",
        "from": "T21n1331_003:0504a01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒香散華懸雜幡蓋供事三寶。",
        "from": "T21n1331_003:0504a02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從今以去不敢復犯破歸戒法。",
        "from": "T21n1331_003:0504a02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言如汝今所言者是為大善。",
        "from": "T21n1331_003:0504a03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如汝眼所見身所更。",
        "from": "T21n1331_003:0504a04_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自作自得非天授與。",
        "from": "T21n1331_003:0504a04_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語長者汝今受是三歸五戒。",
        "from": "T21n1331_003:0504a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫復如前受歸戒法也。",
        "from": "T21n1331_003:0504a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破是歸戒名為再犯。",
        "from": "T21n1331_003:0504a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若三犯者為五官所得便。",
        "from": "T21n1331_003:0504a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輔王小臣都錄監伺五帝使者之所得便。",
        "from": "T21n1331_003:0504a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "收神錄命皆依本罪。",
        "from": "T21n1331_003:0504a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故我說是言。",
        "from": "T21n1331_003:0504a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令清信士女勸受歸戒。",
        "from": "T21n1331_003:0504a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸有三十六鬼神之王隨逐護助。",
        "from": "T21n1331_003:0504a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戒有二十五神。",
        "from": "T21n1331_003:0504a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "營護左右門戶之上辟除凶惡。",
        "from": "T21n1331_003:0504a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六天之上天帝所遣歸戒之神。",
        "from": "T21n1331_003:0504a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡有億億恒沙之數。",
        "from": "T21n1331_003:0504a13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸鬼神王番代擁護不令衰耗。",
        "from": "T21n1331_003:0504a13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天歡喜皆言善哉當共護之。",
        "from": "T21n1331_003:0504a14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是持戒若完具者。",
        "from": "T21n1331_003:0504a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方現在無量諸佛菩薩羅漢皆共稱譽。",
        "from": "T21n1331_003:0504a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是清信士女臨其終時。",
        "from": "T21n1331_003:0504a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛皆分身而往迎之。",
        "from": "T21n1331_003:0504a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不使持戒男子女人墮惡道中。",
        "from": "T21n1331_003:0504a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若戒羸者當益作福德。",
        "from": "T21n1331_003:0504a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "布施持戒忍辱精進一心智慧。",
        "from": "T21n1331_003:0504a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然燈燒香散雜色華。",
        "from": "T21n1331_003:0504a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懸繒幡蓋歌詠讚歎恭敬禮拜。",
        "from": "T21n1331_003:0504a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "益持齋戒亦得過度。",
        "from": "T21n1331_003:0504a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不能如上修行如是功德。",
        "from": "T21n1331_003:0504a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復持戒不完向諸邪道覓求福祐。",
        "from": "T21n1331_003:0504a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三歸五戒億億恒沙諸鬼神王各各離之。",
        "from": "T21n1331_003:0504a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡鬼數來而嬈近之。",
        "from": "T21n1331_003:0504a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因哀致病耗亂其家。",
        "from": "T21n1331_003:0504a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起諸病痛遂致喪亡。",
        "from": "T21n1331_003:0504a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "財物不聚所向不偶。",
        "from": "T21n1331_003:0504a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "死復還墮地獄之中。",
        "from": "T21n1331_003:0504a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖戒具足不持六齋。",
        "from": "T21n1331_003:0504a25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶華樹無果婦人不產。",
        "from": "T21n1331_003:0504a26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種穀不滋治生無利。",
        "from": "T21n1331_003:0504a26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "折耗失本吏無衣幘。",
        "from": "T21n1331_003:0504a27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不持齋戒無利如是。",
        "from": "T21n1331_003:0504a27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言長者。",
        "from": "T21n1331_003:0504a28_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人犯所受破是歸戒。",
        "from": "T21n1331_003:0504a28_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡為天上二百七十神王之所得便。",
        "from": "T21n1331_003:0504a28_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更非外魔之所得便。",
        "from": "T21n1331_003:0504a29_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此鬼神視人善惡。",
        "from": "T21n1331_003:0504b01_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若持禁戒不毀犯者。",
        "from": "T21n1331_003:0504b01_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開人心意示人善惡。",
        "from": "T21n1331_003:0504b02_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人若不善便為作害疏記善惡奏上大王。",
        "from": "T21n1331_003:0504b02_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大王執持隨罪輕重。",
        "from": "T21n1331_003:0504b03_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡其壽命如法苦治不令有怨。",
        "from": "T21n1331_003:0504b04_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使破戒者甘心受之。",
        "from": "T21n1331_003:0504b04_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告梵志。",
        "from": "T21n1331_003:0504b05_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長者執持捨彼異道。",
        "from": "T21n1331_003:0504b05_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於我法中受持歸戒。",
        "from": "T21n1331_003:0504b05_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心不安定而復破犯。",
        "from": "T21n1331_003:0504b06_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂為鬼神之所得便受諸苦痛。",
        "from": "T21n1331_003:0504b06_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今自悔責求哀懺謝。",
        "from": "T21n1331_003:0504b07_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "改更修善作諸福德。",
        "from": "T21n1331_003:0504b08_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅諸惡法今皆得道。",
        "from": "T21n1331_003:0504b08_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合家大小宗族之中。",
        "from": "T21n1331_003:0504b09_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見長者執持罪福報應。",
        "from": "T21n1331_003:0504b09_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉從我受三歸五戒。",
        "from": "T21n1331_003:0504b10_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅持不犯皆得法眼淨。",
        "from": "T21n1331_003:0504b10_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今於此會中。",
        "from": "T21n1331_003:0504b11_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣說長者宿命因緣明驗罪福。",
        "from": "T21n1331_003:0504b11_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "示於後世廣宣流布使得聞知。",
        "from": "T21n1331_003:0504b12_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語梵志若有清信士清信女。",
        "from": "T21n1331_003:0504b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若為邪神惡鬼所得便者。",
        "from": "T21n1331_003:0504b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若橫為縣官所羅。",
        "from": "T21n1331_003:0504b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盜賊剝奪。",
        "from": "T21n1331_003:0504b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遇大疾病厄難之日。",
        "from": "T21n1331_003:0504b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當洗浴身體。",
        "from": "T21n1331_003:0504b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "男子著單衣白帢女當素衣。",
        "from": "T21n1331_003:0504b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "澡漱口齒。",
        "from": "T21n1331_003:0504b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七日七夜長齋菜食。",
        "from": "T21n1331_003:0504b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敷好高座懸繒幡蓋。",
        "from": "T21n1331_003:0504b17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香汁灑地燒栴檀香。",
        "from": "T21n1331_003:0504b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日七轉讚詠此灌頂大章句。",
        "from": "T21n1331_003:0504b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是妙典至真祕藏。",
        "from": "T21n1331_003:0504b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "消滅一切無量災變。",
        "from": "T21n1331_003:0504b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵志白佛言。",
        "from": "T21n1331_003:0504b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天中天已能為我及十方眾生。",
        "from": "T21n1331_003:0504b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說三歸五戒鬼神名竟。",
        "from": "T21n1331_003:0504b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若男子女人欲受者當云何授與。",
        "from": "T21n1331_003:0504b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言若人欲受先禮十方佛。",
        "from": "T21n1331_003:0504b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長跪叉手作如是言。",
        "from": "T21n1331_003:0504b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我弟子某甲盡形壽受三歸五戒。",
        "from": "T21n1331_003:0504b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛菩薩真人聖眾哀念我等。",
        "from": "T21n1331_003:0504b23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵志又問受歸戒有差別不。",
        "from": "T21n1331_003:0504b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言無差別。",
        "from": "T21n1331_003:0504b25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人受者先當列三歸五戒之法。",
        "from": "T21n1331_003:0504b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後以神王名字著歸戒下。",
        "from": "T21n1331_003:0504b26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以好素帛書持此神王名字。",
        "from": "T21n1331_003:0504b26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶持而行。",
        "from": "T21n1331_003:0504b27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當燒香禮敬十方佛。",
        "from": "T21n1331_003:0504b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當取月八日七日持齋。",
        "from": "T21n1331_003:0504b28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲行來常著身上。",
        "from": "T21n1331_003:0504b28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若著頂上若著胸前。",
        "from": "T21n1331_003:0504b29_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若惡魔相逢無不除却。",
        "from": "T21n1331_003:0504b29_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若男子女人著此三歸五戒善神名字時。",
        "from": "T21n1331_003:0504c01_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若入神祠是諸邪神皆驚起為其人作禮。",
        "from": "T21n1331_003:0504c02_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故此人帶持諸佛所說三歸五戒神名字故。",
        "from": "T21n1331_003:0504c03_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語梵志。",
        "from": "T21n1331_003:0504c04_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此歸戒鬼神名字至尊至重。",
        "from": "T21n1331_003:0504c04_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛護念汝好宣行之。",
        "from": "T21n1331_003:0504c05_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說如是。",
        "from": "T21n1331_003:0504c05_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難從坐而起前白佛言。",
        "from": "T21n1331_003:0504c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "演說此法當何名之。",
        "from": "T21n1331_003:0504c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告阿難是經名為灌頂章句歸戒帶佩經。",
        "from": "T21n1331_003:0504c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說是已。",
        "from": "T21n1331_003:0504c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四眾人民天龍八部一切鬼神。",
        "from": "T21n1331_003:0504c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆大歡喜作禮奉行。",
        "from": "T21n1331_003:0504c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灌頂經卷第三",
        "from": "T21n1331_003:0504c10_0",
        "context_with_next_sentence": false
    }
]