[
    {
        "source_sentence": "佛祖綱目卷第十九(暑字號)",
        "from": "X85n1594_019:0581a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甲子(漢平帝元始四年起)丙寅(漢孺子嬰居攝元年)戊辰(改元初始)己巳(新莾建國元年)甲戌(新改天鳳)庚辰(新改地皇)癸未(漢更始元年)乙酉(東漢光武建武元年)丙辰(改元中元)戊午(明帝永平元年)癸亥(東漢明帝永平六年)(壬午)十九祖鳩摩羅多傳法闍夜多鳩摩羅多。",
        "from": "X85n1594_019:0581a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大月氏國婆羅門之子。",
        "from": "X85n1594_019:0581b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔為自在天人。",
        "from": "X85n1594_019:0581b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(欲界六天)見菩薩瓔珞。",
        "from": "X85n1594_019:0581b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忽起愛心。",
        "from": "X85n1594_019:0581b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "墮生忉利。",
        "from": "X85n1594_019:0581b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞憍尸迦說般若波羅蜜多。",
        "from": "X85n1594_019:0581b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以法勝故。",
        "from": "X85n1594_019:0581b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "升於梵天。",
        "from": "X85n1594_019:0581b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以根利故。",
        "from": "X85n1594_019:0581b04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善說法要。",
        "from": "X85n1594_019:0581b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天尊為導師。",
        "from": "X85n1594_019:0581b05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以繼祖時至。",
        "from": "X85n1594_019:0581b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂降月氏。",
        "from": "X85n1594_019:0581b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既得法已。",
        "from": "X85n1594_019:0581b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後至中天竺國。",
        "from": "X85n1594_019:0581b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大士名闍夜多。",
        "from": "X85n1594_019:0581b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "X85n1594_019:0581b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我家父母素信三寶。",
        "from": "X85n1594_019:0581b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而常縈疾瘵。",
        "from": "X85n1594_019:0581b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡所營作。",
        "from": "X85n1594_019:0581b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆不如意。",
        "from": "X85n1594_019:0581b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而我隣家久為旃陀羅行。",
        "from": "X85n1594_019:0581b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而身常勇徤。",
        "from": "X85n1594_019:0581b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所作和合。",
        "from": "X85n1594_019:0581b08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼何幸而我何辜。",
        "from": "X85n1594_019:0581b08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳩摩羅多曰。",
        "from": "X85n1594_019:0581b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何疑乎。",
        "from": "X85n1594_019:0581b09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且善惡之報。",
        "from": "X85n1594_019:0581b09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有三時焉。",
        "from": "X85n1594_019:0581b09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡人但見仁夭暴壽。",
        "from": "X85n1594_019:0581b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逆吉義凶。",
        "from": "X85n1594_019:0581b10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便謂亡因果虗罪福。",
        "from": "X85n1594_019:0581b10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殊不知影響相隨。",
        "from": "X85n1594_019:0581b10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毫釐靡忒。",
        "from": "X85n1594_019:0581b11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縱經百千萬劫。",
        "from": "X85n1594_019:0581b11_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不磨滅。",
        "from": "X85n1594_019:0581b11_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時闍夜多聞是語已。",
        "from": "X85n1594_019:0581b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頓釋所疑。",
        "from": "X85n1594_019:0581b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳩摩羅多曰。",
        "from": "X85n1594_019:0581b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝雖已信三業。",
        "from": "X85n1594_019:0581b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而未明業從惑生。",
        "from": "X85n1594_019:0581b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惑依識有。",
        "from": "X85n1594_019:0581b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識依不覺。",
        "from": "X85n1594_019:0581b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不覺依心。",
        "from": "X85n1594_019:0581b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心本清淨。",
        "from": "X85n1594_019:0581b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無生滅。",
        "from": "X85n1594_019:0581b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無造作。",
        "from": "X85n1594_019:0581b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無報應。",
        "from": "X85n1594_019:0581b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無勝負。",
        "from": "X85n1594_019:0581b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寂寂然靈靈然。",
        "from": "X85n1594_019:0581b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝若入此法門。",
        "from": "X85n1594_019:0581b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可與諸佛同矣。",
        "from": "X85n1594_019:0581b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切善惡有為無為。",
        "from": "X85n1594_019:0581b15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆如夢幻。",
        "from": "X85n1594_019:0581b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闍夜多承言領旨。",
        "from": "X85n1594_019:0581b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即發宿慧。",
        "from": "X85n1594_019:0581b16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懇求出家。",
        "from": "X85n1594_019:0581b16_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳩摩羅多曰。",
        "from": "X85n1594_019:0581b17_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝何許人。",
        "from": "X85n1594_019:0581b17_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可白父母而後出家。",
        "from": "X85n1594_019:0581b17_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜多曰。",
        "from": "X85n1594_019:0581b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我國北印度也。",
        "from": "X85n1594_019:0581b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "去此三千里。",
        "from": "X85n1594_019:0581b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然吾有小術。",
        "from": "X85n1594_019:0581b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往返亦易。",
        "from": "X85n1594_019:0581b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂行其術。",
        "from": "X85n1594_019:0581b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與羅多。",
        "from": "X85n1594_019:0581b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須臾達彼。",
        "from": "X85n1594_019:0581b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜多既聞其父母。",
        "from": "X85n1594_019:0581b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅多遂與受具。",
        "from": "X85n1594_019:0581b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後告之曰。",
        "from": "X85n1594_019:0581b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾今寂滅時至。",
        "from": "X85n1594_019:0581b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當紹行化跡。",
        "from": "X85n1594_019:0581b20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃付法眼偈曰。",
        "from": "X85n1594_019:0581b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性上本無生。",
        "from": "X85n1594_019:0581b21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為對求人說。",
        "from": "X85n1594_019:0581b21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於法既無得。",
        "from": "X85n1594_019:0581b21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何懷決不決。",
        "from": "X85n1594_019:0581b22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又云。",
        "from": "X85n1594_019:0581b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是玅音如來。",
        "from": "X85n1594_019:0581b22_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見性清淨之句。",
        "from": "X85n1594_019:0581b22_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝宜傳布後學。",
        "from": "X85n1594_019:0581b23_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言訖。",
        "from": "X85n1594_019:0581b23_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於座上。",
        "from": "X85n1594_019:0581b23_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以指爪剺面。",
        "from": "X85n1594_019:0581b23_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如紅蓮開。",
        "from": "X85n1594_019:0581b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出大光明。",
        "from": "X85n1594_019:0581b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照耀四眾。",
        "from": "X85n1594_019:0581b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而入寂滅。",
        "from": "X85n1594_019:0581b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闍夜多建塔。",
        "from": "X85n1594_019:0581b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以奉全身。",
        "from": "X85n1594_019:0581c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當此土新莾十四年壬午也○漢永平四年。",
        "from": "X85n1594_019:0581c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明帝夜夢金人。",
        "from": "X85n1594_019:0581c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身長丈餘。",
        "from": "X85n1594_019:0581c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "項佩日輪。",
        "from": "X85n1594_019:0581c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "胸題卍字。",
        "from": "X85n1594_019:0581c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飛行殿庭。",
        "from": "X85n1594_019:0581c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光明照耀。",
        "from": "X85n1594_019:0581c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旦問群臣。",
        "from": "X85n1594_019:0581c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "太史傅毅奏曰。",
        "from": "X85n1594_019:0581c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臣按周書異記。",
        "from": "X85n1594_019:0581c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昭王二十四年甲寅四月八日。",
        "from": "X85n1594_019:0581c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平旦之時。",
        "from": "X85n1594_019:0581c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "異風忽起。",
        "from": "X85n1594_019:0581c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宮殿屋舍悉皆震動。",
        "from": "X85n1594_019:0581c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有五色光氣。",
        "from": "X85n1594_019:0581c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入貫太微。",
        "from": "X85n1594_019:0581c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徧於四方。",
        "from": "X85n1594_019:0581c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡作青紅色。",
        "from": "X85n1594_019:0581c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王問太史蘇繇。",
        "from": "X85n1594_019:0581c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是何祥瑞。",
        "from": "X85n1594_019:0581c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對曰。",
        "from": "X85n1594_019:0581c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方有大聖人生。",
        "from": "X85n1594_019:0581c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王曰。",
        "from": "X85n1594_019:0581c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此何如。",
        "from": "X85n1594_019:0581c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對曰。",
        "from": "X85n1594_019:0581c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此時無他。",
        "from": "X85n1594_019:0581c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一千年後。",
        "from": "X85n1594_019:0581c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聲教被及。",
        "from": "X85n1594_019:0581c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王使鐫石記之。",
        "from": "X85n1594_019:0581c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南郊天祠前。",
        "from": "X85n1594_019:0581c08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以年計之。",
        "from": "X85n1594_019:0581c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至今辛酉。",
        "from": "X85n1594_019:0581c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一千一十年也。",
        "from": "X85n1594_019:0581c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陛下所夢。",
        "from": "X85n1594_019:0581c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將必是乎。",
        "from": "X85n1594_019:0581c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝深信之。",
        "from": "X85n1594_019:0581c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂遣中郎將蔡愔博士王遵秦景等十有八人。",
        "from": "X85n1594_019:0581c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至西域。",
        "from": "X85n1594_019:0581c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "訪求其道○佛知此震旦國眾生緣熟。",
        "from": "X85n1594_019:0581c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將來教化。",
        "from": "X85n1594_019:0581c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梓潼帝君。",
        "from": "X85n1594_019:0581c12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甞言。",
        "from": "X85n1594_019:0581c12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "予受業報。",
        "from": "X85n1594_019:0581c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為卭池龍。",
        "from": "X85n1594_019:0581c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羈於積水之下。",
        "from": "X85n1594_019:0581c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "連年旱虐。",
        "from": "X85n1594_019:0581c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水復為泥。",
        "from": "X85n1594_019:0581c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身既廣大。",
        "from": "X85n1594_019:0581c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無穴可容。",
        "from": "X85n1594_019:0581c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烈日上臨。",
        "from": "X85n1594_019:0581c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內外熱惱。",
        "from": "X85n1594_019:0581c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸鱗甲中。",
        "from": "X85n1594_019:0581c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各生小蟲。",
        "from": "X85n1594_019:0581c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咂囓困苦。",
        "from": "X85n1594_019:0581c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日晨凉。",
        "from": "X85n1594_019:0581c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天光忽開。",
        "from": "X85n1594_019:0581c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五色雲氣。",
        "from": "X85n1594_019:0581c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "浮光而過。",
        "from": "X85n1594_019:0581c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中有瑞相。",
        "from": "X85n1594_019:0581c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "紺髮螺旋。",
        "from": "X85n1594_019:0581c16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金容月瑩。",
        "from": "X85n1594_019:0581c16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "山靈河伯。",
        "from": "X85n1594_019:0581c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬眾稽首。",
        "from": "X85n1594_019:0581c16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讚歎歡喜。",
        "from": "X85n1594_019:0581c16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聲動天地。",
        "from": "X85n1594_019:0581c17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天香繚繞。",
        "from": "X85n1594_019:0581c17_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨處生春。",
        "from": "X85n1594_019:0581c17_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "予乃仰首。",
        "from": "X85n1594_019:0581c17_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "哀號乞垂。",
        "from": "X85n1594_019:0581c17_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救度萬靈。",
        "from": "X85n1594_019:0581c18_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸聖咸謂。",
        "from": "X85n1594_019:0581c18_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "予曰。",
        "from": "X85n1594_019:0581c18_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此西方大聖。",
        "from": "X85n1594_019:0581c18_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正覺世尊。",
        "from": "X85n1594_019:0581c18_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋迦文佛也。",
        "from": "X85n1594_019:0581c19_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今以教法。",
        "from": "X85n1594_019:0581c19_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流行東土。",
        "from": "X85n1594_019:0581c19_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨教化身。",
        "from": "X85n1594_019:0581c19_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將往中國。",
        "from": "X85n1594_019:0581c19_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾既遭逢。",
        "from": "X85n1594_019:0581c20_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宿業可脫。",
        "from": "X85n1594_019:0581c20_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "予乃身自踴躍。",
        "from": "X85n1594_019:0581c20_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入天光中。",
        "from": "X85n1594_019:0581c20_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具陳往昔報應之理。",
        "from": "X85n1594_019:0581c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊答言。",
        "from": "X85n1594_019:0581c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉帝子。",
        "from": "X85n1594_019:0581c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝於向來。",
        "from": "X85n1594_019:0581c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孝家忠國。",
        "from": "X85n1594_019:0581c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復憫世生護持心。",
        "from": "X85n1594_019:0581c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因果未周。",
        "from": "X85n1594_019:0581c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仇敵相爭。",
        "from": "X85n1594_019:0581c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以人我相。",
        "from": "X85n1594_019:0581c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "肆興殘忍。",
        "from": "X85n1594_019:0581c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遷怒於物。",
        "from": "X85n1594_019:0581c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "業債當償。",
        "from": "X85n1594_019:0581c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今復自悔。",
        "from": "X85n1594_019:0581c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲求解脫。",
        "from": "X85n1594_019:0581c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝於此時。",
        "from": "X85n1594_019:0581c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有冤親之想。",
        "from": "X85n1594_019:0581c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與夫嗔恚愚癡念否。",
        "from": "X85n1594_019:0581c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "予聞至理。",
        "from": "X85n1594_019:0582a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心地開明。",
        "from": "X85n1594_019:0582a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內外罄然。",
        "from": "X85n1594_019:0582a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如虗空住。",
        "from": "X85n1594_019:0582a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自顧其身。",
        "from": "X85n1594_019:0582a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨念消滅。",
        "from": "X85n1594_019:0582a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復為男子。",
        "from": "X85n1594_019:0582a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得灌頂智。",
        "from": "X85n1594_019:0582a02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "予皈依焉。",
        "from": "X85n1594_019:0582a02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛祖綱目卷第十九",
        "from": "X85n1594_019:0582a04_0",
        "context_with_next_sentence": false
    }
]