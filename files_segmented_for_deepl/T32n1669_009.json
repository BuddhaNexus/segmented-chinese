[
    {
        "source_sentence": "大宗地玄文本論卷第九",
        "from": "T32n1669_009:0683a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬鳴菩薩造",
        "from": "T32n1669_009:0683a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真諦三藏譯",
        "from": "T32n1669_009:0683a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "◎獨一無二山王自在道路大決擇分",
        "from": "T32n1669_009:0683a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十七",
        "from": "T32n1669_009:0683a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是已說獨地獨天一種廣大無二山王大決擇分。",
        "from": "T32n1669_009:0683a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說獨一無二山王自在道路大決擇分。",
        "from": "T32n1669_009:0683a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_009:0683a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_009:0683a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　自在道路中　　總有千重轉",
        "from": "T32n1669_009:0683a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　謂本上一中　　各各五百故",
        "from": "T32n1669_009:0683a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_009:0683a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就此無二山王自在道路分中。",
        "from": "T32n1669_009:0683a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總有千重差別轉相。",
        "from": "T32n1669_009:0683a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂本上中各各五百故。",
        "from": "T32n1669_009:0683a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈自在道路中總有千重轉。",
        "from": "T32n1669_009:0683a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂本上一中各各五百故。",
        "from": "T32n1669_009:0683a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故本轉形相當如何耶。",
        "from": "T32n1669_009:0683a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_009:0683a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　向本一下轉　　一空一有轉",
        "from": "T32n1669_009:0683a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　乃至第五百　　餘位亦如是",
        "from": "T32n1669_009:0683a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_009:0683b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此偈中為明何義。",
        "from": "T32n1669_009:0683b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲現示依本一門向下下轉。",
        "from": "T32n1669_009:0683b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有一金剛空一金剛。",
        "from": "T32n1669_009:0683b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如其次第一一現前。",
        "from": "T32n1669_009:0683b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漸漸轉入乃至第五百。",
        "from": "T32n1669_009:0683b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是亦復餘諸位中。",
        "from": "T32n1669_009:0683b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漸漸轉入乃至第五百。",
        "from": "T32n1669_009:0683b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有窮盡無有邊際無有始終。",
        "from": "T32n1669_009:0683b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足具足圓滿廣大常恒轉故。",
        "from": "T32n1669_009:0683b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈向本一下轉一空一有轉乃至第五百餘位亦如是故。",
        "from": "T32n1669_009:0683b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上轉形相例此應知。",
        "from": "T32n1669_009:0683b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因明性德修多羅中。",
        "from": "T32n1669_009:0683b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說。",
        "from": "T32n1669_009:0683b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無二一天父子法藏上上轉去。",
        "from": "T32n1669_009:0683b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有其始終無有窮盡。",
        "from": "T32n1669_009:0683b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下下轉入。",
        "from": "T32n1669_009:0683b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有其始終無有窮盡。",
        "from": "T32n1669_009:0683b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有始終者。",
        "from": "T32n1669_009:0683b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從信等位起至五百量故。",
        "from": "T32n1669_009:0683b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無窮盡者。",
        "from": "T32n1669_009:0683b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法藏之海極廣大故。",
        "from": "T32n1669_009:0683b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至廣說故。",
        "from": "T32n1669_009:0683b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩訶無二山王最勝高頂一地大決擇分第十八如是已說獨一無二山王自在道路大決擇分。",
        "from": "T32n1669_009:0683b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說摩訶無二山王最勝高頂一地大決擇分。",
        "from": "T32n1669_009:0683b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_009:0683b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_009:0683b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　本上無窮盡　　建立如是名",
        "from": "T32n1669_009:0683b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　所餘一切位　　亦如是應知",
        "from": "T32n1669_009:0683b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_009:0683b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此偈中為明何義。",
        "from": "T32n1669_009:0683b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲現示本本無窮上上無窮。",
        "from": "T32n1669_009:0683b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本上無窮上本無窮。",
        "from": "T32n1669_009:0683b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一無窮多多無窮。",
        "from": "T32n1669_009:0683b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同同無窮異異無窮。",
        "from": "T32n1669_009:0683b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "等等無窮別別無窮。",
        "from": "T32n1669_009:0683b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有窮無窮無窮。",
        "from": "T32n1669_009:0683b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無窮周遍廣大具足圓滿故。",
        "from": "T32n1669_009:0683b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈本上無窮盡建立如是名所餘一切位亦如是應知故。",
        "from": "T32n1669_009:0683b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩訶衍海修多羅中。",
        "from": "T32n1669_009:0683b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說。",
        "from": "T32n1669_009:0683b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪定摩訶衍海中。",
        "from": "T32n1669_009:0683b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一千二百無窮盡品。",
        "from": "T32n1669_009:0683b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足俱轉。",
        "from": "T32n1669_009:0683b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至廣說故。",
        "from": "T32n1669_009:0683b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大宗地玄文本論卷第九",
        "from": "T32n1669_009:0683b28_0",
        "context_with_next_sentence": false
    }
]