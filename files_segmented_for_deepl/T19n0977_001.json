[
    {
        "source_sentence": "佛說大白傘蓋總持陀羅尼經",
        "from": "T19n0977_001:0404a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元天竺俊辯大師唧[口*捺]銘得哩連得囉磨寧及譯主僧真智等譯敬禮一切最妙上師。",
        "from": "T19n0977_001:0404a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫欲修習白傘蓋佛母者。",
        "from": "T19n0977_001:0404a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寂靜室內。",
        "from": "T19n0977_001:0404a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於軟穩氈上坐已。",
        "from": "T19n0977_001:0404a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然發願云。",
        "from": "T19n0977_001:0404a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為六道一切有情。",
        "from": "T19n0977_001:0404a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於輪迴中令得解脫故。",
        "from": "T19n0977_001:0404a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願我成究竟正覺。",
        "from": "T19n0977_001:0404a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而發願已。",
        "from": "T19n0977_001:0404a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面前空中想白傘蓋佛會。",
        "from": "T19n0977_001:0404a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼等處以真實心念三歸依已。",
        "from": "T19n0977_001:0404a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛會消融為光。",
        "from": "T19n0977_001:0404a12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "融入自身自身成光。",
        "from": "T19n0977_001:0404a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後念莎末斡呪。",
        "from": "T19n0977_001:0404a13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想一切皆空。",
        "from": "T19n0977_001:0404a13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其空中華月輪上。",
        "from": "T19n0977_001:0404a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想白色唵字。",
        "from": "T19n0977_001:0404a14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵字放光。",
        "from": "T19n0977_001:0404a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其光復迴。",
        "from": "T19n0977_001:0404a14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "字種變成白傘金柄。",
        "from": "T19n0977_001:0404a15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "柄上嚴唵字。",
        "from": "T19n0977_001:0404a15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其字放光復回。",
        "from": "T19n0977_001:0404a15_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "字種變成白傘蓋佛母。",
        "from": "T19n0977_001:0404a16_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一面二臂具三目。",
        "from": "T19n0977_001:0404a16_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛跏趺而坐。",
        "from": "T19n0977_001:0404a17_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手作無怖畏印。",
        "from": "T19n0977_001:0404a17_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手執白傘當胸。",
        "from": "T19n0977_001:0404a17_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚴飾種種瓔珞。",
        "from": "T19n0977_001:0404a18_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身色潔白如雪山上日光明照。",
        "from": "T19n0977_001:0404a18_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具喜悅相顯無自性。",
        "from": "T19n0977_001:0404a19_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應觀如鏡中像然。",
        "from": "T19n0977_001:0404a19_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲誦呪時。",
        "from": "T19n0977_001:0404a20_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自己心中蓮華日輪上。",
        "from": "T19n0977_001:0404a20_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵字周圍繞心。",
        "from": "T19n0977_001:0404a21_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪及長短總持等於彼。",
        "from": "T19n0977_001:0404a21_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放光遣除自他一切罪障。",
        "from": "T19n0977_001:0404a21_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及間斷等想已然後讀誦。",
        "from": "T19n0977_001:0404a22_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若疲倦時欲奉施食。",
        "from": "T19n0977_001:0404a23_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則面前置施食。",
        "from": "T19n0977_001:0404a23_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念唵啞吽三字呪攝受。",
        "from": "T19n0977_001:0404a23_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變成甘露。",
        "from": "T19n0977_001:0404a24_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面前空中召請白傘蓋佛母為首。",
        "from": "T19n0977_001:0404a24_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并二十二山塚所居陰母。",
        "from": "T19n0977_001:0404a25_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及七種佛并十方正覺。",
        "from": "T19n0977_001:0404a26_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三種具美淨梵帝釋伴繞等已。",
        "from": "T19n0977_001:0404a26_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想舌變成金剛光筒。",
        "from": "T19n0977_001:0404a27_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦奉食呪曰。",
        "from": "T19n0977_001:0404a27_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩[口*栗]斡(二合)怛達遏哆烏室禰折席[口*捺]怛末嘚哩吽發(怛)[口*依]擔末哩渴渴渴兮渴兮誦三遍或五遍已。",
        "from": "T19n0977_001:0404a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然誦讚歎禱祝。",
        "from": "T19n0977_001:0404b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求索願事等畢。",
        "from": "T19n0977_001:0404b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉送佛會。",
        "from": "T19n0977_001:0404b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其施食棄於淨處。",
        "from": "T19n0977_001:0404b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "回向善根矣。",
        "from": "T19n0977_001:0404b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "啞呤耶怛達遏哆烏室禰折西[口*捺](引)怛末嘚哩捺麻啞末囉唧怛[口*捺]囉禰(梵語)聖一切如來頂髻中出白傘蓋佛母餘無能敵總持敬禮最上三寶(華言)。",
        "from": "T19n0977_001:0404b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T19n0977_001:0404b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時出有壞。",
        "from": "T19n0977_001:0404b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住三十三天善法妙好諸天所居之處。",
        "from": "T19n0977_001:0404b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大比丘并大菩提勇識及天主帝釋眾等集。",
        "from": "T19n0977_001:0404b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時出有壞坐蓮華座。",
        "from": "T19n0977_001:0404b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入於普觀頂髻三昧。",
        "from": "T19n0977_001:0404b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速然出有壞從頂髻中。",
        "from": "T19n0977_001:0404b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出現如是總持密呪法行。",
        "from": "T19n0977_001:0404b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮正覺及一切菩提勇識。",
        "from": "T19n0977_001:0404b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮正覺。",
        "from": "T19n0977_001:0404b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮妙法。",
        "from": "T19n0977_001:0404b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮大眾。",
        "from": "T19n0977_001:0404b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮七俱胝真實究竟正覺及聲聞大眾等。",
        "from": "T19n0977_001:0404b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮所有世間壞怨等。",
        "from": "T19n0977_001:0404b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮所有預流等。",
        "from": "T19n0977_001:0404b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮所有一來等。",
        "from": "T19n0977_001:0404b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮所有不還等。",
        "from": "T19n0977_001:0404b17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮所有世間真實超越等。",
        "from": "T19n0977_001:0404b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮所有入實者等。",
        "from": "T19n0977_001:0404b18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮天仙呪咀及有加祐力能等。",
        "from": "T19n0977_001:0404b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮所有誦持明呪獲成就者等。",
        "from": "T19n0977_001:0404b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮淨梵。",
        "from": "T19n0977_001:0404b20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮帝釋。",
        "from": "T19n0977_001:0404b20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮緊威具美能令退屈苦行之主者等。",
        "from": "T19n0977_001:0404b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮具美嚴五手印無愛子之所歸敬處。",
        "from": "T19n0977_001:0404b21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮具美能摧壞三層宮城住於墓地之中一切陰母所歸敬處。",
        "from": "T19n0977_001:0404b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮出有壞如來種佛。",
        "from": "T19n0977_001:0404b24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮蓮華種佛。",
        "from": "T19n0977_001:0404b24_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮金剛種佛。",
        "from": "T19n0977_001:0404b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮寶珠種佛。",
        "from": "T19n0977_001:0404b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮大象種佛。",
        "from": "T19n0977_001:0404b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮少童種佛。",
        "from": "T19n0977_001:0404b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮龍種佛。",
        "from": "T19n0977_001:0404b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮勇固部器械王佛。",
        "from": "T19n0977_001:0404b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮無量光佛。",
        "from": "T19n0977_001:0404b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮不動佛。",
        "from": "T19n0977_001:0404b27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮藥師瑠璃光王佛。",
        "from": "T19n0977_001:0404b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮娑羅主王華實圓滿佛。",
        "from": "T19n0977_001:0404b28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮釋迦牟尼佛。",
        "from": "T19n0977_001:0404b29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮寶上王佛。",
        "from": "T19n0977_001:0404b29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮最妙普賢佛。",
        "from": "T19n0977_001:0404b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮眾明主佛。",
        "from": "T19n0977_001:0404c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮目圓滿烏巴辣香上王佛。",
        "from": "T19n0977_001:0404c01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼等處敬禮已。",
        "from": "T19n0977_001:0404c02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出有壞母一切如來頂髻中出白傘蓋佛母餘無能敵大迴遮母。",
        "from": "T19n0977_001:0404c02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此決斷一切出者邪魔。",
        "from": "T19n0977_001:0404c04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能決斷餘者一切明呪。",
        "from": "T19n0977_001:0404c04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能迴遮非時橫夭。",
        "from": "T19n0977_001:0404c05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能令有情解脫一切繫縛。",
        "from": "T19n0977_001:0404c05_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能迴遮一切憎嫌惡夢。",
        "from": "T19n0977_001:0404c06_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能摧壞八萬四千邪魔。",
        "from": "T19n0977_001:0404c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能歡悅二十八宿。",
        "from": "T19n0977_001:0404c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能折伏八大房宿。",
        "from": "T19n0977_001:0404c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能迴遮一切冤讎。",
        "from": "T19n0977_001:0404c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能摧壞最極暴惡一切憎嫌惡夢。",
        "from": "T19n0977_001:0404c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能救度毒藥器械水火等難。",
        "from": "T19n0977_001:0404c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無有能敵大緊母　　大掇朴母大力母　大熾然母大威力　　大白蓋母大力母　熾然掛纓白衣母　　聖救度母具嗔皺　勝勢金剛稱念珠　　蓮華昭明金剛名　無有能敵具念珠　　金剛牆等摧壞母　柔善佛等供養母　　柔相威力具大母　聖救度母大力母　　不歿金剛鐵鋜母　金剛少童持種母　　金剛手種金念珠　大赤色及寶珠母　　種明金剛稱頂髻　種相窈窕金剛母　　如金色光具眼母　金剛燭及白色母　　蓮華眼及月光母手印聚彼等一切力故。",
        "from": "T19n0977_001:0404c11_0",
        "context_with_next_sentence": true
    },
