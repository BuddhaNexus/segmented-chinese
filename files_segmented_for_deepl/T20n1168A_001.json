[
    {
        "source_sentence": "佛說大乘八大曼拏羅經",
        "from": "T20n1168A_001:0676a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試光祿卿",
        "from": "T20n1168A_001:0676a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明教大師臣法賢奉　詔譯",
        "from": "T20n1168A_001:0676a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T20n1168A_001:0676a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在補陀落迦山聖觀自在菩薩宮中。",
        "from": "T20n1168A_001:0676a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有無數百千俱胝那由他菩薩恭敬圍繞。",
        "from": "T20n1168A_001:0676a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時會中有一菩薩名寶藏月光。",
        "from": "T20n1168A_001:0676a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即從座起偏袒右肩。",
        "from": "T20n1168A_001:0676a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右膝著地合掌向佛。",
        "from": "T20n1168A_001:0676b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而白佛言世尊如來應供正等覺。",
        "from": "T20n1168A_001:0676b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今有所問願佛慈愍斷我疑惑。",
        "from": "T20n1168A_001:0676b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告寶藏月光菩薩摩訶薩言。",
        "from": "T20n1168A_001:0676b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今問我樂聞之法。",
        "from": "T20n1168A_001:0676b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是義云何當為汝說。",
        "from": "T20n1168A_001:0676b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時寶藏月光菩薩摩訶薩。",
        "from": "T20n1168A_001:0676b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛語已而白佛言。",
        "from": "T20n1168A_001:0676b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊若有善男子善女人。",
        "from": "T20n1168A_001:0676b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲作八大曼拏羅。",
        "from": "T20n1168A_001:0676b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何受持恭敬供養。",
        "from": "T20n1168A_001:0676b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言善哉善哉。",
        "from": "T20n1168A_001:0676b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人。",
        "from": "T20n1168A_001:0676b08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此八大曼拏羅。",
        "from": "T20n1168A_001:0676b09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依法受持恭敬供養。",
        "from": "T20n1168A_001:0676b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大功德出生智慧利益無量。",
        "from": "T20n1168A_001:0676b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至三際眾生。",
        "from": "T20n1168A_001:0676b10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆得成就無等正智。",
        "from": "T20n1168A_001:0676b11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子汝今諦聽。",
        "from": "T20n1168A_001:0676b11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當為汝說此八大曼拏羅。",
        "from": "T20n1168A_001:0676b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具八大菩薩根本心大明。",
        "from": "T20n1168A_001:0676b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若常持誦恭敬供養。",
        "from": "T20n1168A_001:0676b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有五逆重罪。",
        "from": "T20n1168A_001:0676b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸惡悉皆除滅。",
        "from": "T20n1168A_001:0676b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而能成就無上勝義。",
        "from": "T20n1168A_001:0676b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大明曰。",
        "from": "T20n1168A_001:0676b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)摩賀(引)尾囉娑嚩(二合引)賀(引一)此是佛心大明。",
        "from": "T20n1168A_001:0676b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安曼拏羅中心。",
        "from": "T20n1168A_001:0676b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此大明獻佛供養。",
        "from": "T20n1168A_001:0676b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次聖觀自在菩薩心大明曰。",
        "from": "T20n1168A_001:0676b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)紇哩(二合)賀鉢捺摩(二合)室哩(二合)曳娑嚩(二合引)賀(引一)慈氏菩薩心大明曰。",
        "from": "T20n1168A_001:0676b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昧賀(引)囉尼娑嚩(二合引)賀(引一)虛空藏菩薩心大明曰。",
        "from": "T20n1168A_001:0676b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿誐哩婆(二合引)野娑嚩(二合引)賀(引一)普賢菩薩心大明曰。",
        "from": "T20n1168A_001:0676b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莎紇哩(二合)惹野娑嚩(二合引)賀(引一)金剛手菩薩心大明曰。",
        "from": "T20n1168A_001:0676b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱尾囉娑嚩(二合引)賀(引一)",
        "from": "T20n1168A_001:0676b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曼殊室哩菩薩心大明曰。",
        "from": "T20n1168A_001:0676c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "室哩(二合引)暗囉伽娑嚩(二合引)賀(引一)除蓋障菩薩心大明曰。",
        "from": "T20n1168A_001:0676c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儞娑囉嚩娑嚩(二合引)賀(引一)地藏菩薩心大明曰。",
        "from": "T20n1168A_001:0676c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "叱賀(引)囉娑嚩(二合引)賀(引一)如是八大菩薩根本心大明。",
        "from": "T20n1168A_001:0676c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如來曼拏羅。",
        "from": "T20n1168A_001:0676c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人。",
        "from": "T20n1168A_001:0676c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此曼拏羅依法受持。",
        "from": "T20n1168A_001:0676c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "志心持誦根本大明。",
        "from": "T20n1168A_001:0676c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼人速得成就阿耨多羅三藐三菩提。",
        "from": "T20n1168A_001:0676c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說大乘八大曼拏羅經",
        "from": "T20n1168A_001:0676c11_0",
        "context_with_next_sentence": false
    }
]