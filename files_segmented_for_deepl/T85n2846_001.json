[
    {
        "source_sentence": "祈願文",
        "from": "T85n2846_001:1298b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處結壇五日每壇供僧。",
        "from": "T85n2846_001:1298b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一七人散食燃燈。",
        "from": "T85n2846_001:1298b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逐夜唱大□□□□□。",
        "from": "T85n2846_001:1298b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　布壹疋。",
        "from": "T85n2846_001:1298b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "駝皮壹肋□□□□□□□□紙壹帖□□□□□□□□。",
        "from": "T85n2846_001:1298b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右件結壇轉經供僧唱佛燃燈捨施所申意□□□□□龍天八部護衛燉煌梵釋四王安民靜塞所□□□□□吉逐舊歲遠去他方應是嘉瑞嘉祥隨□□□□於□府中天。",
        "from": "T85n2846_001:1298b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝主□衛大猷□□□而歸京。",
        "from": "T85n2846_001:1298b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八表咸臻而献欵。",
        "from": "T85n2846_001:1298b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大王保位寵祿日新。",
        "from": "T85n2846_001:1298b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "等五岳而齊高。",
        "from": "T85n2846_001:1298b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比□□□不替天。",
        "from": "T85n2846_001:1298b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公主永泰竝桃李而芬芳。",
        "from": "T85n2846_001:1298b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫人又安以春花而恒茂□□□書俊□□□□之全才。",
        "from": "T85n2846_001:1298b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸幼郎君□□孝而許國。",
        "from": "T85n2846_001:1298b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小娘子姊妹丞妙治而居閨。",
        "from": "T85n2846_001:1298b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合宅枝羅保歡顏而納慶。",
        "from": "T85n2846_001:1298b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後□□□□□牧童賀　舜日之□□境康寧野老播　堯年之慶田祥。",
        "from": "T85n2846_001:1298b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五稼千門倍。",
        "from": "T85n2846_001:1298b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盈於東皐悉□□□廣收於南畝。",
        "from": "T85n2846_001:1298b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朝庭奉使早拜天顏。",
        "from": "T85n2846_001:1298b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所奏邊情果蒙。",
        "from": "T85n2846_001:1298b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖元時消疾□□勵莫狼刀□藏音狼煙羂罷滅今因壇散詣就場道渴□□□□□三尊幸希迴向。",
        "from": "T85n2846_001:1298b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長興二年十二月二十六日□□西歸義等軍",
        "from": "T85n2846_001:1298c03_0",
        "context_with_next_sentence": false
    }
]