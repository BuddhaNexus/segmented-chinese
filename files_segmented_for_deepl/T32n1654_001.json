[
    {
        "source_sentence": "因緣心論頌",
        "from": "T32n1654_001:0490a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍猛菩薩作",
        "from": "T32n1654_001:0490a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　差別十二支　　能仁說緣生",
        "from": "T32n1654_001:0490a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　於煩惱業苦　　三中俱攝盡",
        "from": "T32n1654_001:0490a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　初八九煩惱　　二及十是業",
        "from": "T32n1654_001:0490b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　餘七皆是苦　　十二唯三攝",
        "from": "T32n1654_001:0490b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　從三生於二　　從二生於七",
        "from": "T32n1654_001:0490b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一復生於三　　此有輪數轉",
        "from": "T32n1654_001:0490b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　諸趣唯因果　　此中無眾生",
        "from": "T32n1654_001:0490b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　唯從於空法　　還生於空法",
        "from": "T32n1654_001:0490b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　誦燈鏡及印　　火精種梅聲",
        "from": "T32n1654_001:0490b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　諸蘊相續結　　不移智應察",
        "from": "T32n1654_001:0490b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　於甚微細事　　若有見斷者",
        "from": "T32n1654_001:0490b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　彼不善因緣　　未見緣生義",
        "from": "T32n1654_001:0490b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　此中無可見　　亦無少安立",
        "from": "T32n1654_001:0490b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　於真以觀真　　見真而解脫",
        "from": "T32n1654_001:0490b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因緣心論釋",
        "from": "T32n1654_001:0490b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍猛菩薩造",
        "from": "T32n1654_001:0490b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中有沙門。",
        "from": "T32n1654_001:0490b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樂聞能聽。",
        "from": "T32n1654_001:0490b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善能憶持。",
        "from": "T32n1654_001:0490b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能悟。",
        "from": "T32n1654_001:0490b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能觀。",
        "from": "T32n1654_001:0490b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及具簡弃。",
        "from": "T32n1654_001:0490b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來詣師所。",
        "from": "T32n1654_001:0490b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於如來教中。",
        "from": "T32n1654_001:0490b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是問。",
        "from": "T32n1654_001:0490b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薄伽梵。",
        "from": "T32n1654_001:0490b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "差別十二支。",
        "from": "T32n1654_001:0490b21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能仁說緣生。",
        "from": "T32n1654_001:0490b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼於何所攝。",
        "from": "T32n1654_001:0490b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今欲樂聞。",
        "from": "T32n1654_001:0490b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知彼問其真義。",
        "from": "T32n1654_001:0490b22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師即呼曰汝於煩惱業苦三中都攝盡。",
        "from": "T32n1654_001:0490b22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作此分別典切之語。",
        "from": "T32n1654_001:0490b23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中十及二。",
        "from": "T32n1654_001:0490b24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故曰為十二支。",
        "from": "T32n1654_001:0490b24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即差別。",
        "from": "T32n1654_001:0490b24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故言差別。",
        "from": "T32n1654_001:0490b25_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如車支分。",
        "from": "T32n1654_001:0490b25_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故說為支。",
        "from": "T32n1654_001:0490b25_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能寂身口。",
        "from": "T32n1654_001:0490b25_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名能仁言。",
        "from": "T32n1654_001:0490b26_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能仁說者。",
        "from": "T32n1654_001:0490b26_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宣暢解釋。",
        "from": "T32n1654_001:0490b26_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說之異名。",
        "from": "T32n1654_001:0490b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼非自性。",
        "from": "T32n1654_001:0490b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "決定士夫。",
        "from": "T32n1654_001:0490b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "假相自在。",
        "from": "T32n1654_001:0490b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時自然隨欲。",
        "from": "T32n1654_001:0490b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化主。",
        "from": "T32n1654_001:0490b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偶遇等所生。",
        "from": "T32n1654_001:0490b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是因緣所生。",
        "from": "T32n1654_001:0490b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此差別十二支法。",
        "from": "T32n1654_001:0490b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於煩惱業苦。",
        "from": "T32n1654_001:0490b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遞手相依。",
        "from": "T32n1654_001:0490b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如束蘆。",
        "from": "T32n1654_001:0490c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彼三中。",
        "from": "T32n1654_001:0490c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並皆攝盡。",
        "from": "T32n1654_001:0490c01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言盡者。",
        "from": "T32n1654_001:0490c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是無餘義也。",
        "from": "T32n1654_001:0490c02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1654_001:0490c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何者煩惱。",
        "from": "T32n1654_001:0490c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何者是業。",
        "from": "T32n1654_001:0490c02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何者是苦。",
        "from": "T32n1654_001:0490c03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此差別之法。",
        "from": "T32n1654_001:0490c03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當於何攝。",
        "from": "T32n1654_001:0490c03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1654_001:0490c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初八九煩惱。",
        "from": "T32n1654_001:0490c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "差別十二支法。",
        "from": "T32n1654_001:0490c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初是無明。",
        "from": "T32n1654_001:0490c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八是愛。",
        "from": "T32n1654_001:0490c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第九是取。",
        "from": "T32n1654_001:0490c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此三是煩惱所攝。",
        "from": "T32n1654_001:0490c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何者是業。",
        "from": "T32n1654_001:0490c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二及十是業。",
        "from": "T32n1654_001:0490c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二是行。",
        "from": "T32n1654_001:0490c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十是有。",
        "from": "T32n1654_001:0490c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此二法是業所攝。",
        "from": "T32n1654_001:0490c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘七皆是苦。",
        "from": "T32n1654_001:0490c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煩惱業之所攝之餘七種是苦。",
        "from": "T32n1654_001:0490c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所攝應知。",
        "from": "T32n1654_001:0490c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂識。",
        "from": "T32n1654_001:0490c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名色。",
        "from": "T32n1654_001:0490c08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六入。",
        "from": "T32n1654_001:0490c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觸。",
        "from": "T32n1654_001:0490c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受。",
        "from": "T32n1654_001:0490c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生。",
        "from": "T32n1654_001:0490c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "老死。",
        "from": "T32n1654_001:0490c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言皆者是惣攝之辭。",
        "from": "T32n1654_001:0490c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即攝愛別離。",
        "from": "T32n1654_001:0490c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怨憎會。",
        "from": "T32n1654_001:0490c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求不得苦。",
        "from": "T32n1654_001:0490c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故此十二支法。",
        "from": "T32n1654_001:0490c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於業煩惱苦中。",
        "from": "T32n1654_001:0490c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並皆攝盡。",
        "from": "T32n1654_001:0490c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言唯者。",
        "from": "T32n1654_001:0490c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是其遮義。",
        "from": "T32n1654_001:0490c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經中所說之法。",
        "from": "T32n1654_001:0490c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中攝盡。",
        "from": "T32n1654_001:0490c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更無有餘。",
        "from": "T32n1654_001:0490c12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1654_001:0490c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此義已知。",
        "from": "T32n1654_001:0490c13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼煩惱業苦。",
        "from": "T32n1654_001:0490c13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何相生。",
        "from": "T32n1654_001:0490c13_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請為解說。",
        "from": "T32n1654_001:0490c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1654_001:0490c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從三生於二。",
        "from": "T32n1654_001:0490c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從三煩惱生於二業。",
        "from": "T32n1654_001:0490c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從二生於七。",
        "from": "T32n1654_001:0490c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂上所說苦法。",
        "from": "T32n1654_001:0490c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七復生於三。",
        "from": "T32n1654_001:0490c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂諸煩惱。",
        "from": "T32n1654_001:0490c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復從三煩惱。",
        "from": "T32n1654_001:0490c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生於二業。",
        "from": "T32n1654_001:0490c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此有輪數轉。",
        "from": "T32n1654_001:0490c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言有者。",
        "from": "T32n1654_001:0490c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有其三種。",
        "from": "T32n1654_001:0490c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂欲。",
        "from": "T32n1654_001:0490c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色。",
        "from": "T32n1654_001:0490c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無色。",
        "from": "T32n1654_001:0490c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中不息。",
        "from": "T32n1654_001:0490c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而作流轉。",
        "from": "T32n1654_001:0490c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼諸異生世間。",
        "from": "T32n1654_001:0490c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而自流轉浪。",
        "from": "T32n1654_001:0490c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言此者。",
        "from": "T32n1654_001:0490c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯不定義。",
        "from": "T32n1654_001:0490c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非如流轉。",
        "from": "T32n1654_001:0490c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次第生於諸有。",
        "from": "T32n1654_001:0490c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此不定也。",
        "from": "T32n1654_001:0490c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1654_001:0490c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何者是身之自在眾生耶。",
        "from": "T32n1654_001:0490c20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼之作用。",
        "from": "T32n1654_001:0490c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其事云何。",
        "from": "T32n1654_001:0490c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1654_001:0490c21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸趣惟因果。",
        "from": "T32n1654_001:0490c21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟除假名。",
        "from": "T32n1654_001:0490c22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中無眾生。",
        "from": "T32n1654_001:0490c22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是真實義。",
        "from": "T32n1654_001:0490c22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非假立有。",
        "from": "T32n1654_001:0490c23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "假立之境。",
        "from": "T32n1654_001:0490c23_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不成實物。",
        "from": "T32n1654_001:0490c23_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1654_001:0490c23_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若如是者。",
        "from": "T32n1654_001:0490c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誰從此世至於他世。",
        "from": "T32n1654_001:0490c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1654_001:0490c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有極微等法。",
        "from": "T32n1654_001:0490c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從於此世。",
        "from": "T32n1654_001:0490c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "移至他世。",
        "from": "T32n1654_001:0490c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖然唯從於空法。",
        "from": "T32n1654_001:0490c25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還生於空法。",
        "from": "T32n1654_001:0490c26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從無我我所。",
        "from": "T32n1654_001:0490c26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煩惱業。",
        "from": "T32n1654_001:0490c26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五種空因。",
        "from": "T32n1654_001:0490c26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還生空無我我所。",
        "from": "T32n1654_001:0490c27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七種苦果之法。",
        "from": "T32n1654_001:0490c27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼則無我我所。",
        "from": "T32n1654_001:0490c27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼此手無我我所。",
        "from": "T32n1654_001:0490c28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖然從自性無我之法。",
        "from": "T32n1654_001:0490c28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還生自性無我之法。",
        "from": "T32n1654_001:0490c29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應如是知。",
        "from": "T32n1654_001:0490c29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說。",
        "from": "T32n1654_001:0490c29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中問曰。",
        "from": "T32n1654_001:0491a01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從自性無我之法。",
        "from": "T32n1654_001:0491a01_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還生自性無我之法者。",
        "from": "T32n1654_001:0491a01_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有何譬喻。",
        "from": "T32n1654_001:0491a02_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中答曰。",
        "from": "T32n1654_001:0491a02_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦燈鏡及印。",
        "from": "T32n1654_001:0491a02_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火精種梅聲。",
        "from": "T32n1654_001:0491a03_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已如是等喻。",
        "from": "T32n1654_001:0491a03_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及假喻立成自性無我及成就彼世應知。",
        "from": "T32n1654_001:0491a03_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如師所誦者。",
        "from": "T32n1654_001:0491a04_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若轉至弟子。",
        "from": "T32n1654_001:0491a05_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師後更無言說。",
        "from": "T32n1654_001:0491a05_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故不至。",
        "from": "T32n1654_001:0491a05_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼弟子誦者。",
        "from": "T32n1654_001:0491a06_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不從餘得。",
        "from": "T32n1654_001:0491a06_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成無因果故。",
        "from": "T32n1654_001:0491a06_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如師所誦臨終心識。",
        "from": "T32n1654_001:0491a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦復如是。",
        "from": "T32n1654_001:0491a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成常過故。",
        "from": "T32n1654_001:0491a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不至他世。",
        "from": "T32n1654_001:0491a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼世亦不從餘得。",
        "from": "T32n1654_001:0491a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成無因果故。",
        "from": "T32n1654_001:0491a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如師所誦。",
        "from": "T32n1654_001:0491a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與弟子誦者。",
        "from": "T32n1654_001:0491a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即彼異彼。",
        "from": "T32n1654_001:0491a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不易施設。",
        "from": "T32n1654_001:0491a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是依彼臨終心識。",
        "from": "T32n1654_001:0491a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生分心識。",
        "from": "T32n1654_001:0491a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得生者。",
        "from": "T32n1654_001:0491a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦復如是。",
        "from": "T32n1654_001:0491a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即彼異彼。",
        "from": "T32n1654_001:0491a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不易施設。",
        "from": "T32n1654_001:0491a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是從燈生燈依於面像。",
        "from": "T32n1654_001:0491a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鏡中現其影像。",
        "from": "T32n1654_001:0491a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從印成文。",
        "from": "T32n1654_001:0491a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從精出火。",
        "from": "T32n1654_001:0491a12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從種生芽。",
        "from": "T32n1654_001:0491a13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從梅生涎。",
        "from": "T32n1654_001:0491a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從聲出嚮。",
        "from": "T32n1654_001:0491a13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即彼異彼。",
        "from": "T32n1654_001:0491a13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不易施設。",
        "from": "T32n1654_001:0491a14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是。",
        "from": "T32n1654_001:0491a14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸蘊相續結。",
        "from": "T32n1654_001:0491a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不移智應察。",
        "from": "T32n1654_001:0491a14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言蘊者。",
        "from": "T32n1654_001:0491a15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即色受相行識蘊也。",
        "from": "T32n1654_001:0491a15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言相續結者。",
        "from": "T32n1654_001:0491a15_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼已從彼因。",
        "from": "T32n1654_001:0491a16_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所生餘者是也。",
        "from": "T32n1654_001:0491a16_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有極微等法。",
        "from": "T32n1654_001:0491a16_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從於此世。",
        "from": "T32n1654_001:0491a17_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "移至他世。",
        "from": "T32n1654_001:0491a17_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故流轉。",
        "from": "T32n1654_001:0491a17_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從於虛妄分別習氣而生。",
        "from": "T32n1654_001:0491a18_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後言應者。",
        "from": "T32n1654_001:0491a18_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是逆觀。",
        "from": "T32n1654_001:0491a18_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義當知反。",
        "from": "T32n1654_001:0491a19_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼應觀諸法無常苦空無我者。",
        "from": "T32n1654_001:0491a19_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則不愚諸事。",
        "from": "T32n1654_001:0491a20_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不遇者。",
        "from": "T32n1654_001:0491a20_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則無有貪。",
        "from": "T32n1654_001:0491a20_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無有貪瞋則不生。",
        "from": "T32n1654_001:0491a20_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無有嗔則無有業。",
        "from": "T32n1654_001:0491a21_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無有業。",
        "from": "T32n1654_001:0491a21_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則無有取。",
        "from": "T32n1654_001:0491a22_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無有取。",
        "from": "T32n1654_001:0491a22_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則不造後有。",
        "from": "T32n1654_001:0491a22_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無有者而則不生。",
        "from": "T32n1654_001:0491a22_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不生者。",
        "from": "T32n1654_001:0491a23_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於身心而苦不生。",
        "from": "T32n1654_001:0491a23_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是不集五種因故。",
        "from": "T32n1654_001:0491a24_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即相餘處。",
        "from": "T32n1654_001:0491a24_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而果不生。",
        "from": "T32n1654_001:0491a24_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此果解脫。",
        "from": "T32n1654_001:0491a25_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故斷除。",
        "from": "T32n1654_001:0491a25_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷常等諸惡見也。",
        "from": "T32n1654_001:0491a25_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中有二訟。",
        "from": "T32n1654_001:0491a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　於甚微細事　　若有見斷者",
        "from": "T32n1654_001:0491a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　彼不善因緣　　未見緣生義",
        "from": "T32n1654_001:0491a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　此中無可見　　亦無少安立",
        "from": "T32n1654_001:0491a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　於真以觀真　　見真如解脫",
        "from": "T32n1654_001:0491b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因緣心論釋一卷",
        "from": "T32n1654_001:0491b02_0",
        "context_with_next_sentence": false
    }
]