[
    {
        "source_sentence": "八名普密陀羅尼經",
        "from": "T21n1365_001:0883c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大唐三藏法師玄奘奉　詔譯",
        "from": "T21n1365_001:0883c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T21n1365_001:0883c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時薄伽梵。",
        "from": "T21n1365_001:0883c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在室羅筏住誓多林給孤獨園。",
        "from": "T21n1365_001:0883c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大苾芻眾千二百五十人俱。",
        "from": "T21n1365_001:0883c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及無量無數菩薩摩訶薩。",
        "from": "T21n1365_001:0883c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并諸天人阿素洛等。",
        "from": "T21n1365_001:0883c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "異類大眾前後圍遶。",
        "from": "T21n1365_001:0883c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告金剛手菩薩言。",
        "from": "T21n1365_001:0883c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子汝所受持諸上明。",
        "from": "T21n1365_001:0883c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪神用威猛功業難成。",
        "from": "T21n1365_001:0883c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖後為益或初暫損。",
        "from": "T21n1365_001:0883c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今有八名普密神呪。",
        "from": "T21n1365_001:0883c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "威德廣大事業易成。",
        "from": "T21n1365_001:0883c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神用祕密始終無損。",
        "from": "T21n1365_001:0883c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能受持者必獲利樂。",
        "from": "T21n1365_001:0883c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當為汝說汝樂聞不。",
        "from": "T21n1365_001:0883c25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時金剛手歡喜踊躍。",
        "from": "T21n1365_001:0883c25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂禮佛足右跪合掌。",
        "from": "T21n1365_001:0883c26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請言世尊唯願為說。",
        "from": "T21n1365_001:0883c26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言諦聽極善思惟。",
        "from": "T21n1365_001:0883c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾今為汝分別演說。",
        "from": "T21n1365_001:0883c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何謂八名普密神呪。",
        "from": "T21n1365_001:0883c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名功德寶藏。",
        "from": "T21n1365_001:0883c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二名莊嚴象耳。",
        "from": "T21n1365_001:0883c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三名善勇猛。",
        "from": "T21n1365_001:0883c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四名勝諦雲。",
        "from": "T21n1365_001:0883c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五名成熾然。",
        "from": "T21n1365_001:0883c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六名微妙色。",
        "from": "T21n1365_001:0884a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七名嚴飾。",
        "from": "T21n1365_001:0884a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八名金剛。",
        "from": "T21n1365_001:0884a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有得聞此八名呪。",
        "from": "T21n1365_001:0884a01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於當來世經七俱胝那庾多百千大劫。",
        "from": "T21n1365_001:0884a02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不墮地獄傍生餓鬼。",
        "from": "T21n1365_001:0884a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將命終時身心安隱。",
        "from": "T21n1365_001:0884a03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見有諸佛及諸菩薩來現其前。",
        "from": "T21n1365_001:0884a04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為說大乘甚深法要。",
        "from": "T21n1365_001:0884a04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既聞法已。",
        "from": "T21n1365_001:0884a05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必得往生覩史多天奉事彌勒。",
        "from": "T21n1365_001:0884a05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後隨彌勒下贍部洲。",
        "from": "T21n1365_001:0884a06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行願漸增乃至究竟。",
        "from": "T21n1365_001:0884a06_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陀羅尼曰。",
        "from": "T21n1365_001:0884a07_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頞[齒*來](擿界反已下同)筏[齒*來]捺[齒*來]朅剌[齒*來]蘇謎蘇摩蘇契薩縛迦摩娑達泥呬梨寐梨薜薜梨悉殿都謎薩縛迦摩娑馱室遮縵怛羅般陀莎呵佛說如是普密呪時。",
        "from": "T21n1365_001:0884a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八千俱胝諸惡神鬼。",
        "from": "T21n1365_001:0884a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆生歡喜捨離毒心。",
        "from": "T21n1365_001:0884a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸佛法僧斷惡修善。",
        "from": "T21n1365_001:0884a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱詣佛所發誠諦言。",
        "from": "T21n1365_001:0884a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願常護持此大神呪。",
        "from": "T21n1365_001:0884a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令受持者身心安樂。",
        "from": "T21n1365_001:0884a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言善哉如汝所願若有善男子善女人。",
        "from": "T21n1365_001:0884a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸佛法僧及金剛手。",
        "from": "T21n1365_001:0884a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能處閑靜堅修梵行。",
        "from": "T21n1365_001:0884a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "親近制多受持讀誦。",
        "from": "T21n1365_001:0884a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求善願無不皆得。",
        "from": "T21n1365_001:0884a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至無上正等菩提。",
        "from": "T21n1365_001:0884a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒憶宿命無有忘失。",
        "from": "T21n1365_001:0884a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時薄伽梵說是經已。",
        "from": "T21n1365_001:0884a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛手等一切菩薩。",
        "from": "T21n1365_001:0884a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸天人阿素洛等。",
        "from": "T21n1365_001:0884a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾會聞佛所說。",
        "from": "T21n1365_001:0884a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆大歡喜。",
        "from": "T21n1365_001:0884a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信受奉行。",
        "from": "T21n1365_001:0884a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八名普密陀羅尼經",
        "from": "T21n1365_001:0884a22_0",
        "context_with_next_sentence": false
    }
]