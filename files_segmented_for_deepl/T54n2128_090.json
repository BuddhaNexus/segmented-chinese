[
    {
        "source_sentence": "一切經音義卷第九十",
        "from": "T54n2128_090:0878b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翻經沙門慧琳撰",
        "from": "T54n2128_090:0878b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前高僧傳音下卷",
        "from": "T54n2128_090:0878b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從第七盡十四。",
        "from": "T54n2128_090:0878b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高僧傳第七卷",
        "from": "T54n2128_090:0878b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忘筌(上网方反下七宣反考聲云筌取魚竹籠也從竹全聲莊子曰得魚忘筌也)。",
        "from": "T54n2128_090:0878b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[仁二+(穩禾)]几(殷靳反桂苑珠叢云[仁二+(穩禾)]憑也從人[穩禾]聲[穩禾]音隱下飢蟻反考聲云凡案屬也韻英云几所以憑也說文踞也象形傳文從木作机亦可通也)。",
        "from": "T54n2128_090:0878b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[譥言+木]魔文(上刑擊反顧野王云[譥言+木]書者所以罪責當代者曉諭慰勞百姓書也漢書云羽[譥言+木]徵天下兵裴駰注云以鳥羽插於[譥言+木]書之上而取其勢速若鳥飛也說文從木敫聲中音擊)。",
        "from": "T54n2128_090:0878b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蕭摹(上蘇彫反說文從草肅聲肅字從[聿干+巾]音女輒反[聿干+巾]在[棩木]上音恚緣反下莫胡反從手莫聲也)。",
        "from": "T54n2128_090:0878b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "較談(上音角集訓云較其優劣也或從攴作[車*支]正從爻作[車*爻]傳文從交作較俗字也)。",
        "from": "T54n2128_090:0878b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傷蠹(下都固反說文木中蟲也從[虫*虫]從橐省聲也)。",
        "from": "T54n2128_090:0878b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剖析(上普口反杜注左傳云中分曰剖說文判也從刀咅聲咅音他口反析音錫)。",
        "from": "T54n2128_090:0878b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翕然(歆邑反孔注尚書云翕合也說文從羽合聲亦轉注字也)。",
        "from": "T54n2128_090:0878b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詆訶(上低禮反蒼頡篇云詆欺也廣雅毀也說文詆訶也從言氐聲氐音同上也)。",
        "from": "T54n2128_090:0878b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[貝*親]遺(初靳反下唯恚反去聲字假借字也文字集略云[貝*親]施也遺贈也或從口作嚫靳音謹近反)。",
        "from": "T54n2128_090:0878b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清確(苦角反古今正字確堅也從石隺聲隺音涸傳文從霍作礭俗字非也)。",
        "from": "T54n2128_090:0878b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "披薜荔(上毘計反下黎計反考聲云薜荔草名也案薜荔藤蘿蔓生附樹如衣貌也)。",
        "from": "T54n2128_090:0878c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逞芬(上勅領反毛詩傳逞極也韻英云通也快也古今正字從辵呈聲也)。",
        "from": "T54n2128_090:0878c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弗緇(下滓師反毛詩傳云黑色也說文從糸甾聲也傳作淄非也)。",
        "from": "T54n2128_090:0878c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弗涅(下年結反孔注論語云涅可以染皂說文云黑土在水中從水圼聲圼音同上)。",
        "from": "T54n2128_090:0878c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煒曄(上韋鬼反下炎劫反王注楚辭云煒曄曰赤光盛貌也或從火作燁並形聲字也)。",
        "from": "T54n2128_090:0878c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彭[凵@(甬用+豩)](下悲旻反人名也說文從山豩聲也豩音同上也)。",
        "from": "T54n2128_090:0878c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鎔鈞(上勇鍾反漢書音義云鎔錢之摸也說文冶器法銅屑之鎔也從金容聲下癸勻反說文云十斤曰鈞從金從勻聲勻音尹遵反)。",
        "from": "T54n2128_090:0878c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "走竄(上緅苟反正體走字也說文趍也從夭從止下倉亂反顧野王云竄逃也文字典說鼠在穴中會意字也)。",
        "from": "T54n2128_090:0878c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "麓山(音鹿毛詩傳曰麓山足也山林之下也從林鹿聲)。",
        "from": "T54n2128_090:0878c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即瘳(下勅留反孔注尚書云瘳差也考聲病損也說文從疒翏聲也翏音力幼反)。",
        "from": "T54n2128_090:0878c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "張邵(常曜反人名也)。",
        "from": "T54n2128_090:0878c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "桎梏(上音質下音攻沃反韻英云在手曰桎在足曰梏鄭眾曰兩手兩足各同一木也亦名杻械拘繫罪人也)。",
        "from": "T54n2128_090:0878c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誘人(由酒反勸也教也形聲字也)。",
        "from": "T54n2128_090:0878c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "路[目*舌](火刮反埤蒼[目*舌]視也從目舌聲舌音還刮反從干從口也)。",
        "from": "T54n2128_090:0878c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[闔去+太]席(上含[木*蓋]反爾雅[闔去+太]謂之扉說文[闔去+太]閉也從門盇聲盇音合傳文從畜作[門@((肱月)/田)]非也)。",
        "from": "T54n2128_090:0878c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨淄(上音林鄭注周禮云以尊就卑曰臨賈注國語云臨治也方言臨照也說文臨監也從臥品聲下滓師反淄州也九州分在青州之地也杜注左傳云淄水名也出泰山梁父西北入汶文字典說從水甾聲甾音同上也)。",
        "from": "T54n2128_090:0878c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徐偡(澤黯反人名也偡猶齊整也)。",
        "from": "T54n2128_090:0878c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嶀嵊(上音戶下音乘前第四卷已釋)。",
        "from": "T54n2128_090:0878c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誄焉(上律委反周禮誄跡也累說生時德行之辭也鄭注禮記誄累也說文謚也從言耒聲耒音雷會反)。",
        "from": "T54n2128_090:0878c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賑施(上真忍反爾雅賑富也郭璞注云隱賑富有也說文從貝辰聲)。",
        "from": "T54n2128_090:0878c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "踟蹰(上雉知反下住誅反考聲云踟蹰猶俳佪也廣疋作躊躇躊音長流反躇音除謂猶豫也古今正字二字並從足知厨皆聲)。",
        "from": "T54n2128_090:0878c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "負笈(下儉劫反考聲云盛書篋也古今正字從竹及聲背上笈也)。",
        "from": "T54n2128_090:0879a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八卷",
        "from": "T54n2128_090:0879a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "綽有(昌灼反爾雅綽緩也毛詩箋云綽寬也文字典說寬緩也從糸卓聲也)。",
        "from": "T54n2128_090:0879a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恧焉(上女六反又女力反方言恧慚也小爾疋云心愧曰恧賈注國語云恥也說文慙也從而心聲)。",
        "from": "T54n2128_090:0879a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "負袠(下陳栗反案袟者裹束書衣皮表也文字典說或從巾作帙俗作袟同也)。",
        "from": "T54n2128_090:0879a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猥來(上烏賄反廣疋云猥眾也博雅多也許叔重注淮南子猥猥惣凡也說文從犬畏聲)。",
        "from": "T54n2128_090:0879a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王鑠(下商灼反宋書云南乎王諱鑠字林玄毛詩傳曰鑠美也銷金也文字典說從金樂聲)。",
        "from": "T54n2128_090:0879a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降蹕(下音畢鄭注周禮云蹕謂止行者清道也若今之警蹕也文字典說從足畢聲)。",
        "from": "T54n2128_090:0879a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "該涉(上哥哀反賈注國語云該備也方言云咸也廣雅云該評也亦包也說文云兼備也從言亥聲)。",
        "from": "T54n2128_090:0879a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昞有(上兵皿反或作昺亦同說文明也從日丙聲或從火作炳用亦同也)。",
        "from": "T54n2128_090:0879a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王嶷(下疑棘反宋朝豫章王名也古今正字從山疑聲)。",
        "from": "T54n2128_090:0879a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "窆于(上悲驗反鄭玄注周禮云窆謂下棺也說文從穴乏聲也)。",
        "from": "T54n2128_090:0879a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周顒(愚恭反人名也)。",
        "from": "T54n2128_090:0879a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憫而(眉殞反杜注左傳云憫憂也古今正字形聲)。",
        "from": "T54n2128_090:0879a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "暀暀(王況反爾雅暀暀美也考聲曰光美皃毛詩暀暀猶往往也說文光美貌也從日往往聲也)。",
        "from": "T54n2128_090:0879a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持操(下草到反王注楚辭云操至也顧野王云持志貞固曰操考聲立志不改也從手喿聲)。",
        "from": "T54n2128_090:0879a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劉[思*劦](嫌頰反梁朝時才名之士也著書四卷名劉子與劉蟉等並皇枝貴族也)。",
        "from": "T54n2128_090:0879a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "訪覈(下衡革反前第五卷中具釋)。",
        "from": "T54n2128_090:0879a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劉瓛(言蹇反人名也)。",
        "from": "T54n2128_090:0879a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "番禺(上坂官反下音愚南海廣州縣名也傳文從阜作隅亦通與廣州隔一小海)。",
        "from": "T54n2128_090:0879a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鑽研(上纂鸞反孟子鑽穿也考聲刺也古今正字從金贊聲傳文從手作攢非也下硯堅反)。",
        "from": "T54n2128_090:0879a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謿謔(上讁交反顧野王云謿謂戲調也楚辭從口作嘲訓釋同下香虐反爾雅謔謂謿戲也毛詩戲謔也說文從言虐聲)。",
        "from": "T54n2128_090:0879a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蔗棗(上遮夜反即甘蔗也美草名下音早甘果名說文羊棗也從二束也)。",
        "from": "T54n2128_090:0879a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "郿縣(上音眉漢書地名屬古扶風形聲)。",
        "from": "T54n2128_090:0879b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相愶(下杴劫反公羊傳愶迫也廣雅怯也顧野王云以力相恐愶也文字典說愶赫也從心脅聲傳文從月作脇非)。",
        "from": "T54n2128_090:0879b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智敞(下昌掌反沙門名)。",
        "from": "T54n2128_090:0879b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧韶(邵招反人名也)。",
        "from": "T54n2128_090:0879b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "德冏(鬼永反僧名傳文從公作冏俗字也)。",
        "from": "T54n2128_090:0879b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東莞(音官地名)。",
        "from": "T54n2128_090:0879b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[弦/心]懸(上形堅反說文[弦/心]急也從心弦聲弦音同上考聲及說文並作[弓*耑]古字也河南密縣有[弦/心]亭傳文作[怡台+強]亦同也)。",
        "from": "T54n2128_090:0879b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "記莂(彼列反分也分別與受記也)。",
        "from": "T54n2128_090:0879b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戴[幊工+朿](下爭革反方言云覆髻謂之幘說文云髮有巾曰幘從巾責聲[朿/貝]從束束音[利禾+夾])。",
        "from": "T54n2128_090:0879b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冥寂(上覓瓶反下情亦反從宀音綿從叔音叔)。",
        "from": "T54n2128_090:0879b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寔賴(上承力反杜注左傳云寔猶是也說文從宀是聲傳中從穴作[穴/是]非也)。",
        "from": "T54n2128_090:0879b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曇斐(非尾反斐文皃沙門名也)。",
        "from": "T54n2128_090:0879b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緘嘿(甲咸反孔注尚書緘閉也說文緘束篋也從糸咸聲下瞢北反寂嘿無聲也或從犬作默亦同也)。",
        "from": "T54n2128_090:0879b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淼漫(上妙褾反考聲云水廣大貌也韻英云大水也從三水傳文從水作渺亦通下滿半反何注公羊傳漫徧也顧野王云不可分別廣雅平也楚辭長也古今正字從水曼聲)。",
        "from": "T54n2128_090:0879b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "粹哲(上雖醉反考聲精純也說文不雜也從米卒聲下知列反)。",
        "from": "T54n2128_090:0879b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鐇斧(上音煩韻詮云廣刃斧也埤蒼云鏟也闊刃斧也文字典說推也從金番聲也)。",
        "from": "T54n2128_090:0879b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第九卷",
        "from": "T54n2128_090:0879b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[雪雨+甡]孛(上旋銳反字苑云彗[竺二+帚]星也呂氏春秋云彗祅星也說文彗掃也從又持甡象形字也甡音銑下盆沒反何注公羊傳孛星如白絮孛孛然祅氣貌也說文從[孛子][孛子]音費從子傳文作[堂土+子]亦通也)。",
        "from": "T54n2128_090:0879b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "痼疾(上音固或作[病丙+古]說文從疒形聲字)。",
        "from": "T54n2128_090:0879b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乾燥(上哥安反下搔到反前已釋)。",
        "from": "T54n2128_090:0879b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[蝷虫+土]如(上恥革反廣雅云析分也說文裂也從土[蝷虫]聲[蝷虫]音尺)。",
        "from": "T54n2128_090:0879b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "石梁塢(烏古反文字集略云小城壁也案塢者堆崇峻兩丘中間名之為塢從阜作隖亦同從土烏聲)。",
        "from": "T54n2128_090:0879b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪[狦(狂王)+木](下楚革反廣雅[狦(狂王)+木]邏也蒼頡篇縶也字統云竪木如牆曰[狦(狂王)+木]說文從木[狦(狂王)]聲也[狦(狂王)]音同上縶音知立反)。",
        "from": "T54n2128_090:0879c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "扁鵲(駢面反說文從戶從扁省聲下搶藥反案扁鵲古之名醫也本姓盧六國時人也)。",
        "from": "T54n2128_090:0879c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戎狛(上而終反說文從甲作[戒廾+甲]今從十作戎俗字也下音陌狩種也蠻[狂王+巢]之類也傳從豸亦通)。",
        "from": "T54n2128_090:0879c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "石斌(彼旻反人名也)。",
        "from": "T54n2128_090:0879c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "混殽(上魂閫反廣雅混濁也下効交反漢書殽和也從殳肴聲肴從肉也)。",
        "from": "T54n2128_090:0879c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "麻襦(乳朱反案襦者今之襖子也麻襦即布袍也說文云襦短衣也從衣形聲字)。",
        "from": "T54n2128_090:0879c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晥然(還綰反何晏注論語云晥尒小笑貌也)。",
        "from": "T54n2128_090:0879c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "石韜(討刀反)。",
        "from": "T54n2128_090:0879c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重茵(下音因鄭注禮記茵蓐也顧野王云以虎皮為蓐也說文車上重席也從草因聲司馬相如子虛賦中從革作鞇亦同)。",
        "from": "T54n2128_090:0879c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應耄(下毛報反左傳老將耄矣杜預云耄亂也禮記八十九十日耄鄭玄曰昏忘也從老毛聲說文作[卄/毫]古字)。",
        "from": "T54n2128_090:0879c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不惛(忽昆反孔注尚書云惛亂也考聲老而多忘也箋詩云惛人無所知說文從心昏聲)。",
        "from": "T54n2128_090:0879c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鐵鎖穿(天結反說文黑金也從金[鐵金]聲[鐵金]音垤次蘇果反廣雅鎖連也謂相鈎連也下音川說文穿通也傳文從身作[穴/身]非也)。",
        "from": "T54n2128_090:0879c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轘裂(詮云轘車轅也鄭注周禮云車裂曰轘又音倦又音患訓義一也從車從還省聲)。",
        "from": "T54n2128_090:0879c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "休祉(勅理反毛詩傳曰祉福也杜注左傳云祉祿也從示止聲)。",
        "from": "T54n2128_090:0879c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嗚噎(上鄔姑反下煙結反尚書嗚呼嗟嘆辭也說文或從欠作[鳥*欠]毛詩傳曰噎謂憂不能息也方言云痛也說文並從口形聲字)。",
        "from": "T54n2128_090:0879c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蹋之(上談合反廣疋蹋履也說文踐也足躡也從足[日/羽]聲[日/羽]音塔傳中作蹹非)。",
        "from": "T54n2128_090:0879c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儁子瑋(上遵峻反與俊字義同下韋鬼反人名也慕容儁兒)。",
        "from": "T54n2128_090:0879c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "澄磴橙蹬(此四字並是佛圖澄名梵語輕重不同致令有數體今通用多是澄字耳)。",
        "from": "T54n2128_090:0879c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燉煌(上鈍魂反下音皇前卷中已具釋訖)。",
        "from": "T54n2128_090:0879c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法綝(丑林反僧名也考聲綝善也上也)。",
        "from": "T54n2128_090:0879c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "芧茨(上夘包反下音慈案茅茨草屋也廣雅覆也毛詩傳曰葢屋也並從草予次皆聲)。",
        "from": "T54n2128_090:0879c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "康泓(鳥宏反)。",
        "from": "T54n2128_090:0880a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "茹芝(上如庶反郭注爾疋茹食也說文從草如聲下音之正作出象形也案芝草者神仙所食瑞草也)。",
        "from": "T54n2128_090:0880a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問[婉夗+(火/又)](臊老反傳文作[女*更]非也)。",
        "from": "T54n2128_090:0880a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俶儻(上充宍反下湯朗反神氣定特名為俶儻並從人)。",
        "from": "T54n2128_090:0880a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髣髴(妨[网(ㄨ*ㄨ)+又]反下芬勿反又音芳未反古今字髣髴聞見不審諦也從髟形聲字也古人或作仿佛音並同上)。",
        "from": "T54n2128_090:0880a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手搦(下儜擿反顧野王云搦猶捉也說文按也從手弱聲)。",
        "from": "T54n2128_090:0880a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淤泥(上於據反下溺提反淤泥汙池水底臭泥也青黑臭爛滓穢者也並從水形聲字也)。",
        "from": "T54n2128_090:0880a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十卷",
        "from": "T54n2128_090:0880a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "基蹠(上紀其反爾疋基始也本也下趾也從土其聲下征亦反王注楚辭云蹠踐也許叔重云行也蹈也廣疋履也傳文從土作墌墌亦基也或作趾並通)。",
        "from": "T54n2128_090:0880a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覺勌(權院反從力卷省聲)。",
        "from": "T54n2128_090:0880a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樊陽(伐袁反考聲云山邊也地志云古之小國名周地邑号也說文從林從爻從[(?*?)/八][(?*?)/八]音攀也)。",
        "from": "T54n2128_090:0880a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蹍室(上尼展反司馬彪注莊子云蹍蹈也廣雅履也古今正字從足展聲也)。",
        "from": "T54n2128_090:0880a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壁隌(下菴感反爾疋隌暗也郭璞云隌然冥闇也言此聖僧作神通也)。",
        "from": "T54n2128_090:0880a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "閬中(郎宕反說文云巴郡有閬中縣今蜀東川有閬州也)。",
        "from": "T54n2128_090:0880a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徒跣(下先典反說文云跣謂足親於地也形聲字)。",
        "from": "T54n2128_090:0880a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "撤懸(上恥列反考聲云毀廢也說文從手徹省聲下音玄鄭注周禮云懸謂鍾磬在簨[箎儿+丘]而廢之不鼓也懸亦掛也轉注字也)。",
        "from": "T54n2128_090:0880a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朱彤(下音同前秦中書令人名也彤赤色也)。",
        "from": "T54n2128_090:0880a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傉檀(農篤反下憚闌反蕃語也是南涼僣号也第三主也)。",
        "from": "T54n2128_090:0880a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "土埭(下音大晉中興書曰求以牛車牽埭取其海稅也考聲云截溝為堰字也)。",
        "from": "T54n2128_090:0880a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "引筰(音昨蒼頡篇云筰竹索也案筰者蜀川西山有深絕澗不可越施竹索也於兩岸人乘其上機關自繫往來如橋梁名曰筰從竹作聲傳文從糸作[糸*乍]非也)。",
        "from": "T54n2128_090:0880a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倏忽(上傷肉反卷初已釋)。",
        "from": "T54n2128_090:0880a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小甌(歐侯反方言云盆之小者謂之甌甌瓦埦也傳文從土作塸非也)。",
        "from": "T54n2128_090:0880b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "襤縷(上音藍下龍主反方言衣無緣曰襤郭璞曰衣敗也破也說文上從衣下從糸形聲字)。",
        "from": "T54n2128_090:0880b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "噉肉(談濫反廣雅啖食也或作啖訓用同)。",
        "from": "T54n2128_090:0880b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辛膾(瑰外說文細切肉也從肉會聲傳文從魚作鱠非也辛葷辛也)。",
        "from": "T54n2128_090:0880b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "負圌(下市緣集訓云竹倉也貯穀麥之圌[竺二+屯]也古今正字從口耑聲也)。",
        "from": "T54n2128_090:0880b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至瞑(冥並反暝夜也)。",
        "from": "T54n2128_090:0880b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殯葬(上賓臏反下臧[(哭、)/(└@人)]反傳文作塟非也)。",
        "from": "T54n2128_090:0880b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餧者(奴磊反或從魚作鯘魚敗臭也論語曰魚餧而肉敗孔注云魚敗曰餧亦從肉作腇並臭壞之魚)。",
        "from": "T54n2128_090:0880b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剡縣(常焰反越州縣名也)。",
        "from": "T54n2128_090:0880b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "濛濛(音蒙樹蔭濛密垂陰)。",
        "from": "T54n2128_090:0880b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "浮沫(下滿鉢反從水末聲末字古木字上加一名末)。",
        "from": "T54n2128_090:0880b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "累躓(下知利反倒仆也)。",
        "from": "T54n2128_090:0880b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懱醜(眠[(囟/比)*力]反鄭注毛詩云蔑輕[仁二+易]也傳文從伐作薎非也[(囟/比)*力]音陛蔑反)。",
        "from": "T54n2128_090:0880b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畦啜(上惠珪反劉熈曰二十五畝為小畦五十畝為大畦下川劣反說文啜甞也)。",
        "from": "T54n2128_090:0880b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲洊(下前薦反)。",
        "from": "T54n2128_090:0880b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大航欄(上何岡反方言云自關而東謂舟為航大船也形聲字)。",
        "from": "T54n2128_090:0880b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "霑溼(上張廉反廣雅霑漬也韓詩溺也古今正字從雨沾聲下深入反說文幽溼也從水溼聲[一/(ㄠ*〡*ㄠ)/工]音同上)。",
        "from": "T54n2128_090:0880b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猜嫉(上釆來反下音疾也)。",
        "from": "T54n2128_090:0880b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒掠(下音略月令云無肆掠即劫也虜掠也又音亮訓用義同)。",
        "from": "T54n2128_090:0880b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執[烈列+(爨(大/火))](倉亂反俗字也正體作爨今見文繁省作[釁分+(烈列)]訛略也左傳云灼也炊也周禮竈也)。",
        "from": "T54n2128_090:0880b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清嚥(煙見反或從言作讌亦通今作宴毛詩安也宴會飲酒樂也)。",
        "from": "T54n2128_090:0880b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[郫卑+((白日+田)/廾)](被悲反漢書蜀郡有[郫卑+((白日+田)/廾)]縣又音毘)。",
        "from": "T54n2128_090:0880b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "綰濟(上彎板反淮南子綰猶攝也案傳文綰指者擕其手指也)。",
        "from": "T54n2128_090:0880b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劉虯(糾幽反韻英云無角龍也荊州隱土名也捨宅為寺)。",
        "from": "T54n2128_090:0880b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王奐(音喚人名也奐文彩貌也玉篇光明炳煥說文從拱也)。",
        "from": "T54n2128_090:0880c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虵鵠(上常遮反即靈蛇龍之類也下紅目反鵠鵠水鳥也鶴之類也壽滿千歲即形變也)。",
        "from": "T54n2128_090:0880c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[醫酉+言]賴(益兮反平聲字也鄭箋詩云[醫酉+言]猶是也然也說文發言聲也形聲字語辭也)。",
        "from": "T54n2128_090:0880c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十一卷",
        "from": "T54n2128_090:0880c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "撥胷(上音鉢廣雅撥除也說文從手發聲下勗恭反說文胷膺也從勹胷聲傳從肉亦通)。",
        "from": "T54n2128_090:0880c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行蠱(音古即蟲毒也以虫毒作法病害人也厭魅之類也會意字也)。",
        "from": "T54n2128_090:0880c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳴鼙(下陛迷反說文云鼙騎鼓也軍行戰鼓也傳文從革作古字亦通也)。",
        "from": "T54n2128_090:0880c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧嵬(五磊反俗字正從人從貴作僓僓長大貌高僧名也)。",
        "from": "T54n2128_090:0880c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蝗毒(上音皇下音毒即蝗蟲為災毒也食苗心者形如蝜螽黃色海中水蟲所化或生於地中也災所感召)。",
        "from": "T54n2128_090:0880c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洒漱(上西禮反正體洗字也傳文作洗俗字也下搜救反前已釋訖)。",
        "from": "T54n2128_090:0880c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蟬蛻(上善延反鄭注禮記蟬蜩也方言蟧鳥鳴於腹下)。",
        "from": "T54n2128_090:0880c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "量褊(下鞭沔反郭注爾疋云褊猶急也說文小也從衣扁聲扁音同上字書窄)。",
        "from": "T54n2128_090:0880c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[(厂一)*臣*頁]神(上以之反方言[(厂一)*臣*頁][舍*頁]也王輔嗣注周易云[(厂一)*臣*頁]養也說文從頁[(厂一)*臣]聲[(厂一)*臣]音同上)。",
        "from": "T54n2128_090:0880c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉斃(毘袂反韻英云斃死也說文云從死敝聲也)。",
        "from": "T54n2128_090:0880c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蚊蝱(上音文下音萌並是嚙人飛蟲前已數處釋也)。",
        "from": "T54n2128_090:0880c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "芬馥(上芳文反下逢福反)。",
        "from": "T54n2128_090:0880c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "秉笏(上兵皿反下音忽手板也)。",
        "from": "T54n2128_090:0880c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鑿麓山(上音昨下音祿前已具訓釋也)。",
        "from": "T54n2128_090:0880c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "碌碌(音祿)。",
        "from": "T54n2128_090:0880c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "螢爝(上音滎滎火虫也下音爵小炬火也俗字也)。",
        "from": "T54n2128_090:0880c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "枯鑠(下傷藥反消盡也)。",
        "from": "T54n2128_090:0880c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "張[乳孚+召](下亭姚反人名也)。",
        "from": "T54n2128_090:0880c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輻湊(上音福下倉奏反顧野王云輻湊者轂也說文湊聚也形聲字)。",
        "from": "T54n2128_090:0880c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧璩(音渠人名)。",
        "from": "T54n2128_090:0880c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨卭(共顒反顒音愚恭反蜀川卭縣名也)。",
        "from": "T54n2128_090:0881a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燔屍(上音煩燒也)。",
        "from": "T54n2128_090:0881a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玁狁(上音險下音允毛詩傳云玁狁北狄名也或作獫並從犬)。",
        "from": "T54n2128_090:0881a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姓俞(丑救反人姓也)。",
        "from": "T54n2128_090:0881a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下邳(下音皮縣名也屬泗州案曹氏郡國云泗州秦併天下立為邳郡有下邳縣七鄉州西北一百八十里)。",
        "from": "T54n2128_090:0881a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拘閡(上音俱下俄葢反傳文從石作硋非)。",
        "from": "T54n2128_090:0881a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "磐杵(上音盤聲類磐大石平而且大也傳作杵書誤也)。",
        "from": "T54n2128_090:0881a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十二卷",
        "from": "T54n2128_090:0881a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "折翅(上蟬熱反考聲折摧也從手下詩志反說文鳥翼也從羽支聲支字從半竹從又也)。",
        "from": "T54n2128_090:0881a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賑貧(上音震又音真忍集訓云賑富也說文救也從貝辰聲或作振亦通)。",
        "from": "T54n2128_090:0881a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧瑜(庚珠反僧名也)。",
        "from": "T54n2128_090:0881a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剋炳(下兵皿反說文炳明也從火丙聲)。",
        "from": "T54n2128_090:0881a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蜚羽(上音非風師名)。",
        "from": "T54n2128_090:0881a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓊蔚(上烏孔反下惲屈反司馬相如子虛賦曰草木盛貌)。",
        "from": "T54n2128_090:0881a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道冏(鬼永反僧名也)。",
        "from": "T54n2128_090:0881a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迺[起巳+孚](上音乃考聲云驚也往也至也說文闕下音赴蒼頡篇[起巳+孚]奔也與赴字義同或作計也)。",
        "from": "T54n2128_090:0881a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "芳苾(上敷亡反下頻蜜反芳苾香氣盛貌)。",
        "from": "T54n2128_090:0881a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶憭(下寮鳥反說文憭惠也從心[(嘹口)小+火]聲[(嘹口)小+火]尞並音[潦小+火])。",
        "from": "T54n2128_090:0881a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "交阯(下音止南海交阯郡也)。",
        "from": "T54n2128_090:0881a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自[敲高+(立/(可一))](音欺韓詩云攲傾也玉篇不正也說文攲側也從攴奇聲)。",
        "from": "T54n2128_090:0881a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "巫覡(上音無女事鬼神日巫男事鬼神日覡音形擊反覡人見神也昔殷時巫咸初作巫以舞下神也象舞形也)。",
        "from": "T54n2128_090:0881a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "叡公(上營惠反高僧名也說文從目作叡傳從曰非)。",
        "from": "T54n2128_090:0881a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餌松术(上音二次俗容反下墜律反一名山薑草藥名甚香肥股之絕粒不飢燒甚香也)。",
        "from": "T54n2128_090:0881a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飤之(上音寺杜注左傳云以飯食設供於人曰飤說文飤糧也從食從人會意字也傳從台作飴非)。",
        "from": "T54n2128_090:0881a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圊中(上音清即圊廁也棄穢之所圂也外形內聲字也)。",
        "from": "T54n2128_090:0881b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孟顗(冝豈反人名也)。",
        "from": "T54n2128_090:0881b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "溜山(流救反)。",
        "from": "T54n2128_090:0881b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小移(以之反依時非時經以足量影小似差時過即不食虛齋恐犯非時食)。",
        "from": "T54n2128_090:0881b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "葷醪(上音薰[卄/公/心][卄/秝]韭薤等臭菜也下音勞即白醪等一切諸酒者也)。",
        "from": "T54n2128_090:0881b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鮮豢(上音仙鮮新也下音患韻詮云養畜也食米曰豢食草曰蒭傳云鮮豢魚豚肥鮮之類)。",
        "from": "T54n2128_090:0881b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憓利(上音惠義與惠字訓用同)。",
        "from": "T54n2128_090:0881b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與[車*爻](下江岳反廣雅[車*爻]明也古今正字試優劣也從車爻聲傳從交俗字也前高僧傳已釋也)。",
        "from": "T54n2128_090:0881b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十三卷",
        "from": "T54n2128_090:0881b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畋獵(上音田尚書曰畋干有洛之表案畋即獵也何注公羊傳云畋者獀狩之總名也或從犬作[狂王+田]下廉輒反鄭箋毛詩云宵畋為獵賈注國語云獵取也說文効獵駈逐禽獸使不害苗所獵得者以享薦宗廟從犬巤聲也傳文從山從鳥作[狂王+(山/鳥)]不成字非也)。",
        "from": "T54n2128_090:0881b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懇到(上肯恨反鄭注周禮懇願也廣雅懇誡也說文懇美也從心貇聲貇音苦本反傳文作墾是墾田字非此用也)。",
        "from": "T54n2128_090:0881b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鐵椷(天結反說文黑金也下音咸或從木作椷廣雅篋謂之椷字統云椷木篋也傳文從了作凾本音含非也俗用非本字誤用)。",
        "from": "T54n2128_090:0881b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "炫耀(上玄絹反廣雅炫明也埤蒼光也下遙照反說文曜照也昡曜二字並從火形聲字也)。",
        "from": "T54n2128_090:0881b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高悝(空回反人名也)。",
        "from": "T54n2128_090:0881b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[火/几]跗(上正體光字也從火從人傳作光俗用變體字也下甫無反亦正趺字也傳從夫作趺俗字亦通也)。",
        "from": "T54n2128_090:0881b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歔欷(上音虛下音希王逸注楚辭云歔欷啼泣貌也)。",
        "from": "T54n2128_090:0881b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鄮塔(上莫候反前第三卷已釋)。",
        "from": "T54n2128_090:0881b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "基墌(征亦反前第七卷已具釋)。",
        "from": "T54n2128_090:0881b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擲碢(徒禾反圓薄而小形似輾碢手擲以為戲亦曰拋碢云擲樗者是也乃江鄉吳越之文言非經史之通語也此字本無諸儒各隨意作之故無定體今並書出未知孰真集訓從土作[土*隋]考聲從石作磗韻詮從木作槩文字集略及韻英從石作碢今且為正)。",
        "from": "T54n2128_090:0881b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孔逭(下桓管反人名也)。",
        "from": "T54n2128_090:0881c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "張邵(常遶反人名也從邑召聲傳文作[君*巳]錯書之也)。",
        "from": "T54n2128_090:0881c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灌蒨(上古換反下阡見反僧名也漢書亦有東方朔字曼蒨亦此字)。",
        "from": "T54n2128_090:0881c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十[舵匕+(火/又)](下桑刀反正體[舵匕+(火/又)]字也或從木作[梡元+(火/又)]用同楊承慶字統云其形謂之船其頭數謂之艘考聲云[舵匕+(火/又)]亦船槽也從舟叜聲叜正叟字)。",
        "from": "T54n2128_090:0881c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必斃(下毘祭反前第十一卷已訓釋傳從黽作鼈錯書也)。",
        "from": "T54n2128_090:0881c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蝘蜓(上於典反下田典反尒疋云蝘蜓守宮也傳文作[蚓〡+(乞乙+小)]或通若作[虫*(乞乙+小)]非)。",
        "from": "T54n2128_090:0881c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竄居([卄/公/心]亂反杜注左傳云竄猶匿也賈注國語云隱也說文鼠在穴中竄會意字也撲然普卜反)。",
        "from": "T54n2128_090:0881c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "物範(下音范顧野王云範謂揩模也說文法也從竹從車從范省聲也)。",
        "from": "T54n2128_090:0881c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鎚鐷(上墜追反今取去聲下音葉即隱起金鍱佛像也或熟銅隱鏤成像以金鍍飾或真金鐷銀鐷隱起而成裝作檀龕是此功德也)。",
        "from": "T54n2128_090:0881c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "張綣(下區院反人名也)。",
        "from": "T54n2128_090:0881c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "窆於(悲驗反說文云葬下棺也從穴乏聲又音補鄧反)。",
        "from": "T54n2128_090:0881c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懾化(上占涉反古今正字云懾懼也挫也從心聶聲傳文從阜非也)。",
        "from": "T54n2128_090:0881c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慊切(上謙琰反鄭玄曰慊猶猒也說文音叶兼反慊疑也從心兼聲)。",
        "from": "T54n2128_090:0881c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掩曜(上淹撿反傳文作揜古字正通揜曜者猶韜光也從手弇聲)。",
        "from": "T54n2128_090:0881c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洒漱(上西禮反說文洒濯也從水西聲傳從先作洗俗字也下搜右反前文已具釋矣)。",
        "from": "T54n2128_090:0881c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[捩犬+乙]腕(上音厄正體字也下烏灌反手腕也俗字也正體古文作掔會意字也)。",
        "from": "T54n2128_090:0881c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姓蕢(苦夬枯恠反人姓也草名也又音匱)。",
        "from": "T54n2128_090:0881c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧旭(下音勗僧名也)。",
        "from": "T54n2128_090:0881c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "砰磕(上拍萌反下堪閤反砰磕大聲也傳從當非也無此字也)。",
        "from": "T54n2128_090:0881c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "浙左(氈熱反即浙江也浙即越州等是左也)。",
        "from": "T54n2128_090:0881c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "褒述(上保毛反杜預曰凡文一字以為褒貶褒言揚其德行)。",
        "from": "T54n2128_090:0881c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "韶武(常招反顧野王云韶舜樂名也故周禮云舞大韶鄭注云紹繼也言舜能繼堯之德也說文從音召聲下武謂武舞也言武舞王者之樂也古今正字云武者定禍亂威服不臣戢兵曰武故以止戈為武字也)。",
        "from": "T54n2128_090:0881c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "激楚(上音擊琴操曲名也司馬彪注莊子云流隱曰激楚辭謂聲清敫而無不通也王逸曰激感也形聲字)。",
        "from": "T54n2128_090:0882a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重複(下風目反廣雅複厚也說文複重也從衣复聲也复音伏)。",
        "from": "T54n2128_090:0882a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "支侖(律屯反集訓云侖思也高僧名也傳大篆作龠音同上今時用以為藥字非此用也傳文從草作菕非也無此字)。",
        "from": "T54n2128_090:0882a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譌廢(上五戈反尚書譌化也鄭箋詩云譌偽也郭注爾雅代以[袖由+芺]言為譌言說文從言為聲傳文多作訛俗字也亦通也)。",
        "from": "T54n2128_090:0882a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忘翥(諸庶反楚辭曰鳥寒騫翥而飛翔方言云翥高舉也集訓云翥謂直飛而疾往也說文從羽者聲)。",
        "from": "T54n2128_090:0882a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "踡跼(上倦員反埤蒼云踡局不伸也字書奇行曲背也說文從足卷聲下音局廣疋跼跲也跲音鉗劫反顧野王云踡局曲不伸展也亦作局用同傳從足亦通)。",
        "from": "T54n2128_090:0882a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戞石(上姦八反孔注尚書云戛擊發樂聲也說文戟也從戈從頁省聲傳文傳寫錯不成字)。",
        "from": "T54n2128_090:0882a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聆語(上歷丁反說文聆聽也從耳令聲也)。",
        "from": "T54n2128_090:0882a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唌唾(上祥延反說文云語唌歎也從口延聲韻英云口中津液也或從水作延下士臥反左傳曰不顧而唾說文云唾口液也從口垂聲或作涶也)。",
        "from": "T54n2128_090:0882a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧璩(上惠字說文從甡甡音詵從又從心彗聲也彗音隨銳反下音渠璩玉名也傳中僧名從玉)。",
        "from": "T54n2128_090:0882a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "罄無(上輕勁反尒疋罄盡也從[午/止]從聲省聲也)。",
        "from": "T54n2128_090:0882a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "秣陵(上漫鉢反地名)。",
        "from": "T54n2128_090:0882a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王昶(暢兩反博雅通也韻英明也從日永聲也)。",
        "from": "T54n2128_090:0882a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱[芏/言](音善說文吉也美也說文從羊從競篆文從羊從言傳從草從言非也不成字也)。",
        "from": "T54n2128_090:0882a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "葆吹(上音保廣雅葆大也說文盛也從草保聲盛陳設樂也下推類反簫笛笙竿之類也)。",
        "from": "T54n2128_090:0882a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謇棘(上揵巘反王注周易云謇難也方言謇吃也語難也下兢力反棘者取其澀難)。",
        "from": "T54n2128_090:0882a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謦欬(上輕郢反喉中聲也下開愛反氣通也)。",
        "from": "T54n2128_090:0882a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十四卷",
        "from": "T54n2128_090:0882a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拯物(無疊韻故無反用[卄/丞]字上聲即是考聲云拯救也助也從手丞聲也)。",
        "from": "T54n2128_090:0882a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劉悛(下音詮人名也)。",
        "from": "T54n2128_090:0882b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晉剡(時冉反越州剡縣地名也出藤紙也)。",
        "from": "T54n2128_090:0882b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持摭(下征適反方言云陳宋之間以手取物曰摭說文正體從石作拓拾取物也從手庶聲也)。",
        "from": "T54n2128_090:0882b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鈆槧(上音緣下慙敢反又妾焰反楊雄荅劉歆書云以鈆[木*適]松槧也釋名云釋書契槧版長三尺也說文槧牘撲也從木斬聲版音板)。",
        "from": "T54n2128_090:0882b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "記糅(下女救反說文糅雜也從米柔聲)。",
        "from": "T54n2128_090:0882b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不刊(下渴干反方言後序云懸諸日月不刊之書是也文字典說云刊削也從刀干聲尚書作栞)。",
        "from": "T54n2128_090:0882b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緜亘(滅編反毛詩傳曰緜緜長而不絕貌也王注楚辭云緜纏也許叔重注淮南子綿猶絡也下岡鄧反韻英云亘長遠也)。",
        "from": "T54n2128_090:0882b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入簉(下[打丁+(丑/丑)]瘦反韻英云豐也齊也考聲云廁也盈也草盛貌也或從草亦通從竹造聲)。",
        "from": "T54n2128_090:0882b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尫餘(枉王反韻英云羸弱也俗音蠖黃反聲訛轉也)。",
        "from": "T54n2128_090:0882b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[怡台+典][怡台+墨](上天典反方言[怡台+典]慙也秦晉之間謂內心慙恥曰[怡台+典]說文亦慚也從心典聲下音墨應劭注漢書云嘿嘿不息得意顧野王云不言也說文從犬作默云大慙逐人從犬黑聲傳文從心作[怡台+墨]非也無此字)。",
        "from": "T54n2128_090:0882b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切經音義卷第九十",
        "from": "T54n2128_090:0882b14_0",
        "context_with_next_sentence": false
    }
]