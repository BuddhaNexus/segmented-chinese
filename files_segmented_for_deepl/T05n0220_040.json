    },
    {
        "source_sentence": "三藏法師玄奘奉　詔譯",
        "from": "T05n0220_040:0222b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初分般若行相品第十之三",
        "from": "T05n0220_040:0222b05_0",
        "context_with_next_sentence": true
    },
    {
