[
    {
        "source_sentence": "北方毘沙門天王隨軍護法真言",
        "from": "T21n1248_001:0225b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "特進試鴻臚卿大興善寺三藏沙門",
        "from": "T21n1248_001:0225b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大廣智不空別行翻譯不入正經",
        "from": "T21n1248_001:0225b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨囉多曩(二合)多羅(二合)多夜耶阿馱佉路步多囉(二合)耶地舍耶(二)吠室囉(二合)摩拏野(三)摩賀囉惹野(四)藥迦灑(二合)地婆多婆(五上)娑都哆娑娑室囉跛囉(二合)娑賀娜耶呬(賢二反)摩馱吒吒(去呼)尾跛囉(二合)灑夜迷跛娜也(二合)他保[月*麗]儞保[月*麗]儞俱沙吠室羅摩拏野摩賀囉惹耶綺(佉罽反)婆(去)馱伽儞多囉摩熾都麼麼(某甲受持)捺娑(上)娑馱耶娑那謨婆誐嚩帝悉殿都麼多囉跛馱儞娑婆(二合)賀若行者受持此呪者。",
        "from": "T21n1248_001:0225b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先須畫像。",
        "from": "T21n1248_001:0225c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彩色中並不得和膠。",
        "from": "T21n1248_001:0225c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於白[疊*毛]上畫一毘沙門神。",
        "from": "T21n1248_001:0225c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七寶莊嚴衣甲。",
        "from": "T21n1248_001:0225c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手執戟矟。",
        "from": "T21n1248_001:0225c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手托腰上。",
        "from": "T21n1248_001:0225c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其神脚下作二夜叉鬼。",
        "from": "T21n1248_001:0225c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身並作黑色。",
        "from": "T21n1248_001:0225c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其毘沙門面。",
        "from": "T21n1248_001:0225c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作甚可畏形惡眼視一切鬼神勢。",
        "from": "T21n1248_001:0225c12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其塔奉釋迦牟尼佛。",
        "from": "T21n1248_001:0225c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教汝若領天兵守界擁護國土。",
        "from": "T21n1248_001:0225c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何護吾法。",
        "from": "T21n1248_001:0225c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即擁遣第三子那吒捧行莫離其側。",
        "from": "T21n1248_001:0225c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝眼毒惡恐損眾生。",
        "from": "T21n1248_001:0225c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時行者。",
        "from": "T21n1248_001:0225c16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦此呪時。",
        "from": "T21n1248_001:0225c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就好地。",
        "from": "T21n1248_001:0225c16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿使有穢惡之處。",
        "from": "T21n1248_001:0225c17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "極令淨潔取牛糞塗地。",
        "from": "T21n1248_001:0225c17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以香泥塗之。",
        "from": "T21n1248_001:0225c18_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散種種花燒香供養。",
        "from": "T21n1248_001:0225c18_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者上下衣服。",
        "from": "T21n1248_001:0225c18_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並須清淨一上廁一洗浴。",
        "from": "T21n1248_001:0225c19_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白月十五日夜。",
        "from": "T21n1248_001:0225c19_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起首對像前。",
        "from": "T21n1248_001:0225c20_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦呪滿十萬遍訖。",
        "from": "T21n1248_001:0225c20_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後更取香泥復塗壇上。",
        "from": "T21n1248_001:0225c20_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更取上妙好花散壇中。",
        "from": "T21n1248_001:0225c21_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設種種飲食。",
        "from": "T21n1248_001:0225c22_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者取薰陸香一百八顆。",
        "from": "T21n1248_001:0225c22_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各呪一遍散著火中。",
        "from": "T21n1248_001:0225c22_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時毘沙門即現其身。",
        "from": "T21n1248_001:0225c23_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者若見神時取上好食飲。",
        "from": "T21n1248_001:0225c23_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養毘沙門神通。",
        "from": "T21n1248_001:0225c24_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者心中所願其神悉見。",
        "from": "T21n1248_001:0225c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若行者不見神時。",
        "from": "T21n1248_001:0225c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即虛空中有聲云。",
        "from": "T21n1248_001:0225c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝但存心持誦。",
        "from": "T21n1248_001:0225c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切事汝欲者悉如意。",
        "from": "T21n1248_001:0225c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不見神及不聞聲。",
        "from": "T21n1248_001:0225c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但有大風。",
        "from": "T21n1248_001:0225c27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或雲遍滿布其壇上。",
        "from": "T21n1248_001:0225c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其法即成就。",
        "from": "T21n1248_001:0225c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無上件等相。",
        "from": "T21n1248_001:0225c28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者誦呪七遍。",
        "from": "T21n1248_001:0225c29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縛得入者其法即成。",
        "from": "T21n1248_001:0225c29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更勿懷疑。",
        "from": "T21n1248_001:0226a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若行者欲得喝囉闍愛敬者。",
        "from": "T21n1248_001:0226a01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取赤小豆一顆呪一遍。",
        "from": "T21n1248_001:0226a02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "投火中燒。",
        "from": "T21n1248_001:0226a02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是滿一百八遍。",
        "from": "T21n1248_001:0226a03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其喝囉闍即遣人喚來極忽。",
        "from": "T21n1248_001:0226a03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲得大官人愛敬者。",
        "from": "T21n1248_001:0226a04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取白芥子一顆。",
        "from": "T21n1248_001:0226a04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一呪投火中燒滿一百八遍。",
        "from": "T21n1248_001:0226a05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其官即自來敬仰大喜。",
        "from": "T21n1248_001:0226a05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法於白月十五日作壇。",
        "from": "T21n1248_001:0226a06_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於像前作八肘。",
        "from": "T21n1248_001:0226a07_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牛糞塗地了。",
        "from": "T21n1248_001:0226a07_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取香泥重塗了。",
        "from": "T21n1248_001:0226a07_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以五色畫作三重院。",
        "from": "T21n1248_001:0226a08_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其壇內於中畫一大輪。",
        "from": "T21n1248_001:0226a08_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四角拔折羅十字安之。",
        "from": "T21n1248_001:0226a09_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二院內。",
        "from": "T21n1248_001:0226a09_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東面畫作一火珠光焰火出。",
        "from": "T21n1248_001:0226a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南面畫三股。",
        "from": "T21n1248_001:0226a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又西面畫一龍口中並出蓮華。",
        "from": "T21n1248_001:0226a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北面畫一大縲。",
        "from": "T21n1248_001:0226a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三院中東面更畫師子。",
        "from": "T21n1248_001:0226a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南面畫龍王。",
        "from": "T21n1248_001:0226a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西面畫孔雀王鳥。",
        "from": "T21n1248_001:0226a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北面畫羅剎手拄棒立地。",
        "from": "T21n1248_001:0226a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壇四角各安香水瓶。",
        "from": "T21n1248_001:0226a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內著雜菓楊枝。",
        "from": "T21n1248_001:0226a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪師洗浴著新淨衣。",
        "from": "T21n1248_001:0226a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於像前設種種飲食供養。",
        "from": "T21n1248_001:0226a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦呪取呵梨勒一顆。",
        "from": "T21n1248_001:0226a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一呪一遍投火中燒之。",
        "from": "T21n1248_001:0226a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿一百八遍。",
        "from": "T21n1248_001:0226a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從今已後。",
        "from": "T21n1248_001:0226a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每日送一[金*迸]金與呪師永無乏少。",
        "from": "T21n1248_001:0226a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若得此物即須用盡。",
        "from": "T21n1248_001:0226a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得留著。",
        "from": "T21n1248_001:0226a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法呪蘇蜜及菓子。",
        "from": "T21n1248_001:0226a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一百八遍火中燒之。",
        "from": "T21n1248_001:0226a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸天皆大歡喜擁護呪師。",
        "from": "T21n1248_001:0226a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲遠行時。",
        "from": "T21n1248_001:0226a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪穀木柱行一切難處。",
        "from": "T21n1248_001:0226a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉得無怖。",
        "from": "T21n1248_001:0226a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若惡人相惱亂者。",
        "from": "T21n1248_001:0226a22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心中想此呪神形。",
        "from": "T21n1248_001:0226a22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復舉心誦呪。",
        "from": "T21n1248_001:0226a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未滿一遍。",
        "from": "T21n1248_001:0226a23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人即不能動。",
        "from": "T21n1248_001:0226a23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲好者。",
        "from": "T21n1248_001:0226a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心放中相便得解脫。",
        "from": "T21n1248_001:0226a24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲令怨家不同住者。",
        "from": "T21n1248_001:0226a24_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取苦練子一百八顆。",
        "from": "T21n1248_001:0226a25_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一呪投火中。",
        "from": "T21n1248_001:0226a26_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人即自遠去。",
        "from": "T21n1248_001:0226a26_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若呪胡麻一百八遍。",
        "from": "T21n1248_001:0226a26_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散著火中。",
        "from": "T21n1248_001:0226a27_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切人盡來供養呪師。",
        "from": "T21n1248_001:0226a27_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若呪白芥子一百八遍散山中。",
        "from": "T21n1248_001:0226a28_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切惡獸不能傷人。",
        "from": "T21n1248_001:0226a29_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其狩見人即自伏地不敢傷害。",
        "from": "T21n1248_001:0226a29_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過去已後方即起去。",
        "from": "T21n1248_001:0226b01_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲乞雨時。",
        "from": "T21n1248_001:0226b01_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取杏子一百八顆都呪三七遍。",
        "from": "T21n1248_001:0226b02_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著有龍水中必得雨下。",
        "from": "T21n1248_001:0226b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲止雨時。",
        "from": "T21n1248_001:0226b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取梧桐子火中燒之即止。",
        "from": "T21n1248_001:0226b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若行者欲乞食。",
        "from": "T21n1248_001:0226b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先呪鉢一百八遍。",
        "from": "T21n1248_001:0226b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨所到處。",
        "from": "T21n1248_001:0226b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然而得一切飲食了。",
        "from": "T21n1248_001:0226b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲食殘者當時自化去。",
        "from": "T21n1248_001:0226b06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若須飲食時。",
        "from": "T21n1248_001:0226b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至心向鉢者誦呪一遍。",
        "from": "T21n1248_001:0226b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其鉢依還如故。",
        "from": "T21n1248_001:0226b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲見功能天者。",
        "from": "T21n1248_001:0226b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日一夜不食。",
        "from": "T21n1248_001:0226b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於佛前燒蘇合香。",
        "from": "T21n1248_001:0226b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪白花一百八遍。",
        "from": "T21n1248_001:0226b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以散神前。",
        "from": "T21n1248_001:0226b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其功能天即自現身。",
        "from": "T21n1248_001:0226b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意所須一切願滿。",
        "from": "T21n1248_001:0226b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求財寶即得稱意。",
        "from": "T21n1248_001:0226b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲辟除一切鬼神。",
        "from": "T21n1248_001:0226b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪白芥子及蘇。",
        "from": "T21n1248_001:0226b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪一顆一遍。",
        "from": "T21n1248_001:0226b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "投火中燒之即得。",
        "from": "T21n1248_001:0226b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有官人。",
        "from": "T21n1248_001:0226b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於三寶中起不善心者。",
        "from": "T21n1248_001:0226b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惱亂佛弟子者。",
        "from": "T21n1248_001:0226b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲令起慈心。",
        "from": "T21n1248_001:0226b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須作降伏。",
        "from": "T21n1248_001:0226b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪手三十一遍。",
        "from": "T21n1248_001:0226b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遙打之其人即著疾。",
        "from": "T21n1248_001:0226b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲好者慈心誦一遍即差。",
        "from": "T21n1248_001:0226b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法呪土二十一遍。",
        "from": "T21n1248_001:0226b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散自身上。",
        "from": "T21n1248_001:0226b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切人見無不敬仰。",
        "from": "T21n1248_001:0226b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲令自身有威光者。",
        "from": "T21n1248_001:0226b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪墨塗額上。",
        "from": "T21n1248_001:0226b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切人見者無不愛仰。",
        "from": "T21n1248_001:0226b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪灰二十一遍。",
        "from": "T21n1248_001:0226b18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗其身大眾中一切人見皆福德相。",
        "from": "T21n1248_001:0226b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾人供養。",
        "from": "T21n1248_001:0226b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲得遠涉道路不被惡人惡狩傷害。",
        "from": "T21n1248_001:0226b20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行加奔馬。",
        "from": "T21n1248_001:0226b21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪左手大母指作印。",
        "from": "T21n1248_001:0226b21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而去欲得於鏡看事。",
        "from": "T21n1248_001:0226b22_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪童子童女問吉凶。",
        "from": "T21n1248_001:0226b22_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人即自下語。",
        "from": "T21n1248_001:0226b23_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令道所病鬼姓名。",
        "from": "T21n1248_001:0226b23_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即知是何等之病。",
        "from": "T21n1248_001:0226b23_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若被蠱毒食者。",
        "from": "T21n1248_001:0226b24_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪所食物二十一遍。",
        "from": "T21n1248_001:0226b24_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其所為毒即吐出。",
        "from": "T21n1248_001:0226b25_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患鬼病呪五色線。",
        "from": "T21n1248_001:0226b25_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一遍一結一百八遍了。",
        "from": "T21n1248_001:0226b26_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫頭上或頂上臂上。",
        "from": "T21n1248_001:0226b26_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切病患除差。",
        "from": "T21n1248_001:0226b27_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患心病。",
        "from": "T21n1248_001:0226b27_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪石榴花汁飲之即愈。",
        "from": "T21n1248_001:0226b27_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲攝狐魅病。",
        "from": "T21n1248_001:0226b28_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪五色線。",
        "from": "T21n1248_001:0226b28_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令童子合成索。",
        "from": "T21n1248_001:0226b29_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一呪一結一百八遍。",
        "from": "T21n1248_001:0226b29_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫項下。",
        "from": "T21n1248_001:0226b29_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復呪楊枝打病者即立愈。",
        "from": "T21n1248_001:0226c01_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患骨節楚病。",
        "from": "T21n1248_001:0226c01_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪宥刀禁其病即愈。",
        "from": "T21n1248_001:0226c02_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲護身。",
        "from": "T21n1248_001:0226c02_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪白芥子二十一遍。",
        "from": "T21n1248_001:0226c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若灰若水亦然。",
        "from": "T21n1248_001:0226c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散四方為界。",
        "from": "T21n1248_001:0226c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若頭病或身病呪蘇烏麻油二十一遍。",
        "from": "T21n1248_001:0226c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩塗其身即愈。",
        "from": "T21n1248_001:0226c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患耳風呪蘇二十一遍食之愈。",
        "from": "T21n1248_001:0226c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患眼。",
        "from": "T21n1248_001:0226c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪杏人油塗之立愈。",
        "from": "T21n1248_001:0226c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患腫。",
        "from": "T21n1248_001:0226c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪石榴黃一百八遍塗上即愈。",
        "from": "T21n1248_001:0226c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或白檀香亦得。",
        "from": "T21n1248_001:0226c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患頭病。",
        "from": "T21n1248_001:0226c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪大黃二十一遍。",
        "from": "T21n1248_001:0226c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗額上即愈。",
        "from": "T21n1248_001:0226c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患時氣病。",
        "from": "T21n1248_001:0226c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪青木香末二十一遍。",
        "from": "T21n1248_001:0226c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和水服之立愈。",
        "from": "T21n1248_001:0226c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患惡瘡疥癬者。",
        "from": "T21n1248_001:0226c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取利蘆未和油塗上立即除愈。",
        "from": "T21n1248_001:0226c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患瘧病。",
        "from": "T21n1248_001:0226c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪楊枝二十一遍令打病人即時除愈。",
        "from": "T21n1248_001:0226c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若被蝎螫。",
        "from": "T21n1248_001:0226c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪乾脯火中燒之。",
        "from": "T21n1248_001:0226c13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "熱拄其上便即除愈。",
        "from": "T21n1248_001:0226c13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若被蛇咬。",
        "from": "T21n1248_001:0226c14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪畢鉢二十一遍。",
        "from": "T21n1248_001:0226c14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗之當下即愈。",
        "from": "T21n1248_001:0226c14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若婦人乳腫。",
        "from": "T21n1248_001:0226c15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪油麻嚼塗上即差。",
        "from": "T21n1248_001:0226c15_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若帶下病者。",
        "from": "T21n1248_001:0226c15_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪丁香水服之即愈。",
        "from": "T21n1248_001:0226c16_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人卒被鬼著狂言荒語。",
        "from": "T21n1248_001:0226c16_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪水令服之。",
        "from": "T21n1248_001:0226c17_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患心病。",
        "from": "T21n1248_001:0226c17_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪黃土塗之。",
        "from": "T21n1248_001:0226c18_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患一切鬼魅者。",
        "from": "T21n1248_001:0226c18_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪石榴枝一百八遍。",
        "from": "T21n1248_001:0226c18_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用打病者即愈。",
        "from": "T21n1248_001:0226c19_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲得成就三事者。",
        "from": "T21n1248_001:0226c19_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當於像前作壇。",
        "from": "T21n1248_001:0226c20_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壇內散種種名花。",
        "from": "T21n1248_001:0226c20_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設種種飲食訖。",
        "from": "T21n1248_001:0227a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取赤銅鈝一枚。",
        "from": "T21n1248_001:0227a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雄黃三兩細擣和蜜水為丸置像前。",
        "from": "T21n1248_001:0227a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取荷葉覆上。",
        "from": "T21n1248_001:0227a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以手掩之呪不息。",
        "from": "T21n1248_001:0227a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當有相現。",
        "from": "T21n1248_001:0227a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若烟出時取塗身上。",
        "from": "T21n1248_001:0227a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成安怛陀那。",
        "from": "T21n1248_001:0227a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若火出時。",
        "from": "T21n1248_001:0227a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手取火焰用塗面上。",
        "from": "T21n1248_001:0227a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生見此人者皆大歡喜敬仰有大威光。",
        "from": "T21n1248_001:0227a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出言教人皆信受恭敬隨順。",
        "from": "T21n1248_001:0227a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有煖氣其人即取塗身即得無盡。",
        "from": "T21n1248_001:0227a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法於舍利塔前呪牛黃。",
        "from": "T21n1248_001:0227a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依前藥法皆得塗身一切福德盡集其身。",
        "from": "T21n1248_001:0227a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所在眾生見如羅闍無異。",
        "from": "T21n1248_001:0227a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若知山間有孔似阿修羅窟者。",
        "from": "T21n1248_001:0227a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當呪白芥子和自身血。",
        "from": "T21n1248_001:0227a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一呪一遍打山間孔。",
        "from": "T21n1248_001:0227a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡一千八遍。",
        "from": "T21n1248_001:0227a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其山窟自開。",
        "from": "T21n1248_001:0227a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其阿修羅女身如火然。",
        "from": "T21n1248_001:0227a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持諸香花迎呪師來。",
        "from": "T21n1248_001:0227a12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者入此窟內。",
        "from": "T21n1248_001:0227a13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽命一大劫身力還如阿修羅。",
        "from": "T21n1248_001:0227a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦如金剛無能損者。",
        "from": "T21n1248_001:0227a14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲出時亦得一切阿修羅女隨從。",
        "from": "T21n1248_001:0227a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不還時亦到火終無所害。",
        "from": "T21n1248_001:0227a15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法將此像向舍利塔前。",
        "from": "T21n1248_001:0227a16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香塗地香花散於壇內。",
        "from": "T21n1248_001:0227a16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上妙供養了。",
        "from": "T21n1248_001:0227a17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取梨木作合子。",
        "from": "T21n1248_001:0227a17_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取安膳那石為末。",
        "from": "T21n1248_001:0227a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用秦膠水為丸。",
        "from": "T21n1248_001:0227a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盛於合子中置壇中。",
        "from": "T21n1248_001:0227a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者著新淨衣。",
        "from": "T21n1248_001:0227a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在於像前一日不食。",
        "from": "T21n1248_001:0227a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此呪一千八遍了。",
        "from": "T21n1248_001:0227a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦無勞三相現。",
        "from": "T21n1248_001:0227a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪一千八遍訖塗眼竟。",
        "from": "T21n1248_001:0227a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即塗脚掌心中胸前兩膊上頂竟。",
        "from": "T21n1248_001:0227a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至心合掌思身如虛空。",
        "from": "T21n1248_001:0227a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有身相作自想時。",
        "from": "T21n1248_001:0227a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者隱形日行千里。",
        "from": "T21n1248_001:0227a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切事業竝皆悉見。",
        "from": "T21n1248_001:0227a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法欲得伏外道入火中。",
        "from": "T21n1248_001:0227a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取自身血魚血。",
        "from": "T21n1248_001:0227a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞋心誦此呪二十一遍。",
        "from": "T21n1248_001:0227a25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "投火中即入火中。",
        "from": "T21n1248_001:0227a25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得一頃食間不燒其身。",
        "from": "T21n1248_001:0227a26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲降伏外道。",
        "from": "T21n1248_001:0227a26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手中結印。",
        "from": "T21n1248_001:0227a27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞋心誦呪一百八遍。",
        "from": "T21n1248_001:0227a27_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外道所作法悉破壞竝不成就。",
        "from": "T21n1248_001:0227a27_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲降伏諸國兵賊眾者。",
        "from": "T21n1248_001:0227a28_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當畫一像身卦紫磨真金甲。",
        "from": "T21n1248_001:0227a29_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於淨室中燒眾名香乳頭薰陸香。",
        "from": "T21n1248_001:0227b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸色香花飲食供養。",
        "from": "T21n1248_001:0227b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真心誦念天王真言十萬遍。",
        "from": "T21n1248_001:0227b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天王領天兵來助。",
        "from": "T21n1248_001:0227b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他國兵敵自退散。",
        "from": "T21n1248_001:0227b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能晝夜誦念不絕。",
        "from": "T21n1248_001:0227b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天王使太子獨健。",
        "from": "T21n1248_001:0227b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "領天兵千人衛護不離其側。",
        "from": "T21n1248_001:0227b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求如意應念隨心。",
        "from": "T21n1248_001:0227b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆得成就。",
        "from": "T21n1248_001:0227b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲降前敵眾者。",
        "from": "T21n1248_001:0227b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於淨室持齋畫一天王形像。",
        "from": "T21n1248_001:0227b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卦紫磨真金甲。",
        "from": "T21n1248_001:0227b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於二丈竿懸。",
        "from": "T21n1248_001:0227b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "軍前五十步指其敵。",
        "from": "T21n1248_001:0227b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其敵不能相患。",
        "from": "T21n1248_001:0227b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印相二小指相鉤。",
        "from": "T21n1248_001:0227b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二無名指向外直豎。",
        "from": "T21n1248_001:0227b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指二頭指反相叉。",
        "from": "T21n1248_001:0227b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大母指向外直豎。",
        "from": "T21n1248_001:0227b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手掌背合即是其印。",
        "from": "T21n1248_001:0227b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前所說一一諸法。",
        "from": "T21n1248_001:0227b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆用此印即得成就。",
        "from": "T21n1248_001:0227b11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今我說根本印。",
        "from": "T21n1248_001:0227b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手右押左內相叉。",
        "from": "T21n1248_001:0227b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豎無名頭相合。",
        "from": "T21n1248_001:0227b13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指屈如鉤。",
        "from": "T21n1248_001:0227b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若迎請時向身招。",
        "from": "T21n1248_001:0227b13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若撥遣時外撥。",
        "from": "T21n1248_001:0227b14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念請時結印當心誦七遍。",
        "from": "T21n1248_001:0227b14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印頂上散。",
        "from": "T21n1248_001:0227b15_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後取念珠專住念誦。",
        "from": "T21n1248_001:0227b15_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說吉祥天女印。",
        "from": "T21n1248_001:0227b16_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手虛心合掌開二頭指二中指二無名指。",
        "from": "T21n1248_001:0227b16_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未屈如蓮華形。",
        "from": "T21n1248_001:0227b17_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指二小指豎合。",
        "from": "T21n1248_001:0227b18_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若念誦當心結印。",
        "from": "T21n1248_001:0227b18_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言七遍頂上散印。",
        "from": "T21n1248_001:0227b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四天王經云。",
        "from": "T21n1248_001:0227b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人誦念此北方毘沙門天王真言。",
        "from": "T21n1248_001:0227b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養苽菓飲食。",
        "from": "T21n1248_001:0227b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每月取一十五日二十一日。",
        "from": "T21n1248_001:0227b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "音聲用單雙絃方響鼓舞。",
        "from": "T21n1248_001:0227b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘇蜜乳粥真心誦念其天王即見其身隨心所願。",
        "from": "T21n1248_001:0227b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解穢陀羅尼。",
        "from": "T21n1248_001:0227b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵俱嚕馱唵[口*(尤尢+曳)]讚天王頌曰。",
        "from": "T21n1248_001:0227b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善利萬物兮惟彼水方　護持國界兮惟我天王　良將受委兮鎮固封彊　精心奉職兮宣託福祥　星宮上勝兮迴架崇岡　靈像儼立兮眾寶嚴莊　威神震曜兮攝伏荒保　又忠貞□兮翅贊聖唐圓行錄云。",
        "from": "T21n1248_001:0227b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方毘沙門天王隨軍護法真言一卷。",
        "from": "T21n1248_001:0227c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空三藏別行翻譯。",
        "from": "T21n1248_001:0227c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八家錄亦爾也。",
        "from": "T21n1248_001:0227c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此法請寫於智山慈順僧正染筆。",
        "from": "T21n1248_001:0227c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "栂尾山之本。",
        "from": "T21n1248_001:0227c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛染院大宣等對挍之。",
        "from": "T21n1248_001:0227c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "予亦挍閱以上本。",
        "from": "T21n1248_001:0227c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　時享和辛酉八月　　長谷僧快道誌　一校加筆畢　　　　　　　　慈順",
        "from": "T21n1248_001:0227c06_0",
        "context_with_next_sentence": false
    }
]