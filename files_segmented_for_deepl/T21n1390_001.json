[
    {
        "source_sentence": "佛說洛叉陀羅尼經",
        "from": "T21n1390_001:0907b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試光祿卿",
        "from": "T21n1390_001:0907b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明教大師臣法賢奉　詔譯",
        "from": "T21n1390_001:0907b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告阿難言。",
        "from": "T21n1390_001:0907b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當諦聽今我與汝。",
        "from": "T21n1390_001:0907b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宣說洛叉陀羅尼。",
        "from": "T21n1390_001:0907b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是陀羅尼難得值遇。",
        "from": "T21n1390_001:0907b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由如諸佛。",
        "from": "T21n1390_001:0907b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出於世間。",
        "from": "T21n1390_001:0907b25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難若有眾生。",
        "from": "T21n1390_001:0907b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得此陀羅尼能受持者。",
        "from": "T21n1390_001:0907b25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人所獲功德如持洛叉佛無異。",
        "from": "T21n1390_001:0907b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能與眾生成大福聚。",
        "from": "T21n1390_001:0907b26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能滅眾生無量重罪。",
        "from": "T21n1390_001:0907b27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊即說洛叉陀羅尼曰。",
        "from": "T21n1390_001:0907b27_11",
        "context_with_next_sentence": true
    },
