[
    {
        "source_sentence": "金剛頂降三世大儀軌法王教中",
        "from": "T20n1040_001:0030c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀自在菩薩心真言一切如來蓮",
        "from": "T20n1040_001:0030c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "花大曼拏攞品一卷",
        "from": "T20n1040_001:0030c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T20n1040_001:0030c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空諡大鑒",
        "from": "T20n1040_001:0030c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正號大廣智大興善寺三藏沙門",
        "from": "T20n1040_001:0030c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空奉　詔譯",
        "from": "T20n1040_001:0030c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時觀自在菩薩摩訶薩。",
        "from": "T20n1040_001:0030c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言。",
        "from": "T20n1040_001:0030c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我欲於此降三世儀軌法王會中說自心真言。",
        "from": "T20n1040_001:0030c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊若纔稱說一切如來。",
        "from": "T20n1040_001:0030c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三昧現前一切怖畏厄難災障。",
        "from": "T20n1040_001:0030c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸病惱皆得消滅。",
        "from": "T20n1040_001:0030c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時薄伽梵。",
        "from": "T20n1040_001:0030c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告觀自在菩薩言。",
        "from": "T20n1040_001:0030c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉汝應善說。",
        "from": "T20n1040_001:0030c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時觀自在菩薩摩訶薩。",
        "from": "T20n1040_001:0030c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "承佛聖旨即說自心真言曰。",
        "from": "T20n1040_001:0030c27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿嚕禮迦(半)娑嚩(二合引)賀(引)適說此心真言時。",
        "from": "T20n1040_001:0030c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來從本剎土。",
        "from": "T20n1040_001:0031a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雲集蘇迷盧頂毘盧遮那大會之中。",
        "from": "T20n1040_001:0031a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圍遶觀自在菩薩摩訶薩復說自心真言大曼拏攞。",
        "from": "T20n1040_001:0031a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　院位擘方設　　內四角四門",
        "from": "T20n1040_001:0031a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　善度最中央　　井形拼四道",
        "from": "T20n1040_001:0031a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　顯成於八位　　分為八葉花",
        "from": "T20n1040_001:0031a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　是色應用朱　　中臺觀自在",
        "from": "T20n1040_001:0031a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　於彼一一葉　　圖畫大覺形",
        "from": "T20n1040_001:0031a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　中院不為門　　內外八線道",
        "from": "T20n1040_001:0031a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　微妙建立已　　阿闍梨當入",
        "from": "T20n1040_001:0031a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　應對曼拏攞　　結大蓮花契",
        "from": "T20n1040_001:0031a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　端身定支節　　住於三摩地",
        "from": "T20n1040_001:0031a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　次結盎俱施　　奉請於聖者",
        "from": "T20n1040_001:0031a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若一切有情　　入此曼拏攞",
        "from": "T20n1040_001:0031a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　雖處於凡位　　貪嗔罪垢滅",
        "from": "T20n1040_001:0031a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　猶如於蓮花　　不為水所著",
        "from": "T20n1040_001:0031a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　勝生亦如是　　不為欲所染",
        "from": "T20n1040_001:0031a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿闍梨即以大蓮花印。",
        "from": "T20n1040_001:0031a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈進力度如鉤。",
        "from": "T20n1040_001:0031a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請觀自在菩薩。",
        "from": "T20n1040_001:0031a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮花鉤真言曰。",
        "from": "T20n1040_001:0031a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)摩訶(引)跛娜摩(二合)吽惹即時觀自在菩薩便至道場現微妙色身。",
        "from": "T20n1040_001:0031a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或放光明。",
        "from": "T20n1040_001:0031a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿闍梨次當如法引弟子入。",
        "from": "T20n1040_001:0031a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸弟子應著白衣。",
        "from": "T20n1040_001:0031a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復以白繒掩覆其服。",
        "from": "T20n1040_001:0031a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以淨帛纏繫其頭。",
        "from": "T20n1040_001:0031a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既引入已而說偈言。",
        "from": "T20n1040_001:0031a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　猶如於蓮花　　不為水所著",
        "from": "T20n1040_001:0031a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　勝生亦如是　　不為諸欲染",
        "from": "T20n1040_001:0031a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即應左手授與弟子蓮花。",
        "from": "T20n1040_001:0031a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手授與念珠。",
        "from": "T20n1040_001:0031a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又說偈言。",
        "from": "T20n1040_001:0031a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　諸佛大蓮花　　於諸曼拏攞",
        "from": "T20n1040_001:0031a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　最為其上首　　常須分別思",
        "from": "T20n1040_001:0031b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　身奉觀自在",
        "from": "T20n1040_001:0031b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後教印及三摩地門。",
        "from": "T20n1040_001:0031b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心真言蓮花曼拏攞品",
        "from": "T20n1040_001:0031b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次如前建立曼拏攞。",
        "from": "T20n1040_001:0031b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者應坐中臺之上。",
        "from": "T20n1040_001:0031b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入一切如來開敷蓮花三摩地。",
        "from": "T20n1040_001:0031b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦一洛叉即得觀自在身。",
        "from": "T20n1040_001:0031b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心真言三昧耶曼拏攞品",
        "from": "T20n1040_001:0031b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復以如前建立曼拏攞。",
        "from": "T20n1040_001:0031b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應取一千蓮花。",
        "from": "T20n1040_001:0031b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一花一誦安中臺以為奉獻。",
        "from": "T20n1040_001:0031b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後引弟子入告三昧耶言(謂諸欲清淨誓也)。",
        "from": "T20n1040_001:0031b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羯磨曼拏攞品",
        "from": "T20n1040_001:0031b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次如前建立曼拏攞。",
        "from": "T20n1040_001:0031b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以四種供養。",
        "from": "T20n1040_001:0031b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次成就像法。",
        "from": "T20n1040_001:0031b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於清淨絹[疊*毛]上。",
        "from": "T20n1040_001:0031b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫觀自在菩薩。",
        "from": "T20n1040_001:0031b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐蘇彌盧頂八佛圍遶。",
        "from": "T20n1040_001:0031b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起白月一日於此像前。",
        "from": "T20n1040_001:0031b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切時無間斷入三摩地念誦。",
        "from": "T20n1040_001:0031b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至畫像放光。",
        "from": "T20n1040_001:0031b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於光明中流出觀自在菩薩及諸如來。",
        "from": "T20n1040_001:0031b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入行者眉間灌頂之處。",
        "from": "T20n1040_001:0031b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅固而住。",
        "from": "T20n1040_001:0031b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即時行者體同觀自在。",
        "from": "T20n1040_001:0031b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剎那頃便見極樂世界。",
        "from": "T20n1040_001:0031b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生皆見去時壽命無量。",
        "from": "T20n1040_001:0031b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今又說成就之法。",
        "from": "T20n1040_001:0031b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦一洛叉。",
        "from": "T20n1040_001:0031b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切蓋障皆得清淨。",
        "from": "T20n1040_001:0031b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦二洛叉。",
        "from": "T20n1040_001:0031b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有五無間等一切諸罪盡滅無餘。",
        "from": "T20n1040_001:0031b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦三洛叉。",
        "from": "T20n1040_001:0031b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切福德來集。",
        "from": "T20n1040_001:0031b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來等備具。",
        "from": "T20n1040_001:0031b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切成智現前。",
        "from": "T20n1040_001:0031b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次若結大印(謂五相成就)誦洛叉數。",
        "from": "T20n1040_001:0031b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經夜無限念誦。",
        "from": "T20n1040_001:0031b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比至晨朝一切如來入行者心。",
        "from": "T20n1040_001:0031b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即體同如來。",
        "from": "T20n1040_001:0031b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若入於口即體同觀自在。",
        "from": "T20n1040_001:0031b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次成就事業。",
        "from": "T20n1040_001:0031b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦一遍護身。",
        "from": "T20n1040_001:0031b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩遍護他及城邑聚落。",
        "from": "T20n1040_001:0031b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三遍即能成辦一切事業。",
        "from": "T20n1040_001:0031b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若阿尾捨應燒檀香(加持八百遍爐中燒薰支分)若息諸鬼魅者。",
        "from": "T20n1040_001:0031b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加呬唎(二合)字誦之。",
        "from": "T20n1040_001:0031c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令一切有情順。",
        "from": "T20n1040_001:0031c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持金剛花與之(赤花代之)令惡心不動。",
        "from": "T20n1040_001:0031c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以白土止息之(真言首加書朱書之名後即卷置一瓶之中以白土實之安於一坑之中後土填之想室彼兌即安須彌山於其上下亦得耳)令悉底利敬愛。",
        "from": "T20n1040_001:0031c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持蘇末那花。",
        "from": "T20n1040_001:0031c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與之伏從。",
        "from": "T20n1040_001:0031c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又欲令一切禽獸隨處不動轉白芥子令一切禽獸不能動。",
        "from": "T20n1040_001:0031c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持白芥子遙向擲之。",
        "from": "T20n1040_001:0031c07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲有所示現。",
        "from": "T20n1040_001:0031c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持孔雀尾遙轉。",
        "from": "T20n1040_001:0031c08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲夢中知一切事。",
        "from": "T20n1040_001:0031c08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三念誦其夜見之。",
        "from": "T20n1040_001:0031c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又欲除惡夢者。",
        "from": "T20n1040_001:0031c09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以油麻護摩。",
        "from": "T20n1040_001:0031c09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又欲止息口舌取雞翅翎護摩。",
        "from": "T20n1040_001:0031c10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若處子婆誐蘂淫女月帛護摩。",
        "from": "T20n1040_001:0031c10_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉嚩始迦羅拏。",
        "from": "T20n1040_001:0031c11_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若求一切事應廣供養觀自在。",
        "from": "T20n1040_001:0031c11_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八日一日夜不食念誦滿一切願。",
        "from": "T20n1040_001:0031c12_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又於觀自在菩薩前。",
        "from": "T20n1040_001:0031c13_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三時時別誦三遍。",
        "from": "T20n1040_001:0031c13_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿三月菩薩滿其所願。",
        "from": "T20n1040_001:0031c14_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又於佛像前三時時別誦三遍。",
        "from": "T20n1040_001:0031c14_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿三月佛滿其願。",
        "from": "T20n1040_001:0031c15_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印品",
        "from": "T20n1040_001:0031c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌以二水二風交地空各開竪。",
        "from": "T20n1040_001:0031c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二火相合是根本印。",
        "from": "T20n1040_001:0031c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "芙蓉合掌是為大蓮花印。",
        "from": "T20n1040_001:0031c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不易大蓮花開竪地空。",
        "from": "T20n1040_001:0031c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是禳奪諸罪印。",
        "from": "T20n1040_001:0031c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又外叉金剛縛。",
        "from": "T20n1040_001:0031c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合二風如蓮花葉。",
        "from": "T20n1040_001:0031c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二空竪相逼是三昧印。",
        "from": "T20n1040_001:0031c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又分二手作蓮花拳。",
        "from": "T20n1040_001:0031c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便以慧拳旋繞定拳。",
        "from": "T20n1040_001:0031c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作開八葉勢是羯磨印(請如來)呬唎(二合)字觀於心月蓮。",
        "from": "T20n1040_001:0031c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是法印。",
        "from": "T20n1040_001:0031c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀諸法如蓮花不著於水。",
        "from": "T20n1040_001:0031c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸法亦然不染欲等。",
        "from": "T20n1040_001:0031c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是智印。",
        "from": "T20n1040_001:0031c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮花阿闍梨。",
        "from": "T20n1040_001:0031c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲授弟子阿闍梨位。",
        "from": "T20n1040_001:0031c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以蓮花授與弟子二掌中。",
        "from": "T20n1040_001:0031c25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又加持蓮花鬘一千八遍。",
        "from": "T20n1040_001:0031c25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫弟子頭上。",
        "from": "T20n1040_001:0031c26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持。",
        "from": "T20n1040_001:0031c26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1040_001:0031c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)跛娜麼(二合)麼(引)攞(引)鼻詵左路(引)計濕嚩(二合)囉數索由是得為蓮花部中阿闍梨。",
        "from": "T20n1040_001:0031c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今說蓮花部修行禁戒儀。",
        "from": "T20n1040_001:0032a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則先供養觀自在菩薩。",
        "from": "T20n1040_001:0032a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應服一掬遇牟怛囉若少分摩夷。",
        "from": "T20n1040_001:0032a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即一切清淨。",
        "from": "T20n1040_001:0032a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然觀自身作觀自在菩薩。",
        "from": "T20n1040_001:0032a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日誦一千遍乃至七日。",
        "from": "T20n1040_001:0032a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禁戒得成就。",
        "from": "T20n1040_001:0032a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從此已後一切食一切處作。",
        "from": "T20n1040_001:0032a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆不為污染。",
        "from": "T20n1040_001:0032a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今說普通一切供養儀。",
        "from": "T20n1040_001:0032a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則二手為金剛拳。",
        "from": "T20n1040_001:0032a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各安於腰側。",
        "from": "T20n1040_001:0032a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向左小低頭。",
        "from": "T20n1040_001:0032a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為恭敬禮。",
        "from": "T20n1040_001:0032a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又二手從額分向後繫鬘勢。",
        "from": "T20n1040_001:0032a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歷度下垂帶。",
        "from": "T20n1040_001:0032a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為獻花鬘。",
        "from": "T20n1040_001:0032a09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又二手從於臍漸上至口寫。",
        "from": "T20n1040_001:0032a09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為獻歌詠。",
        "from": "T20n1040_001:0032a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又二手從口檀慧歷展相旋遶上頂虛合掌。",
        "from": "T20n1040_001:0032a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是獻金剛舞。",
        "from": "T20n1040_001:0032a11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由是供養如來位尚不難得。",
        "from": "T20n1040_001:0032a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "況餘悉地。",
        "from": "T20n1040_001:0032a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故。",
        "from": "T20n1040_001:0032a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切樂不如此金剛薩埵故。",
        "from": "T20n1040_001:0032a12_18",
        "context_with_next_sentence": false
    }
]