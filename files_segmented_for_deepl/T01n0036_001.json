[
    {
        "source_sentence": "佛說本相猗致經",
        "from": "T01n0036_001:0819c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後漢安息國三藏安世高譯",
        "from": "T01n0036_001:0819c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞如是：一時，佛在舍衛國祇樹給孤獨園。",
        "from": "T01n0036_001:0819c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛便告比丘：「本有愛不見不了，今見有從有愛，設是本有愛無有今為有，今見分明。",
        "from": "T01n0036_001:0819c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從是本因緣，令致有愛，有愛，比丘！",
        "from": "T01n0036_001:0819c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從致有本，不為無有本。",
        "from": "T01n0036_001:0819c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0819c26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從有愛致本？",
        "from": "T01n0036_001:0819c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂為癡。",
        "from": "T01n0036_001:0819c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「癡亦，比丘！",
        "from": "T01n0036_001:0819c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有從致本，不為無有本。",
        "from": "T01n0036_001:0819c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0819c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "癡有本從致？",
        "from": "T01n0036_001:0819c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂為五蓋。",
        "from": "T01n0036_001:0819c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「五蓋，比丘！",
        "from": "T01n0036_001:0819c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦從有本致，不為無有本。",
        "from": "T01n0036_001:0819c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0819c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五蓋從有致？",
        "from": "T01n0036_001:0820a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂為三惡行。",
        "from": "T01n0036_001:0820a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「三惡行，比丘！",
        "from": "T01n0036_001:0820a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有本從致，不為無有本。",
        "from": "T01n0036_001:0820a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0820a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三惡行本從致？",
        "from": "T01n0036_001:0820a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂為不攝根。",
        "from": "T01n0036_001:0820a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「不攝根，比丘！",
        "from": "T01n0036_001:0820a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有本從致，不為無有本。",
        "from": "T01n0036_001:0820a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0820a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不攝根從致？",
        "from": "T01n0036_001:0820a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂非本念故。",
        "from": "T01n0036_001:0820a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「非本念，比丘！",
        "from": "T01n0036_001:0820a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有本從致，不為無有本。",
        "from": "T01n0036_001:0820a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0820a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非本念從致？",
        "from": "T01n0036_001:0820a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂不信故。",
        "from": "T01n0036_001:0820a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「不信，比丘！",
        "from": "T01n0036_001:0820a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有本從致，不為無有本。",
        "from": "T01n0036_001:0820a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0820a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不信本從致？",
        "from": "T01n0036_001:0820a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂惡非法聞故。",
        "from": "T01n0036_001:0820a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「非法聞，比丘！",
        "from": "T01n0036_001:0820a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有本從致，不為無有本。",
        "from": "T01n0036_001:0820a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0820a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非法聞本從致？",
        "from": "T01n0036_001:0820a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂非賢者人事。",
        "from": "T01n0036_001:0820a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「非賢者亦有本從致，不為無有本。",
        "from": "T01n0036_001:0820a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0820a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非賢者從致？",
        "from": "T01n0036_001:0820a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂非賢者人共會樂。",
        "from": "T01n0036_001:0820a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「如是，比丘，已不賢者聚會滿，令不賢者事滿；已不賢者事滿，令非法滿；已非法滿，令不信滿；已不信滿，令非本念滿；已非本念滿，令不攝根滿；已不攝根滿，令三惡行犯法滿；已三惡行犯法滿，令五蓋滿；已五蓋滿，令癡滿；已癡滿，令有世間愛滿。",
        "from": "T01n0036_001:0820a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是愛樂滿，稍轉稍轉猗增有。",
        "from": "T01n0036_001:0820a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「度世智慧解脫亦有本，不為無有本。",
        "from": "T01n0036_001:0820a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為度世智慧解脫本？",
        "from": "T01n0036_001:0820a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂七覺意為本。",
        "from": "T01n0036_001:0820a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「七覺意從有本，不為無有本。",
        "from": "T01n0036_001:0820a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為七覺意從有本？",
        "from": "T01n0036_001:0820a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂為四意止。",
        "from": "T01n0036_001:0820a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「四意止從有本，不為無有本。",
        "from": "T01n0036_001:0820a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0820a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四意止從有本？",
        "from": "T01n0036_001:0820a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂三清淨行。",
        "from": "T01n0036_001:0820a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「三清淨，比丘！",
        "from": "T01n0036_001:0820a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有本從行，不為無有本。",
        "from": "T01n0036_001:0820a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0820a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三清淨有本？",
        "from": "T01n0036_001:0820a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂為守攝根。",
        "from": "T01n0036_001:0820a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「守攝根亦有本，不為無有本。",
        "from": "T01n0036_001:0820a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0820a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "守攝根有本？",
        "from": "T01n0036_001:0820a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂為本念故。",
        "from": "T01n0036_001:0820b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「本念，比丘！",
        "from": "T01n0036_001:0820b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有本，不為無有本。",
        "from": "T01n0036_001:0820b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0820b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本念本？",
        "from": "T01n0036_001:0820b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂為信本。",
        "from": "T01n0036_001:0820b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「信本，比丘！",
        "from": "T01n0036_001:0820b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有本，不為無有本。",
        "from": "T01n0036_001:0820b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為，比丘！",
        "from": "T01n0036_001:0820b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信本有本？",
        "from": "T01n0036_001:0820b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂聞法經本。",
        "from": "T01n0036_001:0820b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「聞法經亦有本，不為無有本。",
        "from": "T01n0036_001:0820b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為聞法經本？",
        "from": "T01n0036_001:0820b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂事賢者本。",
        "from": "T01n0036_001:0820b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「事賢者亦有本，不為無有本。",
        "from": "T01n0036_001:0820b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為事賢者有本？",
        "from": "T01n0036_001:0820b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂賢者聚本。",
        "from": "T01n0036_001:0820b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「如是，比丘！",
        "from": "T01n0036_001:0820b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聚賢者能得事賢者；已事賢者，便聞法言；已聞法言，便致信本；已致信本，便得念本；已得念本，便攝守根；已攝守根，便得三清淨；已有三清淨，便得四意止；已得四意止本，便有七覺意；已有七覺意，便有無為解脫得度世。",
        "from": "T01n0036_001:0820b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是解脫度世，轉轉本，令得度世。」",
        "from": "T01n0036_001:0820b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說如是，弟子受行。",
        "from": "T01n0036_001:0820b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說本相猗致經",
        "from": "T01n0036_001:0820b17_0",
        "context_with_next_sentence": false
    }
]