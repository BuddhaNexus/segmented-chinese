[
    {
        "source_sentence": "金剛頂經金剛界大道場毘盧",
        "from": "T18n0878_001:0335c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遮那如來自受用身內證智眷屬",
        "from": "T18n0878_001:0335c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法身異名佛最上乘祕密三摩地",
        "from": "T18n0878_001:0335c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮懺文",
        "from": "T18n0878_001:0335c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T18n0878_001:0335c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空諡大鑑",
        "from": "T18n0878_001:0335c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正號大廣智大興善寺三藏沙門",
        "from": "T18n0878_001:0335c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空奉　詔譯",
        "from": "T18n0878_001:0335c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初禮請佛梵歎依常。",
        "from": "T18n0878_001:0335c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南謨常住三世淨妙法身金剛界大悲毘盧遮那佛。",
        "from": "T18n0878_001:0336a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南謨金剛堅固自性身阿閦佛。",
        "from": "T18n0878_001:0336a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南謨福德莊嚴聚身寶生佛。",
        "from": "T18n0878_001:0336a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南謨受用智慧身阿彌陀佛。",
        "from": "T18n0878_001:0336a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南謨作變化身不空成就佛。",
        "from": "T18n0878_001:0336a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南謨大圓鏡智金剛波羅蜜出生。",
        "from": "T18n0878_001:0336a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡虛空遍法界塵剎波羅蜜等一切諸佛。",
        "from": "T18n0878_001:0336a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南謨平等性智寶波羅蜜出生。",
        "from": "T18n0878_001:0336a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡虛空遍法界塵剎波羅蜜等一切諸佛。",
        "from": "T18n0878_001:0336a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南謨妙觀察智法波羅蜜出生。",
        "from": "T18n0878_001:0336a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡虛空遍法界塵剎波羅蜜等一切諸佛。",
        "from": "T18n0878_001:0336a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南謨成所作智羯磨波羅蜜出生。",
        "from": "T18n0878_001:0336a13_0",
        "context_with_next_sentence": true
    },
    {
