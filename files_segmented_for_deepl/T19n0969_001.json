[
    {
        "source_sentence": "佛頂最勝陀羅尼經序",
        "from": "T19n0969_001:0355a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙門彥悰序",
        "from": "T19n0969_001:0355a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫業理綿微。",
        "from": "T19n0969_001:0355a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二乘不足臻其極。",
        "from": "T19n0969_001:0355a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神宰恍惚。",
        "from": "T19n0969_001:0355a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十地未易暨其深。",
        "from": "T19n0969_001:0355a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則知賦命交加。",
        "from": "T19n0969_001:0355a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "罕言於孔宣父。",
        "from": "T19n0969_001:0355a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "報應叢雜。",
        "from": "T19n0969_001:0355a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冥昧於太史公。",
        "from": "T19n0969_001:0355a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是以先笑後號。",
        "from": "T19n0969_001:0355a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鸇雀祥而莫准。",
        "from": "T19n0969_001:0355a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始凶終吉。",
        "from": "T19n0969_001:0355a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘穀妖而弗驗。",
        "from": "T19n0969_001:0355a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或倚或伏之說。",
        "from": "T19n0969_001:0355a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "柱下庶欲照其幾。",
        "from": "T19n0969_001:0355a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為禍為福之談。",
        "from": "T19n0969_001:0355a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塞上僅可隣其次。",
        "from": "T19n0969_001:0355a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若乃採緣洞業索果明因。",
        "from": "T19n0969_001:0355a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儔絜大於百家。",
        "from": "T19n0969_001:0355a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孰有京於十力。",
        "from": "T19n0969_001:0355a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故能息善住之萬惡。",
        "from": "T19n0969_001:0355a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "杜閻摩之圓戶。",
        "from": "T19n0969_001:0355a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉凡階聖。",
        "from": "T19n0969_001:0355a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "引短成修。",
        "from": "T19n0969_001:0355a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比宋景之退法星。",
        "from": "T19n0969_001:0355a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶蔕介於三舍。",
        "from": "T19n0969_001:0355a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偶魯陽之揮落日。",
        "from": "T19n0969_001:0355a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故齟齬於再中。",
        "from": "T19n0969_001:0355a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何只庇託延祺見聞招賴。",
        "from": "T19n0969_001:0355a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恧神香於異域。",
        "from": "T19n0969_001:0355a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恥靈草於瓊田。",
        "from": "T19n0969_001:0355a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若斯而已哉。",
        "from": "T19n0969_001:0355a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃將輕埃附而九惱祛。",
        "from": "T19n0969_001:0355a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清吹獵而三障殄。",
        "from": "T19n0969_001:0355a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皎鏡齊光於日寶。",
        "from": "T19n0969_001:0355a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "暉煥比麗於天金。",
        "from": "T19n0969_001:0355a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖事若反掌而乘機顯妙。",
        "from": "T19n0969_001:0355a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奚可以常人之耳目。",
        "from": "T19n0969_001:0355a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擬議大聖之希夷者焉。",
        "from": "T19n0969_001:0355a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經以儀鳳四年正月五日。",
        "from": "T19n0969_001:0355a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朝散郎行鴻臚寺典客令杜行顗。",
        "from": "T19n0969_001:0355a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與寧遠將軍度婆等。",
        "from": "T19n0969_001:0355a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉詔譯進。",
        "from": "T19n0969_001:0355a28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時有廟諱國諱皆隱而避之。",
        "from": "T19n0969_001:0355a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即世尊為聖尊。",
        "from": "T19n0969_001:0355a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世界為生界。",
        "from": "T19n0969_001:0355a29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大勢為大趣。",
        "from": "T19n0969_001:0355b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救治為救除之類是也。",
        "from": "T19n0969_001:0355b01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上讀訖謂行顗曰。",
        "from": "T19n0969_001:0355b02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既是聖言不須避諱。",
        "from": "T19n0969_001:0355b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "杜時奉詔以正屬有故而寢焉。",
        "from": "T19n0969_001:0355b02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無幾。",
        "from": "T19n0969_001:0355b03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勅中天法師地婆訶羅。",
        "from": "T19n0969_001:0355b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於東西二京太原弘福寺等傳譯法寶。",
        "from": "T19n0969_001:0355b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而杜每充其選。",
        "from": "T19n0969_001:0355b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "余時又參末席。",
        "from": "T19n0969_001:0355b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "杜甞謂余曰。",
        "from": "T19n0969_001:0355b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弟子庸材不閑文體。",
        "from": "T19n0969_001:0355b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈師據勅刪正。",
        "from": "T19n0969_001:0355b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦願依文筆削。",
        "from": "T19n0969_001:0355b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "余辭以不敏。",
        "from": "T19n0969_001:0355b07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "載涉暄寒。",
        "from": "T19n0969_001:0355b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "荏苒之間此君長逝。",
        "from": "T19n0969_001:0355b08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "余歎惋流涕思其若人。",
        "from": "T19n0969_001:0355b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又懼寢彼鴻恩乖於貝牒。",
        "from": "T19n0969_001:0355b09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因請沙門道成等十人。",
        "from": "T19n0969_001:0355b09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈天竺法師再詳幽趣。",
        "from": "T19n0969_001:0355b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庶臨文不諱。",
        "from": "T19n0969_001:0355b10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上奉皇私。",
        "from": "T19n0969_001:0355b11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曲盡方言。",
        "from": "T19n0969_001:0355b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下符流便。",
        "from": "T19n0969_001:0355b11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故乃具表曲委陳諸始末。",
        "from": "T19n0969_001:0355b11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俾夫披覽之土無猜此教焉。",
        "from": "T19n0969_001:0355b12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "于時永淳元年五月二十三日也。",
        "from": "T19n0969_001:0355b13_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛頂最勝陀羅尼經",
        "from": "T19n0969_001:0355b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大唐天竺三藏地婆訶羅譯",
        "from": "T19n0969_001:0355b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T19n0969_001:0355b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在舍衛國祇樹給孤獨園。",
        "from": "T19n0969_001:0355b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大比丘眾八千人俱。",
        "from": "T19n0969_001:0355b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有菩薩摩訶薩等。",
        "from": "T19n0969_001:0355b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切皆得正智明炬。",
        "from": "T19n0969_001:0355b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照於諸法無所罣礙。",
        "from": "T19n0969_001:0355b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其名曰文殊師利菩薩蓮華勝藏菩薩。",
        "from": "T19n0969_001:0355b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離諸障菩薩觀世音菩薩。",
        "from": "T19n0969_001:0355b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得大勢菩薩執金剛菩薩。",
        "from": "T19n0969_001:0355b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空藏菩薩普賢菩薩。",
        "from": "T19n0969_001:0355b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彌勒菩薩持地菩薩摩訶薩等。",
        "from": "T19n0969_001:0355b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是上首三萬二千人俱。",
        "from": "T19n0969_001:0355b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有萬梵天王善吒天等。",
        "from": "T19n0969_001:0355b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各從餘世界來至佛所。",
        "from": "T19n0969_001:0355b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有萬二千天帝。",
        "from": "T19n0969_001:0355b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與無量諸天龍八部人非人等來至佛所。",
        "from": "T19n0969_001:0355b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊四眾圍繞。",
        "from": "T19n0969_001:0355c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養恭敬而為說法。",
        "from": "T19n0969_001:0355c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當是之時釋天眾中。",
        "from": "T19n0969_001:0355c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有一天人名曰善住。",
        "from": "T19n0969_001:0355c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與諸天女處大寶宮。",
        "from": "T19n0969_001:0355c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放逸嬉戲恣情受樂。",
        "from": "T19n0969_001:0355c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其夜有聲呼善住曰。",
        "from": "T19n0969_001:0355c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "却後七日汝定命終。",
        "from": "T19n0969_001:0355c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於閻浮提七返生死。",
        "from": "T19n0969_001:0355c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從是已後復入地獄。",
        "from": "T19n0969_001:0355c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從地獄出後得人身。",
        "from": "T19n0969_001:0355c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生盲貧窮具受諸苦。",
        "from": "T19n0969_001:0355c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善住聞已心驚惶怖。",
        "from": "T19n0969_001:0355c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身毛為竪戰慄不安。",
        "from": "T19n0969_001:0355c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奔馳往詣彼釋天所。",
        "from": "T19n0969_001:0355c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陳已所聞請求哀救。",
        "from": "T19n0969_001:0355c08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時釋天王聞善住言。",
        "from": "T19n0969_001:0355c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心生驚悼默而思曰。",
        "from": "T19n0969_001:0355c09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何七生彼生何類。",
        "from": "T19n0969_001:0355c09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是念已即見善住。",
        "from": "T19n0969_001:0355c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "死相現前受於猪身。",
        "from": "T19n0969_001:0355c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畢猪身已復受狗身。",
        "from": "T19n0969_001:0355c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是次第狐身猴身。",
        "from": "T19n0969_001:0355c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒蛇之身烏身鷲身。",
        "from": "T19n0969_001:0355c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是七生恒食臭穢。",
        "from": "T19n0969_001:0355c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時釋天王見此事已。",
        "from": "T19n0969_001:0355c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深哀善住必受斯苦。",
        "from": "T19n0969_001:0355c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自念於彼無如之何。",
        "from": "T19n0969_001:0355c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟佛世尊方能救濟。",
        "from": "T19n0969_001:0355c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時釋天王於夜後分。",
        "from": "T19n0969_001:0355c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "齎諸華鬘衣服瓔珞種種香等。",
        "from": "T19n0969_001:0355c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詣舍衛國供養世尊。",
        "from": "T19n0969_001:0355c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮佛雙足却住一面。",
        "from": "T19n0969_001:0355c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而白佛言世尊。",
        "from": "T19n0969_001:0355c16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我所止宮有一天子名曰善住。",
        "from": "T19n0969_001:0355c17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耽荒戲樂縱情遊處。",
        "from": "T19n0969_001:0355c17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜忽有聲呼而謂曰。",
        "from": "T19n0969_001:0355c18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝後七日必定命終。",
        "from": "T19n0969_001:0355c18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於閻浮提受七惡報。",
        "from": "T19n0969_001:0355c19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所生之處常食臭穢。",
        "from": "T19n0969_001:0355c19_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從是已後復入地獄。",
        "from": "T19n0969_001:0355c20_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從地獄出得生人中。",
        "from": "T19n0969_001:0355c20_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盲瞽貧窮具受眾苦。",
        "from": "T19n0969_001:0355c21_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向我求救無如之何。",
        "from": "T19n0969_001:0355c21_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我思世尊方能救彼。",
        "from": "T19n0969_001:0355c22_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊聞見釋天陳請畢已。",
        "from": "T19n0969_001:0355c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂放種種雜色光明。",
        "from": "T19n0969_001:0355c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流照十方一切世界。",
        "from": "T19n0969_001:0355c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照已還至佛世尊所。",
        "from": "T19n0969_001:0355c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右繞三匝從佛口入。",
        "from": "T19n0969_001:0355c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛便微笑告釋天言。",
        "from": "T19n0969_001:0355c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我有如是清淨諸趣灌頂最勝大陀羅尼。",
        "from": "T19n0969_001:0355c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能滿汝願救於彼苦。",
        "from": "T19n0969_001:0355c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦大饒益一切眾生。",
        "from": "T19n0969_001:0355c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天帝我此神呪力。",
        "from": "T19n0969_001:0355c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能滅除一切業障地獄畜生。",
        "from": "T19n0969_001:0355c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "閻摩羅等種種諸苦。",
        "from": "T19n0969_001:0355c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又能破壞地獄等道。",
        "from": "T19n0969_001:0355c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸眾生發正覺路。",
        "from": "T19n0969_001:0355c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天帝若有暫能聽聞之者。",
        "from": "T19n0969_001:0356a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉得消滅一切業障生死諸苦。",
        "from": "T19n0969_001:0356a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當獲善利得宿命智。",
        "from": "T19n0969_001:0356a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從一佛國至一佛國。",
        "from": "T19n0969_001:0356a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從一天中生一天中。",
        "from": "T19n0969_001:0356a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所生之處常識宿命。",
        "from": "T19n0969_001:0356a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有學習此經呪者。",
        "from": "T19n0969_001:0356a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現百年限更增其壽。",
        "from": "T19n0969_001:0356a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身口意淨泰然安樂。",
        "from": "T19n0969_001:0356a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛觀視諸天衛護。",
        "from": "T19n0969_001:0356a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切菩薩咸加慈念。",
        "from": "T19n0969_001:0356a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若常讀誦此經呪者。",
        "from": "T19n0969_001:0356a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼三惡道所有諸苦息滅不行。",
        "from": "T19n0969_001:0356a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切佛剎諸天福門導之令入。",
        "from": "T19n0969_001:0356a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時天帝釋聞佛讚說此陀羅尼。",
        "from": "T19n0969_001:0356a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前白佛言世尊。",
        "from": "T19n0969_001:0356a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願如來憐愍攝護一切眾生。",
        "from": "T19n0969_001:0356a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請說所陳清淨諸趣佛頂最勝大陀羅尼。",
        "from": "T19n0969_001:0356a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊受天帝請。",
        "from": "T19n0969_001:0356a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即為演說陀羅尼曰。",
        "from": "T19n0969_001:0356a12_0",
        "context_with_next_sentence": true
    },
    {
