[
    {
        "source_sentence": "大般若波羅蜜多經關法卷第五",
        "from": "X24n0448_005:0032c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大唐三藏法師　玄奘　譯",
        "from": "X24n0448_005:0032c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "○大般若波羅蜜多經卷第二百五",
        "from": "X24n0448_005:0032c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△初分難信解品第三十四之二十四",
        "from": "X24n0448_005:0032c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『編』復次善現。",
        "from": "X24n0448_005:0032c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "般若(換頭)波羅蜜多清淨故(色)(廣)清淨。",
        "from": "X24n0448_005:0032c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(色)(略)清淨故一切智智清淨。",
        "from": "X24n0448_005:0032c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故。",
        "from": "X24n0448_005:0032c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若般若(換頭)波羅蜜多清淨。",
        "from": "X24n0448_005:0032c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若(色)(略)清淨。",
        "from": "X24n0448_005:0032c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一切智智清淨。",
        "from": "X24n0448_005:0032c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無二無二分。",
        "from": "X24n0448_005:0032c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無別無斷故。",
        "from": "X24n0448_005:0032c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(次受想行識。",
        "from": "X24n0448_005:0032c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至佛菩提換。",
        "from": "X24n0448_005:0032c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "靜慮為使至菩提。",
        "from": "X24n0448_005:0032c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "都八十四頭。",
        "from": "X24n0448_005:0032c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用編字關。",
        "from": "X24n0448_005:0032c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每色起頭。",
        "from": "X24n0448_005:0032c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呼復次。",
        "from": "X24n0448_005:0032c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上界法安善現。",
        "from": "X24n0448_005:0032c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下界不呼善現。",
        "from": "X24n0448_005:0032c18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一廣二略。",
        "from": "X24n0448_005:0032c18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遇乃至有略。",
        "from": "X24n0448_005:0032c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遇使避本位不呼。",
        "from": "X24n0448_005:0032c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡至圓相處。",
        "from": "X24n0448_005:0032c19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即佛菩提終)。",
        "from": "X24n0448_005:0032c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "『開』復次善現。",
        "from": "X24n0448_005:0032c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切智智清淨故(色)(廣)清淨。",
        "from": "X24n0448_005:0032c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(色)(略)清淨故般若(換頭)波羅蜜多清淨。",
        "from": "X24n0448_005:0032c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故。",
        "from": "X24n0448_005:0032c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一切智智清淨。",
        "from": "X24n0448_005:0032c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若(色)(略)清淨。",
        "from": "X24n0448_005:0032c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若般若(換頭)波羅蜜多清淨。",
        "from": "X24n0448_005:0032c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無二無二分。",
        "from": "X24n0448_005:0032c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無別無斷故。",
        "from": "X24n0448_005:0032c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(從第二百四十四卷末。",
        "from": "X24n0448_005:0032c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用開字關。",
        "from": "X24n0448_005:0032c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "般若色起。",
        "from": "X24n0448_005:0032c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉頭用一切智智。",
        "from": "X24n0448_005:0032c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在前從此關。",
        "from": "X24n0448_005:0032c26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入後四十卷止。",
        "from": "X24n0448_005:0032c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二百八十四卷。",
        "from": "X24n0448_005:0032c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經文前後。",
        "from": "X24n0448_005:0032c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卷頭各添大般若波羅蜜多經初分難信解品第三十四之二十四。",
        "from": "X24n0448_005:0032c27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各隨數增之)。",
        "from": "X24n0448_005:0032c28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色　受想行識。",
        "from": "X24n0448_005:0033a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼處　耳鼻舌身意處。",
        "from": "X24n0448_005:0033a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色處　聲香味觸法處。",
        "from": "X24n0448_005:0033a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼界　色界(乃)眼識界及眼觸(至)眼觸為緣所生諸受。",
        "from": "X24n0448_005:0033a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耳界　聲界(乃)耳識界及耳觸(至)耳觸為緣所生諸受。",
        "from": "X24n0448_005:0033a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鼻界　香界(乃)鼻識界及鼻觸(至)鼻觸為緣所生諸受。",
        "from": "X24n0448_005:0033a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舌界　味界(乃)舌識界及舌觸(至)舌觸為緣所生諸受。",
        "from": "X24n0448_005:0033a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身界　觸界(乃)身識界及身觸(至)身觸為緣所生諸受。",
        "from": "X24n0448_005:0033a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "意界　法界(乃)意識界及意觸(至)意觸為緣所生諸受。",
        "from": "X24n0448_005:0033a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地界　水火風空識界。",
        "from": "X24n0448_005:0033a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無明　行(乃)識。",
        "from": "X24n0448_005:0033a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名色。",
        "from": "X24n0448_005:0033a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六處。",
        "from": "X24n0448_005:0033a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觸。",
        "from": "X24n0448_005:0033a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受。",
        "from": "X24n0448_005:0033a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛。",
        "from": "X24n0448_005:0033a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取。",
        "from": "X24n0448_005:0033a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有。",
        "from": "X24n0448_005:0033a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生(至)老死愁歎苦憂惱。",
        "from": "X24n0448_005:0033a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "布施波羅蜜多　淨戒(乃)安忍精進靜慮(至)般若波羅蜜多(作使加五波羅蜜多)。",
        "from": "X24n0448_005:0033a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內空　外空(乃)內外空。",
        "from": "X24n0448_005:0033a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空空。",
        "from": "X24n0448_005:0033a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大空。",
        "from": "X24n0448_005:0033a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勝義空。",
        "from": "X24n0448_005:0033a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有為空。",
        "from": "X24n0448_005:0033a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無為空。",
        "from": "X24n0448_005:0033a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畢竟空。",
        "from": "X24n0448_005:0033a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無際空。",
        "from": "X24n0448_005:0033a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散空。",
        "from": "X24n0448_005:0033a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無變異空。",
        "from": "X24n0448_005:0033a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本性空。",
        "from": "X24n0448_005:0033a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自相空。",
        "from": "X24n0448_005:0033a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共相空。",
        "from": "X24n0448_005:0033a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切法空。",
        "from": "X24n0448_005:0033a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可得空。",
        "from": "X24n0448_005:0033a23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無性空。",
        "from": "X24n0448_005:0033a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自性空(至)無性自性空。",
        "from": "X24n0448_005:0033a24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真如　法界(乃)法性。",
        "from": "X24n0448_005:0033b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不虗妄性。",
        "from": "X24n0448_005:0033b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不變異性。",
        "from": "X24n0448_005:0033b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平等性。",
        "from": "X24n0448_005:0033b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離生性。",
        "from": "X24n0448_005:0033b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法定法住。",
        "from": "X24n0448_005:0033b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實際虗。",
        "from": "X24n0448_005:0033b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空界(至)不思議界。",
        "from": "X24n0448_005:0033b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦聖諦　集。",
        "from": "X24n0448_005:0033b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅。",
        "from": "X24n0448_005:0033b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道聖諦(作使加二聖諦)。",
        "from": "X24n0448_005:0033b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四靜慮　四無量。",
        "from": "X24n0448_005:0033b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四無色定。",
        "from": "X24n0448_005:0033b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八解脫　八勝處。",
        "from": "X24n0448_005:0033b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九次第定。",
        "from": "X24n0448_005:0033b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十徧處。",
        "from": "X24n0448_005:0033b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四念住　四正斷(乃)四神足。",
        "from": "X24n0448_005:0033b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五根。",
        "from": "X24n0448_005:0033b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五力。",
        "from": "X24n0448_005:0033b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七等覺支(至)八聖道支。",
        "from": "X24n0448_005:0033b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空解脫門　無相。",
        "from": "X24n0448_005:0033b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無願解脫門(作使加解脫門)。",
        "from": "X24n0448_005:0033b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩十地。",
        "from": "X24n0448_005:0033b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五眼　六神通。",
        "from": "X24n0448_005:0033b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛十力　四無所畏(乃)四無礙解。",
        "from": "X24n0448_005:0033b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大慈。",
        "from": "X24n0448_005:0033b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大悲。",
        "from": "X24n0448_005:0033b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大喜。",
        "from": "X24n0448_005:0033b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大捨(至)十八佛不共法。",
        "from": "X24n0448_005:0033b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無忘失法　恒住捨性。",
        "from": "X24n0448_005:0033b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切智　道相智。",
        "from": "X24n0448_005:0033b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切相智。",
        "from": "X24n0448_005:0033b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切陀羅尼門　一切三摩地門。",
        "from": "X24n0448_005:0033b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "預流果　一來。",
        "from": "X24n0448_005:0033b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不還。",
        "from": "X24n0448_005:0033b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿羅漢果(使加二果)。",
        "from": "X24n0448_005:0033b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獨覺菩提。",
        "from": "X24n0448_005:0033b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切菩薩摩訶薩行。",
        "from": "X24n0448_005:0033b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛無上正等菩提。",
        "from": "X24n0448_005:0033b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "〔秋〕第二百五(之二十四)　般若波羅蜜多(色起○)靜慮(色起○)。",
        "from": "X24n0448_005:0033b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二百六(之二十五)　精進波羅蜜多(色起○)安忍(色起○)。",
        "from": "X24n0448_005:0033b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二百七(之二十六)　淨戒波羅蜜多(色起○)布施(色起○)。",
        "from": "X24n0448_005:0033b23_0",
        "context_with_next_sentence": true
