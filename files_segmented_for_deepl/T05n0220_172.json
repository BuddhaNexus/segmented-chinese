        "from": "T05n0220_172:0928a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「復次，舍利子！",
        "from": "T05n0220_172:0928a25_0",
        "context_with_next_sentence": true
    },
    {
