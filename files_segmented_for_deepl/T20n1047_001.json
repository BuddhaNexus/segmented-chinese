[
    {
        "source_sentence": "佛說聖六字大明王陀羅尼經",
        "from": "T20n1047_001:0044a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試鴻臚少卿",
        "from": "T20n1047_001:0044a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳法大師臣施護奉　詔譯",
        "from": "T20n1047_001:0044a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T20n1047_001:0044a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時世尊。",
        "from": "T20n1047_001:0044a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在舍衛國祇樹給孤獨園。",
        "from": "T20n1047_001:0044a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告尊者阿難言。",
        "from": "T20n1047_001:0044a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難。",
        "from": "T20n1047_001:0044a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當諦聽。",
        "from": "T20n1047_001:0044a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我有六字大明王陀羅尼。",
        "from": "T20n1047_001:0044a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃是過去無量諸佛。",
        "from": "T20n1047_001:0044a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸大士之所宣說。",
        "from": "T20n1047_001:0044a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難。",
        "from": "T20n1047_001:0044a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今復為汝等及末世有情。",
        "from": "T20n1047_001:0044a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宣說是陀羅尼。",
        "from": "T20n1047_001:0044a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當使有情。",
        "from": "T20n1047_001:0044a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於未來世。",
        "from": "T20n1047_001:0044a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得此陀羅尼者。",
        "from": "T20n1047_001:0044a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而得大利。",
        "from": "T20n1047_001:0044a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時阿難及娑婆世界主。",
        "from": "T20n1047_001:0044a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大梵天王帝釋四天大王等。",
        "from": "T20n1047_001:0044a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白世尊言。",
        "from": "T20n1047_001:0044a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊大慈大悲願為宣說。",
        "from": "T20n1047_001:0044a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當令我等及末世。",
        "from": "T20n1047_001:0044a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有情於長夜中。",
        "from": "T20n1047_001:0044a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得利益安樂。",
        "from": "T20n1047_001:0044a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊即說六字大明王陀羅尼曰。",
        "from": "T20n1047_001:0044a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也(二合)他(引一)難底隷(引二)摩度摩底(三)曼拏哩(引)計(引四)布哩(引)曩禰計(引)娑嚩(二合引)賀(引五)爾時佛告阿難言。",
        "from": "T20n1047_001:0044a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生。",
        "from": "T20n1047_001:0044a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得聞此大明王陀羅尼章句名字。",
        "from": "T20n1047_001:0044a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信心受持憶念讀誦。",
        "from": "T20n1047_001:0044a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及以香花。",
        "from": "T20n1047_001:0044a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種供養者。",
        "from": "T20n1047_001:0044a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人所有王怖水火等怖。",
        "from": "T20n1047_001:0044a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至盜賊怨家之怖。",
        "from": "T20n1047_001:0044a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是諸怖。",
        "from": "T20n1047_001:0044a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆解脫。",
        "from": "T20n1047_001:0044a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復毒藥。",
        "from": "T20n1047_001:0044a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至刀兵。",
        "from": "T20n1047_001:0044a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不能傷害。",
        "from": "T20n1047_001:0044a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復天人非人。",
        "from": "T20n1047_001:0044a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜叉羅剎俱畔拏。",
        "from": "T20n1047_001:0044b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至羯吒布單曩等。",
        "from": "T20n1047_001:0044b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所持所魅。",
        "from": "T20n1047_001:0044b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不能侵害。",
        "from": "T20n1047_001:0044b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復瘧病。",
        "from": "T20n1047_001:0044b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日二日三日四日。",
        "from": "T20n1047_001:0044b02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或復多日。",
        "from": "T20n1047_001:0044b03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或復須臾。",
        "from": "T20n1047_001:0044b03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復頭痛耳痛脊痛腹痛。",
        "from": "T20n1047_001:0044b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至水腫諸惡病苦。",
        "from": "T20n1047_001:0044b04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆不能侵害。",
        "from": "T20n1047_001:0044b04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其晝夜。",
        "from": "T20n1047_001:0044b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常獲安隱。",
        "from": "T20n1047_001:0044b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切所求。",
        "from": "T20n1047_001:0044b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆得如願。",
        "from": "T20n1047_001:0044b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊。",
        "from": "T20n1047_001:0044b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又說陀羅尼曰。",
        "from": "T20n1047_001:0044b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也(二合)他(引一)阿拏哩(引二)波拏哩(引三)那彌(引)那摩儞(四)嚩迦細(引五)俱酥計(引六)句(引)迦切計(引七)部多誐囉(二合)切(引八)帝濟(引)嚩底(九)野舍嚩底娑嚩(二合引)賀(引十)佛告阿難。",
        "from": "T20n1047_001:0044b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生。",
        "from": "T20n1047_001:0044b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得聞是陀羅尼者。",
        "from": "T20n1047_001:0044b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我不見有一切天龍夜叉羅剎。",
        "from": "T20n1047_001:0044b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畢哩多毘舍遮部多俱畔拏。",
        "from": "T20n1047_001:0044b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "布單曩羯吒布單曩。",
        "from": "T20n1047_001:0044b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至人非人等。",
        "from": "T20n1047_001:0044b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切行不饒益者。",
        "from": "T20n1047_001:0044b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敢有違逆。",
        "from": "T20n1047_001:0044b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難如是等行不饒益者。",
        "from": "T20n1047_001:0044b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有違逆我如是陀羅尼者。",
        "from": "T20n1047_001:0044b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當頭破七分。",
        "from": "T20n1047_001:0044b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如阿梨樹枝。",
        "from": "T20n1047_001:0044b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊。",
        "from": "T20n1047_001:0044b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復告阿難言。",
        "from": "T20n1047_001:0044b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是陀羅尼功德力勢。",
        "from": "T20n1047_001:0044b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量無邊利益眾生。",
        "from": "T20n1047_001:0044b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有窮盡。",
        "from": "T20n1047_001:0044b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當受持。",
        "from": "T20n1047_001:0044b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣令流布。",
        "from": "T20n1047_001:0044b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於後後世。",
        "from": "T20n1047_001:0044b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿令斷絕。",
        "from": "T20n1047_001:0044b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說是經已。",
        "from": "T20n1047_001:0044b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難及大梵天王天帝釋四天大王等。",
        "from": "T20n1047_001:0044b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛所說。",
        "from": "T20n1047_001:0044b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡喜信受。",
        "from": "T20n1047_001:0044b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮佛而退。",
        "from": "T20n1047_001:0044b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說聖六字大明王陀羅尼經",
        "from": "T20n1047_001:0044b22_0",
        "context_with_next_sentence": false
    }
]