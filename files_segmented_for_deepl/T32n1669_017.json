[
    {
        "source_sentence": "大宗地玄文本論卷第十七",
        "from": "T32n1669_017:0689a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬鳴菩薩造",
        "from": "T32n1669_017:0689a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真諦三藏譯",
        "from": "T32n1669_017:0689a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "◎解脫山王大道路大決擇分第三十三如是已說解脫山王根本地地無礙自在大決擇分。",
        "from": "T32n1669_017:0689a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說解脫山王大道路大決擇分。",
        "from": "T32n1669_017:0689a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_017:0689a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_017:0689a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　山王道路中　　前所說量中",
        "from": "T32n1669_017:0689a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　增空空有有　　位位轉勝生",
        "from": "T32n1669_017:0689a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_017:0689b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就解脫山王大道路分中則有三轉。",
        "from": "T32n1669_017:0689b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為三。",
        "from": "T32n1669_017:0689b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者空空轉。",
        "from": "T32n1669_017:0689b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十空無為一一各各生十空故。",
        "from": "T32n1669_017:0689b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者有有轉。",
        "from": "T32n1669_017:0689b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十有無為一一各各生十有故。",
        "from": "T32n1669_017:0689b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者位位轉。",
        "from": "T32n1669_017:0689b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五十一位一一各各生五十故。",
        "from": "T32n1669_017:0689b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為三。",
        "from": "T32n1669_017:0689b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取自相生非他相故。",
        "from": "T32n1669_017:0689b05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈山王道路中前所說量中增空空有有位位轉勝生故。",
        "from": "T32n1669_017:0689b05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘種種門轉轉增數。",
        "from": "T32n1669_017:0689b07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應廣通達。",
        "from": "T32n1669_017:0689b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣大無盡解脫海海摩訶山王大決擇分第三十四如是已說解脫山王大道路大決擇分。",
        "from": "T32n1669_017:0689b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說廣大無盡解脫海海摩訶山王大決擇分。",
        "from": "T32n1669_017:0689b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_017:0689b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_017:0689b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　前所說諸轉　　無有窮盡故",
        "from": "T32n1669_017:0689b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_017:0689b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此偈中為明何義。",
        "from": "T32n1669_017:0689b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲現示空生自空無有窮盡。",
        "from": "T32n1669_017:0689b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空生異空無有窮盡。",
        "from": "T32n1669_017:0689b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空生諸有無有窮盡。",
        "from": "T32n1669_017:0689b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有亦如是無有窮盡。",
        "from": "T32n1669_017:0689b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "位亦如是無有窮盡。",
        "from": "T32n1669_017:0689b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重重無窮亂轉無窮。",
        "from": "T32n1669_017:0689b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無其邊際無其始終。",
        "from": "T32n1669_017:0689b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甚深甚深周遍周遍廣大轉行。",
        "from": "T32n1669_017:0689b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則是解脫海海山王無礙自在體相用故。",
        "from": "T32n1669_017:0689b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈前所說諸轉無有窮盡故故。",
        "from": "T32n1669_017:0689b19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "◎大宗地玄文本論卷第十七",
        "from": "T32n1669_017:0689b20_12",
        "context_with_next_sentence": false
    }
]