[
    {
        "source_sentence": "冥報記輯書卷第六",
        "from": "X88n1648_006:0319a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐汾州孝義縣人路伯達至永徽年中。",
        "from": "X88n1648_006:0319a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "負同縣人錢一千文。",
        "from": "X88n1648_006:0319a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後乃違契拒諱。",
        "from": "X88n1648_006:0319a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及執契往徵。",
        "from": "X88n1648_006:0319a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂共錢主於佛前為信誓曰。",
        "from": "X88n1648_006:0319a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若我未還公。",
        "from": "X88n1648_006:0319a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願吾死後與公家作牛畜。",
        "from": "X88n1648_006:0319a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言訖未逾一年而死。",
        "from": "X88n1648_006:0319a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至二歲時向錢主家牸牛產一赤犢子。",
        "from": "X88n1648_006:0319a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "額上生白毛為路伯達三字。",
        "from": "X88n1648_006:0319a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其子姪等恥之。",
        "from": "X88n1648_006:0319a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將錢五千文求贖。",
        "from": "X88n1648_006:0319a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "主不肯與。",
        "from": "X88n1648_006:0319a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃施與隰城縣啟福寺僧真如。",
        "from": "X88n1648_006:0319a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "助造十五級浮圖。",
        "from": "X88n1648_006:0319a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人有見者發心止惡。",
        "from": "X88n1648_006:0319a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "競投錢物布施。",
        "from": "X88n1648_006:0319a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐右金吾兵曹京兆韋知十至永徽中煑一羊脚。",
        "from": "X88n1648_006:0319a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "半日猶生。",
        "from": "X88n1648_006:0319a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知十怒家人曰。",
        "from": "X88n1648_006:0319a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用柴十倍於常。",
        "from": "X88n1648_006:0319a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不知何意。",
        "from": "X88n1648_006:0319a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如此更命重煑。",
        "from": "X88n1648_006:0319a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還復如故。",
        "from": "X88n1648_006:0319a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃命剖之。",
        "from": "X88n1648_006:0319a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其中遂得一銅像。",
        "from": "X88n1648_006:0319a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長徑寸焉。",
        "from": "X88n1648_006:0319a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光明照灼相好成就。",
        "from": "X88n1648_006:0319a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其家一生不敢食酒肉。",
        "from": "X88n1648_006:0319a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中山郎餘令親聞說之。",
        "from": "X88n1648_006:0319a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐坊州人上柱國王懷智至顯慶初亡歿。",
        "from": "X88n1648_006:0319b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其母孫氏及弟懷善懷表並存。",
        "from": "X88n1648_006:0319b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至四年六月。",
        "from": "X88n1648_006:0319b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壅州高陵有一人。",
        "from": "X88n1648_006:0319b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "失其姓名。",
        "from": "X88n1648_006:0319b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "死經七日背上已爛而甦。",
        "from": "X88n1648_006:0319b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此人於地下見懷智云。",
        "from": "X88n1648_006:0319b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見任泰山錄事。",
        "from": "X88n1648_006:0319b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遣此人執筆。",
        "from": "X88n1648_006:0319b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口授為書。",
        "from": "X88n1648_006:0319b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂之曰。",
        "from": "X88n1648_006:0319b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝雖合死今方便放汝歸家。",
        "from": "X88n1648_006:0319b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宜為我持此書至坊州訪我家。",
        "from": "X88n1648_006:0319b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通人兼白我孃。",
        "from": "X88n1648_006:0319b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懷智今為太山錄事參軍。",
        "from": "X88n1648_006:0319b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幸蒙安泰。",
        "from": "X88n1648_006:0319b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但家中曾貸寺家木作門。",
        "from": "X88n1648_006:0319b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此既功德物。",
        "from": "X88n1648_006:0319b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請早酬償之。",
        "from": "X88n1648_006:0319b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懷善即死。",
        "from": "X88n1648_006:0319b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不合久住。",
        "from": "X88n1648_006:0319b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速作經像救助。",
        "from": "X88n1648_006:0319b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不然恐無濟理。",
        "from": "X88n1648_006:0319b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此人既蘇之後。",
        "from": "X88n1648_006:0319b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即齎書故送其舍。",
        "from": "X88n1648_006:0319b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所論家事無不闇合。",
        "from": "X88n1648_006:0319b09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至經三日懷善遂即暴死。",
        "from": "X88n1648_006:0319b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合州道俗聞者莫不增修功德。",
        "from": "X88n1648_006:0319b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[邱丘+(夕/鹿)]州人勳衛侯智純說之。",
        "from": "X88n1648_006:0319b11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夏侯均者冀州阜城人也。",
        "from": "X88n1648_006:0319b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯慶二年病經四十餘日昏亂殆死。",
        "from": "X88n1648_006:0319b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自云。",
        "from": "X88n1648_006:0319b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被配作牛頻經苦訴。",
        "from": "X88n1648_006:0319b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "訴云。",
        "from": "X88n1648_006:0319b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嘗三度於隱師處受戒懺悔。",
        "from": "X88n1648_006:0319b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自省無過。",
        "from": "X88n1648_006:0319b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何忍遣作牛身受苦如是。",
        "from": "X88n1648_006:0319b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "均已被配磨坊。",
        "from": "X88n1648_006:0319b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經二十日。",
        "from": "X88n1648_006:0319b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦使後為勘當受戒。",
        "from": "X88n1648_006:0319b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是實不虗。",
        "from": "X88n1648_006:0319b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始得免罪。",
        "from": "X88n1648_006:0319b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此人生平甚有膂力。",
        "from": "X88n1648_006:0319b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酗酒好鬬。",
        "from": "X88n1648_006:0319b16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今現斷酒肉。",
        "from": "X88n1648_006:0319b16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清信賢者。",
        "from": "X88n1648_006:0319b17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為隱師弟子齋戒不絕。",
        "from": "X88n1648_006:0319b17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐京都西市北店有王會師者其母先終。",
        "from": "X88n1648_006:0319b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "服制已畢。",
        "from": "X88n1648_006:0319b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至顯慶二年內。",
        "from": "X88n1648_006:0319b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其家乃產一青黃母狗。",
        "from": "X88n1648_006:0319b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "會師妻為其盜食。",
        "from": "X88n1648_006:0319b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃以杖擊之數下。",
        "from": "X88n1648_006:0319b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "狗遂作人語曰。",
        "from": "X88n1648_006:0319b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我是汝姑。",
        "from": "X88n1648_006:0319b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "新婦杖我大錯。",
        "from": "X88n1648_006:0319b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為嚴酷家人過甚。",
        "from": "X88n1648_006:0319b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂得此報。",
        "from": "X88n1648_006:0319b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今既被打羞向汝家。",
        "from": "X88n1648_006:0319b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因即走出。",
        "from": "X88n1648_006:0319b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "會師聞而洟泣拘以歸家。",
        "from": "X88n1648_006:0319b22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而復還去。",
        "from": "X88n1648_006:0319b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡經四五。",
        "from": "X88n1648_006:0319b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "會師見其意正乃屈請市北大街中。",
        "from": "X88n1648_006:0319b23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正是己店北大牆。",
        "from": "X88n1648_006:0319b24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後作小舍安置。",
        "from": "X88n1648_006:0319b24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每日送食。",
        "from": "X88n1648_006:0319b24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "市人及行客就親者極眾。",
        "from": "X88n1648_006:0319b24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "投餅與者不可勝數。",
        "from": "X88n1648_006:0319c01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此犬常不離此舍。",
        "from": "X88n1648_006:0319c01_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過齋時即不肯食。",
        "from": "X88n1648_006:0319c02_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經一二歲莫知所之。",
        "from": "X88n1648_006:0319c02_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐汾州孝義縣縣泉村人劉摩兒至顯慶四年八月二十七日。",
        "from": "X88n1648_006:0319c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遇患而終。",
        "from": "X88n1648_006:0319c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其男師保明日又死。",
        "from": "X88n1648_006:0319c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父子平生行皆險詖。",
        "from": "X88n1648_006:0319c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其北隣有祁隴威。",
        "from": "X88n1648_006:0319c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因採樵被車輾死。",
        "from": "X88n1648_006:0319c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經數日而蘇。",
        "from": "X88n1648_006:0319c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃見摩兒男師保在鑊湯中。",
        "from": "X88n1648_006:0319c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須臾之間。",
        "from": "X88n1648_006:0319c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皮肉俱盡。",
        "from": "X88n1648_006:0319c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無復人形。",
        "from": "X88n1648_006:0319c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯見白骨。",
        "from": "X88n1648_006:0319c07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如此良久。",
        "from": "X88n1648_006:0319c07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還復本形。",
        "from": "X88n1648_006:0319c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隴威問其故。",
        "from": "X88n1648_006:0319c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對曰。",
        "from": "X88n1648_006:0319c08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為我射獵故受此罪。",
        "from": "X88n1648_006:0319c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又謂保曰。",
        "from": "X88n1648_006:0319c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卿父何在。",
        "from": "X88n1648_006:0319c08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對曰。",
        "from": "X88n1648_006:0319c09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我父罪重不可卒見。",
        "from": "X88n1648_006:0319c09_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卿既即還。",
        "from": "X88n1648_006:0319c09_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請白家中。",
        "from": "X88n1648_006:0319c09_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為修齋福。",
        "from": "X88n1648_006:0319c09_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言訖被使催促。",
        "from": "X88n1648_006:0319c10_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前至府舍。",
        "from": "X88n1648_006:0319c10_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見館宇崇峻。",
        "from": "X88n1648_006:0319c10_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執仗者二十餘人。",
        "from": "X88n1648_006:0319c10_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一官人問之曰。",
        "from": "X88n1648_006:0319c11_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝比有何福業。",
        "from": "X88n1648_006:0319c11_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對曰。",
        "from": "X88n1648_006:0319c11_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隴威去年正月在獨村看讀一切經。",
        "from": "X88n1648_006:0319c11_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脫衫一領布施。",
        "from": "X88n1648_006:0319c12_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼受五戒。",
        "from": "X88n1648_006:0319c12_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至今不犯。",
        "from": "X88n1648_006:0319c13_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "官人乃云。",
        "from": "X88n1648_006:0319c13_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若如所云。",
        "from": "X88n1648_006:0319c13_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量功德。",
        "from": "X88n1648_006:0319c13_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何須來此。",
        "from": "X88n1648_006:0319c13_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃索簿勘見簿曰。",
        "from": "X88n1648_006:0319c14_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人合死不虗。",
        "from": "X88n1648_006:0319c14_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "側注云。",
        "from": "X88n1648_006:0319c14_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受戒布施福助更合延壽。",
        "from": "X88n1648_006:0319c14_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃遣人送還。",
        "from": "X88n1648_006:0319c15_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當即蘇活。",
        "from": "X88n1648_006:0319c15_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐冀州舘陶縣主簿姓周忘其名字。",
        "from": "X88n1648_006:0319c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至顯慶四年十一月奉使於臨渝關互市。",
        "from": "X88n1648_006:0319c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當去之時。",
        "from": "X88n1648_006:0319c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將佐史等二人從往。",
        "from": "X88n1648_006:0319c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周將錢帛稍多。",
        "from": "X88n1648_006:0319c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二人乃以土囊壓而殺之。",
        "from": "X88n1648_006:0319c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有錢帛咸盜將去。",
        "from": "X88n1648_006:0319c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯有隨身衣服充斂。",
        "from": "X88n1648_006:0320a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至歲暮乃入妻夢。",
        "from": "X88n1648_006:0320a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具說被殺之狀。",
        "from": "X88n1648_006:0320a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼言所盜財物藏隱之處。",
        "from": "X88n1648_006:0320a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妻乃依此告官。",
        "from": "X88n1648_006:0320a02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "官司案辯具得實狀錢。",
        "from": "X88n1648_006:0320a03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帛竝獲二人。",
        "from": "X88n1648_006:0320a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆坐處死。",
        "from": "X88n1648_006:0320a03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相州智力寺僧慧永云。",
        "from": "X88n1648_006:0320a04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當親見明庭觀道士劉仁實說之。",
        "from": "X88n1648_006:0320a04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐隴西李虔觀今居鄭州。",
        "from": "X88n1648_006:0320a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至顯慶五年丁父憂。",
        "from": "X88n1648_006:0320a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃刺血寫金剛般若經及般若心經各一卷隨願往生經一卷。",
        "from": "X88n1648_006:0320a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出外將入。",
        "from": "X88n1648_006:0320a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即一浴身。",
        "from": "X88n1648_006:0320a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後忽聞院中有異香非常。",
        "from": "X88n1648_006:0320a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "郁烈隣側。",
        "from": "X88n1648_006:0320a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並就觀之無不稱歎。",
        "from": "X88n1648_006:0320a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中山郎餘令。",
        "from": "X88n1648_006:0320a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曾過鄭州見彼親友。",
        "from": "X88n1648_006:0320a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具陳說之。",
        "from": "X88n1648_006:0320a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐相州滏陽縣人信都元方少有操尚。",
        "from": "X88n1648_006:0320a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尤好釋典。",
        "from": "X88n1648_006:0320a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年二十九。",
        "from": "X88n1648_006:0320a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至顯慶五年春正月死。",
        "from": "X88n1648_006:0320a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "死後月餘。",
        "from": "X88n1648_006:0320a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其兄法觀寺僧道傑。",
        "from": "X88n1648_006:0320a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "情切友情。",
        "from": "X88n1648_006:0320a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃將一巫者至家。",
        "from": "X88n1648_006:0320a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遣求元方與語。",
        "from": "X88n1648_006:0320a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法觀又頗解法術。",
        "from": "X88n1648_006:0320a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃作一符攝得元方。",
        "from": "X88n1648_006:0320a14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令巫者問其由委。",
        "from": "X88n1648_006:0320a14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "巫者不識字。",
        "from": "X88n1648_006:0320a15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遣解書人執筆。",
        "from": "X88n1648_006:0320a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "巫者為元方口授。",
        "from": "X88n1648_006:0320a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作書一紙。",
        "from": "X88n1648_006:0320a16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與同學馮行基。",
        "from": "X88n1648_006:0320a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具述平生之意。",
        "from": "X88n1648_006:0320a16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并詩二首。",
        "from": "X88n1648_006:0320a16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及其家中亦留書。",
        "from": "X88n1648_006:0320a17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "啟文理順序言詞悽愴。",
        "from": "X88n1648_006:0320a17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其書疏大抵勸修功德。",
        "from": "X88n1648_006:0320a17_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及遣念佛寫經。",
        "from": "X88n1648_006:0320a18_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為殺生之業。",
        "from": "X88n1648_006:0320a18_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "罪之大者。",
        "from": "X88n1648_006:0320a18_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無過於此。",
        "from": "X88n1648_006:0320a19_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又云。",
        "from": "X88n1648_006:0320a19_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元方不入地獄。",
        "from": "X88n1648_006:0320a19_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不墮鬼中。",
        "from": "X88n1648_006:0320a19_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前蒙冥官處分。",
        "from": "X88n1648_006:0320a19_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令於石州李仁師家為男。",
        "from": "X88n1648_006:0320a20_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但為隴州吳山縣石名遠於華嶽祈子。",
        "from": "X88n1648_006:0320a20_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃改與石家為男。",
        "from": "X88n1648_006:0320a21_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又云。",
        "from": "X88n1648_006:0320a21_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受生日逼忽迫不得更住。",
        "from": "X88n1648_006:0320a21_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從二月受胎。",
        "from": "X88n1648_006:0320a22_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至十二月誕育。",
        "from": "X88n1648_006:0320a22_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願兄等慈流就彼相看也。",
        "from": "X88n1648_006:0320a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言訖洟泣而去。",
        "from": "X88n1648_006:0320a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "河東薛大造寓居滏陽。",
        "from": "X88n1648_006:0320a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前任吳山縣令自云。",
        "from": "X88n1648_006:0320a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具識名遠。",
        "from": "X88n1648_006:0320a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智力寺僧慧永法真等說之。",
        "from": "X88n1648_006:0320a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐封元則渤海長河人也。",
        "from": "X88n1648_006:0320b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至顯慶中為光祿寺太官掌膳。",
        "from": "X88n1648_006:0320b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時有西蕃客于闐王來朝。",
        "from": "X88n1648_006:0320b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食料餘羊。",
        "from": "X88n1648_006:0320b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡至數十百口。",
        "from": "X88n1648_006:0320b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王並託元則送於僧寺。",
        "from": "X88n1648_006:0320b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放作長生。",
        "from": "X88n1648_006:0320b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元則乃竊令屠家烹宰。",
        "from": "X88n1648_006:0320b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "收其錢直。",
        "from": "X88n1648_006:0320b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍朔元年夏六月。",
        "from": "X88n1648_006:0320b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雒陽大雨震雷霹靂元則。",
        "from": "X88n1648_006:0320b05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於宣仁門外大街中殺之。",
        "from": "X88n1648_006:0320b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "折其項裂血流灑地。",
        "from": "X88n1648_006:0320b06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀者盈衢莫不驚愕。",
        "from": "X88n1648_006:0320b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冥報記輯書卷第六",
        "from": "X88n1648_006:0320b09_0",
        "context_with_next_sentence": false
    }
]