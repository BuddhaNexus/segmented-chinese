        "from": "T21n1285_001:0354a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是陀羅尼已。",
        "from": "T21n1285_001:0354b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復白佛言。",
        "from": "T21n1285_001:0354b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊此陀羅尼。",
        "from": "T21n1285_001:0354b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人依法持誦七遍。",
        "from": "T21n1285_001:0354b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人所作勝利皆得成就。",
        "from": "T21n1285_001:0354b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復有人於白月十五日潔淨持戒。",
        "from": "T21n1285_001:0354b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日三時焚沈檀香。",
        "from": "T21n1285_001:0354b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此陀羅尼滿八千遍。",
        "from": "T21n1285_001:0354b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人隨願獲得金銀珍寶悉皆如意。",
        "from": "T21n1285_001:0354b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時大夜叉主。",
        "from": "T21n1285_001:0354b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是陀羅尼已禮佛而退。",
        "from": "T21n1285_001:0354b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說是經已。",
        "from": "T21n1285_001:0354b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸菩薩眾聞佛所說。",
        "from": "T21n1285_001:0354b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆大歡喜信受奉行。",
        "from": "T21n1285_001:0354b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說寶賢陀羅尼經",
        "from": "T21n1285_001:0354b15_0",
        "context_with_next_sentence": false
    }
]