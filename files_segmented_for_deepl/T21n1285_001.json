[
    {
        "source_sentence": "佛說寶賢陀羅尼經",
        "from": "T21n1285_001:0353c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試光祿卿",
        "from": "T21n1285_001:0353c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明教大師臣法賢奉　詔譯",
        "from": "T21n1285_001:0353c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T21n1285_001:0353c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在舍衛國祇樹給孤獨園。",
        "from": "T21n1285_001:0353c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大菩薩眾俱。",
        "from": "T21n1285_001:0354a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時有大夜叉主名曰寶賢。",
        "from": "T21n1285_001:0354a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來詣佛所。",
        "from": "T21n1285_001:0354a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到已頭面禮足。",
        "from": "T21n1285_001:0354a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌恭敬而白佛言。",
        "from": "T21n1285_001:0354a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我有大祕密心陀羅尼樂欲宣說。",
        "from": "T21n1285_001:0354a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊加哀覆護。",
        "from": "T21n1285_001:0354a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊此陀羅尼。",
        "from": "T21n1285_001:0354a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有比丘比丘尼優婆塞優婆夷等。",
        "from": "T21n1285_001:0354a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日三時至心誦念者。",
        "from": "T21n1285_001:0354a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為彼人常作依怙。",
        "from": "T21n1285_001:0354a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切事皆得吉祥。",
        "from": "T21n1285_001:0354a06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至飲食衣服臥具金銀珍寶財穀等我常供給。",
        "from": "T21n1285_001:0354a07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及作一切勝利悉使成就。",
        "from": "T21n1285_001:0354a08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦令一切人眾普得愛敬。",
        "from": "T21n1285_001:0354a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯除婬怒癡等惡不善法。",
        "from": "T21n1285_001:0354a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘諸所作悉皆隨意。",
        "from": "T21n1285_001:0354a09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊知此寶賢大夜叉主誠心信重。",
        "from": "T21n1285_001:0354a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲安樂一切眾生貧苦惱故。",
        "from": "T21n1285_001:0354a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而告之言。",
        "from": "T21n1285_001:0354a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉寶賢大夜叉主。",
        "from": "T21n1285_001:0354a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能為利益諸眾生故。",
        "from": "T21n1285_001:0354a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦樂聞隨意宣說。",
        "from": "T21n1285_001:0354a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時寶賢大夜叉主。",
        "from": "T21n1285_001:0354a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說陀羅尼。",
        "from": "T21n1285_001:0354a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰。",
        "from": "T21n1285_001:0354a15_0",
        "context_with_next_sentence": true
    },
