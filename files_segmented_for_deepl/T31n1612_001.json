[
    {
        "source_sentence": "大乘五蘊論一卷",
        "from": "T31n1612_001:0848b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世親菩薩造",
        "from": "T31n1612_001:0848b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏法師玄奘奉　詔譯",
        "from": "T31n1612_001:0848b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如薄伽梵略說五蘊：一者、色蘊，二者、受蘊，三者、想蘊，四者、行蘊，五者、識蘊。",
        "from": "T31n1612_001:0848b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何色蘊？",
        "from": "T31n1612_001:0848b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂四大種及四大種所造諸色。",
        "from": "T31n1612_001:0848b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何四大種？",
        "from": "T31n1612_001:0848b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂地界、水界、火界、風界。",
        "from": "T31n1612_001:0848b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何地界？",
        "from": "T31n1612_001:0848b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂堅強性。",
        "from": "T31n1612_001:0848b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何水界？",
        "from": "T31n1612_001:0848b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂流濕性。",
        "from": "T31n1612_001:0848b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何火界？",
        "from": "T31n1612_001:0848b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂溫燥性。",
        "from": "T31n1612_001:0848b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何風界？",
        "from": "T31n1612_001:0848b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂輕等動性。",
        "from": "T31n1612_001:0848b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何四大種所造諸色？",
        "from": "T31n1612_001:0848b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂眼根、耳根、鼻根、舌根、身根，色、聲、香、味、所觸一分、無表色等。",
        "from": "T31n1612_001:0848b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何眼根？",
        "from": "T31n1612_001:0848b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂色為境清淨色。",
        "from": "T31n1612_001:0848b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何耳根？",
        "from": "T31n1612_001:0848b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂聲為境清淨色。",
        "from": "T31n1612_001:0848b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何鼻根？",
        "from": "T31n1612_001:0848b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂香為境清淨色。",
        "from": "T31n1612_001:0848b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何舌根？",
        "from": "T31n1612_001:0848b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂味為境清淨色。",
        "from": "T31n1612_001:0848b15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何身根？",
        "from": "T31n1612_001:0848b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂所觸為境清淨色。",
        "from": "T31n1612_001:0848b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為色？",
        "from": "T31n1612_001:0848b16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂眼境界，顯色、形色及表色等。",
        "from": "T31n1612_001:0848b17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為聲？",
        "from": "T31n1612_001:0848b17_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂耳境界，執受大種因聲，非執受大種因聲，俱大種因聲。",
        "from": "T31n1612_001:0848b18_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為香？",
        "from": "T31n1612_001:0848b19_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂鼻境界，好香、惡香、及所餘香。",
        "from": "T31n1612_001:0848b19_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為味？",
        "from": "T31n1612_001:0848b20_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂舌境界，甘味、酢味、醎味、辛味、苦味、淡味。",
        "from": "T31n1612_001:0848b20_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為所觸一分？",
        "from": "T31n1612_001:0848b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂身境界，除四大種餘所造觸，滑性、澁性、重性、輕性、冷、飢、渴等。",
        "from": "T31n1612_001:0848b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名為無表色等？",
        "from": "T31n1612_001:0848b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂有表業及三摩地所生色等，無見無對。",
        "from": "T31n1612_001:0848b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何受蘊？",
        "from": "T31n1612_001:0848b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂三領納：一、苦，二、樂，三、不苦不樂。",
        "from": "T31n1612_001:0848b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樂謂滅時有和合欲；苦謂生時有乖離欲；不苦不樂謂無二欲。",
        "from": "T31n1612_001:0848b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何想蘊？",
        "from": "T31n1612_001:0848b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於境界，取種種相。",
        "from": "T31n1612_001:0848b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何行蘊？",
        "from": "T31n1612_001:0848c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂除受想，諸餘心法及心不相應行。",
        "from": "T31n1612_001:0848c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名為諸餘心法？",
        "from": "T31n1612_001:0848c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂彼諸法與心相應。",
        "from": "T31n1612_001:0848c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼復云何？",
        "from": "T31n1612_001:0848c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂觸、作意、受、想、思；欲、勝解、念、三摩地、慧；信、慚、愧、無貪善根、無瞋善根、無癡善根、精進、輕安、不放逸、捨、不害；貪、瞋、慢、無明、見、疑；忿、恨、覆、惱、嫉、慳、誑、諂、憍、害，無慚、無愧，惛沈、掉舉、不信、懈怠、放逸、忘念、散亂、不正知；惡作、睡眠、尋、伺。",
        "from": "T31n1612_001:0848c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸心法：五是遍行，五是別境，十一是善，六是煩惱，餘是隨煩惱，四是不決定。",
        "from": "T31n1612_001:0848c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為觸？",
        "from": "T31n1612_001:0848c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂三和合分別為性。",
        "from": "T31n1612_001:0848c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何作意？",
        "from": "T31n1612_001:0848c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂能令心發悟為性。",
        "from": "T31n1612_001:0848c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為思？",
        "from": "T31n1612_001:0848c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於功德過失及俱相違，令心造作，意業為性。",
        "from": "T31n1612_001:0848c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為欲？",
        "from": "T31n1612_001:0848c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於可愛事，希望為性。",
        "from": "T31n1612_001:0848c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何勝解？",
        "from": "T31n1612_001:0848c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於決定事，即如所了印可為性。",
        "from": "T31n1612_001:0848c15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為念？",
        "from": "T31n1612_001:0848c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於串習事，令心不忘明記為性。",
        "from": "T31n1612_001:0848c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何三摩地？",
        "from": "T31n1612_001:0848c17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於所觀事，令心一境不散為性。",
        "from": "T31n1612_001:0848c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為慧？",
        "from": "T31n1612_001:0848c18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂即於彼擇法為性；或如理所引，或不如理所引，或俱非所引。",
        "from": "T31n1612_001:0848c18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為信？",
        "from": "T31n1612_001:0848c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於業果諸諦寶中，極正符順，心淨為性。",
        "from": "T31n1612_001:0848c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為慚？",
        "from": "T31n1612_001:0848c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂自增上，及法增上，於所作罪羞恥為性。",
        "from": "T31n1612_001:0848c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為愧？",
        "from": "T31n1612_001:0848c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂世增上，於所作罪羞恥為性。",
        "from": "T31n1612_001:0848c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何無貪？",
        "from": "T31n1612_001:0848c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂貪對治，令深厭患無著為性。",
        "from": "T31n1612_001:0848c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何無瞋？",
        "from": "T31n1612_001:0848c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂瞋對治，以慈為性。",
        "from": "T31n1612_001:0848c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何無癡？",
        "from": "T31n1612_001:0848c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂癡對治，以其如實正行為性。",
        "from": "T31n1612_001:0848c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何精進？",
        "from": "T31n1612_001:0848c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂懈怠對治，心於善品勇悍為性。",
        "from": "T31n1612_001:0848c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何輕安？",
        "from": "T31n1612_001:0848c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂麁重對治，身心調暢堪能為性。",
        "from": "T31n1612_001:0848c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何不放逸？",
        "from": "T31n1612_001:0848c30_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂放逸對治，即是無貪乃至精進。",
        "from": "T31n1612_001:0848c30_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依止此故，捨不善法及即修彼對治善法。",
        "from": "T31n1612_001:0849a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為捨？",
        "from": "T31n1612_001:0849a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂即無貪乃至精進。",
        "from": "T31n1612_001:0849a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依止此故，獲得所有心平等性，心正直性，心無發悟性。",
        "from": "T31n1612_001:0849a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又由此故，於已除遣染污法中，無染安住。",
        "from": "T31n1612_001:0849a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何不害？",
        "from": "T31n1612_001:0849a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂害對治，以悲為性。",
        "from": "T31n1612_001:0849a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為貪？",
        "from": "T31n1612_001:0849a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於五取蘊染愛耽著為性。",
        "from": "T31n1612_001:0849a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為瞋？",
        "from": "T31n1612_001:0849a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於有情樂作損害為性。",
        "from": "T31n1612_001:0849a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為慢？",
        "from": "T31n1612_001:0849a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂七慢：一、慢，二、過慢，三、慢過慢，四、我慢，五、增上慢，六、卑慢，七、邪慢。",
        "from": "T31n1612_001:0849a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何慢？",
        "from": "T31n1612_001:0849a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於劣計己勝、或於等計己等，心高舉為性。",
        "from": "T31n1612_001:0849a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何過慢？",
        "from": "T31n1612_001:0849a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於等計己勝、或於勝計己等，心高舉為性。",
        "from": "T31n1612_001:0849a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何慢過慢？",
        "from": "T31n1612_001:0849a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於勝計己勝，心高舉為性。",
        "from": "T31n1612_001:0849a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何我慢？",
        "from": "T31n1612_001:0849a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於五取蘊隨觀為我或為我所，心高舉為性。",
        "from": "T31n1612_001:0849a13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何增上慢？",
        "from": "T31n1612_001:0849a14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於未得增上殊勝所證法中謂我已得，心高舉為性。",
        "from": "T31n1612_001:0849a14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何卑慢？",
        "from": "T31n1612_001:0849a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於多分殊勝計己少分，下劣心高舉為性。",
        "from": "T31n1612_001:0849a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何邪慢？",
        "from": "T31n1612_001:0849a17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂實無德計己有德，心高舉為性。",
        "from": "T31n1612_001:0849a17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何無明？",
        "from": "T31n1612_001:0849a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於業果及諦寶中無智為性。",
        "from": "T31n1612_001:0849a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此復二種，所謂俱生、分別所起。",
        "from": "T31n1612_001:0849a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又欲纏貪、瞋及欲纏無明，名三不善根，謂貪不善根、瞋不善根、癡不善根。",
        "from": "T31n1612_001:0849a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為見？",
        "from": "T31n1612_001:0849a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂五見：一、薩迦耶見，二、邊執見，三、邪見，四、見取，五、戒禁取。",
        "from": "T31n1612_001:0849a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何薩迦耶見？",
        "from": "T31n1612_001:0849a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於五取蘊隨觀為我或為我所，染污慧為性。",
        "from": "T31n1612_001:0849a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何邊執見？",
        "from": "T31n1612_001:0849a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂即由彼增上力故，隨觀為常或復為斷，染污慧為性。",
        "from": "T31n1612_001:0849a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何邪見？",
        "from": "T31n1612_001:0849a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂或謗因或復謗果、或謗作用或壞善事，染污慧為性。",
        "from": "T31n1612_001:0849a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何見取？",
        "from": "T31n1612_001:0849a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂即於三見及彼所依諸蘊，隨觀為最為上為勝為極，染污慧為性。",
        "from": "T31n1612_001:0849a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何戒禁取？",
        "from": "T31n1612_001:0849b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於戒禁及彼所依諸蘊，隨觀為清淨為解脫為出離，染污慧為性。",
        "from": "T31n1612_001:0849b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為疑？",
        "from": "T31n1612_001:0849b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於諦等猶豫為性。",
        "from": "T31n1612_001:0849b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸煩惱中，後三見及疑唯分別起，餘通俱生及分別起。",
        "from": "T31n1612_001:0849b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為忿？",
        "from": "T31n1612_001:0849b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂遇現前不饒益事心損惱為性。",
        "from": "T31n1612_001:0849b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為恨？",
        "from": "T31n1612_001:0849b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂結怨不捨為性。",
        "from": "T31n1612_001:0849b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為覆？",
        "from": "T31n1612_001:0849b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於自罪覆藏為性。",
        "from": "T31n1612_001:0849b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為惱？",
        "from": "T31n1612_001:0849b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂發暴惡言尤蛆為性。",
        "from": "T31n1612_001:0849b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為嫉？",
        "from": "T31n1612_001:0849b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於他盛事心妬為性。",
        "from": "T31n1612_001:0849b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為慳？",
        "from": "T31n1612_001:0849b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂施相違心悋為性。",
        "from": "T31n1612_001:0849b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為誑？",
        "from": "T31n1612_001:0849b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂為誑他詐現不實事為性。",
        "from": "T31n1612_001:0849b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為諂？",
        "from": "T31n1612_001:0849b11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂覆藏自過方便所攝心曲為性。",
        "from": "T31n1612_001:0849b11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為憍？",
        "from": "T31n1612_001:0849b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於自盛事染著倨傲心恃為性。",
        "from": "T31n1612_001:0849b12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為害？",
        "from": "T31n1612_001:0849b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於諸有情損惱為性。",
        "from": "T31n1612_001:0849b13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何無慚？",
        "from": "T31n1612_001:0849b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於所作罪不自羞恥為性。",
        "from": "T31n1612_001:0849b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何無愧？",
        "from": "T31n1612_001:0849b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於所作罪不羞恥他為性。",
        "from": "T31n1612_001:0849b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何惛沈？",
        "from": "T31n1612_001:0849b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂心不調暢無所堪能蒙昧為性。",
        "from": "T31n1612_001:0849b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何掉舉？",
        "from": "T31n1612_001:0849b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂心不寂靜為性。",
        "from": "T31n1612_001:0849b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何不信？",
        "from": "T31n1612_001:0849b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂信所對治，於業果等不正信順，心不清淨為性。",
        "from": "T31n1612_001:0849b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何懈怠？",
        "from": "T31n1612_001:0849b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂精進所治，於諸善品心不勇猛為性。",
        "from": "T31n1612_001:0849b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何放逸？",
        "from": "T31n1612_001:0849b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂即由貪瞋癡懈怠故，於諸煩惱心不防護，於諸善品不能修習為性。",
        "from": "T31n1612_001:0849b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何失念？",
        "from": "T31n1612_001:0849b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂染污念，於諸善法不能明記為性。",
        "from": "T31n1612_001:0849b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何散亂？",
        "from": "T31n1612_001:0849b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂貪瞋癡分心流蕩為性。",
        "from": "T31n1612_001:0849b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何不正知？",
        "from": "T31n1612_001:0849b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於身語意現前行中不正依住為性。",
        "from": "T31n1612_001:0849b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何惡作？",
        "from": "T31n1612_001:0849b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂心變悔為性。",
        "from": "T31n1612_001:0849b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何睡眠？",
        "from": "T31n1612_001:0849b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂不自在轉心極昧略為性。",
        "from": "T31n1612_001:0849b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為尋？",
        "from": "T31n1612_001:0849b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂能尋求意言分別思慧差別令心麁為性。",
        "from": "T31n1612_001:0849b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為伺？",
        "from": "T31n1612_001:0849b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂能伺察意言分別思慧差別令心細為性。",
        "from": "T31n1612_001:0849b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何心不相應行？",
        "from": "T31n1612_001:0849b29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂依色、心、心法分位，但假建立，不可施設決定異性及不異性。",
        "from": "T31n1612_001:0849c01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼復云何？",
        "from": "T31n1612_001:0849c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂得、無想等至、滅盡等至、無想所有、命根、眾同分、生、老、住、無常、名身、句身、文身、異生性如是等類。",
        "from": "T31n1612_001:0849c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為得？",
        "from": "T31n1612_001:0849c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂若獲若成就。",
        "from": "T31n1612_001:0849c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此復三種，謂若種子、若自在、若現前，如其所應。",
        "from": "T31n1612_001:0849c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何無想等至？",
        "from": "T31n1612_001:0849c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂已離遍淨貪、未離上貪，由出離想作意為先，不恒現行心心法滅為性。",
        "from": "T31n1612_001:0849c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何滅盡等至？",
        "from": "T31n1612_001:0849c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂已離無所有處貪，從第一有更求勝進，由止息想作意為先，不恒現行及恒行一分心心法滅為性。",
        "from": "T31n1612_001:0849c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何無想所有？",
        "from": "T31n1612_001:0849c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂無想等至果。",
        "from": "T31n1612_001:0849c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無想有情天中生已，不恒現行心心法滅為性。",
        "from": "T31n1612_001:0849c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何命根？",
        "from": "T31n1612_001:0849c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於眾同分中，先業所引，住時決定為性。",
        "from": "T31n1612_001:0849c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何眾同分？",
        "from": "T31n1612_001:0849c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂諸有情自類相似為性。",
        "from": "T31n1612_001:0849c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為生？",
        "from": "T31n1612_001:0849c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於眾同分中，諸行本無今有為性。",
        "from": "T31n1612_001:0849c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為老？",
        "from": "T31n1612_001:0849c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂即如是諸行相續變異為性。",
        "from": "T31n1612_001:0849c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為住？",
        "from": "T31n1612_001:0849c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂即如是諸行相續隨轉為性。",
        "from": "T31n1612_001:0849c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何無常？",
        "from": "T31n1612_001:0849c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂即如是諸行相續謝滅為性。",
        "from": "T31n1612_001:0849c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名身？",
        "from": "T31n1612_001:0849c20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂諸法自性增語為性。",
        "from": "T31n1612_001:0849c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何句身？",
        "from": "T31n1612_001:0849c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂諸法差別增語為性。",
        "from": "T31n1612_001:0849c21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何文身？",
        "from": "T31n1612_001:0849c22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂諸字為性，以能表彰前二種故；亦名為顯，由與名句為所依止顯了義故；亦名為字，非差別門所變易故。",
        "from": "T31n1612_001:0849c22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何異生性？",
        "from": "T31n1612_001:0849c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於諸聖法不得為性。",
        "from": "T31n1612_001:0849c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等類已說行蘊。",
        "from": "T31n1612_001:0849c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何識蘊？",
        "from": "T31n1612_001:0849c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於所緣境了別為性。",
        "from": "T31n1612_001:0849c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名心意，由採集故、意所攝故。",
        "from": "T31n1612_001:0849c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最勝心者，謂阿賴耶識。",
        "from": "T31n1612_001:0849c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故？",
        "from": "T31n1612_001:0849c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此識中諸行種子皆採集故。",
        "from": "T31n1612_001:0849c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此行緣不可分別，前後一類相續隨轉。",
        "from": "T31n1612_001:0850a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又由此故，從滅盡等至、無想等至、無想所有起者，了別境名轉識還生，待所緣緣差別轉故、數數間斷還復轉故、又令生死流轉旋還故。",
        "from": "T31n1612_001:0850a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿賴耶識者，謂能攝藏一切種子故，又能攝藏我慢相故，又復緣身為境界故。",
        "from": "T31n1612_001:0850a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即此亦名阿陀那識，能執持身故。",
        "from": "T31n1612_001:0850a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最勝意者，謂緣阿賴耶識為境，恒與我癡、我見、我慢及我愛等相應之識，前後一類相續隨轉，除阿羅漢果及與聖道滅盡等至現在前位。",
        "from": "T31n1612_001:0850a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問：「以何義故說名為蘊？」",
        "from": "T31n1612_001:0850a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答：「以積聚義說名為蘊。",
        "from": "T31n1612_001:0850a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂世相續品類趣處差別色等總略攝故。」",
        "from": "T31n1612_001:0850a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有十二處，謂眼處色處、耳處聲處、鼻處香處、舌處味處、身處觸處、意處法處。",
        "from": "T31n1612_001:0850a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼等五處及色聲香味處，如前已釋。",
        "from": "T31n1612_001:0850a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言觸處者，謂四大種及前所說所觸一分。",
        "from": "T31n1612_001:0850a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言意處者，即是識蘊。",
        "from": "T31n1612_001:0850a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言法處者，謂受想行蘊無表色等及與無為。",
        "from": "T31n1612_001:0850a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何無為？",
        "from": "T31n1612_001:0850a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂虛空無為、非擇滅無為、擇滅無為及真如等。",
        "from": "T31n1612_001:0850a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何虛空？",
        "from": "T31n1612_001:0850a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂若容受諸色。",
        "from": "T31n1612_001:0850a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何非擇滅？",
        "from": "T31n1612_001:0850a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂若滅非離繫。",
        "from": "T31n1612_001:0850a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此復云何？",
        "from": "T31n1612_001:0850a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂離煩惱對治而諸蘊畢竟不生。",
        "from": "T31n1612_001:0850a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何擇滅？",
        "from": "T31n1612_001:0850a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂若滅是離繫。",
        "from": "T31n1612_001:0850a21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此復云何？",
        "from": "T31n1612_001:0850a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂由煩惱對治故諸蘊畢竟不生。",
        "from": "T31n1612_001:0850a22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何真如？",
        "from": "T31n1612_001:0850a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂諸法法性、法無我性。",
        "from": "T31n1612_001:0850a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問：「以何義故名為處耶？」",
        "from": "T31n1612_001:0850a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答：「諸識生長門義，是處義。」",
        "from": "T31n1612_001:0850a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有十八界，謂眼界、色界、眼識界，耳界、聲界、耳識界，鼻界、香界、鼻識界，舌界、味界、舌識界，身界、觸界、身識界，意界、法界、意識界。",
        "from": "T31n1612_001:0850a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼等諸界及色等諸界，如處中說。",
        "from": "T31n1612_001:0850a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六識界者，謂依眼等根、緣色等境，了別為性。",
        "from": "T31n1612_001:0850a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言意界者，謂即彼無間滅等，為欲顯示第六意識，及廣建立十八界故。",
        "from": "T31n1612_001:0850b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是色蘊，即十處十界及法處法界，一分識蘊即意處及七心界，餘三蘊及色蘊一分并諸無為即法處法界。",
        "from": "T31n1612_001:0850b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問以何義故說名為界。",
        "from": "T31n1612_001:0850b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答以能任持無作用性自相義故。",
        "from": "T31n1612_001:0850b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說名為界。",
        "from": "T31n1612_001:0850b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問：「以何義故宣說蘊等？」",
        "from": "T31n1612_001:0850b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答：「為欲對治三種我執。",
        "from": "T31n1612_001:0850b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如其次第三種我執者，謂一性我執、受者我執、作者我執。」",
        "from": "T31n1612_001:0850b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次此十八界幾有色？",
        "from": "T31n1612_001:0850b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂十界一少分即色蘊自性。",
        "from": "T31n1612_001:0850b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾無色？",
        "from": "T31n1612_001:0850b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂所餘界。",
        "from": "T31n1612_001:0850b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾有見？",
        "from": "T31n1612_001:0850b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂一色界。",
        "from": "T31n1612_001:0850b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾無見？",
        "from": "T31n1612_001:0850b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂所餘界。",
        "from": "T31n1612_001:0850b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾有對？",
        "from": "T31n1612_001:0850b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂十有色界。",
        "from": "T31n1612_001:0850b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若彼於是處有所障礙，是有對義。",
        "from": "T31n1612_001:0850b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾無對？",
        "from": "T31n1612_001:0850b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂所餘界。",
        "from": "T31n1612_001:0850b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾有漏？",
        "from": "T31n1612_001:0850b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂十五界及後三少分，由於是處煩惱起故、現所行處故。",
        "from": "T31n1612_001:0850b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾無漏？",
        "from": "T31n1612_001:0850b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂後三少分。",
        "from": "T31n1612_001:0850b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾欲界繫？",
        "from": "T31n1612_001:0850b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂一切。",
        "from": "T31n1612_001:0850b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾色界繫？",
        "from": "T31n1612_001:0850b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂十四，除香、味，鼻、舌識。",
        "from": "T31n1612_001:0850b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾無色界繫？",
        "from": "T31n1612_001:0850b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂後三。",
        "from": "T31n1612_001:0850b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾不繫？",
        "from": "T31n1612_001:0850b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂即彼無漏界。",
        "from": "T31n1612_001:0850b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾蘊所攝？",
        "from": "T31n1612_001:0850b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂除無為。",
        "from": "T31n1612_001:0850b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾取蘊所攝？",
        "from": "T31n1612_001:0850b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂有漏。",
        "from": "T31n1612_001:0850b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾善？",
        "from": "T31n1612_001:0850b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾不善？",
        "from": "T31n1612_001:0850b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾無記？",
        "from": "T31n1612_001:0850b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂十，通三種七心界及色聲法界，八無記。",
        "from": "T31n1612_001:0850b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾是內？",
        "from": "T31n1612_001:0850c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂十二，除色、聲、香、味、觸及法界。",
        "from": "T31n1612_001:0850c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾是外？",
        "from": "T31n1612_001:0850c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂六即所除。",
        "from": "T31n1612_001:0850c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾有緣？",
        "from": "T31n1612_001:0850c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂七心界及法界少分心所有法。",
        "from": "T31n1612_001:0850c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾無緣？",
        "from": "T31n1612_001:0850c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂餘十及法界少分。",
        "from": "T31n1612_001:0850c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾有分別？",
        "from": "T31n1612_001:0850c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂意界、意識界、法界少分。",
        "from": "T31n1612_001:0850c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾執受？",
        "from": "T31n1612_001:0850c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂五內界及四界少分，謂色、香、味、觸。",
        "from": "T31n1612_001:0850c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾非執受？",
        "from": "T31n1612_001:0850c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂餘九四少分。",
        "from": "T31n1612_001:0850c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾同分？",
        "from": "T31n1612_001:0850c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂五內有色界，由與自識等境界故。",
        "from": "T31n1612_001:0850c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾彼同分？",
        "from": "T31n1612_001:0850c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂即彼自識空時與自類等故。",
        "from": "T31n1612_001:0850c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大乘五蘊論一卷",
        "from": "T31n1612_001:0850c10_0",
        "context_with_next_sentence": false
    }
]