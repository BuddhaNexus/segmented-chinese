[
    {
        "source_sentence": "佛說華積陀羅尼神呪經",
        "from": "T21n1356_001:0874c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吳月支國居士支謙譯",
        "from": "T21n1356_001:0874c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T21n1356_001:0874c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在阿耨達池龍王宮中。",
        "from": "T21n1356_001:0874c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大比丘五百人俱。",
        "from": "T21n1356_001:0874c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及菩薩摩訶薩具足一千。",
        "from": "T21n1356_001:0874c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉得一切陀羅尼。",
        "from": "T21n1356_001:0874c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘一生在住十住地。",
        "from": "T21n1356_001:0874c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當作佛。",
        "from": "T21n1356_001:0874c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如王太子。",
        "from": "T21n1356_001:0874c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱於十方皆成佛道。",
        "from": "T21n1356_001:0874c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具大莊嚴不疑如來一切功德。",
        "from": "T21n1356_001:0874c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天龍八部一時俱會。",
        "from": "T21n1356_001:0874c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時師子威菩薩。",
        "from": "T21n1356_001:0874c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從座而起右膝著地。",
        "from": "T21n1356_001:0874c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌向佛而白佛言。",
        "from": "T21n1356_001:0874c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊若善男子善女人。",
        "from": "T21n1356_001:0874c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於如來所而修供養功德多不。",
        "from": "T21n1356_001:0874c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告師子威勿作是語。",
        "from": "T21n1356_001:0874c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人。",
        "from": "T21n1356_001:0874c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於如來所而修供養功德多不。",
        "from": "T21n1356_001:0874c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故。",
        "from": "T21n1356_001:0874c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來無量戒定慧解脫解脫知見之所成就。",
        "from": "T21n1356_001:0874c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人。",
        "from": "T21n1356_001:0874c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於如來所若存若亡而修供養。",
        "from": "T21n1356_001:0874c20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師子威當知是人終必於三乘中。",
        "from": "T21n1356_001:0874c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨其因緣而得解脫。",
        "from": "T21n1356_001:0874c22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次師子威。",
        "from": "T21n1356_001:0874c22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人見佛世尊。",
        "from": "T21n1356_001:0874c23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心生歡喜尊重讚歎。",
        "from": "T21n1356_001:0874c23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以諸供養施於如來。",
        "from": "T21n1356_001:0874c24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復有人於如來滅後。",
        "from": "T21n1356_001:0874c24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見舍利心生歡喜。",
        "from": "T21n1356_001:0874c25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是二功德等無有異。",
        "from": "T21n1356_001:0874c25_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次師子威。",
        "from": "T21n1356_001:0874c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若在家菩薩以諸珍寶積如須彌。",
        "from": "T21n1356_001:0874c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養聲聞及辟支佛。",
        "from": "T21n1356_001:0874c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不如出家菩薩發菩提心。",
        "from": "T21n1356_001:0874c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以一金錢供養如來。",
        "from": "T21n1356_001:0874c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次師子威若復有人。",
        "from": "T21n1356_001:0874c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於千萬歲親近世尊備修供養。",
        "from": "T21n1356_001:0874c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人如來滅後發菩提心。",
        "from": "T21n1356_001:0874c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以一香華入僧伽藍。",
        "from": "T21n1356_001:0875a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉足下足作如是言。",
        "from": "T21n1356_001:0875a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無世尊。",
        "from": "T21n1356_001:0875a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此深解殷重信心。",
        "from": "T21n1356_001:0875a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養如來舍利寶塔。",
        "from": "T21n1356_001:0875a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不以求報不以疑惑。",
        "from": "T21n1356_001:0875a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未得解脫中間。",
        "from": "T21n1356_001:0875a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若千萬億劫不墮惡道。",
        "from": "T21n1356_001:0875a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師子威諦聽。",
        "from": "T21n1356_001:0875a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有陀羅尼名曰華積。",
        "from": "T21n1356_001:0875a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今當說。",
        "from": "T21n1356_001:0875a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸天人多所饒益。",
        "from": "T21n1356_001:0875a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子。",
        "from": "T21n1356_001:0875a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能於華積陀羅尼呪。",
        "from": "T21n1356_001:0875a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持讀誦親近依行。",
        "from": "T21n1356_001:0875a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功德勝彼。",
        "from": "T21n1356_001:0875a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人世世得一聞持。",
        "from": "T21n1356_001:0875a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不墮諸惡險棘道中離諸艱難。",
        "from": "T21n1356_001:0875a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常見妙寶常見諸佛諸根常具。",
        "from": "T21n1356_001:0875a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不生下賤卑隷人家。",
        "from": "T21n1356_001:0875a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常得不離菩薩弘心。",
        "from": "T21n1356_001:0875a10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常得種種無量慧辯。",
        "from": "T21n1356_001:0875a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為無量十方諸佛如來之所知見。",
        "from": "T21n1356_001:0875a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃說呪曰。",
        "from": "T21n1356_001:0875a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但施(峙耶反)他(一)杜羅禰(二)拖羅禰(三)拖羅尼(四)拖羅尼(五)侔尼(六)波羅婆(負荷反)娑禰(七)悉諦(八)栴禘(九)那侔紙(十)嫟訶(引)囇(狸迤反十一)魯伽婆底(十二)佛陀婆底(十三)底囇(狸迤反十四)柯羅知(胝迤反十五)羅竪波伽知(胝迤反十六)知(胝迤反)杜婆底(十七)毘捨羅佛地(十八)達摩婆徙(十九)惡叉耶葛俾(二十)葛波毘伽知(胝迤二十一)阿媚多葛波休多大多禰(二十二)闍波底(二十三)哆嫟(微耶反)沙摩憙(吳音反)知(胝迤反二十四)蜀伽羅婆底(二十五)陟沙拏佛朕(持引反)地耶佛地瑣訶(引二十六)師子威當知。",
        "from": "T21n1356_001:0875a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子能於此華積神呪。",
        "from": "T21n1356_001:0875a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若讀若誦。",
        "from": "T21n1356_001:0875a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人當於三月四月九月。",
        "from": "T21n1356_001:0875a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從八日至十五日。",
        "from": "T21n1356_001:0875a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心憶念如來相好。",
        "from": "T21n1356_001:0875a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜中三誦華積神呪。",
        "from": "T21n1356_001:0875a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日中亦三。",
        "from": "T21n1356_001:0875a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至月圓時。",
        "from": "T21n1356_001:0875a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當以香華燈燭於形像前而修供養。",
        "from": "T21n1356_001:0875a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并誦華積陀羅尼呪。",
        "from": "T21n1356_001:0875a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人是夜夢見如來。",
        "from": "T21n1356_001:0875a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相好具足坐蓮華座為眾說法。",
        "from": "T21n1356_001:0875a28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦得華積陀羅尼呪。",
        "from": "T21n1356_001:0875a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於現世中常得強記。",
        "from": "T21n1356_001:0875a29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智慧深信。",
        "from": "T21n1356_001:0875a29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從今身乃至涅槃。",
        "from": "T21n1356_001:0875b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常一聞持一切聞見。",
        "from": "T21n1356_001:0875b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切經教一切學解。",
        "from": "T21n1356_001:0875b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切技藝通達無礙。",
        "from": "T21n1356_001:0875b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切三昧深得清淨。",
        "from": "T21n1356_001:0875b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解四聖諦無上法輪。",
        "from": "T21n1356_001:0875b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是經已。",
        "from": "T21n1356_001:0875b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師子威等天龍八部。",
        "from": "T21n1356_001:0875b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切大眾歡喜奉行。",
        "from": "T21n1356_001:0875b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華積陀羅尼神呪經",
        "from": "T21n1356_001:0875b05_0",
        "context_with_next_sentence": false
    }
]