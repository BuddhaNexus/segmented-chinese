[
    {
        "source_sentence": "供養護世八天法",
        "from": "T21n1295_001:0380a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "青龍寺沙門法全集",
        "from": "T21n1295_001:0380a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘇婆呼經云。",
        "from": "T21n1295_001:0380a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修真言行者。",
        "from": "T21n1295_001:0380a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日日供養護方天神。",
        "from": "T21n1295_001:0380a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則無災難。",
        "from": "T21n1295_001:0380a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於道場外八方。",
        "from": "T21n1295_001:0380a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敷茅草或荷葉。",
        "from": "T21n1295_001:0380a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或塗圓壇為十位。",
        "from": "T21n1295_001:0380a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於帝釋左右安梵天地天。",
        "from": "T21n1295_001:0380a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若道場外無置位處。",
        "from": "T21n1295_001:0380a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於道場前閑淨處為方界。",
        "from": "T21n1295_001:0380a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中在八方。",
        "from": "T21n1295_001:0380a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中央安置梵天地天。",
        "from": "T21n1295_001:0380a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以施十方天。",
        "from": "T21n1295_001:0380a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食應用雜粥。",
        "from": "T21n1295_001:0380a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂粳米油麻。",
        "from": "T21n1295_001:0380a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先置一淨葉令極清淨。",
        "from": "T21n1295_001:0380a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香美盛一器中。",
        "from": "T21n1295_001:0380a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每坐先置一淨葉或淨荼椀。",
        "from": "T21n1295_001:0380a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以淨瓶盛香水。",
        "from": "T21n1295_001:0380a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後[跍*月]跪合掌。",
        "from": "T21n1295_001:0380a18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "召請十方世天。",
        "from": "T21n1295_001:0380a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謹請東方憍尸迦天主與諸眷屬來降於此。",
        "from": "T21n1295_001:0380a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我所辦供願垂納受。",
        "from": "T21n1295_001:0380a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謹請東南方火天山等與諸眷屬來降於此。",
        "from": "T21n1295_001:0380a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我所辦供願垂納受。",
        "from": "T21n1295_001:0380a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謹請南方琰摩天主。",
        "from": "T21n1295_001:0380a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅剎主。",
        "from": "T21n1295_001:0380a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天部多天王。",
        "from": "T21n1295_001:0380a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水天龍王。",
        "from": "T21n1295_001:0380a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風神王等。",
        "from": "T21n1295_001:0380a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多聞天王。",
        "from": "T21n1295_001:0380a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伊舍那天王。",
        "from": "T21n1295_001:0380a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大梵天王。",
        "from": "T21n1295_001:0380a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謹請地居所有諸大神王與諸眷屬來降於此。",
        "from": "T21n1295_001:0380a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我所辦供願垂納受。",
        "from": "T21n1295_001:0380a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願冥加衛護於我。",
        "from": "T21n1295_001:0380a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心想本方諮白。",
        "from": "T21n1295_001:0380a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "退請。",
        "from": "T21n1295_001:0380b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以梵名請世天。",
        "from": "T21n1295_001:0380b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東北伊舍那眷屬部多眾戟印三昧拳火竪屈風背。",
        "from": "T21n1295_001:0380b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伊舍那真言曰。",
        "from": "T21n1295_001:0380b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三(去)滿多沒馱喃(引)嚕(心)捺囉(二合)野娑嚩(二合)賀(引)觀想護方天王各從天宮降臨來此。",
        "from": "T21n1295_001:0380b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依本方位摽心令住。",
        "from": "T21n1295_001:0380b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝釋印。",
        "from": "T21n1295_001:0380b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內縛二風申如針。",
        "from": "T21n1295_001:0380b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝釋天王真言曰。",
        "from": "T21n1295_001:0380b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三(去)滿多沒馱喃(引)鑠吃羅(二合)也娑嚩(二合)賀(引)火仙。",
        "from": "T21n1295_001:0380b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定掌火空相捻三角。",
        "from": "T21n1295_001:0380b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惠竪四輪空度橫在掌中。",
        "from": "T21n1295_001:0380b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風屈三召。",
        "from": "T21n1295_001:0380b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火天真言曰。",
        "from": "T21n1295_001:0380b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱喃阿[木*疑]曩(二合)曳娑嚩(二合)賀焰摩。",
        "from": "T21n1295_001:0380b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定惠合地風背入月。",
        "from": "T21n1295_001:0380b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "焰摩王真言曰。",
        "from": "T21n1295_001:0380b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱喃縛(無背反)嚩娑嚩多野(二合)娑嚩(二合)賀羅剎恐怖形。",
        "from": "T21n1295_001:0380b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西南方持刀。",
        "from": "T21n1295_001:0380b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1295_001:0380b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼多沒馱喃羅(入)吃察(二合)娑地跛多娑嚩(二合)賀水天執羂索。",
        "from": "T21n1295_001:0380b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1295_001:0380b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多母馱喃阿播鉢多曳娑嚩(二合)賀縛庾風天憧智拳地水竪。",
        "from": "T21n1295_001:0380b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風天真言曰。",
        "from": "T21n1295_001:0380b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼多沒馱喃嚩(引)野吠娑嚩賀多聞虛心合二地入掌交。",
        "from": "T21n1295_001:0380b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空竪風側屈一寸不相著。",
        "from": "T21n1295_001:0380b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多聞天王真言曰。",
        "from": "T21n1295_001:0380b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多母馱喃味室囉(二合)摩拏野娑縛賀梵天持紅蓮三昧空持水。",
        "from": "T21n1295_001:0380b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵王真言曰。",
        "from": "T21n1295_001:0380b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多母馱喃鉢羅(二合)若鉢哆曳娑縛賀地神持寶瓶。",
        "from": "T21n1295_001:0380b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地神真言曰。",
        "from": "T21n1295_001:0380c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼多母馱喃鉢囉(二合)體(丁以反)吠曳娑嚩(二合)賀日天福智仰水入空持剎火輪欲相並水火自相持。",
        "from": "T21n1295_001:0380c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1295_001:0380c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多母馱喃阿儞怛也(二合)野娑嚩(二合)賀月天三昧手持於白蓮華。",
        "from": "T21n1295_001:0380c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空捻火初節。",
        "from": "T21n1295_001:0380c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1295_001:0380c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼多母馱喃戰捺囉(二合)野娑婆賀稱名敬白。",
        "from": "T21n1295_001:0380c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護方天王十二宮神。",
        "from": "T21n1295_001:0380c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九執大天二十八宿。",
        "from": "T21n1295_001:0380c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "業道冥宮本命宿主。",
        "from": "T21n1295_001:0380c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今遇此災變。",
        "from": "T21n1295_001:0380c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "某事相凌敬謝天眾。",
        "from": "T21n1295_001:0380c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "順佛教勅。",
        "from": "T21n1295_001:0380c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受我迎請。",
        "from": "T21n1295_001:0380c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉來赴會響此。",
        "from": "T21n1295_001:0380c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "單誠發歡喜心。",
        "from": "T21n1295_001:0380c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為我某甲息除災障。",
        "from": "T21n1295_001:0380c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增長福壽。",
        "from": "T21n1295_001:0380c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或云。",
        "from": "T21n1295_001:0380c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從今日起首習真言。",
        "from": "T21n1295_001:0380c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我承大聖護念力故。",
        "from": "T21n1295_001:0380c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拔濟我等一切有情輪迴苦業。",
        "from": "T21n1295_001:0380c17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願護世天主遊空宿曜。",
        "from": "T21n1295_001:0380c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "順佛教令輪施我無畏令安樂住。",
        "from": "T21n1295_001:0380c18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應供養淨香水。",
        "from": "T21n1295_001:0380c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寫少水葉上以獻。",
        "from": "T21n1295_001:0380c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1295_001:0380c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿密哩(二合)帝吽泮吒",
        "from": "T21n1295_001:0380c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗香真言曰。",
        "from": "T21n1295_001:0380c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中名指彈少塗香獻淡。",
        "from": "T21n1295_001:0380c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼多沒馱(引)喃(引)尾戍馱誐□納婆(二合)嚩娑嚩(二合)賀花鬘真言曰。",
        "from": "T21n1295_001:0380c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相反仰蓮掌置額右旋轉。",
        "from": "T21n1295_001:0380c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱喃摩賀(引)妹怛哩也(二合)毘庾(二合)訥[(薩文+(立一))/木]帝娑嚩(二合)賀焚香真言曰。",
        "from": "T21n1295_001:0380c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地水火相背。",
        "from": "T21n1295_001:0380c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二風側相合。",
        "from": "T21n1295_001:0380c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空捻風輪上節開。",
        "from": "T21n1295_001:0380c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱喃□摩馱怛嚩(二合)拏[(薩文+(立一))/木]帝娑嚩(二合)賀飲食真言曰。",
        "from": "T21n1295_001:0381a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密合。",
        "from": "T21n1295_001:0381a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱喃阿囉囉迦囉囉末隣(上)捺娜[方*耳]沫捺禰摩賀沫瀝娑嚩(二合)賀燈明真言曰。",
        "from": "T21n1295_001:0381a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右羽為拳竪。",
        "from": "T21n1295_001:0381a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火輪空押風捻火中節。",
        "from": "T21n1295_001:0381a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱喃怛他(引)[(薩文+(立一))/木]多(引)囉[吃乙+日](二合)娑叵(二合)囉儜嚩婆(引)娑曩誐誐[猱柔+弟]那哩也(二合)娑嚩(二合)賀虛空藏菩薩普供養真言曰。",
        "from": "T21n1295_001:0381a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫薩嚩怛他[(薩文+(立一))/木]帝驃(一)尾濕嚩(二合)目契弊薩嚩他欠[嗚?+?]娜[(薩文+(立一))/木]帝娑頗(引)儞[牟*含]誐誐娜鉤娑婆(二合)賀觀想護方天王各從天宮降臨來此。",
        "from": "T21n1295_001:0381a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依本方位摽心令住。",
        "from": "T21n1295_001:0381a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱名敬白。",
        "from": "T21n1295_001:0381a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護方天王十二宮神。",
        "from": "T21n1295_001:0381a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九執大天二十八宿。",
        "from": "T21n1295_001:0381a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "業道冥宮本命宿主。",
        "from": "T21n1295_001:0381a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今遇此災變。",
        "from": "T21n1295_001:0381a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "某事相凌敬謝天眾。",
        "from": "T21n1295_001:0381a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "順佛教勅。",
        "from": "T21n1295_001:0381a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受我迎請。",
        "from": "T21n1295_001:0381a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉來赴會響此。",
        "from": "T21n1295_001:0381a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "單誠發歡喜心。",
        "from": "T21n1295_001:0381a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為我某息除災障。",
        "from": "T21n1295_001:0381a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增長福壽(或云。",
        "from": "T21n1295_001:0381a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "某從今日起首若干日來。",
        "from": "T21n1295_001:0381a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦某真言。",
        "from": "T21n1295_001:0381a20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其間守護道場。",
        "from": "T21n1295_001:0381a20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅除障難。",
        "from": "T21n1295_001:0381a21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘事某白)我承大聖護念力故。",
        "from": "T21n1295_001:0381a21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拔濟我等及一切有情輪迴苦業。",
        "from": "T21n1295_001:0381a21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願護方天主遊空宿曜。",
        "from": "T21n1295_001:0381a22_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "順佛教令輪故施我無畏令安樂住。",
        "from": "T21n1295_001:0381a23_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請白了已。",
        "from": "T21n1295_001:0381a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應供養淨瓶香水葉上以獻。",
        "from": "T21n1295_001:0381a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1295_001:0381a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱(引)喃(引)印捺羅(二合)邪遏鉗次以中名指彈獻少塗香以獻即言獻淡。",
        "from": "T21n1295_001:0381a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以一花置之於坐次。",
        "from": "T21n1295_001:0381a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "献花名補澁(二合)甘(補甘反)焚董□香。",
        "from": "T21n1295_001:0381a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於座前梵語燒香。",
        "from": "T21n1295_001:0381a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號之度(引)補甘(二合)。",
        "from": "T21n1295_001:0381a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次卒一杓粥置於葉上。",
        "from": "T21n1295_001:0381b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又言未臨。",
        "from": "T21n1295_001:0381b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次用小蠟燭插在粥上。",
        "from": "T21n1295_001:0381b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵云儞(引)補甘(二合)布(引)惹室哩(二合引)泥(去)嚩補怛囉(二合)寫扇底俱嚕娑嚩(二合引)賀。",
        "from": "T21n1295_001:0381b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每位從水至燭獻畢然向。",
        "from": "T21n1295_001:0381b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其次其燭作意獻諸位。",
        "from": "T21n1295_001:0381b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未遍已來莫令燭滅。",
        "from": "T21n1295_001:0381b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或須助伴驅使數人。",
        "from": "T21n1295_001:0381b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各執一事以祇供。",
        "from": "T21n1295_001:0381b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一一自取。",
        "from": "T21n1295_001:0381b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恐燭不終法事似不吉祥。",
        "from": "T21n1295_001:0381b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "再白求事。",
        "from": "T21n1295_001:0381b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或可施繞。",
        "from": "T21n1295_001:0381b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或復安坐。",
        "from": "T21n1295_001:0381b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念大吉祥真言三七遍。",
        "from": "T21n1295_001:0381b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又念佛慈護真言一百八遍。",
        "from": "T21n1295_001:0381b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將此持誦饒益諸天。",
        "from": "T21n1295_001:0381b11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願諸天樂轉更增勝。",
        "from": "T21n1295_001:0381b11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供獻之間多不如法。",
        "from": "T21n1295_001:0381b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願慈悲施我歡喜。",
        "from": "T21n1295_001:0381b12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師應執爐遍乞歡喜求請無災。",
        "from": "T21n1295_001:0381b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說偈□明發遣天主諸天自在威神力。",
        "from": "T21n1295_001:0381b13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今□求者速成就。",
        "from": "T21n1295_001:0381b14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願天主還本宮後。",
        "from": "T21n1295_001:0381b15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若請時垂哀赴。",
        "from": "T21n1295_001:0381b15_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)吉里吉里嚩日羅(二合)目娑嚩(二合)賀(引)次施一切鬼神食。",
        "from": "T21n1295_001:0381b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘇婆呼經云。",
        "from": "T21n1295_001:0381b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次蘇婆呼。",
        "from": "T21n1295_001:0381b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若念誦人欲得成就作持諸法令無障難得悉地者。",
        "from": "T21n1295_001:0381b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以諸飲食祭祀諸天修羅藥叉龍王迦嚕拏共命鳥羯吒布單那乾闥婆部多諸鬼魅等。",
        "from": "T21n1295_001:0381b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居地上或處虛空。",
        "from": "T21n1295_001:0381b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令念誦人無諸難事。",
        "from": "T21n1295_001:0381b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "意願所求皆悉滿足。",
        "from": "T21n1295_001:0381b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言契印牢結護。",
        "from": "T21n1295_001:0381b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右膝著地啟請言曰。",
        "from": "T21n1295_001:0381b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居妙高山諸天部多。",
        "from": "T21n1295_001:0381b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居歡喜園及餘天宮。",
        "from": "T21n1295_001:0381b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居日月。",
        "from": "T21n1295_001:0381b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居河海或陂澤泉井。",
        "from": "T21n1295_001:0381b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居村落。",
        "from": "T21n1295_001:0381b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居神廟。",
        "from": "T21n1295_001:0381b25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居空室。",
        "from": "T21n1295_001:0381b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居天室。",
        "from": "T21n1295_001:0381b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或住伽藍制帝。",
        "from": "T21n1295_001:0381b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居外道草菴。",
        "from": "T21n1295_001:0381b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居家室。",
        "from": "T21n1295_001:0381b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居庫藏。",
        "from": "T21n1295_001:0381b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或住街巷。",
        "from": "T21n1295_001:0381b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居四□道邊。",
        "from": "T21n1295_001:0381b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或依獨樹。",
        "from": "T21n1295_001:0381b28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或在大路。",
        "from": "T21n1295_001:0381b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或住塚間。",
        "from": "T21n1295_001:0381b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居屍多林。",
        "from": "T21n1295_001:0381b29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或寄住大林。",
        "from": "T21n1295_001:0381b29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居師子大虫遊行之處。",
        "from": "T21n1295_001:0381b29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或住大砂磧中。",
        "from": "T21n1295_001:0381c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或居海洲諸上妙處。",
        "from": "T21n1295_001:0381c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諮居與諸眷屬降臨來此。",
        "from": "T21n1295_001:0381c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我所營辦花鬘塗香飲食及妙燈明。",
        "from": "T21n1295_001:0381c02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願垂影嚮。",
        "from": "T21n1295_001:0381c03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我所求願事滿足。",
        "from": "T21n1295_001:0381c03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其果即以銅器或淨瓫。",
        "from": "T21n1295_001:0381c04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置種種甘美飲食菓子甜脆等。",
        "from": "T21n1295_001:0381c04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆須擘碎。",
        "from": "T21n1295_001:0381c05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦施焰口餓鬼食真言。",
        "from": "T21n1295_001:0381c05_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起慈悲心。",
        "from": "T21n1295_001:0381c06_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "己手按盆。",
        "from": "T21n1295_001:0381c06_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持三二七遍。",
        "from": "T21n1295_001:0381c06_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又念妙色身如來真言。",
        "from": "T21n1295_001:0381c07_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持水灑。",
        "from": "T21n1295_001:0381c07_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又念普供養印明。",
        "from": "T21n1295_001:0381c07_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至誠運求所禮花香飲食廣多細妙。",
        "from": "T21n1295_001:0381c08_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又念偈曰。",
        "from": "T21n1295_001:0381c09_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　以我所修福　　普霑於鬼趣",
        "from": "T21n1295_001:0381c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　食已免飢苦　　捨身生樂處",
        "from": "T21n1295_001:0381c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即將障外或於宅□□方散施。",
        "from": "T21n1295_001:0381c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或施長流水中。",
        "from": "T21n1295_001:0381c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施與無名□鬼神食之。",
        "from": "T21n1295_001:0381c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1295_001:0381c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多母馱(引)南(引)印娜囉(二合)耶(二)仡哩(二合)賀拏(二合)伊[牟*含](引三)遏鉗(四)巘淡(五)補澁鉗(補甘反)度(引)補甘(二合引)麼臨(八)儞(引)補甘(二合)布(引)若室哩(二合引十)泥嚩補怛囉(二合)寫(十一)扇底俱嚕娑嚩(二合引)賀(引十二)總云。",
        "from": "T21n1295_001:0381c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命諸佛及帝釋。",
        "from": "T21n1295_001:0381c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受此遏伽塗香花鬘燒香粥食及妙燈明供養。",
        "from": "T21n1295_001:0381c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉祥天子與某甲作息災。",
        "from": "T21n1295_001:0381c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福智增盛。",
        "from": "T21n1295_001:0381c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉祥圓滿。",
        "from": "T21n1295_001:0381c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故言　娑嚩(二合引)賀。",
        "from": "T21n1295_001:0381c21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施十方天王真言印契等。",
        "from": "T21n1295_001:0381c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡受諸法及一期念誦畢及護摩灌頂等。",
        "from": "T21n1295_001:0381c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆須施諸天速得無難。",
        "from": "T21n1295_001:0381c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌曰。",
        "from": "T21n1295_001:0381c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　觀心法界生　　成本忿怒壇",
        "from": "T21n1295_001:0381c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　三隅娜誐形　　猛熾徹梵天",
        "from": "T21n1295_001:0381c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　三部次□□　　引索四召請",
        "from": "T21n1295_001:0381c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　成身誦三遍　　或復一□□",
        "from": "T21n1295_001:0381c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　事畢發願已　　後結本方尊",
        "from": "T21n1295_001:0382a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　各誦本真言　　送聖護方天",
        "from": "T21n1295_001:0382a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　依法念誦畢　　歌讚發願已",
        "from": "T21n1295_001:0382a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　解界送聖天　　方出道場外",
        "from": "T21n1295_001:0382a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　八隅塗圓壇　　而為十聖位",
        "from": "T21n1295_001:0382a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　於帝釋左右　　置梵天地天",
        "from": "T21n1295_001:0382a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　或復於龍方　　梵地相對置",
        "from": "T21n1295_001:0382a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　日天月天位　　曜宿皆隨轉",
        "from": "T21n1295_001:0382a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　中位觀四臂　　不動忿怒王",
        "from": "T21n1295_001:0382a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　兩手金剛拳　　風地曲如鉤",
        "from": "T21n1295_001:0382a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　以安口兩邊　　右鉤左持索",
        "from": "T21n1295_001:0382a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　須彌山為坐　　身外現八峯",
        "from": "T21n1295_001:0382a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　利銳金剛輪　　洪滿紺青色",
        "from": "T21n1295_001:0382a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　蓬頭垂髮索　　威焰如劫燒",
        "from": "T21n1295_001:0382a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　勢如[(薩文+(立一))/木]嚕拏　　赫奕無能對　魔羅及鉢底　　攝怖而奔竄　乘昔大誓力　　現茲暴惡形　吞嘩諸障惱　　安慰修行者　布列方位已　　供養以雜粥　以粳米油麻　　菉豆相和煮　然於諸壇位　　各置一瓷器　先以淨香水　　少傾於器中　次奉以□□　　用指而彈指　又奉諸名花　　次燒香□□　然以一杓粥　　皆以殷重心　各加持三遍　　次執小蠟燭　用攝於粥上　　事未畢已來　此燭勿令滅　　於圓寂字上　加所求願言　　聖力密加持　所作皆悉地　　斯乃助修行　獨期願亦得　　若欲請施時　標心令安位　　依法而供養　決定無疑念　　願者皆成就　九執日為主　　諸宿月為主　梵地沒捺囉　　今當次第說　八葉鉢頭摩　　沒囉賀摩印　十度如閼伽　　地天沒捺囉　以定握惠腕　　惠竪舒五輪　空輪入月橫　　風狀鉤來去　火天請送印　　將禪捻戒度　向外以三彈　　是遣沒捺羅真言如經說。",
        "from": "T21n1295_001:0382a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵名歎如來十號功德",
        "from": "T21n1295_001:0382b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛他誐姤(如來)囉翰(應供)三藐三母馱(正遍知)尾儞耶(二合月)□□(行)播拏(足)索誐姤(善逝)路迦□□姤拏怛囉(二合無上士)補嚕灑娜[方*耳]耶些羅體(調御丁以反)捨些跢(丈夫)泥嚩(天)難者摩拏史耶(人)南者母度(佛)薄誐鑁(世尊)天供次第先於道場外。",
        "from": "T21n1295_001:0382b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敷茅或蓮葉或青草等。",
        "from": "T21n1295_001:0382b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗圓壇并辦備六種具蠟燭等。",
        "from": "T21n1295_001:0382b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次著座三部被甲護身。",
        "from": "T21n1295_001:0382b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次加持香水(用枳里枳羅印明二十一遍。",
        "from": "T21n1295_001:0382b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散灑四方供物等)次用羯磨印加持七遍。",
        "from": "T21n1295_001:0382b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次加持壇(用枳里枳羅明。",
        "from": "T21n1295_001:0382b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七遍若二十一遍)次淨地(如大儀)次五大願并陳事由。",
        "from": "T21n1295_001:0382b25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及誦般若經典饒□□天等。",
        "from": "T21n1295_001:0382b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次大輪金剛印明地壇等□觀想壇中 字變成獨古杵。",
        "from": "T21n1295_001:0382b27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "杵變成四臂明王(如護摩說)次四字明(如大儀軌)想本來住尊加被壇中尊冥合無二也。",
        "from": "T21n1295_001:0382b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次隨方召請誦大鉤召印明。",
        "from": "T21n1295_001:0382c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1295_001:0382c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三曼沒馱(引)喃(一)阿(去息呼)薩嚩怛囉(二合)鉢囉(二合)底訶諦(二)怛他[(薩文+(立一))/木]黨矩奢(三)昌地折哩也(二合)鉢哩布囉迦(四)娑嚩(二合)賀(引)次誦梵名勸請　次[跍*月]跪誦漠語勸謁(如法全儀軌。",
        "from": "T21n1295_001:0382c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若繁者不誦好也)　次誦四智讚。",
        "from": "T21n1295_001:0382c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次捧閼伽奉獻不動明王　次座印。",
        "from": "T21n1295_001:0382c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次網。",
        "from": "T21n1295_001:0382c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次炎。",
        "from": "T21n1295_001:0382c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次塗花燒。",
        "from": "T21n1295_001:0382c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如常念誦奉獻不動尊　次一一供獻諸天等(隨方位各各可置一器每遏伽等日誦各各本明三　□□□□□□□□□□)。",
        "from": "T21n1295_001:0382c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次普供養明。",
        "from": "T21n1295_001:0382c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結印加持五處。",
        "from": "T21n1295_001:0382c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次不動慈救真言一百八遍。",
        "from": "T21n1295_001:0382c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慇懃祈願數了。",
        "from": "T21n1295_001:0382c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前結護懺悔。",
        "from": "T21n1295_001:0382c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次事主天真言一百八遍。",
        "from": "T21n1295_001:0382c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一千八十遍。",
        "from": "T21n1295_001:0382c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可念為灌護施主也。",
        "from": "T21n1295_001:0382c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次念如來慈護真言一百八遍。",
        "from": "T21n1295_001:0382c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又誦大吉祥天真言一百八遍(回向之義也)。",
        "from": "T21n1295_001:0382c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次後供養奉獻不動明王。",
        "from": "T21n1295_001:0382c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次誦普供養明供養明王□海會塵剎諸尊護世天等。",
        "from": "T21n1295_001:0382c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次四智讚及天龍八部讚。",
        "from": "T21n1295_001:0382c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次迴向。",
        "from": "T21n1295_001:0382c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次解界。",
        "from": "T21n1295_001:0382c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如例一一解。",
        "from": "T21n1295_001:0382c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次閼伽。",
        "from": "T21n1295_001:0382c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次發遣。",
        "from": "T21n1295_001:0382c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次三部護身。",
        "from": "T21n1295_001:0382c18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如先初出道",
        "from": "T21n1295_001:0382c18_9",
        "context_with_next_sentence": false
    }
]