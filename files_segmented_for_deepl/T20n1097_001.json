[
    {
        "source_sentence": "不空羂索陀羅尼自在王呪經",
        "from": "T20n1097_001:0421b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卷上",
        "from": "T20n1097_001:0421b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐天竺三藏寶思惟奉　詔譯",
        "from": "T20n1097_001:0421b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南謨囉哆那怛囉夜耶　南謨阿唎耶　阿弭哆婆耶　怛他孽多耶　南謨阿唎耶　跋嚧吉帝　失筏囉耶　菩地薩埵耶　摩訶薩埵耶　摩訶迦嚧尼迦耶　怛跌他　唵阿慕伽　鉢囉底喝多[合*牛][合*牛][口*半]吒　娑婆訶如是所說不空陀羅尼自在王呪。",
        "from": "T20n1097_001:0421b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是一切祕密神呪之主。",
        "from": "T20n1097_001:0421b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人能誦此神呪成就之者。",
        "from": "T20n1097_001:0421b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即能通達一切神呪。",
        "from": "T20n1097_001:0421b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但是呪法所有事業皆得圓滿。",
        "from": "T20n1097_001:0421b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就尊者說不空神呪功德分第一",
        "from": "T20n1097_001:0421b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時聖觀自在菩薩摩訶薩。",
        "from": "T20n1097_001:0421b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說不空羂索神呪王法。",
        "from": "T20n1097_001:0421b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若成就者福不唐捐。",
        "from": "T20n1097_001:0421c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令無量無邊諸眾生界。",
        "from": "T20n1097_001:0421c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切業障悉皆清淨。",
        "from": "T20n1097_001:0421c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能集無量福德資糧。",
        "from": "T20n1097_001:0421c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增長善根方便善巧。",
        "from": "T20n1097_001:0421c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通達無邊智慧境界。",
        "from": "T20n1097_001:0421c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六波羅蜜多皆得圓滿。",
        "from": "T20n1097_001:0421c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復能證得無上菩提。",
        "from": "T20n1097_001:0421c04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四無所畏十八不共一切佛法。",
        "from": "T20n1097_001:0421c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及四聖諦神足根力菩提分法。",
        "from": "T20n1097_001:0421c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復能示現靜慮解脫諸三摩地三摩鉢底。",
        "from": "T20n1097_001:0421c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復能證得聲聞辟支佛及如來地。",
        "from": "T20n1097_001:0421c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就般若聰慧利根。",
        "from": "T20n1097_001:0421c08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大威德精進勢力。",
        "from": "T20n1097_001:0421c08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足辯才騰空隱形。",
        "from": "T20n1097_001:0421c09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為斫羯囉伐賴底曷囉闍一切自在。",
        "from": "T20n1097_001:0421c09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能成呪仙獲世安樂。",
        "from": "T20n1097_001:0421c10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得吉祥瓶及如意珠。",
        "from": "T20n1097_001:0421c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨應所作一切皆得。",
        "from": "T20n1097_001:0421c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "驅策使者進退無違。",
        "from": "T20n1097_001:0421c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令羅剎娑隨意而轉。",
        "from": "T20n1097_001:0421c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復能入於阿素洛窟。",
        "from": "T20n1097_001:0421c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或燒眾香或置藥眼中。",
        "from": "T20n1097_001:0421c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能見地下一切伏藏。",
        "from": "T20n1097_001:0421c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所為事業自在成就。",
        "from": "T20n1097_001:0421c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "療治眾病調伏鬼神守護壇場。",
        "from": "T20n1097_001:0421c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸龍王歡喜悅樂。",
        "from": "T20n1097_001:0421c15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降雨止雨皆得自在。",
        "from": "T20n1097_001:0421c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復能於彼一切人中而得自在。",
        "from": "T20n1097_001:0421c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令一切眾罪厄難悉皆銷滅。",
        "from": "T20n1097_001:0421c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "資財寶物所求皆得。",
        "from": "T20n1097_001:0421c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復能成就無邊陀羅尼門及三摩地門。",
        "from": "T20n1097_001:0421c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大不空羂索神呪之法。",
        "from": "T20n1097_001:0421c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸呪中功德最上殊勝廣大。",
        "from": "T20n1097_001:0421c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令一切天龍藥叉健闥縛阿素洛揭路茶緊捺洛莫呼洛伽。",
        "from": "T20n1097_001:0421c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸部多鬼一切餓鬼。",
        "from": "T20n1097_001:0421c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畢舍遮拘畔茶羅剎娑。",
        "from": "T20n1097_001:0421c22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "星宿鬼障礙鬼等。",
        "from": "T20n1097_001:0421c22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "承事供養尊重讚歎。",
        "from": "T20n1097_001:0421c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復為帝釋諸梵王等之所擁護。",
        "from": "T20n1097_001:0421c23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復為一切成呪仙者共所慶慰。",
        "from": "T20n1097_001:0421c24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復為常離一切散亂無所著者之所承習。",
        "from": "T20n1097_001:0421c25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持所說供養恭敬尊重讚歎。",
        "from": "T20n1097_001:0421c26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就受持供養神呪法分第二",
        "from": "T20n1097_001:0421c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時聖觀自在菩薩摩訶薩。",
        "from": "T20n1097_001:0421c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說神呪之法。",
        "from": "T20n1097_001:0421c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有欲誦此神呪時。",
        "from": "T20n1097_001:0421c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應先洗浴著新淨衣。",
        "from": "T20n1097_001:0421c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "潔淨誦呪受菩薩戒。",
        "from": "T20n1097_001:0421c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起慈悲心哀愍眾生意樂。",
        "from": "T20n1097_001:0422a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作真實語遠離嫉妬。",
        "from": "T20n1097_001:0422a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利益安樂一切有情。",
        "from": "T20n1097_001:0422a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直心淨信愛樂功德。",
        "from": "T20n1097_001:0422a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨離貪愛瞋恚愚癡。",
        "from": "T20n1097_001:0422a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憶念三寶期心報恩。",
        "from": "T20n1097_001:0422a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應廣供養聖觀自在菩薩摩訶薩。",
        "from": "T20n1097_001:0422a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒香散花塗香末香。",
        "from": "T20n1097_001:0422a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莊嚴幢幡及諸華蓋。",
        "from": "T20n1097_001:0422a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於尊者前一心淨信意樂堅固。",
        "from": "T20n1097_001:0422a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應誦聖者不空羂索心神呪王一百八遍。",
        "from": "T20n1097_001:0422a06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即入不空陀羅尼三摩地門。",
        "from": "T20n1097_001:0422a07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作蓮花印禁出入息默然而住。",
        "from": "T20n1097_001:0422a08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由是此人所應作事皆得具足。",
        "from": "T20n1097_001:0422a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於夢中得見聖觀自在菩薩。",
        "from": "T20n1097_001:0422a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或現苾芻形或現婆羅門形或現童子形。",
        "from": "T20n1097_001:0422a10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或現帝王形或現宰官形。",
        "from": "T20n1097_001:0422a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讚誦呪人作如是言。",
        "from": "T20n1097_001:0422a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉丈夫汝能受持我之所說。",
        "from": "T20n1097_001:0422a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝所愛樂皆得圓滿。",
        "from": "T20n1097_001:0422a13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今持呪心何所欲。",
        "from": "T20n1097_001:0422a13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意求索能令汝願速得成就。",
        "from": "T20n1097_001:0422a14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由是得滅一切業障。",
        "from": "T20n1097_001:0422a14_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永不墮於地獄畜生。",
        "from": "T20n1097_001:0422a15_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常生一切諸善趣中。",
        "from": "T20n1097_001:0422a15_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "般若現前辯才無滯增長正念。",
        "from": "T20n1097_001:0422a16_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天隨順而為伴侶。",
        "from": "T20n1097_001:0422a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所作自在無有障礙具大威神。",
        "from": "T20n1097_001:0422a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復能持呪隨心所欲。",
        "from": "T20n1097_001:0422a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為一切怨賊侵奪。",
        "from": "T20n1097_001:0422a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢聖讚歎。",
        "from": "T20n1097_001:0422a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常為國王大臣婆羅門居士等之所敬重。",
        "from": "T20n1097_001:0422a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生之所愛樂。",
        "from": "T20n1097_001:0422a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養恭敬承事禮拜善言讚歎。",
        "from": "T20n1097_001:0422a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常與一切驚怖之者作歸依處。",
        "from": "T20n1097_001:0422a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能得一切善人腹心。",
        "from": "T20n1097_001:0422a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有常誦此神呪者。",
        "from": "T20n1097_001:0422a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三業惡障皆悉銷滅。",
        "from": "T20n1097_001:0422a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦萬遍。",
        "from": "T20n1097_001:0422a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所須飲食臥具湯藥衣服什物無所乏少。",
        "from": "T20n1097_001:0422a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安樂無病長壽自在。",
        "from": "T20n1097_001:0422a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就親見聖觀自在菩薩法分第三",
        "from": "T20n1097_001:0422a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲得親見聖觀自在菩薩。",
        "from": "T20n1097_001:0422a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應往寂靜山寺塔廟之中。",
        "from": "T20n1097_001:0422a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或園林河邊深山巖谷阿蘭若處。",
        "from": "T20n1097_001:0422a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著新淨衣持八戒齋。",
        "from": "T20n1097_001:0422a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受菩薩戒三業清淨。",
        "from": "T20n1097_001:0422a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入三摩地修四梵行無所悋惜。",
        "from": "T20n1097_001:0422b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸持戒福德人邊常求歡喜。",
        "from": "T20n1097_001:0422b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後於聖觀自在菩薩像前受其呪法既受法已。",
        "from": "T20n1097_001:0422b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即便於彼清淨之處。",
        "from": "T20n1097_001:0422b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉祥草上結加趺坐。",
        "from": "T20n1097_001:0422b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應誦不空羂索心呪一心不動。",
        "from": "T20n1097_001:0422b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若晝若夜不驚不怖。",
        "from": "T20n1097_001:0422b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至忽聞虛空之中。",
        "from": "T20n1097_001:0422b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有虎嘯聲亦不驚怖。",
        "from": "T20n1097_001:0422b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見聞空中諸天音樂歌舞唱伎不應驚異。",
        "from": "T20n1097_001:0422b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或見天雨曼陀羅華摩訶曼陀羅華。",
        "from": "T20n1097_001:0422b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鉢特摩華嗢鉢羅華。",
        "from": "T20n1097_001:0422b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱沒陀華奔茶利華等亦不驚怪。",
        "from": "T20n1097_001:0422b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時聖觀自在菩薩。",
        "from": "T20n1097_001:0422b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至其人前唱言善哉善哉。",
        "from": "T20n1097_001:0422b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝能如是供養於我。",
        "from": "T20n1097_001:0422b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦持神呪何所求耶。",
        "from": "T20n1097_001:0422b11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其誦呪人即從坐起。",
        "from": "T20n1097_001:0422b11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於尊者前燒香散華。",
        "from": "T20n1097_001:0422b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水和粳米及諸雜華。",
        "from": "T20n1097_001:0422b12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養聖觀自在菩薩。",
        "from": "T20n1097_001:0422b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右遶三匝頭面禮足。",
        "from": "T20n1097_001:0422b13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩爾時隨其意樂悉皆與之。",
        "from": "T20n1097_001:0422b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就畫像幀法分第四",
        "from": "T20n1097_001:0422b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時尊者說畫像法及成就呪法。",
        "from": "T20n1097_001:0422b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若畫聖觀自在菩薩形像之時。",
        "from": "T20n1097_001:0422b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應織絹[疊*毛]隨其長短不截兩頭。",
        "from": "T20n1097_001:0422b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所用綵色和以香膠勿取餘膠。",
        "from": "T20n1097_001:0422b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫師先受八戒齋法。",
        "from": "T20n1097_001:0422b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後方畫聖觀自在菩薩形像。",
        "from": "T20n1097_001:0422b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其身黃白首戴華冠紺髮分被兩肩前後。",
        "from": "T20n1097_001:0422b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈顏和悅放百千光。",
        "from": "T20n1097_001:0422b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清淨殊勝面有三目。",
        "from": "T20n1097_001:0422b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以純白綖交絡肩臆。",
        "from": "T20n1097_001:0422b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以瑿泥耶鹿王皮而覆肩上。",
        "from": "T20n1097_001:0422b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莊飾寶帶以繫其腰。",
        "from": "T20n1097_001:0422b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊者四臂。",
        "from": "T20n1097_001:0422b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左邊上手執持蓮華。",
        "from": "T20n1097_001:0422b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下手執持澡瓶。",
        "from": "T20n1097_001:0422b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右邊上手施無畏下手執數珠。",
        "from": "T20n1097_001:0422b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆以珍寶而嚴飾之。",
        "from": "T20n1097_001:0422b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身著天衣立蓮華上。",
        "from": "T20n1097_001:0422b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大威德瓔珞短長。",
        "from": "T20n1097_001:0422b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "交髆垂下耳璫臂印。",
        "from": "T20n1097_001:0422b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及以環釧皆寶飾之。",
        "from": "T20n1097_001:0422b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂上畫作無量壽佛。",
        "from": "T20n1097_001:0422b27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其幀內畫種種華。",
        "from": "T20n1097_001:0422b28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲作壇於春秋時。",
        "from": "T20n1097_001:0422b28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白月八日或十五日。",
        "from": "T20n1097_001:0422b29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉祥星下選擇好時無風無雲。",
        "from": "T20n1097_001:0422b29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於城外擇取好處隨其方地。",
        "from": "T20n1097_001:0422c01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有諸樹木根莖枝葉。",
        "from": "T20n1097_001:0422c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華果茂盛條蔓交加。",
        "from": "T20n1097_001:0422c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周遍皆有流泉浴池。",
        "from": "T20n1097_001:0422c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處處青草皆悉充遍。",
        "from": "T20n1097_001:0422c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應用黃土為泥塗地。",
        "from": "T20n1097_001:0422c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種修理盡令如法。",
        "from": "T20n1097_001:0422c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不於荊棘瓦礫骨石諸惡土地。",
        "from": "T20n1097_001:0422c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其壇四面以金精赤土雄黃石灰及紫金色。",
        "from": "T20n1097_001:0422c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "青黃赤白及以紫色。",
        "from": "T20n1097_001:0422c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫其界道於壇方面各開一門。",
        "from": "T20n1097_001:0422c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "門外各有二吉祥柱。",
        "from": "T20n1097_001:0422c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其壇內應畫螺形萬字香印。",
        "from": "T20n1097_001:0422c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "門內各有二吉祥瓶。",
        "from": "T20n1097_001:0422c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨其處所應作浴池。",
        "from": "T20n1097_001:0422c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "池內周遍嗢鉢羅華俱沒頭華奔茶利迦華。",
        "from": "T20n1097_001:0422c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於池四邊應畫鵝形。",
        "from": "T20n1097_001:0422c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如貫華鬘周匝圍遶。",
        "from": "T20n1097_001:0422c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應置像幀壇場之內。",
        "from": "T20n1097_001:0422c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散種種華燒種種香。",
        "from": "T20n1097_001:0422c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幢幡寶蓋而供養之。",
        "from": "T20n1097_001:0422c12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或用金銀諸寶及以赤銅作吉祥瓶。",
        "from": "T20n1097_001:0422c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盛滿一切諸妙香藥末尼真珠金銀等寶和雜盛之。",
        "from": "T20n1097_001:0422c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以雜華繩繫其瓶項。",
        "from": "T20n1097_001:0422c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取燒稻穀作華。",
        "from": "T20n1097_001:0422c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散以嚴飾壇上。",
        "from": "T20n1097_001:0422c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應以白縵覆之。",
        "from": "T20n1097_001:0422c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於壇四角各立一人。",
        "from": "T20n1097_001:0422c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身被甲仗而守護之。",
        "from": "T20n1097_001:0422c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時呪師每日三時。",
        "from": "T20n1097_001:0422c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洗浴清潔著新淨衣。",
        "from": "T20n1097_001:0422c18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三業清淨受持律儀。",
        "from": "T20n1097_001:0422c18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於聖觀自在菩薩像前。",
        "from": "T20n1097_001:0422c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應以乳糜酥酪粆糖石蜜。",
        "from": "T20n1097_001:0422c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盛滿器中而供養之。",
        "from": "T20n1097_001:0422c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後以栴檀沈水蘇合龍腦。",
        "from": "T20n1097_001:0422c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等香和雜燒之而為供養。",
        "from": "T20n1097_001:0422c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於像幀前。",
        "from": "T20n1097_001:0422c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敷吉祥草結加趺坐。",
        "from": "T20n1097_001:0422c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作蓮華印安心合掌。",
        "from": "T20n1097_001:0422c22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮一切諸佛菩薩。",
        "from": "T20n1097_001:0422c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦護身呪。",
        "from": "T20n1097_001:0422c23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰第二。",
        "from": "T20n1097_001:0422c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　阿慕伽　鉢囉底訶多囉叉囉叉(自稱名)[合*牛]泮吒若欲護身時取自髮一莖。",
        "from": "T20n1097_001:0422c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應以此呪呪二十一遍。",
        "from": "T20n1097_001:0422c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常護自身無能損害。",
        "from": "T20n1097_001:0422c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪。",
        "from": "T20n1097_001:0422c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼神呪呪曰第三。",
        "from": "T20n1097_001:0423a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　阿慕伽　漫陀　[合*牛][合*牛]　泮吒若呪鬼神先誦此呪。",
        "from": "T20n1097_001:0423a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪五色綖一百八遍繫壇四面。",
        "from": "T20n1097_001:0423a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後呪水一百八遍。",
        "from": "T20n1097_001:0423a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或呪芥子或呪淨灰。",
        "from": "T20n1097_001:0423a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨四方面壇內散之。",
        "from": "T20n1097_001:0423a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即令一切惡鬼神等無能惱亂。",
        "from": "T20n1097_001:0423a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禁惡鬼呪呪曰第四。",
        "from": "T20n1097_001:0423a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　阿慕伽　鉢囉底訶多唵[合*牛]泮吒若禁惡鬼應誦此呪。",
        "from": "T20n1097_001:0423a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪水一百八遍。",
        "from": "T20n1097_001:0423a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或呪白芥子或呪淨灰。",
        "from": "T20n1097_001:0423a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨四方面壇外散之。",
        "from": "T20n1097_001:0423a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸惡鬼不得其便。",
        "from": "T20n1097_001:0423a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禁惡魔呪呪曰第五。",
        "from": "T20n1097_001:0423a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　阿慕伽　帝曬路枳耶　毘闍耶　俱嚕磨[合*牛]泮吒若呪魔時應誦此呪。",
        "from": "T20n1097_001:0423a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪水一百八遍。",
        "from": "T20n1097_001:0423a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或白芥子及呪淨灰。",
        "from": "T20n1097_001:0423a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其壇外散灑十方。",
        "from": "T20n1097_001:0423a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即禁諸惡魔不能嬈亂。",
        "from": "T20n1097_001:0423a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禁諸惡魔鬼呪呪曰第六。",
        "from": "T20n1097_001:0423a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　阿慕伽　囉叉(自稱名)娑婆訶若欲禁諸惡魔鬼時。",
        "from": "T20n1097_001:0423a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應以此呪呪水一百八遍。",
        "from": "T20n1097_001:0423a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或白芥子及呪淨灰。",
        "from": "T20n1097_001:0423a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "點自頂上額上心上及兩肩上。",
        "from": "T20n1097_001:0423a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍灑其身。",
        "from": "T20n1097_001:0423a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即令一切惡鬼惡魔不能惱亂。",
        "from": "T20n1097_001:0423a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪同伴人呪呪曰第七。",
        "from": "T20n1097_001:0423a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　阿慕伽　[合*牛]佉",
        "from": "T20n1097_001:0423a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲呪同伴人時。",
        "from": "T20n1097_001:0423a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應誦此呪。",
        "from": "T20n1097_001:0423a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪水一百八遍。",
        "from": "T20n1097_001:0423a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或白芥子及呪淨灰。",
        "from": "T20n1097_001:0423a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其壇內散灑同伴。",
        "from": "T20n1097_001:0423a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即護彼人。",
        "from": "T20n1097_001:0423a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡魔鬼眾無能惱亂。",
        "from": "T20n1097_001:0423a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪香呪呪曰第八。",
        "from": "T20n1097_001:0423a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　阿慕伽　淡磨淡磨　[合*牛]泮吒若燒香時。",
        "from": "T20n1097_001:0423b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以此呪。",
        "from": "T20n1097_001:0423b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪種種香二十一遍。",
        "from": "T20n1097_001:0423b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後燒香恭敬供養。",
        "from": "T20n1097_001:0423b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪飲食及華果呪呪曰第九。",
        "from": "T20n1097_001:0423b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　阿慕伽　[起-巳+乞]醯利儜　[起-巳+乞]醯唎儜　[合*牛]泮吒若以飲食及華果等欲散之時。",
        "from": "T20n1097_001:0423b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先誦此呪呪一百八遍。",
        "from": "T20n1097_001:0423b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後於其壇外。",
        "from": "T20n1097_001:0423b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨四方面而遍散之。",
        "from": "T20n1097_001:0423b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨作事成就呪呪曰第十。",
        "from": "T20n1097_001:0423b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　阿慕伽　毘闍耶　[合*牛]泮吒隨作何事若誦此呪悉皆成就。",
        "from": "T20n1097_001:0423b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊能持呪者。",
        "from": "T20n1097_001:0423b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是法已。",
        "from": "T20n1097_001:0423b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其壇內。",
        "from": "T20n1097_001:0423b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應誦不空陀羅尼自在王呪。",
        "from": "T20n1097_001:0423b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見聖觀自在菩薩像幀動時。",
        "from": "T20n1097_001:0423b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或煙出時或焰出時。",
        "from": "T20n1097_001:0423b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如常誦呪不應驚怖。",
        "from": "T20n1097_001:0423b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見幀動得富貴自在。",
        "from": "T20n1097_001:0423b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見煙出成就曷囉闍位。",
        "from": "T20n1097_001:0423b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見焰出騰空自在得成呪仙。",
        "from": "T20n1097_001:0423b17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其誦呪者若見焰出。",
        "from": "T20n1097_001:0423b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心無驚怖身不動搖。",
        "from": "T20n1097_001:0423b18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得聖觀自在菩薩。",
        "from": "T20n1097_001:0423b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現其人前而安慰之。",
        "from": "T20n1097_001:0423b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人見已所求皆遂。",
        "from": "T20n1097_001:0423b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得菩薩諸三摩地。",
        "from": "T20n1097_001:0423b20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於阿耨多羅三藐三菩提得不退轉。",
        "from": "T20n1097_001:0423b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現身憶念宿世生事。",
        "from": "T20n1097_001:0423b21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及得無量百千功德。",
        "from": "T20n1097_001:0423b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就使者能辦事法分第五",
        "from": "T20n1097_001:0423b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時聖觀自在菩薩。",
        "from": "T20n1097_001:0423b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說使者能辦事法。",
        "from": "T20n1097_001:0423b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此之使者即是聖觀自在菩薩。",
        "from": "T20n1097_001:0423b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空羂索王神呪之使。",
        "from": "T20n1097_001:0423b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲驅使。",
        "from": "T20n1097_001:0423b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應以綵色及[疊*毛]布幀上。",
        "from": "T20n1097_001:0423b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫使者形又作藥叉童子像。",
        "from": "T20n1097_001:0423b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭髮直竪如盛火焰。",
        "from": "T20n1097_001:0423b27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面目瞋怒綠眼平鼻。",
        "from": "T20n1097_001:0423b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形貌赤色身服赤衣。",
        "from": "T20n1097_001:0423b28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口出四牙二上二下。",
        "from": "T20n1097_001:0423b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其舌於口或入或出。",
        "from": "T20n1097_001:0423b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一手持劍一手執索。",
        "from": "T20n1097_001:0423b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚴身之具皆悉周備。",
        "from": "T20n1097_001:0423c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲作法時。",
        "from": "T20n1097_001:0423c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於白月八日或十四日持齋潔淨。",
        "from": "T20n1097_001:0423c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安置幀像四衢道中或空室內。",
        "from": "T20n1097_001:0423c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應以華香及諸飲食。",
        "from": "T20n1097_001:0423c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除血肉等種種供養。",
        "from": "T20n1097_001:0423c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其持呪者先應誦呪自防護身。",
        "from": "T20n1097_001:0423c04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於[疊*毛]幀前誦不空王呪一百八遍。",
        "from": "T20n1097_001:0423c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時使者現其人前。",
        "from": "T20n1097_001:0423c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "語持呪者言欲何所須。",
        "from": "T20n1097_001:0423c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見處分皆能成辦。",
        "from": "T20n1097_001:0423c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其持呪者隨心所欲使者依行。",
        "from": "T20n1097_001:0423c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "于時使者隨所見聞悉皆具說。",
        "from": "T20n1097_001:0423c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若持呪人日索金錢百文應時即得。",
        "from": "T20n1097_001:0423c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然不應別用及生悋惜。",
        "from": "T20n1097_001:0423c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但應供養佛法僧寶。",
        "from": "T20n1097_001:0423c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是之事但當自知。",
        "from": "T20n1097_001:0423c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得輒向人說之。",
        "from": "T20n1097_001:0423c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦復不得共人結怨。",
        "from": "T20n1097_001:0423c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應食淨食不得雜食。",
        "from": "T20n1097_001:0423c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每食之時先減己身一分飲食。",
        "from": "T20n1097_001:0423c12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養使者然後自食。",
        "from": "T20n1097_001:0423c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常須憶念不得忘之。",
        "from": "T20n1097_001:0423c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心常捨離貪瞋癡等不得妄語。",
        "from": "T20n1097_001:0423c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出誠諦言為他說法。",
        "from": "T20n1097_001:0423c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切眾生常起饒益慈悲之心。",
        "from": "T20n1097_001:0423c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復以香華衣服幢幡寶蓋。",
        "from": "T20n1097_001:0423c16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗香末香燒香散華。",
        "from": "T20n1097_001:0423c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒常供養聖觀自在菩薩作報恩心。",
        "from": "T20n1097_001:0423c17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能如是即彼使者日日供承。",
        "from": "T20n1097_001:0423c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得五百人所須資具。",
        "from": "T20n1097_001:0423c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲食衣服塗香末香。",
        "from": "T20n1097_001:0423c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒香散華一切具足。",
        "from": "T20n1097_001:0423c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至持呪之者盡形已來。",
        "from": "T20n1097_001:0423c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意皆得無所乏少。",
        "from": "T20n1097_001:0423c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "◎不空羂索陀羅尼自在王呪經卷上",
        "from": "T20n1097_001:0423c21_6",
        "context_with_next_sentence": false
    }
]