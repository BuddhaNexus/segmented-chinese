[
    {
        "source_sentence": "佛說無量大慈教經",
        "from": "T85n2903_001:1445a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有。",
        "from": "T85n2903_001:1445a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時阿難白佛言。",
        "from": "T85n2903_001:1445a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "T85n2903_001:1445a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯有闡提眾生。",
        "from": "T85n2903_001:1445a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難化難育。",
        "from": "T85n2903_001:1445a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊答。",
        "from": "T85n2903_001:1445a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如牛耕田由人所遣。",
        "from": "T85n2903_001:1445a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獼猴作舞由人所教。",
        "from": "T85n2903_001:1445a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造罪眾生由人所勸。",
        "from": "T85n2903_001:1445a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時阿難白佛言。",
        "from": "T85n2903_001:1445a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "T85n2903_001:1445a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生藥生西方國者。",
        "from": "T85n2903_001:1445a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣我身及向我口眼中不淨我亦不辭。",
        "from": "T85n2903_001:1445a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語阿難。",
        "from": "T85n2903_001:1445a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經大聖若有眾生勤誦此經者。",
        "from": "T85n2903_001:1445a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "免得三難苦。",
        "from": "T85n2903_001:1445a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一難禁囚。",
        "from": "T85n2903_001:1445a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二難病人。",
        "from": "T85n2903_001:1445a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三難地獄。",
        "from": "T85n2903_001:1445a18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告言。",
        "from": "T85n2903_001:1445a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難。",
        "from": "T85n2903_001:1445a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為閻浮眾生難化難育故。",
        "from": "T85n2903_001:1445a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開此經藏出其殊別。",
        "from": "T85n2903_001:1445a20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙經為汝演說。",
        "from": "T85n2903_001:1445a20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人現此經者不生清淨。",
        "from": "T85n2903_001:1445a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知是人與我無業。",
        "from": "T85n2903_001:1445a21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切經典廣說妙言。",
        "from": "T85n2903_001:1445a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "引度眾生。",
        "from": "T85n2903_001:1445a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一稱我名者。",
        "from": "T85n2903_001:1445a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉聞悉見。",
        "from": "T85n2903_001:1445a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨聲往救。",
        "from": "T85n2903_001:1445a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今身劫剝師僧者死墮寒氷地獄。",
        "from": "T85n2903_001:1445a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又生蠶中為他煮剝。",
        "from": "T85n2903_001:1445a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如此等以逕八萬之劫。",
        "from": "T85n2903_001:1445a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘受畜生身。",
        "from": "T85n2903_001:1445a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以逕五百劫。",
        "from": "T85n2903_001:1445a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蠢動眾生皆悉作遍。",
        "from": "T85n2903_001:1445a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後受人身。",
        "from": "T85n2903_001:1445a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "癃殘盲跛。",
        "from": "T85n2903_001:1445a26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五百劫中恒受啞報。",
        "from": "T85n2903_001:1445a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊答言。",
        "from": "T85n2903_001:1445a27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難。",
        "from": "T85n2903_001:1445a27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行不淨行尼僧死墮鐵窟中地獄。",
        "from": "T85n2903_001:1445a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八萬刀輪一時來下。",
        "from": "T85n2903_001:1445a28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斬截其身。",
        "from": "T85n2903_001:1445a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時阿難白佛言。",
        "from": "T85n2903_001:1445a29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如佛思量。",
        "from": "T85n2903_001:1445a29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此事超越三界。",
        "from": "T85n2903_001:1445a29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊語阿難。",
        "from": "T85n2903_001:1445b01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲酒醉亂死墮飲銅地獄中。",
        "from": "T85n2903_001:1445b01_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時阿難白佛言。",
        "from": "T85n2903_001:1445b02_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "T85n2903_001:1445b02_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酒之無命。",
        "from": "T85n2903_001:1445b02_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何故戒之。",
        "from": "T85n2903_001:1445b03_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊復語阿難。",
        "from": "T85n2903_001:1445b03_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲酒醉亂。",
        "from": "T85n2903_001:1445b03_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不識尊親。",
        "from": "T85n2903_001:1445b04_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我見振旦國有人因酒婬母。",
        "from": "T85n2903_001:1445b04_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破其五戒。",
        "from": "T85n2903_001:1445b04_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是以禁之。",
        "from": "T85n2903_001:1445b05_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊語諸大菩薩等。",
        "from": "T85n2903_001:1445b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我從成佛以來。",
        "from": "T85n2903_001:1445b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於今五百餘劫。",
        "from": "T85n2903_001:1445b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經典披遍。",
        "from": "T85n2903_001:1445b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讀誦周匝。",
        "from": "T85n2903_001:1445b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未聞是言。",
        "from": "T85n2903_001:1445b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未聞是說。",
        "from": "T85n2903_001:1445b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生聞此經者。",
        "from": "T85n2903_001:1445b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宿種善因。",
        "from": "T85n2903_001:1445b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宿種善果。",
        "from": "T85n2903_001:1445b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思尋此經。",
        "from": "T85n2903_001:1445b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可思量。",
        "from": "T85n2903_001:1445b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可稱盡。",
        "from": "T85n2903_001:1445b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時如來語諸菩薩。",
        "from": "T85n2903_001:1445b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我說此語。",
        "from": "T85n2903_001:1445b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重如太山。",
        "from": "T85n2903_001:1445b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生聞者輕如微塵。",
        "from": "T85n2903_001:1445b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此法難聞亦復難見。",
        "from": "T85n2903_001:1445b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語阿難。",
        "from": "T85n2903_001:1445b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生聞此經者。",
        "from": "T85n2903_001:1445b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心生歎喜。",
        "from": "T85n2903_001:1445b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如此之人盡心為說。",
        "from": "T85n2903_001:1445b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊復語阿難。",
        "from": "T85n2903_001:1445b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我見眾生墮落三塗。",
        "from": "T85n2903_001:1445b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若將刀割。",
        "from": "T85n2903_001:1445b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我身體由斯可忍。",
        "from": "T85n2903_001:1445b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我不忍見於眾生受大苦惱。",
        "from": "T85n2903_001:1445b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酸悲忍苦。",
        "from": "T85n2903_001:1445b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身上演出光明。",
        "from": "T85n2903_001:1445b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍照十方。",
        "from": "T85n2903_001:1445b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有我緣者得見我身。",
        "from": "T85n2903_001:1445b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有緣者不見我想爾時阿難白佛言。",
        "from": "T85n2903_001:1445b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "T85n2903_001:1445b17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如何二種心。",
        "from": "T85n2903_001:1445b17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一種眾生則有見者。",
        "from": "T85n2903_001:1445b18_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則無見者。",
        "from": "T85n2903_001:1445b18_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時如來復語阿難。",
        "from": "T85n2903_001:1445b18_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日月普照。",
        "from": "T85n2903_001:1445b19_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盲者不見。",
        "from": "T85n2903_001:1445b19_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時阿難重白。",
        "from": "T85n2903_001:1445b19_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "T85n2903_001:1445b20_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何是盲。",
        "from": "T85n2903_001:1445b20_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何是明。",
        "from": "T85n2903_001:1445b20_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊答言。",
        "from": "T85n2903_001:1445b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難。",
        "from": "T85n2903_001:1445b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修福者是明。",
        "from": "T85n2903_001:1445b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不修福者是盲。",
        "from": "T85n2903_001:1445b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難復白佛言。",
        "from": "T85n2903_001:1445b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "T85n2903_001:1445b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修福者不見由何。",
        "from": "T85n2903_001:1445b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊告阿難。",
        "from": "T85n2903_001:1445b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修福不見我身。",
        "from": "T85n2903_001:1445b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "障閉佛光污埿三寶。",
        "from": "T85n2903_001:1445b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是以不得見我。",
        "from": "T85n2903_001:1445b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語菩薩。",
        "from": "T85n2903_001:1445b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞是此經者心生歡喜。",
        "from": "T85n2903_001:1445b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如子見母遠行得歸。",
        "from": "T85n2903_001:1445b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如飢得食如渴得漿。",
        "from": "T85n2903_001:1445b25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如此之人盡心為說。",
        "from": "T85n2903_001:1445b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語阿難。",
        "from": "T85n2903_001:1445b26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食肉之者喻如群狗。",
        "from": "T85n2903_001:1445b26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爭骨各各貪多見。",
        "from": "T85n2903_001:1445b27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其猪羊常作殺想見。",
        "from": "T85n2903_001:1445b27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其臭肉如猫趣鼠。",
        "from": "T85n2903_001:1445b28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惠心用意。",
        "from": "T85n2903_001:1445b28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今身信解佛法者。",
        "from": "T85n2903_001:1445b28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從人中來。",
        "from": "T85n2903_001:1445b29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今身不信佛者。",
        "from": "T85n2903_001:1445b29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從畜生中來。",
        "from": "T85n2903_001:1445b29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造罪不懺悔者。",
        "from": "T85n2903_001:1445c01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喻如運金填國損其寶物。",
        "from": "T85n2903_001:1445c01_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造罪悔者如病得藥還復差。",
        "from": "T85n2903_001:1445c02_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "損汗衣水洗還得清淨。",
        "from": "T85n2903_001:1445c02_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語普廣菩薩。",
        "from": "T85n2903_001:1445c03_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用我語者一偈成佛。",
        "from": "T85n2903_001:1445c03_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不信我者喻如海中求針。",
        "from": "T85n2903_001:1445c04_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "狂費功力無得見曰。",
        "from": "T85n2903_001:1445c04_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語諸菩薩。",
        "from": "T85n2903_001:1445c05_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今身盜他物者。",
        "from": "T85n2903_001:1445c05_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來生與他作癡牛令他苦打。",
        "from": "T85n2903_001:1445c05_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非時苦使受牛身。",
        "from": "T85n2903_001:1445c06_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以遙五百劫與他作奴。",
        "from": "T85n2903_001:1445c06_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "任他駈使心。",
        "from": "T85n2903_001:1445c07_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是逃避令被捉得苦刑。",
        "from": "T85n2903_001:1445c07_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印面非理苦持。",
        "from": "T85n2903_001:1445c08_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語菩薩。",
        "from": "T85n2903_001:1445c08_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伽藍中二種心。",
        "from": "T85n2903_001:1445c08_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者善心。",
        "from": "T85n2903_001:1445c09_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者惡心。",
        "from": "T85n2903_001:1445c09_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為惡人。",
        "from": "T85n2903_001:1445c09_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生入寺之時。",
        "from": "T85n2903_001:1445c09_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯從眾僧乞索。",
        "from": "T85n2903_001:1445c10_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或求僧長短或噉德食都無慚愧。",
        "from": "T85n2903_001:1445c10_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餅果菜茹懷狹歸家。",
        "from": "T85n2903_001:1445c11_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如此之死墮鐵叉地獄。",
        "from": "T85n2903_001:1445c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名為善人。",
        "from": "T85n2903_001:1445c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生入寺之時。",
        "from": "T85n2903_001:1445c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見僧恭敬。",
        "from": "T85n2903_001:1445c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見佛禮拜。",
        "from": "T85n2903_001:1445c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受戒懺悔。",
        "from": "T85n2903_001:1445c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨於財物。",
        "from": "T85n2903_001:1445c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經營三寶。",
        "from": "T85n2903_001:1445c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不惜身命。",
        "from": "T85n2903_001:1445c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護持大法。",
        "from": "T85n2903_001:1445c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如此之人舉足一步。",
        "from": "T85n2903_001:1445c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天堂自來受果。",
        "from": "T85n2903_001:1445c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如樹提伽。",
        "from": "T85n2903_001:1445c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提伽則名為最上善人也。",
        "from": "T85n2903_001:1445c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告大眾。",
        "from": "T85n2903_001:1445c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我向所論種種因果。",
        "from": "T85n2903_001:1445c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經一名殊別。",
        "from": "T85n2903_001:1445c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二名殊勝。",
        "from": "T85n2903_001:1445c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三名菩薩。",
        "from": "T85n2903_001:1445c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生聞此經者。",
        "from": "T85n2903_001:1445c18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一發善心得生淨土。",
        "from": "T85n2903_001:1445c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告菩薩。",
        "from": "T85n2903_001:1445c19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞我說者心生歎喜。",
        "from": "T85n2903_001:1445c19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如旱得水。",
        "from": "T85n2903_001:1445c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苗稼蘇活。",
        "from": "T85n2903_001:1445c20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不受我語者如石浸水。",
        "from": "T85n2903_001:1445c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有潤時。",
        "from": "T85n2903_001:1445c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時阿難白佛言。",
        "from": "T85n2903_001:1445c21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "T85n2903_001:1445c21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等見振旦國。",
        "from": "T85n2903_001:1445c21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有人從七歲修福至於百年。",
        "from": "T85n2903_001:1445c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨命終時破其五戒。",
        "from": "T85n2903_001:1445c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此人得福以不。",
        "from": "T85n2903_001:1445c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊復語阿難。",
        "from": "T85n2903_001:1445c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喻如轂車。",
        "from": "T85n2903_001:1445c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上萬里之坂。",
        "from": "T85n2903_001:1445c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨頭飜車速本。",
        "from": "T85n2903_001:1445c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所損何有得期。",
        "from": "T85n2903_001:1445c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縱取少多如雲影。",
        "from": "T85n2903_001:1445c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日斥時之光喻如一口之食。",
        "from": "T85n2903_001:1445c26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能得久飽。",
        "from": "T85n2903_001:1445c26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語眾生。",
        "from": "T85n2903_001:1445c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等廣說因緣共同。",
        "from": "T85n2903_001:1445c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成佛普勸眾生。",
        "from": "T85n2903_001:1445c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同修淨行。",
        "from": "T85n2903_001:1445c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切世間天人阿修羅等。",
        "from": "T85n2903_001:1445c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛所說。",
        "from": "T85n2903_001:1445c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆大歡喜作禮而去。",
        "from": "T85n2903_001:1445c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說無量大慈教經一卷",
        "from": "T85n2903_001:1446a01_0",
        "context_with_next_sentence": false
    }
]