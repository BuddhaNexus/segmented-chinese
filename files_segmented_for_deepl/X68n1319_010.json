[
    {
        "source_sentence": "御製序",
        "from": "X68n1319_010:0532b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頻呼小玉緣何事。",
        "from": "X68n1319_010:0532b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為要檀郎認得聲。",
        "from": "X68n1319_010:0532b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓悟勤禪師因此言下頓徹。",
        "from": "X68n1319_010:0532b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斯語也。",
        "from": "X68n1319_010:0532b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "綺語耶。",
        "from": "X68n1319_010:0532b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪語耶。",
        "from": "X68n1319_010:0532b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗毒鼓邊。",
        "from": "X68n1319_010:0532b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈容側耳。",
        "from": "X68n1319_010:0532b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但於綺語禪語一佇思擬議。",
        "from": "X68n1319_010:0532b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則劍去遠矣。",
        "from": "X68n1319_010:0532b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亙古亙今。",
        "from": "X68n1319_010:0532b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亙上亙下。",
        "from": "X68n1319_010:0532b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亙東亙西。",
        "from": "X68n1319_010:0532b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亙南亙北。",
        "from": "X68n1319_010:0532b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆是微塵。",
        "from": "X68n1319_010:0532b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一微塵中。",
        "from": "X68n1319_010:0532b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆建寶剎。",
        "from": "X68n1319_010:0532b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一寶剎中。",
        "from": "X68n1319_010:0532b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆作佛事。",
        "from": "X68n1319_010:0532b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡諸佛事。",
        "from": "X68n1319_010:0532b16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆具如來正法眼藏。",
        "from": "X68n1319_010:0532b16_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "涅槃妙心。",
        "from": "X68n1319_010:0532b17_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於何開口說得。",
        "from": "X68n1319_010:0532b17_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於何著手揀得。",
        "from": "X68n1319_010:0532b17_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開口說者。",
        "from": "X68n1319_010:0532b18_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是者個。",
        "from": "X68n1319_010:0532b18_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著手揀者。",
        "from": "X68n1319_010:0532b18_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦是者個。",
        "from": "X68n1319_010:0532b18_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈得以者個說者個。",
        "from": "X68n1319_010:0532b18_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以者個揀者個。",
        "from": "X68n1319_010:0532b19_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略一佇思擬議。",
        "from": "X68n1319_010:0532b19_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "這邊是地水火風。",
        "from": "X68n1319_010:0532b19_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鐵山岌岌。",
        "from": "X68n1319_010:0532b20_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那邊是見聞知覺。",
        "from": "X68n1319_010:0532b20_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玉海沈沈。",
        "from": "X68n1319_010:0532b20_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何由撒手懸厓。",
        "from": "X68n1319_010:0532b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡是開眼說夢。",
        "from": "X68n1319_010:0532b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以古德云。",
        "from": "X68n1319_010:0532b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欠一法。",
        "from": "X68n1319_010:0532b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不成法身。",
        "from": "X68n1319_010:0532b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若剩一法。",
        "from": "X68n1319_010:0532b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不成法身。",
        "from": "X68n1319_010:0532b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有一法。",
        "from": "X68n1319_010:0532b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不成法身。",
        "from": "X68n1319_010:0532b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無一法。",
        "from": "X68n1319_010:0532b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不成法身。",
        "from": "X68n1319_010:0532b23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此薦取。",
        "from": "X68n1319_010:0532b23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開口說亦可。",
        "from": "X68n1319_010:0532b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著手揀亦可。",
        "from": "X68n1319_010:0532b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如或未能。",
        "from": "X68n1319_010:0532b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可惜孤負圓悟平生許多絡索。",
        "from": "X68n1319_010:0532b24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即圓明主人一番選錄。",
        "from": "X68n1319_010:0532b24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦復鈍置不少。",
        "from": "X68n1319_010:0532c01_17",
        "context_with_next_sentence": false
    }
]