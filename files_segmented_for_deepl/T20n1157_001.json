[
    {
        "source_sentence": "香王菩薩陀羅尼呪經一卷",
        "from": "T20n1157_001:0651b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏法師沙門義淨譯",
        "from": "T20n1157_001:0651b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南謨曷喇怛娜怛喇夜(引)也(一)南謨阿離耶跋盧吉羝說囉(引)也(二)菩提薩埵(引)也(三)莫訶薩埵(引)也(四)南謨健陀(引)羅曷囉社(引)也(五)菩提薩埵(引)也(六)莫訶薩埵(引)也(七)伊只[打丁+致](八)彌只[打丁+致](九)漢底(丁里反十)曼底(十一)薩婆頞他(上十二)娑(引)彈儞(十三)迦利沙般拏(引十四)地鉢謎(引十五)鉢唎拽叱(十六)莎(引)訶(十七)此陀羅尼先。",
        "from": "T20n1157_001:0651b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須志心誦十萬遍。",
        "from": "T20n1157_001:0651b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然於觀世音像前。",
        "from": "T20n1157_001:0651b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作四肘方壇。",
        "from": "T20n1157_001:0651b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取花等名香供養。",
        "from": "T20n1157_001:0651b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜用淨牛蘇然燈。",
        "from": "T20n1157_001:0651b25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫令斷明。",
        "from": "T20n1157_001:0651b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其夜半呪師須起。",
        "from": "T20n1157_001:0651b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著淨潔衣應好莊束。",
        "from": "T20n1157_001:0651b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "像前誦此陀羅尼一千八遍。",
        "from": "T20n1157_001:0651b27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至曉須了不得惛睡。",
        "from": "T20n1157_001:0651b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其日呪師自有人送錢財等物。",
        "from": "T20n1157_001:0651b28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更若別求大事。",
        "from": "T20n1157_001:0651b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須畫香王菩薩於絹帛上其畫匠不得過中食。",
        "from": "T20n1157_001:0651b29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須受八齋戒。",
        "from": "T20n1157_001:0651c01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其畫像法任其大小。",
        "from": "T20n1157_001:0651c01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身肉皆白面貌端正頭戴天冠頸有瓔珞。",
        "from": "T20n1157_001:0651c02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右臂垂下五指皆申施無畏手。",
        "from": "T20n1157_001:0651c03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其五指端各雨甘露施於五道眾生。",
        "from": "T20n1157_001:0651c04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手下并畫黑鬼三五箇。",
        "from": "T20n1157_001:0651c04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左臂屈肘手當左嬭。",
        "from": "T20n1157_001:0651c05_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以把蓮花。",
        "from": "T20n1157_001:0651c05_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其花從脚下蓮華生出其花白紅色。",
        "from": "T20n1157_001:0651c06_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "足下蓮華亦白紅色。",
        "from": "T20n1157_001:0651c06_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "項背圓光。",
        "from": "T20n1157_001:0651c07_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上有傘蓋。",
        "from": "T20n1157_001:0651c07_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五色錦綺以為衣服。",
        "from": "T20n1157_001:0651c07_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩重珠條絡於髆上。",
        "from": "T20n1157_001:0651c08_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一赤色一黃色。",
        "from": "T20n1157_001:0651c08_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其像當日須成。",
        "from": "T20n1157_001:0651c09_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得用皮膠。",
        "from": "T20n1157_001:0651c09_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "像前燒香誦十萬遍一日一夜不食。",
        "from": "T20n1157_001:0651c09_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先須作一大水方壇訖。",
        "from": "T20n1157_001:0651c10_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然即以栴檀磨之。",
        "from": "T20n1157_001:0651c11_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作十二指小方壇八箇。",
        "from": "T20n1157_001:0651c11_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初以三壇。",
        "from": "T20n1157_001:0651c12_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一用供養佛。",
        "from": "T20n1157_001:0651c12_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一用供養法。",
        "from": "T20n1157_001:0651c12_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一用供養僧。",
        "from": "T20n1157_001:0651c13_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四壇供養阿彌陀佛。",
        "from": "T20n1157_001:0651c13_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五供養觀世音菩薩。",
        "from": "T20n1157_001:0651c13_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六供養大勢至菩薩。",
        "from": "T20n1157_001:0651c14_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第七供養香王菩薩。",
        "from": "T20n1157_001:0651c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八供養諸善神等。",
        "from": "T20n1157_001:0651c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆取好淨食果子香華。",
        "from": "T20n1157_001:0651c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四邊安置。",
        "from": "T20n1157_001:0651c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其法隨以何月十四十五日作。",
        "from": "T20n1157_001:0651c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然取闍帝花(此云荳蔻華)一千八莖。",
        "from": "T20n1157_001:0651c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦一遍擲像身胸上。",
        "from": "T20n1157_001:0651c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作法以後每夜半須起。",
        "from": "T20n1157_001:0651c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "像前常誦一千八遍。",
        "from": "T20n1157_001:0651c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至自有人送錢財。",
        "from": "T20n1157_001:0651c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其錢財亦不得積聚慳貪。",
        "from": "T20n1157_001:0651c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即須用度及施貧乏。",
        "from": "T20n1157_001:0651c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是香王菩薩法。",
        "from": "T20n1157_001:0651c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次有一別行要略法。",
        "from": "T20n1157_001:0651c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持呪之法。",
        "from": "T20n1157_001:0651c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者洗浴著新淨衣。",
        "from": "T20n1157_001:0651c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受八戒齋。",
        "from": "T20n1157_001:0651c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結淨一如壇法。",
        "from": "T20n1157_001:0651c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依次第而布供養散花燒香然燈。",
        "from": "T20n1157_001:0651c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從夜半起。",
        "from": "T20n1157_001:0651c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在壇內端坐。",
        "from": "T20n1157_001:0651c25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦呪一千八遍須瞻蔔花一千八莖。",
        "from": "T20n1157_001:0651c25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦呪一遍擲一莖花。",
        "from": "T20n1157_001:0651c26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著菩薩身上。",
        "from": "T20n1157_001:0651c26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當誦呪之時。",
        "from": "T20n1157_001:0651c27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不用聞鳥鳴聲。",
        "from": "T20n1157_001:0651c27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用三白食解齋。",
        "from": "T20n1157_001:0651c27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒安息香供養。",
        "from": "T20n1157_001:0651c28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日別濾水香湯洗浴。",
        "from": "T20n1157_001:0651c28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別著衣裳。",
        "from": "T20n1157_001:0651c29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大小行來皆須洗淨。",
        "from": "T20n1157_001:0651c29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平旦夜半入壇之時並須嚼楊枝。",
        "from": "T20n1157_001:0651c29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後入壇一心誦呪。",
        "from": "T20n1157_001:0652a01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫思量外事即見神驗光明。",
        "from": "T20n1157_001:0652a02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見驗即休。",
        "from": "T20n1157_001:0652a02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不見光亦有動相。",
        "from": "T20n1157_001:0652a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所行之處大獲果報。",
        "from": "T20n1157_001:0652a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持呪之法不得破戒。",
        "from": "T20n1157_001:0652a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身亦不得近觸著女人觸著女人者即無驗。",
        "from": "T20n1157_001:0652a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香王菩薩陀羅尼呪經",
        "from": "T20n1157_001:0652a06_0",
        "context_with_next_sentence": false
    }
]