[
    {
        "source_sentence": "僧伽和尚欲入涅槃說六度經",
        "from": "T85n2920_001:1463b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾告於閻浮提中善男子善女人。",
        "from": "T85n2920_001:1463b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾自生閻浮。",
        "from": "T85n2920_001:1463b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為大慈父教化眾生。",
        "from": "T85n2920_001:1463b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輪迴世間。",
        "from": "T85n2920_001:1463b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經今無始曠劫分身萬億。",
        "from": "T85n2920_001:1463b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救度眾生。",
        "from": "T85n2920_001:1463c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為見閻浮提眾生多造惡業。",
        "from": "T85n2920_001:1463c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不信佛法。",
        "from": "T85n2920_001:1463c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡業者多。",
        "from": "T85n2920_001:1463c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾不忍見。",
        "from": "T85n2920_001:1463c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾身便入涅槃。",
        "from": "T85n2920_001:1463c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利形像遍於閻浮。",
        "from": "T85n2920_001:1463c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "引化眾生。",
        "from": "T85n2920_001:1463c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以後像法世界滿正法興時。",
        "from": "T85n2920_001:1463c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾與彌勒尊佛同時下生。",
        "from": "T85n2920_001:1463c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共坐化城。",
        "from": "T85n2920_001:1463c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救度善緣。",
        "from": "T85n2920_001:1463c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元居本宅。",
        "from": "T85n2920_001:1463c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在於東海。",
        "from": "T85n2920_001:1463c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是過去先世淨土緣。",
        "from": "T85n2920_001:1463c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為眾生頑愚難化。",
        "from": "T85n2920_001:1463c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不信佛法。",
        "from": "T85n2920_001:1463c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多造惡業。",
        "from": "T85n2920_001:1463c07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾離本處身至西方教化眾生。",
        "from": "T85n2920_001:1463c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號為釋迦牟尼佛。",
        "from": "T85n2920_001:1463c08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東國遂被五百毒龍陷為大海。",
        "from": "T85n2920_001:1463c08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生沈在海中。",
        "from": "T85n2920_001:1463c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化為黿鼉魚鼈。",
        "from": "T85n2920_001:1463c09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾身已後却從西方胡國中來生於閻浮。",
        "from": "T85n2920_001:1463c10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救度善緣。",
        "from": "T85n2920_001:1463c11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛性種子。",
        "from": "T85n2920_001:1463c11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾見閻浮眾生。",
        "from": "T85n2920_001:1463c11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍境凶惡。",
        "from": "T85n2920_001:1463c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自相吞食。",
        "from": "T85n2920_001:1463c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可開化。",
        "from": "T85n2920_001:1463c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾今遂入涅槃。",
        "from": "T85n2920_001:1463c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利本骨願住泗州已後。",
        "from": "T85n2920_001:1463c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人。",
        "from": "T85n2920_001:1463c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈心孝順。",
        "from": "T85n2920_001:1463c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬吾形像長齋菜食。",
        "from": "T85n2920_001:1463c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念吾名字。",
        "from": "T85n2920_001:1463c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是之人散在閻浮。",
        "from": "T85n2920_001:1463c15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾愍見惡世力兵競起。",
        "from": "T85n2920_001:1463c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸惡逼身不得自在。",
        "from": "T85n2920_001:1463c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾後與彌勒尊佛下生本國。",
        "from": "T85n2920_001:1463c16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "足踏海水枯竭。",
        "from": "T85n2920_001:1463c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂使諸天龍神八部聖眾在於東海中心。",
        "from": "T85n2920_001:1463c17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修造化城。",
        "from": "T85n2920_001:1463c18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金銀為壁。",
        "from": "T85n2920_001:1463c18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "琉璃為地。",
        "from": "T85n2920_001:1463c19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七寶為殿。",
        "from": "T85n2920_001:1463c19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾後至閻浮。",
        "from": "T85n2920_001:1463c19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與流佛法。",
        "from": "T85n2920_001:1463c19_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯傳此經。",
        "from": "T85n2920_001:1463c20_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教化善緣。",
        "from": "T85n2920_001:1463c20_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六度弟子歸我。",
        "from": "T85n2920_001:1463c20_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化城免在閻浮。",
        "from": "T85n2920_001:1463c20_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受其苦難悉得安穩。",
        "from": "T85n2920_001:1463c21_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣食自然。",
        "from": "T85n2920_001:1463c21_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長受極樂。",
        "from": "T85n2920_001:1463c21_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天魔外道弱水隔之。",
        "from": "T85n2920_001:1463c22_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不來為害。",
        "from": "T85n2920_001:1463c22_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾當度六種之人　第一度者。",
        "from": "T85n2920_001:1463c22_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孝順父母敬重三寶　第二度者。",
        "from": "T85n2920_001:1463c23_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不殺眾生　第三度者。",
        "from": "T85n2920_001:1463c24_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不飲酒食肉　第四度者。",
        "from": "T85n2920_001:1463c24_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平等好心不為偷盜　第五度者。",
        "from": "T85n2920_001:1463c25_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭陀苦行。",
        "from": "T85n2920_001:1463c26_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "好修橋梁并諸功德　第六度者。",
        "from": "T85n2920_001:1463c26_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憐貧念病。",
        "from": "T85n2920_001:1463c27_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "布施衣食。",
        "from": "T85n2920_001:1463c27_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "極濟窮無。",
        "from": "T85n2920_001:1463c27_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如此善道六度之人。",
        "from": "T85n2920_001:1463c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾先使百童子領上寶船載。",
        "from": "T85n2920_001:1463c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過弱水免使沈溺得入化城。",
        "from": "T85n2920_001:1463c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不是吾六度之人。",
        "from": "T85n2920_001:1463c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見吾此經心不信受毀謗正法。",
        "from": "T85n2920_001:1464a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知此人宿世罪根身受惡報。",
        "from": "T85n2920_001:1464a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或逢盜賊兵瘴而死。",
        "from": "T85n2920_001:1464a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或被水火焚漂。",
        "from": "T85n2920_001:1464a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或被時行惡病遭官落獄。",
        "from": "T85n2920_001:1464a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不善眾生皆受無量苦惱。",
        "from": "T85n2920_001:1464a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "死入地獄。",
        "from": "T85n2920_001:1464a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有出期。",
        "from": "T85n2920_001:1464a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬劫不復人道。",
        "from": "T85n2920_001:1464a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子善女人。",
        "from": "T85n2920_001:1464a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書寫此經。",
        "from": "T85n2920_001:1464a05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "志意受持。",
        "from": "T85n2920_001:1464a06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若逢劫水劫火黑風天暗。",
        "from": "T85n2920_001:1464a06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾故無量光明照汝。",
        "from": "T85n2920_001:1464a06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因緣俱來佛國。",
        "from": "T85n2920_001:1464a07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同歸化城。",
        "from": "T85n2920_001:1464a07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉得解脫。",
        "from": "T85n2920_001:1464a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無僧伽　南無僧禁吒　莎訶達多姪他耶唵　跋勒攝　娑婆訶僧伽和尚經",
        "from": "T85n2920_001:1464a08_1",
        "context_with_next_sentence": false
    }
]