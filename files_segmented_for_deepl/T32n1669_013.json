[
    {
        "source_sentence": "大宗地玄文本論卷第十三",
        "from": "T32n1669_013:0686a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬鳴菩薩造",
        "from": "T32n1669_013:0686a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真諦三藏譯",
        "from": "T32n1669_013:0686a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩訶本地具足品藏非患道路大決擇分第二十五如是已說一種功德摩訶本地明白離惡品藏大決擇分。",
        "from": "T32n1669_013:0686a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說摩訶本地具足品藏非患道路大決擇分。",
        "from": "T32n1669_013:0686a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_013:0686a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_013:0686a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　本地道路中　　則有二種門",
        "from": "T32n1669_013:0686a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　謂橫轉竪轉　　以之為門量",
        "from": "T32n1669_013:0686a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_013:0686a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就摩訶本地具足品藏非患道路分中。",
        "from": "T32n1669_013:0686a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則有二門。",
        "from": "T32n1669_013:0686a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為二。",
        "from": "T32n1669_013:0686a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者橫轉遍到俱行門。",
        "from": "T32n1669_013:0686a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者竪轉無雜一路門。",
        "from": "T32n1669_013:0686a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為二。",
        "from": "T32n1669_013:0686a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是二門以為門量。",
        "from": "T32n1669_013:0686a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈本地道路中則有二種門。",
        "from": "T32n1669_013:0686a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂橫轉竪轉以之為門量故。",
        "from": "T32n1669_013:0686a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且橫轉門形相如何。",
        "from": "T32n1669_013:0686a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_013:0686a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　四種事位中　　有總及別中",
        "from": "T32n1669_013:0686a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　各增十重轉　　無前後一時",
        "from": "T32n1669_013:0686a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　其法門數量　　例前應了知",
        "from": "T32n1669_013:0686a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_013:0686a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此偈中為明何義。",
        "from": "T32n1669_013:0686a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲現示性相本末四種事中。",
        "from": "T32n1669_013:0686a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各各有總別之位中。",
        "from": "T32n1669_013:0686a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一位位皆悉各各增十重數。",
        "from": "T32n1669_013:0686a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無前無後一時俱轉。",
        "from": "T32n1669_013:0686a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則是橫轉遍到俱行門形相故。",
        "from": "T32n1669_013:0686a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈四種事位中有總及別中各增十重轉無前後一時故。",
        "from": "T32n1669_013:0686a28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中數量亦復轉勝超過前量。",
        "from": "T32n1669_013:0686b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "配例應了。",
        "from": "T32n1669_013:0686b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈其法門數量例前應了知故。",
        "from": "T32n1669_013:0686b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是已說橫轉遍到俱行門。",
        "from": "T32n1669_013:0686b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說竪轉無雜一路門。",
        "from": "T32n1669_013:0686b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_013:0686b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_013:0686b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　前說諸位中　　如次不超過",
        "from": "T32n1669_013:0686b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　各增十重轉　　一明究竟故",
        "from": "T32n1669_013:0686b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_013:0686b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此偈中為明何義。",
        "from": "T32n1669_013:0686b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲現示如前所說總別位中。",
        "from": "T32n1669_013:0686b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如其次第無有超過。",
        "from": "T32n1669_013:0686b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一各各增十重轉。",
        "from": "T32n1669_013:0686b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一事明了一事究竟。",
        "from": "T32n1669_013:0686b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦無雜亂亦無合集。",
        "from": "T32n1669_013:0686b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一向明轉竪轉無雜一路門形相故。",
        "from": "T32n1669_013:0686b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈前說諸位中如次不超過各增十重轉一明究竟故。",
        "from": "T32n1669_013:0686b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故大金剛寶王法界印藏修多羅中。",
        "from": "T32n1669_013:0686b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說。",
        "from": "T32n1669_013:0686b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次文殊師利。",
        "from": "T32n1669_013:0686b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶王道品者。",
        "from": "T32n1669_013:0686b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二事轉。",
        "from": "T32n1669_013:0686b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為二。",
        "from": "T32n1669_013:0686b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者一區轉。",
        "from": "T32n1669_013:0686b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者具面轉。",
        "from": "T32n1669_013:0686b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言一區者。",
        "from": "T32n1669_013:0686b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道雖廣多先一道量。",
        "from": "T32n1669_013:0686b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永究竟故。",
        "from": "T32n1669_013:0686b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言具面者。",
        "from": "T32n1669_013:0686b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有諸道一時行故。",
        "from": "T32n1669_013:0686b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至廣說故。",
        "from": "T32n1669_013:0686b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩訶寶輪王廣大圓滿無上地地大決擇分第二十六如是已說摩訶本地具足品藏非患道路大決擇分。",
        "from": "T32n1669_013:0686b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說摩訶寶輪王廣大圓滿無上地地大決擇分。",
        "from": "T32n1669_013:0686b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_013:0686b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_013:0686b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　總別無盡故　　建立本法體",
        "from": "T32n1669_013:0686b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_013:0686b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此偈中為明何義。",
        "from": "T32n1669_013:0686b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲現示本法體中以總攝別以別攝總。",
        "from": "T32n1669_013:0686b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以總攝總以別攝別。",
        "from": "T32n1669_013:0686b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能攝所攝無有窮盡。",
        "from": "T32n1669_013:0686b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法門大海甚深廣大。",
        "from": "T32n1669_013:0686b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義理詮趣周遍圓滿。",
        "from": "T32n1669_013:0686b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "究竟自在故。",
        "from": "T32n1669_013:0686b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈總別無盡故建立本法體故。",
        "from": "T32n1669_013:0686b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩訶衍地藏無上極說不可思議心地品論修多羅中。",
        "from": "T32n1669_013:0686b28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說。",
        "from": "T32n1669_013:0686b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶山海中同類無盡別類無盡具足圓滿。",
        "from": "T32n1669_013:0686b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有窮盡無有始終。",
        "from": "T32n1669_013:0686c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有邊際無有分界。",
        "from": "T32n1669_013:0686c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦廣大相亦小狹相。",
        "from": "T32n1669_013:0686c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至廣說故。",
        "from": "T32n1669_013:0686c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大宗地玄文本論卷第十三",
        "from": "T32n1669_013:0686c04_0",
        "context_with_next_sentence": false
    }
]