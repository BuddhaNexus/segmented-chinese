    },
    {
        "source_sentence": "青面北天陀羅尼法一卷",
        "from": "T55n2160_001:1059b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "般若心經略疏一卷",
        "from": "T55n2160_001:1059b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "達磨系圖一卷",
        "from": "T55n2160_001:1059b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛受苦決義一卷(十五紙)",
        "from": "T55n2160_001:1059b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法性章一卷(加青辨量判斷一卷十紙)二定二戒體一卷四分律鈔數義一卷(二十紙)四分律鈔音訓一卷(十五紙)道場頂一卷(闕)已上三十四部三十四卷同帙(雜第三六十紙)。",
        "from": "T55n2160_001:1059b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總日本國求法目錄一卷",
        "from": "T55n2160_001:1059b30_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相送集四卷(甲乙丙丁戊己庚　百紙)已上二部五卷同帙。",
        "from": "T55n2160_001:1059c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦供養具樣",
        "from": "T55n2160_001:1059c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五鈷拔折羅樣一口",
        "from": "T55n2160_001:1059c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五鈷金剛鈴樣一口",
        "from": "T55n2160_001:1059c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛輪二口",
        "from": "T55n2160_001:1059c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛羯磨二口",
        "from": "T55n2160_001:1059c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言和上付法印信三鈷拔折羅一口",
        "from": "T55n2160_001:1059c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已上念誦供養具樣也。",
        "from": "T55n2160_001:1059c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右件念誦法門等并念誦供養具樣等。",
        "from": "T55n2160_001:1059c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向越府龍興寺。",
        "from": "T55n2160_001:1059c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詣順曉和上所。",
        "from": "T55n2160_001:1059c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即最澄并義真。",
        "from": "T55n2160_001:1059c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逐和上到湖鏡東峯山道場。",
        "from": "T55n2160_001:1059c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和上導兩僧治道場。",
        "from": "T55n2160_001:1059c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "引入五部灌頂曼荼羅壇場。",
        "from": "T55n2160_001:1059c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現蒙授真言法。",
        "from": "T55n2160_001:1059c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又灌頂真言水。",
        "from": "T55n2160_001:1059c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便寫取上件念誦法門并供養具樣。",
        "from": "T55n2160_001:1059c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勘定已畢。",
        "from": "T55n2160_001:1059c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最澄等。",
        "from": "T55n2160_001:1059c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深蒙郎中慈造。",
        "from": "T55n2160_001:1059c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "去年向台州。",
        "from": "T55n2160_001:1059c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩僧等。",
        "from": "T55n2160_001:1059c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受大小二乘戒。",
        "from": "T55n2160_001:1059c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又寫取數百卷文書。",
        "from": "T55n2160_001:1059c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今年進越府。",
        "from": "T55n2160_001:1059c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二僧入五部灌頂壇。",
        "from": "T55n2160_001:1059c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又抄取念誦法門。",
        "from": "T55n2160_001:1059c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後都總二百三十部。",
        "from": "T55n2160_001:1059c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四百六十卷也。",
        "from": "T55n2160_001:1059c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能事已畢。",
        "from": "T55n2160_001:1059c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今歸本鄉今欲請當州印信。",
        "from": "T55n2160_001:1059c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外方學徒等。",
        "from": "T55n2160_001:1059c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將示求法元由矣。",
        "from": "T55n2160_001:1059c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然則郎中傳法之功。",
        "from": "T55n2160_001:1059c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攀福於現當。",
        "from": "T55n2160_001:1059c21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "群生聽法之德。",
        "from": "T55n2160_001:1059c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "期果於妙覺。",
        "from": "T55n2160_001:1059c22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願使君。",
        "from": "T55n2160_001:1059c22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "近登三台位。",
        "from": "T55n2160_001:1059c22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠證三點果。",
        "from": "T55n2160_001:1059c22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後。",
        "from": "T55n2160_001:1059c23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪通三界。",
        "from": "T55n2160_001:1059c23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "橫撥十方。",
        "from": "T55n2160_001:1059c23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六道四生一切含靈。",
        "from": "T55n2160_001:1059c23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同入禪門。",
        "from": "T55n2160_001:1059c24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱遊慧苑。",
        "from": "T55n2160_001:1059c24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信謗平等。",
        "from": "T55n2160_001:1059c24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自他得益歟。",
        "from": "T55n2160_001:1059c24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大唐貞元貳拾壹年歲次乙酉五月朔己巳拾參日辛巳日本國求法僧　最澄錄日本國求法譯語僧　義真日本國求法傔從丹　福成孔夫子云。",
        "from": "T55n2160_001:1059c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾聞。",
        "from": "T55n2160_001:1060a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方有聖人焉。",
        "from": "T55n2160_001:1060a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其教以清淨無為為本。",
        "from": "T55n2160_001:1060a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不染不著為妙。",
        "from": "T55n2160_001:1060a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其化人也。",
        "from": "T55n2160_001:1060a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足功德。",
        "from": "T55n2160_001:1060a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃為圓明。",
        "from": "T55n2160_001:1060a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最澄闍梨。",
        "from": "T55n2160_001:1060a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性稟生知之才。",
        "from": "T55n2160_001:1060a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來自禮義之國。",
        "from": "T55n2160_001:1060a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬里求法。",
        "from": "T55n2160_001:1060a05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "視險若夷。",
        "from": "T55n2160_001:1060a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不憚艱勞。",
        "from": "T55n2160_001:1060a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神力保護。",
        "from": "T55n2160_001:1060a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南登天台之巔。",
        "from": "T55n2160_001:1060a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西泛鏡湖之水。",
        "from": "T55n2160_001:1060a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "窮智者之法門。",
        "from": "T55n2160_001:1060a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "探灌頂之神祕。",
        "from": "T55n2160_001:1060a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可謂法門龍象。",
        "from": "T55n2160_001:1060a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "青蓮出池。",
        "from": "T55n2160_001:1060a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將此大乘往傳本國。",
        "from": "T55n2160_001:1060a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求茲印信執以為憑。",
        "from": "T55n2160_001:1060a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昨者陸台州已與題記。",
        "from": "T55n2160_001:1060a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故具所覩爰申直筆。",
        "from": "T55n2160_001:1060a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大唐貞元二十一年五月十五日。",
        "from": "T55n2160_001:1060a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朝議郎使持節明州諸軍事守明州刺史上柱國滎陽鄭審則書。",
        "from": "T55n2160_001:1060a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日本國入唐使。",
        "from": "T55n2160_001:1060a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持節大使從四位上行太政官右大",
        "from": "T55n2160_001:1060a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辨兼越前守　　　藤原朝臣　葛野麿準判官兼譯語正六位上行備前掾笠　　臣　　田作錄事正六位上行式部省少錄兼伊勢大目勳六等　　山田造　大庭錄事正六位上行太政官左少史兼常陸少目　　　　上毛野公　　穎人山家將來目錄序昔者吾三聖之入唐求法也。",
        "from": "T55n2160_001:1060a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆有將來錄。",
        "from": "T55n2160_001:1060a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先是刊行於世矣。",
        "from": "T55n2160_001:1060a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今復殊壽此摹錄者。",
        "from": "T55n2160_001:1060a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓋使其真蹟不朽也。",
        "from": "T55n2160_001:1060a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫國朝文物之盛。",
        "from": "T55n2160_001:1060a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云緇云素。",
        "from": "T55n2160_001:1060a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫太於延曆弘仁聖代。",
        "from": "T55n2160_001:1060a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至若其書法精眇。",
        "from": "T55n2160_001:1060a27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則晚唐以還聲書之士實所不能及。",
        "from": "T55n2160_001:1060a27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人僉知焉。",
        "from": "T55n2160_001:1060a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宋人讚曰幾二王亦非誣。",
        "from": "T55n2160_001:1060a28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當時尚然。",
        "from": "T55n2160_001:1060a29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "況今世存數千字者。",
        "from": "T55n2160_001:1060a29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可謂希世巨寶也。",
        "from": "T55n2160_001:1060b01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖然毖之臧中。",
        "from": "T55n2160_001:1060b01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則非但人知稀。",
        "from": "T55n2160_001:1060b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或恐不能全璧於後世。",
        "from": "T55n2160_001:1060b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈可不憾乎。",
        "from": "T55n2160_001:1060b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今茲辛巳之夏。",
        "from": "T55n2160_001:1060b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法曼超公齎郭填之本徠曰。",
        "from": "T55n2160_001:1060b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大師入滅壹千年。",
        "from": "T55n2160_001:1060b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於今真蹟儼可見矣。",
        "from": "T55n2160_001:1060b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今刻之以置開□塔院。",
        "from": "T55n2160_001:1060b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則猶傳永世可不盛觀哉。",
        "from": "T55n2160_001:1060b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於茲吾儕彈指隨憙而諾矣。",
        "from": "T55n2160_001:1060b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頃日剞劂告功成。",
        "from": "T55n2160_001:1060b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同法切責題一言。",
        "from": "T55n2160_001:1060b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "予固辭不許。",
        "from": "T55n2160_001:1060b07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故不顧唯質而不文書其始末云。",
        "from": "T55n2160_001:1060b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文政辛巳僧自恣日。",
        "from": "T55n2160_001:1060b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "延曆寺遮那業苾芻亮照撰併書。",
        "from": "T55n2160_001:1060b09_1",
        "context_with_next_sentence": false
    }
]