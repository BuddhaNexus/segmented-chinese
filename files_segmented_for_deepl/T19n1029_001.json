[
    {
        "source_sentence": "佛說安宅陀羅尼呪經",
        "from": "T19n1029_001:0744a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T19n1029_001:0744a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在舍衛國祇陀林中給孤精舍。",
        "from": "T19n1029_001:0744a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大比丘眾千二百五十人大菩薩眾四萬人俱。",
        "from": "T19n1029_001:0744a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娑婆世界主梵天王。",
        "from": "T19n1029_001:0744a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋提桓因四大天王。",
        "from": "T19n1029_001:0744a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提頭賴吒天王毘樓勒[蘜匊+奇]天王。",
        "from": "T19n1029_001:0744a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘樓博叉天王。",
        "from": "T19n1029_001:0744a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘沙門天王。",
        "from": "T19n1029_001:0744a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將二十八部鬼師大將軍。",
        "from": "T19n1029_001:0744a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等諸天八萬四千阿利多。",
        "from": "T19n1029_001:0744a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將其子及眷屬悉來在會。",
        "from": "T19n1029_001:0744a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時去此佛世界。",
        "from": "T19n1029_001:0744a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過十萬億佛土。",
        "from": "T19n1029_001:0744a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有世界名眾花。",
        "from": "T19n1029_001:0744a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛號最勝燈王如來應供等正覺。",
        "from": "T19n1029_001:0744a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今現在說法。",
        "from": "T19n1029_001:0744a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遣二菩薩一名大光二名無量光。",
        "from": "T19n1029_001:0744a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而告之言。",
        "from": "T19n1029_001:0744a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子汝等持此陀羅尼呪。",
        "from": "T19n1029_001:0744a28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至娑婆世界與釋迦牟尼佛。",
        "from": "T19n1029_001:0744a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此呪多所饒益。",
        "from": "T19n1029_001:0744a29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令眾生長夜安隱。",
        "from": "T19n1029_001:0744a29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲大善利色力名譽。",
        "from": "T19n1029_001:0744b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T19n1029_001:0744b01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多跌他　波羅殊隷　殊隷　殊隷隷沙摩鞮摩訶娑摩鞮　娑慢帝　摩訶娑慢帝　娑隷娑羅隷　莎呵時二菩薩從佛。",
        "from": "T19n1029_001:0744b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持陀羅尼呪。",
        "from": "T19n1029_001:0744b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如壯夫屈申臂項。",
        "from": "T19n1029_001:0744b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾花國沒到舍衛國祇陀林中給孤獨精舍。",
        "from": "T19n1029_001:0744b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時二菩薩前詣佛所。",
        "from": "T19n1029_001:0744b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭面禮足却住一面。",
        "from": "T19n1029_001:0744b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而白佛言。",
        "from": "T19n1029_001:0744b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊去此世界過十萬億佛土。",
        "from": "T19n1029_001:0744b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有世界名眾花。",
        "from": "T19n1029_001:0744b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛號最勝燈王如來應供等正覺。",
        "from": "T19n1029_001:0744b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今現在說法。",
        "from": "T19n1029_001:0744b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遣我等來問訊世尊。",
        "from": "T19n1029_001:0744b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少病少惱氣力安不。",
        "from": "T19n1029_001:0744b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸弟子等。",
        "from": "T19n1029_001:0744b11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為若魔若天若鬼若羅剎。",
        "from": "T19n1029_001:0744b11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若部多若鳩槃茶若富單那若毘舍遮。",
        "from": "T19n1029_001:0744b12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若餓鬼若阿修羅若迦樓羅若干人鬼。",
        "from": "T19n1029_001:0744b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若乾陀鬼疫病鬼。",
        "from": "T19n1029_001:0744b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若使人狂鬼若亂人心鬼。",
        "from": "T19n1029_001:0744b14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若方道鬼若起尸鬼。",
        "from": "T19n1029_001:0744b15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一日發病鬼。",
        "from": "T19n1029_001:0744b15_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若二日若三日若四日乃至七日發病鬼。",
        "from": "T19n1029_001:0744b15_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若常發病鬼。",
        "from": "T19n1029_001:0744b16_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等諸橫之所惱者。",
        "from": "T19n1029_001:0744b17_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最勝燈王如來遣陀羅尼呪。",
        "from": "T19n1029_001:0744b17_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來與世尊。",
        "from": "T19n1029_001:0744b18_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦欲令此娑婆世界眾生長夜安隱。",
        "from": "T19n1029_001:0744b18_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲大善利色力名譽。",
        "from": "T19n1029_001:0744b19_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說呪如上。",
        "from": "T19n1029_001:0744b19_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛告阿難。",
        "from": "T19n1029_001:0744b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當受持讀誦通利。",
        "from": "T19n1029_001:0744b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為他解說書寫供養此陀羅尼呪。",
        "from": "T19n1029_001:0744b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何佛出世難。",
        "from": "T19n1029_001:0744b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持是呪亦復甚難。",
        "from": "T19n1029_001:0744b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難若有受持讀誦通利。",
        "from": "T19n1029_001:0744b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得大福利。",
        "from": "T19n1029_001:0744b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說摩尼羅亶經",
        "from": "T19n1029_001:0744b25_0",
        "context_with_next_sentence": false
    }
]