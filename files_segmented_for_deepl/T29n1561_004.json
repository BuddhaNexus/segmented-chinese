[
    {
        "source_sentence": "阿毘達磨俱舍論實義疏卷第四",
        "from": "T29n1561_004:0326c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊者安惠造",
        "from": "T29n1561_004:0326c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一剎那頌曰。",
        "from": "T29n1561_004:0326c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　內五有熟養　　聲無異熟生",
        "from": "T29n1561_004:0326c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　八無礙等流　　亦異熟生性",
        "from": "T29n1561_004:0326c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　餘三實唯法　　剎那唯後三",
        "from": "T29n1561_004:0327a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內五即是眼之五界。",
        "from": "T29n1561_004:0327a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十八界中幾是見幾非見頌曰。",
        "from": "T29n1561_004:0327a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　眼法界一分　　八種說名見",
        "from": "T29n1561_004:0327a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　五識俱生惠　　非見不度故",
        "from": "T29n1561_004:0327a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　眼見色同分　　非彼能依識",
        "from": "T29n1561_004:0327a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　傳說不能觀　　被障諸色故",
        "from": "T29n1561_004:0327a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T29n1561_004:0327a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼全是見故。",
        "from": "T29n1561_004:0327a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法界一分八種是見。",
        "from": "T29n1561_004:0327a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘皆非見。",
        "from": "T29n1561_004:0327a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論今當略辯。",
        "from": "T29n1561_004:0327a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此決定相。",
        "from": "T29n1561_004:0327a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌曰。",
        "from": "T29n1561_004:0327a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　眼不下於身　　色識非上眼",
        "from": "T29n1561_004:0327a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　色於識一切　　二於身亦然",
        "from": "T29n1561_004:0327a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如眼耳亦然　　次三皆自地",
        "from": "T29n1561_004:0327a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　身識自下地　　意不定應知",
        "from": "T29n1561_004:0327a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論又經中說二十二根。",
        "from": "T29n1561_004:0327a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂眼根．耳根．鼻根．舌根．身根．意根．女根．男根．命根．樂根．苦根．喜根．憂根．捨根．信根．勤根．念根．定根．惠根．未知當知根．已知根．具知根。",
        "from": "T29n1561_004:0327a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋曰。",
        "from": "T29n1561_004:0327a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是何經所說。",
        "from": "T29n1561_004:0327a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂有梵志名曰生測。",
        "from": "T29n1561_004:0327a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來詣佛所。",
        "from": "T29n1561_004:0327a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡喜問訊。",
        "from": "T29n1561_004:0327a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在一面坐。",
        "from": "T29n1561_004:0327a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而白佛言。",
        "from": "T29n1561_004:0327a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施設幾根攝諸根盡。",
        "from": "T29n1561_004:0327a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言。",
        "from": "T29n1561_004:0327a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我說二十二根攝諸根盡。",
        "from": "T29n1561_004:0327a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有遮此更說餘根。",
        "from": "T29n1561_004:0327a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知彼說有言無義。",
        "from": "T29n1561_004:0327a21_11",
        "context_with_next_sentence": false
    }
]