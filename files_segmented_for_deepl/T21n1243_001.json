[
    {
        "source_sentence": "佛說出生一切如來法眼遍照大",
        "from": "T21n1243_001:0207c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "力明王經卷上",
        "from": "T21n1243_001:0207c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天中印度摩伽陀國那爛陀寺",
        "from": "T21n1243_001:0207c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏賜紫沙門臣法護奉　詔譯",
        "from": "T21n1243_001:0207c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T21n1243_001:0207c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時世尊。",
        "from": "T21n1243_001:0207c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在摩訶母質隣那山。",
        "from": "T21n1243_001:0207c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於大寶樓閣中為眾說法。",
        "from": "T21n1243_001:0207c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時一切諸佛稱揚讚歎。",
        "from": "T21n1243_001:0207c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛手菩薩摩訶薩。",
        "from": "T21n1243_001:0207c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是彼眾俱來會坐。",
        "from": "T21n1243_001:0207c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼於世尊普遍焰鬘蓮華座最初東邊而坐。",
        "from": "T21n1243_001:0207c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛以右手安慰眾生。",
        "from": "T21n1243_001:0207c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次佛右邊四臂大力明王。",
        "from": "T21n1243_001:0207c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手向佛頂禮。",
        "from": "T21n1243_001:0207c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手執拂左上手執金剛索。",
        "from": "T21n1243_001:0207c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右上手持金剛棒。",
        "from": "T21n1243_001:0207c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼眼如朱髮如熾火如焰上聳。",
        "from": "T21n1243_001:0207c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次金剛手并諸眷屬。",
        "from": "T21n1243_001:0207c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次右邊降三世明王。",
        "from": "T21n1243_001:0207c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左邊甘露軍拏利。",
        "from": "T21n1243_001:0207c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形如半月奮迅威猛。",
        "from": "T21n1243_001:0207c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形容瘮惡赤色如朱。",
        "from": "T21n1243_001:0207c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大恐怖金剛。",
        "from": "T21n1243_001:0207c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能破一切煩惱堅執本心。",
        "from": "T21n1243_001:0207c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次聖降三世明王。",
        "from": "T21n1243_001:0207c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖摩麼計。",
        "from": "T21n1243_001:0207c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甘露軍拏利。",
        "from": "T21n1243_001:0207c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛鉤金剛索。",
        "from": "T21n1243_001:0207c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其門裏復有二忿怒金剛。",
        "from": "T21n1243_001:0207c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左持金剛鎚。",
        "from": "T21n1243_001:0207c20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右執金剛棒。",
        "from": "T21n1243_001:0207c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次佛左邊觀自在菩薩并諸眷屬。",
        "from": "T21n1243_001:0207c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次佛右邊聖白衣觀自在菩薩。",
        "from": "T21n1243_001:0207c21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及多羅菩薩。",
        "from": "T21n1243_001:0207c22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘俱胝菩薩。",
        "from": "T21n1243_001:0207c22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左邊馬頭明王。",
        "from": "T21n1243_001:0207c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次摩賀大白。",
        "from": "T21n1243_001:0207c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大吉祥菩薩如是等百千菩薩摩訶薩。",
        "from": "T21n1243_001:0207c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復東方日月天等。",
        "from": "T21n1243_001:0207c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提頭賴吒天王。",
        "from": "T21n1243_001:0207c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝釋天主。",
        "from": "T21n1243_001:0207c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東南方焰摩及火天并諸大仙。",
        "from": "T21n1243_001:0207c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南方閻謨那羅延。",
        "from": "T21n1243_001:0207c26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西南方羅叉主。",
        "from": "T21n1243_001:0207c26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方水天龍主。",
        "from": "T21n1243_001:0207c27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西北方風天。",
        "from": "T21n1243_001:0207c27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方俱尾羅。",
        "from": "T21n1243_001:0207c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東北方伊捨曩。",
        "from": "T21n1243_001:0207c28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝釋天主。",
        "from": "T21n1243_001:0207c28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那羅延天。",
        "from": "T21n1243_001:0207c28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娑訶世主大梵天王。",
        "from": "T21n1243_001:0207c28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魔醯首羅諸天子眾。",
        "from": "T21n1243_001:0207c29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等無量無數清淨諸天及諸天人。",
        "from": "T21n1243_001:0207c29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各各以自眷屬圍遶。",
        "from": "T21n1243_001:0208a01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在如來前恭敬合掌瞻仰尊顏。",
        "from": "T21n1243_001:0208a02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告金剛手祕密主言。",
        "from": "T21n1243_001:0208a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祕密主若有持此大教明王經。",
        "from": "T21n1243_001:0208a03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為說呪曰。",
        "from": "T21n1243_001:0208a04_18",
        "context_with_next_sentence": true
    },
