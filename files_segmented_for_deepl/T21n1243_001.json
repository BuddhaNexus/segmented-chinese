        "from": "T21n1243_001:0208a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應於曼拏羅中安置大力明王。",
        "from": "T21n1243_001:0209a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後作觀密作護持。",
        "from": "T21n1243_001:0209a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當歸命頂禮一切佛法僧。",
        "from": "T21n1243_001:0209a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獨覺長老舍利弗聲聞眾等。",
        "from": "T21n1243_001:0209a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命大牟尼慈氏等一切大菩薩眾。",
        "from": "T21n1243_001:0209a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂禮正遍知覺者。",
        "from": "T21n1243_001:0209a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命持金剛并諸眷屬。",
        "from": "T21n1243_001:0209a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說真言曰。",
        "from": "T21n1243_001:0209a07_1",
        "context_with_next_sentence": true
    },
