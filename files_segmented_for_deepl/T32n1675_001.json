[
    {
        "source_sentence": "讚法界頌",
        "from": "T32n1675_001:0754b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖龍樹菩薩造",
        "from": "T32n1675_001:0754b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天北印度烏填曩國帝釋宮寺",
        "from": "T32n1675_001:0754b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏傳法大師賜紫沙門臣施護",
        "from": "T32n1675_001:0754b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉　詔譯",
        "from": "T32n1675_001:0754b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　歸命十方佛　　法身及報化",
        "from": "T32n1675_001:0754b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　願共諸眾生　　速成法界性",
        "from": "T32n1675_001:0754b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　輪迴三惡道　　法界理凝然",
        "from": "T32n1675_001:0754b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　本來常清淨　　諸相不能遷",
        "from": "T32n1675_001:0754b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　寂靜如虛空　　處處悉周遍",
        "from": "T32n1675_001:0754b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　體皆離彼此　　非深復非淺",
        "from": "T32n1675_001:0754b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　乳未轉變時　　酥醍醐不見",
        "from": "T32n1675_001:0754b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　煩惱未伏除　　法界無由顯",
        "from": "T32n1675_001:0754b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如酥處乳中　　酥本妙光瑩",
        "from": "T32n1675_001:0754b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　法界煩惱覆　　圓滿體清淨",
        "from": "T32n1675_001:0754b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如燈被障礙　　非能照餘物",
        "from": "T32n1675_001:0754b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無明恒覆心　　法界非明了",
        "from": "T32n1675_001:0754b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如燈離障礙　　處處物能照",
        "from": "T32n1675_001:0754b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　煩惱破壞時　　真如恒顯現",
        "from": "T32n1675_001:0754b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　初中及最後　　二障不能擾",
        "from": "T32n1675_001:0754b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如淨瑠璃珠　　恒時光照曜",
        "from": "T32n1675_001:0754b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　光明物所障　　被障明非見",
        "from": "T32n1675_001:0754c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　法界煩惱覆　　真如理難顯",
        "from": "T32n1675_001:0754c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　圓寂體光潔　　輪迴不能染",
        "from": "T32n1675_001:0754c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　勤求趣法界　　輪迴非能揜",
        "from": "T32n1675_001:0754c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如米糠纏裹　　穀體米非無",
        "from": "T32n1675_001:0754c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　煩惱覆真如　　真如煩惱有",
        "from": "T32n1675_001:0754c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如穀去其糠　　米體自然見",
        "from": "T32n1675_001:0754c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若離煩惱糠　　法界理方顯",
        "from": "T32n1675_001:0754c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　妄執有世間　　芭蕉終無實",
        "from": "T32n1675_001:0754c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　法界非世間　　亦非虛妄見",
        "from": "T32n1675_001:0754c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如人飲甘露　　熱惱悉皆除",
        "from": "T32n1675_001:0754c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若證法界性　　煩惱熱皆棄",
        "from": "T32n1675_001:0754c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　滅除煩惱焰　　法界甘露現",
        "from": "T32n1675_001:0754c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一切有情中　　高下皆平等",
        "from": "T32n1675_001:0754c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　體實果不生　　執種果非有",
        "from": "T32n1675_001:0754c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　智慧出生時　　有為非法界",
        "from": "T32n1675_001:0754c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　法界本無處　　究竟方可證",
        "from": "T32n1675_001:0754c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　清淨恒光潔　　日月皆瑩淨",
        "from": "T32n1675_001:0754c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　法界無垢染　　如龍夜雨塵",
        "from": "T32n1675_001:0754c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　況似羅睺面　　光明恒燦然",
        "from": "T32n1675_001:0754c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　譬如火浣布　　處火能離染",
        "from": "T32n1675_001:0754c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　垢去布猶存　　光明轉瑩淨",
        "from": "T32n1675_001:0754c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　貪愛令心染　　虛妄有輪迴",
        "from": "T32n1675_001:0754c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　亦如火浣布　　真空妄非有",
        "from": "T32n1675_001:0754c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　三毒生死本　　智慧火能燒",
        "from": "T32n1675_001:0754c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　法界體常有　　朗然恒照曜",
        "from": "T32n1675_001:0754c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　煩惱染稱垢　　世尊恒所宣",
        "from": "T32n1675_001:0754c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　垢滅真如顯　　如汲地中泉",
        "from": "T32n1675_001:0754c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　法界體無垢　　根隨能覆藏",
        "from": "T32n1675_001:0754c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若除煩惱盡　　瑩淨叵難量",
        "from": "T32n1675_001:0755a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　法界本無我　　二形及女男",
        "from": "T32n1675_001:0755a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　體無虛妄執　　何處更思惟",
        "from": "T32n1675_001:0755a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　法界離憎愛　　根塵境本無",
        "from": "T32n1675_001:0755a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　虛妄執為因　　差別從此生",
        "from": "T32n1675_001:0755a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　真空非苦惱　　貪愛苦惱因",
        "from": "T32n1675_001:0755a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　耽染由妄想　　三界乃輪迴",
        "from": "T32n1675_001:0755a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　懷孕在於腹　　嬰子未言見",
        "from": "T32n1675_001:0755a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　二障覆真如　　法界不能證",
        "from": "T32n1675_001:0755a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　種種生疑慮　　見慢及恚癡",
        "from": "T32n1675_001:0755a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　妄計有真實　　真實計非有",
        "from": "T32n1675_001:0755a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　兔角體非有　　妄執令真實",
        "from": "T32n1675_001:0755a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　法界離妄執　　妄執真非有",
        "from": "T32n1675_001:0755a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如色必破壞　　微塵猶可知",
        "from": "T32n1675_001:0755a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　法界非破壞　　三時不能得",
        "from": "T32n1675_001:0755a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　有生還有滅　　榮辱亦皆隨",
        "from": "T32n1675_001:0755a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　法界非生滅　　云何言所知",
        "from": "T32n1675_001:0755a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　兔角本非有　　三世猶可思",
        "from": "T32n1675_001:0755a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　真空非兔角　　思慮不能知",
        "from": "T32n1675_001:0755a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　真空稱善逝　　色相悉皆亡",
        "from": "T32n1675_001:0755a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　應化隨緣有　　修因離執非",
        "from": "T32n1675_001:0755a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　圓通如日月　　水現影皆同",
        "from": "T32n1675_001:0755a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　色聲雙泯絕　　差別云何有",
        "from": "T32n1675_001:0755a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　三世可尋思　　生緣時決定",
        "from": "T32n1675_001:0755a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若悟己身法　　己身云何有",
        "from": "T32n1675_001:0755a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如水居熱際　　處熱覺悟非",
        "from": "T32n1675_001:0755a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　寒際理亦然　　圓通皆如是",
        "from": "T32n1675_001:0755a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　心恒煩惱覆　　迷惑不能了",
        "from": "T32n1675_001:0755a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若離煩惱纏　　覺悟而非有",
        "from": "T32n1675_001:0755a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如眼觀諸色　　離障能照曜",
        "from": "T32n1675_001:0755b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　真空理亦然　　照曜離生滅",
        "from": "T32n1675_001:0755b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　耳識聞於聲　　離妄及分別",
        "from": "T32n1675_001:0755b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　法界性亦然　　分別妄非有",
        "from": "T32n1675_001:0755b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　鼻能嗅諸香　　妄執性非有",
        "from": "T32n1675_001:0755b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　色相二俱亡　　真空亦如是",
        "from": "T32n1675_001:0755b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　舌根自性空　　味界恒遠離",
        "from": "T32n1675_001:0755b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　識空體亦然　　法界理如是",
        "from": "T32n1675_001:0755b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　身根自性淨　　冷煖觸非有",
        "from": "T32n1675_001:0755b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　法界理亦然　　觸處常遠離",
        "from": "T32n1675_001:0755b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　意緣法稱最　　自性恒遠離",
        "from": "T32n1675_001:0755b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　諸法性本空　　圓通理如是",
        "from": "T32n1675_001:0755b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　見聞及覺知　　相應法亦空",
        "from": "T32n1675_001:0755b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　了絕諸妄想　　見聞理亦非",
        "from": "T32n1675_001:0755b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　根塵起妄執　　清淨體源無",
        "from": "T32n1675_001:0755b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　迷執有根塵　　根塵理非有",
        "from": "T32n1675_001:0755b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　世間並出世　　空性本無差",
        "from": "T32n1675_001:0755b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　我法由迷起　　遍計自輪迴",
        "from": "T32n1675_001:0755b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　法界理清淨　　貪瞋癡本無",
        "from": "T32n1675_001:0755b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　迷悟從心起　　三毒法假名",
        "from": "T32n1675_001:0755b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　迷執自纏縛　　了達假名智",
        "from": "T32n1675_001:0755b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　菩提非近遠　　三世理非有",
        "from": "T32n1675_001:0755b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　煩惱籠迷執　　世尊經所宣",
        "from": "T32n1675_001:0755b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　智生惑染滅　　妄執勿相纏",
        "from": "T32n1675_001:0755b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　去來執最勝　　體空猶可思",
        "from": "T32n1675_001:0755b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　菩提非妄執　　正證亦知非",
        "from": "T32n1675_001:0755b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　水乳同一處　　鵝飲乳非雜",
        "from": "T32n1675_001:0755b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　生空煩惱離　　二障亦非雜",
        "from": "T32n1675_001:0755b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　妄執我非無　　了達本非有",
        "from": "T32n1675_001:0755b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　涅盤清淨理　　二我俱非立",
        "from": "T32n1675_001:0755c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　三檀齊修施　　尸羅離過非",
        "from": "T32n1675_001:0755c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　忍因端正果　　精進勇懃依",
        "from": "T32n1675_001:0755c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　靜慮令心止　　般若用無疑",
        "from": "T32n1675_001:0755c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　願兼方便力　　安住勝菩提",
        "from": "T32n1675_001:0755c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　菩提難妄執　　真空生滅無",
        "from": "T32n1675_001:0755c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　了達空本性　　二相亦非有",
        "from": "T32n1675_001:0755c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　乳糖離甘蔗　　離蔗糖非有",
        "from": "T32n1675_001:0755c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　三乘趣菩提　　離種體非有",
        "from": "T32n1675_001:0755c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　守護稻穀種　　芽莖必得生",
        "from": "T32n1675_001:0755c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　守護菩提種　　菩提從此起",
        "from": "T32n1675_001:0755c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　譬如於黑月　　光明未能見",
        "from": "T32n1675_001:0755c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　有情煩惱纏　　真如未明顯",
        "from": "T32n1675_001:0755c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　月初光雖有　　漸漸而增長",
        "from": "T32n1675_001:0755c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　初地證菩提　　菩提未圓滿",
        "from": "T32n1675_001:0755c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　十五月圓滿　　處處光皎潔",
        "from": "T32n1675_001:0755c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　解脫顯法身　　法身理無缺",
        "from": "T32n1675_001:0755c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　染污意相應　　纏縛俱生滅",
        "from": "T32n1675_001:0755c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　解脫一切障　　三世悟非有",
        "from": "T32n1675_001:0755c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　初大僧祇滿　　三檀普遍修",
        "from": "T32n1675_001:0755c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　斷除分別障　　歡喜智難儔",
        "from": "T32n1675_001:0755c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　三業悞兼犯　　防非重及輕",
        "from": "T32n1675_001:0755c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　尸羅圓滿戒　　離垢獨標名",
        "from": "T32n1675_001:0755c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　二障恒時染　　俱空慧刃除",
        "from": "T32n1675_001:0755c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　發光能照曜　　破滅漸無餘",
        "from": "T32n1675_001:0755c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　遠離根隨染　　漸增焰慧威",
        "from": "T32n1675_001:0755c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　菩提稱最勝　　燒照轉光輝",
        "from": "T32n1675_001:0755c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　真俗稱二智　　相應互起違",
        "from": "T32n1675_001:0755c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　合令無所礙　　難勝事恒時",
        "from": "T32n1675_001:0755c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　十二緣生智　　巡環理趣全",
        "from": "T32n1675_001:0756a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　甚深稱最勝　　般若現於前",
        "from": "T32n1675_001:0756a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　世俗二乘行　　久修道已明",
        "from": "T32n1675_001:0756a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　相無功用滿　　最後稱遠行",
        "from": "T32n1675_001:0756a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　智用無分別　　恒時任運成",
        "from": "T32n1675_001:0756a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　眾魔降退散　　不動獨彰名",
        "from": "T32n1675_001:0756a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　善慧名無礙　　十方演法希",
        "from": "T32n1675_001:0756a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　身雲甘露雨　　應物最堪依",
        "from": "T32n1675_001:0756a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　眾德猶如水　　虛空喻似身",
        "from": "T32n1675_001:0756a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　重麁皆蔽塞　　大法智稱雲",
        "from": "T32n1675_001:0756a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　審諦輪迴事　　孰能免業牽",
        "from": "T32n1675_001:0756a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　要知無苦惱　　淨土勿相纏",
        "from": "T32n1675_001:0756a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　歸命佛真子　　位登智慧雲",
        "from": "T32n1675_001:0756a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　細微皆斷盡　　超苦離諸塵",
        "from": "T32n1675_001:0756a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　灌頂諸光照　　根塵普遍身",
        "from": "T32n1675_001:0756a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　金剛寂大定　　眾苦勿相親",
        "from": "T32n1675_001:0756a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　大寶花王座　　俱胝眾妙成",
        "from": "T32n1675_001:0756a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　莊嚴皆普遍　　功德實難思",
        "from": "T32n1675_001:0756a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　十力兼無畏　　三身四智圓",
        "from": "T32n1675_001:0756a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　六通恒自在　　應物化機緣",
        "from": "T32n1675_001:0756a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　照曜如圓月　　恒時焰熾燃",
        "from": "T32n1675_001:0756a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　十方無不遍　　燦爛轉光鮮",
        "from": "T32n1675_001:0756a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　永絕緣生染　　恒時處涅盤",
        "from": "T32n1675_001:0756a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　菩提稱最勝　　化益物情歡",
        "from": "T32n1675_001:0756a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　智用深如海　　隨機現應身",
        "from": "T32n1675_001:0756a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　水清來月影　　處處度迷津",
        "from": "T32n1675_001:0756a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　況似頗胝寶　　隨緣現影同",
        "from": "T32n1675_001:0756a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　物情根有感　　周普事無窮",
        "from": "T32n1675_001:0756a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　餓鬼恒飢渴　　不能見水泉",
        "from": "T32n1675_001:0756a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　眾生無少信　　宿業自縈纏",
        "from": "T32n1675_001:0756b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　化現身諸相　　光明皆燦然",
        "from": "T32n1675_001:0756b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　佛雖恒在世　　不覩宿無緣",
        "from": "T32n1675_001:0756b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　曉了塵沙界　　根隨染久無",
        "from": "T32n1675_001:0756b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　二空殊勝智　　妙用化童愚",
        "from": "T32n1675_001:0756b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　清淨絕諸垢　　自他受用身",
        "from": "T32n1675_001:0756b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　恒居色究竟　　利益五乘人",
        "from": "T32n1675_001:0756b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　救護眾生苦　　俱胝壽命長",
        "from": "T32n1675_001:0756b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　二嚴無有盡　　功德叵難量",
        "from": "T32n1675_001:0756b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　佛演一乘法　　隨機悟淺深",
        "from": "T32n1675_001:0756b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　蓮花無垢染　　玉本絕瑕侵",
        "from": "T32n1675_001:0756b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　少分而稱讚　　廣宣理趣玄",
        "from": "T32n1675_001:0756b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　願將諸功德　　普利施人天",
        "from": "T32n1675_001:0756b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讚法界頌",
        "from": "T32n1675_001:0756b14_0",
        "context_with_next_sentence": false
    }
]