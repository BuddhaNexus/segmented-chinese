[
    {
        "source_sentence": "壹輸盧迦論一卷",
        "from": "T30n1573_001:0253a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍樹菩薩造",
        "from": "T30n1573_001:0253a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後魏瞿曇留支雒陽譯",
        "from": "T30n1573_001:0253a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　體自體無常　　如是體無體",
        "from": "T30n1573_001:0253a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　自體性無體　　故說空無常",
        "from": "T30n1573_001:0253a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T30n1573_001:0253a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以何義故造此一偈論。",
        "from": "T30n1573_001:0253a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說何等義破何等人。",
        "from": "T30n1573_001:0253a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T30n1573_001:0253a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為讀誦者於廣大部生懈倦心。",
        "from": "T30n1573_001:0253a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又為聰叡先已廣習無量諸論於如來法海義中思惟而生懈倦。",
        "from": "T30n1573_001:0253a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於無常自體空不異義中。",
        "from": "T30n1573_001:0253a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生異相疑。",
        "from": "T30n1573_001:0253a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為斷此疑故造斯論。",
        "from": "T30n1573_001:0253a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說何義者。",
        "from": "T30n1573_001:0253a13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今當說。",
        "from": "T30n1573_001:0253a13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂一切法無常自體空。",
        "from": "T30n1573_001:0253a14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自體空不離無常。",
        "from": "T30n1573_001:0253a14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切法自性自體空。",
        "from": "T30n1573_001:0253a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故無有常。",
        "from": "T30n1573_001:0253a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸佛緣覺聲聞。",
        "from": "T30n1573_001:0253a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於空法中而得出離。",
        "from": "T30n1573_001:0253a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非於諸行斷常法中而得解脫。",
        "from": "T30n1573_001:0253a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈言。",
        "from": "T30n1573_001:0253a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　滅空住有體　　則成於常見",
        "from": "T30n1573_001:0253a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若謂後時滅　　則成於斷見",
        "from": "T30n1573_001:0253a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此義故。",
        "from": "T30n1573_001:0253a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說一切法自體空。",
        "from": "T30n1573_001:0253a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛緣覺聲聞羅漢。",
        "from": "T30n1573_001:0253a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此義中得利益故。",
        "from": "T30n1573_001:0253a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破何等人者。",
        "from": "T30n1573_001:0253a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今當說。",
        "from": "T30n1573_001:0253a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有所得人離於諸行。",
        "from": "T30n1573_001:0253a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說有無常則非正見。",
        "from": "T30n1573_001:0253a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無常離有為。",
        "from": "T30n1573_001:0253a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無常則常猶如虛空。",
        "from": "T30n1573_001:0253a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若如是者。",
        "from": "T30n1573_001:0253a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則有為無為體無差別。",
        "from": "T30n1573_001:0253a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有為與無為合。",
        "from": "T30n1573_001:0253a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無為合故則瓶不可破。",
        "from": "T30n1573_001:0253a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無為與有為合。",
        "from": "T30n1573_001:0253a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有為合故則涅槃可壞。",
        "from": "T30n1573_001:0253a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不異者。",
        "from": "T30n1573_001:0253a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則一切法不可破壞。",
        "from": "T30n1573_001:0253a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如涅槃常非緣生故。",
        "from": "T30n1573_001:0253a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若諸行非因緣生。",
        "from": "T30n1573_001:0253a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不異虛空涅槃者。",
        "from": "T30n1573_001:0253a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則有為法不名無常。",
        "from": "T30n1573_001:0253a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若諸行非因緣生。",
        "from": "T30n1573_001:0253a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是無常者。",
        "from": "T30n1573_001:0253a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則虛空涅槃不名為常。",
        "from": "T30n1573_001:0253a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若如是者。",
        "from": "T30n1573_001:0253b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則有為無為無有勝法。",
        "from": "T30n1573_001:0253b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無常離有為。",
        "from": "T30n1573_001:0253b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶名無常者。",
        "from": "T30n1573_001:0253b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則有為離常。",
        "from": "T30n1573_001:0253b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應名為常。",
        "from": "T30n1573_001:0253b02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但是事不然。",
        "from": "T30n1573_001:0253b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若如是者。",
        "from": "T30n1573_001:0253b03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等修多羅中作如是說。",
        "from": "T30n1573_001:0253b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以何義說。",
        "from": "T30n1573_001:0253b04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今所說以何義說。",
        "from": "T30n1573_001:0253b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今所說義不相應。",
        "from": "T30n1573_001:0253b04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非汝邪思之所能量。",
        "from": "T30n1573_001:0253b05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故汝說非為正見。",
        "from": "T30n1573_001:0253b05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人有所得。",
        "from": "T30n1573_001:0253b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂過去未來現在法自體成者。",
        "from": "T30n1573_001:0253b06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知是人則非正見。",
        "from": "T30n1573_001:0253b07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故。",
        "from": "T30n1573_001:0253b07_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無因生見故。",
        "from": "T30n1573_001:0253b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若言未來體非因緣生自體成者。",
        "from": "T30n1573_001:0253b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則現在法亦非因緣生自性體成。",
        "from": "T30n1573_001:0253b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以未來現在自性平等無差別故。",
        "from": "T30n1573_001:0253b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若性平等者。",
        "from": "T30n1573_001:0253b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現在有法皆從緣生。",
        "from": "T30n1573_001:0253b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未來法何故非緣生。",
        "from": "T30n1573_001:0253b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今此義為以修多羅說為依義說。",
        "from": "T30n1573_001:0253b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說不相應則無理趣。",
        "from": "T30n1573_001:0253b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無理趣則不可信。",
        "from": "T30n1573_001:0253b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若未來法無因緣生自性成者。",
        "from": "T30n1573_001:0253b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未來之法猶如虛空無有因緣。",
        "from": "T30n1573_001:0253b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離因緣故非因緣生。",
        "from": "T30n1573_001:0253b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則無實未來體。",
        "from": "T30n1573_001:0253b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無未來故現在過去亦無。",
        "from": "T30n1573_001:0253b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現在過去無故三世無體。",
        "from": "T30n1573_001:0253b16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有體者則是常見無因生故。",
        "from": "T30n1573_001:0253b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若佛弟子有所得見。",
        "from": "T30n1573_001:0253b17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則與外道迦毘羅等無差別。",
        "from": "T30n1573_001:0253b18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此論非為迦毘羅憂樓迦諸外道等。",
        "from": "T30n1573_001:0253b18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為汝等輩同見之人故造斯論。",
        "from": "T30n1573_001:0253b19_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向說破何等人。",
        "from": "T30n1573_001:0253b20_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為令汝等有所得人斷除邪見故作斯論。",
        "from": "T30n1573_001:0253b20_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一輸盧迦偈句義今當釋。",
        "from": "T30n1573_001:0253b21_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈言自體性無常者。",
        "from": "T30n1573_001:0253b22_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自體名有生。",
        "from": "T30n1573_001:0253b22_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有法故名為體。",
        "from": "T30n1573_001:0253b22_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有所得人於此法中心取為體。",
        "from": "T30n1573_001:0253b23_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此法於陰界入中有聲緣轉。",
        "from": "T30n1573_001:0253b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如說一體二體多體。",
        "from": "T30n1573_001:0253b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如彼此人一二眾多各有自體故名自體。",
        "from": "T30n1573_001:0253b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如地水火風堅濕熱動各有自性。",
        "from": "T30n1573_001:0253b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是各各自相自體故言自體。",
        "from": "T30n1573_001:0253b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有所得人謂生住滅同相者。",
        "from": "T30n1573_001:0253b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是義不然。",
        "from": "T30n1573_001:0253b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自體性無常故。",
        "from": "T30n1573_001:0253b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼體名有所得人分別故生。",
        "from": "T30n1573_001:0253b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故離諸法無無常體。",
        "from": "T30n1573_001:0253b29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以自相無常故。",
        "from": "T30n1573_001:0253b29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如佛告比丘。",
        "from": "T30n1573_001:0253c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸行皆悉無常。",
        "from": "T30n1573_001:0253c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此說故。",
        "from": "T30n1573_001:0253c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離法有無常自相者。",
        "from": "T30n1573_001:0253c02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是事不然。",
        "from": "T30n1573_001:0253c02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若汝不了云何無常。",
        "from": "T30n1573_001:0253c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今當說。",
        "from": "T30n1573_001:0253c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈言如是體無體故。",
        "from": "T30n1573_001:0253c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體無體者。",
        "from": "T30n1573_001:0253c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝所分別無常者。",
        "from": "T30n1573_001:0253c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼無常無體。",
        "from": "T30n1573_001:0253c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故體無體。",
        "from": "T30n1573_001:0253c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自性無體故言無體。",
        "from": "T30n1573_001:0253c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈言自體性無體者。",
        "from": "T30n1573_001:0253c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離無體更無別體故。",
        "from": "T30n1573_001:0253c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言自體無體。",
        "from": "T30n1573_001:0253c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若汝意謂離無體而有體者。",
        "from": "T30n1573_001:0253c07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是義不然。",
        "from": "T30n1573_001:0253c07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以汝此法非修多羅說故。",
        "from": "T30n1573_001:0253c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若謂無體是自體者。",
        "from": "T30n1573_001:0253c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是亦不然。",
        "from": "T30n1573_001:0253c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以修多羅所不說故。",
        "from": "T30n1573_001:0253c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "T30n1573_001:0253c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於何等修多羅中說如此法。",
        "from": "T30n1573_001:0253c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於佛世尊修多羅中。",
        "from": "T30n1573_001:0253c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "都無此義。",
        "from": "T30n1573_001:0253c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以非經說故不成就。",
        "from": "T30n1573_001:0253c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非大聖修多羅所說之義則不應信。",
        "from": "T30n1573_001:0253c11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故非唯言說而得取證。",
        "from": "T30n1573_001:0253c12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈言故說空無常者。",
        "from": "T30n1573_001:0253c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如調伏三蜜提經中說。",
        "from": "T30n1573_001:0253c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告三蜜提。",
        "from": "T30n1573_001:0253c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼空無常無不動。",
        "from": "T30n1573_001:0253c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無不壞無不變。",
        "from": "T30n1573_001:0253c14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故。",
        "from": "T30n1573_001:0253c15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性如是故。",
        "from": "T30n1573_001:0253c15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耳鼻舌身意亦復如是。",
        "from": "T30n1573_001:0253c15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "T30n1573_001:0253c16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此修多羅中說空說無常。",
        "from": "T30n1573_001:0253c16_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以是義故。",
        "from": "T30n1573_001:0253c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知諸法空無常。",
        "from": "T30n1573_001:0253c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無常無體。",
        "from": "T30n1573_001:0253c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故諸法性自無體則無體義成。",
        "from": "T30n1573_001:0253c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能如是入修多羅義。",
        "from": "T30n1573_001:0253c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其義則成。",
        "from": "T30n1573_001:0253c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不入修多羅。",
        "from": "T30n1573_001:0253c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其義則壞。",
        "from": "T30n1573_001:0253c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以我所說入修多羅。",
        "from": "T30n1573_001:0253c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其義則成。",
        "from": "T30n1573_001:0253c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故性自無體其義成就。",
        "from": "T30n1573_001:0253c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一輸盧迦論一卷。",
        "from": "T30n1573_001:0253c21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡諸法體性法物事有名異義同。",
        "from": "T30n1573_001:0253c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故或言體。",
        "from": "T30n1573_001:0253c22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或言性。",
        "from": "T30n1573_001:0253c22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或言法。",
        "from": "T30n1573_001:0253c22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或言有。",
        "from": "T30n1573_001:0253c23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或言物。",
        "from": "T30n1573_001:0253c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫不皆是有之差別。",
        "from": "T30n1573_001:0253c23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正音云私婆婆。",
        "from": "T30n1573_001:0253c23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或譯為自體體。",
        "from": "T30n1573_001:0253c24_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或譯為無法有法。",
        "from": "T30n1573_001:0253c24_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或譯為無自性性。",
        "from": "T30n1573_001:0253c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壹輸盧迦論一卷",
        "from": "T30n1573_001:0253c26_0",
        "context_with_next_sentence": false
    }
]