[
    {
        "source_sentence": "觀世音菩薩救苦經",
        "from": "X01n0034_001:0469c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無救苦觀世音菩薩。",
        "from": "X01n0034_001:0469c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百千萬億佛。",
        "from": "X01n0034_001:0469c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒河沙數佛。",
        "from": "X01n0034_001:0469c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量功德佛。",
        "from": "X01n0034_001:0469c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告阿難言。",
        "from": "X01n0034_001:0469c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經大聖。",
        "from": "X01n0034_001:0469c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能救獄囚。",
        "from": "X01n0034_001:0469c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能救重病。",
        "from": "X01n0034_001:0469c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能救千災百難苦。",
        "from": "X01n0034_001:0469c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人。",
        "from": "X01n0034_001:0469c08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦得一千遍。",
        "from": "X01n0034_001:0469c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一身離苦難。",
        "from": "X01n0034_001:0469c08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦得一萬遍。",
        "from": "X01n0034_001:0469c09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合家離苦難。",
        "from": "X01n0034_001:0469c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無佛力威。",
        "from": "X01n0034_001:0469c09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無佛力護。",
        "from": "X01n0034_001:0469c09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使人無惡心。",
        "from": "X01n0034_001:0469c10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令人身得度。",
        "from": "X01n0034_001:0469c10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "回光菩薩。",
        "from": "X01n0034_001:0469c10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "回善菩薩。",
        "from": "X01n0034_001:0469c10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿耨大天王。",
        "from": "X01n0034_001:0469c10_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正殿菩薩。",
        "from": "X01n0034_001:0469c11_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩邱摩邱。",
        "from": "X01n0034_001:0469c11_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清淨比邱。",
        "from": "X01n0034_001:0469c11_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "官事得散。",
        "from": "X01n0034_001:0469c11_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "私事得休。",
        "from": "X01n0034_001:0469c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸大菩薩。",
        "from": "X01n0034_001:0469c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五百羅漢。",
        "from": "X01n0034_001:0469c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救護弟子身。",
        "from": "X01n0034_001:0469c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆離苦難。",
        "from": "X01n0034_001:0469c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然觀世音。",
        "from": "X01n0034_001:0469c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纓絡不須解。",
        "from": "X01n0034_001:0469c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勤誦千萬遍。",
        "from": "X01n0034_001:0469c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "災難自然得解脫。",
        "from": "X01n0034_001:0469c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信受奉行。",
        "from": "X01n0034_001:0469c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說真言曰。",
        "from": "X01n0034_001:0469c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今菩今菩提　陀羅尼帝　尼佉羅帝　菩提薩婆訶",
        "from": "X01n0034_001:0469c15_0",
        "context_with_next_sentence": false
    }
]