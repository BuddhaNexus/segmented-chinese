[
    {
        "source_sentence": "都部陀羅尼目一卷",
        "from": "T18n0903_001:0898c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同王司特進試鴻臚卿肅國",
        "from": "T18n0903_001:0898c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空諡大鑒",
        "from": "T18n0903_001:0898c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正號大廣智大興善寺三藏沙門",
        "from": "T18n0903_001:0898c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空奉　詔譯",
        "from": "T18n0903_001:0898c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瑜伽本經都十萬偈。",
        "from": "T18n0903_001:0898c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有十八會。",
        "from": "T18n0903_001:0898c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初會經名一切如來真實攝。",
        "from": "T18n0903_001:0898c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其經說五部。",
        "from": "T18n0903_001:0898c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛部(毘盧遮那佛以為部主)金剛部(阿閦佛為部主)寶部(寶生佛以為部主)蓮花部(阿彌陀佛以為部主)羯磨部(不空成就佛以為部主)彼五部主。",
        "from": "T18n0903_001:0898c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各有四菩薩以為眷屬。",
        "from": "T18n0903_001:0898c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前右左背而安列。",
        "from": "T18n0903_001:0898c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四內供養。",
        "from": "T18n0903_001:0898c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各屬四部。",
        "from": "T18n0903_001:0898c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次第應知。",
        "from": "T18n0903_001:0898c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四外供養亦屬四部。",
        "from": "T18n0903_001:0898c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四門鉤索鎖鈴。",
        "from": "T18n0903_001:0898c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四部次第應知。",
        "from": "T18n0903_001:0898c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又有四方賢劫中十六大菩薩。",
        "from": "T18n0903_001:0898c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "表賢劫中一切菩薩。",
        "from": "T18n0903_001:0898c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又外有五類天。",
        "from": "T18n0903_001:0898c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一類有四天。",
        "from": "T18n0903_001:0898c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總有二十天并妃后。",
        "from": "T18n0903_001:0898c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有五類成二十。",
        "from": "T18n0903_001:0898c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五類者上界四天。",
        "from": "T18n0903_001:0898c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　住虛空四天　　遊虛空四天",
        "from": "T18n0903_001:0898c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　地居有四天　　居地底四天",
        "from": "T18n0903_001:0898c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瑜伽部曼茶羅有四。",
        "from": "T18n0903_001:0898c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一金剛界。",
        "from": "T18n0903_001:0898c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二降三世。",
        "from": "T18n0903_001:0898c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三遍調伏。",
        "from": "T18n0903_001:0898c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四一切義成就。",
        "from": "T18n0903_001:0898c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此四曼茶羅。",
        "from": "T18n0903_001:0898c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "表毘盧遮那佛內四智菩薩。",
        "from": "T18n0903_001:0898c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂金剛灌頂蓮花羯磨為四智。",
        "from": "T18n0903_001:0898c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又四智謂大圓鏡平等性妙觀察成所作為四智矣。",
        "from": "T18n0903_001:0898c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又一一曼茶羅。",
        "from": "T18n0903_001:0898c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建立六曼茶羅。",
        "from": "T18n0903_001:0898c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂大曼茶羅。",
        "from": "T18n0903_001:0898c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三昧耶曼茶羅。",
        "from": "T18n0903_001:0898c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法曼茶羅。",
        "from": "T18n0903_001:0898c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羯磨曼茶羅。",
        "from": "T18n0903_001:0898c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四印曼茶羅。",
        "from": "T18n0903_001:0899a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一印曼茶羅。",
        "from": "T18n0903_001:0899a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯降三世曼茶羅具十曼茶羅。",
        "from": "T18n0903_001:0899a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘皆具六曼茶羅一切印契一切法要。",
        "from": "T18n0903_001:0899a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以四智印攝盡。",
        "from": "T18n0903_001:0899a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大智印。",
        "from": "T18n0903_001:0899a03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以五相成本尊瑜伽。",
        "from": "T18n0903_001:0899a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三昧耶印(以二手和合金剛縛發生成印)。",
        "from": "T18n0903_001:0899a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法智印(名本尊種子法身三摩地一切契經文義)羯磨智印(以二手金剛拳如執持器仗幖幟如身威儀形)。",
        "from": "T18n0903_001:0899a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又瑜伽中四種眼(敬愛法)。",
        "from": "T18n0903_001:0899a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "熾盛眼(鉤召)忿怒眼(降伏心殺害煩惱也)慈悲眼(除毒息怨敵也)。",
        "from": "T18n0903_001:0899a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又一切如來教集瑜伽中。",
        "from": "T18n0903_001:0899a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一百二十種護摩。",
        "from": "T18n0903_001:0899a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依二十五種爐。",
        "from": "T18n0903_001:0899a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於護摩爐中。",
        "from": "T18n0903_001:0899a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "契印幖幟各異。",
        "from": "T18n0903_001:0899a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求迅速成辦。",
        "from": "T18n0903_001:0899a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間出世間成就果報。",
        "from": "T18n0903_001:0899a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸會浩汗文義稍多。",
        "from": "T18n0903_001:0899a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恐文繁。",
        "from": "T18n0903_001:0899a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且略指方隅。",
        "from": "T18n0903_001:0899a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依毘盧遮那成道經。",
        "from": "T18n0903_001:0899a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大本十萬偈。",
        "from": "T18n0903_001:0899a13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可有三百卷經。",
        "from": "T18n0903_001:0899a13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐國所譯略本七卷。",
        "from": "T18n0903_001:0899a14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經中。",
        "from": "T18n0903_001:0899a14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說一百六十心十緣生句。",
        "from": "T18n0903_001:0899a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及五輪。",
        "from": "T18n0903_001:0899a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地輪水輪火輪風輪空輪也。",
        "from": "T18n0903_001:0899a15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經中。",
        "from": "T18n0903_001:0899a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二種修行。",
        "from": "T18n0903_001:0899a16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩提心以為因。",
        "from": "T18n0903_001:0899a16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大悲以為根本方便為究竟。",
        "from": "T18n0903_001:0899a17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依勝義世俗。",
        "from": "T18n0903_001:0899a17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二諦若依勝義修行。",
        "from": "T18n0903_001:0899a18_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建立法身曼茶羅。",
        "from": "T18n0903_001:0899a18_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故此經中說。",
        "from": "T18n0903_001:0899a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先稱虛空中曼茶羅。",
        "from": "T18n0903_001:0899a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故觀本尊法身。",
        "from": "T18n0903_001:0899a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離形色猶如虛空。",
        "from": "T18n0903_001:0899a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住如是三摩地。",
        "from": "T18n0903_001:0899a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若依世俗諦修行。",
        "from": "T18n0903_001:0899a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依四輪以為曼茶羅。",
        "from": "T18n0903_001:0899a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本尊。",
        "from": "T18n0903_001:0899a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖者若黃色。",
        "from": "T18n0903_001:0899a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住地輪曼茶羅(其形方名金輪)聖者若白色。",
        "from": "T18n0903_001:0899a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住水輪曼茶羅(其形圓名水輪)聖者若赤色。",
        "from": "T18n0903_001:0899a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住火輪曼茶羅(其形三角)聖者若青若黑。",
        "from": "T18n0903_001:0899a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住風輪曼茶羅(其形如半月)大曼茶羅安於八葉蓮花臺。",
        "from": "T18n0903_001:0899a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五佛四菩薩安於臺葉。",
        "from": "T18n0903_001:0899a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中曼茶羅外。",
        "from": "T18n0903_001:0899a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又有三種曼茶羅。",
        "from": "T18n0903_001:0899a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者一切如來曼茶羅。",
        "from": "T18n0903_001:0899a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者釋迦牟尼曼茶羅。",
        "from": "T18n0903_001:0899a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者文殊師利曼茶羅。",
        "from": "T18n0903_001:0899b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此曼茶羅。",
        "from": "T18n0903_001:0899b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為大悲胎藏曼茶羅。",
        "from": "T18n0903_001:0899b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若弟子受灌頂法。",
        "from": "T18n0903_001:0899b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小曼茶羅極微妙委曲。",
        "from": "T18n0903_001:0899b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘部所不代。",
        "from": "T18n0903_001:0899b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中修行供養。",
        "from": "T18n0903_001:0899b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼存二種法謂事與理為二也此經中。",
        "from": "T18n0903_001:0899b04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護摩火天有四十種。",
        "from": "T18n0903_001:0899b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就中一十二種火為最勝。",
        "from": "T18n0903_001:0899b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爐形及木。",
        "from": "T18n0903_001:0899b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有乳果類苦練。",
        "from": "T18n0903_001:0899b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所用各不同。",
        "from": "T18n0903_001:0899b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東西南北祈願各殊。",
        "from": "T18n0903_001:0899b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內外護摩亦依五輪。",
        "from": "T18n0903_001:0899b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求四種事速疾成就。",
        "from": "T18n0903_001:0899b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "息災增益降伏敬愛。",
        "from": "T18n0903_001:0899b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所請火天各各不同。",
        "from": "T18n0903_001:0899b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寂靜熙怡忿怒喜怒。",
        "from": "T18n0903_001:0899b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次第應知。",
        "from": "T18n0903_001:0899b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若蘇悉地經。",
        "from": "T18n0903_001:0899b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教中依三部。",
        "from": "T18n0903_001:0899b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂佛部(五佛頂等)蓮花部種類甚多。",
        "from": "T18n0903_001:0899b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛部金剛薩埵等。",
        "from": "T18n0903_001:0899b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變化無量。",
        "from": "T18n0903_001:0899b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有三種三昧耶。",
        "from": "T18n0903_001:0899b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛部蓮花部金剛部。",
        "from": "T18n0903_001:0899b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有三部心真言。",
        "from": "T18n0903_001:0899b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾那爾迦(半音呼一)阿嚧力(二)嚩日囉(二合)地叨(二合)(三)部主有三種。",
        "from": "T18n0903_001:0899b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金輪王佛頂佛部主。",
        "from": "T18n0903_001:0899b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮花部主馬頭觀自在。",
        "from": "T18n0903_001:0899b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛部主三世勝金剛。",
        "from": "T18n0903_001:0899b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三種部母。",
        "from": "T18n0903_001:0899b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛部佛眼以為部母。",
        "from": "T18n0903_001:0899b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮花部白衣觀自在以為部母。",
        "from": "T18n0903_001:0899b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛部忙麼鷄菩薩以為部母。",
        "from": "T18n0903_001:0899b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三種明妃。",
        "from": "T18n0903_001:0899b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛部無能勝菩薩以為明妃。",
        "from": "T18n0903_001:0899b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮花部多羅菩薩以為明妃。",
        "from": "T18n0903_001:0899b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛部金剛孫那利菩薩以為明妃。",
        "from": "T18n0903_001:0899b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三種忿怒。",
        "from": "T18n0903_001:0899b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不動尊佛部忿怒。",
        "from": "T18n0903_001:0899b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忿怒鉤蓮花部忿怒。",
        "from": "T18n0903_001:0899b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "軍茶利金剛部忿怒。",
        "from": "T18n0903_001:0899b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有四種界。",
        "from": "T18n0903_001:0899b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛橛地界。",
        "from": "T18n0903_001:0899b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛牆八方界。",
        "from": "T18n0903_001:0899b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛網上方界。",
        "from": "T18n0903_001:0899b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密縫。",
        "from": "T18n0903_001:0899b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿三莽儗儞界。",
        "from": "T18n0903_001:0899b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又有四種界。",
        "from": "T18n0903_001:0899b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結護曼茶羅。",
        "from": "T18n0903_001:0899b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛索護東方。",
        "from": "T18n0903_001:0899b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛幢幡護西方。",
        "from": "T18n0903_001:0899b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛迦利護南方。",
        "from": "T18n0903_001:0899b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛峯護北方。",
        "from": "T18n0903_001:0899b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又大界名商羯羅。",
        "from": "T18n0903_001:0899c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設佛頂護輪王等隣近。",
        "from": "T18n0903_001:0899c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不被障礙。",
        "from": "T18n0903_001:0899c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經中。",
        "from": "T18n0903_001:0899c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修真言者成就世間悉地。",
        "from": "T18n0903_001:0899c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依時依處。",
        "from": "T18n0903_001:0899c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時者謂三時。",
        "from": "T18n0903_001:0899c03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處謂本尊像前。",
        "from": "T18n0903_001:0899c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有供養五種。",
        "from": "T18n0903_001:0899c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除閼伽。",
        "from": "T18n0903_001:0899c04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者塗香。",
        "from": "T18n0903_001:0899c04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者花鬘。",
        "from": "T18n0903_001:0899c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者燒香。",
        "from": "T18n0903_001:0899c05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者飲食。",
        "from": "T18n0903_001:0899c05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者燈明。",
        "from": "T18n0903_001:0899c05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白(北)黃(東)黑(南)赤(西)隨息災增益降伏敬愛。",
        "from": "T18n0903_001:0899c05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求應知。",
        "from": "T18n0903_001:0899c06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就者十八種物隨身。",
        "from": "T18n0903_001:0899c07_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣如經說。",
        "from": "T18n0903_001:0899c07_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三時澡浴三時換衣。",
        "from": "T18n0903_001:0899c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一月分為四時。",
        "from": "T18n0903_001:0899c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從月生一日至八日。",
        "from": "T18n0903_001:0899c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應作息災。",
        "from": "T18n0903_001:0899c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從九日至十五日。",
        "from": "T18n0903_001:0899c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應作增益。",
        "from": "T18n0903_001:0899c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從十六日至二十三日。",
        "from": "T18n0903_001:0899c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應作降伏。",
        "from": "T18n0903_001:0899c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從二十四日至月盡日。",
        "from": "T18n0903_001:0899c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為敬愛法。",
        "from": "T18n0903_001:0899c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[麩夫+主]呬耶經亦同蘇悉地。",
        "from": "T18n0903_001:0899c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說分布曼茶羅。",
        "from": "T18n0903_001:0899c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及絣地法。",
        "from": "T18n0903_001:0899c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經中極微細。",
        "from": "T18n0903_001:0899c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可具錄。",
        "from": "T18n0903_001:0899c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又八方。",
        "from": "T18n0903_001:0899c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其護持帝王營從兵法。",
        "from": "T18n0903_001:0899c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五天竺國深敬信佛法。",
        "from": "T18n0903_001:0899c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於帝王可傳。",
        "from": "T18n0903_001:0899c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘇婆呼童子經。",
        "from": "T18n0903_001:0899c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經中說辦求成就人護摩。",
        "from": "T18n0903_001:0899c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "杵金銀銅鐵石水精佉陀羅木等。",
        "from": "T18n0903_001:0899c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量種各不同杵。",
        "from": "T18n0903_001:0899c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五股三股一股。",
        "from": "T18n0903_001:0899c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長十六指為上。",
        "from": "T18n0903_001:0899c18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二指為中。",
        "from": "T18n0903_001:0899c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八指為下。",
        "from": "T18n0903_001:0899c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至一指節者為下。",
        "from": "T18n0903_001:0899c19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經中說。",
        "from": "T18n0903_001:0899c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不持金剛杵。",
        "from": "T18n0903_001:0899c19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦者無由得成就。",
        "from": "T18n0903_001:0899c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛鈴者。",
        "from": "T18n0903_001:0899c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是般若波羅蜜義金剛杵者是。",
        "from": "T18n0903_001:0899c20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩提心義。",
        "from": "T18n0903_001:0900a01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能壞斷常二邊。",
        "from": "T18n0903_001:0900a01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "契合中道。",
        "from": "T18n0903_001:0900a02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有十六菩薩位。",
        "from": "T18n0903_001:0900a02_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦表十六空為中道。",
        "from": "T18n0903_001:0900a02_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩邊各有五股。",
        "from": "T18n0903_001:0900a03_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五佛五智義。",
        "from": "T18n0903_001:0900a03_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦表十波羅蜜。",
        "from": "T18n0903_001:0900a04_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能摧十種煩惱。",
        "from": "T18n0903_001:0900a04_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成十種真如。",
        "from": "T18n0903_001:0900a04_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便證十地。",
        "from": "T18n0903_001:0900a05_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證金剛三摩地獲金剛智。",
        "from": "T18n0903_001:0900a05_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐金剛座。",
        "from": "T18n0903_001:0900a05_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦是一切智智。",
        "from": "T18n0903_001:0900a06_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名如來自覺聖智。",
        "from": "T18n0903_001:0900a06_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不修此三摩地智。",
        "from": "T18n0903_001:0900a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得成佛者。",
        "from": "T18n0903_001:0900a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有是處。",
        "from": "T18n0903_001:0900a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恐文繁不能廣述。",
        "from": "T18n0903_001:0900a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若廣釋。",
        "from": "T18n0903_001:0900a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "窮劫不可說盡。",
        "from": "T18n0903_001:0900a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怚唎三昧耶經。",
        "from": "T18n0903_001:0900a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同毘盧遮那集會。",
        "from": "T18n0903_001:0900a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有聖眾修行教法。",
        "from": "T18n0903_001:0900a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自性成就。",
        "from": "T18n0903_001:0900a10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此教中修行者。",
        "from": "T18n0903_001:0900a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但住菩提心。",
        "from": "T18n0903_001:0900a10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大悲志願。",
        "from": "T18n0903_001:0900a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不捨無盡眾生界。",
        "from": "T18n0903_001:0900a11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所不應事不可建不可食若為者犯三摩耶。",
        "from": "T18n0903_001:0900a11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經中。",
        "from": "T18n0903_001:0900a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說誦大輪金剛真言。",
        "from": "T18n0903_001:0900a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不染諸愆過。",
        "from": "T18n0903_001:0900a13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為方便。",
        "from": "T18n0903_001:0900a13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現生一切真言速疾成就。",
        "from": "T18n0903_001:0900a13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經中不動尊等。",
        "from": "T18n0903_001:0900a14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四十二如來僮僕使者。",
        "from": "T18n0903_001:0900a14_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若修真言行菩薩。",
        "from": "T18n0903_001:0900a15_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅持菩提心。",
        "from": "T18n0903_001:0900a15_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等承事供養擁護。",
        "from": "T18n0903_001:0900a16_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食彼修行者殘食。",
        "from": "T18n0903_001:0900a16_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼等至無上菩提諸有作障者毘那夜迦不。",
