[
    {
        "source_sentence": "佛說金色迦那鉢底陀羅尼經",
        "from": "T21n1269_001:0303a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏金剛智奉　詔譯",
        "from": "T21n1269_001:0303a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T21n1269_001:0303a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在舍衛國迦蘭竹林園。",
        "from": "T21n1269_001:0303a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大說法愍念一切眾生。",
        "from": "T21n1269_001:0303a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此陀羅尼。",
        "from": "T21n1269_001:0303a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屬金色迦那鉢底。",
        "from": "T21n1269_001:0303a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊即說真言曰。",
        "from": "T21n1269_001:0303a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨率都底(一)迦吒迦吒(二)底吒摩吒(三)起哩(二合)亹拏(二合四)起哩(二合)亹拏(二合五)畔惹畔惹(二合六)曩謨率都帝(七)護嚕捺(八)跛惹尼曳(九)娑縛賀(十)阿部多(十一)浪拏乞懺(二合)質多(十二)曷羅(引)娑摩誐磋底(丁以反十三)摩訶野(十四)摩賀悉底(二合十五)諾乞(二合)曩(十六)鉢羅(二合)句跛野(引)弭(引十七)矩嚕矩嚕(十八)祖嚕祖嚕(十九)謨嚕謨嚕(二十)曩謨曩謨(二十一)佛告舍利弗。",
        "from": "T21n1269_001:0303a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是金色迦那鉢底除障難真言。",
        "from": "T21n1269_001:0303a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若眾生受持真言者。",
        "from": "T21n1269_001:0303a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所作諸法無不成。",
        "from": "T21n1269_001:0303a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大驗當知。",
        "from": "T21n1269_001:0303a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定有障難。",
        "from": "T21n1269_001:0303a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於白月若黑月二日八日十五日。",
        "from": "T21n1269_001:0303a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牛糞塗地作壇種種供養所作成就。",
        "from": "T21n1269_001:0303a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金色迦那鉢底白佛言。",
        "from": "T21n1269_001:0303a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當擁護持此真言者。",
        "from": "T21n1269_001:0303a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生持此真言者。",
        "from": "T21n1269_001:0303a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "某人所須資財臥具衣服飲食金銀珍寶奴婢從不舊乏少。",
        "from": "T21n1269_001:0303a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切真言法速令成所。",
        "from": "T21n1269_001:0303a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作就如意。",
        "from": "T21n1269_001:0303a28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其像形人身象頭六臂。",
        "from": "T21n1269_001:0303a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於白[疊*毛]上畫之。",
        "from": "T21n1269_001:0303a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刻作用得。",
        "from": "T21n1269_001:0303a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白檀紫檀苦練木通用。",
        "from": "T21n1269_001:0303b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘木不得。",
        "from": "T21n1269_001:0303b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若綵色勿用皮膠。",
        "from": "T21n1269_001:0303b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須用香汁及有汁木。",
        "from": "T21n1269_001:0303b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其身正立。",
        "from": "T21n1269_001:0303b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鼻向右曲。",
        "from": "T21n1269_001:0303b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左上手把刀。",
        "from": "T21n1269_001:0303b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次手把歡喜團。",
        "from": "T21n1269_001:0303b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下手把劍棒。",
        "from": "T21n1269_001:0303b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次手把縛折羅。",
        "from": "T21n1269_001:0303b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下手把索。",
        "from": "T21n1269_001:0303b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身作金色。",
        "from": "T21n1269_001:0303b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脚蹈金山。",
        "from": "T21n1269_001:0303b05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭上五色雲。",
        "from": "T21n1269_001:0303b05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雲內有四天王及諸仙散華。",
        "from": "T21n1269_001:0303b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左邊有俱摩羅淨軍。",
        "from": "T21n1269_001:0303b06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右邊有阿吒薄俱元率大將。",
        "from": "T21n1269_001:0303b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向下左畫美女。",
        "from": "T21n1269_001:0303b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "音樂供養立。",
        "from": "T21n1269_001:0303b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右畫四大藥叉。",
        "from": "T21n1269_001:0303b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各執器仗一半頭一猪頭一象頭一馬頭。",
        "from": "T21n1269_001:0303b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆著虎皮褌。",
        "from": "T21n1269_001:0303b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作像之時莫令見雜人等。",
        "from": "T21n1269_001:0303b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書畢。",
        "from": "T21n1269_001:0303b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒香清淨不食五辛。",
        "from": "T21n1269_001:0303b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於淨室中安置其壇。",
        "from": "T21n1269_001:0303b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方圓大小隨意作之。",
        "from": "T21n1269_001:0303b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壇中燒安悉香白膠沈水香等。",
        "from": "T21n1269_001:0303b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用隨餅乳粥歡喜團蘇蜜菓子種種供養。",
        "from": "T21n1269_001:0303b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所作隨心。",
        "from": "T21n1269_001:0303b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若鬼神難調伏者。",
        "from": "T21n1269_001:0303b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "搓白線呪之。",
        "from": "T21n1269_001:0303b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一遍一結滿四十九結。",
        "from": "T21n1269_001:0303b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫左臂。",
        "from": "T21n1269_001:0303b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切鬼悉皆被縛一受持人。",
        "from": "T21n1269_001:0303b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不須邪婬妄語作誑。",
        "from": "T21n1269_001:0303b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其禁食已止芸薹葫荽五辛。",
        "from": "T21n1269_001:0303b17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得食。",
        "from": "T21n1269_001:0303b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故入色孝家生產處。",
        "from": "T21n1269_001:0303b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印法并通用。",
        "from": "T21n1269_001:0303b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸毘那翼迦法通用。",
        "from": "T21n1269_001:0303b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿親惡人。",
        "from": "T21n1269_001:0303b19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同行者施之無妨。",
        "from": "T21n1269_001:0303b19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金色迦那鉢底經",
        "from": "T21n1269_001:0303b20_0",
        "context_with_next_sentence": false
    }
]