[
    {
        "source_sentence": "掌中論一卷",
        "from": "T31n1621_001:0884b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陳那菩薩造",
        "from": "T31n1621_001:0884b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏法師義淨奉　制譯",
        "from": "T31n1621_001:0884b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T31n1621_001:0884b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於三界但有假名。",
        "from": "T31n1621_001:0884b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實無外境。",
        "from": "T31n1621_001:0884b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由妄執故。",
        "from": "T31n1621_001:0884b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今欲為彼未證真者。",
        "from": "T31n1621_001:0884b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "決擇諸法自性之門。",
        "from": "T31n1621_001:0884b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令無倒解故造斯論。",
        "from": "T31n1621_001:0884b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌曰。",
        "from": "T31n1621_001:0884b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　於繩作蛇解　　見繩知境無",
        "from": "T31n1621_001:0884b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若了彼分時　　知如蛇解謬",
        "from": "T31n1621_001:0884b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T31n1621_001:0884b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如於非遠不分明處唯見繩蛇相似之事。",
        "from": "T31n1621_001:0884b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未能了彼差別自性。",
        "from": "T31n1621_001:0884b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被惑亂故。",
        "from": "T31n1621_001:0884b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定執為蛇。",
        "from": "T31n1621_001:0884b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後時了彼差別法已。",
        "from": "T31n1621_001:0884b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知由妄執誑亂生故。",
        "from": "T31n1621_001:0884b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但是錯解無有實事。",
        "from": "T31n1621_001:0884b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復於繩處支分差別。",
        "from": "T31n1621_001:0884b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善觀察時。",
        "from": "T31n1621_001:0884b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繩之自體。",
        "from": "T31n1621_001:0884b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不可得。",
        "from": "T31n1621_001:0884b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是知已。",
        "from": "T31n1621_001:0884b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有繩解。",
        "from": "T31n1621_001:0884b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如蛇覺。",
        "from": "T31n1621_001:0884b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯有妄識。",
        "from": "T31n1621_001:0884b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如於繩處有惑亂識。",
        "from": "T31n1621_001:0884b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦於彼分毫釐等處。",
        "from": "T31n1621_001:0884b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知相假藉。",
        "from": "T31n1621_001:0884b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無實可得。",
        "from": "T31n1621_001:0884b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故緣繩及分等心。",
        "from": "T31n1621_001:0884b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有相狀。",
        "from": "T31n1621_001:0884b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但唯妄識。",
        "from": "T31n1621_001:0884b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌曰。",
        "from": "T31n1621_001:0884b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　諸有假設事　　詳觀自性時",
        "from": "T31n1621_001:0884b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　從他皆假名　　乃至世俗境",
        "from": "T31n1621_001:0884b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T31n1621_001:0884c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如於繩等支分之處。",
        "from": "T31n1621_001:0884c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別別分析。",
        "from": "T31n1621_001:0884c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "審觀察時。",
        "from": "T31n1621_001:0884c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知無實體。",
        "from": "T31n1621_001:0884c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯是妄心。",
        "from": "T31n1621_001:0884c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是應知。",
        "from": "T31n1621_001:0884c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸法。",
        "from": "T31n1621_001:0884c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但是假名。",
        "from": "T31n1621_001:0884c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如瓶衣等物。",
        "from": "T31n1621_001:0884c03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藉埿縷等成。",
        "from": "T31n1621_001:0884c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至言說識所行境未至破壞。",
        "from": "T31n1621_001:0884c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為瓶等。",
        "from": "T31n1621_001:0884c04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言從他者。",
        "from": "T31n1621_001:0884c04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂從世俗言說而有。",
        "from": "T31n1621_001:0884c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非於勝義。",
        "from": "T31n1621_001:0884c05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌曰。",
        "from": "T31n1621_001:0884c05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無分非見故　　至極同非有",
        "from": "T31n1621_001:0884c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　但由惑亂心　　智者不應執",
        "from": "T31n1621_001:0884c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T31n1621_001:0884c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復執云。",
        "from": "T31n1621_001:0884c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸有假事。",
        "from": "T31n1621_001:0884c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至極微位不可分析。",
        "from": "T31n1621_001:0884c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復無方分是實有者。",
        "from": "T31n1621_001:0884c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此即猶如空花。",
        "from": "T31n1621_001:0884c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及兔角等。",
        "from": "T31n1621_001:0884c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可見故。",
        "from": "T31n1621_001:0884c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無力能生緣彼識故。",
        "from": "T31n1621_001:0884c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所執極微。",
        "from": "T31n1621_001:0884c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定非實有。",
        "from": "T31n1621_001:0884c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以須說不可見因。",
        "from": "T31n1621_001:0884c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由彼不能安立極微。",
        "from": "T31n1621_001:0884c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成實有故。",
        "from": "T31n1621_001:0884c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何。",
        "from": "T31n1621_001:0884c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由有方分事差別故。",
        "from": "T31n1621_001:0884c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如現見有瓶衣等物。",
        "from": "T31n1621_001:0884c13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東西北等。",
        "from": "T31n1621_001:0884c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方分別故。",
        "from": "T31n1621_001:0884c14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斯皆現有。",
        "from": "T31n1621_001:0884c14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "支分可得。",
        "from": "T31n1621_001:0884c14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若言極微是現有者。",
        "from": "T31n1621_001:0884c14_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必有方分。",
        "from": "T31n1621_001:0884c15_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別異性故。",
        "from": "T31n1621_001:0884c15_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是則應許東西北等。",
        "from": "T31n1621_001:0884c15_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "支分別故。",
        "from": "T31n1621_001:0884c16_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此實極微理不成就。",
        "from": "T31n1621_001:0884c16_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦非一體。",
        "from": "T31n1621_001:0884c16_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多分成故。",
        "from": "T31n1621_001:0884c17_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見事別故。",
        "from": "T31n1621_001:0884c17_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一實極微定不可得。",
        "from": "T31n1621_001:0884c17_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是應捨極微之論。",
        "from": "T31n1621_001:0884c18_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故智者了知三界咸是妄情。",
        "from": "T31n1621_001:0884c18_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲求妙理不應執實。",
        "from": "T31n1621_001:0884c19_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌曰。",
        "from": "T31n1621_001:0884c19_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　妄有非實故　　與所見不同",
        "from": "T31n1621_001:0884c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　由境相虛妄　　能緣亦非有",
        "from": "T31n1621_001:0884c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T31n1621_001:0884c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若言我亦於彼瓶衣等事。",
        "from": "T31n1621_001:0884c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "許彼自性是不可得。",
        "from": "T31n1621_001:0884c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆是妄識之所分別。",
        "from": "T31n1621_001:0884c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然而緣彼相狀亂識是其實有。",
        "from": "T31n1621_001:0884c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀健達婆城。",
        "from": "T31n1621_001:0884c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及幻人等。",
        "from": "T31n1621_001:0884c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其識是有。",
        "from": "T31n1621_001:0884c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設有此識。",
        "from": "T31n1621_001:0884c25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦非實故。",
        "from": "T31n1621_001:0884c25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與所見事不相應故。",
        "from": "T31n1621_001:0884c25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此惑亂識。",
        "from": "T31n1621_001:0884c26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於所緣境。",
        "from": "T31n1621_001:0884c26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作有性解。",
        "from": "T31n1621_001:0884c26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼事自性已明非有。",
        "from": "T31n1621_001:0884c26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "境既是無。",
        "from": "T31n1621_001:0884c27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能緣妄識亦非實有。",
        "from": "T31n1621_001:0884c27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何令彼妄識有耶。",
        "from": "T31n1621_001:0884c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然於世間不曾見有無能生種子有所生芽等。",
        "from": "T31n1621_001:0884c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由斯汝說幻城等喻道理不成。",
        "from": "T31n1621_001:0884c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌曰。",
        "from": "T31n1621_001:0885a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　斯皆是假設　　善覺者能知",
        "from": "T31n1621_001:0885a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　智人斷煩惱　　易若除蛇怖",
        "from": "T31n1621_001:0885a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T31n1621_001:0885a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如說三界但有假名。",
        "from": "T31n1621_001:0885a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瓶等麁覺既除遣已。",
        "from": "T31n1621_001:0885a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知從名言而有其事。",
        "from": "T31n1621_001:0885a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善觀察者。",
        "from": "T31n1621_001:0885a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能了知已。",
        "from": "T31n1621_001:0885a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於繩處蛇怖除遣。",
        "from": "T31n1621_001:0885a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復審思惟了彼差別於繩等處妄執亦無。",
        "from": "T31n1621_001:0885a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是觀時。",
        "from": "T31n1621_001:0885a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切能生離染之法。",
        "from": "T31n1621_001:0885a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "易速蠲除煩惱羅網。",
        "from": "T31n1621_001:0885a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸業果自當斷滅。",
        "from": "T31n1621_001:0885a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有別頌曰。",
        "from": "T31n1621_001:0885a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　智人觀俗事　　當隨俗所行",
        "from": "T31n1621_001:0885a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　欲求煩惱斷　　要明真勝義",
        "from": "T31n1621_001:0885a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如世人於諸俗事瓶衣等處。",
        "from": "T31n1621_001:0885a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為實有名瓶衣等。",
        "from": "T31n1621_001:0885a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智者亦爾。",
        "from": "T31n1621_001:0885a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當順世間而興言說。",
        "from": "T31n1621_001:0885a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知非實有。",
        "from": "T31n1621_001:0885a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若樂觀察煩惱過失。",
        "from": "T31n1621_001:0885a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求解脫者。",
        "from": "T31n1621_001:0885a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宜於如是真勝義中。",
        "from": "T31n1621_001:0885a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周遍推尋。",
        "from": "T31n1621_001:0885a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如理作意。",
        "from": "T31n1621_001:0885a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸境處。",
        "from": "T31n1621_001:0885a15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及能緣妄。",
        "from": "T31n1621_001:0885a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識煩惱繫縛。",
        "from": "T31n1621_001:0885a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不復生長。",
        "from": "T31n1621_001:0885a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掌中論一卷",
        "from": "T31n1621_001:0885a17_0",
        "context_with_next_sentence": false
    }
]