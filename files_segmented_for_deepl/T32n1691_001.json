        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以水同研。",
        "from": "T32n1691_001:0788b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後入乳汁相和煎熟。",
