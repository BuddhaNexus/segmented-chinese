[
    {
        "source_sentence": "迦葉仙人說醫女人經",
        "from": "T32n1691_001:0787b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試光祿",
        "from": "T32n1691_001:0787b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卿明教大師臣法賢奉　詔譯",
        "from": "T32n1691_001:0787b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時[口*尒]嚩迦仙人忽作是念。",
        "from": "T32n1691_001:0787b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間眾生皆從女人而生其身。",
        "from": "T32n1691_001:0787b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而彼女人從初懷孕至滿十月。",
        "from": "T32n1691_001:0787b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或復延胎至十二月方始產生。",
        "from": "T32n1691_001:0787c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於中間有其病患。",
        "from": "T32n1691_001:0787c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於病患時極受苦痛。",
        "from": "T32n1691_001:0787c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今方便請問於師。",
        "from": "T32n1691_001:0787c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稟受方藥與作救療。",
        "from": "T32n1691_001:0787c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是念已。",
        "from": "T32n1691_001:0787c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即詣於師迦葉仙人。",
        "from": "T32n1691_001:0787c03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伸師資禮而作問言。",
        "from": "T32n1691_001:0787c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大師迦葉是大智者。",
        "from": "T32n1691_001:0787c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今欲有所問願垂聽許。",
        "from": "T32n1691_001:0787c05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉仙言。",
        "from": "T32n1691_001:0787c05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恣汝所問。",
        "from": "T32n1691_001:0787c06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時[口*尒]嚩迦仙人白言。",
        "from": "T32n1691_001:0787c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "女人懷孕期當十月或十二月。",
        "from": "T32n1691_001:0787c06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日滿方生。",
        "from": "T32n1691_001:0787c07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何中間有諸病患逐致胎藏轉動不安。",
        "from": "T32n1691_001:0787c07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有損者苦惱無量。",
        "from": "T32n1691_001:0787c08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我師大智願為宣說救療如是病苦方藥。",
        "from": "T32n1691_001:0787c09_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是問已。",
        "from": "T32n1691_001:0787c10_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聽受而住。",
        "from": "T32n1691_001:0787c10_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時迦葉仙人。",
        "from": "T32n1691_001:0787c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告[口*尒]嚩迦仙言。",
        "from": "T32n1691_001:0787c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "女人懷孕不知保護。",
        "from": "T32n1691_001:0787c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂使胎藏得不安隱。",
        "from": "T32n1691_001:0787c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今為汝略說隨月保護之藥。",
        "from": "T32n1691_001:0787c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懷孕之人。",
        "from": "T32n1691_001:0787c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一月內胎藏不安者當。",
        "from": "T32n1691_001:0787c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用栴檀香蓮華優鉢羅花入水。",
        "from": "T32n1691_001:0787c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同研後入乳汁乳糖同煎。",
        "from": "T32n1691_001:0787c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "溫服此藥能令初懷孕者。",
        "from": "T32n1691_001:0787c15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無諸損惱而得安樂。",
        "from": "T32n1691_001:0787c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次告[口*尒]嚩迦仙言。",
        "from": "T32n1691_001:0787c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "女人懷孕於第二月胎藏不安者。",
        "from": "T32n1691_001:0787c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用青色憂鉢羅花。",
        "from": "T32n1691_001:0787c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱母那花根。",
        "from": "T32n1691_001:0787c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蔆角仁羯細嚕迦等藥。",
        "from": "T32n1691_001:0787c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸藥等分。",
        "from": "T32n1691_001:0787c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擣篩為粖用乳汁煎候冷。",
        "from": "T32n1691_001:0787c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "服之此藥能令胎藏。",
        "from": "T32n1691_001:0787c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不損疼痛止息晝夜安隱。",
        "from": "T32n1691_001:0787c20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次女人懷孕至第三月胎藏不安者。",
        "from": "T32n1691_001:0787c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用迦俱[日*(匕/示)*(入/米)]藥叱囉迦俱[口*(匕/示)*(入/米)]藥及蓖麻根等諸藥。",
        "from": "T32n1691_001:0787c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "等分以水相和研令極細。",
        "from": "T32n1691_001:0787c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又入乳汁同煎令熟。",
        "from": "T32n1691_001:0787c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後入乳糖及蜜。",
        "from": "T32n1691_001:0787c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相和冷服。",
        "from": "T32n1691_001:0787c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此藥能安。",
        "from": "T32n1691_001:0787c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "胎藏止息。",
        "from": "T32n1691_001:0787c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疼痛若有患者服之安樂。",
        "from": "T32n1691_001:0787c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次女人懷孕至第四月胎藏不安者。",
        "from": "T32n1691_001:0787c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用蒺蔾草根并枝葉等。",
        "from": "T32n1691_001:0787c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憂鉢羅花并及莖[卄/幹]等分用之。",
        "from": "T32n1691_001:0787c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以水相和研令極細。",
        "from": "T32n1691_001:0787c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復用乳汁同煎令熟。",
        "from": "T32n1691_001:0787c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "候冷服之。",
        "from": "T32n1691_001:0788a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此藥能安胎藏止息疼痛。",
        "from": "T32n1691_001:0788a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "患者服之而得安樂。",
        "from": "T32n1691_001:0788a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次女人懷孕至第五月胎藏不安者。",
        "from": "T32n1691_001:0788a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用瓠子根及憂鉢羅花。",
        "from": "T32n1691_001:0788a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各用等分擣篩令細。",
        "from": "T32n1691_001:0788a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後入蒲萄汁乳汁乳糖同煎候冷服之。",
        "from": "T32n1691_001:0788a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此藥能安胎藏止息疼痛。",
        "from": "T32n1691_001:0788a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "患者服之而得安樂。",
        "from": "T32n1691_001:0788a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次女人懷孕至第六月胎藏不安者。",
        "from": "T32n1691_001:0788a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用閉阿羅藥子摩地迦羅惹藥薩訖多嚩藥各用等分。",
        "from": "T32n1691_001:0788a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以水相和研令極細。",
        "from": "T32n1691_001:0788a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復入乳汁同煎。",
        "from": "T32n1691_001:0788a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後入乳糖及蜜。",
        "from": "T32n1691_001:0788a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "候冷服之。",
        "from": "T32n1691_001:0788a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此藥能安胎藏止息疼痛。",
        "from": "T32n1691_001:0788a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "患者服之而得安樂。",
        "from": "T32n1691_001:0788a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次女人懷孕至第七月胎藏不安者。",
        "from": "T32n1691_001:0788a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用蒺[卄/梨]草枝葉并根。",
        "from": "T32n1691_001:0788a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擣篩為粖。",
        "from": "T32n1691_001:0788a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用乳糖及蜜為丸。",
        "from": "T32n1691_001:0788a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用肉汁服之。",
        "from": "T32n1691_001:0788a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復以肉汁飱飯食之。",
        "from": "T32n1691_001:0788a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或食菉豆粥飯。",
        "from": "T32n1691_001:0788a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此藥及飯能安胎藏。",
        "from": "T32n1691_001:0788a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "患者服食而得安樂。",
        "from": "T32n1691_001:0788a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次女人懷孕至第八月胎藏不安者。",
        "from": "T32n1691_001:0788a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用三[牟*含]誐藥蓮花青憂鉢羅花蒺[卄/梨]草各等分以冷水相和研令極細。",
        "from": "T32n1691_001:0788a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後入乳汁及糖蜜等同煎。",
        "from": "T32n1691_001:0788a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "候冷服之。",
        "from": "T32n1691_001:0788a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此藥能安胎藏止息疼痛。",
        "from": "T32n1691_001:0788a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "患者服之而得安樂。",
        "from": "T32n1691_001:0788a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次女人懷孕至第九月胎藏不安者。",
        "from": "T32n1691_001:0788a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用蓖麻根迦俱[口*(匕/示)*(入/米)]藥舍羅鉢赧尼藥沒哩賀底藥。",
        "from": "T32n1691_001:0788a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各等分以冷水相和研令極細。",
        "from": "T32n1691_001:0788a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入乳汁同煎。",
        "from": "T32n1691_001:0788a24_3",
