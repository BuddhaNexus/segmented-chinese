    },
    {
        "source_sentence": "此藥能安胎藏止息疼痛。",
        "from": "T32n1691_001:0788a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "患者服之而得安樂。",
        "from": "T32n1691_001:0788a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次女人懷孕至第十月胎藏不安者。",
        "from": "T32n1691_001:0788a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用菉豆憂鉢羅花。",
        "from": "T32n1691_001:0788a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "等分以水相和研令極細。",
        "from": "T32n1691_001:0788a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復入乳糖及蜜并乳汁同煎。",
        "from": "T32n1691_001:0788a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "候冷服之。",
        "from": "T32n1691_001:0788a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此藥能安胎藏止息疼痛。",
        "from": "T32n1691_001:0788a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "患者服之而得安樂。",
        "from": "T32n1691_001:0788b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次女人懷孕延胎十一月胎藏不安者。",
        "from": "T32n1691_001:0788b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用青憂鉢羅花娑路剛藥蓮花并莖。",
        "from": "T32n1691_001:0788b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "等分以冷水相和研令極細。",
        "from": "T32n1691_001:0788b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後入乳汁乳糖同煎。",
        "from": "T32n1691_001:0788b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "候冷服之。",
        "from": "T32n1691_001:0788b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此藥能安胎藏止息疼痛。",
        "from": "T32n1691_001:0788b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "患者服之而得安樂。",
        "from": "T32n1691_001:0788b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次女人懷孕延至第十二月胎藏不安者。",
        "from": "T32n1691_001:0788b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用迦俱[口*(匕/示)*(入/米)]藥叱囉迦俱[口*(匕/示)*(入/米)]藥甘草憂鉢羅花。",
        "from": "T32n1691_001:0788b07_1",
