[
    {
        "source_sentence": "大乘中觀釋論卷第五",
        "from": "T30n1567_005:0146a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安慧菩薩造",
        "from": "T30n1567_005:0146a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試鴻臚卿傳",
        "from": "T30n1567_005:0146a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵大師賜紫沙門臣法護等奉　詔譯",
        "from": "T30n1567_005:0146a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "◎觀染法染者品第六",
        "from": "T30n1567_005:0146a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次有異宗言。",
        "from": "T30n1567_005:0146a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於勝義諦中諸界處等性與無性有所知解。",
        "from": "T30n1567_005:0146a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由彼雜染成其有性。",
        "from": "T30n1567_005:0146a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如佛所言。",
        "from": "T30n1567_005:0146a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "染者著染法故。",
        "from": "T30n1567_005:0146a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行自損害行他損害行俱損害。",
        "from": "T30n1567_005:0146a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是乃至癡者著於癡法。",
        "from": "T30n1567_005:0146a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其義亦然。",
        "from": "T30n1567_005:0146a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論者言。",
        "from": "T30n1567_005:0146a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中非無雜染道理。",
        "from": "T30n1567_005:0146a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖有如幻。",
        "from": "T30n1567_005:0146a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂以無實染等體故。",
        "from": "T30n1567_005:0146a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但以行聚所成。",
        "from": "T30n1567_005:0146a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世俗言說。",
        "from": "T30n1567_005:0146a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非勝義諦。",
        "from": "T30n1567_005:0146a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何所以邪。",
        "from": "T30n1567_005:0146a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此言染法。",
        "from": "T30n1567_005:0146a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "染者為先有邪。",
        "from": "T30n1567_005:0146a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為後有邪。",
        "from": "T30n1567_005:0146a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為染者染法俱時起邪。",
        "from": "T30n1567_005:0146a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三皆不然。",
        "from": "T30n1567_005:0146a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故頌言。",
        "from": "T30n1567_005:0146a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若先有染者　　後有其染法",
        "from": "T30n1567_005:0146a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　云何離染法　　而有染者生",
        "from": "T30n1567_005:0146a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋曰。",
        "from": "T30n1567_005:0146a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此說畢竟應知此義遮其所離。",
        "from": "T30n1567_005:0146a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若染法無體。",
        "from": "T30n1567_005:0146a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "染者亦然。",
        "from": "T30n1567_005:0146a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "止其所作非熟無果有果成熟。",
        "from": "T30n1567_005:0146a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其義可見。",
        "from": "T30n1567_005:0146a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若離染法有染者成。",
        "from": "T30n1567_005:0146a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是乃應別有染法。",
        "from": "T30n1567_005:0146a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此即染者因染法得。",
        "from": "T30n1567_005:0146a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若爾云何染者有故染法即成。",
        "from": "T30n1567_005:0146a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "染法有故染者即成。",
        "from": "T30n1567_005:0146a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後起染。",
        "from": "T30n1567_005:0146a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此義顯明。",
        "from": "T30n1567_005:0146a25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如果成熟。",
        "from": "T30n1567_005:0146a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有愛境即染法成。",
        "from": "T30n1567_005:0146a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無愛境云何有染。",
        "from": "T30n1567_005:0146a26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當止遣安立過失。",
        "from": "T30n1567_005:0146a26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中亦非先有染者後起染法。",
        "from": "T30n1567_005:0146a27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故頌遣言。",
        "from": "T30n1567_005:0146a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　有染者復染　　云何當可得",
        "from": "T30n1567_005:0146a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋曰。",
        "from": "T30n1567_005:0146b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所依染法無體性故。",
        "from": "T30n1567_005:0146b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如已成熟。",
        "from": "T30n1567_005:0146b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次頌言。",
        "from": "T30n1567_005:0146b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若有若無染　　染者亦復然",
        "from": "T30n1567_005:0146b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋曰。",
        "from": "T30n1567_005:0146b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若先有染者後有染法。",
        "from": "T30n1567_005:0146b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即一切處。",
        "from": "T30n1567_005:0146b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如染法體。",
        "from": "T30n1567_005:0146b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故先無染者道理。",
        "from": "T30n1567_005:0146b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令為證成此義。",
        "from": "T30n1567_005:0146b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故頌言。",
        "from": "T30n1567_005:0146b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　染者先有染　　離染者染成",
        "from": "T30n1567_005:0146b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋曰。",
        "from": "T30n1567_005:0146b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "染法若無愛體可作。",
        "from": "T30n1567_005:0146b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是中其或見有所成。",
        "from": "T30n1567_005:0146b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此非道理。",
        "from": "T30n1567_005:0146b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有異宗言。",
        "from": "T30n1567_005:0146b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離彼染者別有染法。",
        "from": "T30n1567_005:0146b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離彼染法染者可立。",
        "from": "T30n1567_005:0146b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論者言。",
        "from": "T30n1567_005:0146b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若離染法得有染者。",
        "from": "T30n1567_005:0146b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中非有。",
        "from": "T30n1567_005:0146b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何所以邪。",
        "from": "T30n1567_005:0146b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若先有染法後有染者即離染者乃有染法。",
        "from": "T30n1567_005:0146b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中染法不有。",
        "from": "T30n1567_005:0146b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即無愛境能成染法道理。",
        "from": "T30n1567_005:0146b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以愛境後有故。",
        "from": "T30n1567_005:0146b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由如是故。",
        "from": "T30n1567_005:0146b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若離自性別因染者。",
        "from": "T30n1567_005:0146b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得有染法。",
        "from": "T30n1567_005:0146b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即彼染法因染者得。",
        "from": "T30n1567_005:0146b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若爾即非愛境能成染法。",
        "from": "T30n1567_005:0146b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以愛境居後故。",
        "from": "T30n1567_005:0146b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所成不可得。",
        "from": "T30n1567_005:0146b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若如此者豈非過邪。",
        "from": "T30n1567_005:0146b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦非先有染法後成染者。",
        "from": "T30n1567_005:0146b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次頌言。",
        "from": "T30n1567_005:0146b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　有染復染者　　亦云何當得",
        "from": "T30n1567_005:0146b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T30n1567_005:0146b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或離染法有其染者。",
        "from": "T30n1567_005:0146b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既有對待過失。",
        "from": "T30n1567_005:0146b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中染法染者二法同起而可成邪。",
        "from": "T30n1567_005:0146b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故頌答言。",
        "from": "T30n1567_005:0146b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如是若同生　　亦復非道理",
        "from": "T30n1567_005:0146b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　染法染者二　　此當云何用",
        "from": "T30n1567_005:0146b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋曰。",
        "from": "T30n1567_005:0146b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂以染法染者二相離性互有所違。",
        "from": "T30n1567_005:0146b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若法未生二俱無性。",
        "from": "T30n1567_005:0146b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若法已生所作無體。",
        "from": "T30n1567_005:0146b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但由愛境所成染法故有染者。",
        "from": "T30n1567_005:0146b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而彼愛境及所起染法悉亦止遣。",
        "from": "T30n1567_005:0146b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T30n1567_005:0146c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼染法染者為一性可合邪。",
        "from": "T30n1567_005:0146c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為異性合邪。",
        "from": "T30n1567_005:0146c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故頌答言。",
        "from": "T30n1567_005:0146c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　彼染法染者　　非一性有合",
        "from": "T30n1567_005:0146c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋曰。",
        "from": "T30n1567_005:0146c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此何所以。",
        "from": "T30n1567_005:0146c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非彼一性而可合故。",
        "from": "T30n1567_005:0146c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有二法合即極成。",
        "from": "T30n1567_005:0146c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中應知。",
        "from": "T30n1567_005:0146c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是提婆達多起染。",
        "from": "T30n1567_005:0146c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不復為彼提婆達多染者之因。",
        "from": "T30n1567_005:0146c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼若說合。",
        "from": "T30n1567_005:0146c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此非道理。",
        "from": "T30n1567_005:0146c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次異性亦不可合。",
        "from": "T30n1567_005:0146c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故頌遣言。",
        "from": "T30n1567_005:0146c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　異性若有合　　云何當可得",
        "from": "T30n1567_005:0146c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋曰。",
        "from": "T30n1567_005:0146c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "異法異性若有合者。",
        "from": "T30n1567_005:0146c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼即相違而非一處。",
        "from": "T30n1567_005:0146c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有二法生道理可得。",
        "from": "T30n1567_005:0146c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後當止遣。",
        "from": "T30n1567_005:0146c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次頌言。",
        "from": "T30n1567_005:0146c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若一性可合　　離伴亦應合",
        "from": "T30n1567_005:0146c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋曰。",
        "from": "T30n1567_005:0146c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此言合者。",
        "from": "T30n1567_005:0146c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同體為義。",
        "from": "T30n1567_005:0146c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡一性者即是因義。",
        "from": "T30n1567_005:0146c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若此一性定有合者。",
        "from": "T30n1567_005:0146c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前所說提婆達多。",
        "from": "T30n1567_005:0146c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合義應見。",
        "from": "T30n1567_005:0146c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故無一性因。",
        "from": "T30n1567_005:0146c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可合道理。",
        "from": "T30n1567_005:0146c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖於一性因無合可成。",
        "from": "T30n1567_005:0146c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然若止其合義。",
        "from": "T30n1567_005:0146c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應知有過。",
        "from": "T30n1567_005:0146c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次頌言。",
        "from": "T30n1567_005:0146c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　異性若有合　　離伴亦應合",
        "from": "T30n1567_005:0146c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋曰。",
        "from": "T30n1567_005:0146c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中所說。",
        "from": "T30n1567_005:0146c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如瓶衣。",
        "from": "T30n1567_005:0146c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼等異性而不能合。",
        "from": "T30n1567_005:0146c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若立合者。",
        "from": "T30n1567_005:0146c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非有異性相合因故。",
        "from": "T30n1567_005:0146c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前即有對待過失。",
        "from": "T30n1567_005:0146c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "異性若合有所得者。",
        "from": "T30n1567_005:0146c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼非異性亦應得合。",
        "from": "T30n1567_005:0146c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此豈無過。",
        "from": "T30n1567_005:0146c24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次頌言。",
        "from": "T30n1567_005:0146c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若異性有合　　染染者何用",
        "from": "T30n1567_005:0146c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋曰。",
        "from": "T30n1567_005:0146c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中說合。",
        "from": "T30n1567_005:0146c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無合道理。",
        "from": "T30n1567_005:0146c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何所以邪。",
        "from": "T30n1567_005:0146c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是二法各各已成別別自體。",
        "from": "T30n1567_005:0146c27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即無別法為所成義。",
        "from": "T30n1567_005:0146c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是中亦無所成可得。",
        "from": "T30n1567_005:0146c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故知異性無有詮表。",
        "from": "T30n1567_005:0146c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若計異性得有合者。",
        "from": "T30n1567_005:0146c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故頌遣言。",
        "from": "T30n1567_005:0147a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若染染者二　　各各自體成",
        "from": "T30n1567_005:0147a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　是二若有合　　前亦應得合",
        "from": "T30n1567_005:0147a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋曰。",
        "from": "T30n1567_005:0147a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而彼二法汝今云何。",
        "from": "T30n1567_005:0147a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此說得成各各體故。",
        "from": "T30n1567_005:0147a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若或染法染者。",
        "from": "T30n1567_005:0147a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是二汝以何義分別二法各各自體令成其合論中言或者。",
        "from": "T30n1567_005:0147a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此說合義。",
        "from": "T30n1567_005:0147a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若起分別時虛無果利。",
        "from": "T30n1567_005:0147a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼等自體無所成性。",
        "from": "T30n1567_005:0147a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若言合者。",
        "from": "T30n1567_005:0147a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是中染者。",
        "from": "T30n1567_005:0147a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有少分染法可作染法。",
        "from": "T30n1567_005:0147a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦非染者可有。",
        "from": "T30n1567_005:0147a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次頌言。",
        "from": "T30n1567_005:0147a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　異相不成合　　汝欲求成合",
        "from": "T30n1567_005:0147a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　合相若已成　　復欲成其異",
        "from": "T30n1567_005:0147a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋曰。",
        "from": "T30n1567_005:0147a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合性不成。",
        "from": "T30n1567_005:0147a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼義極成。",
        "from": "T30n1567_005:0147a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一法異性合不可得。",
        "from": "T30n1567_005:0147a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "異性極成不成合義。",
        "from": "T30n1567_005:0147a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故無染法染者二法同時。",
        "from": "T30n1567_005:0147a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或復次第異性可生。",
        "from": "T30n1567_005:0147a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦復更互相離性故。",
        "from": "T30n1567_005:0147a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由是此中無合道理。",
        "from": "T30n1567_005:0147a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此觀察如汝意欲。",
        "from": "T30n1567_005:0147a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等異性次第可起。",
        "from": "T30n1567_005:0147a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或復同時。",
        "from": "T30n1567_005:0147a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今為證成此義。",
        "from": "T30n1567_005:0147a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故頌遣言。",
        "from": "T30n1567_005:0147a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如是染染者　　非合不合成",
        "from": "T30n1567_005:0147a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　諸法亦如染　　非合不合成",
        "from": "T30n1567_005:0147a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋曰。",
        "from": "T30n1567_005:0147a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中云何。",
        "from": "T30n1567_005:0147a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂非唯染法染者。",
        "from": "T30n1567_005:0147a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非合不合。",
        "from": "T30n1567_005:0147a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸法亦然。",
        "from": "T30n1567_005:0147a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "◎◎觀有為品第七之一復次或有人言。",
        "from": "T30n1567_005:0147a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勝義諦中有彼貪等諸雜染法。",
        "from": "T30n1567_005:0147a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以有為故。",
        "from": "T30n1567_005:0147a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如眼等。",
        "from": "T30n1567_005:0147a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論者言。",
        "from": "T30n1567_005:0147a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若如是說。",
        "from": "T30n1567_005:0147a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍所成故立喻不正。",
        "from": "T30n1567_005:0147a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故。",
        "from": "T30n1567_005:0147a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勝義諦中若有眼等。",
        "from": "T30n1567_005:0147a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應有生等有為諸相。",
        "from": "T30n1567_005:0147a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若其無者。",
        "from": "T30n1567_005:0147a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如兔角等。",
        "from": "T30n1567_005:0147a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦應有彼有為諸相。",
        "from": "T30n1567_005:0147a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故有為諸相。",
        "from": "T30n1567_005:0147a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於勝義諦中決定不成。",
        "from": "T30n1567_005:0147b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何生等有為諸相而可成立。",
        "from": "T30n1567_005:0147b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中應問。",
        "from": "T30n1567_005:0147b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼生等法增上所作。",
        "from": "T30n1567_005:0147b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是有為耶。",
        "from": "T30n1567_005:0147b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是無為耶。",
        "from": "T30n1567_005:0147b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二俱不然。",
        "from": "T30n1567_005:0147b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故頌答言。",
        "from": "T30n1567_005:0147b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若生是有為　　即應有三相",
        "from": "T30n1567_005:0147b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋曰。",
        "from": "T30n1567_005:0147b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如能相。",
        "from": "T30n1567_005:0147b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中有為能相如是。",
        "from": "T30n1567_005:0147b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不然云何有為諸相。",
        "from": "T30n1567_005:0147b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中所說二法和合三相遍行故。",
        "from": "T30n1567_005:0147b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中三相亦悉止遣彼法自相。",
        "from": "T30n1567_005:0147b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次頌言。",
        "from": "T30n1567_005:0147b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若生是無為　　不作有為相",
        "from": "T30n1567_005:0147b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋曰。",
        "from": "T30n1567_005:0147b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是所成應當遮遣安立過失。",
        "from": "T30n1567_005:0147b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故。",
        "from": "T30n1567_005:0147b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無為自體亦無性故。",
        "from": "T30n1567_005:0147b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如滅法。",
        "from": "T30n1567_005:0147b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此如是決定。",
        "from": "T30n1567_005:0147b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有如是過失。",
        "from": "T30n1567_005:0147b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是住與無常。",
        "from": "T30n1567_005:0147b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘法亦然。",
        "from": "T30n1567_005:0147b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆同生法。",
        "from": "T30n1567_005:0147b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論者言。",
        "from": "T30n1567_005:0147b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相者即是理法執持為性。",
        "from": "T30n1567_005:0147b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是所說。",
        "from": "T30n1567_005:0147b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何生等為復相離。",
        "from": "T30n1567_005:0147b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有彼業用而可轉耶。",
        "from": "T30n1567_005:0147b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或不離耶。",
        "from": "T30n1567_005:0147b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次頌言。",
        "from": "T30n1567_005:0147b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　生等三法離　　即無相業用",
        "from": "T30n1567_005:0147b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋曰。",
        "from": "T30n1567_005:0147b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何生等有為諸法次第可轉。",
        "from": "T30n1567_005:0147b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若法體未生。",
        "from": "T30n1567_005:0147b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即住滅二法不能為彼作有為相。",
        "from": "T30n1567_005:0147b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以未生故。",
        "from": "T30n1567_005:0147b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是即能相無所詮表。",
        "from": "T30n1567_005:0147b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若法自體畢竟已滅。",
        "from": "T30n1567_005:0147b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即能相無體故無生住二法。",
        "from": "T30n1567_005:0147b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以彼滅法自無性故。",
        "from": "T30n1567_005:0147b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已生即無住。",
        "from": "T30n1567_005:0147b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無住即無生滅亦無性。",
        "from": "T30n1567_005:0147b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若言無常隨逐住法。",
        "from": "T30n1567_005:0147b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即不能作有為之相。",
        "from": "T30n1567_005:0147b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故百論頌言。",
        "from": "T30n1567_005:0147b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　住何有滅相　　無常何有住",
        "from": "T30n1567_005:0147b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若先有住法　　後不復應有",
        "from": "T30n1567_005:0147b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若常有無常　　有住不有常",
        "from": "T30n1567_005:0147c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　或先有其常　　後即不有常",
        "from": "T30n1567_005:0147c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無常與住同　　若有其體者",
        "from": "T30n1567_005:0147c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　有常即邪妄　　或有住亦妄",
        "from": "T30n1567_005:0147c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌意如是。",
        "from": "T30n1567_005:0147c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有為法不相離者。",
        "from": "T30n1567_005:0147c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何以一物體於一時中而得和合。",
        "from": "T30n1567_005:0147c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故頌遣言。",
        "from": "T30n1567_005:0147c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　不於一時中　　生住滅和合",
        "from": "T30n1567_005:0147c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋曰。",
        "from": "T30n1567_005:0147c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以互相違故。",
        "from": "T30n1567_005:0147c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有人言。",
        "from": "T30n1567_005:0147c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有彼同種因性一時可生。",
        "from": "T30n1567_005:0147c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或復次第所作得成。",
        "from": "T30n1567_005:0147c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論者言。",
        "from": "T30n1567_005:0147c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此復云何。",
        "from": "T30n1567_005:0147c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或復於彼有體中得耶。",
        "from": "T30n1567_005:0147c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或離能相所相有所得耶。",
        "from": "T30n1567_005:0147c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有體得者。",
        "from": "T30n1567_005:0147c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非於有體中有實體性同生可得。",
        "from": "T30n1567_005:0147c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦非所作別有其因。",
        "from": "T30n1567_005:0147c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若或無體。",
        "from": "T30n1567_005:0147c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此即亦無同生可有。",
        "from": "T30n1567_005:0147c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故體中無有如是決定生等。",
        "from": "T30n1567_005:0147c15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如生自相。",
        "from": "T30n1567_005:0147c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦復云何次第可成。",
        "from": "T30n1567_005:0147c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次頌言。",
        "from": "T30n1567_005:0147c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　生住滅諸相　　別有有為相",
        "from": "T30n1567_005:0147c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　有即是無窮　　無即非有為",
        "from": "T30n1567_005:0147c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋曰。",
        "from": "T30n1567_005:0147c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若離生等別有生法而可成者。",
        "from": "T30n1567_005:0147c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼即定有異法可立。",
        "from": "T30n1567_005:0147c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是乃有無窮之過。",
        "from": "T30n1567_005:0147c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有為無為之相故非和合。",
        "from": "T30n1567_005:0147c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次犢子部師言。",
        "from": "T30n1567_005:0147c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生等諸法雖是有為。",
        "from": "T30n1567_005:0147c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何可說為無窮耶。",
        "from": "T30n1567_005:0147c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼宗頌言。",
        "from": "T30n1567_005:0147c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　生生之所生　　唯生於本生",
        "from": "T30n1567_005:0147c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　本生之所生　　復生於生生",
        "from": "T30n1567_005:0147c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此頌意者。",
        "from": "T30n1567_005:0147c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸法生時并法自體。",
        "from": "T30n1567_005:0147c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有十五法共成生法。",
        "from": "T30n1567_005:0147c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即彼如是有生住異滅諸法具足。",
        "from": "T30n1567_005:0148a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此如是法無有差別。",
        "from": "T30n1567_005:0148a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若差別分別者有十五法。",
        "from": "T30n1567_005:0148a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂一生。",
        "from": "T30n1567_005:0148a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二住。",
        "from": "T30n1567_005:0148a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三滅。",
        "from": "T30n1567_005:0148a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四若是白法。",
        "from": "T30n1567_005:0148a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即正解脫生。",
        "from": "T30n1567_005:0148a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五若是黑法。",
        "from": "T30n1567_005:0148a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即邪解脫生。",
        "from": "T30n1567_005:0148a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六若非出離法。",
        "from": "T30n1567_005:0148a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即非出離法生。",
        "from": "T30n1567_005:0148a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七若是出離法。",
        "from": "T30n1567_005:0148a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即出離法生。",
        "from": "T30n1567_005:0148a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八生生。",
        "from": "T30n1567_005:0148a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九住住。",
        "from": "T30n1567_005:0148a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十滅滅。",
        "from": "T30n1567_005:0148a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十一正解脫眷屬。",
        "from": "T30n1567_005:0148a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二邪解脫眷屬。",
        "from": "T30n1567_005:0148a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十三非出離法眷屬。",
        "from": "T30n1567_005:0148a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十四出離法眷屬。",
        "from": "T30n1567_005:0148a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除本生自體成十四法。",
        "from": "T30n1567_005:0148a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若并本生法總成十五。",
        "from": "T30n1567_005:0148a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生生及本生二法為始意。",
        "from": "T30n1567_005:0148a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為此中生生所生唯本生生。",
        "from": "T30n1567_005:0148a09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更無別法本生所生還生生生。",
        "from": "T30n1567_005:0148a10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是乃生諸餘法等。",
        "from": "T30n1567_005:0148a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此即不墮無窮之過。",
        "from": "T30n1567_005:0148a11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論者言。",
        "from": "T30n1567_005:0148a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是所說皆非道理。",
        "from": "T30n1567_005:0148a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故頌破言。",
        "from": "T30n1567_005:0148a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若謂是生生　　能生於本生",
        "from": "T30n1567_005:0148a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　生生從本生　　何能生本生",
        "from": "T30n1567_005:0148a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋曰。",
        "from": "T30n1567_005:0148a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自體如是無有性故。",
        "from": "T30n1567_005:0148a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次頌破言。",
        "from": "T30n1567_005:0148a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若謂是本生　　能生於生生",
        "from": "T30n1567_005:0148a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　本生從彼生　　何能生生生",
        "from": "T30n1567_005:0148a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋曰。",
        "from": "T30n1567_005:0148a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自體如是無有性故。",
        "from": "T30n1567_005:0148a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復有言。",
        "from": "T30n1567_005:0148a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生生生時。",
        "from": "T30n1567_005:0148a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即當能生。",
        "from": "T30n1567_005:0148a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以無別故。",
        "from": "T30n1567_005:0148a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次頌破言。",
        "from": "T30n1567_005:0148a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若謂生生時　　能生本生者",
        "from": "T30n1567_005:0148a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　生生若未生　　何能生本生",
        "from": "T30n1567_005:0148a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋曰。",
        "from": "T30n1567_005:0148a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生生若未生。",
        "from": "T30n1567_005:0148a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生時即無體。",
        "from": "T30n1567_005:0148a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無體即不生。",
        "from": "T30n1567_005:0148a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有何力能能生本生。",
        "from": "T30n1567_005:0148a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無力能即無詮表。",
        "from": "T30n1567_005:0148a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "◎大乘中觀釋論卷第五",
        "from": "T30n1567_005:0148a27_2",
        "context_with_next_sentence": false
    }
]