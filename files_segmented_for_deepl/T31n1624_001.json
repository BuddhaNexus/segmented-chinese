[
    {
        "source_sentence": "觀所緣緣論",
        "from": "T31n1624_001:0888b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陳那菩薩造",
        "from": "T31n1624_001:0888b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏法師玄奘奉　詔譯",
        "from": "T31n1624_001:0888b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸有欲令眼等五識。",
        "from": "T31n1624_001:0888b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以外色作所緣緣者。",
        "from": "T31n1624_001:0888b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或執極微許有實體。",
        "from": "T31n1624_001:0888b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能生識故或執和合。",
        "from": "T31n1624_001:0888b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以識生時帶彼相故。",
        "from": "T31n1624_001:0888b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二俱非理。",
        "from": "T31n1624_001:0888b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何。",
        "from": "T31n1624_001:0888b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　極微於五識　　設緣非所緣",
        "from": "T31n1624_001:0888b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　彼相識無故　　猶如眼根等",
        "from": "T31n1624_001:0888b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所緣緣者。",
        "from": "T31n1624_001:0888b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂能緣識帶彼相起及有實體。",
        "from": "T31n1624_001:0888b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令能緣識託彼而生色等極微。",
        "from": "T31n1624_001:0888b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設有實體能生五識容有緣義。",
        "from": "T31n1624_001:0888b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然非所緣。",
        "from": "T31n1624_001:0888b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如眼根等於眼等識無彼相故。",
        "from": "T31n1624_001:0888b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是極微於眼等識。",
        "from": "T31n1624_001:0888b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無所緣義。",
        "from": "T31n1624_001:0888b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　和合於五識　　設所緣非緣",
        "from": "T31n1624_001:0888b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　彼體實無故　　猶如第二月",
        "from": "T31n1624_001:0888b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色等和合於眼識等有彼相故。",
        "from": "T31n1624_001:0888b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設作所緣然無緣義。",
        "from": "T31n1624_001:0888b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如眼錯亂見第二月。",
        "from": "T31n1624_001:0888b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼無實體不能生故。",
        "from": "T31n1624_001:0888b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是和合於眼等識無有緣義故。",
        "from": "T31n1624_001:0888b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外二事於所緣緣互闕一支。",
        "from": "T31n1624_001:0888b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱不應理。",
        "from": "T31n1624_001:0888b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有執色等各有多相。",
        "from": "T31n1624_001:0888b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中一分是現量境故。",
        "from": "T31n1624_001:0888b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸極微相資各有一和集相。",
        "from": "T31n1624_001:0888b22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此相實有各能發生。",
        "from": "T31n1624_001:0888b23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "似己相識故與五識作所緣緣。",
        "from": "T31n1624_001:0888b23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此亦非理。",
        "from": "T31n1624_001:0888b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何。",
        "from": "T31n1624_001:0888b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　和集如堅等　　設於眼等識",
        "from": "T31n1624_001:0888b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　是緣非所緣　　許極微相故",
        "from": "T31n1624_001:0888b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如堅等相雖是實有。",
        "from": "T31n1624_001:0888b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於眼等識容有緣義。",
        "from": "T31n1624_001:0888b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而非所緣。",
        "from": "T31n1624_001:0888b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼等識上無彼相故。",
        "from": "T31n1624_001:0888b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色等極微諸和集相。",
        "from": "T31n1624_001:0888b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "理亦應爾。",
        "from": "T31n1624_001:0888b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼俱執為極微相故。",
        "from": "T31n1624_001:0888b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執眼等識能緣極微。",
        "from": "T31n1624_001:0888b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸和集相復有別失。",
        "from": "T31n1624_001:0888c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　瓶甌等覺相　　彼執應無別",
        "from": "T31n1624_001:0888c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　非形別故別　　形別非實故",
        "from": "T31n1624_001:0888c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瓶甌等物大小等者。",
        "from": "T31n1624_001:0888c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能成極微多少同故。",
        "from": "T31n1624_001:0888c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣彼覺相應無差別。",
        "from": "T31n1624_001:0888c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若謂彼物形相別故覺相別者。",
        "from": "T31n1624_001:0888c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "理亦不然。",
        "from": "T31n1624_001:0888c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂等別形唯在瓶等假法上有。",
        "from": "T31n1624_001:0888c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非極微故彼不應執。",
        "from": "T31n1624_001:0888c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "極微亦有差別形相所以者何。",
        "from": "T31n1624_001:0888c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　極微量等故　　形別惟在假",
        "from": "T31n1624_001:0888c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　析彼至極微　　彼覺定捨故",
        "from": "T31n1624_001:0888c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非瓶甌等能成極微有形量別。",
        "from": "T31n1624_001:0888c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨微圓相故。",
        "from": "T31n1624_001:0888c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知別形在假非實。",
        "from": "T31n1624_001:0888c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又形別物析至極微。",
        "from": "T31n1624_001:0888c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼覺定捨非青等物。",
        "from": "T31n1624_001:0888c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "析至極微彼覺可捨。",
        "from": "T31n1624_001:0888c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此形別唯世俗有。",
        "from": "T31n1624_001:0888c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非如青等亦在實物。",
        "from": "T31n1624_001:0888c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故五識所緣緣體非外色等其理極成。",
        "from": "T31n1624_001:0888c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼所緣緣豈全不有。",
        "from": "T31n1624_001:0888c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非全不有。",
        "from": "T31n1624_001:0888c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若爾云何。",
        "from": "T31n1624_001:0888c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　內色如外現　　為識所緣緣",
        "from": "T31n1624_001:0888c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　許彼相在識　　及能生識故",
        "from": "T31n1624_001:0888c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外境雖無。",
        "from": "T31n1624_001:0888c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而有內色似外境現。",
        "from": "T31n1624_001:0888c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為所緣緣。",
        "from": "T31n1624_001:0888c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "許眼等識帶彼相起及從彼生。",
        "from": "T31n1624_001:0888c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具二義故。",
        "from": "T31n1624_001:0888c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此內境相既不離識。",
        "from": "T31n1624_001:0888c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如何俱起。",
        "from": "T31n1624_001:0888c21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能作識緣。",
        "from": "T31n1624_001:0888c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　決定相隨故　　俱時亦作緣",
        "from": "T31n1624_001:0888c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　或前為後緣　　引彼功能故",
        "from": "T31n1624_001:0888c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "境相與識定相隨故。",
        "from": "T31n1624_001:0888c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖俱時起亦作識緣。",
        "from": "T31n1624_001:0888c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因明者說。",
        "from": "T31n1624_001:0888c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若此與彼有無相隨。",
        "from": "T31n1624_001:0888c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖俱時生而亦得有因果相故。",
        "from": "T31n1624_001:0888c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或前識相為後識緣。",
        "from": "T31n1624_001:0888c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "引本識中生似自果功能令起不違理故。",
        "from": "T31n1624_001:0888c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若五識生唯緣內色。",
        "from": "T31n1624_001:0888c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如何亦說眼等為緣。",
        "from": "T31n1624_001:0888c28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　識上色功能　　名五根應理",
        "from": "T31n1624_001:0888c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　功能與境色　　無始互為因",
        "from": "T31n1624_001:0889a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以能發識比知有根。",
        "from": "T31n1624_001:0889a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此但功能非外所造故。",
        "from": "T31n1624_001:0889a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本識上五色功能名眼等根。",
        "from": "T31n1624_001:0889a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不違理。",
        "from": "T31n1624_001:0889a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功能發識理無別故。",
        "from": "T31n1624_001:0889a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在識在餘雖不可說。",
        "from": "T31n1624_001:0889a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而外諸法理非有故。",
        "from": "T31n1624_001:0889a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定應許此在識非餘。",
        "from": "T31n1624_001:0889a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此根功能與前境色。",
        "from": "T31n1624_001:0889a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從無始際展轉為因。",
        "from": "T31n1624_001:0889a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂此功能至成熟位。",
        "from": "T31n1624_001:0889a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生現識上五內境色。",
        "from": "T31n1624_001:0889a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此內境色復能引起異熟識上五根功能。",
        "from": "T31n1624_001:0889a07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "根境二色與識一異或非一異。",
        "from": "T31n1624_001:0889a08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨樂應說。",
        "from": "T31n1624_001:0889a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是諸識。",
        "from": "T31n1624_001:0889a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟內境相為所緣緣。",
        "from": "T31n1624_001:0889a09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "理善成立。",
        "from": "T31n1624_001:0889a10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀所緣緣論",
        "from": "T31n1624_001:0889a11_0",
        "context_with_next_sentence": false
    }
]