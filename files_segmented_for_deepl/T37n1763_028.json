[
    {
        "source_sentence": "大般涅槃經集解卷第二十八",
        "from": "T37n1763_028:0479a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　釋觀三十六物　釋四意止觀",
        "from": "T37n1763_028:0479a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　結定果　出仙豫王殺",
        "from": "T37n1763_028:0479a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　婆羅門事",
        "from": "T37n1763_028:0479a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　釋四諦義　釋三苦義",
        "from": "T37n1763_028:0479a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　釋四諦有漏無漏義　釋八苦義",
        "from": "T37n1763_028:0479a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖行品之第二",
        "from": "T37n1763_028:0479a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次善男子菩薩摩訶薩(至)除去皮肉唯觀白骨。",
        "from": "T37n1763_028:0479a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_028:0479a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_028:0479a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二明定門也。",
        "from": "T37n1763_028:0479a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有八段。",
        "from": "T37n1763_028:0479a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一總觀身。",
        "from": "T37n1763_028:0479a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有三十六佛。",
        "from": "T37n1763_028:0479a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二移觀。",
        "from": "T37n1763_028:0479a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀四色覓我不得。",
        "from": "T37n1763_028:0479a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三觀手足支節相柱。",
        "from": "T37n1763_028:0479a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為觀轉明。",
        "from": "T37n1763_028:0479a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四見一切皆青也。",
        "from": "T37n1763_028:0479a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此觀始成故。",
        "from": "T37n1763_028:0479a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五諸佛現而復隱。",
        "from": "T37n1763_028:0479a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令其思而易觀。",
        "from": "T37n1763_028:0479a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六觀心。",
        "from": "T37n1763_028:0479a21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是則前觀身後觀心具三。",
        "from": "T37n1763_028:0479a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "意止二。",
        "from": "T37n1763_028:0479a22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既已具。",
        "from": "T37n1763_028:0479a22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則四亦具也。",
        "from": "T37n1763_028:0479a22_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故第七結堪忍地。",
        "from": "T37n1763_028:0479a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八明既得勝定。",
        "from": "T37n1763_028:0479a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必能為物破戒也。",
        "from": "T37n1763_028:0479a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此先明身意止也。",
        "from": "T37n1763_028:0479a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以人人多於身中計有淨也。",
        "from": "T37n1763_028:0479a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此即第一觀也。",
        "from": "T37n1763_028:0479a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰。",
        "from": "T37n1763_028:0479a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二門有五段。",
        "from": "T37n1763_028:0479a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四念觀即四也。",
        "from": "T37n1763_028:0479a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五結果。",
        "from": "T37n1763_028:0479a26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此下觀身無我。",
        "from": "T37n1763_028:0479a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復作是念骨色相異(至)即得斷除一切色欲。",
        "from": "T37n1763_028:0479a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_028:0479a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_028:0479a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏斷鮮淨之欲也。",
        "from": "T37n1763_028:0479a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因色起欲未盡下形貌等三。",
        "from": "T37n1763_028:0479a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆因色起也。",
        "from": "T37n1763_028:0479b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_028:0479b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二移觀四色。",
        "from": "T37n1763_028:0479b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覓我不得也。",
        "from": "T37n1763_028:0479b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復作是念如是骨者(至)二姿態欲三細觸欲。",
        "from": "T37n1763_028:0479b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_028:0479b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_028:0479b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三觀也。",
        "from": "T37n1763_028:0479b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩摩訶薩觀青骨時(至)青黃赤白鴿等色光。",
        "from": "T37n1763_028:0479b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_028:0479b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_028:0479b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為觀轉明。",
        "from": "T37n1763_028:0479b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是以第四觀青。",
        "from": "T37n1763_028:0479b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則一切皆青也。",
        "from": "T37n1763_028:0479b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩於是一一諸光明中(至)光中諸佛忽然不現。",
        "from": "T37n1763_028:0479b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_028:0479b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_028:0479b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "抱疑來久見像。",
        "from": "T37n1763_028:0479b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便問。",
        "from": "T37n1763_028:0479b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忽然不見者。",
        "from": "T37n1763_028:0479b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀道未明。",
        "from": "T37n1763_028:0479b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不答而滅也。",
        "from": "T37n1763_028:0479b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_028:0479b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五諸佛現而復隱。",
        "from": "T37n1763_028:0479b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令其思而易觀也。",
        "from": "T37n1763_028:0479b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰。",
        "from": "T37n1763_028:0479b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一身念處觀也。",
        "from": "T37n1763_028:0479b11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復作是念或識是我(至)猶如流水亦復非我。",
        "from": "T37n1763_028:0479b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_028:0479b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_028:0479b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二心念處觀。",
        "from": "T37n1763_028:0479b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復作是念若識非我(至)當於何處而生貪欲。",
        "from": "T37n1763_028:0479b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_028:0479b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰。",
        "from": "T37n1763_028:0479b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三法念處觀。",
        "from": "T37n1763_028:0479b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若被罵辱復於何處(至)則墮地獄畜生餓鬼。",
        "from": "T37n1763_028:0479b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_028:0479b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_028:0479b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "領佛不答之旨。",
        "from": "T37n1763_028:0479b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃觀識求我。",
        "from": "T37n1763_028:0479b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又不可得觀出入息。",
        "from": "T37n1763_028:0479b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或是我者。",
        "from": "T37n1763_028:0479b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有息則生。",
        "from": "T37n1763_028:0479b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無息則死。",
        "from": "T37n1763_028:0479b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀之直是風性。",
        "from": "T37n1763_028:0479b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦復非我息。",
        "from": "T37n1763_028:0479b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以風多風。",
        "from": "T37n1763_028:0479b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃四大合成而一性。",
        "from": "T37n1763_028:0479b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非我和合。",
        "from": "T37n1763_028:0479b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦無復作是念。",
        "from": "T37n1763_028:0479b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉無有我者。",
        "from": "T37n1763_028:0479b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得無我空。",
        "from": "T37n1763_028:0479b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見第一義。",
        "from": "T37n1763_028:0479b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則見世諦也。",
        "from": "T37n1763_028:0479b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_028:0479b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或息是我者。",
        "from": "T37n1763_028:0479b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六即是心意止觀也。",
        "from": "T37n1763_028:0479b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但見生滅念念流謝。",
        "from": "T37n1763_028:0479b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈有一神常而不滅耶。",
        "from": "T37n1763_028:0479b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何處當有受罵辱者。",
        "from": "T37n1763_028:0479b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此明無我。",
        "from": "T37n1763_028:0479b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但見眾法我心自息也。",
        "from": "T37n1763_028:0479b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰。",
        "from": "T37n1763_028:0479b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四明受念處觀也。",
        "from": "T37n1763_028:0479b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩爾時作是觀已(至)是故名為住堪忍地。",
        "from": "T37n1763_028:0479b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_028:0479b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_028:0479b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結定果也。",
        "from": "T37n1763_028:0479b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_028:0479b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堪忍地者。",
        "from": "T37n1763_028:0479b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第七結果。",
        "from": "T37n1763_028:0479b28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂三地也。",
        "from": "T37n1763_028:0479b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三地菩薩。",
        "from": "T37n1763_028:0479b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脩十二門種。",
        "from": "T37n1763_028:0479b29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得忍度成就。",
        "from": "T37n1763_028:0479b29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既能安心忍理。",
        "from": "T37n1763_028:0479c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故能於事不起瞋也。",
        "from": "T37n1763_028:0479c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰。",
        "from": "T37n1763_028:0479c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五結果。",
        "from": "T37n1763_028:0479c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉菩薩摩訶薩白佛言(至)菩薩摩訶薩得毀淨戒。",
        "from": "T37n1763_028:0479c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_028:0479c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_028:0479c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上浮囊之譬。",
        "from": "T37n1763_028:0479c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輕重不犯。",
        "from": "T37n1763_028:0479c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而菩薩護法。",
        "from": "T37n1763_028:0479c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應化隨緣。",
        "from": "T37n1763_028:0479c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二言相食。",
        "from": "T37n1763_028:0479c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宜項會也。",
        "from": "T37n1763_028:0479c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未得不動地者。",
        "from": "T37n1763_028:0479c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明未得。",
        "from": "T37n1763_028:0479c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶尚不犯。",
        "from": "T37n1763_028:0479c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況已得。",
        "from": "T37n1763_028:0479c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若為攝取眾生得毀戒也。",
        "from": "T37n1763_028:0479c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_028:0479c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八明得深定已。",
        "from": "T37n1763_028:0479c07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能隨緣益物。",
        "from": "T37n1763_028:0479c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得破戒也。",
        "from": "T37n1763_028:0479c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰。",
        "from": "T37n1763_028:0479c08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問意定初依人位也。",
        "from": "T37n1763_028:0479c08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以不待辨慧後。",
        "from": "T37n1763_028:0479c09_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而此中問者。",
        "from": "T37n1763_028:0479c09_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以戒定之位。",
        "from": "T37n1763_028:0479c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本居五方便中無相行已上。",
        "from": "T37n1763_028:0479c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是聖人位也。",
        "from": "T37n1763_028:0479c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初依人雖復信根永立。",
        "from": "T37n1763_028:0479c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要未得真解。",
        "from": "T37n1763_028:0479c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲為利益之道得破戒。",
        "from": "T37n1763_028:0479c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不解言菩薩寧當自墮地獄。",
        "from": "T37n1763_028:0479c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不令眾生退菩提心。",
        "from": "T37n1763_028:0479c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此因緣。",
        "from": "T37n1763_028:0479c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可得破戒而不名為犯也。",
        "from": "T37n1763_028:0479c13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時文殊師利菩薩(至)若墮阿鼻地獄無有是處。",
        "from": "T37n1763_028:0479c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_028:0479c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_028:0479c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至言實須以事證也。",
        "from": "T37n1763_028:0479c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛讚文殊師利(至)乃有如是無量勢力。",
        "from": "T37n1763_028:0479c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_028:0479c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_028:0479c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為法斷命。",
        "from": "T37n1763_028:0479c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是護法。",
        "from": "T37n1763_028:0479c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "況直爾出入無期。",
        "from": "T37n1763_028:0479c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心同虛空者乎。",
        "from": "T37n1763_028:0479c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰。",
        "from": "T37n1763_028:0479c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "引本行為證也。",
        "from": "T37n1763_028:0479c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下梵行中。",
        "from": "T37n1763_028:0479c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明三無量。",
        "from": "T37n1763_028:0479c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當復廣其事也。",
        "from": "T37n1763_028:0479c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次迦葉又有聖行所謂四聖諦苦集滅道。",
        "from": "T37n1763_028:0479c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_028:0479c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_028:0479c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說慧也。",
        "from": "T37n1763_028:0479c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧緣真諦。",
        "from": "T37n1763_028:0479c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辨真諦有三種。",
        "from": "T37n1763_028:0479c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初明四諦。",
        "from": "T37n1763_028:0479c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中辨二諦。",
        "from": "T37n1763_028:0479c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後略明實諦也。",
        "from": "T37n1763_028:0479c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此下盡八字五翻。",
        "from": "T37n1763_028:0479c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說四諦也。",
        "from": "T37n1763_028:0479c24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_028:0479c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧生由境故寄境以明慧也。",
        "from": "T37n1763_028:0479c24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有四科明義。",
        "from": "T37n1763_028:0479c25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一明四諦。",
        "from": "T37n1763_028:0479c25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二明二諦。",
        "from": "T37n1763_028:0479c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三明實諦。",
        "from": "T37n1763_028:0479c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四歎經也。",
        "from": "T37n1763_028:0479c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就四諦中有四翻。",
        "from": "T37n1763_028:0479c26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一以五重略辨四諦。",
        "from": "T37n1763_028:0479c27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二就八苦門。",
        "from": "T37n1763_028:0479c27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣明苦諦。",
        "from": "T37n1763_028:0479c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三寄菩薩觀行。",
        "from": "T37n1763_028:0479c28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "驗其所以。",
        "from": "T37n1763_028:0479c28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是可厭之法。",
        "from": "T37n1763_028:0479c29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四略會通二教也。",
        "from": "T37n1763_028:0479c29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰。",
        "from": "T37n1763_028:0479c29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三門明慧也。",
        "from": "T37n1763_028:0480a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有六段。",
        "from": "T37n1763_028:0480a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一就四諦辨慧。",
        "from": "T37n1763_028:0480a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二就真俗。",
        "from": "T37n1763_028:0480a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三就實諦。",
        "from": "T37n1763_028:0480a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四遣執。",
        "from": "T37n1763_028:0480a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五結慧果。",
        "from": "T37n1763_028:0480a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六明登初地。",
        "from": "T37n1763_028:0480a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後進脩二十五三昧。",
        "from": "T37n1763_028:0480a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進得不動自在。",
        "from": "T37n1763_028:0480a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地上至金剛心也。",
        "from": "T37n1763_028:0480a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就四諦門中。",
        "from": "T37n1763_028:0480a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分為三章。",
        "from": "T37n1763_028:0480a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一有五重。",
        "from": "T37n1763_028:0480a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此第一重。",
        "from": "T37n1763_028:0480a05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略辨四諦也。",
        "from": "T37n1763_028:0480a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦來逼心。",
        "from": "T37n1763_028:0480a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心則生惱。",
        "from": "T37n1763_028:0480a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂逼迮相也。",
        "from": "T37n1763_028:0480a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煩惱結業。",
        "from": "T37n1763_028:0480a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能生來苦。",
        "from": "T37n1763_028:0480a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生長相也。",
        "from": "T37n1763_028:0480a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生死因盡。",
        "from": "T37n1763_028:0480a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寂滅相也。",
        "from": "T37n1763_028:0480a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣中忘相之知大乘道也。",
        "from": "T37n1763_028:0480a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉苦者逼迮相(至)滅者寂滅相道者大乘相。",
        "from": "T37n1763_028:0480a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_028:0480a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_028:0480a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逼迫眾生也。",
        "from": "T37n1763_028:0480a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_028:0480a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一重以十四種色是苦具。",
        "from": "T37n1763_028:0480a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心為苦體。",
        "from": "T37n1763_028:0480a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言不起則已。",
        "from": "T37n1763_028:0480a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起則為緣所逼。",
        "from": "T37n1763_028:0480a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故言逼迫相也。",
        "from": "T37n1763_028:0480a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十惡為因。",
        "from": "T37n1763_028:0480a12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無明為緣。",
        "from": "T37n1763_028:0480a12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以能生長未來故。",
        "from": "T37n1763_028:0480a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名集也。",
        "from": "T37n1763_028:0480a13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偏教以三心滅處為滅。",
        "from": "T37n1763_028:0480a13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓教以常住之體絕眾相。",
        "from": "T37n1763_028:0480a14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為滅也。",
        "from": "T37n1763_028:0480a14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破彼三因。",
        "from": "T37n1763_028:0480a15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大乘常住之因為道。",
        "from": "T37n1763_028:0480a15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次善男子苦者現相(至)滅者除相道者能除相。",
        "from": "T37n1763_028:0480a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_028:0480a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_028:0480a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現惱切故。",
        "from": "T37n1763_028:0480a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言現相也。",
        "from": "T37n1763_028:0480a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_028:0480a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二重謂法顯現者也。",
        "from": "T37n1763_028:0480a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾之為力。",
        "from": "T37n1763_028:0480a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能[廷壬+(同(一/口)+己)]轉行人。",
        "from": "T37n1763_028:0480a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受六道苦果。",
        "from": "T37n1763_028:0480a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉相也。",
        "from": "T37n1763_028:0480a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓果之體。",
        "from": "T37n1763_028:0480a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除一切惡。",
        "from": "T37n1763_028:0480a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂之除也。",
        "from": "T37n1763_028:0480a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無漏之體性。",
        "from": "T37n1763_028:0480a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除戒五方便。",
        "from": "T37n1763_028:0480a20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖是伏除。",
        "from": "T37n1763_028:0480a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未是永斷。",
        "from": "T37n1763_028:0480a21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦相從為道。",
        "from": "T37n1763_028:0480a21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取其感果之義。",
        "from": "T37n1763_028:0480a21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶集諦攝也。",
        "from": "T37n1763_028:0480a22_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰。",
        "from": "T37n1763_028:0480a22_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二重也。",
        "from": "T37n1763_028:0480a22_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "麁著易知。",
        "from": "T37n1763_028:0480a22_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為現相也。",
        "from": "T37n1763_028:0480a23_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因謝果起。",
        "from": "T37n1763_028:0480a23_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉因得果。",
        "from": "T37n1763_028:0480a23_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉相也。",
        "from": "T37n1763_028:0480a23_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅者。",
        "from": "T37n1763_028:0480a24_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "據所除為論也。",
        "from": "T37n1763_028:0480a24_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道者。",
        "from": "T37n1763_028:0480a24_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "據除為論也。",
        "from": "T37n1763_028:0480a24_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次善男子苦者有三相(至)道者脩戒定慧。",
        "from": "T37n1763_028:0480a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_028:0480a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_028:0480a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三相者。",
        "from": "T37n1763_028:0480a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦受性苦。",
        "from": "T37n1763_028:0480a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能苦眾生。",
        "from": "T37n1763_028:0480a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂苦苦也。",
        "from": "T37n1763_028:0480a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不苦不樂以求。",
        "from": "T37n1763_028:0480a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須為行苦也。",
        "from": "T37n1763_028:0480a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樂受滅時生。",
        "from": "T37n1763_028:0480a27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦為壞苦也。",
        "from": "T37n1763_028:0480a28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_028:0480a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三重釋苦義。",
        "from": "T37n1763_028:0480a28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具在中也。",
        "from": "T37n1763_028:0480a28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "集謂二十五有者。",
        "from": "T37n1763_028:0480a29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此乃舉於所集。",
        "from": "T37n1763_028:0480a29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以明能集也。",
        "from": "T37n1763_028:0480a29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰。",
        "from": "T37n1763_028:0480b01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三重也。",
        "from": "T37n1763_028:0480b01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔化下愚。",
        "from": "T37n1763_028:0480b01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開三受教門。",
        "from": "T37n1763_028:0480b01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以三苦。",
        "from": "T37n1763_028:0480b02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "點壞三受也。",
        "from": "T37n1763_028:0480b02_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦之重者。",
        "from": "T37n1763_028:0480b02_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為苦苦。",
        "from": "T37n1763_028:0480b02_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以果來壞因。",
        "from": "T37n1763_028:0480b03_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明樂受之體。",
        "from": "T37n1763_028:0480b03_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是苦故。",
        "from": "T37n1763_028:0480b03_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱為壞苦也。",
        "from": "T37n1763_028:0480b03_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通前二心。",
        "from": "T37n1763_028:0480b04_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行緣動求。",
        "from": "T37n1763_028:0480b04_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為行苦也二十五有。",
        "from": "T37n1763_028:0480b04_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因為集諦。",
        "from": "T37n1763_028:0480b05_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果為滅諦。",
        "from": "T37n1763_028:0480b05_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脩戒定慧為道諦。",
        "from": "T37n1763_028:0480b05_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次善男子有漏法者(至)無漏因者則名為道。",
        "from": "T37n1763_028:0480b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_028:0480b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_028:0480b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有為是苦諦。",
        "from": "T37n1763_028:0480b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無為是滅諦。",
        "from": "T37n1763_028:0480b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有漏業為苦因。",
        "from": "T37n1763_028:0480b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天漏業為道因也。",
        "from": "T37n1763_028:0480b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_028:0480b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四重明有漏因果者。",
        "from": "T37n1763_028:0480b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義與毘曇相應也。",
        "from": "T37n1763_028:0480b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰。",
        "from": "T37n1763_028:0480b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重也。",
        "from": "T37n1763_028:0480b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "指就雨廂易見也。",
        "from": "T37n1763_028:0480b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次善男子八相名苦(至)三念處大悲是名為道。",
        "from": "T37n1763_028:0480b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_028:0480b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_028:0480b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五重明四諦也。",
        "from": "T37n1763_028:0480b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十力乃至大悲為道者。",
        "from": "T37n1763_028:0480b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛心為無礙。",
        "from": "T37n1763_028:0480b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛心為解脫。",
        "from": "T37n1763_028:0480b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有雙道之義。",
        "from": "T37n1763_028:0480b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故假稱道也。",
        "from": "T37n1763_028:0480b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰。",
        "from": "T37n1763_028:0480b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五重廣三苦為八苦也。",
        "from": "T37n1763_028:0480b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生苦具二。",
        "from": "T37n1763_028:0480b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂苦苦及壞苦也。",
        "from": "T37n1763_028:0480b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以潤生之愛從樂受而來。",
        "from": "T37n1763_028:0480b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因中說果。",
        "from": "T37n1763_028:0480b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即壞苦也。",
        "from": "T37n1763_028:0480b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取出生之生。",
        "from": "T37n1763_028:0480b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則有苦苦。",
        "from": "T37n1763_028:0480b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心即苦體。",
        "from": "T37n1763_028:0480b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "墮地獄。",
        "from": "T37n1763_028:0480b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為苦苦也。",
        "from": "T37n1763_028:0480b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怨憎求不得。",
        "from": "T37n1763_028:0480b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦苦苦攝。",
        "from": "T37n1763_028:0480b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其餘五苦。",
        "from": "T37n1763_028:0480b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉應壞苦攝也。",
        "from": "T37n1763_028:0480b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子生者出相所謂五種(至)是故名為五盛陰苦。",
        "from": "T37n1763_028:0480b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_028:0480b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_028:0480b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以總七苦。",
        "from": "T37n1763_028:0480b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更作一苦名者。",
        "from": "T37n1763_028:0480b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人謂七苦異陰而有。",
        "from": "T37n1763_028:0480b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今說即陰是也。",
        "from": "T37n1763_028:0480b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_028:0480b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此第二就八苦門。",
        "from": "T37n1763_028:0480b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣明苦諦也。",
        "from": "T37n1763_028:0480b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大般涅槃經集解卷第二十八",
        "from": "T37n1763_028:0480b25_0",
        "context_with_next_sentence": false
    }
]