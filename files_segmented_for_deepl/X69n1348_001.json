[
    {
        "source_sentence": "雪庵從瑾禪師頌古集",
        "from": "X69n1348_001:0272b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "女子出定",
        "from": "X69n1348_001:0272b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誰在畵樓西。",
        "from": "X69n1348_001:0272b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相逢語咲低。",
        "from": "X69n1348_001:0272b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到家春色晚。",
        "from": "X69n1348_001:0272b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "花落鷓鴣啼。",
        "from": "X69n1348_001:0272b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利弗入城。",
        "from": "X69n1348_001:0272b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遙見月上女出城大地絕纖塵。",
        "from": "X69n1348_001:0272b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面南看北斗。",
        "from": "X69n1348_001:0272b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嫁雞逐雞飛。",
        "from": "X69n1348_001:0272b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嫁狗逐狗走。",
        "from": "X69n1348_001:0272b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "楞嚴經。",
        "from": "X69n1348_001:0272b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛謂阿難。",
        "from": "X69n1348_001:0272b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見見之時。",
        "from": "X69n1348_001:0272b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見非是見。",
        "from": "X69n1348_001:0272b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見猶離見。",
        "from": "X69n1348_001:0272b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見不能及隔墻見角便騎牛。",
        "from": "X69n1348_001:0272b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "騎入紅塵閙市遊。",
        "from": "X69n1348_001:0272b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遊徧歸來欄裏臥。",
        "from": "X69n1348_001:0272b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三更半夜失踪由。",
        "from": "X69n1348_001:0272b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉門前剎竿",
        "from": "X69n1348_001:0272b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "家家門口透長安。",
        "from": "X69n1348_001:0272b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不見纖毫眼界寬。",
        "from": "X69n1348_001:0272b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無法無人誰付曯。",
        "from": "X69n1348_001:0272b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難兄難弟自相謾。",
        "from": "X69n1348_001:0272b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師子尊者赴罽賓",
        "from": "X69n1348_001:0272b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘊空誰見法中王。",
        "from": "X69n1348_001:0272b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覿體何曾礙劍光。",
        "from": "X69n1348_001:0272b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "古廟藤蘿穿戶牖。",
        "from": "X69n1348_001:0272b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷碑風雨碎文章。",
        "from": "X69n1348_001:0272b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "達磨見梁武帝",
        "from": "X69n1348_001:0272b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "踏翻地軸地不動。",
        "from": "X69n1348_001:0272b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "推倒天關天更高。",
        "from": "X69n1348_001:0272b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "穩泛鐵船歸少室。",
        "from": "X69n1348_001:0272b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至今天下起風濤。",
        "from": "X69n1348_001:0272c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "達磨隻履西歸",
        "from": "X69n1348_001:0272c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九年冷坐已敗闕。",
        "from": "X69n1348_001:0272c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隻履西歸更脫空。",
        "from": "X69n1348_001:0272c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後代兒孫空妄想。",
        "from": "X69n1348_001:0272c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鷓鴣啼不為春風。",
        "from": "X69n1348_001:0272c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二祖安心",
        "from": "X69n1348_001:0272c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自有覓不得。",
        "from": "X69n1348_001:0272c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無端面發紅。",
        "from": "X69n1348_001:0272c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翻身喫一[蹎*頁]。",
        "from": "X69n1348_001:0272c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩手摸虗空。",
        "from": "X69n1348_001:0272c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六祖。",
        "from": "X69n1348_001:0272c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因僧問。",
        "from": "X69n1348_001:0272c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黃梅衣鉢。",
        "from": "X69n1348_001:0272c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是何人得。",
        "from": "X69n1348_001:0272c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祖云。",
        "from": "X69n1348_001:0272c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "會佛法者得。",
        "from": "X69n1348_001:0272c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧曰。",
        "from": "X69n1348_001:0272c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和尚還得不。",
        "from": "X69n1348_001:0272c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祖曰不得。",
        "from": "X69n1348_001:0272c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧曰。",
        "from": "X69n1348_001:0272c08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因甚不得。",
        "from": "X69n1348_001:0272c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祖曰。",
        "from": "X69n1348_001:0272c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我不會佛法不會黃梅佛法。",
        "from": "X69n1348_001:0272c09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夢中合眼惺惺。",
        "from": "X69n1348_001:0272c10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此地無金二兩。",
        "from": "X69n1348_001:0272c10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俗人酤酒三升。",
        "from": "X69n1348_001:0272c10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忠國師三喚侍者",
        "from": "X69n1348_001:0272c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一箭射雙鵰。",
        "from": "X69n1348_001:0272c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雙鵰隨手落。",
        "from": "X69n1348_001:0272c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "波搖岳陽城。",
        "from": "X69n1348_001:0272c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月滿滕王閣。",
        "from": "X69n1348_001:0272c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬祖云。",
        "from": "X69n1348_001:0272c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自從胡亂後三十年。",
        "from": "X69n1348_001:0272c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不曾缺鹽醬喫當年高甲已登科。",
        "from": "X69n1348_001:0272c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讀盡人間萬卷書。",
        "from": "X69n1348_001:0272c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今日一身天地窄。",
        "from": "X69n1348_001:0272c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思量好事不如無。",
        "from": "X69n1348_001:0272c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百丈曰。",
        "from": "X69n1348_001:0272c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "老僧昔日被馬大師一喝。",
        "from": "X69n1348_001:0272c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直得三日耳聾眼黑一喝當頭雷電奔。",
        "from": "X69n1348_001:0272c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人間說亦暗銷魂。",
        "from": "X69n1348_001:0272c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "看來豈止聾三日。",
        "from": "X69n1348_001:0272c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直至如今海嶽昏。",
        "from": "X69n1348_001:0272c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南泉曰。",
        "from": "X69n1348_001:0272c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬祖說即心即佛。",
        "from": "X69n1348_001:0272c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王老師不恁麼道。",
        "from": "X69n1348_001:0272c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不是心不是佛不是物。",
        "from": "X69n1348_001:0272c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恁麼道。",
        "from": "X69n1348_001:0272c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還有過麼剃頭頭光生。",
        "from": "X69n1348_001:0272c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洗脚脚清爽。",
        "from": "X69n1348_001:0272c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脫衣上牀眠。",
        "from": "X69n1348_001:0272c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "抓著通身痒。",
        "from": "X69n1348_001:0272c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夾山曰。",
        "from": "X69n1348_001:0272c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當初在大梅。",
        "from": "X69n1348_001:0272c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "失却一隻眼竹籬茆舍酒旗斜。",
        "from": "X69n1348_001:0272c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一个胡芦敗兩家。",
        "from": "X69n1348_001:0273a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酒後不知天與地。",
        "from": "X69n1348_001:0273a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皈來滿地是桃花。",
        "from": "X69n1348_001:0273a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "槃山將順世。",
        "from": "X69n1348_001:0273a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告眾曰。",
        "from": "X69n1348_001:0273a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有人邈得我真否。",
        "from": "X69n1348_001:0273a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將所寫真呈。",
        "from": "X69n1348_001:0273a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆不契師意。",
        "from": "X69n1348_001:0273a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普化出曰。",
        "from": "X69n1348_001:0273a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "某甲邈得。",
        "from": "X69n1348_001:0273a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師曰。",
        "from": "X69n1348_001:0273a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何不呈似老僧。",
        "from": "X69n1348_001:0273a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化乃打筋斗而出。",
        "from": "X69n1348_001:0273a05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師曰。",
        "from": "X69n1348_001:0273a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "這漢向後掣風狂去在徹底氷壺無影像。",
        "from": "X69n1348_001:0273a05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倒翻筋斗摸難成。",
        "from": "X69n1348_001:0273a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千峯雨歇黃梅後。",
        "from": "X69n1348_001:0273a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "桂魄還從海上生。",
        "from": "X69n1348_001:0273a08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魯祖面壁",
        "from": "X69n1348_001:0273a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無目仙人揣骨頭。",
        "from": "X69n1348_001:0273a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "暗中摸索認王侯。",
        "from": "X69n1348_001:0273a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "價高畢竟無人買。",
        "from": "X69n1348_001:0273a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冷却构欄懡[怡台+羅]休。",
        "from": "X69n1348_001:0273a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥山造石頭之室",
        "from": "X69n1348_001:0273a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恁麼不得總不得。",
        "from": "X69n1348_001:0273a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脫却布衫赤骨律。",
        "from": "X69n1348_001:0273a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劈頭一搭忽翻身。",
        "from": "X69n1348_001:0273a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便見口開并眼白。",
        "from": "X69n1348_001:0273a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時人見此一株花如夢相似",
        "from": "X69n1348_001:0273a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同根一體都如夢。",
        "from": "X69n1348_001:0273a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夢裏惺惺眼又花。",
        "from": "X69n1348_001:0273a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蝴蝶飛來過墻去。",
        "from": "X69n1348_001:0273a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不知春色落誰家。",
        "from": "X69n1348_001:0273a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甘贄設粥",
        "from": "X69n1348_001:0273a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甘贄設粥。",
        "from": "X69n1348_001:0273a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南泉打鍋。",
        "from": "X69n1348_001:0273a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一般病痛。",
        "from": "X69n1348_001:0273a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徹底誵訛。",
        "from": "X69n1348_001:0273a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更有些兒好笑。",
        "from": "X69n1348_001:0273a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明朝餓殺禪和。",
        "from": "X69n1348_001:0273a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南泉平常心是道",
        "from": "X69n1348_001:0273a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟得平常達本鄉。",
        "from": "X69n1348_001:0273a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時人多怕落平常。",
        "from": "X69n1348_001:0273a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "青春只有九十日。",
        "from": "X69n1348_001:0273a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爛醉都無一百場。",
        "from": "X69n1348_001:0273a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "趙州曰老僧使得十二時",
        "from": "X69n1348_001:0273a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鍾送黃昏雞報曉。",
        "from": "X69n1348_001:0273b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "趙州何用間煩惱。",
        "from": "X69n1348_001:0273b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "裂破虗空作兩邊。",
        "from": "X69n1348_001:0273b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "古廟香爐出芝草。",
        "from": "X69n1348_001:0273b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "趙州行脚到臨濟",
        "from": "X69n1348_001:0273b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨濟趙州。",
        "from": "X69n1348_001:0273b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪林宗匝。",
        "from": "X69n1348_001:0273b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "特地相逢。",
        "from": "X69n1348_001:0273b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恰似相撲。",
        "from": "X69n1348_001:0273b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "撞見今時行脚僧。",
        "from": "X69n1348_001:0273b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呼為兩箇閒和尚。",
        "from": "X69n1348_001:0273b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "秘魔木杈",
        "from": "X69n1348_001:0273b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "威風凜凜不容攀。",
        "from": "X69n1348_001:0273b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "跳入懷中便解顏。",
        "from": "X69n1348_001:0273b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不是酒腸寬似海。",
        "from": "X69n1348_001:0273b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爭知詩膽大如山。",
        "from": "X69n1348_001:0273b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祇林降魔",
        "from": "X69n1348_001:0273b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無魔無我已降魔。",
        "from": "X69n1348_001:0273b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "添得時人眼裏花。",
        "from": "X69n1348_001:0273b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今日鏌鎁無用處。",
        "from": "X69n1348_001:0273b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "也知賊不打貧家。",
        "from": "X69n1348_001:0273b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨濟陞堂。",
        "from": "X69n1348_001:0273b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有僧出。",
        "from": "X69n1348_001:0273b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師便喝。",
        "from": "X69n1348_001:0273b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亦喝。",
        "from": "X69n1348_001:0273b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便禮拜。",
        "from": "X69n1348_001:0273b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師便打棒頭有眼。",
        "from": "X69n1348_001:0273b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼裏無筋。",
        "from": "X69n1348_001:0273b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多逢濁富。",
        "from": "X69n1348_001:0273b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "罕遇清貧。",
        "from": "X69n1348_001:0273b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自入洞門烟鎻斷。",
        "from": "X69n1348_001:0273b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不知世上幾經春。",
        "from": "X69n1348_001:0273b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "趙州問大同禪師。",
        "from": "X69n1348_001:0273b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大死底人却活時如何。",
        "from": "X69n1348_001:0273b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師曰。",
        "from": "X69n1348_001:0273b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不許夜行。",
        "from": "X69n1348_001:0273b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "投明須到棚前夜半弄傀儡。",
        "from": "X69n1348_001:0273b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行動威儀去就全。",
        "from": "X69n1348_001:0273b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "子細思量無道理。",
        "from": "X69n1348_001:0273b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "裏頭畢竟有人牽。",
        "from": "X69n1348_001:0273b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "興化與旻德問答",
        "from": "X69n1348_001:0273b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一喝兩喝。",
        "from": "X69n1348_001:0273b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "全機出沒。",
        "from": "X69n1348_001:0273b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賓主歷然。",
        "from": "X69n1348_001:0273b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未免俱瞎。",
        "from": "X69n1348_001:0273b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "半夜摸烏龜。",
        "from": "X69n1348_001:0273b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明月照積雪。",
        "from": "X69n1348_001:0273b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "興化見同參來。",
        "from": "X69n1348_001:0273b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纔上法堂便喝。",
        "from": "X69n1348_001:0273b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亦喝須彌倒卓。",
        "from": "X69n1348_001:0273b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "海水逆流。",
        "from": "X69n1348_001:0273b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同參相訪。",
        "from": "X69n1348_001:0273b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作盡冤讎。",
        "from": "X69n1348_001:0273b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "休休。",
        "from": "X69n1348_001:0273b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明日黃花蝶也愁。",
        "from": "X69n1348_001:0273b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "巖頭。",
        "from": "X69n1348_001:0273c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因僧問古帆不掛時如何。",
        "from": "X69n1348_001:0273c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師曰。",
        "from": "X69n1348_001:0273c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後園驢喫草古帆未掛時。",
        "from": "X69n1348_001:0273c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後園驢喫草。",
        "from": "X69n1348_001:0273c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日短苦夜長。",
        "from": "X69n1348_001:0273c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行人須及早。",
        "from": "X69n1348_001:0273c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乾峯一路涅槃門",
        "from": "X69n1348_001:0273c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當面非暗投。",
        "from": "X69n1348_001:0273c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應機皆直說。",
        "from": "X69n1348_001:0273c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乾峯與雲門。",
        "from": "X69n1348_001:0273c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩口同一舌。",
        "from": "X69n1348_001:0273c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若是續貂人。",
        "from": "X69n1348_001:0273c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弄巧便成拙。",
        "from": "X69n1348_001:0273c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雲門上堂。",
        "from": "X69n1348_001:0273c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞聲悟道。",
        "from": "X69n1348_001:0273c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見色明心。",
        "from": "X69n1348_001:0273c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂舉起手曰。",
        "from": "X69n1348_001:0273c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀世音菩薩。",
        "from": "X69n1348_001:0273c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將錢買餬餅。",
        "from": "X69n1348_001:0273c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放下手曰。",
        "from": "X69n1348_001:0273c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元來祇是饅頭頓超見色聞聲句。",
        "from": "X69n1348_001:0273c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不涉明心悟道言。",
        "from": "X69n1348_001:0273c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "花落鳥啼巖下寺。",
        "from": "X69n1348_001:0273c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月明人喚渡船。",
        "from": "X69n1348_001:0273c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禾山解打鼓",
        "from": "X69n1348_001:0273c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當陽打動番南鼓。",
        "from": "X69n1348_001:0273c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬象森羅立地聞。",
        "from": "X69n1348_001:0273c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不是大家齊則劇。",
        "from": "X69n1348_001:0273c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難消白日到黃昏。",
        "from": "X69n1348_001:0273c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法華。",
        "from": "X69n1348_001:0273c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因僧問生死事大請師相救。",
        "from": "X69n1348_001:0273c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師曰。",
        "from": "X69n1348_001:0273c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洞庭湖裏失却船洞庭湖裏失却船。",
        "from": "X69n1348_001:0273c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "赤脚波斯水底眠。",
        "from": "X69n1348_001:0273c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡大地人呼不起。",
        "from": "X69n1348_001:0273c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "春風吹入杏花村。",
        "from": "X69n1348_001:0273c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黃龍三關",
        "from": "X69n1348_001:0273c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我手何似佛手。",
        "from": "X69n1348_001:0273c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌面南看北斗。",
        "from": "X69n1348_001:0273c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兔推明月上千峯。",
        "from": "X69n1348_001:0273c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "引得寒山開笑口。",
        "from": "X69n1348_001:0273c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我脚何似驢脚。",
        "from": "X69n1348_001:0273c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "急走歸家日將落。",
        "from": "X69n1348_001:0273c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自古長安如鏡平。",
        "from": "X69n1348_001:0273c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無端醉倒黃番綽。",
        "from": "X69n1348_001:0273c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人人有箇生緣。",
        "from": "X69n1348_001:0274a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且非東土與西天。",
        "from": "X69n1348_001:0274a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擊珊瑚樹枝枝好。",
        "from": "X69n1348_001:0274a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "撒水銀珠顆顆圓。",
        "from": "X69n1348_001:0274a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛手驢脚生緣。",
        "from": "X69n1348_001:0274a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南海波斯泛鐵船。",
        "from": "X69n1348_001:0274a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精金美玉團堆賣。",
        "from": "X69n1348_001:0274a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畢竟何曾直一錢。",
        "from": "X69n1348_001:0274a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五祖演曰。",
        "from": "X69n1348_001:0274a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倩女離魂。",
        "from": "X69n1348_001:0274a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那个是真底南枝向暖北枝寒。",
        "from": "X69n1348_001:0274a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何事春風作兩般。",
        "from": "X69n1348_001:0274a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憑仗高樓莫吹",
        "from": "X69n1348_001:0274a06_4",
        "context_with_next_sentence": false
    }
]