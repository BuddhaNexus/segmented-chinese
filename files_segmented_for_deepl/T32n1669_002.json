[
    {
        "source_sentence": "大宗地玄文本論卷第二",
        "from": "T32n1669_002:0671a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬鳴菩薩造",
        "from": "T32n1669_002:0671a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真諦三藏譯",
        "from": "T32n1669_002:0671a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一種金剛道路大決擇分第三",
        "from": "T32n1669_002:0671a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是已說歸依德處因緣大決擇分。",
        "from": "T32n1669_002:0671a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說一種金剛道路大決擇分。",
        "from": "T32n1669_002:0671a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_002:0671a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_002:0671a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一種金剛地　　總有五種位",
        "from": "T32n1669_002:0671a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　謂漸次究竟　　及圓滿等非",
        "from": "T32n1669_002:0671a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　并及等是位　　如是五種位",
        "from": "T32n1669_002:0671a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　諸修多羅中　　具足無餘說",
        "from": "T32n1669_002:0671a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_002:0671a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一種金剛道路無礙本地地中。",
        "from": "T32n1669_002:0671a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總有幾位。",
        "from": "T32n1669_002:0671a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣說雖無量。",
        "from": "T32n1669_002:0671a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略說有五種。",
        "from": "T32n1669_002:0671a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是五位。",
        "from": "T32n1669_002:0671a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切總則。",
        "from": "T32n1669_002:0671a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切根本。",
        "from": "T32n1669_002:0671a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切中藏。",
        "from": "T32n1669_002:0671a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切出生。",
        "from": "T32n1669_002:0671a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈一種金剛地總有五種位故。",
        "from": "T32n1669_002:0671a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名為五種本位。",
        "from": "T32n1669_002:0671a25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者無超次第漸轉位。",
        "from": "T32n1669_002:0671a26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者無餘究竟總持位。",
        "from": "T32n1669_002:0671a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者周遍圓滿廣大位。",
        "from": "T32n1669_002:0671a27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者一切諸法俱非位。",
        "from": "T32n1669_002:0671a27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者一切諸法俱是位。",
        "from": "T32n1669_002:0671a28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為五種根本位。",
        "from": "T32n1669_002:0671a28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈謂漸次究竟及圓滿等非并及等是位故。",
        "from": "T32n1669_002:0671a29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是五位自家宣說龜則方說。",
        "from": "T32n1669_002:0671b01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是五位直是方說非宣說量。",
        "from": "T32n1669_002:0671b01_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈如是五種位諸修多羅中具足無餘說故。",
        "from": "T32n1669_002:0671b02_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "位所依止本數名字。",
        "from": "T32n1669_002:0671b03_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_002:0671b04_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_002:0671b04_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　所依止本數　　總有五十一",
        "from": "T32n1669_002:0671b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　謂虛假光明　　四十種名字",
        "from": "T32n1669_002:0671b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　不動真金剛　　十種本名字",
        "from": "T32n1669_002:0671b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　及大極地故　　是名所依數",
        "from": "T32n1669_002:0671b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_002:0671b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五種本位所依止之名字差別。",
        "from": "T32n1669_002:0671b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其數幾有。",
        "from": "T32n1669_002:0671b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣說雖無量。",
        "from": "T32n1669_002:0671b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略說有五十一種名字。",
        "from": "T32n1669_002:0671b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是五十一種根本名字。",
        "from": "T32n1669_002:0671b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切天地一切父母。",
        "from": "T32n1669_002:0671b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切體性一切所依。",
        "from": "T32n1669_002:0671b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈所依止本數總有五十一故。",
        "from": "T32n1669_002:0671b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名為五十一數。",
        "from": "T32n1669_002:0671b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂虛假光明分中有四十種。",
        "from": "T32n1669_002:0671b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真金剛中有十種數。",
        "from": "T32n1669_002:0671b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此五十中。",
        "from": "T32n1669_002:0671b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加大極自然陀羅尼地故。",
        "from": "T32n1669_002:0671b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故成立五十一數。",
        "from": "T32n1669_002:0671b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名字分中四十種數當何等相。",
        "from": "T32n1669_002:0671b16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂十種愛樂心。",
        "from": "T32n1669_002:0671b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十種識知心。",
        "from": "T32n1669_002:0671b17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十種修道心。",
        "from": "T32n1669_002:0671b17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十種不退心。",
        "from": "T32n1669_002:0671b17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各差別故。",
        "from": "T32n1669_002:0671b18_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名為十愛樂心。",
        "from": "T32n1669_002:0671b18_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者必叉多。",
        "from": "T32n1669_002:0671b18_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者阿摩呵尸。",
        "from": "T32n1669_002:0671b19_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者諦度毘梨耶。",
        "from": "T32n1669_002:0671b19_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者和羅只度。",
        "from": "T32n1669_002:0671b19_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者奢摩陀提尸。",
        "from": "T32n1669_002:0671b20_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六者摩訶阿毘跋致多。",
        "from": "T32n1669_002:0671b20_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七者阿羅婆訶尼。",
        "from": "T32n1669_002:0671b21_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八者婆[口*彌]多阿梨羅訶諦。",
        "from": "T32n1669_002:0671b21_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九者尸羅俱尸阿尸羅。",
        "from": "T32n1669_002:0671b22_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十者摩訶毘呵阿僧那。",
        "from": "T32n1669_002:0671b22_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為十。",
        "from": "T32n1669_002:0671b23_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名為十識知心。",
        "from": "T32n1669_002:0671b23_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者留伽度。",
        "from": "T32n1669_002:0671b23_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者留諦迦度。",
        "from": "T32n1669_002:0671b24_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者留羅伽。",
        "from": "T32n1669_002:0671b24_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者留摩訶。",
        "from": "T32n1669_002:0671b24_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者安婆娑。",
        "from": "T32n1669_002:0671b25_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六者毘跋致。",
        "from": "T32n1669_002:0671b25_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七者阿毘跋致。",
        "from": "T32n1669_002:0671b25_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八者必叉伽。",
        "from": "T32n1669_002:0671b26_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九者必阿羅。",
        "from": "T32n1669_002:0671b26_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十者留山迦。",
        "from": "T32n1669_002:0671b26_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為十。",
        "from": "T32n1669_002:0671b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名為十修道心。",
        "from": "T32n1669_002:0671b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者度伽呵。",
        "from": "T32n1669_002:0671b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者度安爾。",
        "from": "T32n1669_002:0671b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者度只羅。",
        "from": "T32n1669_002:0671b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者度和差。",
        "from": "T32n1669_002:0671b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者度利他。",
        "from": "T32n1669_002:0671b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六者度生婆諦。",
        "from": "T32n1669_002:0671b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七者度沙必。",
        "from": "T32n1669_002:0671b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八者度阿訶。",
        "from": "T32n1669_002:0671b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九者度佛阿。",
        "from": "T32n1669_002:0671c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十者度叉一婆。",
        "from": "T32n1669_002:0671c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為十。",
        "from": "T32n1669_002:0671c01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名為十不退心。",
        "from": "T32n1669_002:0671c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者羅諦流沙。",
        "from": "T32n1669_002:0671c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者羅曇沙。",
        "from": "T32n1669_002:0671c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者必自伽。",
        "from": "T32n1669_002:0671c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者法必他。",
        "from": "T32n1669_002:0671c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者佛度陀。",
        "from": "T32n1669_002:0671c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六者羅叉必七者師羅文伽。",
        "from": "T32n1669_002:0671c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八者婆訶諦。",
        "from": "T32n1669_002:0671c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九者婆羅提弗陀。",
        "from": "T32n1669_002:0671c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十者達摩邊伽。",
        "from": "T32n1669_002:0671c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為十。",
        "from": "T32n1669_002:0671c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈謂虛假光明四十種名字故。",
        "from": "T32n1669_002:0671c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為十真金剛心。",
        "from": "T32n1669_002:0671c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者鳩摩羅伽。",
        "from": "T32n1669_002:0671c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者須何伽一婆。",
        "from": "T32n1669_002:0671c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者須那迦。",
        "from": "T32n1669_002:0671c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者須陀洹。",
        "from": "T32n1669_002:0671c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者斯陀含。",
        "from": "T32n1669_002:0671c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六者阿那含。",
        "from": "T32n1669_002:0671c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七者阿羅漢。",
        "from": "T32n1669_002:0671c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八者阿尼羅漢。",
        "from": "T32n1669_002:0671c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九者阿那訶訶。",
        "from": "T32n1669_002:0671c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十者阿訶羅弗。",
        "from": "T32n1669_002:0671c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為十。",
        "from": "T32n1669_002:0671c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈不動真金剛十種本名字故。",
        "from": "T32n1669_002:0671c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此五十中。",
        "from": "T32n1669_002:0671c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更復加婆伽婆佛陀應審觀察。",
        "from": "T32n1669_002:0671c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名五十一種名字。",
        "from": "T32n1669_002:0671c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈及大極地故是名所依數故。",
        "from": "T32n1669_002:0671c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是五十一種心中。",
        "from": "T32n1669_002:0671c13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼無超次第漸轉位。",
        "from": "T32n1669_002:0671c14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安立屬當其相云何。",
        "from": "T32n1669_002:0671c14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_002:0671c15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　五十一位中　　如次無超轉",
        "from": "T32n1669_002:0671c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一中具一切　　名為漸轉位",
        "from": "T32n1669_002:0671c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_002:0671c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯一行者。",
        "from": "T32n1669_002:0671c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五十一種別相位中迴向趣入。",
        "from": "T32n1669_002:0671c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如其次第無超過法。",
        "from": "T32n1669_002:0671c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何。",
        "from": "T32n1669_002:0671c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此門位量法如是故。",
        "from": "T32n1669_002:0671c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈五十一位中如次無超轉故。",
        "from": "T32n1669_002:0671c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是行者。",
        "from": "T32n1669_002:0671c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以何行相而漸漸轉。",
        "from": "T32n1669_002:0671c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂具具轉故。",
        "from": "T32n1669_002:0671c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名為具具轉相。",
        "from": "T32n1669_002:0671c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂一信心位中具餘五十心而轉。",
        "from": "T32n1669_002:0671c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至一大極地位中具餘五十心而轉故。",
        "from": "T32n1669_002:0671c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若爾一物耶異物耶。",
        "from": "T32n1669_002:0671c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實是異物而一物故。",
        "from": "T32n1669_002:0671c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何。",
        "from": "T32n1669_002:0671c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一信心中具一切位。",
        "from": "T32n1669_002:0671c25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非餘位故。",
        "from": "T32n1669_002:0671c25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而一信中具一切位。",
        "from": "T32n1669_002:0671c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同餘餘位中具一切位故。",
        "from": "T32n1669_002:0671c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以何義故。",
        "from": "T32n1669_002:0671c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一信心中具諸餘位。",
        "from": "T32n1669_002:0671c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更須漸轉。",
        "from": "T32n1669_002:0671c27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具一中具而不能具多中具故。",
        "from": "T32n1669_002:0671c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故須轉。",
        "from": "T32n1669_002:0671c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此門中五十一中。",
        "from": "T32n1669_002:0671c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切具具皆悉具足。",
        "from": "T32n1669_002:0671c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方名漸轉位故。",
        "from": "T32n1669_002:0671c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈一中具一切名為漸轉位故。",
        "from": "T32n1669_002:0672a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩訶衍金剛種子修多羅中。",
        "from": "T32n1669_002:0672a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說。",
        "from": "T32n1669_002:0672a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛道路足行行者。",
        "from": "T32n1669_002:0672a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大事而決定轉。",
        "from": "T32n1669_002:0672a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名為二決定轉。",
        "from": "T32n1669_002:0672a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者遍度通達轉。",
        "from": "T32n1669_002:0672a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者具具增長轉。",
        "from": "T32n1669_002:0672a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為二。",
        "from": "T32n1669_002:0672a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言遍度通達轉者。",
        "from": "T32n1669_002:0672a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通遍經過五十一種大道路故。",
        "from": "T32n1669_002:0672a05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言具具增長轉者。",
        "from": "T32n1669_002:0672a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一位中攝諸位故。",
        "from": "T32n1669_002:0672a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至廣說故。",
        "from": "T32n1669_002:0672a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是已說無超次第漸轉門。",
        "from": "T32n1669_002:0672a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說無餘究竟總持門。",
        "from": "T32n1669_002:0672a08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_002:0672a08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_002:0672a09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　五十一位中　　隨其先得入",
        "from": "T32n1669_002:0672a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　攝一切一切　　名無餘究竟",
        "from": "T32n1669_002:0672a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_002:0672a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有五十一別相位中。",
        "from": "T32n1669_002:0672a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有行者以信趣入。",
        "from": "T32n1669_002:0672a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有行者以真金地而趣入。",
        "from": "T32n1669_002:0672a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有行者以大極地而趣入。",
        "from": "T32n1669_002:0672a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等諸行者。",
        "from": "T32n1669_002:0672a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨其先得入位之量。",
        "from": "T32n1669_002:0672a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡攝一切一切位地。",
        "from": "T32n1669_002:0672a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "究竟無餘。",
        "from": "T32n1669_002:0672a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦無移轉亦無出入。",
        "from": "T32n1669_002:0672a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一白白。",
        "from": "T32n1669_002:0672a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故說言總持門位。",
        "from": "T32n1669_002:0672a16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈五十一位中隨其先得入攝一切一切名無餘究竟故。",
        "from": "T32n1669_002:0672a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難入未曾有會修多羅中。",
        "from": "T32n1669_002:0672a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說。",
        "from": "T32n1669_002:0672a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　迴向則信心　　信心則佛地",
        "from": "T32n1669_002:0672a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　佛地則十地　　究竟有何次",
        "from": "T32n1669_002:0672a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至廣說故。",
        "from": "T32n1669_002:0672a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是已說無餘究竟總持門。",
        "from": "T32n1669_002:0672a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說周遍圓滿廣大門。",
        "from": "T32n1669_002:0672a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_002:0672a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_002:0672a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　五十一種位　　無前後一時",
        "from": "T32n1669_002:0672a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　俱轉俱行故　　名周遍圓滿",
        "from": "T32n1669_002:0672a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_002:0672a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五十一位無有前後一時俱。",
        "from": "T32n1669_002:0672a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉一時俱行無有所餘。",
        "from": "T32n1669_002:0672a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦五十一別相位中。",
        "from": "T32n1669_002:0672a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有無量無邊諸位。",
        "from": "T32n1669_002:0672a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有前後一時俱轉。",
        "from": "T32n1669_002:0672a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時俱行無有所餘。",
        "from": "T32n1669_002:0672a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故說言圓滿位焉。",
        "from": "T32n1669_002:0672a29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈五十一種位無前後一時俱轉俱行故名周遍圓滿故。",
        "from": "T32n1669_002:0672a29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法界法輪無盡中藏修多羅中。",
        "from": "T32n1669_002:0672b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說。",
        "from": "T32n1669_002:0672b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時文殊師利聞世尊所說即從坐起。",
        "from": "T32n1669_002:0672b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌頂禮前白佛言。",
        "from": "T32n1669_002:0672b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "T32n1669_002:0672b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名為橫轉無向修道人者。",
        "from": "T32n1669_002:0672b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如宜世尊為諸大眾。",
        "from": "T32n1669_002:0672b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宣說開示如是大事。",
        "from": "T32n1669_002:0672b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是世尊即告文殊師利言。",
        "from": "T32n1669_002:0672b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一種無二一道一區。",
        "from": "T32n1669_002:0672b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大地行者一切行道。",
        "from": "T32n1669_002:0672b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無前無後一時發起。",
        "from": "T32n1669_002:0672b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時同轉一時住持。",
        "from": "T32n1669_002:0672b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時證入。",
        "from": "T32n1669_002:0672b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時安立。",
        "from": "T32n1669_002:0672b08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名橫轉無向道人。",
        "from": "T32n1669_002:0672b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至廣說故。",
        "from": "T32n1669_002:0672b09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是已說周遍圓滿廣大門。",
        "from": "T32n1669_002:0672b09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說一切諸法俱非門。",
        "from": "T32n1669_002:0672b10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_002:0672b11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_002:0672b11_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　諸無量無邊　　一切種種位",
        "from": "T32n1669_002:0672b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　皆悉非建立　　名俱非位地",
        "from": "T32n1669_002:0672b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_002:0672b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此偈中為明何義。",
        "from": "T32n1669_002:0672b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲現示此俱非門非因非果非位非地非有非無非名非義非事非理非壞非常非生非滅一切一切皆悉非故。",
        "from": "T32n1669_002:0672b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈諸無量無邊一切種種位皆悉非建立名俱非位地故。",
        "from": "T32n1669_002:0672b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若爾以何義故建立位名。",
        "from": "T32n1669_002:0672b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以其非義立為位故。",
        "from": "T32n1669_002:0672b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大明修多羅中。",
        "from": "T32n1669_002:0672b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說。",
        "from": "T32n1669_002:0672b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無位位第一位。",
        "from": "T32n1669_002:0672b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至廣說故。",
        "from": "T32n1669_002:0672b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是已說一切諸法俱非門。",
        "from": "T32n1669_002:0672b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說一切諸法俱是門。",
        "from": "T32n1669_002:0672b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_002:0672b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_002:0672b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一切種種法　　無非金剛身",
        "from": "T32n1669_002:0672b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　以一身義故　　名為俱是門",
        "from": "T32n1669_002:0672b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_002:0672b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量無邊一切位法。",
        "from": "T32n1669_002:0672b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切皆悉是金剛身。",
        "from": "T32n1669_002:0672b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "等無差別唯依一身。",
        "from": "T32n1669_002:0672b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故說言俱是門焉。",
        "from": "T32n1669_002:0672b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何。",
        "from": "T32n1669_002:0672b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此門中。",
        "from": "T32n1669_002:0672b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無一一法而非金剛真實身故。",
        "from": "T32n1669_002:0672b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈一切種種法無非金剛身以一身義故名為俱是門故。",
        "from": "T32n1669_002:0672b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種金大地修多羅中。",
        "from": "T32n1669_002:0672b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說。",
        "from": "T32n1669_002:0672c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無病道人唯見積影。",
        "from": "T32n1669_002:0672c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不見散身故。",
        "from": "T32n1669_002:0672c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛實輪山王大決擇分第四",
        "from": "T32n1669_002:0672c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是已說一種金剛道路大決擇分。",
        "from": "T32n1669_002:0672c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說金剛寶輪山王大決擇分。",
        "from": "T32n1669_002:0672c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_002:0672c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_002:0672c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　漸是盡滿非　　一時及前後",
        "from": "T32n1669_002:0672c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　與俱并俱非　　一異時處轉",
        "from": "T32n1669_002:0672c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_002:0672c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此偈中為明何義。",
        "from": "T32n1669_002:0672c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲現示一身。",
        "from": "T32n1669_002:0672c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛大力寶輪山王體中次第漸轉諸法等是無餘究竟周遍圓滿俱非絕離。",
        "from": "T32n1669_002:0672c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此五種位。",
        "from": "T32n1669_002:0672c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時轉前後轉。",
        "from": "T32n1669_002:0672c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱有轉俱非轉。",
        "from": "T32n1669_002:0672c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦一時轉亦異時轉。",
        "from": "T32n1669_002:0672c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦一處轉亦異處轉。",
        "from": "T32n1669_002:0672c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足具足自在自在無所障礙故。",
        "from": "T32n1669_002:0672c11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大嚴盡地虛空法界修多羅中。",
        "from": "T32n1669_002:0672c12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說。",
        "from": "T32n1669_002:0672c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次龍明。",
        "from": "T32n1669_002:0672c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝前所問。",
        "from": "T32n1669_002:0672c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名為金剛本身廣大地地無障無礙恒沙功德品者。",
        "from": "T32n1669_002:0672c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殊勝極妙不可思議不可思議。",
        "from": "T32n1669_002:0672c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五種金剛道路足行地地。",
        "from": "T32n1669_002:0672c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無盡中藏之根本自性。",
        "from": "T32n1669_002:0672c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出生增長所依止藏。",
        "from": "T32n1669_002:0672c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至廣說故。",
        "from": "T32n1669_002:0672c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "◎大宗地玄文本論卷第二",
        "from": "T32n1669_002:0672c17_5",
        "context_with_next_sentence": false
    }
]