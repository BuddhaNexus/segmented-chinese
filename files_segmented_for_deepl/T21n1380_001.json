[
    {
        "source_sentence": "佛說善樂長者經",
        "from": "T21n1380_001:0902c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝奉大夫試光祿卿",
        "from": "T21n1380_001:0902c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明教大師臣法賢奉　詔譯",
        "from": "T21n1380_001:0902c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T21n1380_001:0902c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在迦毘羅城。",
        "from": "T21n1380_001:0902c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大眾俱。",
        "from": "T21n1380_001:0902c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時有一釋種長者。",
        "from": "T21n1380_001:0902c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名曰善樂住在本處。",
        "from": "T21n1380_001:0902c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而彼長者信重三寶。",
        "from": "T21n1380_001:0902c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸依佛歸依法歸依僧。",
        "from": "T21n1380_001:0902c25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯信佛法僧不復信於諸餘外道。",
        "from": "T21n1380_001:0902c25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯求佛法僧善解苦集滅道。",
        "from": "T21n1380_001:0902c26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見真實理獲大果報。",
        "from": "T21n1380_001:0902c27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而當非大即證菩提。",
        "from": "T21n1380_001:0902c27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時彼長者於所見色忽然不見。",
        "from": "T21n1380_001:0902c28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時善樂長者遙向世尊作是願言。",
        "from": "T21n1380_001:0902c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我佛世尊大慈大悲。",
        "from": "T21n1380_001:0902c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願施我眼目施我眼光明施我眼清淨除我眼昏暗。",
        "from": "T21n1380_001:0903a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發是願已又作是言。",
        "from": "T21n1380_001:0903a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今歸依佛歸依善逝。",
        "from": "T21n1380_001:0903a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊以清淨天耳過於人耳。",
        "from": "T21n1380_001:0903a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遙聞彼長者發願言已。",
        "from": "T21n1380_001:0903a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即告尊者阿難言。",
        "from": "T21n1380_001:0903a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難汝今持我祕密神呪。",
        "from": "T21n1380_001:0903a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往彼釋種善樂長者處。",
        "from": "T21n1380_001:0903a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與作救拔護持令得寂靜安樂。",
        "from": "T21n1380_001:0903a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至與彼四眾亦作利益安樂故。",
        "from": "T21n1380_001:0903a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊為善樂長者等。",
        "from": "T21n1380_001:0903a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此清淨眼祕密大神呪曰　怛[寧*也](切身下同)他(引一)呬里彌里企里(二)呬係(引)多(三)忽喻忽喻(四)忽夜(引)尾底(五)忽盧忽盧忽盧(六)訥訥訥訥盧(七)以此清淨眼祕密神呪句力。",
        "from": "T21n1380_001:0903a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼得清淨除去昏暗除去翳障。",
        "from": "T21n1380_001:0903a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至除去風毒眼病黃毒眼病。",
        "from": "T21n1380_001:0903a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "痰毒眼病癊毒眼病。",
        "from": "T21n1380_001:0903a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無痛無腫無惱無苦無諸眵淚。",
        "from": "T21n1380_001:0903a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等病皆悉消除。",
        "from": "T21n1380_001:0903a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復以戒真實力修行真實力苦。",
        "from": "T21n1380_001:0903a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行真實力祕密神呪真實力緣生法真實力。",
        "from": "T21n1380_001:0903a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦法真實力集法真實力。",
        "from": "T21n1380_001:0903a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅法真實力道法真實力。",
        "from": "T21n1380_001:0903a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須陀洹真實力斯陀含真實力。",
        "from": "T21n1380_001:0903a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿那含真實力阿羅漢真實力。",
        "from": "T21n1380_001:0903a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣覺真實力菩薩真實力。",
        "from": "T21n1380_001:0903a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛真實力法真實力聖眾真實力。",
        "from": "T21n1380_001:0903a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以如是等真實力故。",
        "from": "T21n1380_001:0903a21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼得清淨除去昏暗除去翳障。",
        "from": "T21n1380_001:0903a22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有風毒眼病黃毒眼病。",
        "from": "T21n1380_001:0903a23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "痰毒眼病癊毒眼病。",
        "from": "T21n1380_001:0903a23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆不能侵。",
        "from": "T21n1380_001:0903a24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無痛無腫無惱無苦無諸眵淚等。",
        "from": "T21n1380_001:0903a24_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告阿難此祕密神呪。",
        "from": "T21n1380_001:0903a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是過去六正等正覺同共宣說。",
        "from": "T21n1380_001:0903a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂尾鉢尸如來正等正覺。",
        "from": "T21n1380_001:0903a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "室棄如來正等正覺。",
        "from": "T21n1380_001:0903a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尾說部如來正等正覺。",
        "from": "T21n1380_001:0903a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "訖囉葛忖陀如來正等正覺。",
        "from": "T21n1380_001:0903a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "葛那葛牟尼如來正等正覺。",
        "from": "T21n1380_001:0903a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉波如來正等正覺。",
        "from": "T21n1380_001:0903b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今第七釋迦牟尼如來正等正覺亦如是說。",
        "from": "T21n1380_001:0903b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至四大天王。",
        "from": "T21n1380_001:0903b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂持國天王增長天王。",
        "from": "T21n1380_001:0903b03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣目天王多聞天王。",
        "from": "T21n1380_001:0903b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及帝釋天主娑婆界主大梵天王等。",
        "from": "T21n1380_001:0903b04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆亦隨喜宣說。",
        "from": "T21n1380_001:0903b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告阿難。",
        "from": "T21n1380_001:0903b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我未見彼天界梵界及諸魔界。",
        "from": "T21n1380_001:0903b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至沙門婆羅門等界。",
        "from": "T21n1380_001:0903b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人持誦此祕密大神呪三遍者。",
        "from": "T21n1380_001:0903b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有諸病苦而能侵害。",
        "from": "T21n1380_001:0903b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有昏暗內外翳障等。",
        "from": "T21n1380_001:0903b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至天所作病龍所作病。",
        "from": "T21n1380_001:0903b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜叉所作病阿脩羅所作病。",
        "from": "T21n1380_001:0903b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦樓羅所作病乾闥婆所作病。",
        "from": "T21n1380_001:0903b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳩盤茶所作病必隷多所作病。",
        "from": "T21n1380_001:0903b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "富單那所作病羯吒富單那所作病。",
        "from": "T21n1380_001:0903b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘舍左所作病羅剎所作病。",
        "from": "T21n1380_001:0903b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羯枯陀所作病[口*大]多拏所作病。",
        "from": "T21n1380_001:0903b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "星曜所持乃至中毒。",
        "from": "T21n1380_001:0903b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸邪呪法等悉皆消除。",
        "from": "T21n1380_001:0903b14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復阿難如是祕密大神呪法。",
        "from": "T21n1380_001:0903b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有患者日念三遍。",
        "from": "T21n1380_001:0903b15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人眼得清淨如前見物此。",
        "from": "T21n1380_001:0903b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是清淨眼祕密大神呪有大力故。",
        "from": "T21n1380_001:0903b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼得如是清淨。",
        "from": "T21n1380_001:0903b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除去昏暗及內外瞖障。",
        "from": "T21n1380_001:0903b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至風毒障黃毒障痰毒障。",
        "from": "T21n1380_001:0903b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "痰毒障等皆悉消除。",
        "from": "T21n1380_001:0903b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無痛無苦無諸眵淚。",
        "from": "T21n1380_001:0903b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復清淨戒真實力修行真實力。",
        "from": "T21n1380_001:0903b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五通仙真實力賢聖真實力聖藥真實力。",
        "from": "T21n1380_001:0903b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祕密句真實力緣生法真實力。",
        "from": "T21n1380_001:0903b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦法真實力集法真實力。",
        "from": "T21n1380_001:0903b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅法真實力道法真實力。",
        "from": "T21n1380_001:0903b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須陀洹真實力斯陀含真實力。",
        "from": "T21n1380_001:0903b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿那含真實力阿羅漢真實力。",
        "from": "T21n1380_001:0903b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣覺真實力菩薩真實力佛真實力。",
        "from": "T21n1380_001:0903b25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以如是等真實力故。",
        "from": "T21n1380_001:0903b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼得清淨。",
        "from": "T21n1380_001:0903b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊復說祕密神呪曰。",
        "from": "T21n1380_001:0903b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那謨(引)沒馱寫(一句)婆誐嚩都(引)悉殿覩滿怛囉(二合)鉢那莎(引)賀(二)佛告阿難此祕密大神呪。",
        "from": "T21n1380_001:0903b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人日日三時。",
        "from": "T21n1380_001:0903c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與前神呪同共依法誦念。",
        "from": "T21n1380_001:0903c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人所有眼病悉皆消除。",
        "from": "T21n1380_001:0903c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時尊者阿難白佛言世尊。",
        "from": "T21n1380_001:0903c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今承佛聖旨持此祕密大神呪。",
        "from": "T21n1380_001:0903c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往彼善樂長者處廣為宣說。",
        "from": "T21n1380_001:0903c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與彼善樂長者及四眾等作大利益。",
        "from": "T21n1380_001:0903c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說是經已尊者阿難及諸大眾。",
        "from": "T21n1380_001:0903c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛所說皆大歡喜信受奉行。",
        "from": "T21n1380_001:0903c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說善樂長者經",
        "from": "T21n1380_001:0903c09_0",
        "context_with_next_sentence": false
    }
]