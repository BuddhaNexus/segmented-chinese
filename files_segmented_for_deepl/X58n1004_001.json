[
    {
        "source_sentence": "五蘊觀",
        "from": "X58n1004_001:0425b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙門　澄觀　述",
        "from": "X58n1004_001:0425b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "X58n1004_001:0425b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡夫之人欲求解脫。",
        "from": "X58n1004_001:0425b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當云何修。",
        "from": "X58n1004_001:0425b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "X58n1004_001:0425b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當修二觀。",
        "from": "X58n1004_001:0425b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二觀者何。",
        "from": "X58n1004_001:0425b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一人空觀。",
        "from": "X58n1004_001:0425b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二法空觀。",
        "from": "X58n1004_001:0425b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫生死之本莫過人法二執。",
        "from": "X58n1004_001:0425b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迷身心總相。",
        "from": "X58n1004_001:0425b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故執人我為實有。",
        "from": "X58n1004_001:0425b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迷五蘊自相。",
        "from": "X58n1004_001:0425b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故計法我為實有。",
        "from": "X58n1004_001:0425b11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "計人我者。",
        "from": "X58n1004_001:0425b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用初觀照之。",
        "from": "X58n1004_001:0425b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知五蘊和合假名為人。",
        "from": "X58n1004_001:0425b12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一諦觀。",
        "from": "X58n1004_001:0425b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但見五蘊。",
        "from": "X58n1004_001:0425b13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求人我相終不可得。",
        "from": "X58n1004_001:0425b13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名為五蘊。",
        "from": "X58n1004_001:0425b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色受想行識是。",
        "from": "X58n1004_001:0425b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何觀之。",
        "from": "X58n1004_001:0425b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身則色蘊。",
        "from": "X58n1004_001:0425b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂地水火風是。",
        "from": "X58n1004_001:0425b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相如何。",
        "from": "X58n1004_001:0425b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅則地。",
        "from": "X58n1004_001:0425b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "潤則水。",
        "from": "X58n1004_001:0425b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煖則火。",
        "from": "X58n1004_001:0425b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "動則風。",
        "from": "X58n1004_001:0425b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀心則四蘊。",
        "from": "X58n1004_001:0425b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂受想行識是其相。",
        "from": "X58n1004_001:0425b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如何領納為受。",
        "from": "X58n1004_001:0425b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取相為相。",
        "from": "X58n1004_001:0425b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造作為行。",
        "from": "X58n1004_001:0425b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了別為識。",
        "from": "X58n1004_001:0425b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能依此身心相。",
        "from": "X58n1004_001:0425b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諦觀分明。",
        "from": "X58n1004_001:0425b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切處但見五蘊。",
        "from": "X58n1004_001:0425b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求人我相終不可得。",
        "from": "X58n1004_001:0425b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名人空觀。",
        "from": "X58n1004_001:0425b19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘此觀。",
        "from": "X58n1004_001:0425b19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行出分段生死。",
        "from": "X58n1004_001:0425b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永處涅槃。",
        "from": "X58n1004_001:0425b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名二乘解脫。",
        "from": "X58n1004_001:0425b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "計法我者用後觀照之。",
        "from": "X58n1004_001:0425b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知一一蘊皆從緣生。",
        "from": "X58n1004_001:0425b20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "都無自性。",
        "from": "X58n1004_001:0425b21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求蘊相不可得。",
        "from": "X58n1004_001:0425b21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則五蘊皆空。",
        "from": "X58n1004_001:0425b21_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名法空觀。",
        "from": "X58n1004_001:0425b21_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若二觀雙照。",
        "from": "X58n1004_001:0425b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了人我法我。",
        "from": "X58n1004_001:0425b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畢竟空無所有。",
        "from": "X58n1004_001:0425b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離諸怖畏。",
        "from": "X58n1004_001:0425b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "度一切苦厄。",
        "from": "X58n1004_001:0425b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出變易生死。",
        "from": "X58n1004_001:0425b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名究竟解脫。",
        "from": "X58n1004_001:0425b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "X58n1004_001:0425b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫求解脫。",
        "from": "X58n1004_001:0425b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祗是了妄證真。",
        "from": "X58n1004_001:0425b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但能契真如理寂。",
        "from": "X58n1004_001:0425b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然無念則便離縛。",
        "from": "X58n1004_001:0425b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何假興心觀蘊方求解脫。",
        "from": "X58n1004_001:0425c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈不乖理哉。",
        "from": "X58n1004_001:0425c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "X58n1004_001:0425c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離蘊真妄約何而立。",
        "from": "X58n1004_001:0425c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且五蘊者身心之異名。",
        "from": "X58n1004_001:0425c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行人若不識身心。",
        "from": "X58n1004_001:0425c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真妄何能懸契。",
        "from": "X58n1004_001:0425c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不達真妄之本。",
        "from": "X58n1004_001:0425c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸行徒施。",
        "from": "X58n1004_001:0425c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故經云。",
        "from": "X58n1004_001:0425c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於虗空終不能成。",
        "from": "X58n1004_001:0425c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斯之謂也。",
        "from": "X58n1004_001:0425c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且計人我者。",
        "from": "X58n1004_001:0425c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡夫之執也。",
        "from": "X58n1004_001:0425c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "計法我者。",
        "from": "X58n1004_001:0425c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二乘之滯也。",
        "from": "X58n1004_001:0425c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故令修二觀。",
        "from": "X58n1004_001:0425c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方能了妄證真。",
        "from": "X58n1004_001:0425c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈可離也。",
        "from": "X58n1004_001:0425c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五蘊觀(終)",
        "from": "X58n1004_001:0425c08_0",
        "context_with_next_sentence": false
    }
]