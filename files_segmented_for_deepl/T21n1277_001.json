[
    {
        "source_sentence": "速疾立驗魔醯首羅天說阿尾",
        "from": "T21n1277_001:0329b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奢法一卷",
        "from": "T21n1277_001:0329b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T21n1277_001:0329b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空諡大鑒",
        "from": "T21n1277_001:0329b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正號大廣智大興善寺三藏沙門",
        "from": "T21n1277_001:0329b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空奉　詔譯",
        "from": "T21n1277_001:0329b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時那羅延天。",
        "from": "T21n1277_001:0329b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在於香醉山頂。",
        "from": "T21n1277_001:0329b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請魔醯首羅於自在宮中。",
        "from": "T21n1277_001:0329b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養頭面禮足。",
        "from": "T21n1277_001:0329b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白魔醯首羅言。",
        "from": "T21n1277_001:0329b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我所乘迦樓羅使者。",
        "from": "T21n1277_001:0329b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能成辦世間所求事要。",
        "from": "T21n1277_001:0329b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且不能速疾。",
        "from": "T21n1277_001:0329b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願大天為未來有情。",
        "from": "T21n1277_001:0329b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說速疾立驗阿尾奢法。",
        "from": "T21n1277_001:0329b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時魔醯首羅告那羅延言。",
        "from": "T21n1277_001:0329b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當諦聽。",
        "from": "T21n1277_001:0329b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為汝宣說速疾成辦使者之法。",
        "from": "T21n1277_001:0329b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能作息災增益降伏敬愛。",
        "from": "T21n1277_001:0329b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能於夜摩界往來使役。",
        "from": "T21n1277_001:0329b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能知未來善惡吉凶成敗。",
        "from": "T21n1277_001:0329b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旱澇不調隣國侵擾。",
        "from": "T21n1277_001:0329b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡人叛亂種種災祥。",
        "from": "T21n1277_001:0329b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲知未來事者。",
        "from": "T21n1277_001:0329b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當簡擇四五童男或童女。",
        "from": "T21n1277_001:0329b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可年七八歲。",
        "from": "T21n1277_001:0329b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身上無瘢痕[病-丙+猒]記。",
        "from": "T21n1277_001:0329b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聰慧靈利。",
        "from": "T21n1277_001:0329b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先令一七日服素食或三日食。",
        "from": "T21n1277_001:0329b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡欲作法。",
        "from": "T21n1277_001:0329b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要須吉日或鬼宿或歲宿直。",
        "from": "T21n1277_001:0329b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甘露直直日最勝。",
        "from": "T21n1277_001:0329b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沐浴遍身塗香。",
        "from": "T21n1277_001:0329b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著淨衣口含龍腦豆蔻。",
        "from": "T21n1277_001:0329b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持誦者面向東坐。",
        "from": "T21n1277_001:0329b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身前以白檀香塗一小壇。",
        "from": "T21n1277_001:0329b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可一肘量。",
        "from": "T21n1277_001:0329b25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令童女等立於壇上。",
        "from": "T21n1277_001:0329b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散花於童女前。",
        "from": "T21n1277_001:0329b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置一閼伽爐取安息香。",
        "from": "T21n1277_001:0329b26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大印真言加持七遍。",
        "from": "T21n1277_001:0329b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒令童女熏手。",
        "from": "T21n1277_001:0329b27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又取赤花加持七遍。",
        "from": "T21n1277_001:0329b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安童女掌中。",
        "from": "T21n1277_001:0329b28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便以手掩面。",
        "from": "T21n1277_001:0329b28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則持誦者結大印。",
        "from": "T21n1277_001:0329b28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手合掌外相交。",
        "from": "T21n1277_001:0329b29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左押右虛其掌即成。",
        "from": "T21n1277_001:0329b29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此印加持自身五處。",
        "from": "T21n1277_001:0329c01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂額右肩左肩心喉。",
        "from": "T21n1277_001:0329c01_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂上散印即誦真言曰。",
        "from": "T21n1277_001:0329c02_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨婆誐嚩底摩賀(引)母捺[口*疑](二合)迦吒攞施棄底哩(二合)施[口*疑]底哩(二合)路左儞伊捨(引)儞跛輸跛帝娑嚩(二合引)賀(引)則以此印按其童女頂。",
        "from": "T21n1277_001:0329c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則想於頭上。",
        "from": "T21n1277_001:0329c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三角赤色熾盛火輪。",
        "from": "T21n1277_001:0329c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光誦真言七遍。",
        "from": "T21n1277_001:0329c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火輪真言曰。",
        "from": "T21n1277_001:0329c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿儗儞(二合)施棄娑嚩(二合引)賀則以此印按童女口上。",
        "from": "T21n1277_001:0329c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彼口中想水輪。",
        "from": "T21n1277_001:0329c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白色半月形。",
        "from": "T21n1277_001:0329c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言七遍。",
        "from": "T21n1277_001:0329c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1277_001:0329c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵惹攞祖哩(二合)拏(引)摩抳娑嚩(二合引)賀次應移印按彼心中。",
        "from": "T21n1277_001:0329c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想地輪形方黃色。",
        "from": "T21n1277_001:0329c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦七遍真言曰。",
        "from": "T21n1277_001:0329c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵摩賀麼攞跛囉訖囉(二合)摩娑嚩(二合引)賀次應移印按彼臍中。",
        "from": "T21n1277_001:0329c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想風輪其形圓黑色。",
        "from": "T21n1277_001:0329c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦七遍真言曰。",
        "from": "T21n1277_001:0329c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵尾曩多(引)句攞曩那娑嚩(二合引)賀(引)次應以大印加持彼兩脚。",
        "from": "T21n1277_001:0329c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想迦樓羅誦真言曰。",
        "from": "T21n1277_001:0329c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵跛乞史(二合)囉(引)惹跛那娑嚩(二合引)賀(引)次應以大印誦甲冑真言。",
        "from": "T21n1277_001:0329c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持童女遍身旋轉。",
        "from": "T21n1277_001:0329c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1277_001:0330a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵迦嚩左摩部(引)多(引)地跛帝娑嚩(二合引)賀行者次應自身為魔醯首羅天。",
        "from": "T21n1277_001:0330a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三目頭冠瓔珞莊嚴。",
        "from": "T21n1277_001:0330a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭冠上有佛半月。",
        "from": "T21n1277_001:0330a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "項上青。",
        "from": "T21n1277_001:0330a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十八臂。",
        "from": "T21n1277_001:0330a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手持種種器仗。",
        "from": "T21n1277_001:0330a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以龍為紳線。",
        "from": "T21n1277_001:0330a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "角絡繫。",
        "from": "T21n1277_001:0330a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又彼塗血寫及。",
        "from": "T21n1277_001:0330a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須臾頃觀自身已。",
        "from": "T21n1277_001:0330a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應以大印護彼童女一百八命節。",
        "from": "T21n1277_001:0330a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1277_001:0330a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)蜜哩(二合)體尾野(二合)吒多(二)惹縛(引)揄囉迦苫結其大印及誦真言。",
        "from": "T21n1277_001:0330a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍身旋繞加持。",
        "from": "T21n1277_001:0330a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則護一百八種命節。",
        "from": "T21n1277_001:0330a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次又以大印。",
        "from": "T21n1277_001:0330a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言加持花香及閼伽等。",
        "from": "T21n1277_001:0330a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次又以大印真言結十方界。",
        "from": "T21n1277_001:0330a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則應對此童女前。",
        "from": "T21n1277_001:0330a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦魔醯首羅使者真言曰。",
        "from": "T21n1277_001:0330a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唎躋吒迦(一)麼多那(引)誐(二)尾灑娜跛拏(三)薩摩那誐囉(四)尾訖囉(二合)摩(五)尾羅(引)薩誐底(六)[口*梨]濟吒迦(七)布囉(二合)哩跛(引)擔囕(二合八)左羅左羅(九)左里左里(十)跛拏跛拏(十一)跛尼跛尼(十二)伴(上)爾伴爾(十三)羯恥羯恥(十四)阿尾捨阿尾捨(十五)[口*梨]躋吒迦(十六)嚕奴噌(二合)抧孃(二合)跋野底娑嚩(二合引)賀(十七引)此真言應誦七遍。",
        "from": "T21n1277_001:0330a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則彼童女戰動。",
        "from": "T21n1277_001:0330a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知聖者入身。",
        "from": "T21n1277_001:0330a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則更彈指誦真言。",
        "from": "T21n1277_001:0330a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無現驗。",
        "from": "T21n1277_001:0330a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次誦摧迫使者真言曰。",
        "from": "T21n1277_001:0330a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[口*梨][口*梨]耶(一)摩魯呬多(二引)素囉素囉(三引)布爾多賀那賀那(四)波囉(二合)憾麼(二合引)那尼(五)那覩嚕(二合)尼都嚕(二合)尼(六)謨尼謨尼(七)伴(上)爾伴(上)爾(八)羯恥羯恥(九)阿尾捨阿尾捨(十)[口*梨]躋吒迦(十一)嚕奴噌(二合)枳孃(二合)跛野帝(十二)娑嚩(二合引)訶(十三引)誦此真言必速應驗。",
        "from": "T21n1277_001:0330a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問未來善惡一切災祥。",
        "from": "T21n1277_001:0330b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不語或語遲。",
        "from": "T21n1277_001:0330b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則結棒印。",
        "from": "T21n1277_001:0330b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手合掌。",
        "from": "T21n1277_001:0330b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二無名指外支。",
        "from": "T21n1277_001:0330b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指並立。",
        "from": "T21n1277_001:0330b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指各鉤無名指頭。",
        "from": "T21n1277_001:0330b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指各令押中交。",
        "from": "T21n1277_001:0330b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言曰。",
        "from": "T21n1277_001:0330b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)母那誐(二合)囉(二)都嚕都嚕(三)娑嚩(二合引)賀(引四)結此棒印。",
        "from": "T21n1277_001:0330b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則語問種種事已。",
        "from": "T21n1277_001:0330b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大印真言加持閼伽。",
        "from": "T21n1277_001:0330b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三灑童女面。",
        "from": "T21n1277_001:0330b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結解。",
        "from": "T21n1277_001:0330b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此使者真言先應誦一萬遍則法成。",
        "from": "T21n1277_001:0330b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則見身來。",
        "from": "T21n1277_001:0330b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須獻閼伽乞願。",
        "from": "T21n1277_001:0330b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願聖者一切處一切時使用皆辦。",
        "from": "T21n1277_001:0330b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則隱不現。",
        "from": "T21n1277_001:0330b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已後欲使。",
        "from": "T21n1277_001:0330b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗一小壇著香花飲食。",
        "from": "T21n1277_001:0330b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言一百八遍。",
        "from": "T21n1277_001:0330b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則現身。",
        "from": "T21n1277_001:0330b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則言龍宮中取長年藥如意寶珠。",
        "from": "T21n1277_001:0330b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或使夜摩王處。",
        "from": "T21n1277_001:0330b16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "延命增益壽命。",
        "from": "T21n1277_001:0330b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或使上取妙甘露。",
        "from": "T21n1277_001:0330b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或使他國問其善惡。",
        "from": "T21n1277_001:0330b17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能助軍陣摧破他敵。",
        "from": "T21n1277_001:0330b17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種使用悉能成辦。",
        "from": "T21n1277_001:0330b18_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此法一切迦樓羅法中最殊勝。",
        "from": "T21n1277_001:0330b18_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祕密難得。",
        "from": "T21n1277_001:0330b19_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當揀擇法器堪傳授者而傳與。",
        "from": "T21n1277_001:0330b19_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非人器人即損他。",
        "from": "T21n1277_001:0330b20_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已後此法不成。",
        "from": "T21n1277_001:0330b20_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故應極祕密勿妄傳受之。",
        "from": "T21n1277_001:0330b21_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大自在天迦樓羅陀羅尼曰。",
        "from": "T21n1277_001:0330b21_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那謨婆伽嚩底(一)嚕馱囉野(二)瞋那劫波囉野(三)薩嚩微那延迦囉野(四)薩嚩羯摩莎馱那野(五)薩嚩嚩尸迦羅拏野(六)薩嚩設覩嚕(二合)尾那捨那野(七)唵(八)迦波羅質擔瞋那迦波羅部擔(九)嚕訥嚕(二合)枳孃(二合)跛野帝(十)娑嚩(二合引)賀此陀羅尼。",
        "from": "T21n1277_001:0330b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "調伏人取赤芭蕉葉畫彼人。",
        "from": "T21n1277_001:0330b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心上書名字。",
        "from": "T21n1277_001:0330b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦陀羅尼一百八遍。",
        "from": "T21n1277_001:0330c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即埋牛糞中即調伏。",
        "from": "T21n1277_001:0330c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲令竪子對相憎。",
        "from": "T21n1277_001:0330c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多羅葉畫彼男女形。",
        "from": "T21n1277_001:0330c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書名相背。",
        "from": "T21n1277_001:0330c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取線纏。",
        "from": "T21n1277_001:0330c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取鼠狼毛山鷄毛蛇退皮燒熏。",
        "from": "T21n1277_001:0330c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦陀羅尼一百八遍。",
        "from": "T21n1277_001:0330c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安劫波羅中。",
        "from": "T21n1277_001:0330c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於屍林中埋著。",
        "from": "T21n1277_001:0330c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便相憎。",
        "from": "T21n1277_001:0330c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除却依舊。",
        "from": "T21n1277_001:0330c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若令相憐者。",
        "from": "T21n1277_001:0330c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還於多羅葉上畫彼夫形。",
        "from": "T21n1277_001:0330c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書名相合。",
        "from": "T21n1277_001:0330c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取白線繫。",
        "from": "T21n1277_001:0330c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取雀兒毛蛇皮燒熏。",
        "from": "T21n1277_001:0330c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦陀羅尼一百八遍。",
        "from": "T21n1277_001:0330c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即相憐。",
        "from": "T21n1277_001:0330c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法欲令人相打。",
        "from": "T21n1277_001:0330c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於大虫皮或於牛皮。",
        "from": "T21n1277_001:0330c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫二人相把頭髻。",
        "from": "T21n1277_001:0330c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書名著。",
        "from": "T21n1277_001:0330c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取線纏著。",
        "from": "T21n1277_001:0330c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於火上重。",
        "from": "T21n1277_001:0330c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "埋碓臼下。",
        "from": "T21n1277_001:0330c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得日日相打。",
        "from": "T21n1277_001:0330c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除劫止。",
        "from": "T21n1277_001:0330c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲調伏人者。",
        "from": "T21n1277_001:0330c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取貝多葉上畫人形。",
        "from": "T21n1277_001:0330c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書名誦陀羅尼一百八遍。",
        "from": "T21n1277_001:0330c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於床下埋。",
        "from": "T21n1277_001:0330c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得調伏。",
        "from": "T21n1277_001:0330c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法令尿血者。",
        "from": "T21n1277_001:0330c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取多羅葉畫彼人。",
        "from": "T21n1277_001:0330c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書姓名。",
        "from": "T21n1277_001:0330c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取釘誦陀羅尼一百八遍。",
        "from": "T21n1277_001:0330c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釘上七七日。",
        "from": "T21n1277_001:0330c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得除去。",
        "from": "T21n1277_001:0330c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說眼藥法。",
        "from": "T21n1277_001:0330c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若調伏竪子者。",
        "from": "T21n1277_001:0330c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取蛇頭竭羅安善那青木香寫甲蜂二介。",
        "from": "T21n1277_001:0330c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於黑月十四日擣作抹。",
        "from": "T21n1277_001:0330c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和肉點眼角。",
        "from": "T21n1277_001:0330c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切竪子隨逐。",
        "from": "T21n1277_001:0330c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天上亦來非但人間也。",
        "from": "T21n1277_001:0330c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速疾立驗魔醯首羅天說迦婁羅阿尾奢法一卷",
        "from": "T21n1277_001:0331a01_0",
        "context_with_next_sentence": false
    }
]