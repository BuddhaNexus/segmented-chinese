[
    {
        "source_sentence": "印沙佛文",
        "from": "T85n2842_001:1295a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仰啟蓮花藏界清淨法身。",
        "from": "T85n2842_001:1295a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百億如來恒沙化佛。",
        "from": "T85n2842_001:1295a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清涼山頂大聖文殊。",
        "from": "T85n2842_001:1295a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雞足嵓山得道羅漢。",
        "from": "T85n2842_001:1295a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍宮祕典鷲嶺微言。",
        "from": "T85n2842_001:1295a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道眼他心一切賢聖。",
        "from": "T85n2842_001:1295a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願發神足。",
        "from": "T85n2842_001:1295a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運悲心。",
        "from": "T85n2842_001:1295a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降臨道場。",
        "from": "T85n2842_001:1295a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證明功德。",
        "from": "T85n2842_001:1295a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "厥今置淨壇於八表。",
        "from": "T85n2842_001:1295a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敷佛於四門。",
        "from": "T85n2842_001:1295a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中央建觀音之場。",
        "from": "T85n2842_001:1295a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋眾轉金言之部。",
        "from": "T85n2842_001:1295a10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設香饌供三世諸佛。",
        "from": "T85n2842_001:1295a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散淨食與水陸生靈。",
        "from": "T85n2842_001:1295a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合境虔恭傾城懇顙者。",
        "from": "T85n2842_001:1295a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為誰施作。",
        "from": "T85n2842_001:1295a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時則有我府一太保　瑞福攘災之加會也　伏惟我□太保門傳閥閱。",
        "from": "T85n2842_001:1295a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應賢五百之年。",
        "from": "T85n2842_001:1295a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "岳瀆清生資。",
        "from": "T85n2842_001:1295a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當期半千之士。",
        "from": "T85n2842_001:1295a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文過周孔雅操。",
        "from": "T85n2842_001:1295a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入木而七分。",
        "from": "T85n2842_001:1295a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彎弓穿揚舉矢。",
        "from": "T85n2842_001:1295a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猿啼而遶樹寬弘治眾。",
        "from": "T85n2842_001:1295a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬姓歎異瑞之風。",
        "from": "T85n2842_001:1295a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設法安人。",
        "from": "T85n2842_001:1295a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千家賀堯年之慶。",
        "from": "T85n2842_001:1295a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加以信珠皎淨心鏡先明。",
        "from": "T85n2842_001:1295a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憑釋教以定八方。",
        "from": "T85n2842_001:1295a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉□聖賢而安社稷。",
        "from": "T85n2842_001:1295a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先陳志懇。",
        "from": "T85n2842_001:1295a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相鷲嶺而傾心。",
        "from": "T85n2842_001:1295a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請佛迎僧。",
        "from": "T85n2842_001:1295a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結淨壇於九地。",
        "from": "T85n2842_001:1295a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六時轉念經聲動曉於合城。",
        "from": "T85n2842_001:1295a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "士庶誥天。",
        "from": "T85n2842_001:1295a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鈴梵洪鳴於滿城。",
        "from": "T85n2842_001:1295a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時也太陽北照。",
        "from": "T85n2842_001:1295a21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凝凍相容。",
        "from": "T85n2842_001:1295a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧徒念誦以周圓。",
        "from": "T85n2842_001:1295a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灌頂精誠已五日總斯多善。",
        "from": "T85n2842_001:1295a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無限朦因先用莊嚴。",
        "from": "T85n2842_001:1295a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵釋四王龍天八部。",
        "from": "T85n2842_001:1295a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願威光轉盛。",
        "from": "T85n2842_001:1295a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福力彌增興運慈悲。",
        "from": "T85n2842_001:1295a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救人護國。",
        "from": "T85n2842_001:1295a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當今聖主永坐皇城。",
        "from": "T85n2842_001:1295a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十道爭馳赤心向國。",
        "from": "T85n2842_001:1295a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又特是福伏用莊嚴。",
        "from": "T85n2842_001:1295a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我太保貴位。",
        "from": "T85n2842_001:1295a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願福如海岳。",
        "from": "T85n2842_001:1295a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永虎節而迎祥。",
        "from": "T85n2842_001:1295a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祿極江准。",
        "from": "T85n2842_001:1295a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "治河西而代。",
        "from": "T85n2842_001:1295a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國太吉慶比日月而漸圓。",
        "from": "T85n2842_001:1295a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公主夫人恒昌保芳顏而永潔。",
        "from": "T85n2842_001:1295a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剌使尚書等固受願接踵而紹隆。",
        "from": "T85n2842_001:1295a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸小娘子迎長壽貞闌而皎皎。",
        "from": "T85n2842_001:1295b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後河清海晏不聞憂苦之聲。",
        "from": "T85n2842_001:1295b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四寖來降永絕烽煙塵之戰。",
        "from": "T85n2842_001:1295b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三災彌滅盡九橫於海嵎。",
        "from": "T85n2842_001:1295b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勵疫消災。",
        "from": "T85n2842_001:1295b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "送飢荒於地戶。",
        "from": "T85n2842_001:1295b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散霑法界。",
        "from": "T85n2842_001:1295b04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普及有情類。",
        "from": "T85n2842_001:1295b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此勝因城登覺道摩訶般若。",
        "from": "T85n2842_001:1295b04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫曠賢大劫有聖人焉。",
        "from": "T85n2842_001:1295b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出釋氏宮名薄伽梵。",
        "from": "T85n2842_001:1295b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心凝大寂身意無邊。",
        "from": "T85n2842_001:1295b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈化眾生號之為佛。",
        "from": "T85n2842_001:1295b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "厥今坐前社足等故。",
        "from": "T85n2842_001:1295b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以三春上津四□。",
        "from": "T85n2842_001:1295b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初分脫塔印沙啟加願者。",
        "from": "T85n2842_001:1295b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉為躬保願功德之福會也。",
        "from": "T85n2842_001:1295b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯公乃金聲夙顯玉譽早聞。",
        "from": "T85n2842_001:1295b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "列位名班昇榮冕職。",
        "from": "T85n2842_001:1295b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂乃妙回宿殖善芽發於今生。",
        "from": "T85n2842_001:1295b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "業果先淳道心堅。",
        "from": "T85n2842_001:1295b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此日四大而無注曉。",
        "from": "T85n2842_001:1295b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五蘊而皆空脫。",
        "from": "T85n2842_001:1295b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千里之真容印恒沙之遍跡。",
        "from": "T85n2842_001:1295b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更能焚香。",
        "from": "T85n2842_001:1295b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "効外諸僧徒於福事之前。",
        "from": "T85n2842_001:1295b14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散飡遍所於水陸之分。",
        "from": "T85n2842_001:1295b14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此印佛功德迴向福因。",
        "from": "T85n2842_001:1295b15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先用莊嚴梵釋四王龍天八部。",
        "from": "T85n2842_001:1295b15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願威光轉勝福力彌增救人護國。",
        "from": "T85n2842_001:1295b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願使聖躬延受五穀豐登。",
        "from": "T85n2842_001:1295b17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "管內人安歌謠滿城。",
        "from": "T85n2842_001:1295b17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又持勝福伏用莊嚴施主即體。",
        "from": "T85n2842_001:1295b18_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願身而玉樹恒淨恒明體。",
        "from": "T85n2842_001:1295b19_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若金剛常堅常固。",
        "from": "T85n2842_001:1295b19_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今世後世莫絕善緣。",
        "from": "T85n2842_001:1295b20_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此世他生善牙增長。",
        "from": "T85n2842_001:1295b20_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後散霑法界普及有情類。",
        "from": "T85n2842_001:1295b21_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此勝因齊",
        "from": "T85n2842_001:1295b21_27",
        "context_with_next_sentence": false
    }
]