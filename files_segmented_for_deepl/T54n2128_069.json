[
    {
        "source_sentence": "一切經音義卷第六十九",
        "from": "T54n2128_069:0755c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翻經沙門慧琳撰",
        "from": "T54n2128_069:0755c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "音阿毘達磨大毘婆沙論第九帙",
        "from": "T54n2128_069:0755c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從八十一盡第二百。",
        "from": "T54n2128_069:0755c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八十一卷",
        "from": "T54n2128_069:0755c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "浮瓠(乎故反郭注爾雅云瓠壺也說文從瓜夸聲夸音詡于反又上聲呼論從艸作[荂*瓜]俗字也)。",
        "from": "T54n2128_069:0755c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八十二卷",
        "from": "T54n2128_069:0755c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皴皮(七荀反埤蒼云皴皵也說文從皮夋聲夋音同上皵音七略反郭注山海經云皵亦謂皮皴也)。",
        "from": "T54n2128_069:0755c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八十三卷",
        "from": "T54n2128_069:0755c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "矛[矛*(替曰+貝)](上莫侯反說文云酋矛也長二丈建於兵車也象形也古今作[戒廾+矛]下催筭反考聲[矛*(替曰+貝)]短矛也廣雅[矛*(替曰+貝)]謂之鋋文字典說云從矛贊聲鋋音口延反)。",
        "from": "T54n2128_069:0755c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鶖鷺(上音秋下盧故反西國池名亦鳥名也)。",
        "from": "T54n2128_069:0755c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蚇蠖(鳥郭反考聲蟲名也俗名步屈蟲也)。",
        "from": "T54n2128_069:0755c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塳[土*孛](上僕蒙反下盆沒反論文塳[土*孛]謂煙氣皃也今作[火*蓬]烰字書並無此字)。",
        "from": "T54n2128_069:0755c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婆斯瑟搋(坼加反梵語梵志妻名也)。",
        "from": "T54n2128_069:0755c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刮去(開滑反鄭玄注禮記云刮以刃掠去也文字典說云從刀舌聲舌音滑)。",
        "from": "T54n2128_069:0755c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "矜[譇者+(舛夕)](若花反孔注尚書[譇者+(舛夕)]猶憍恣過以自大也說文從言[大/(舛夕)]聲從大從[(巨匚)*(十/十)]音枯化反從夸作者非)。",
        "from": "T54n2128_069:0755c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八十四卷",
        "from": "T54n2128_069:0756a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[病丙+手]脹(上璞江反埤蒼胖亦脹也下張亮反杜注左傳云脹腹滿也古今正字並從肉丰長聲或從疒作庠痮也)。",
        "from": "T54n2128_069:0756a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八十五卷",
        "from": "T54n2128_069:0756a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[夕*巴]轉(冤遠反說文[夕*巴]轉[月*卜]也從夕巴聲巴音節或作宛亦通)。",
        "from": "T54n2128_069:0756a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "抱弄(上袍[曰/月]反下籠東反杜注左傳云弄戲也尒雅云玩也說文從廾玉聲艸音拱論作挊非也)。",
        "from": "T54n2128_069:0756a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八十六卷",
        "from": "T54n2128_069:0756a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掉舉(條弔反賈注國語云掉搖也廣雅振也說文從手卓聲)。",
        "from": "T54n2128_069:0756a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八十七卷",
        "from": "T54n2128_069:0756a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有伺(司利反顧野王云伺猶候也鄭玄注周禮云伺猶察也說文從人司聲)。",
        "from": "T54n2128_069:0756a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八十八卷(無字訓釋)",
        "from": "T54n2128_069:0756a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八十九卷",
        "from": "T54n2128_069:0756a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嫉[堅土+革](上秦悉反王逸注楚辭云害賢日嫉說文從女疾聲下客顏反考聲[堅土+革]遴也古今正字從革臤聲論作慳俗字也)。",
        "from": "T54n2128_069:0756a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第九十卷",
        "from": "T54n2128_069:0756a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羯利藍頞部曇(頞音按梵語初受胎精血相未成形一七二七也)。",
        "from": "T54n2128_069:0756a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "齅香(休又反說文以鼻就臭也從鼻從臭亦聲)。",
        "from": "T54n2128_069:0756a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿毘達磨大毘婆沙論第十袟",
        "from": "T54n2128_069:0756a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第九十一卷　第九十二卷　第九十三卷　第九十四卷　第九十五卷　第九十六卷(已上六卷文易不音訓)第九十七卷阿笈摩(笈音儉輒反)。",
        "from": "T54n2128_069:0756a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第九十八卷",
        "from": "T54n2128_069:0756a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勍[敵ㄆ+殳](上巨迎反廣雅勍武也埤蒼健也左傳云勍[敵ㄆ+殳]之人是也說文強也從力京聲下亭歷反)。",
        "from": "T54n2128_069:0756a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第九十九卷",
        "from": "T54n2128_069:0756b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飄散(上匹霄反毛詩傳曰飄猶吹也說文迥風也從風票聲飄同下珊幹反顧野王云散謂分流飛走不聚也說文飛散也從支昔聲或從雚作[雚*殳]論作散俗字)。",
        "from": "T54n2128_069:0756b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獼猴(上蜜[卑(白日)]反下候鉤反西國地名也)。",
        "from": "T54n2128_069:0756b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百卷",
        "from": "T54n2128_069:0756b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "訶擯(必振反司馬彪注莊子云擯棄也史記云相與排擯也說文從手賓聲論作擯俗字)。",
        "from": "T54n2128_069:0756b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿毘達磨大毘婆沙論第十一帙",
        "from": "T54n2128_069:0756b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百一卷　第一百二卷(並無字訓釋)第一百三卷覆[榷木+月](上峯福反蒼頡篇覆倒也文字典說從西復聲下訶各反王逸注楚辭云有菜曰羹無菜曰[榷木+月]說文從肉[榷木+月]聲音呼郭反)。",
        "from": "T54n2128_069:0756b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羂索(上[治口+月]兖反桂菀珠藂云以繩繞繫取物謂之羂字統作[(ㄇ@(企止))/肙]云施繩於道也從冈肙聲下桑各反顧野王云糺繩曰索說文草木有莖葉可以為繩索也從糸從市)。",
        "from": "T54n2128_069:0756b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[金*矛]矟(上莫俟反考聲戈類也前第八十三卷已釋或作矛下雙捉反廣雅矟兵器也亦矛也埤蒼云矟長一丈八尺文字典說從矛肖聲論作柟非也)。",
        "from": "T54n2128_069:0756b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "驚駭(行騃反文字典說駭亦驚也從馬亥聲騃音崖買反)。",
        "from": "T54n2128_069:0756b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掘鑿(上羣勿反考聲掘穿也斷也謂斷其根也說文從手屈聲下藏作反聲類鑿鏨也說文穿木具也從金[(鑿臼+(函了))金]亦聲鏨音雜含反)。",
        "from": "T54n2128_069:0756b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百四卷",
        "from": "T54n2128_069:0756b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鐵鉆(儉炎反說文鉆[金*截]銸也從金占聲論作鉗[金*截]枷也結束[金*截]也非此義銸音聶)。",
        "from": "T54n2128_069:0756b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "排盾(上敗埋反下脣準反前第二十一卷已具釋)。",
        "from": "T54n2128_069:0756b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "防捍(上縛亡反下寒案反前第五十卷釋訖)。",
        "from": "T54n2128_069:0756b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百五卷",
        "from": "T54n2128_069:0756b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陴[阿可+兒](上普閇反下[生*兒]計反廣雅云壀堄城上女牆孔也蒼頡篇小垣也說文從騰卑兒皆聲論或從人作俾倪亦通也)。",
        "from": "T54n2128_069:0756b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母豬(陟閭反西國池名也)。",
        "from": "T54n2128_069:0756c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "扇搋等(坼加反梵語)。",
        "from": "T54n2128_069:0756c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百六卷第一百七卷第一百八卷",
        "from": "T54n2128_069:0756c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百九卷　第一百一十卷",
        "from": "T54n2128_069:0756c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(已上五卷並無字可音)",
        "from": "T54n2128_069:0756c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿毘達磨大毘婆沙論第十二帙",
        "from": "T54n2128_069:0756c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百一十一卷　第一百一十二卷",
        "from": "T54n2128_069:0756c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(無字音釋)",
        "from": "T54n2128_069:0756c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百一十三卷",
        "from": "T54n2128_069:0756c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "跳躍(上調寮反下羊灼反顧野王云跳躍謂騰躍也說文二字並從足兆翟聲也)。",
        "from": "T54n2128_069:0756c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稊稗(上弟[黍禾+利]反考聲稊草名也文字典說云從禾弟聲或作蕛亦作苐[薯者+(第(竺二)+八)]下陴賣反杜注左傳云稗之似穀者也文字典說從禾卑聲)。",
        "from": "T54n2128_069:0756c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "短命(端夘反字書短促也不長也說文有所長短以矢為正故從矢豆聲論從手作[打丁+豆]非也)。",
        "from": "T54n2128_069:0756c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百一十四卷",
        "from": "T54n2128_069:0756c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一羆(彼眉反尒雅羆似熊而黃白郭注云羆長頭高脚猛憨多力能拔木關西呼為猳熊說文如熊黃白從熊羆省聲古文[骨*(素糸+辛)]也猳音加也)。",
        "from": "T54n2128_069:0756c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[生/(丹、+│)]紺(甘憾反說文紺染帛[生/(丹、+│)]而楊赤色也從糸甘聲或作[糸*金][糸*禁]也)。",
        "from": "T54n2128_069:0756c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擘以(補麥反文字典說擘破裂也從手辟聲也)。",
        "from": "T54n2128_069:0756c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百一十五卷",
        "from": "T54n2128_069:0756c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攈多(君運反韻詮云攈拾也說文亦拾也從手麇聲論作捃亦通)。",
        "from": "T54n2128_069:0756c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欑[身*(秦禾)](上殂巒反鄭注禮記攢聚也說文積也從木贊聲下食亦反說文云[身*(乏之+夫)]弓弩發於身而中於遠從身從[乏之+夫]篆文從寸寸法度也與論同也)。",
        "from": "T54n2128_069:0756c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百一十六卷",
        "from": "T54n2128_069:0756c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誼擾(上呼元反聲類諠譁也說文從言宣聲下饒少反孝聲擾攪也煩彼也說文煩也從手憂聲音奴刀反字從頁從已止反其手足也從憂者非也)。",
        "from": "T54n2128_069:0757a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二皰(蒲皃反法義理)。",
        "from": "T54n2128_069:0757a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始褰持等(褰音欺乾反梵語也)。",
        "from": "T54n2128_069:0757a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誅戮(隆竹反賈注國語云戮煞也說文從戈翏聲)。",
        "from": "T54n2128_069:0757a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[具八+(巢果)]首([敲高+日]遙反顧野王云謂懸首於水上及竿頭以肆其[古/手]也又廣雅[具八+(巢果)]礫也說文倒首也賈侍中說此斷其首倒懸即[具八+(巢果)]字也論作梟為不孝鳥也與人[具八+(巢果)]首乖也)。",
        "from": "T54n2128_069:0757a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祠禱(上似滋反白虎通日祠者嗣也顧野王云祠亦祭之總名也下刀老反鄭注禮記云求福日禱得福曰祠說文二字並從示司壽聲)。",
        "from": "T54n2128_069:0757a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酷法(空谷反說文作嚳云以虎害之也從學省告聲亦作俈今通作酷從酉告聲)。",
        "from": "T54n2128_069:0757a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書撽(形擊反考聲撽木簡也長二尺有所召書上以傳之史記檄羽撽數罪之書也文字典說從木敫聲敫音擊也)。",
        "from": "T54n2128_069:0757a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百一十七卷",
        "from": "T54n2128_069:0757a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼膾(上苦回反下古外反廣雅膾割也即今之居者也)。",
        "from": "T54n2128_069:0757a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蟒類(上忙牓反尒雅云蟒王虵郭注云蟒虵之大者也故曰王也文字典說從虫莾聲)。",
        "from": "T54n2128_069:0757a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弶取(強向反字書弶施[(ㄇ@(企止))/ㄙ/月]於道以取禽獸也說文從弓京聲作[木*(強ㄙ+口)]俗也)。",
        "from": "T54n2128_069:0757a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百一十八卷",
        "from": "T54n2128_069:0757a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拊奏(孚武反顧野王云拊猶拍也周禮云合奏擊拊鄭眾注云或擊或拊也孔注尚書云拊亦擊也說文從平付聲)。",
        "from": "T54n2128_069:0757a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百一十九卷",
        "from": "T54n2128_069:0757a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[禾*((朿木+大)/貝)]中(紫錫反考聲積聚也鄭注禮記委多曰積文字典說從禾[(朿木+大)/貝]聲論從艸作[卄/積]非也)。",
        "from": "T54n2128_069:0757a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "揩拭(上客皆反下十軄反揩摩也鄭注禮記云拭猶淨也說文並從手皆式聲也)。",
        "from": "T54n2128_069:0757a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顛仆(上典年反鄭注禮記云倒仆為顛也說文從頁真聲正作傎論從二真非也下蒲北反說文傾頓也從人上聲)。",
        "from": "T54n2128_069:0757a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "駈擯(必刃反前第一百卷中已釋訖)。",
        "from": "T54n2128_069:0757a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "靈龕(苦甘反孝聲鑿山壁為坎也廣雅龕盛也文字典說著佛像處也從龍今聲俗從合作龕非也)。",
        "from": "T54n2128_069:0757a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "機阱(情勁反考聲穽穿地陷獸也文字典說從[阜(口/十)+(耜耒)]井聲論從穴作穽亦通古作汬也)。",
        "from": "T54n2128_069:0757a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覘望(諂廉反杜注左傳云覘伺也鄭注禮記云覘[門@視]視也文字典說從見占聲)。",
        "from": "T54n2128_069:0757b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蹇鈍(上建偃反孝聲蹇難也說文蹇跛也從足寒省聲下徒困反蒼頡篇鈍頑也聲類鈍不利也說文從金毛聲)。",
        "from": "T54n2128_069:0757b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百二十卷",
        "from": "T54n2128_069:0757b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "胎膜(上貸來反廣雅云三月為胎顧野王云未生也說文婦孕二月也從肉台聲下忙愽反說文肉間膜也從肉莫聲)。",
        "from": "T54n2128_069:0757b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蚊蜹(上勿分反說文作蟁齧人飛蟲也從[虫*虫]民聲下而銳反國語蜹蛾蜂蠆皆能害人顧野王云今有虫似蜹齧人謂之含毒即此也說文秦謂之蜹楚謂之蚊從虫苪聲也)。",
        "from": "T54n2128_069:0757b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蠛蠓(上眠結反下蒙董反莊子云[狂王+表]之於木若蠛蠓於地也顧野王曰小飛蟲也郭注尒雅云小蟲似蜹也說文並從虫蔑蒙聲也)。",
        "from": "T54n2128_069:0757b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蚔行(佶移反考聲蚳有毛蟲也說文蛙也從虫氏聲蛙音口圭反)。",
        "from": "T54n2128_069:0757b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "踐蹋(上前剪反毛詩傳云踐行皃也鄭注禮記踐履也升也下潭[門@(舄臼+下)]反廣雅蹋亦履也說文亦踐也二字並從足戔[日/羽]聲論作蹹俗字也)。",
        "from": "T54n2128_069:0757b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摑裂(上孤獲反下連晳反廣雅裂分也字書擘也文字典說從衣列聲)。",
        "from": "T54n2128_069:0757b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿毘達磨大毘婆沙論第十三帙",
        "from": "T54n2128_069:0757b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百二十一卷",
        "from": "T54n2128_069:0757b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稼穡(上家暇反下所棘反前第二十一卷已釋訖)。",
        "from": "T54n2128_069:0757b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逮[勝月+舟](臺載反毛詩傳曰逮及也說文從辵聿聲)。",
        "from": "T54n2128_069:0757b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百二十二卷(無字音釋)",
        "from": "T54n2128_069:0757b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百二十三卷",
        "from": "T54n2128_069:0757b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "渭物[打丁+費]子([打丁+費][夗夕+月]味反梵語也)。",
        "from": "T54n2128_069:0757b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "骸骨(薤皆反顧野王云骸身體之骨惣名也說文從骨亥聲)。",
        "from": "T54n2128_069:0757b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百二十四卷",
        "from": "T54n2128_069:0757b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唼食(子荅反埤蒼噆齧[辰/肉]也莊子云蚊[亡/虫][口*替]膚也說文銜也從口妾聲或作[口*(一/巾)]正作[口*替]也)。",
        "from": "T54n2128_069:0757b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鹹鹵(上洽緘反尒雅云鹹苦也郭注云苦即大鹹也說文北方味也從鹵咸聲下盧古反說文鹵西方鹹地也又西方謂之鹵從西省鹵[宋木+彖]監形也)。",
        "from": "T54n2128_069:0757b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百二十五卷",
        "from": "T54n2128_069:0757c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鉤餌(上狗侯反考聲鈎取也曲也引也顧野王云鈎謂也說文從金句聲下而志反蒼頡篇云餌食也大戴禮云鷹隼魚鼈所得之餌也說文從食耳聲)。",
        "from": "T54n2128_069:0757c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鞭撻(上必綿反顧野王云鞭用革以朴罪人也說文從革便聲下他怛反孔注尚書云撻笞也鄭玄扶也古今正字從手達聲古文作撻論從草作靼音折非義也)。",
        "from": "T54n2128_069:0757c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[女*((奐大)/比)]邏吒(上批計反中羅賀反下茶暇反梵語)。",
        "from": "T54n2128_069:0757c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "囚縶(砧立反杜注左傳縶拘繫也前第五十卷已貝釋)。",
        "from": "T54n2128_069:0757c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藏竄(七亂反杜注左傳云竄匿也顧野王云竄猶逃也古今正字從穴從鼠會意字也)。",
        "from": "T54n2128_069:0757c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百二十六卷",
        "from": "T54n2128_069:0757c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婆私瑟[打丁+虎](圻加反梵語)。",
        "from": "T54n2128_069:0757c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "麟角(栗珍反此論第七卷中已具釋訖)。",
        "from": "T54n2128_069:0757c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恬寂(上簟兼反孔注尚書云恬安也方言靜也從心舌聲亦會意字也)。",
        "from": "T54n2128_069:0757c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百二十七卷",
        "from": "T54n2128_069:0757c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "哮吼(上孝交反埤蒼云哮嚇大怒也古今正字云哮豕驚聲從口孝聲下虛垢反考聲鳴之大也牛虎曰吼古今正字作吽謂獸聲也從牛口聲亦作拘呴)。",
        "from": "T54n2128_069:0757c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百二十八卷",
        "from": "T54n2128_069:0757c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蚊蠛(上勿分反下眠結反前百二十卷中已具釋訖)。",
        "from": "T54n2128_069:0757c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百二十九卷(無字音釋)",
        "from": "T54n2128_069:0757c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百三十卷",
        "from": "T54n2128_069:0757c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲吮(旋兖反集訓云吮口嗍也說文吮欶也從口允聲論作[口*(兗兄+允)]非也或作冤)。",
        "from": "T54n2128_069:0757c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鎔銅(上勇鍾反漢書云猶金之在鎔唯冶之所鑄說文冶器法也從金容聲)。",
        "from": "T54n2128_069:0757c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舌[肉*(咒几+(一/巾))](昂各反考聲[月*(咒几+(一/巾))]齗也說文作[遻(這言)]論從口作[口*(遻(這言))]非也)。",
        "from": "T54n2128_069:0757c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙潬(檀爛反尒雅云潬沙出也孝聲水中沙出也說文從水單聲)。",
        "from": "T54n2128_069:0757c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嗟惋(上借邪反毛詩傳云嗟歎辭也古今正字云憂歎也從口差聲下烏喚反考聲惋歎恨也文字集略云驚異也從心宛聲也)。",
        "from": "T54n2128_069:0758a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[金*矛][矛*(替曰+貝)](上莫侯反前第一百三卷釋訖古作矛下倉喚反考聲云[矛*(替曰+貝)]短矛也南越謂之[矛*殳]廣雅云[矛*(替曰+貝)]謂之鋋古今正字從矛贊聲古文[矛*(?/衣)]今論文作攢非也)。",
        "from": "T54n2128_069:0758a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿毘達磨大毘婆沙論第十四帙",
        "from": "T54n2128_069:0758a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百三十一卷",
        "from": "T54n2128_069:0758a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "登躡(黏輒反方言躡登也廣雅履也說文蹈也從足聶聲也)。",
        "from": "T54n2128_069:0758a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲遮準陀(準音朱允反梵語)。",
        "from": "T54n2128_069:0758a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百三十二卷",
        "from": "T54n2128_069:0758a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相糅(女救反鄭注禮記云揉雜也文字典說從米柔聲正作粈也)。",
        "from": "T54n2128_069:0758a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百三十三卷",
        "from": "T54n2128_069:0758a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畔喋婆(喋音徒叶反梵語大災風也)。",
        "from": "T54n2128_069:0758a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飜騰(上孚袁反廣雅飜飛也顧野王云飜高去也說文從飛番聲亦作翻下鄧登反韓詩云騰乘也無不乘浚也廣雅騰奔也說文馳也從馬從騰省聲勝從舟從月者非也)。",
        "from": "T54n2128_069:0758a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耕馭(魚據反文字典說馭駕馭馬也從馬從又與御同)。",
        "from": "T54n2128_069:0758a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[打丁+替]擊(上殂桓反前第一百十五卷已釋訖也)。",
        "from": "T54n2128_069:0758a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百三十四卷",
        "from": "T54n2128_069:0758a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縈纏(上伊營反毛詩傳云縈旋也說文收韏也從糸熒省聲下徹連反說文纏約也從糸廛聲)。",
        "from": "T54n2128_069:0758a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百三十五卷",
        "from": "T54n2128_069:0758a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擺揬(上八買反廣雅云埤開也考聲擺揮手也說文兩手擊也從手罷聲或作押論作擺誤也下徒訥反廣雅云揬衛也字書揬揩也說文從手突聲突從穴從犬論從山作突非也)。",
        "from": "T54n2128_069:0758a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百三十六卷",
        "from": "T54n2128_069:0758a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緝[糸*((朿木+大)/貝)](上侵入反毛詩箋云緝續也下井昔反郭注爾雅云續繼也說文亦緝也二字竝從糸責咠皆聲)。",
        "from": "T54n2128_069:0758a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毳時(巛苪反鄭注禮記云毳毛之細者也鄭眾云毳[罽厂]衣也說文從三毛)。",
        "from": "T54n2128_069:0758a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "抖擻(上兜口反下涑厚反考聲云抖擻振也涑音叜俟反)。",
        "from": "T54n2128_069:0758b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百三十七卷(無字音釋)",
        "from": "T54n2128_069:0758b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百三十八卷",
        "from": "T54n2128_069:0758b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須鐮(上思朱反說文從彡從頁顧野王云所須待之須從彡作須從水作須音誨今俗行已久且依也下斂鹽反考聲鎌刈物者也方言刈[利禾+勾]也說文鍥也從金兼聲或作鐮亦通[利禾+勾]音鉤)。",
        "from": "T54n2128_069:0758b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須插(楚洽反聲類插謂刺物使入也說文從手[臿臼+(凵@(人*人))]聲[臿臼+(凵@(人*人))]字從千從臼)。",
        "from": "T54n2128_069:0758b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百三十九卷　第一百四十卷(無字音釋)阿毘達磨大毘婆沙論第十五帙第一百四十一卷膠濘(上絞肴反第七十八已釋訖下寧定反杜注左傳濘泥也考聲泥淖也說文從水寧聲淖音女教反)。",
        "from": "T54n2128_069:0758b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "局故(開玉反毛詩傳云[尸@句]曲也卷也廣雅近也說文[尸@句]促也從口在尺下復句之也象形)。",
        "from": "T54n2128_069:0758b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百四十二卷",
        "from": "T54n2128_069:0758b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "匍匐(上僕逋反下朋北反毛詩箋云匍匐言盡力也顧野王云手行也說文云匍手行也匍伏地也二字並從勹甫畐皆聲音包)。",
        "from": "T54n2128_069:0758b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灘磧(上撻丹反說文水[泳永+(而/而)]而乾也從水灘聲下青歷反廣雅云磧瀨也水淺石見也說文從石[朿/貝]聲)。",
        "from": "T54n2128_069:0758b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如穴(玄決反毛詩箋云鑿地曰穴說文土室也從宀八論作穴誤也)。",
        "from": "T54n2128_069:0758b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百四十三卷",
        "from": "T54n2128_069:0758b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如[卸ㄗ+(坐土+(一/廾))](並冥反顧野王云缾汲水器也文字典說從[午/止]并聲)。",
        "from": "T54n2128_069:0758b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "錯謬(上倉洛反考聲錯誤也顧野王云以交合錯亂之錯從辵作[這言+((人*人)/(人*人)/日)]說文[這言+交]也從辵[(俎且+(俎且))/日]聲下眉幼反鄭注禮記云謬誤也說文狂者之志言也從言翏聲)。",
        "from": "T54n2128_069:0758b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百四十四卷(無字音釋)",
        "from": "T54n2128_069:0758b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百四十五卷摩摩[巳/廾]多([巳/廾]音餘之反梵語)第一百四十六卷(無字音釋)第一百四十七卷蹲坐(徂魂反說文蹲踞也從足尊聲踞音居御反)。",
        "from": "T54n2128_069:0758b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百四十八卷",
        "from": "T54n2128_069:0758c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "埏埴(上設氈反淮南子云挻抑土為器也說文從手延聲從土作埏者非下時軄反孔注尚書云土黏曰埴也說文從土直聲)。",
        "from": "T54n2128_069:0758c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憒閙(上迴外反下拏効反前第二十六卷中具釋訖)。",
        "from": "T54n2128_069:0758c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百四十九卷",
        "from": "T54n2128_069:0758c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "粖薪(上忙鉢反下悉津反西國藥名也此國佮艾取火之屬也哈音感合反)。",
        "from": "T54n2128_069:0758c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "指拈(念添反廣雅拈指持也說文從手占聲論作捻俗字)。",
        "from": "T54n2128_069:0758c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眩亂(上玄絹反賈注國語眩或也顛冒也說文目無常主也從目玄聲)。",
        "from": "T54n2128_069:0758c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百五十卷",
        "from": "T54n2128_069:0758c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朅地羅鈎(上褰[薩/女]反下苟侯反梵語)。",
        "from": "T54n2128_069:0758c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "挑善行眼(上眺聊反聲類云挑猶抉也說文從手兆聲抉音伊決反)。",
        "from": "T54n2128_069:0758c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[猒/土]迮(上黯甲反杜注左傳[猒/土]亦笮也禮記所謂鎮笮也說文從土猒聲下爭格反聲類迮迫也考聲狹小也文字典說云從辵乍聲亦作窄也)。",
        "from": "T54n2128_069:0758c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金絛(計勞反鄭注周禮云其樊纓以絛絲飾之考聲絛織[糸*系]如繩然也說文織成也從糸條省聲論從韋作韜又作縚非也)。",
        "from": "T54n2128_069:0758c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "撓攪(上好高反廣雅云撓亂也說文攪也從手堯聲亦作薅論作[打丁+毛]非也下交巧反毛詩傳云攪亂也字書撓也說文從手從覺聲)。",
        "from": "T54n2128_069:0758c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿毘達磨大毘婆沙論第十六帙",
        "from": "T54n2128_069:0758c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百五十一卷",
        "from": "T54n2128_069:0758c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蠐螬(上薺賷反下造遭反前第二十七卷中已具釋)。",
        "from": "T54n2128_069:0758c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風飈(褾遙反郭注尒雅云飈暴風自下而上也尸子曰暴風[秀*責]飊也文字典說從風猋聲)。",
        "from": "T54n2128_069:0758c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百五十二卷(無字音釋)",
        "from": "T54n2128_069:0758c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百五十三卷打揵穉(次件焉反下馳致反梵語也)第一百五十四卷翅翮(上施至反說文作翄翼也從羽支亦作翨忯下行革反郭注爾雅云翮鳥羽根也說文羽莖也從羽鬲聲)。",
        "from": "T54n2128_069:0758c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攀攬(上盼蠻反王逸注楚辭云攀引也廣雅戀也說文作艸云引也從反艸今作攀從手樊聲下藍膽反廣雅云攬取也說文撮持也從手覽聲亦作擥)。",
        "from": "T54n2128_069:0759a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嫉慳(上秦悉反下客姦反前第八十九卷中已具釋訖)。",
        "from": "T54n2128_069:0759a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百五十五卷(無字音釋)",
        "from": "T54n2128_069:0759a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百五十六卷",
        "from": "T54n2128_069:0759a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "止撥(半末反毛詩傳云撥治也王逸云棄也廣雅除也亦猶絕也說文亦治也從手發聲)。",
        "from": "T54n2128_069:0759a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不夗轉(冤遠反前第八十五卷中已釋)。",
        "from": "T54n2128_069:0759a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百五十七卷",
        "from": "T54n2128_069:0759a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為拳(倦員反考聲云拳手拳也案論文云五指合為拳即握掌也說文手也從手卷省聲論作捲用力也非其義也)。",
        "from": "T54n2128_069:0759a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百五十八卷　第一百五十九卷",
        "from": "T54n2128_069:0759a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　第一百六十卷(已上並無字音訓)阿毘達磨大毘婆沙論第十七袟第一百六十一卷(無字音釋)第一百六十二卷敬憚(壇旦反毛詩箋云憚難也畏也文字典說從心單聲)。",
        "from": "T54n2128_069:0759a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羸劣(上累為反下戀輟反前第六十三卷中釋訖)。",
        "from": "T54n2128_069:0759a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶豫(余茹反顧野王云猶豫謂不定也說文豫獸名從象矛聲)。",
        "from": "T54n2128_069:0759a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百六十三卷(無字音釋)",
        "from": "T54n2128_069:0759a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百六十四卷",
        "from": "T54n2128_069:0759a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朽敗(休久反蒼頡篇朽腐也說文作[((亥〦+止)人)*ㄎ]義同從歺丂聲從木作朽或字也)。",
        "from": "T54n2128_069:0759a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百六十五卷(無字音釋)",
        "from": "T54n2128_069:0759a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百六十六卷",
        "from": "T54n2128_069:0759a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "船栰(煩發反馬注論語云大曰筏小曰桴考聲栰縛竹木浮之水上也說文正作橃云海大船也從木發聲或作[舟*發]亦作筏)。",
        "from": "T54n2128_069:0759a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[生/(丹、+│)]瘀(於據反廣雅云瘀病也說文積血也從疒於聲)。",
        "from": "T54n2128_069:0759b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "膖脹(上璞江反下張亮反前第八十四卷中釋訖)。",
        "from": "T54n2128_069:0759b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "骨鎻(蘇果反漢書云[瑙囟+貝]以環相鈎連也考聲云連鐶也文字典說從金[巢果+貝]聲論作璅非也)。",
        "from": "T54n2128_069:0759b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髀骨(鼙米反考聲[骨*坒]股也說文股外也從骨卑聲論從肉作[月*坒]俗字也)。",
        "from": "T54n2128_069:0759b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頷輪(前此論第八卷已具釋)。",
        "from": "T54n2128_069:0759b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "澡漱(上遭草反顧野王云澡猶洗潔也說文洗手也從水喿聲論從草作藻誤也下搜[聽(德彳)+皮]反顧野王云漱盥盪也古今正字從水欶聲)。",
        "from": "T54n2128_069:0759b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[要/月]絛(討刀反第一百五十卷中已釋訖)。",
        "from": "T54n2128_069:0759b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竦密(粟勇反廣雅云竦上也顧野王云竦高也說文從立朿聲論作聳誤也)。",
        "from": "T54n2128_069:0759b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曲僂(蔞主反考聲云傴僂俯身也廣雅僂背僂亦曲也說文僂尪也從人婁聲)。",
        "from": "T54n2128_069:0759b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百六十七卷",
        "from": "T54n2128_069:0759b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蟲蛆(七余反聲類[月*且]蠅子也說文乳肉中也從虫且聲或從肉作[月*且])。",
        "from": "T54n2128_069:0759b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百六十八卷　第一百六十九卷",
        "from": "T54n2128_069:0759b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　第一百七十卷(已上並無字音釋)阿毘達磨大毘婆沙論第十八袟第一百七十一卷見朹(五骨反前第八卷中具已釋訖)。",
        "from": "T54n2128_069:0759b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[朿木+大]亂(上拏効反第一百四十八卷具釋訖)。",
        "from": "T54n2128_069:0759b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百七十二卷",
        "from": "T54n2128_069:0759b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欻然(熏欝反此論前第十五卷中具釋訖)。",
        "from": "T54n2128_069:0759b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嘷[唎禾](上胡高反楚辭虎豹是嘷說文嘷猶咆也從口臯聲下澆竅反顧野王云[唎禾]呼也說文高聲也正作嘂從[器犬][┴*├]聲古作[利禾+言][口*(目/(巠工))]曒論作[唎禾]俗字[器犬]音側立反[┴*├]音吉留反)。",
        "from": "T54n2128_069:0759b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煻煨(上蕩即反考聲煻灰兼細火也下猥回反廣雅云煨熅也說文盆中火也二字並從火唐畏皆聲熅音蘊文反)。",
        "from": "T54n2128_069:0759b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "銛利(思廉反漢書音義云銛利也蒼頡篇銛[金*截]也考聲刀劒利也說文從金舌聲[金*截]音接閻反)。",
        "from": "T54n2128_069:0759c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鐵[此/束](醉髓反字書[此/束]鳥喙也文字典說從此從朿亦作觜論作[口*(隹/乃)]俗字鐵正作鐵)。",
        "from": "T54n2128_069:0759c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廁圂(上惻志反下魂困反此論前第十二卷中已具釋訖)。",
        "from": "T54n2128_069:0759c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "矩拉婆(上俱禹反下藍蹋反梵語也)。",
        "from": "T54n2128_069:0759c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百七十三卷",
        "from": "T54n2128_069:0759c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "央掘利魔羅(上鞅薑反下群物反梵語也)。",
        "from": "T54n2128_069:0759c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[傲士+出]慢(上熬誥反杜注左傳云[傲士+出]不敬也廣雅傲亦慢也字書倨也說文從人[敖士+出]聲論從心作慠非也下蠻晏反孔注尚書慢輕慢典教也顧野王云慢猶輕侮也說文惰也從心曼聲)。",
        "from": "T54n2128_069:0759c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[這言+(坐土+(一/廾))]石(伯孟反鄭注禮記云迸散也字書云亦散走也文字典說云從辵并聲亦作跰)。",
        "from": "T54n2128_069:0759c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百七十四卷(無字音釋)",
        "from": "T54n2128_069:0759c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百七十五卷",
        "from": "T54n2128_069:0759c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "銷鎔(上小焦反顧野王云銷猶散也說文鑠也下勇鍾反說文鎔治器法也二字並從金肖容皆聲鑠音商約反)。",
        "from": "T54n2128_069:0759c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百七十六卷",
        "from": "T54n2128_069:0759c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蟒身(忙牓反郭注爾雅蟒虵之大者也前第一百一十七卷中昊釋訖)。",
        "from": "T54n2128_069:0759c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一摶(叚巒反廣雅云摶手握使相著也說文從手專聲)。",
        "from": "T54n2128_069:0759c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畦中(惠圭反前此論第六十六卷中具釋訖)。",
        "from": "T54n2128_069:0759c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "躊躇(上宙留反下佇驢反廣雅云躊躇猶豫也顧野王云謂淹留而躊躇也說文二字並從足壽者皆聲也)。",
        "from": "T54n2128_069:0759c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百七十七卷",
        "from": "T54n2128_069:0759c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不凹(黯甲反抱[袖由+├]子云凹墊下也考聲謂中下也象形)。",
        "from": "T54n2128_069:0759c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不凸(田結反韻略云凸高起皃也象形字也)。",
        "from": "T54n2128_069:0759c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "足跟(艮痕反釋名足後曰跟說文足踵也從足艮聲)。",
        "from": "T54n2128_069:0759c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皺緩(上郡瘦反此論第三十八卷中具釋訖下桓管反爾雅緩舒也顧野王云緩猶寬也說文從糸爰聲)。",
        "from": "T54n2128_069:0759c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攣急(劣圓反考聲攣拘也顧野王云癵謂拘曲也說文孫也從手[戀心]聲或從疒作癵論作戀誤)。",
        "from": "T54n2128_069:0759c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繄泥(噎奚反梵語)。",
        "from": "T54n2128_069:0759c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傍敧(起羇反顧野王云攲以為不正也從攴奇聲奇字正從介作奇攴從半竹作攴)。",
        "from": "T54n2128_069:0760a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幖幟(上必遙反下嗤至反前第七十三卷已釋訖)。",
        "from": "T54n2128_069:0760a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "哳哳(展列反梵語數法也)。",
        "from": "T54n2128_069:0760a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "跋邏攙(上盤末反次羅賀反下懺衫反梵語)。",
        "from": "T54n2128_069:0760a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百七十八卷",
        "from": "T54n2128_069:0760a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "割劓(上于遏反孔注尚書云割害也廣雅截也下冝器反鄭注禮記劓截其臯也說文次鼻也並從刀或作[利禾+臬])。",
        "from": "T54n2128_069:0760a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不瞚(輸閏反莊子云終日視而不瞚呂氏春秋云万世猶一瞚說文目開闔也或作瞬)。",
        "from": "T54n2128_069:0760a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百七十九卷",
        "from": "T54n2128_069:0760a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顦悴(上情遙反下慈醉反楚辭顦悴憂愁之容也說文或作憔亦作[烈列+(面*隹)]悴或從頁作顇並通也)。",
        "from": "T54n2128_069:0760a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羞[赤*皮](上秀由反下拏蕳反方言[赤*皮]媿也說文靣慙赤從皮赤聲)。",
        "from": "T54n2128_069:0760a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百八十卷",
        "from": "T54n2128_069:0760a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[而/火]等([蓻/火]兖反說文從火而聲)。",
        "from": "T54n2128_069:0760a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿毘達磨大毘婆沙論第十九帙",
        "from": "T54n2128_069:0760a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百八十一卷",
        "from": "T54n2128_069:0760a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蹻足(起驕反文頴注漢書云蹻猶翹也說文舉足高皃也從足喬聲或作趫)。",
        "from": "T54n2128_069:0760a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "麁鞭(上醋胡反下額更反古今正字牢也考聲堅也正作鞕又作硬俗字也)。",
        "from": "T54n2128_069:0760a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "素怛纜毘奈耶(怛音單葛反纜音羅淡反梵語唐云經律藏也)。",
        "from": "T54n2128_069:0760a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百八十二卷第一百八十三卷",
        "from": "T54n2128_069:0760a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百八十四卷　第一百八十五卷",
        "from": "T54n2128_069:0760a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(已上並無字可音)",
        "from": "T54n2128_069:0760a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百八十六卷",
        "from": "T54n2128_069:0760a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嗤誚(上齒之反字書嗤戲笑皃也文字典說從口[山/一/虫]聲或作[虫*欠][山/虫]字從[屮/一]從虫[屮/一]古文之也下樵笑反孔注尚書誚讓也或作譙也)。",
        "from": "T54n2128_069:0760a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百八十七卷",
        "from": "T54n2128_069:0760a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憺怕(上譚濫反顧野王云憺恬靜也王逸注楚辭云憺猶安也下普百反子虛賦云怕[前刖+ㄅ]無為憺[前刖+ㄅ]自持廣雅怕靜也說文無為也並從心詹白皆聲論從水作澹泊非此也)。",
        "from": "T54n2128_069:0760b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "溉田(基未反顧野王云溉灌注也說文灌也從水既聲)。",
        "from": "T54n2128_069:0760b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百八十八卷",
        "from": "T54n2128_069:0760b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鐵礦(號猛反爾雅鐵璞謂之磺說文銅[金*截]璞也從石黃聲論作丱通用或作礦也)。",
        "from": "T54n2128_069:0760b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樓櫓(上漏頭反文字集略城上守禦屋也下盧古反釋名櫓者上露無覆屋也說文並從木)。",
        "from": "T54n2128_069:0760b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "埤堄(上普閇反下倪計反廣雅城上女牆也前第一百五卷已具釋訖)。",
        "from": "T54n2128_069:0760b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "裸形(華瓦反顧野王云裸脫衣露祖也說文從衣或作躶倮也)。",
        "from": "T54n2128_069:0760b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百八十九卷",
        "from": "T54n2128_069:0760b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "腐敗(扶武反廣雅腐臭敗也說文腐爛也從肉)。",
        "from": "T54n2128_069:0760b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百九十卷",
        "from": "T54n2128_069:0760b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耽嗜(上荅含反爾雅躭取樂過度也下時至反孔注尚書嗜無肰足也說文人嗜慾喜之也或作暏[飢几+耆][酉*耆])。",
        "from": "T54n2128_069:0760b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結憾(含暗反孔注論語云憾恨也文字典說從心感聲也)。",
        "from": "T54n2128_069:0760b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿毘達磨大毘婆沙論第二十帙",
        "from": "T54n2128_069:0760b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百九十一卷",
        "from": "T54n2128_069:0760b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "驚慴(詹涉反郭注爾雅云懼慴也說文慴怖懼也從心或作懾)。",
        "from": "T54n2128_069:0760b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "驚駭(鞋界反怖也)。",
        "from": "T54n2128_069:0760b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百九十二卷(無字音釋)",
        "from": "T54n2128_069:0760b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百九十三卷",
        "from": "T54n2128_069:0760b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "囹圄(上歷丁反下魚舉文鄭注禮記囹圄所以禁守繫者若今之別獄也說文守也並從囗囗音韋)。",
        "from": "T54n2128_069:0760b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百九十四卷(無字音釋)",
        "from": "T54n2128_069:0760b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百九十五卷",
        "from": "T54n2128_069:0760b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繼嗣(上雞藝反王弼注周易云繼謂不絕也賈逵注國語云繼餘也爾雅繼紹也說文續也從糸從[斷斤][斷斤]亦聲或作[斷斤][斷斤]音絕下詞恣反孔注尚書嗣亦繼也毛詩箋云嗣續也說文侯嗣國也從口從冊司聲或作孠古字也冊音[竺二+(秉千)]也)。",
        "from": "T54n2128_069:0760b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百九十六卷(無字音釋)",
        "from": "T54n2128_069:0760c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百九十七卷",
        "from": "T54n2128_069:0760c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隤壤(上隊雷反廣雅隤壞也說文墜下也從阜或作隤下穰掌反孔注尚書無塊曰壞劉兆注穀梁傳云鑿地出土日壤說文從土襄聲也)。",
        "from": "T54n2128_069:0760c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瘡疣(右憂反廣雅肬腫也蒼頡篇云疣病也說文從疒或從肉作肬又作[病丙+又])。",
        "from": "T54n2128_069:0760c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百九十八卷",
        "from": "T54n2128_069:0760c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "騫持(丘乾反前第一百二十六卷已釋訖)。",
        "from": "T54n2128_069:0760c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "守阸(鸎賣反杜注左傳云阸地險也王逸注楚辭云阸傾危也說文塞也從阜或作隘作阸俗字也)。",
        "from": "T54n2128_069:0760c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "撾打(竹蝸反聲類云撾捶也)。",
        "from": "T54n2128_069:0760c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一百九十九卷(無字音釋)",
        "from": "T54n2128_069:0760c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二百卷",
        "from": "T54n2128_069:0760c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[虫*折]蝪(上星亦反下盈隻反爾雅云蠑螈[虫*折]蜴蝘蜒守宮也郭璞注云異語別四名也說文云蝘蜒在草日[虫*折]蜴也從虫蠑音榮螈音原蝘音偃蜒音田典反)。",
        "from": "T54n2128_069:0760c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切經音義卷第六十九",
        "from": "T54n2128_069:0760c14_0",
        "context_with_next_sentence": false
    }
]