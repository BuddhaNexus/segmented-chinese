[
    {
        "source_sentence": "維摩詰所說經卷十三",
        "from": "X19n0347_013:0573c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法供養品",
        "from": "X19n0347_013:0573c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時釋提桓因。",
        "from": "X19n0347_013:0573c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於大眾中。",
        "from": "X19n0347_013:0573c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言。",
        "from": "X19n0347_013:0573c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "X19n0347_013:0573c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我雖從佛及文殊師利聞百千經。",
        "from": "X19n0347_013:0573c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未曾聞此不可思議自在神通決定實相經典。",
        "from": "X19n0347_013:0573c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如我解佛所說義趣。",
        "from": "X19n0347_013:0573c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生。",
        "from": "X19n0347_013:0573c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞此經法。",
        "from": "X19n0347_013:0573c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信解受持。",
        "from": "X19n0347_013:0573c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讀誦之者。",
        "from": "X19n0347_013:0573c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必得是法不疑。",
        "from": "X19n0347_013:0573c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況如說修行。",
        "from": "X19n0347_013:0574a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斯人則為閉眾惡趣。",
        "from": "X19n0347_013:0574a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開諸善門。",
        "from": "X19n0347_013:0574a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常為諸佛之所護念。",
        "from": "X19n0347_013:0574a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降伏外學。",
        "from": "X19n0347_013:0574a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摧滅魔怨。",
        "from": "X19n0347_013:0574a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修治菩提。",
        "from": "X19n0347_013:0574a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安處道場。",
        "from": "X19n0347_013:0574a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "履踐如來所行之跡。",
        "from": "X19n0347_013:0574a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "X19n0347_013:0574a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有受持讀誦。",
        "from": "X19n0347_013:0574a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所說修行者。",
        "from": "X19n0347_013:0574a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當與諸眷屬。",
        "from": "X19n0347_013:0574a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養給事。",
        "from": "X19n0347_013:0574a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所在聚落城邑。",
        "from": "X19n0347_013:0574a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "山林曠野。",
        "from": "X19n0347_013:0574a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有是經處。",
        "from": "X19n0347_013:0574a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦與諸眷屬。",
        "from": "X19n0347_013:0574a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聽受法故。",
        "from": "X19n0347_013:0574a06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同到其所。",
        "from": "X19n0347_013:0574a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其未信者。",
        "from": "X19n0347_013:0574a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當令生信。",
        "from": "X19n0347_013:0574a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其已信者。",
        "from": "X19n0347_013:0574a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當為作護。",
        "from": "X19n0347_013:0574a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言。",
        "from": "X19n0347_013:0574a07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉。",
        "from": "X19n0347_013:0574a07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天帝。",
        "from": "X19n0347_013:0574a08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如汝所說。",
        "from": "X19n0347_013:0574a08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我助汝喜。",
        "from": "X19n0347_013:0574a08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經廣說過去未來現在諸佛。",
        "from": "X19n0347_013:0574a08_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可思議阿耨多羅三藐三菩提。",
        "from": "X19n0347_013:0574a09_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故天帝。",
        "from": "X19n0347_013:0574a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人。",
        "from": "X19n0347_013:0574a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持讀誦供養是經者。",
        "from": "X19n0347_013:0574a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則為供養去來今佛。",
        "from": "X19n0347_013:0574a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天帝。",
        "from": "X19n0347_013:0574a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正使三千大千世界。",
        "from": "X19n0347_013:0574a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來滿中。",
        "from": "X19n0347_013:0574a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如甘蔗竹葦。",
        "from": "X19n0347_013:0574a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稻麻叢林。",
        "from": "X19n0347_013:0574a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子。",
        "from": "X19n0347_013:0574a12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善女人。",
        "from": "X19n0347_013:0574a13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或以一劫。",
        "from": "X19n0347_013:0574a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或減一劫。",
        "from": "X19n0347_013:0574a13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恭敬尊重。",
        "from": "X19n0347_013:0574a13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讚歎供養。",
        "from": "X19n0347_013:0574a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉諸所安。",
        "from": "X19n0347_013:0574a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至諸佛滅後。",
        "from": "X19n0347_013:0574a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以一一全身舍利。",
        "from": "X19n0347_013:0574a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起七寶塔。",
        "from": "X19n0347_013:0574a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縱廣一四天下。",
        "from": "X19n0347_013:0574a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高至梵天。",
        "from": "X19n0347_013:0574a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "表剎莊嚴。",
        "from": "X19n0347_013:0574a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以一切華香瓔珞。",
        "from": "X19n0347_013:0574a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幢幡伎樂。",
        "from": "X19n0347_013:0574a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微妙第一。",
        "from": "X19n0347_013:0574a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一劫。",
        "from": "X19n0347_013:0574a16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若減一劫。",
        "from": "X19n0347_013:0574a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而供養之。",
        "from": "X19n0347_013:0574a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天帝。",
        "from": "X19n0347_013:0574a17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於意云何。",
        "from": "X19n0347_013:0574a17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人植福。",
        "from": "X19n0347_013:0574a17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寧為多不。",
        "from": "X19n0347_013:0574a18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋提桓因言。",
        "from": "X19n0347_013:0574a18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甚多。",
        "from": "X19n0347_013:0574a18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "X19n0347_013:0574a18_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼之福德。",
        "from": "X19n0347_013:0574a18_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若以百千億劫。",
        "from": "X19n0347_013:0574a19_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說不能盡。",
        "from": "X19n0347_013:0574a19_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告天帝。",
        "from": "X19n0347_013:0574a19_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知是善男子。",
        "from": "X19n0347_013:0574a19_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善女人。",
        "from": "X19n0347_013:0574a20_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞是不可思議解脫經典。",
        "from": "X19n0347_013:0574a20_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信解受持。",
        "from": "X19n0347_013:0574a20_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讀誦修行。",
        "from": "X19n0347_013:0574a21_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福多於彼。",
        "from": "X19n0347_013:0574a21_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何。",
        "from": "X19n0347_013:0574a21_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛菩提。",
        "from": "X19n0347_013:0574a21_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆從此生。",
        "from": "X19n0347_013:0574a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩提之相。",
        "from": "X19n0347_013:0574a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可限量。",
        "from": "X19n0347_013:0574a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以是因緣。",
        "from": "X19n0347_013:0574a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福不可量。",
        "from": "X19n0347_013:0574a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告天帝。",
        "from": "X19n0347_013:0574a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過去無量阿僧祇劫。",
        "from": "X19n0347_013:0574a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時世有佛。",
        "from": "X19n0347_013:0574a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號曰藥王如來。",
        "from": "X19n0347_013:0574a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應供。",
        "from": "X19n0347_013:0574a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正遍知。",
        "from": "X19n0347_013:0574a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明行足。",
        "from": "X19n0347_013:0574a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善逝。",
        "from": "X19n0347_013:0574a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間解。",
        "from": "X19n0347_013:0574a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無上士。",
        "from": "X19n0347_013:0574b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "調御丈夫。",
        "from": "X19n0347_013:0574b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天人師。",
        "from": "X19n0347_013:0574b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛。",
        "from": "X19n0347_013:0574b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "X19n0347_013:0574b01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世界名大莊嚴。",
        "from": "X19n0347_013:0574b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劫名莊嚴。",
        "from": "X19n0347_013:0574b02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛壽二十小劫。",
        "from": "X19n0347_013:0574b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其聲聞僧。",
        "from": "X19n0347_013:0574b02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十六億那由他。",
        "from": "X19n0347_013:0574b02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩僧。",
        "from": "X19n0347_013:0574b03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有十二億。",
        "from": "X19n0347_013:0574b03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天帝。",
        "from": "X19n0347_013:0574b03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時有轉輪聖王。",
        "from": "X19n0347_013:0574b03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名曰寶葢。",
        "from": "X19n0347_013:0574b04_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七寶具足。",
        "from": "X19n0347_013:0574b04_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "主四天下。",
        "from": "X19n0347_013:0574b04_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王有千子。",
        "from": "X19n0347_013:0574b04_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端正勇徤。",
        "from": "X19n0347_013:0574b05_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能伏怨敵。",
        "from": "X19n0347_013:0574b05_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時寶蓋。",
        "from": "X19n0347_013:0574b05_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與其眷屬。",
        "from": "X19n0347_013:0574b05_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養藥王如來。",
        "from": "X19n0347_013:0574b05_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施諸所安。",
        "from": "X19n0347_013:0574b06_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至滿五劫。",
        "from": "X19n0347_013:0574b06_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過五劫已。",
        "from": "X19n0347_013:0574b06_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告其千子。",
        "from": "X19n0347_013:0574b06_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等亦當如我。",
        "from": "X19n0347_013:0574b07_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以深心供養於佛。",
        "from": "X19n0347_013:0574b07_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是千子。",
        "from": "X19n0347_013:0574b07_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受父王命。",
        "from": "X19n0347_013:0574b08_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養藥王如來。",
        "from": "X19n0347_013:0574b08_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復滿五劫。",
        "from": "X19n0347_013:0574b08_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切施安。",
        "from": "X19n0347_013:0574b08_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其王一子。",
        "from": "X19n0347_013:0574b09_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名曰月葢。",
        "from": "X19n0347_013:0574b09_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獨坐思惟。",
        "from": "X19n0347_013:0574b09_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寧有供養殊過此者。",
        "from": "X19n0347_013:0574b09_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以佛神力。",
        "from": "X19n0347_013:0574b10_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空中有天曰。",
        "from": "X19n0347_013:0574b10_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子。",
        "from": "X19n0347_013:0574b10_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法之供養。",
        "from": "X19n0347_013:0574b10_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勝諸供養。",
        "from": "X19n0347_013:0574b11_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即問何謂法之供養。",
        "from": "X19n0347_013:0574b11_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天曰。",
        "from": "X19n0347_013:0574b11_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝可往問藥王如來。",
        "from": "X19n0347_013:0574b11_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當廣為汝說法之供養。",
        "from": "X19n0347_013:0574b12_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即時月蓋王子。",
        "from": "X19n0347_013:0574b12_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行詣藥王如來。",
        "from": "X19n0347_013:0574b13_54",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稽首佛足却住一面。",
        "from": "X19n0347_013:0574b13_55",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊。",
        "from": "X19n0347_013:0574b13_56",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸供養中。",
        "from": "X19n0347_013:0574b14_57",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法供養勝。",
        "from": "X19n0347_013:0574b14_58",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名為法之供養(起句)。",
        "from": "X19n0347_013:0574b14_59",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言善男子。",
        "from": "X19n0347_013:0574b15_60",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法供養者。",
        "from": "X19n0347_013:0574b15_61",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛所說深經。",
        "from": "X19n0347_013:0574b15_62",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切世間。",
        "from": "X19n0347_013:0574b15_63",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難信難受。",
        "from": "X19n0347_013:0574b16_64",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微妙難見。",
        "from": "X19n0347_013:0574b16_65",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清淨無染。",
        "from": "X19n0347_013:0574b16_66",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非但分別思惟之所能得。",
        "from": "X19n0347_013:0574b16_67",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩法藏所攝。",
        "from": "X19n0347_013:0574b17_68",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陀羅尼印印之。",
        "from": "X19n0347_013:0574b17_69",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至不退轉。",
        "from": "X19n0347_013:0574b18_70",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就六度。",
        "from": "X19n0347_013:0574b18_71",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善分別義。",
        "from": "X19n0347_013:0574b18_72",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "順菩提法。",
        "from": "X19n0347_013:0574b18_73",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾經之上。",
        "from": "X19n0347_013:0574b18_74",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入大慈悲。",
        "from": "X19n0347_013:0574b19_75",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離眾魔事及諸邪見。",
        "from": "X19n0347_013:0574b19_76",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "順因緣法。",
        "from": "X19n0347_013:0574b19_77",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無我無人。",
        "from": "X19n0347_013:0574b20_78",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無眾生。",
        "from": "X19n0347_013:0574b20_79",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無壽命。",
        "from": "X19n0347_013:0574b20_80",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空無相無作無起。",
        "from": "X19n0347_013:0574b20_81",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令眾生坐於道場而轉法輪。",
        "from": "X19n0347_013:0574b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天龍神。",
        "from": "X19n0347_013:0574b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乾達婆等。",
        "from": "X19n0347_013:0574b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所共歎譽。",
        "from": "X19n0347_013:0574b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令眾生。",
        "from": "X19n0347_013:0574b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入佛法藏。",
        "from": "X19n0347_013:0574b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝諸賢聖一切智慧。",
        "from": "X19n0347_013:0574b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說諸菩薩所行之道。",
        "from": "X19n0347_013:0574b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依於諸法實相之義。",
        "from": "X19n0347_013:0574b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明宣無常苦空無我寂滅之法。",
        "from": "X19n0347_013:0574b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能救一切毀禁眾生。",
        "from": "X19n0347_013:0574b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸魔外道。",
        "from": "X19n0347_013:0574c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及貪著者。",
        "from": "X19n0347_013:0574c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能使怖畏。",
        "from": "X19n0347_013:0574c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛賢聖。",
        "from": "X19n0347_013:0574c01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所共稱歎。",
        "from": "X19n0347_013:0574c02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "背生死苦。",
        "from": "X19n0347_013:0574c02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "示涅槃樂。",
        "from": "X19n0347_013:0574c02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方三世諸佛所說。",
        "from": "X19n0347_013:0574c02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若聞如是等經。",
        "from": "X19n0347_013:0574c03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信解受持讀誦。",
        "from": "X19n0347_013:0574c03_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以方便力。",
        "from": "X19n0347_013:0574c03_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸眾生。",
        "from": "X19n0347_013:0574c04_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分別解說。",
        "from": "X19n0347_013:0574c04_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯示分明。",
        "from": "X19n0347_013:0574c04_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "守護法故。",
        "from": "X19n0347_013:0574c04_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名法之供養(收句)。",
        "from": "X19n0347_013:0574c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又於諸法如說修行。",
        "from": "X19n0347_013:0574c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨順十二因緣。",
        "from": "X19n0347_013:0574c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離諸邪見。",
        "from": "X19n0347_013:0574c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得無生忍。",
        "from": "X19n0347_013:0574c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "決定無我。",
        "from": "X19n0347_013:0574c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有眾生。",
        "from": "X19n0347_013:0574c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而於因緣果報。",
        "from": "X19n0347_013:0574c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無違無諍。",
        "from": "X19n0347_013:0574c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離諸我所。",
        "from": "X19n0347_013:0574c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依於義。",
        "from": "X19n0347_013:0574c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不依語。",
        "from": "X19n0347_013:0574c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依於智。",
        "from": "X19n0347_013:0574c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不依識。",
        "from": "X19n0347_013:0574c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依了義經。",
        "from": "X19n0347_013:0574c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不依不了義經。",
        "from": "X19n0347_013:0574c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依於法。",
        "from": "X19n0347_013:0574c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不依人。",
        "from": "X19n0347_013:0574c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨順法相。",
        "from": "X19n0347_013:0574c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無所入。",
        "from": "X19n0347_013:0574c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無所歸。",
        "from": "X19n0347_013:0574c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無明畢竟滅。",
        "from": "X19n0347_013:0574c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故諸行亦畢竟滅。",
        "from": "X19n0347_013:0574c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至生畢竟滅。",
        "from": "X19n0347_013:0574c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故老死亦畢竟滅作如是觀。",
        "from": "X19n0347_013:0574c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二因緣。",
        "from": "X19n0347_013:0574c11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有盡相。",
        "from": "X19n0347_013:0574c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不復起見。",
        "from": "X19n0347_013:0574c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名最上法之供養(又收句)。",
        "from": "X19n0347_013:0574c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告天帝。",
        "from": "X19n0347_013:0574c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王子月蓋從藥王佛。",
        "from": "X19n0347_013:0574c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞如是法。",
        "from": "X19n0347_013:0574c13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得柔順忍。",
        "from": "X19n0347_013:0574c13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即解寶衣嚴身之具。",
        "from": "X19n0347_013:0574c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以供養佛。",
        "from": "X19n0347_013:0574c14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言。",
        "from": "X19n0347_013:0574c14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "X19n0347_013:0574c14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來滅後。",
        "from": "X19n0347_013:0574c14_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當行法供養。",
        "from": "X19n0347_013:0574c14_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "守護正法。",
        "from": "X19n0347_013:0574c15_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願以威神。",
        "from": "X19n0347_013:0574c15_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加哀建立。",
        "from": "X19n0347_013:0574c15_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令我得降伏魔怨。",
        "from": "X19n0347_013:0574c15_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修菩薩行。",
        "from": "X19n0347_013:0574c16_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛知其深心所念。",
        "from": "X19n0347_013:0574c16_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而記之曰。",
        "from": "X19n0347_013:0574c16_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝於末後。",
        "from": "X19n0347_013:0574c17_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "守護法城。",
        "from": "X19n0347_013:0574c17_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天帝。",
        "from": "X19n0347_013:0574c17_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時王子月蓋。",
        "from": "X19n0347_013:0574c17_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見法清淨。",
        "from": "X19n0347_013:0574c17_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛授記。",
        "from": "X19n0347_013:0574c18_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以信出家。",
        "from": "X19n0347_013:0574c18_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修習善法。",
        "from": "X19n0347_013:0574c18_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精進不久。",
        "from": "X19n0347_013:0574c18_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得五神通。",
        "from": "X19n0347_013:0575a01_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具菩薩道。",
        "from": "X19n0347_013:0575a01_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得陀羅尼無斷辯才。",
        "from": "X19n0347_013:0575a01_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於佛滅後。",
        "from": "X19n0347_013:0575a01_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以其所得神通總持辯才之力。",
        "from": "X19n0347_013:0575a02_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿十小劫。",
        "from": "X19n0347_013:0575a02_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥王如來所轉法輪。",
        "from": "X19n0347_013:0575a03_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨而分布。",
        "from": "X19n0347_013:0575a03_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月蓋比丘。",
        "from": "X19n0347_013:0575a03_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以守護法。",
        "from": "X19n0347_013:0575a03_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勤行精進。",
        "from": "X19n0347_013:0575a04_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於此身。",
        "from": "X19n0347_013:0575a04_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化百萬億人。",
        "from": "X19n0347_013:0575a04_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於阿耨多羅三藐三菩提。",
        "from": "X19n0347_013:0575a04_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立不退轉。",
        "from": "X19n0347_013:0575a05_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十四那由他人。",
        "from": "X19n0347_013:0575a05_54",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深發聲聞辟支佛心。",
        "from": "X19n0347_013:0575a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量眾生。",
        "from": "X19n0347_013:0575a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得生天上。",
        "from": "X19n0347_013:0575a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天帝。",
        "from": "X19n0347_013:0575a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時王寶葢。",
        "from": "X19n0347_013:0575a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈異人乎。",
        "from": "X19n0347_013:0575a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今現得佛號寶焰如來。",
        "from": "X19n0347_013:0575a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其王千子。",
        "from": "X19n0347_013:0575a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即賢劫中千佛是也。",
        "from": "X19n0347_013:0575a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從迦羅鳩孫馱為始得佛。",
        "from": "X19n0347_013:0575a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最後如來。",
        "from": "X19n0347_013:0575a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號曰樓至。",
        "from": "X19n0347_013:0575a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月蓋比丘。",
        "from": "X19n0347_013:0575a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則我身是。",
        "from": "X19n0347_013:0575a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是。",
        "from": "X19n0347_013:0575a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天帝。",
        "from": "X19n0347_013:0575a10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知此要。",
        "from": "X19n0347_013:0575a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以法供養。",
        "from": "X19n0347_013:0575a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸供養。",
        "from": "X19n0347_013:0575a10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為上為最。",
        "from": "X19n0347_013:0575a10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一無比。",
        "from": "X19n0347_013:0575a11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故天帝。",
        "from": "X19n0347_013:0575a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當以法之供養供",
        "from": "X19n0347_013:0575a11_18",
        "context_with_next_sentence": false
    }
]