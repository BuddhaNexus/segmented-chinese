[
    {
        "source_sentence": "救諸眾生一切苦難經",
        "from": "T85n2915_001:1461c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天台山中有一老師。",
        "from": "T85n2915_001:1461c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年可九□□□□□□□神悲哭眼中泣血唱言。",
        "from": "T85n2915_001:1461c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦□□□□□□□弟子惠通合掌頂禮。",
        "from": "T85n2915_001:1461c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼中泣淚□□□□□災難如何得免。",
        "from": "T85n2915_001:1461c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師報言惠通。",
        "from": "T85n2915_001:1461c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□□□□眾生亡沒普念彌勒□□□□□□□已比相相魏之地。",
        "from": "T85n2915_001:1461c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正在其中愚□□□□□□□三月四月鬼兵亂起無邊無際。",
        "from": "T85n2915_001:1461c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八月九月□□□末劫眾生行善鬼兵自滅。",
        "from": "T85n2915_001:1461c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天地黑闇得免□寫一本免一門。",
        "from": "T85n2915_001:1461c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寫兩本免六親。",
        "from": "T85n2915_001:1461c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寫三本免一村。",
        "from": "T85n2915_001:1461c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流傳者是弟子。",
        "from": "T85n2915_001:1461c17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謗此經者。",
        "from": "T85n2915_001:1461c17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入阿鼻地獄無有出期。",
        "from": "T85n2915_001:1461c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見此經不寫者滅門。",
        "from": "T85n2915_001:1461c18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至心讀誦者得成佛道。",
        "from": "T85n2915_001:1461c18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　黑風西北起　　東南興鬼兵",
        "from": "T85n2915_001:1461c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　永常天地闇　　何得心□□",
        "from": "T85n2915_001:1461c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　先須斷酒肉　　貪嗔更莫生",
        "from": "T85n2915_001:1461c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　人能慎此事　　佛道一時□",
        "from": "T85n2915_001:1461c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救諸眾生一切苦難經一卷",
        "from": "T85n2915_001:1461c24_0",
        "context_with_next_sentence": false
    }
]