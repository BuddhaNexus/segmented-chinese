[
    {
        "source_sentence": "居士傳三",
        "from": "X88n1646_003:0187c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二張王宗周雷傳",
        "from": "X88n1646_003:0187c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "張萊民名野。",
        "from": "X88n1646_003:0187c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尋陽人也。",
        "from": "X88n1646_003:0187c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性孝友。",
        "from": "X88n1646_003:0187c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "田宅悉推與弟。",
        "from": "X88n1646_003:0187c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甘苦與九族共之。",
        "from": "X88n1646_003:0187c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "州郡屢辟不應。",
        "from": "X88n1646_003:0188a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朝命拜散騎常侍亦不就。",
        "from": "X88n1646_003:0188a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入廬山白蓮社。",
        "from": "X88n1646_003:0188a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱遠公門人。",
        "from": "X88n1646_003:0188a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義熈十四年與家人別入室端坐而逝。",
        "from": "X88n1646_003:0188a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年六十九(東林傳)。",
        "from": "X88n1646_003:0188a03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "張秀實名詮。",
        "from": "X88n1646_003:0188a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "野之族子也。",
        "from": "X88n1646_003:0188a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尚情高逸。",
        "from": "X88n1646_003:0188a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "好古樂道。",
        "from": "X88n1646_003:0188a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "荷鉏力田。",
        "from": "X88n1646_003:0188a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶經不釋。",
        "from": "X88n1646_003:0188a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以散騎常侍徵不起。",
        "from": "X88n1646_003:0188a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庾悅憐其貧。",
        "from": "X88n1646_003:0188a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薦為尋陽令。",
        "from": "X88n1646_003:0188a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不應。",
        "from": "X88n1646_003:0188a06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已而入廬山依遠公。",
        "from": "X88n1646_003:0188a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "研窮佛經深有悟入。",
        "from": "X88n1646_003:0188a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宋景平元年西向念佛無疾而卒。",
        "from": "X88n1646_003:0188a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年六十五(東林傳)。",
        "from": "X88n1646_003:0188a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王喬之琅邪人。",
        "from": "X88n1646_003:0188a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "官臨賀大守。",
        "from": "X88n1646_003:0188a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已而入白蓮社事遠公。",
        "from": "X88n1646_003:0188a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與劉遺民諸賢作念佛三昧詩。",
        "from": "X88n1646_003:0188a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而喬之詩獨傳於世。",
        "from": "X88n1646_003:0188a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詩曰。",
        "from": "X88n1646_003:0188a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙用在茲。",
        "from": "X88n1646_003:0188a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "涉有覧無。",
        "from": "X88n1646_003:0188a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神由昧徹。",
        "from": "X88n1646_003:0188a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識以照麤。",
        "from": "X88n1646_003:0188a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "積微自引。",
        "from": "X88n1646_003:0188a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因功本虗。",
        "from": "X88n1646_003:0188a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "泯彼三觀。",
        "from": "X88n1646_003:0188a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忘此毫餘。",
        "from": "X88n1646_003:0188a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其二曰。",
        "from": "X88n1646_003:0188a12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寂寞何始。",
        "from": "X88n1646_003:0188a12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "履元通微。",
        "from": "X88n1646_003:0188a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "融然忘適。",
        "from": "X88n1646_003:0188a13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃廓靈暉。",
        "from": "X88n1646_003:0188a13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心游緬域。",
        "from": "X88n1646_003:0188a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得不踐機。",
        "from": "X88n1646_003:0188a13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用之以冲。",
        "from": "X88n1646_003:0188a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "會之以希。",
        "from": "X88n1646_003:0188a14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其三曰。",
        "from": "X88n1646_003:0188a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神姿天凝。",
        "from": "X88n1646_003:0188a14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓映朝雲。",
        "from": "X88n1646_003:0188a14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與化而咸。",
        "from": "X88n1646_003:0188a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與佛斯群。",
        "from": "X88n1646_003:0188a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應不以方。",
        "from": "X88n1646_003:0188a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受者自分。",
        "from": "X88n1646_003:0188a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寂爾淵鏡。",
        "from": "X88n1646_003:0188a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謝彼塵紛。",
        "from": "X88n1646_003:0188a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其四曰。",
        "from": "X88n1646_003:0188a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慨自一生。",
        "from": "X88n1646_003:0188a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夙乏慧識。",
        "from": "X88n1646_003:0188a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "託崇淵人。",
        "from": "X88n1646_003:0188a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庶藉冥力。",
        "from": "X88n1646_003:0188a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思轉毫功。",
        "from": "X88n1646_003:0188a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在深不測。",
        "from": "X88n1646_003:0188a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至哉之念。",
        "from": "X88n1646_003:0188a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "注心西極。",
        "from": "X88n1646_003:0188a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠公為作序云。",
        "from": "X88n1646_003:0188a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫稱三昧者何。",
        "from": "X88n1646_003:0188a18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "專思寂想之謂也。",
        "from": "X88n1646_003:0188a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思專則志一不分。",
        "from": "X88n1646_003:0188a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想寂則氣虗神朗。",
        "from": "X88n1646_003:0188a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "氣虗則智悟其照。",
        "from": "X88n1646_003:0188a19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神朗則無幽不徹。",
        "from": "X88n1646_003:0188a19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是二者自然之元符。",
        "from": "X88n1646_003:0188a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "會一而致用也。",
        "from": "X88n1646_003:0188a20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又諸三昧。",
        "from": "X88n1646_003:0188a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其名甚眾。",
        "from": "X88n1646_003:0188a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功高易進。",
        "from": "X88n1646_003:0188a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念佛為先。",
        "from": "X88n1646_003:0188a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何者。",
        "from": "X88n1646_003:0188a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "窮元極寂尊號如來。",
        "from": "X88n1646_003:0188a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體神合變應不以方。",
        "from": "X88n1646_003:0188a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故令入斯定者昧然忘知。",
        "from": "X88n1646_003:0188a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即所緣以成鑒。",
        "from": "X88n1646_003:0188a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鑒明則內照交映而萬象生焉。",
        "from": "X88n1646_003:0188a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非耳目之所暨而聞見行焉。",
        "from": "X88n1646_003:0188a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是靈相湛一清明自然。",
        "from": "X88n1646_003:0188a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元音叩心滯情融朗。",
        "from": "X88n1646_003:0188b01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非天下之至妙孰能與於此哉。",
        "from": "X88n1646_003:0188b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以奉法諸賢。",
        "from": "X88n1646_003:0188b02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸思一揆之契。",
        "from": "X88n1646_003:0188b02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "感寸陰之將頹。",
        "from": "X88n1646_003:0188b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懼來儲之未積。",
        "from": "X88n1646_003:0188b03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洗心法堂。",
        "from": "X88n1646_003:0188b03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "整襟清向。",
        "from": "X88n1646_003:0188b03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜分忘寢。",
        "from": "X88n1646_003:0188b03_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夙興唯勤。",
        "from": "X88n1646_003:0188b03_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庶夫貞詣之功以通三乘之志。",
        "from": "X88n1646_003:0188b04_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仰援超步㧞茅之興。",
        "from": "X88n1646_003:0188b04_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俯引弱進垂策其後。",
        "from": "X88n1646_003:0188b05_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此覧眾萹之揮翰。",
        "from": "X88n1646_003:0188b05_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈徒文詠而已哉(宏明集．盧山集○喬之一作齊之。",
        "from": "X88n1646_003:0188b05_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或其字也)。",
        "from": "X88n1646_003:0188b06_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宗少文名炳。",
        "from": "X88n1646_003:0188b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南陽人。",
        "from": "X88n1646_003:0188b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙善琴書。",
        "from": "X88n1646_003:0188b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精於理論。",
        "from": "X88n1646_003:0188b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殷仲堪．桓元．劉裕並以主簿辟不就。",
        "from": "X88n1646_003:0188b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃入廬山事遠師。",
        "from": "X88n1646_003:0188b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "築室依白蓮社居之。",
        "from": "X88n1646_003:0188b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既而兄臧為南平守。",
        "from": "X88n1646_003:0188b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偪與俱還江陵。",
        "from": "X88n1646_003:0188b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "閒居絕俗。",
        "from": "X88n1646_003:0188b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頗營稼穡以給其家。",
        "from": "X88n1646_003:0188b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衡陽王義季親訪之。",
        "from": "X88n1646_003:0188b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少文角巾布衣引見不拜。",
        "from": "X88n1646_003:0188b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王曰處先生以重祿可乎。",
        "from": "X88n1646_003:0188b11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對曰。",
        "from": "X88n1646_003:0188b11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祿如秋草。",
        "from": "X88n1646_003:0188b12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時過即腐。",
        "from": "X88n1646_003:0188b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宋受禪。",
        "from": "X88n1646_003:0188b12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三徵不應。",
        "from": "X88n1646_003:0188b12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雅好山水。",
        "from": "X88n1646_003:0188b12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西陟荊巫。",
        "from": "X88n1646_003:0188b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南登衝岳。",
        "from": "X88n1646_003:0188b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以疾還江陵。",
        "from": "X88n1646_003:0188b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歎曰。",
        "from": "X88n1646_003:0188b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "老病俱至。",
        "from": "X88n1646_003:0188b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名山不可再登。",
        "from": "X88n1646_003:0188b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯澄懷觀道。",
        "from": "X88n1646_003:0188b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臥以遊之。",
        "from": "X88n1646_003:0188b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡所游履。",
        "from": "X88n1646_003:0188b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉圖之於壁。",
        "from": "X88n1646_003:0188b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂人曰。",
        "from": "X88n1646_003:0188b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "撫琴動操欲令眾山皆響。",
        "from": "X88n1646_003:0188b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甞著神不滅論。",
        "from": "X88n1646_003:0188b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其略云。",
        "from": "X88n1646_003:0188b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "群生之神。",
        "from": "X88n1646_003:0188b16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其極雖齊。",
        "from": "X88n1646_003:0188b16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而隨緣遷流。",
        "from": "X88n1646_003:0188b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成麤妙之識。",
        "from": "X88n1646_003:0188b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫舜生於瞽。",
        "from": "X88n1646_003:0188b17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舜之神也必非瞽之所生。",
        "from": "X88n1646_003:0188b17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則商均之神又非舜之所育。",
        "from": "X88n1646_003:0188b17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生育之前素有麤妙矣。",
        "from": "X88n1646_003:0188b18_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既本立於未生之先。",
        "from": "X88n1646_003:0188b18_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則知不滅於既死之後矣。",
        "from": "X88n1646_003:0188b19_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神也者。",
        "from": "X88n1646_003:0188b19_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙萬物而為言矣。",
        "from": "X88n1646_003:0188b19_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若資形以造。",
        "from": "X88n1646_003:0188b20_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨形以滅。",
        "from": "X88n1646_003:0188b20_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則以形為本。",
        "from": "X88n1646_003:0188b20_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何妙以言乎。",
        "from": "X88n1646_003:0188b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誠能澄不滅之本。",
        "from": "X88n1646_003:0188b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稟日損之學。",
        "from": "X88n1646_003:0188b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "損之又損以至無為。",
        "from": "X88n1646_003:0188b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無為則無當於生矣。",
        "from": "X88n1646_003:0188b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無生則無身。",
        "from": "X88n1646_003:0188b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無身而有神。",
        "from": "X88n1646_003:0188b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法身之謂也。",
        "from": "X88n1646_003:0188b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今黃帝．虞舜．姬公．孔父世之所仰而信者也。",
        "from": "X88n1646_003:0188b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀其縱轡升天。",
        "from": "X88n1646_003:0188b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍潛鳳颺。",
        "from": "X88n1646_003:0188b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "反風起禾。",
        "from": "X88n1646_003:0188b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "絕粒弦歌。",
        "from": "X88n1646_003:0188b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦皆由窮神為體。",
        "from": "X88n1646_003:0188c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故神功所應倜儻無方也。",
        "from": "X88n1646_003:0188c01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫洪範庶徵休咎之應。",
        "from": "X88n1646_003:0188c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆由心來。",
        "from": "X88n1646_003:0188c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逮白虹貫日。",
        "from": "X88n1646_003:0188c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "太白入昴。",
        "from": "X88n1646_003:0188c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寒谷生黍崩城隕霜之類。",
        "from": "X88n1646_003:0188c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆發自人情而遠形天事。",
        "from": "X88n1646_003:0188c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "固相為形影矣。",
        "from": "X88n1646_003:0188c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫形無無影。",
        "from": "X88n1646_003:0188c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聲無無響。",
        "from": "X88n1646_003:0188c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦情無無報矣。",
        "from": "X88n1646_003:0188c04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈直貫日隕霜之類哉。",
        "from": "X88n1646_003:0188c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫不隨情曲應。",
        "from": "X88n1646_003:0188c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "物無遁形。",
        "from": "X88n1646_003:0188c05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但或結於身。",
        "from": "X88n1646_003:0188c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或播於事。",
        "from": "X88n1646_003:0188c06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "交賒紛綸。",
        "from": "X88n1646_003:0188c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯昧渺漫。",
        "from": "X88n1646_003:0188c06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孰覩其際哉。",
        "from": "X88n1646_003:0188c06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾變盈世。",
        "from": "X88n1646_003:0188c07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "群象滿目。",
        "from": "X88n1646_003:0188c07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆萬世以來精感之所集矣。",
        "from": "X88n1646_003:0188c07_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故佛經云。",
        "from": "X88n1646_003:0188c08_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸法從意生形。",
        "from": "X88n1646_003:0188c08_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又云。",
        "from": "X88n1646_003:0188c08_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心為法本。",
        "from": "X88n1646_003:0188c08_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心作天堂。",
        "from": "X88n1646_003:0188c09_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心作地獄。",
        "from": "X88n1646_003:0188c09_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義由此也。",
        "from": "X88n1646_003:0188c09_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是以清心潔情必妙生於英麗之境。",
        "from": "X88n1646_003:0188c09_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "濁情滓行必永滯於三塗之域。",
        "from": "X88n1646_003:0188c10_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫神聖圓照而無思營之識者。",
        "from": "X88n1646_003:0188c10_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由心與物絕。",
        "from": "X88n1646_003:0188c11_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯神而已。",
        "from": "X88n1646_003:0188c11_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故虗明之本終始長住。",
        "from": "X88n1646_003:0188c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可凋矣。",
        "from": "X88n1646_003:0188c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今以悟空息心。",
        "from": "X88n1646_003:0188c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心用止而情識歇。",
        "from": "X88n1646_003:0188c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則神明全矣。",
        "from": "X88n1646_003:0188c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顏子知其如此。",
        "from": "X88n1646_003:0188c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故處有若無。",
        "from": "X88n1646_003:0188c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "撫實若虗。",
        "from": "X88n1646_003:0188c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不見有犯而不校也。",
        "from": "X88n1646_003:0188c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今觀顏子之屢空。",
        "from": "X88n1646_003:0188c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則知其有之實無矣。",
        "from": "X88n1646_003:0188c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且舟壑潛謝。",
        "from": "X88n1646_003:0188c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變速奔電。",
        "from": "X88n1646_003:0188c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將來未至。",
        "from": "X88n1646_003:0188c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過去已滅。",
        "from": "X88n1646_003:0188c16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現在不住。",
        "from": "X88n1646_003:0188c16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞬息之頃無一毫可據。",
        "from": "X88n1646_003:0188c16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將欲何守而以為有乎。",
        "from": "X88n1646_003:0188c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甚矣偽有之蔽神也。",
        "from": "X88n1646_003:0188c17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今有明鏡於斯。",
        "from": "X88n1646_003:0188c17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "紛穢集之。",
        "from": "X88n1646_003:0188c18_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微則其照藹然。",
        "from": "X88n1646_003:0188c18_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "積則其照朏然。",
        "from": "X88n1646_003:0188c18_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彌厚則照而昧矣。",
        "from": "X88n1646_003:0188c18_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人之神理有類於此。",
        "from": "X88n1646_003:0188c19_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偽有累神。",
        "from": "X88n1646_003:0188c19_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成精麤之識。",
        "from": "X88n1646_003:0188c19_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識附於神。",
        "from": "X88n1646_003:0188c20_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故雖死不滅。",
        "from": "X88n1646_003:0188c20_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漸之以空。",
        "from": "X88n1646_003:0188c20_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必將習漸至盡而窮本神矣。",
        "from": "X88n1646_003:0188c20_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "泥洹之謂也。",
        "from": "X88n1646_003:0188c21_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫自古所以不顯至道者。",
        "from": "X88n1646_003:0188c21_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將存其生也。",
        "from": "X88n1646_003:0188c22_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而苦由生來。",
        "from": "X88n1646_003:0188c22_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昧者不知矣。",
        "from": "X88n1646_003:0188c22_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且時則無止。",
        "from": "X88n1646_003:0188c22_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運則無窮。",
        "from": "X88n1646_003:0188c23_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既往積劫無數無邊。",
        "from": "X88n1646_003:0188c23_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆一瞬所閱以及今耳。",
        "from": "X88n1646_003:0188c23_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今積瞬以至百年。",
        "from": "X88n1646_003:0188c24_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曾幾何時而又鮮克半焉。",
        "from": "X88n1646_003:0188c24_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫物之媚於朝露之身者。",
        "from": "X88n1646_003:0188c24_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "類無清遐之實矣。",
        "from": "X88n1646_003:0189a01_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何為甘臭腐於漏刻。",
        "from": "X88n1646_003:0189a01_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以枉長存之神。",
        "from": "X88n1646_003:0189a02_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而不自踈於遐遠之風哉。",
        "from": "X88n1646_003:0189a02_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元嘉二十年卒。",
        "from": "X88n1646_003:0189a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年六十九(宋書．東林傳．宏明集)。",
        "from": "X88n1646_003:0189a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周道祖名續之。",
        "from": "X88n1646_003:0189a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雁門人。",
        "from": "X88n1646_003:0189a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父歿。",
        "from": "X88n1646_003:0189a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過江居豫章。",
        "from": "X88n1646_003:0189a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年十二詣范寗受業。",
        "from": "X88n1646_003:0189a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通五經五緯。",
        "from": "X88n1646_003:0189a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "養志窮居。",
        "from": "X88n1646_003:0189a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精研老易。",
        "from": "X88n1646_003:0189a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既而入廬山白蓮社。",
        "from": "X88n1646_003:0189a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為身不可遺。",
        "from": "X88n1646_003:0189a06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘累宜絕。",
        "from": "X88n1646_003:0189a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂終身不娶妻。",
        "from": "X88n1646_003:0189a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "布衣蔬食。",
        "from": "X88n1646_003:0189a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朝命徵太學博士。",
        "from": "X88n1646_003:0189a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不就。",
        "from": "X88n1646_003:0189a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劉裕北伐。",
        "from": "X88n1646_003:0189a07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世子守彭城。",
        "from": "X88n1646_003:0189a07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迎之舘於安東寺。",
        "from": "X88n1646_003:0189a08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "延入講禮。",
        "from": "X88n1646_003:0189a08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月餘還山。",
        "from": "X88n1646_003:0189a08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "裕還鎮。",
        "from": "X88n1646_003:0189a08_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復遣使迎之。",
        "from": "X88n1646_003:0189a09_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宋受禪。",
        "from": "X88n1646_003:0189a09_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為開舘東郭外。",
        "from": "X88n1646_003:0189a09_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "招集生徒以禮經教授。",
        "from": "X88n1646_003:0189a09_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或問。",
        "from": "X88n1646_003:0189a10_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身為處士。",
        "from": "X88n1646_003:0189a10_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時踐王庭。",
        "from": "X88n1646_003:0189a10_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何也。",
        "from": "X88n1646_003:0189a10_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "X88n1646_003:0189a10_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心馳魏闕者。",
        "from": "X88n1646_003:0189a10_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以江湖為桎梏。",
        "from": "X88n1646_003:0189a11_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "情致兩忘者。",
        "from": "X88n1646_003:0189a11_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "市朝亦巖穴耳。",
        "from": "X88n1646_003:0189a11_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時號通隱先生。",
        "from": "X88n1646_003:0189a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未幾移疾鍾山。",
        "from": "X88n1646_003:0189a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "景平元年卒。",
        "from": "X88n1646_003:0189a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年六十七(宋書)。",
        "from": "X88n1646_003:0189a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雷仲倫名次宗。",
        "from": "X88n1646_003:0189a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南昌人。",
        "from": "X88n1646_003:0189a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少入廬山事遠師。",
        "from": "X88n1646_003:0189a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "篤志好學不交世務。",
        "from": "X88n1646_003:0189a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精於毛詩．三禮之學。",
        "from": "X88n1646_003:0189a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本州辟從事。",
        "from": "X88n1646_003:0189a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朝以員外散騎侍郎徵。",
        "from": "X88n1646_003:0189a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並不就。",
        "from": "X88n1646_003:0189a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年五十餘。",
        "from": "X88n1646_003:0189a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與子姪書。",
        "from": "X88n1646_003:0189a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以言所守曰。",
        "from": "X88n1646_003:0189a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫生之修短咸有定分。",
        "from": "X88n1646_003:0189a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定分之外不可以智力求。",
        "from": "X88n1646_003:0189a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但當於所稟之中順而勿害耳。",
        "from": "X88n1646_003:0189a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾少嬰羸患。",
        "from": "X88n1646_003:0189a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為性好閒。",
        "from": "X88n1646_003:0189a18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故在童稚之年已懷遠迹之意。",
        "from": "X88n1646_003:0189a19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "暨於弱冠。",
        "from": "X88n1646_003:0189a19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂託業廬山。",
        "from": "X88n1646_003:0189a19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逮事釋和尚。",
        "from": "X88n1646_003:0189a20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於時師友淵源務訓弘道。",
        "from": "X88n1646_003:0189a20_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外慕等夷內懷悱發。",
        "from": "X88n1646_003:0189a20_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是洗氣神明玩心墳典。",
        "from": "X88n1646_003:0189a21_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勉志勤躬夜以繼日。",
        "from": "X88n1646_003:0189a21_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爰有山水之好。",
        "from": "X88n1646_003:0189a22_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晤言之歎。",
        "from": "X88n1646_003:0189a22_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實足以通理輔性。",
        "from": "X88n1646_003:0189a22_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成夫亹亹之業。",
        "from": "X88n1646_003:0189a23_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樂以忘憂。",
        "from": "X88n1646_003:0189a23_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不知朝日之晏矣。",
        "from": "X88n1646_003:0189a23_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自游道[(歹*又)/食]風二十餘載。",
        "from": "X88n1646_003:0189a23_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淵匠既傾。",
        "from": "X88n1646_003:0189a24_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "良朋凋索。",
        "from": "X88n1646_003:0189a24_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "續以釁逆違天。",
        "from": "X88n1646_003:0189a24_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "備甞荼蓼。",
        "from": "X88n1646_003:0189b01_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疇昔誠願頓盡一朝。",
        "from": "X88n1646_003:0189b01_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心慮荒散情意衰損。",
        "from": "X88n1646_003:0189b01_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故遂與汝曹歸耕壟畔山居谷飲。",
        "from": "X88n1646_003:0189b02_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人理久絕日月不處。",
        "from": "X88n1646_003:0189b02_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忽復十年。",
        "from": "X88n1646_003:0189b03_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "崦嵫將迫前塗幾何。",
        "from": "X88n1646_003:0189b03_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及今耄未至昏衰不及頓。",
        "from": "X88n1646_003:0189b03_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尚可厲志於所期。",
        "from": "X88n1646_003:0189b04_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縱心於所託。",
        "from": "X88n1646_003:0189b04_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "棲誠來生之津梁。",
        "from": "X88n1646_003:0189b04_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "專氣暮年之攝養。",
        "from": "X88n1646_003:0189b05_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在心所期盡於此矣。",
        "from": "X88n1646_003:0189b05_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自今以往。",
        "from": "X88n1646_003:0189b05_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "家事大小一勿見關。",
        "from": "X88n1646_003:0189b06_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宋元嘉十五年徵至京師開舘雞籠山。",
        "from": "X88n1646_003:0189b06_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聚徒教授。",
        "from": "X88n1646_003:0189b07_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除給事中。",
        "from": "X88n1646_003:0189b07_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不就。",
        "from": "X88n1646_003:0189b07_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頃之還廬山。",
        "from": "X88n1646_003:0189b07_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十五年又徵詣京師為築室鍾山西巖下。",
        "from": "X88n1646_003:0189b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂之招隱舘。",
        "from": "X88n1646_003:0189b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使為皇太子諸王講喪服經。",
        "from": "X88n1646_003:0189b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是歲卒。",
        "from": "X88n1646_003:0189b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年六十三(宋書)。",
        "from": "X88n1646_003:0189b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知歸子曰。",
        "from": "X88n1646_003:0189b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白蓮社一百二十三人。",
        "from": "X88n1646_003:0189b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居士之可得紀者七人焉。",
        "from": "X88n1646_003:0189b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "才敏如康樂。",
        "from": "X88n1646_003:0189b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲求入社。",
        "from": "X88n1646_003:0189b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠公拒之。",
        "from": "X88n1646_003:0189b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誠慎所與哉。",
        "from": "X88n1646_003:0189b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而七人之外。",
        "from": "X88n1646_003:0189b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流風遺書不可得而見矣。",
        "from": "X88n1646_003:0189b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "予亦不得而稱之也。",
        "from": "X88n1646_003:0189b13_9",
        "context_with_next_sentence": false
    }
]