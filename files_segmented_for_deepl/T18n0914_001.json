[
    {
        "source_sentence": "火吽軌別錄",
        "from": "T18n0914_001:0936b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東北方大自在天王明。",
        "from": "T18n0914_001:0936b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩　麼　三　曼　跢　勃　馱　喃　唵　伊(去)舍　曩(去)　曳　娑嚩(二合引)　訶(引)(三七遍)東方帝釋天王明。",
        "from": "T18n0914_001:0936c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩　麼　三　曼　跢　勃　馱　喃　唵　因(去)捺囉(二合)　也　娑嚩(二合引)　訶(引)東南方火天王明。",
        "from": "T18n0914_001:0936c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩　麼　三　曼　跢　勃　馱　喃　唵　阿(上)　蘖曩(二合)曳　娑嚩(二合引)　訶(引)南方炎魔羅天王明。",
        "from": "T18n0914_001:0936c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩　麼　三　曼　跢　勃　馱　喃　唵　焰　磨曳　娑嚩(二合引)　訶(引)西南方涅哩底(丁以反)天王明。",
        "from": "T18n0914_001:0936c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩　麼　三　曼　跢　勃　馱　喃　唵　囉(去)　羯叉娑(去)　地　鉢跢(二合)　曳　娑嚩(二合引)　訶(引)西方水天王明。",
        "from": "T18n0914_001:0936c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩　麼　三　曼　跢　勃　馱　喃　唵　嚩　嚕拏　曳(去)　娑嚩(二合引)　訶(引)西北方風天王明。",
        "from": "T18n0914_001:0937a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩　麼　三　曼　跢　勃　馱　喃　唵　嚩(去)夜　吠　娑嚩(二合引)　訶(引)北方毘沙門天王明。",
        "from": "T18n0914_001:0937a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩　麼　三　曼　跢　勃　馱　喃　唵　藥　叉蜜　那也(二合)　陀　哩　娑嚩(二合引)　訶(引)上方大梵天王明(帝釋右)。",
        "from": "T18n0914_001:0937a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩　麼　三　曼　跢　勃　馱　喃　唵　跛囉(二合)惹(去)　鉢跢(二合)　曳　娑嚩(二合引)　訶(引)下方地居天王明(水天左)。",
        "from": "T18n0914_001:0937a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩　麼　三　曼　跢　勃　馱　喃　唵　跛羅(二合)智　謎曳(二合引)　娑嚩(引二合)　訶(引)日天子明(帝釋左)。",
        "from": "T18n0914_001:0937a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩　麼　三　曼　跢　勃　馱　喃　唵　阿(去)儞夜(二合)　多也(二合)　娑嚩(二合引)　訶(引)月天子明(水天右)。",
        "from": "T18n0914_001:0937a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩　麼　三　曼　跢　勃　馱　喃　唵　戰　捺囉(二合引)也　娑嚩(二合引)　訶(引)諸星天子明(同月天位)。",
        "from": "T18n0914_001:0937b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩　麼　三　曼　跢　勃　馱　喃　唵　諾　叉多羅(二合)　悌　縛　跢　曳　娑嚩(二合引)　訶(引)諸龍王明(同水天明)。",
        "from": "T18n0914_001:0937b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩　麼　三　曼　跢　勃　馱　喃　唵　謎　伽(去)捨　拏　曳　娑嚩(二合引)　訶(引)一切使者及諸鬼神等明(於東北方散施)。",
        "from": "T18n0914_001:0937b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妣　止　妣　止　毘　舍　止　喃　娑嚩(二合引)　訶(引)遇(上)　遇(上)　部　多(去)　喃　娑嚩(二合引)　訶(引)凡持誦不動尊真言。",
        "from": "T18n0914_001:0937b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆須作壇。",
        "from": "T18n0914_001:0937b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壇法即後樣是。",
        "from": "T18n0914_001:0937b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆以青色黑色花燒沈香安息香。",
        "from": "T18n0914_001:0937b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨時果子飲食燈火香水供養。",
        "from": "T18n0914_001:0937b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲供養時。",
        "from": "T18n0914_001:0937b28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者澡浴著新衣。",
        "from": "T18n0914_001:0937b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先誦本尊真言結本尊印。",
        "from": "T18n0914_001:0937c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以杓取香水灑諸供養物。",
        "from": "T18n0914_001:0937c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後供養名為淨護。",
        "from": "T18n0914_001:0937c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即諸魔不能穢惡。",
        "from": "T18n0914_001:0937c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼用虛空庫心供養法。",
        "from": "T18n0914_001:0937c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最不可思議。",
        "from": "T18n0914_001:0937c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "布置供養具訖。",
        "from": "T18n0914_001:0937c04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦三昧耶真言。",
        "from": "T18n0914_001:0937c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辟除結界。",
        "from": "T18n0914_001:0937c04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘盧遮那三昧耶真言(著座誦真言結印)。",
        "from": "T18n0914_001:0937c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜　麼　三　漫　跢　沒　馱　喃　唵　遏　三謎　底哩(二合)　三　謎　三　麼　曳　薩縛(二合引)訶(引)右結三昧耶印(合掌是也)印額左右肩心喉。",
        "from": "T18n0914_001:0937c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各誦前件真言一遍。",
        "from": "T18n0914_001:0937c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結不動尊印誦不動尊真言七遍。",
        "from": "T18n0914_001:0937c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左旋為辟除。",
        "from": "T18n0914_001:0937c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又誦三遍。",
        "from": "T18n0914_001:0937c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋為結界結界已。",
        "from": "T18n0914_001:0937c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結鉤印(以二手內交竪右頭指)請一切佛菩薩執金剛等。",
        "from": "T18n0914_001:0937c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降集道場。",
        "from": "T18n0914_001:0937c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鉤印真言曰。",
        "from": "T18n0914_001:0937c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜　麼　三　漫　多　沒　馱　喃　阿(引入)　薩　嚩怛囉(二合)　鉢囉(二合)　底(丁以反)　訶　多　怛　他(引)蘖　黨(引)　拘　捨　步　地　折　哩野(二合)波　哩　布　洛　迦　薩嚩(二合引)　訶(引)右誦一遍一鉤三鉤已。",
        "from": "T18n0914_001:0937c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即觀一切諸佛菩薩無量聖眾等悉皆降集。",
        "from": "T18n0914_001:0937c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後坐禮拜懺悔發願陳說所求。",
        "from": "T18n0914_001:0937c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後結毘盧遮那心印(以無名小指外交。",
        "from": "T18n0914_001:0937c30_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪中指屈上節。",
        "from": "T18n0914_001:0937c30_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令頭相拄以頭指各微屈。",
        "from": "T18n0914_001:0937c30_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以頭附中指上節側。",
        "from": "T18n0914_001:0938a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大母指外交之)誦五字真言如後。",
        "from": "T18n0914_001:0938a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘盧遮那降伏四魔解脫六趣滿足一切智智真言曰。",
        "from": "T18n0914_001:0938a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿(引)　味　羅　吽　欠",
        "from": "T18n0914_001:0938a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右對尊像結前印誦此真言七遍。",
        "from": "T18n0914_001:0938a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然把數珠誦百遍。",
        "from": "T18n0914_001:0938a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至千遍。",
        "from": "T18n0914_001:0938a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多多益善。",
        "from": "T18n0914_001:0938a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後結不動尊印(即劍印是也)。",
        "from": "T18n0914_001:0938a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦不動尊真言。",
        "from": "T18n0914_001:0938a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰。",
        "from": "T18n0914_001:0938a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜　麼　三　漫　哆　末　日囉(二合)　赧　戰拏　摩　訶(引)　嚕　瑟　拏　薩發　吒　野　吽怛囉(二合)　吒　桁(引)　莽(引)右對尊像結前印誦此真言七遍。",
        "from": "T18n0914_001:0938a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後把數珠誦百遍。",
        "from": "T18n0914_001:0938a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至千遍。",
        "from": "T18n0914_001:0938a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多多益善。",
        "from": "T18n0914_001:0938a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦了還須結本尊印又誦真言七遍。",
        "from": "T18n0914_001:0938a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又殷勤發願禮拜已。",
        "from": "T18n0914_001:0938a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更結前三昧耶印誦三昧耶真言三遍。",
        "from": "T18n0914_001:0938a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更結劍印左旋一匝解界已。",
        "from": "T18n0914_001:0938a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀諸聖眾各歸所土。",
        "from": "T18n0914_001:0938a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後出道場。",
        "from": "T18n0914_001:0938a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行住坐臥心念本尊真言。",
        "from": "T18n0914_001:0938a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不令閒斷。",
        "from": "T18n0914_001:0938a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不動尊施食真言。",
        "from": "T18n0914_001:0938a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜　麼　三　滿　哆　末　日囉(二合)　赧　怛囉(二合)吒　阿　畝　伽　戰　拏　麼　訶　[口*路]　瑟拏　薩發(二合)　吒　野　吽　怛囉(二合)　麼　野怛羅(二合)　麼　野　吽　怛羅(二合)　吒　桁(引)　莽(引)右每食常出一分食供養。",
        "from": "T18n0914_001:0938a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言一遍。",
        "from": "T18n0914_001:0938b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切處常得擁護。",
        "from": "T18n0914_001:0938b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "澡浴真言。",
        "from": "T18n0914_001:0938b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　主　薩嚩(二合)　訶(政和上處得之)右以水一抄誦前真言七遍。",
        "from": "T18n0914_001:0938b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持一抄水灑身。",
        "from": "T18n0914_001:0938b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即當洗浴。",
        "from": "T18n0914_001:0938b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前施八方食取豆果餅飯胡麻粳米塗香屑諸華等。",
        "from": "T18n0914_001:0938b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和水安瓶盆中。",
        "from": "T18n0914_001:0938b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以歡喜心四方散之。",
        "from": "T18n0914_001:0938b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施諸天鬼神。",
        "from": "T18n0914_001:0938b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗香末香眷屬。",
        "from": "T18n0914_001:0938b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各以其真言施之。",
        "from": "T18n0914_001:0938b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東北方自在天真言曰。",
        "from": "T18n0914_001:0938b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　伊　舍　那　耶　娑嚩(二合引)　訶(引)東方帝釋天真言曰。",
        "from": "T18n0914_001:0938b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　因　陀羅(二合)　曳　娑嚩(二合引)　訶(引)東南方火天真言曰。",
        "from": "T18n0914_001:0938b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　阿　哦那(二合)　曳　娑嚩(二合引)　訶(引)東方琰魔王真言曰。",
        "from": "T18n0914_001:0938b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　琰　魔　耶　娑嚩(二合引)　訶(引)西南方羅剎娑真言曰。",
        "from": "T18n0914_001:0938b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　羅　剎　娑　阿　地　鉢多(二合)　曳　娑嚩(二合引)訶(引)西方諸龍水神真言曰。",
        "from": "T18n0914_001:0938b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　嚩　魯　拏　耶　娑嚩(二合引)　訶(引)西北方諸風神真言曰。",
        "from": "T18n0914_001:0938c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　嚩　耶　鞞　娑嚩(二合引)　訶(引)北方諸夜叉真言曰。",
        "from": "T18n0914_001:0938c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　藥　叉　苾　地耶(二合)　鉢多(二合)　耶娑嚩(二合引)　訶(引)上方施諸類鬼神真言曰。",
        "from": "T18n0914_001:0938c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　顒　伊　顒　伊　曼　娑　儞　步　多　南娑嚩(二合引)　訶(引)下方真言曰。",
        "from": "T18n0914_001:0938c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　比　止　比　止　毘　舍　遮　南　娑嚩(二合引)訶(引)如上作法施已。",
        "from": "T18n0914_001:0938c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當淨手漱口還入壇中。",
        "from": "T18n0914_001:0938c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮佛菩薩如常念誦。",
        "from": "T18n0914_001:0938c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　汝天親護者　　於諸佛導師",
        "from": "T18n0914_001:0938c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　修行殊勝行　　淨地波羅蜜",
        "from": "T18n0914_001:0938c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如破魔軍眾　　釋師子救世",
        "from": "T18n0914_001:0938c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　我亦降伏魔　　我畫曼荼羅",
        "from": "T18n0914_001:0938c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩　莫　三　漫　跢　沒　馱　南　唵　畢哩(二合)體　微曳(二合)　娑嚩(二合引)　訶(引)火[合*牛]儀軌一卷護摩法　　　　攬字可觀菩提心。",
        "from": "T18n0914_001:0938c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先住",
        "from": "T18n0914_001:0939a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即請火天言。",
        "from": "T18n0914_001:0939a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　我今奉請　　火天之首　　火中之仙　先行宗敬　　降臨此座　　納受護摩請火天真言曰。",
        "from": "T18n0914_001:0939a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩　莫　三　漫　哆　沒　馱　南　唵　阿(上)　蘖曩(二合)曳　娑嚩(二合引)　訶(引)又火天印呪。",
        "from": "T18n0914_001:0939a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "準根本印。",
        "from": "T18n0914_001:0939a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯改屈二中指如半環。",
        "from": "T18n0914_001:0939a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勢頭勿相著。",
        "from": "T18n0914_001:0939a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是一法印。",
        "from": "T18n0914_001:0939a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請喚火天而供養之。",
        "from": "T18n0914_001:0939a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若獻供畢發遣火天。",
        "from": "T18n0914_001:0939a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則却伸二中指頭。",
        "from": "T18n0914_001:0939a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印呪曰。",
        "from": "T18n0914_001:0939a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜　謨　皤　伽　嚩　底(一)　隖　瑟膩　莎　野(二)　翳呬曳(三)　諦　劯(儒照反)　摩　理　禰(奴禮反)　榱　起(其宗反)娜　曳　窣嚩(二合引)　訶(引)是一印呪燒火天食。",
        "from": "T18n0914_001:0939a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時之三遍先請火天燒食供養後乃燒食供養諸佛菩薩金剛及諸神呪等。",
        "from": "T18n0914_001:0939a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火天呪言。",
        "from": "T18n0914_001:0939b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　阿　起娜(二合)　曳　&#;(乎合反)　寫　合　寫　縛歌　曩　野　莎嚩(二合引)　訶(引)事已可火天發遣用前火天呪印。",
        "from": "T18n0914_001:0939b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火口東　　　　　　　　　　或本有此圖。",
        "from": "T18n0914_001:0939b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火[合*牛]儀軌音義",
        "from": "T18n0914_001:0939b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "的(韻會丁歷反說文明也徐日光的然也增韻實也又端也又丁激反遠貌明見也亦的字明也曉也)　脆(清歲反易折也言香脆餅食者是胡脆餅歟)　著銜蒸作(蓋是飽[飢-几+俞]蒸餅等之類歟)　柈(步干反盤也)　奠(大見反定也置也薦也陳也調也)　豆(大候反量名也爾雅食器也今上下合調置也)　饌(上卷反飲食也)　芬(孚云反香和也)　葩(普加反草華之白也又皅字也今上下合盛也)　嚴被(被備也同音故代用之)　珤(古寶字也)　餐(千丹蒼千二反吞也無功而食祿曰素餐)　酷(口告反熟也)　烈(力折反威葉也菓空敷榮謂之為酷烈也)　低(當稽反也)　塊(口清口回二反也堛善逼反)　瀘(撫屢浮也滓陀武也)　精(子清反好也明也善也靈也正也)　抹(叵達叵結二反糜也糜正皮也饘也[示*祭]為糜碎[示*糜]止彼反碎也穰也精也今或糜也)　抄(初教反掠也強取)　糤(蘇旱反餅也熬稻粻餭又作饊)　菉豆(上音錄王芻也)　熅(郁文反烟熅無氣鬱烟也)　勃(蒲沒反健也盛也上府芻褒屢下賀鄔縷也)　爆(博教普角二反謂皮散起也)　吒(都嫁反噴也上葩託銘驅下曩樓)　疇(真流類也)　課(告臥反誡也)　秉(鄙直反執也)　類(力對反絲節也)　捏(乃結反捺搦也)。",
        "from": "T18n0914_001:0939b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火壇有三。",
        "from": "T18n0914_001:0939b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一安穩二富饒三除怨。",
        "from": "T18n0914_001:0939b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初安穩。",
        "from": "T18n0914_001:0939b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於露地撩治一壇中畫佛坐蓮花坐。",
        "from": "T18n0914_001:0939b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壇四面畫般若菩薩聲聞佛慈愍菩薩。",
        "from": "T18n0914_001:0939b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫佛相好。",
        "from": "T18n0914_001:0939c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫一朔。",
        "from": "T18n0914_001:0939c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更畫阿鉢羅爾多淨居天龍王。",
        "from": "T18n0914_001:0939c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壇中以香花供養。",
        "from": "T18n0914_001:0939c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於壇西南角作火壇。",
        "from": "T18n0914_001:0939c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高四指。",
        "from": "T18n0914_001:0939c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中穿火壇。",
        "from": "T18n0914_001:0939c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方一肘。",
        "from": "T18n0914_001:0939c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取白土塗爐中央。",
        "from": "T18n0914_001:0939c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臺高四指。",
        "from": "T18n0914_001:0939c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從輪一指。",
        "from": "T18n0914_001:0939c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中燒桑木或搆木柿木。",
        "from": "T18n0914_001:0939c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "木長一搩手。",
        "from": "T18n0914_001:0939c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取樹枝頭肥者蘇蜜酪塗兩頭。",
        "from": "T18n0914_001:0939c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一呪一燒。",
        "from": "T18n0914_001:0939c07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取大麥小麥蘇蜜酪乳。",
        "from": "T18n0914_001:0939c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一呪燒滿一千八遍。",
        "from": "T18n0914_001:0939c08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一三時時別□□滿七日。",
        "from": "T18n0914_001:0939c08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或半月一月即息寂靜。",
        "from": "T18n0914_001:0939c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著鮮白衣。",
        "from": "T18n0914_001:0939c09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於壇中念誦。",
        "from": "T18n0914_001:0939c10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即向火壇中作燒火法。",
        "from": "T18n0914_001:0939c10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用月一日起首。",
        "from": "T18n0914_001:0939c10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於黃昏作法。",
        "from": "T18n0914_001:0939c11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當爾之處淨居天集會得成安穩法。",
        "from": "T18n0914_001:0939c11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面向北坐。",
        "from": "T18n0914_001:0939c12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "茅萆席上。",
        "from": "T18n0914_001:0939c12_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結加趺坐。",
        "from": "T18n0914_001:0939c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒火壇一日三時泥壇香花供養已。",
        "from": "T18n0914_001:0939c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香自無針刺。",
        "from": "T18n0914_001:0939c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用供養即得除滅一切災厄。",
        "from": "T18n0914_001:0939c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦得長命。",
        "from": "T18n0914_001:0939c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除一切病。",
        "from": "T18n0914_001:0939c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有無□變怪。",
        "from": "T18n0914_001:0939c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並作此法。",
        "from": "T18n0914_001:0939c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說富饒火壇法。",
        "from": "T18n0914_001:0939c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲作火法者。",
        "from": "T18n0914_001:0939c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先須撩治一壇中畫佛。",
        "from": "T18n0914_001:0939c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四面觀音大勢及諸大菩薩梵天王妙吉祥天諸大藥叉諸仙大威德及訶利底母等。",
        "from": "T18n0914_001:0939c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此供養。",
        "from": "T18n0914_001:0939c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此法用十五日起首。",
        "from": "T18n0914_001:0939c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日欲昏時面向東坐。",
        "from": "T18n0914_001:0939c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "茅草圓席上。",
        "from": "T18n0914_001:0939c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "交□坐著淨衣發歡喜心。",
        "from": "T18n0914_001:0939c22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此時諸天集。",
        "from": "T18n0914_001:0939c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時彼人作法。",
        "from": "T18n0914_001:0939c23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天歡喜。",
        "from": "T18n0914_001:0939c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得成就。",
        "from": "T18n0914_001:0939c24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此壇外南邊作一大壇。",
        "from": "T18n0914_001:0939c24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方二肘。",
        "from": "T18n0914_001:0939c24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安修闊二指。",
        "from": "T18n0914_001:0939c25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "穿土深一肘。",
        "from": "T18n0914_001:0939c25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中心臺闊一搩手。",
        "from": "T18n0914_001:0939c25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高四指。",
        "from": "T18n0914_001:0939c26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用赤土壇。",
        "from": "T18n0914_001:0939c26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於臺中從蓮花之香供養。",
        "from": "T18n0914_001:0939c26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒搆木乳粥蘇酪等。",
        "from": "T18n0914_001:0939c27_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或烏麻和飯亦得。",
        "from": "T18n0914_001:0939c27_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日三時時別一千八遍。",
        "from": "T18n0914_001:0939c28_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其木磔手取樹枝柯指許。",
        "from": "T18n0914_001:0939c28_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "麁者蘇蜜乳酪塗兩頭一呪一燒。",
        "from": "T18n0914_001:0939c29_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿一千八遍或一百八遍。",
        "from": "T18n0914_001:0940a01_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或七日或半月。",
        "from": "T18n0914_001:0940a01_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是作法。",
        "from": "T18n0914_001:0940a02_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法即得成就。",
        "from": "T18n0914_001:0940a02_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三說降怨火壇法。",
        "from": "T18n0914_001:0940a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲降怨火法者。",
        "from": "T18n0914_001:0940a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用黑月八日或十五日。",
        "from": "T18n0914_001:0940a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或從夜半。",
        "from": "T18n0914_001:0940a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此時是諸惡鬼藥叉羅剎集會時。",
        "from": "T18n0914_001:0940a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即作三角壇。",
        "from": "T18n0914_001:0940a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□向南。",
        "from": "T18n0914_001:0940a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用黑土掃壇一肘。",
        "from": "T18n0914_001:0940a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高三指。",
        "from": "T18n0914_001:0940a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□闊二指。",
        "from": "T18n0914_001:0940a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中捉三叉取驢糞駱駝糞灰取三種物泥壇。",
        "from": "T18n0914_001:0940a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有怨家相惱瞋心即作此法。",
        "from": "T18n0914_001:0940a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著赤衣或青衣。",
        "from": "T18n0914_001:0940a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或更作大惡法取血染衣裳。",
        "from": "T18n0914_001:0940a09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "胡蹲坐。",
        "from": "T18n0914_001:0940a10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是足相蹋。",
        "from": "T18n0914_001:0940a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西向皺眉努眼咬齒。",
        "from": "T18n0914_001:0940a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端身正直心出語。",
        "from": "T18n0914_001:0940a11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壇亦得取糠和芥子。",
        "from": "T18n0914_001:0940a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日三時泥壇。",
        "from": "T18n0914_001:0940a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其壇上龍臭花取棘針。",
        "from": "T18n0914_001:0940a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "木撚火苦練木埿□壇亦得。",
        "from": "T18n0914_001:0940a13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取糠和芥子取自身血和。",
        "from": "T18n0914_001:0940a14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一呪一燒滿一千八遍。",
        "from": "T18n0914_001:0940a14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日三時時加如是燒人骨頭髮如是等物。",
        "from": "T18n0914_001:0940a15_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發願瞋之。",
        "from": "T18n0914_001:0940a16_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一呪一燒。",
        "from": "T18n0914_001:0940a16_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱名彼。",
        "from": "T18n0914_001:0940a16_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是降怨法。",
        "from": "T18n0914_001:0940a17_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令前人長病錢物散失。",
        "from": "T18n0914_001:0940a17_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身死血出。",
        "from": "T18n0914_001:0940a17_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身之節落人不喜見。",
        "from": "T18n0914_001:0940a18_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又說都火壇法。",
        "from": "T18n0914_001:0940a18_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深十六指。",
        "from": "T18n0914_001:0940a19_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闊三十二指。",
        "from": "T18n0914_001:0940a19_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東面著輪。",
        "from": "T18n0914_001:0940a19_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南面三古叉。",
        "from": "T18n0914_001:0940a19_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西面著龍。",
        "from": "T18n0914_001:0940a20_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北面著跋折羅。",
        "from": "T18n0914_001:0940a20_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中央著蓮花。",
        "from": "T18n0914_001:0940a20_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "余供養。",
        "from": "T18n0914_001:0940a21_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前作之。",
        "from": "T18n0914_001:0940a21_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又須□火星日起首。",
        "from": "T18n0914_001:0940a21_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說諸火天印法。",
        "from": "T18n0914_001:0940a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說諸火神印。",
        "from": "T18n0914_001:0940a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說成就時節。",
        "from": "T18n0914_001:0940a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若用五月中作法決定有雨。",
        "from": "T18n0914_001:0940a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若用三月中作法。",
        "from": "T18n0914_001:0940a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "決定有種種礙現。",
        "from": "T18n0914_001:0940a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若用正月作法。",
        "from": "T18n0914_001:0940a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "決定有種種障礙。",
        "from": "T18n0914_001:0940a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若用九月中作法。",
        "from": "T18n0914_001:0940a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "決之定有欻雷電。",
        "from": "T18n0914_001:0940a27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等月。",
        "from": "T18n0914_001:0940a28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆須十五日有是等相現。",
        "from": "T18n0914_001:0940a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "決定成就。",
        "from": "T18n0914_001:0940a28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作安穩法者。",
        "from": "T18n0914_001:0940b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用各月十五日七日三日十三日。",
        "from": "T18n0914_001:0940b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等日。",
        "from": "T18n0914_001:0940b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並是鬼星日也。",
        "from": "T18n0914_001:0940b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又日月蝕時最為第一。",
        "from": "T18n0914_001:0940b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二月三月十月十一月作富饒法。",
        "from": "T18n0914_001:0940b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四月五月作降怨法。",
        "from": "T18n0914_001:0940b04_6",
        "context_with_next_sentence": false
    }
]