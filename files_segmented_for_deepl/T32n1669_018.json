[
    {
        "source_sentence": "大宗地玄文本論卷第十八",
        "from": "T32n1669_018:0689c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬鳴菩薩造",
        "from": "T32n1669_018:0689c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真諦三藏譯",
        "from": "T32n1669_018:0689c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "◎摩訶空塵海藏王道路大決擇分第三十五如是已說廣大無盡解脫海海摩訶山王大決擇分。",
        "from": "T32n1669_018:0689c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說摩訶空塵海藏王道路大決擇分。",
        "from": "T32n1669_018:0689c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_018:0689c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_018:0689c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　海王道路中　　具足百自在",
        "from": "T32n1669_018:0689c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　以此因緣故　　建立海王名",
        "from": "T32n1669_018:0689c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_018:0689c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此偈中為明何義。",
        "from": "T32n1669_018:0689c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲現示海王藏中無有別法。",
        "from": "T32n1669_018:0689c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯有自在故。",
        "from": "T32n1669_018:0689c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有幾自在。",
        "from": "T32n1669_018:0689c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂百種故。",
        "from": "T32n1669_018:0689c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為百。",
        "from": "T32n1669_018:0689c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者時自在。",
        "from": "T32n1669_018:0689c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十二法。",
        "from": "T32n1669_018:0689c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦一時轉亦異時轉。",
        "from": "T32n1669_018:0689c15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦轉時中不轉。",
        "from": "T32n1669_018:0689c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不轉時中轉。",
        "from": "T32n1669_018:0689c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦遠時轉亦近時轉。",
        "from": "T32n1669_018:0689c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至無量故。",
        "from": "T32n1669_018:0689c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者處自在。",
        "from": "T32n1669_018:0689c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一異等處轉。",
        "from": "T32n1669_018:0689c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至無量故。",
        "from": "T32n1669_018:0689c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者物自在。",
        "from": "T32n1669_018:0689c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為同異等用。",
        "from": "T32n1669_018:0689c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至無量故。",
        "from": "T32n1669_018:0689c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者周遍自在。",
        "from": "T32n1669_018:0689c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無所不通等。",
        "from": "T32n1669_018:0689c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至無量故。",
        "from": "T32n1669_018:0689c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者大小自在。",
        "from": "T32n1669_018:0689c20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "極重極微等。",
        "from": "T32n1669_018:0689c20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至無量故。",
        "from": "T32n1669_018:0689c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六者有無自在。",
        "from": "T32n1669_018:0689c21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦現亦隱等。",
        "from": "T32n1669_018:0689c21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至無量故。",
        "from": "T32n1669_018:0689c22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七者寂動自在。",
        "from": "T32n1669_018:0689c22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦定亦散等。",
        "from": "T32n1669_018:0689c22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至無量故。",
        "from": "T32n1669_018:0689c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八者甚深自在。",
        "from": "T32n1669_018:0689c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可思議等事。",
        "from": "T32n1669_018:0689c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至無量故。",
        "from": "T32n1669_018:0689c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九者不自在自在。",
        "from": "T32n1669_018:0689c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以逆等事。",
        "from": "T32n1669_018:0689c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至無量故。",
        "from": "T32n1669_018:0689c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十者無礙自在。",
        "from": "T32n1669_018:0689c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以逆順事等。",
        "from": "T32n1669_018:0689c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至無量故。",
        "from": "T32n1669_018:0689c25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至第百自在自在。",
        "from": "T32n1669_018:0689c26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無盡自在等皆悉自在。",
        "from": "T32n1669_018:0689c26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至無量故。",
        "from": "T32n1669_018:0689c27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前所說三十二法。",
        "from": "T32n1669_018:0689c27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是自在具足圓滿無闕失轉。",
        "from": "T32n1669_018:0689c27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此義故立海王名。",
        "from": "T32n1669_018:0689c28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應審思擇。",
        "from": "T32n1669_018:0689c29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈海王道路中具足百自在。",
        "from": "T32n1669_018:0689c29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此因緣故建立海王名故。",
        "from": "T32n1669_018:0690a01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覺花修多羅中。",
        "from": "T32n1669_018:0690a01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說。",
        "from": "T32n1669_018:0690a02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一廣分大海門中。",
        "from": "T32n1669_018:0690a02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若廣說有一十方世界之塵量自在。",
        "from": "T32n1669_018:0690a02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若略說有一百自在。",
        "from": "T32n1669_018:0690a03_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至廣說故。",
        "from": "T32n1669_018:0690a04_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大不可思議重重不可稱量阿說本王大決擇分第三十六如是已說摩訶空塵海藏王道路大決擇分。",
        "from": "T32n1669_018:0690a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說大不可思議重重不可稱量阿說本王大決擇分。",
        "from": "T32n1669_018:0690a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_018:0690a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_018:0690a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　阿說本王中　　有十方塵量",
        "from": "T32n1669_018:0690a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　十方空塵量　　三十三法海",
        "from": "T32n1669_018:0690a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_018:0690a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此偈中為明何義。",
        "from": "T32n1669_018:0690a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲現示阿說海中具足圓滿十方世界之塵數。",
        "from": "T32n1669_018:0690a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方世界之塵數。",
        "from": "T32n1669_018:0690a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十三法海。",
        "from": "T32n1669_018:0690a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方世界之塵數。",
        "from": "T32n1669_018:0690a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方虛空之塵數三十三法海故如偈阿說本王中有十方塵量十方空塵量三十三法海故。",
        "from": "T32n1669_018:0690a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本王修多羅中。",
        "from": "T32n1669_018:0690a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說。",
        "from": "T32n1669_018:0690a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告大眾言。",
        "from": "T32n1669_018:0690a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我以三達智。",
        "from": "T32n1669_018:0690a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通達一切法。",
        "from": "T32n1669_018:0690a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無所障礙無所闕失。",
        "from": "T32n1669_018:0690a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而有一海不可思議。",
        "from": "T32n1669_018:0690a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可思議不能窮了。",
        "from": "T32n1669_018:0690a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂空塵本王性德圓滿自在自在無盡藏海。",
        "from": "T32n1669_018:0690a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至廣說故。",
        "from": "T32n1669_018:0690a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大宗地玄文本論卷第十八",
        "from": "T32n1669_018:0690a22_0",
        "context_with_next_sentence": false
    }
]