[
    {
        "source_sentence": "佛說一切如來安像三昧儀",
        "from": "T21n1418_001:0933b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "軌經",
        "from": "T21n1418_001:0933b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試鴻臚少卿",
        "from": "T21n1418_001:0933b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳法大師臣施護奉　詔譯",
        "from": "T21n1418_001:0933b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊入三摩地。",
        "from": "T21n1418_001:0933b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名一切如來金剛安像三昧。",
        "from": "T21n1418_001:0933b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從三昧起。",
        "from": "T21n1418_001:0933b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說彼塑畫雕造莊嚴一切佛。",
        "from": "T21n1418_001:0933b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸賢聖之眾。",
        "from": "T21n1418_001:0933b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安像慶讚儀軌之法。",
        "from": "T21n1418_001:0933b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲造佛像。",
        "from": "T21n1418_001:0933b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先得了知十種法則阿闍梨。",
        "from": "T21n1418_001:0933b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復須善知佛身一切圓滿之相。",
        "from": "T21n1418_001:0933b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及停分大小之量。",
        "from": "T21n1418_001:0933b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於息災增益等。",
        "from": "T21n1418_001:0933b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言印法儀軌之事皆悉明了。",
        "from": "T21n1418_001:0933b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然得於彼寺舍殿塔之內造諸尊像。",
        "from": "T21n1418_001:0933b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若所造佛像儀相闕少。",
        "from": "T21n1418_001:0933b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得安像慶讚。",
        "from": "T21n1418_001:0933b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若儀相不圓。",
        "from": "T21n1418_001:0933b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼眾生現世未來得大苦怖。",
        "from": "T21n1418_001:0933b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故一心求造圓滿。",
        "from": "T21n1418_001:0933b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若圓滿已。",
        "from": "T21n1418_001:0933b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令前知法阿闍梨。",
        "from": "T21n1418_001:0933b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依真言儀軌。",
        "from": "T21n1418_001:0933b17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請佛安像供養慶讚。",
        "from": "T21n1418_001:0933b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得如來賢聖。",
        "from": "T21n1418_001:0933b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降臨隨喜成就功德。",
        "from": "T21n1418_001:0933b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若造像畢已經久時。",
        "from": "T21n1418_001:0933b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而不行安像慶讚。",
        "from": "T21n1418_001:0933b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其後時反獲不吉。",
        "from": "T21n1418_001:0933b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設復有人供養禮拜。",
        "from": "T21n1418_001:0933b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終無福利。",
        "from": "T21n1418_001:0933b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如無智人人相不具。",
        "from": "T21n1418_001:0933b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼世間而生輕慢。",
        "from": "T21n1418_001:0933b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若阿闍梨請安佛像。",
        "from": "T21n1418_001:0933b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而不知法及闕少儀軌。",
        "from": "T21n1418_001:0933b22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如酥入灰。",
        "from": "T21n1418_001:0933b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲作護摩必無果報。",
        "from": "T21n1418_001:0933b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故造像須具相圓滿。",
        "from": "T21n1418_001:0933b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令安像慶讚儀軌具足。",
        "from": "T21n1418_001:0933b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸有情得大福報。",
        "from": "T21n1418_001:0933b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時有一菩薩而白佛言。",
        "from": "T21n1418_001:0933b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊佛身無相。",
        "from": "T21n1418_001:0933b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶若虛空遍一切處。",
        "from": "T21n1418_001:0933b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何今說令安佛像。",
        "from": "T21n1418_001:0933b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言汝今諦聽。",
        "from": "T21n1418_001:0933b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為久修行者。",
        "from": "T21n1418_001:0933b28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說彼法身無相無為。",
        "from": "T21n1418_001:0933b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍一切處猶在虛空。",
        "from": "T21n1418_001:0933b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本自寂然不生不滅。",
        "from": "T21n1418_001:0933b29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何安像。",
        "from": "T21n1418_001:0933c01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今為初發心眾生。",
        "from": "T21n1418_001:0933c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼得福作如是說安像慶讚結淨之法。",
        "from": "T21n1418_001:0933c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲建曼拏羅。",
        "from": "T21n1418_001:0933c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先揀吉星吉日。",
        "from": "T21n1418_001:0933c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於結界勝地清淨之處。",
        "from": "T21n1418_001:0933c03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陳設傘蓋幢幡香華燈果。",
        "from": "T21n1418_001:0933c03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然令阿闍梨依法儀軌一心召請。",
        "from": "T21n1418_001:0933c04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先於結界地上下。",
        "from": "T21n1418_001:0933c05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五色粉絣線作曼拏羅。",
        "from": "T21n1418_001:0933c05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請五族如來及菩薩眷屬等。",
        "from": "T21n1418_001:0933c06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須依此根本儀軌。",
        "from": "T21n1418_001:0933c06_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得依別儀請召。",
        "from": "T21n1418_001:0933c07_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又阿闍梨入佛堂殿安像之時。",
        "from": "T21n1418_001:0933c07_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須手腕及指。",
        "from": "T21n1418_001:0933c08_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶釧鐶等莊嚴於身。",
        "from": "T21n1418_001:0933c08_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿闍梨并弟子。",
        "from": "T21n1418_001:0933c09_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心觀想如來一切圓滿之相。",
        "from": "T21n1418_001:0933c09_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後合掌作禮瞻仰聖賢。",
        "from": "T21n1418_001:0933c10_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以清淨香花香水燈果依法儀軌獻閼伽香燈。",
        "from": "T21n1418_001:0933c10_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獻已入定心離疑念。",
        "from": "T21n1418_001:0933c11_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言三遍。",
        "from": "T21n1418_001:0933c12_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三摩野薩怛鑁",
        "from": "T21n1418_001:0933c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三摩野薩怛鑁",
        "from": "T21n1418_001:0933c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言已而啟請言。",
        "from": "T21n1418_001:0933c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如一切佛安住覩史陀天。",
        "from": "T21n1418_001:0933c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦如佛在摩邪夫人胎藏。",
        "from": "T21n1418_001:0933c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願佛慈悲住此亦然。",
        "from": "T21n1418_001:0933c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願佛恒住弟子某甲。",
        "from": "T21n1418_001:0933c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為發菩提心受此香花供養。",
        "from": "T21n1418_001:0933c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請佛像已。",
        "from": "T21n1418_001:0933c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復與施主弟子受戒。",
        "from": "T21n1418_001:0933c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有供養儀則等。",
        "from": "T21n1418_001:0933c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切須依阿闍梨言教不得怠慢。",
        "from": "T21n1418_001:0933c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼施主得大果報。",
        "from": "T21n1418_001:0933c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請召如來安住坐位。",
        "from": "T21n1418_001:0933c20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以香華燈菓飲食。",
        "from": "T21n1418_001:0933c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言獻閼伽供養。",
        "from": "T21n1418_001:0933c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有佛像面東安置。",
        "from": "T21n1418_001:0933c22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用黃衣蓋覆。",
        "from": "T21n1418_001:0933c22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿闍梨作觀想。",
        "from": "T21n1418_001:0933c22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想佛如一聚火。",
        "from": "T21n1418_001:0933c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言七遍擲白芥子。",
        "from": "T21n1418_001:0933c23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩哩嚩(二合)播(引)波娑普(二合)吒那賀曩嚩日囉(二合引)野薩嚩(二合)賀誦此真言已。",
        "from": "T21n1418_001:0933c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復想如來如真實身諸相圓滿。",
        "from": "T21n1418_001:0933c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然以唵阿吽三字安在像身三處用唵字安頂上。",
        "from": "T21n1418_001:0933c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用阿字安口上。",
        "from": "T21n1418_001:0933c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用吽字安心上。",
        "from": "T21n1418_001:0933c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦得本尊根本真言但安心上。",
        "from": "T21n1418_001:0933c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若請召印或金剛印或鉤印依法而作。",
        "from": "T21n1418_001:0934a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人無力隨力令作。",
        "from": "T21n1418_001:0934a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所用器物或金銀寶玉銅石瓦器木器螺盃及樹葉。",
        "from": "T21n1418_001:0934a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆得為閼伽供養。",
        "from": "T21n1418_001:0934a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若法中使銅器作閼伽。",
        "from": "T21n1418_001:0934a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最上如金石等。",
        "from": "T21n1418_001:0934a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛像故暗。",
        "from": "T21n1418_001:0934a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗香油在上。",
        "from": "T21n1418_001:0934a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用草刷子洗刷令淨。",
        "from": "T21n1418_001:0934a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然用歌讚妓樂令童女歌舞等。",
        "from": "T21n1418_001:0934a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復誦香油真言。",
        "from": "T21n1418_001:0934a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩哩嚩(二合)怛他誐多迦野尾戍馱儞(引)娑嚩(二合)賀(引)若賢瓶用五或八。",
        "from": "T21n1418_001:0934a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以白檀香塗之。",
        "from": "T21n1418_001:0934a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種香水滿彼瓶中而即加持。",
        "from": "T21n1418_001:0934a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復令童女合線。",
        "from": "T21n1418_001:0934a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "染成五色上好新鮮纏瓶周迴。",
        "from": "T21n1418_001:0934a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復用五寶五藥花菓等入於賢瓶。",
        "from": "T21n1418_001:0934a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦最勝佛頂真言或白傘蓋真言。",
        "from": "T21n1418_001:0934a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摋白芥子以為加持。",
        "from": "T21n1418_001:0934a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或念息災真言亦得。",
        "from": "T21n1418_001:0934a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若洗浴佛像。",
        "from": "T21n1418_001:0934a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先塗香油復用尼俱陀樹葉優曇樹葉為粖。",
        "from": "T21n1418_001:0934a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五穀五種淨水以銅器盛之。",
        "from": "T21n1418_001:0934a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洗浴佛像。",
        "from": "T21n1418_001:0934a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然令弟子一切眾人。",
        "from": "T21n1418_001:0934a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦於偈讚及諸歌樂。",
        "from": "T21n1418_001:0934a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為吉祥之音。",
        "from": "T21n1418_001:0934a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "適悅尊像如灌頂儀。",
        "from": "T21n1418_001:0934a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則乃誦佛眼菩薩真言能除一切垢。",
        "from": "T21n1418_001:0934a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩哩嚩(二合)囉祖(引)波賀囉拏娑嚩(二合引)賀(引)誦此真言已。",
        "from": "T21n1418_001:0934a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿闍梨言。",
        "from": "T21n1418_001:0934a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如佛降生之時沐浴佛身。",
        "from": "T21n1418_001:0934a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來亦復如是。",
        "from": "T21n1418_001:0934a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今以清淨最上之水洗浴佛像。",
        "from": "T21n1418_001:0934a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復誦灌頂真言。",
        "from": "T21n1418_001:0934a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩哩嚩(二合)怛他(引)誐哆(引)鼻詵(引)迦三摩野室哩(二合)曳(引)吽若是畫像功德。",
        "from": "T21n1418_001:0934a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以鏡照彼所畫功德。",
        "from": "T21n1418_001:0934a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用前五種淨水寫於鏡上。",
        "from": "T21n1418_001:0934a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沐浴鏡中影像。",
        "from": "T21n1418_001:0934a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若前金銀等像及所畫幀像。",
        "from": "T21n1418_001:0934a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沐浴畢已即誦著衣真言。",
        "from": "T21n1418_001:0934a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉(二合)嚩(引)娑細(引)娑嚩(二合引)賀次誦安耳真言。",
        "from": "T21n1418_001:0934b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵迦囉拏(二合)摩路(引)馱囉拏吽次誦安髮髻真言。",
        "from": "T21n1418_001:0934b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩哩嚩(二合)枳(引)娑阿嚩哆(引)囉拏吽娑嚩(二合)賀(引)次誦安指甲真言。",
        "from": "T21n1418_001:0934b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩哩嚩(二合)輸曩佉砌那曩囉惹娑嚩(二合)賀次誦安髭鬚真言。",
        "from": "T21n1418_001:0934b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩哩嚩(二合)娑摩(二合)酥嚕(二合)馱囉拏吽吽呬凌(二合)次誦獻塗香真言。",
        "from": "T21n1418_001:0934b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉(二合)巘弟(引)娑嚩(二合引)賀(引)次誦安莊嚴真言。",
        "from": "T21n1418_001:0934b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉(二合)婆囉拏尾部瑟尼娑嚩(二合)賀誦此真言已。",
        "from": "T21n1418_001:0934b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依灌頂儀入曼拏羅。",
        "from": "T21n1418_001:0934b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與佛像誦本尊三昧真言。",
        "from": "T21n1418_001:0934b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵吽怛落仡哩惡",
        "from": "T21n1418_001:0934b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言已。",
        "from": "T21n1418_001:0934b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是戴冠面東安置佛像。",
        "from": "T21n1418_001:0934b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼施主得大福利。",
        "from": "T21n1418_001:0934b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次用香花等供養。",
        "from": "T21n1418_001:0934b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先誦獻花真言。",
        "from": "T21n1418_001:0934b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉(二合)補瑟閉(二合引)吽誦此真言已白言。",
        "from": "T21n1418_001:0934b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此花最上或水中生或陸地生。",
        "from": "T21n1418_001:0934b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今志心供養願佛納受。",
        "from": "T21n1418_001:0934b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次誦獻香真言。",
        "from": "T21n1418_001:0934b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉(二合)度閉",
        "from": "T21n1418_001:0934b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言已白言。",
        "from": "T21n1418_001:0934b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此香最上或自然香或和合香。",
        "from": "T21n1418_001:0934b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今志心供養願佛納受。",
        "from": "T21n1418_001:0934b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次誦塗香真言。",
        "from": "T21n1418_001:0934b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉(二合)巘弟(引)娑嚩(二合)賀誦此真言已白言。",
        "from": "T21n1418_001:0934c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是塗香最上殊勝願佛納受。",
        "from": "T21n1418_001:0934c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次誦燈真言。",
        "from": "T21n1418_001:0934c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉(二合)禰閉(引)娑嚩(二合)賀誦此真言已白言。",
        "from": "T21n1418_001:0934c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是燈供養或酥燈或油燈光明破暗。",
        "from": "T21n1418_001:0934c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今志心供養願佛納受。",
        "from": "T21n1418_001:0934c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "消除一切愚暗。",
        "from": "T21n1418_001:0934c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次誦食真言。",
        "from": "T21n1418_001:0934c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵賓努波(引)旦鉢囉(二合)底仡哩(二合)恨曩娑嚩(二合)賀誦此真言已白言。",
        "from": "T21n1418_001:0934c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是食供養最上飲食色香美味一切具足。",
        "from": "T21n1418_001:0934c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今志心供養願佛納受。",
        "from": "T21n1418_001:0934c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是供養儀則既已周備。",
        "from": "T21n1418_001:0934c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復為佛像開眼之光明。",
        "from": "T21n1418_001:0934c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如點眼相似。",
        "from": "T21n1418_001:0934c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦開眼光真言二道。",
        "from": "T21n1418_001:0934c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵作芻作芻三滿哆　作芻尾戍馱儞(引)娑嚩賀唵儞(引)怛囉(二合)波吒路(引)波賀(引)囉拏呬凌(二合)次誦佛入定真言。",
        "from": "T21n1418_001:0934c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵禰[尾*(烰子+日)](切身)儞(引)曩三摩(引)地[尾*也](切身)曩必哩(二合)拏儞娑嚩(二合)賀誦此真言已。",
        "from": "T21n1418_001:0934c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復令施主弟子入曼拏羅。",
        "from": "T21n1418_001:0934c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿闍梨白言。",
        "from": "T21n1418_001:0934c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今請我安慶佛像。",
        "from": "T21n1418_001:0934c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今為汝所作儀軌悉皆周畢。",
        "from": "T21n1418_001:0934c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宜應灌頂誦此真言。",
        "from": "T21n1418_001:0934c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三摩野薩怛鑁",
        "from": "T21n1418_001:0934c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次作護摩。",
        "from": "T21n1418_001:0934c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或一千或五百或二百五十遍。",
        "from": "T21n1418_001:0934c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若護摩爐宜四方作。",
        "from": "T21n1418_001:0934c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用有白乳汁者。",
        "from": "T21n1418_001:0934c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "濕柴入於爐內。",
        "from": "T21n1418_001:0934c27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復用酥酪蜜及五穀合和用作護摩。",
        "from": "T21n1418_001:0934c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能與自身及一切弟子息災增益。",
        "from": "T21n1418_001:0934c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦作護摩真言。",
        "from": "T21n1418_001:0934c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉(二合)喻世(引)娑嚩(二合)賀此是作護摩真言　次誦增壽真言。",
        "from": "T21n1418_001:0935a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿誐蹉賀哩賓誐羅禰鉢哆(二合)[口*爾]賀嚩(二合)路(引)呬哆(引)乞叉(二合)禰(引)呬那那(引)波野伊難左彌(引)迦哩焰(二合)俱嚕娑嚩(二合)賀此是增壽真言。",
        "from": "T21n1418_001:0935a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次誦息災增益真言。",
        "from": "T21n1418_001:0935a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉(二合)布瑟吒(二合)曳娑嚩(二合)賀此是息災增益真言。",
        "from": "T21n1418_001:0935a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次為火天於護摩爐。",
        "from": "T21n1418_001:0935a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安置座位作其觀想。",
        "from": "T21n1418_001:0935a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀護摩爐三分之中。",
        "from": "T21n1418_001:0935a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空其一分為火天座位。",
        "from": "T21n1418_001:0935a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵酥鉢囉(二合)底瑟姹(二合)哆嚩日囉(二合)娑嚩(二合)賀此是安置火天真言。",
        "from": "T21n1418_001:0935a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以香花等獻護世及伊舍那天。",
        "from": "T21n1418_001:0935a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦護世真言。",
        "from": "T21n1418_001:0935a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印捺囉(二合)野娑嚩(二合)賀",
        "from": "T21n1418_001:0935a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言已。",
        "from": "T21n1418_001:0935a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作其妓樂歌舞等發遣白言。",
        "from": "T21n1418_001:0935a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上來廣作佛事供養多不如法。",
        "from": "T21n1418_001:0935a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所來梵天天子并諸天眾等。",
        "from": "T21n1418_001:0935a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為其施主依此儀軌。",
        "from": "T21n1418_001:0935a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切處作大息災增益。",
        "from": "T21n1418_001:0935a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往彼佛剎復有請召願賜降臨。",
        "from": "T21n1418_001:0935a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地水火風神等往本界中。",
        "from": "T21n1418_001:0935a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發善心愛敬。",
        "from": "T21n1418_001:0935a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所安佛像經百千劫。",
        "from": "T21n1418_001:0935a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護持施主及子孫眷屬。",
        "from": "T21n1418_001:0935a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並阿闍梨一切弟子普獲吉祥。",
        "from": "T21n1418_001:0935a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作壇法吉時者。",
        "from": "T21n1418_001:0935a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正月二月三月四月五月十一月十二月。",
        "from": "T21n1418_001:0935a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若所用日於白月。",
        "from": "T21n1418_001:0935a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二日三日七日十日十三日十五日。",
        "from": "T21n1418_001:0935a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黑月亦得。",
        "from": "T21n1418_001:0935a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復須就吉星宿直日。",
        "from": "T21n1418_001:0935a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即可作法。",
        "from": "T21n1418_001:0935a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若翼星畢星斗星嘴星尾星心星鬼星箕星室星。",
        "from": "T21n1418_001:0935a29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是最上吉星。",
        "from": "T21n1418_001:0935b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求一切事皆得成就。",
        "from": "T21n1418_001:0935b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若井星得作法成就所求必遂。",
        "from": "T21n1418_001:0935b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼得外來眷屬。",
        "from": "T21n1418_001:0935b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若尾星斗星嘴星。",
        "from": "T21n1418_001:0935b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得人敬愛及得財物。",
        "from": "T21n1418_001:0935b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若參星得息災。",
        "from": "T21n1418_001:0935b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若箕星得子孫長壽。",
        "from": "T21n1418_001:0935b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若室星恒得美事成就。",
        "from": "T21n1418_001:0935b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若鬼星得一切最勝。",
        "from": "T21n1418_001:0935b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若心星房星得快樂稱意。",
        "from": "T21n1418_001:0935b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若木星水星金星吉善第一。",
        "from": "T21n1418_001:0935b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若此三星和合直日。",
        "from": "T21n1418_001:0935b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作安像慶讚儀則得最上福。",
        "from": "T21n1418_001:0935b07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若前第三第七第六第十及第十一星。",
        "from": "T21n1418_001:0935b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此五星若與月合。",
        "from": "T21n1418_001:0935b09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同用大吉。",
        "from": "T21n1418_001:0935b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若第二第六第十及第十一星與水星合。",
        "from": "T21n1418_001:0935b10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同用大吉。",
        "from": "T21n1418_001:0935b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若第二第七第九及第十一星與木星合。",
        "from": "T21n1418_001:0935b11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同用作召請結淨最善大吉。",
        "from": "T21n1418_001:0935b12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若第六第七第十星與金星合。",
        "from": "T21n1418_001:0935b12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同用大吉。",
        "from": "T21n1418_001:0935b13_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若第一第三第五星能破財物。",
        "from": "T21n1418_001:0935b13_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿闍梨應如是善知星辰之力。",
        "from": "T21n1418_001:0935b14_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如作法日若得星曜和順。",
        "from": "T21n1418_001:0935b15_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所作之事於剎那之頃必得成就。",
        "from": "T21n1418_001:0935b15_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若安像慶讚獲福最上。",
        "from": "T21n1418_001:0935b16_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當得轉輪王位大國王位無復疑惑。",
        "from": "T21n1418_001:0935b17_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又若星曜相順無一切障難。",
        "from": "T21n1418_001:0935b17_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所安佛像塔廟。",
        "from": "T21n1418_001:0935b18_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世世生生俱得成就人獲福利。",
        "from": "T21n1418_001:0935b18_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若造像工人及諸雜使之者。",
        "from": "T21n1418_001:0935b19_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱與財帛善言慰安勿令失所。",
        "from": "T21n1418_001:0935b20_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方合儀法。",
        "from": "T21n1418_001:0935b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有供養如來餘剩之食。",
        "from": "T21n1418_001:0935b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人食者皆得滅罪阿闍梨即教化施主。",
        "from": "T21n1418_001:0935b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結緣增福施利所有金銀財帛象馬車乘。",
        "from": "T21n1418_001:0935b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨力布施志心奉上。",
        "from": "T21n1418_001:0935b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令阿闍梨歡喜。",
        "from": "T21n1418_001:0935b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢聖亦歡喜。",
        "from": "T21n1418_001:0935b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復於比丘比丘尼優婆塞優婆夷。",
        "from": "T21n1418_001:0935b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以其財食隨力供養令福圓滿。",
        "from": "T21n1418_001:0935b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於初夜分作大妓樂歌舞旛華螺鈸。",
        "from": "T21n1418_001:0935b26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及施主眷屬圍繞恭敬。",
        "from": "T21n1418_001:0935b27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曼拏羅內所有供養物色。",
        "from": "T21n1418_001:0935b27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並須捨與阿闍梨。",
        "from": "T21n1418_001:0935b28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得別處破用。",
        "from": "T21n1418_001:0935b28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不得別比丘處施與。",
        "from": "T21n1418_001:0935b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不合儀法。",
        "from": "T21n1418_001:0935b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壇法既畢。",
        "from": "T21n1418_001:0935b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即收壇物。",
        "from": "T21n1418_001:0935c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若五色粉送入河中。",
        "from": "T21n1418_001:0935c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用衢摩邪淨水洗拭壇地。",
        "from": "T21n1418_001:0935c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如法結淨功德成就。",
        "from": "T21n1418_001:0935c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來安像三昧儀軌經",
        "from": "T21n1418_001:0935c03_0",
        "context_with_next_sentence": false
    }
]