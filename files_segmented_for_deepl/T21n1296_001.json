[
    {
        "source_sentence": "十天儀軌",
        "from": "T21n1296_001:0382c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東北方大自在天印",
        "from": "T21n1296_001:0382c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手作拳安腰右。",
        "from": "T21n1296_001:0382c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手五指直竪相著。",
        "from": "T21n1296_001:0382c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地水二指屈中節。",
        "from": "T21n1296_001:0382c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火風空三指各少相去即成。",
        "from": "T21n1296_001:0382c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1296_001:0382c27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩麼三曼跢勃馱南唵伊(去)舍曩(去)曳娑嚩訶唵嚕捺囉耶娑婆訶東方帝釋天印右手同前。",
        "from": "T21n1296_001:0382c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手五指直竪相著。",
        "from": "T21n1296_001:0383a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地水二指屈中節。",
        "from": "T21n1296_001:0383a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以風著火背。",
        "from": "T21n1296_001:0383a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空屈中節。",
        "from": "T21n1296_001:0383a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1296_001:0383a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵因捺囉耶娑嚩訶",
        "from": "T21n1296_001:0383a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又唵斫羯也娑嚩呵",
        "from": "T21n1296_001:0383a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東南方火天印",
        "from": "T21n1296_001:0383a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手作拳安腰左。",
        "from": "T21n1296_001:0383a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手五指直竪相著。",
        "from": "T21n1296_001:0383a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈空納著掌中。",
        "from": "T21n1296_001:0383a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風屈中節。",
        "from": "T21n1296_001:0383a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與娑婆訶相招。",
        "from": "T21n1296_001:0383a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1296_001:0383a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿哦那曳娑婆訶",
        "from": "T21n1296_001:0383a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南方焰摩天印",
        "from": "T21n1296_001:0383a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先合掌二地屈中節背相著。",
        "from": "T21n1296_001:0383a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二風亦屈背著。",
        "from": "T21n1296_001:0383a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大指押各風中節即成。",
        "from": "T21n1296_001:0383a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1296_001:0383a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵焰摩耶娑嚩訶",
        "from": "T21n1296_001:0383a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西南方羅剎天印",
        "from": "T21n1296_001:0383a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手作拳安腰。",
        "from": "T21n1296_001:0383a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手五指直竪相著。",
        "from": "T21n1296_001:0383a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈地水中節。",
        "from": "T21n1296_001:0383a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以空端押地水甲即成。",
        "from": "T21n1296_001:0383a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1296_001:0383a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵地哩底曳娑嚩訶",
        "from": "T21n1296_001:0383a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方水天印",
        "from": "T21n1296_001:0383a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手如前。",
        "from": "T21n1296_001:0383a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手掘拳。",
        "from": "T21n1296_001:0383a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空勿入掌中。",
        "from": "T21n1296_001:0383a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風即直竪中節小屈即成。",
        "from": "T21n1296_001:0383a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1296_001:0383a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩嚕拏耶娑嚩訶",
        "from": "T21n1296_001:0383a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西北方風天印",
        "from": "T21n1296_001:0383a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手如前。",
        "from": "T21n1296_001:0383a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手五指直竪相著。",
        "from": "T21n1296_001:0383a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地水屈中節即成。",
        "from": "T21n1296_001:0383a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1296_001:0383a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩耶吠(微曳反)娑嚩訶",
        "from": "T21n1296_001:0383a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方毘沙門天印",
        "from": "T21n1296_001:0383a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先合掌十指內相叉。",
        "from": "T21n1296_001:0383a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二水竪頭相柱。",
        "from": "T21n1296_001:0383a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二風竪微屈。",
        "from": "T21n1296_001:0383a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左空入右掌中。",
        "from": "T21n1296_001:0383a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "押左火甲。",
        "from": "T21n1296_001:0383a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右空越左空背。",
        "from": "T21n1296_001:0383a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入左掌中押右火甲。",
        "from": "T21n1296_001:0383b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二風與訶相招。",
        "from": "T21n1296_001:0383b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1296_001:0383b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵吠室羅嚩拏耶娑嚩訶",
        "from": "T21n1296_001:0383b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上方梵天印",
        "from": "T21n1296_001:0383b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手作拳安腰右。",
        "from": "T21n1296_001:0383b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手五指相著竪之小屈。",
        "from": "T21n1296_001:0383b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其高少許過肩。",
        "from": "T21n1296_001:0383b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成蓮花相真言曰。",
        "from": "T21n1296_001:0383b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵沒羅(二合)含摩寧娑嚩訶",
        "from": "T21n1296_001:0383b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下方地天印",
        "from": "T21n1296_001:0383b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先合掌風以下四指頭相柱。",
        "from": "T21n1296_001:0383b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前方大開。",
        "from": "T21n1296_001:0383b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二空各著風側。",
        "from": "T21n1296_001:0383b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1296_001:0383b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵畢哩體(地以反)微曳娑嚩訶",
        "from": "T21n1296_001:0383b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日天印",
        "from": "T21n1296_001:0383b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不異前地印。",
        "from": "T21n1296_001:0383b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但以二空頭各跓水下節。",
        "from": "T21n1296_001:0383b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1296_001:0383b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿(去)儞底也娑嚩訶",
        "from": "T21n1296_001:0383b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月天印",
        "from": "T21n1296_001:0383b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前梵天印。",
        "from": "T21n1296_001:0383b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但作掌中有月潔白相。",
        "from": "T21n1296_001:0383b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1296_001:0383b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵戰捺羅(去)也娑嚩訶",
        "from": "T21n1296_001:0383b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七曜印",
        "from": "T21n1296_001:0383b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先合掌十指相著。",
        "from": "T21n1296_001:0383b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二風與二空極相開。",
        "from": "T21n1296_001:0383b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1296_001:0383b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孽羅(二合)醯濕嚩(二合)哩耶鉢囉(二合)跛多而偷(二合)底羅摩耶娑嚩訶二十八宿印先虛心合掌。",
        "from": "T21n1296_001:0383b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二火外相叉。",
        "from": "T21n1296_001:0383b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二空亦相叉。",
        "from": "T21n1296_001:0383b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1296_001:0383b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諾乞灑(二合)怛羅(二合)涅(寧去反)蘇那儞曳娑嚩訶十天供儀軌",
        "from": "T21n1296_001:0383b27_0",
        "context_with_next_sentence": false
    }
]