    {
        "source_sentence": "聞此真言頭破作七分如阿梨樹枝。",
        "from": "T21n1245_001:0219a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1245_001:0219a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也(二合)他(引)呬隷彌隷呬彌隷吉里帝(二合引)母隷呬隷吉里帝(二合)母隷唵凍彌(引)凍彌(引)烏凍麼烏凍彌(引)烏企彌企虞(引)努(引)呬(引)部凌(二合)誐里乙里(二合)史散多儞娑嚩(二合引)賀(引)世尊若有聲聞苾芻苾芻尼優婆塞優婆夷。",
        "from": "T21n1245_001:0219a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此經中受持讀誦。",
        "from": "T21n1245_001:0219a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮敬供養廣為人說。",
        "from": "T21n1245_001:0219a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼諸藥叉及彼父母兄弟男女眷屬等。",
        "from": "T21n1245_001:0219a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆不能為害。",
        "from": "T21n1245_001:0219a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有惱亂者即失威力。",
        "from": "T21n1245_001:0219a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能得藥叉三昧。",
        "from": "T21n1245_001:0219a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭破作七分如阿梨樹枝。",
        "from": "T21n1245_001:0219a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不能往阿拏迦嚩帝大城中住。",
        "from": "T21n1245_001:0219a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊若有聲聞苾芻苾芻尼優婆塞優婆夷。",
        "from": "T21n1245_001:0219a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此經中受持讀誦。",
        "from": "T21n1245_001:0219a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮敬供養廣為人說。",
        "from": "T21n1245_001:0219a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼諸羅剎及彼父母兄弟男女眷屬等。",
        "from": "T21n1245_001:0219a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆不能為害。",
        "from": "T21n1245_001:0219a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常來親近侍奉供養。",
        "from": "T21n1245_001:0219a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有惱亂者即失威力。",
        "from": "T21n1245_001:0219a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能得羅剎三昧。",
        "from": "T21n1245_001:0219a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭破作七分如阿梨樹枝。",
        "from": "T21n1245_001:0219a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不能往阿拏迦嚩帝大城中住。",
        "from": "T21n1245_001:0219a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1245_001:0219a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也(二合)他呬隷呬隷護嚕護嚕喻彌喻彌羅嚩帝曳(引)娑嚩(二合引)賀(引)毘沙門天王說此阿吒曩胝經已。",
        "from": "T21n1245_001:0219a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮世尊足却住一面。",
        "from": "T21n1245_001:0219a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時釋迦牟尼佛。",
        "from": "T21n1245_001:0219a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於夜分中告苾芻眾言。",
        "from": "T21n1245_001:0219a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是此經有大威力能為明護。",
        "from": "T21n1245_001:0219a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼毘沙門天王與無數百千藥叉眷屬。",
        "from": "T21n1245_001:0219a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放大光明照祇陀園一切境界。",
        "from": "T21n1245_001:0219a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說伽他曰。",
        "from": "T21n1245_001:0219a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　歸命大無畏　　正覺二足尊",
        "from": "T21n1245_001:0219a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　諸天以天眼　　觀我無所見",
        "from": "T21n1245_001:0219b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　過去未來佛　　三世慈悲主",
        "from": "T21n1245_001:0219b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一一正遍知　　我今歸命禮",
        "from": "T21n1245_001:0219b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此偈已而白佛言。",
        "from": "T21n1245_001:0219b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有諸聲聞苾芻苾芻尼優婆塞優婆夷。",
        "from": "T21n1245_001:0219b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於曠野林間樹下經行坐臥。",
        "from": "T21n1245_001:0219b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有諸藥叉非人之類。",
        "from": "T21n1245_001:0219b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有信佛言者有少信者。",
        "from": "T21n1245_001:0219b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有極惡不信者。",
        "from": "T21n1245_001:0219b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惱亂有情令不安隱。",
        "from": "T21n1245_001:0219b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此經典利樂眾生。",
        "from": "T21n1245_001:0219b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝諸苾芻諦聽諦聽。",
        "from": "T21n1245_001:0219b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經真實有大威力能為救護。",
        "from": "T21n1245_001:0219b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至天人常得密護。",
        "from": "T21n1245_001:0219b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當受持廣宣流布。",
        "from": "T21n1245_001:0219b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是語已天龍藥叉人非人等皆大歡喜信受奉行。",
        "from": "T21n1245_001:0219b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說毘沙門天王經",
        "from": "T21n1245_001:0219b12_0",
        "context_with_next_sentence": false
    }
]