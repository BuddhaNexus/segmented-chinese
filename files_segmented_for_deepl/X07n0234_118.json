[
    {
        "source_sentence": "大方廣佛華嚴經疏卷第一百一十八(入第七十八經下半)清涼山沙門　澄觀　述晉水沙門　(淨源)　錄疏注經△五有四喻。",
        "from": "X07n0234_118:0936a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝五地德。",
        "from": "X07n0234_118:0936a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子如海島中(至)非業惑等能變其心。",
        "from": "X07n0234_118:0936a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入塵利人。",
        "from": "X07n0234_118:0936a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不染俗故。",
        "from": "X07n0234_118:0936a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子譬如小火(至)而其心燈無減無盡。",
        "from": "X07n0234_118:0936a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△六有六喻。",
        "from": "X07n0234_118:0936a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝六地德。",
        "from": "X07n0234_118:0936a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子譬如一燈(至)種種暗障悉能除盡。",
        "from": "X07n0234_118:0936a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "般若現前。",
        "from": "X07n0234_118:0936a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頓被闇故。",
        "from": "X07n0234_118:0936a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子譬如燈炷(至)終不住於二乘解脫。",
        "from": "X07n0234_118:0936a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△七有五句。",
        "from": "X07n0234_118:0936a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝七地德。",
        "from": "X07n0234_118:0936a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子譬如迦陵頻伽鳥(至)凡夫二乘悉不能及。",
        "from": "X07n0234_118:0936a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善入方便。",
        "from": "X07n0234_118:0936a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得自在故。",
        "from": "X07n0234_118:0936a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△八有十四喻。",
        "from": "X07n0234_118:0936a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝八地德。",
        "from": "X07n0234_118:0936a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子如人學射(至)一切見欲無明鉤鎖。",
        "from": "X07n0234_118:0936a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無功發心。",
        "from": "X07n0234_118:0936a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能滅相惑等故。",
        "from": "X07n0234_118:0936a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子如有磁石(至)摧伏一切諸魔外道。",
        "from": "X07n0234_118:0936a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△九有十喻。",
        "from": "X07n0234_118:0936a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝九地德。",
        "from": "X07n0234_118:0936a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子譬如有人(至)心無疲厭亦無染著。",
        "from": "X07n0234_118:0936a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "延壽益生等故。",
        "from": "X07n0234_118:0936a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子譬如有人(至)無智不信謂為不淨。",
        "from": "X07n0234_118:0936a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△十有六喻。",
        "from": "X07n0234_118:0936a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝十地德。",
        "from": "X07n0234_118:0936a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子譬如有藥(至)諸煩惱病悉得除滅。",
        "from": "X07n0234_118:0936a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂除惑習。",
        "from": "X07n0234_118:0936a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成一切佛法故。",
        "from": "X07n0234_118:0936a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子譬如有人(至)扶踈蔭映普覆世間。",
        "from": "X07n0234_118:0936b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△五有十七喻。",
        "from": "X07n0234_118:0936b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝等覺位功德。",
        "from": "X07n0234_118:0936b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子譬如金剛(至)一切餘心所不能持。",
        "from": "X07n0234_118:0936b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△四結釋所屬。",
        "from": "X07n0234_118:0936b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子菩提心者(至)已得如是大功德故。",
        "from": "X07n0234_118:0936b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂上來多德。",
        "from": "X07n0234_118:0936b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋獲善利之言故。",
        "from": "X07n0234_118:0936b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二正授法界四。",
        "from": "X07n0234_118:0936b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初授法體四。",
        "from": "X07n0234_118:0936b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初攝入方便二。",
        "from": "X07n0234_118:0936b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初牒問勸證。",
        "from": "X07n0234_118:0936b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子如汝所問(至)學已成就無量功德。",
        "from": "X07n0234_118:0936b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此下入第七十九經。",
        "from": "X07n0234_118:0936b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二求證方便。",
        "from": "X07n0234_118:0936b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時善財童子恭敬右繞(至)開樓閣門令我得入。",
        "from": "X07n0234_118:0936b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二加令證入。",
        "from": "X07n0234_118:0936b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時彌勒菩薩前詣樓閣(至)其門即開命善財入。",
        "from": "X07n0234_118:0936b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先約緣加。",
        "from": "X07n0234_118:0936b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令其就法亡言會旨。",
        "from": "X07n0234_118:0936b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則佛法門開。",
        "from": "X07n0234_118:0936b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故云前詣等。",
        "from": "X07n0234_118:0936b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即開理智之門。",
        "from": "X07n0234_118:0936b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "示令悟入也。",
        "from": "X07n0234_118:0936b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善財心喜入已還閉。",
        "from": "X07n0234_118:0936b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後約因。",
        "from": "X07n0234_118:0936b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自證悟佛知見。",
        "from": "X07n0234_118:0936b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則入法界。",
        "from": "X07n0234_118:0936b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從迷之悟。",
        "from": "X07n0234_118:0936b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加行趣入。",
        "from": "X07n0234_118:0936b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有門理之殊。",
        "from": "X07n0234_118:0936b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證已契合。",
        "from": "X07n0234_118:0936b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能所兩亡。",
        "from": "X07n0234_118:0936b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即妄而真。",
        "from": "X07n0234_118:0936b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更無入處。",
        "from": "X07n0234_118:0936b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故云還閉。",
        "from": "X07n0234_118:0936b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△三見所證境二。",
        "from": "X07n0234_118:0936b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初別明所見。",
        "from": "X07n0234_118:0936b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二總顯見相。",
        "from": "X07n0234_118:0936b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前中有六。",
        "from": "X07n0234_118:0936b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一見依報。",
        "from": "X07n0234_118:0936b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二見正報。",
        "from": "X07n0234_118:0936b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三見伴菩薩。",
        "from": "X07n0234_118:0936b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四見諸佛。",
        "from": "X07n0234_118:0936b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五見閣中主閣。",
        "from": "X07n0234_118:0936b21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六總見嚴具作用。",
        "from": "X07n0234_118:0936b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此之六段。",
        "from": "X07n0234_118:0936b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆悲智之中所有。",
        "from": "X07n0234_118:0936b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前中分二。",
        "from": "X07n0234_118:0936b22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初見。",
        "from": "X07n0234_118:0936b22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後益。",
        "from": "X07n0234_118:0936b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前中有三。",
        "from": "X07n0234_118:0936b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初直見一重莊嚴。",
        "from": "X07n0234_118:0936b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見其樓閣廣博無量同於虗空。",
        "from": "X07n0234_118:0936b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同虗空者。",
        "from": "X07n0234_118:0936c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱法性故。",
        "from": "X07n0234_118:0936c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上標。",
        "from": "X07n0234_118:0936c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下列。",
        "from": "X07n0234_118:0936c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿僧祇寶以為其地(至)阿僧祇摩尼寶放大光明。",
        "from": "X07n0234_118:0936c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後結。",
        "from": "X07n0234_118:0936c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等無量阿僧祇諸莊嚴具以為莊嚴。",
        "from": "X07n0234_118:0936c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二依中有依。",
        "from": "X07n0234_118:0936c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又見其中有無量百千(至)不相障礙亦無雜亂。",
        "from": "X07n0234_118:0936c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一中見多。",
        "from": "X07n0234_118:0936c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即微細門。",
        "from": "X07n0234_118:0936c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦主伴門。",
        "from": "X07n0234_118:0936c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△三一多自在。",
        "from": "X07n0234_118:0936c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善財童子於一處中見一切處一切諸處悉如是見。",
        "from": "X07n0234_118:0936c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一處見多。",
        "from": "X07n0234_118:0936c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即自在門。",
        "from": "X07n0234_118:0936c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二明得益。",
        "from": "X07n0234_118:0936c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時善財童子見毗盧遮那(至)普見一切普申敬禮。",
        "from": "X07n0234_118:0936c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二見正報二。",
        "from": "X07n0234_118:0936c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初總標。",
        "from": "X07n0234_118:0936c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纔始稽首以彌勒菩薩(至)不可思議自在境界。",
        "from": "X07n0234_118:0936c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二別顯五。",
        "from": "X07n0234_118:0936c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初見發心時。",
        "from": "X07n0234_118:0936c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂或見彌勒菩薩(至)親近供養悉皆明見。",
        "from": "X07n0234_118:0936c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二見其修行得法時。",
        "from": "X07n0234_118:0936c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或見彌勒最初證得(至)受於無上菩提之記。",
        "from": "X07n0234_118:0936c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△三隨類攝生時。",
        "from": "X07n0234_118:0936c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或見彌勒為轉輪王(至)種種方便調伏眾生。",
        "from": "X07n0234_118:0936c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△四見處會說法二。",
        "from": "X07n0234_118:0936c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初所處會殊。",
        "from": "X07n0234_118:0936c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或復見為護世天王(至)諸菩薩眾而演說法。",
        "from": "X07n0234_118:0936c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二顯所說之法。",
        "from": "X07n0234_118:0936c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或見讚說初地乃至(至)讚說一切佛灌頂門。",
        "from": "X07n0234_118:0936c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△五總見行用。",
        "from": "X07n0234_118:0936c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或見彌勒於百千年(至)以方便力現諸神變。",
        "from": "X07n0234_118:0936c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△三見伴菩薩身雲演說。",
        "from": "X07n0234_118:0937a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或見諸菩薩入變化三昧(至)如是等一切諸功德門。",
        "from": "X07n0234_118:0937a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前與無量眷屬。",
        "from": "X07n0234_118:0937a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△四見諸佛攝化之德。",
        "from": "X07n0234_118:0937a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或復於中見諸如來(至)種種不同悉皆明見。",
        "from": "X07n0234_118:0937a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△五見閣中主閣。",
        "from": "X07n0234_118:0937a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別明慈氏一生當作。",
        "from": "X07n0234_118:0937a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中有二。",
        "from": "X07n0234_118:0937a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初見。",
        "from": "X07n0234_118:0937a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復於彼莊嚴藏內(至)住持教法皆悉不同。",
        "from": "X07n0234_118:0937a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二得益。",
        "from": "X07n0234_118:0937a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時善財自見其身(至)憶持不忘通達無礙。",
        "from": "X07n0234_118:0937a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△六見聞嚴具作用神變。",
        "from": "X07n0234_118:0937a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中十段。",
        "from": "X07n0234_118:0937a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初聞網等演法三。",
        "from": "X07n0234_118:0937a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初近聞。",
        "from": "X07n0234_118:0937a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復聞一切諸樓閣內(至)悉聞其音敷暢辨了。",
        "from": "X07n0234_118:0937a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二遠用。",
        "from": "X07n0234_118:0937a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又聞某處有某菩薩(至)教化如是無量眾生。",
        "from": "X07n0234_118:0937a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△三得益。",
        "from": "X07n0234_118:0937a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善財童子聞如是等(至)及諸解脫諸三昧門。",
        "from": "X07n0234_118:0937a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二見寶鏡作用。",
        "from": "X07n0234_118:0937a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又見一切諸寶鏡中(至)三時懺悔迴向發願。",
        "from": "X07n0234_118:0937a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△三見寶柱放光。",
        "from": "X07n0234_118:0937a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又見一切諸寶柱中(至)或作一切諸光明色。",
        "from": "X07n0234_118:0937a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△四見寶像威儀。",
        "from": "X07n0234_118:0937a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又見彼閻浮檀金童女(至)曲躬瞻仰目不暫捨。",
        "from": "X07n0234_118:0937a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△五瓔珞等出生。",
        "from": "X07n0234_118:0937a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又見彼真珠瓔珞(至)一切皆以眾寶莊嚴。",
        "from": "X07n0234_118:0937a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△六蓮華重現。",
        "from": "X07n0234_118:0937b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復見彼優鉢羅華(至)三十二相莊嚴其身。",
        "from": "X07n0234_118:0937b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△七寶地現像。",
        "from": "X07n0234_118:0937b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復見彼淨瑠璃地(至)及諸樓閣莊嚴色像。",
        "from": "X07n0234_118:0937b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△八樹現半身。",
        "from": "X07n0234_118:0937b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又於寶樹枝葉華果(至)兜沙羅色光明。",
        "from": "X07n0234_118:0937b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兜沙羅。",
        "from": "X07n0234_118:0937b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此云霜氷。",
        "from": "X07n0234_118:0937b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝青色光明(至)瞻波迦華色光明。",
        "from": "X07n0234_118:0937b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞻波迦。",
        "from": "X07n0234_118:0937b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此云黃色。",
        "from": "X07n0234_118:0937b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△九半月現。",
        "from": "X07n0234_118:0937b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又見諸樓閣半月像中(至)種種光明普照十方。",
        "from": "X07n0234_118:0937b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△十譬現本事。",
        "from": "X07n0234_118:0937b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文二。",
        "from": "X07n0234_118:0937b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初見慈氏修行隨類攝生。",
        "from": "X07n0234_118:0937b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又見諸樓閣周迴四壁(至)修行諸度一切色像。",
        "from": "X07n0234_118:0937b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二覩所事友觀喻善財。",
        "from": "X07n0234_118:0937b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又見彌勒曾所承事(至)不思議事莫生疲厭。",
        "from": "X07n0234_118:0937b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二總顯見相二。",
        "from": "X07n0234_118:0937b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初法。",
        "from": "X07n0234_118:0937b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時善財童子。",
        "from": "X07n0234_118:0937b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先謂何力能見。",
        "from": "X07n0234_118:0937b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得不忘失憶念力故。",
        "from": "X07n0234_118:0937b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以何眼見。",
        "from": "X07n0234_118:0937b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得見十方清淨眼故。",
        "from": "X07n0234_118:0937b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次將何智見。",
        "from": "X07n0234_118:0937b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得善觀察無礙智故得諸菩薩自在智故。",
        "from": "X07n0234_118:0937b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次依何位見。",
        "from": "X07n0234_118:0937b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得諸菩薩已入智地廣大解故。",
        "from": "X07n0234_118:0937b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次於何處見。",
        "from": "X07n0234_118:0937c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切樓閣一一物中(至)自在境界諸莊嚴事。",
        "from": "X07n0234_118:0937c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後結前來所不說境。",
        "from": "X07n0234_118:0937c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△二喻。",
        "from": "X07n0234_118:0937c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如有人於睡夢中(至)能見如是自在境。",
        "from": "X07n0234_118:0937c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喻顯見相。",
        "from": "X07n0234_118:0937c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以有十喻者。",
        "from": "X07n0234_118:0937c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所喻別故。",
        "from": "X07n0234_118:0937c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然有二意。",
        "from": "X07n0234_118:0937c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一喻能見因緣別故。",
        "from": "X07n0234_118:0937c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二喻所見境相別故。",
        "from": "X07n0234_118:0937c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且初意者。",
        "from": "X07n0234_118:0937c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上一總喻能見所見。",
        "from": "X07n0234_118:0937c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆如夢事。",
        "from": "X07n0234_118:0937c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大小無礙等。",
        "from": "X07n0234_118:0937c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下九皆別。",
        "from": "X07n0234_118:0937c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二喻自因力。",
        "from": "X07n0234_118:0937c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨自業故。",
        "from": "X07n0234_118:0937c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦表善財凡命將終故。",
        "from": "X07n0234_118:0937c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三四二句並喻緣力。",
        "from": "X07n0234_118:0937c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍鬼所持故。",
        "from": "X07n0234_118:0937c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然鬼持自他不同。",
        "from": "X07n0234_118:0937c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍持自他同體。",
        "from": "X07n0234_118:0937c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五喻友依報力。",
        "from": "X07n0234_118:0937c10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六是定力。",
        "from": "X07n0234_118:0937c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七喻性空力。",
        "from": "X07n0234_118:0937c10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八喻法界自在力。",
        "from": "X07n0234_118:0937c11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九喻智定無二之力。",
        "from": "X07n0234_118:0937c11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大智海印之三昧故。",
        "from": "X07n0234_118:0937c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十幻智自在力。",
        "from": "X07n0234_118:0937c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "△次約所喻境相別者。",
        "from": "X07n0234_118:0937c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦初總。",
        "from": "X07n0234_118:0937c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘別。",
        "from": "X07n0234_118:0937c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如有人將欲命終(至)得見一切莊嚴境界。",
        "from": "X07n0234_118:0937c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨命現業喻。",
        "from": "X07n0234_118:0937c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喻所見冥應。",
        "from": "X07n0234_118:0937c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如有人為鬼所持(至)若有問者靡不能答。",
        "from": "X07n0234_118:0937c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非人所持喻。",
        "from": "X07n0234_118:0937c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喻所見能說。",
        "from": "X07n0234_118:0937c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如有人為龍所持(至)於少時間謂無量劫。",
        "from": "X07n0234_118:0937c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍宮淹久喻。",
        "from": "X07n0234_118:0937c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喻念劫圓融。",
        "from": "X07n0234_118:0937c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如梵宮名莊嚴藏(至)種種差別不相雜亂。",
        "from": "X07n0234_118:0937c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵宮廣現喻。",
        "from": "X07n0234_118:0937c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喻一多無礙。",
        "from": "X07n0234_118:0937c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如比丘入徧處定(至)一切境界悉皆明了。",
        "from": "X07n0234_118:0937c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徧處定境喻。",
        "from": "X07n0234_118:0937c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喻所見明了。",
        "from": "X07n0234_118:0937c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如有人於虗空中(至)悉分別知無有障礙。",
        "from": "X07n0234_118:0937c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乾城依空喻。",
        "from": "X07n0234_118:0938a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喻事理無礙。",
        "from": "X07n0234_118:0938a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如夜叉宮殿與人宮殿(至)各隨其業所見不同。",
        "from": "X07n0234_118:0938a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同處異見喻。",
        "from": "X07n0234_118:0938a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喻隱顯自在。",
        "from": "X07n0234_118:0938a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如大海於中悉見三千世界一切色像。",
        "from": "X07n0234_118:0938a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "海現三千喻。",
        "from": "X07n0234_118:0938a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喻頓見近遠。",
        "from": "X07n0234_118:0938a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如幻師以幻力故(至)一切莊嚴自在境界。",
        "from": "X07n0234_118:0938a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幻現眾多喻。",
        "from": "X07n0234_118:0938a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喻所見純雜無礙。",
