[
    {
        "source_sentence": "佛說摩利支天經",
        "from": "T21n1255b_001:0260b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐三藏沙門大廣智",
        "from": "T21n1255b_001:0260b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空奉　詔譯",
        "from": "T21n1255b_001:0260b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T21n1255b_001:0260b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時薄伽梵。",
        "from": "T21n1255b_001:0260b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在室羅筏城逝多林給孤獨園。",
        "from": "T21n1255b_001:0260b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告諸苾芻有天女名摩利支。",
        "from": "T21n1255b_001:0260b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大神通自在之力。",
        "from": "T21n1255b_001:0260b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常行日月天前。",
        "from": "T21n1255b_001:0260b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日天月天不能見彼。",
        "from": "T21n1255b_001:0260b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼能見日。",
        "from": "T21n1255b_001:0260b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能見無人能知。",
        "from": "T21n1255b_001:0260b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能捉無人能縛。",
        "from": "T21n1255b_001:0260b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能害無人能欺誑。",
        "from": "T21n1255b_001:0260b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能債其財物。",
        "from": "T21n1255b_001:0260b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能責罰。",
        "from": "T21n1255b_001:0260b11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為怨家能得其便。",
        "from": "T21n1255b_001:0260b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告諸苾芻。",
        "from": "T21n1255b_001:0260b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有知彼摩利支天名常憶念者。",
        "from": "T21n1255b_001:0260b13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼人亦不可見亦不可知。",
        "from": "T21n1255b_001:0260b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不可捉亦不可縛。",
        "from": "T21n1255b_001:0260b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不可害亦不可欺誑。",
        "from": "T21n1255b_001:0260b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不為人債其財物。",
        "from": "T21n1255b_001:0260b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不為人之所責罰。",
        "from": "T21n1255b_001:0260b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不為怨家能得其便。",
        "from": "T21n1255b_001:0260b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人。",
        "from": "T21n1255b_001:0260b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知彼摩利支天名求加護者。",
        "from": "T21n1255b_001:0260b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應作是言。",
        "from": "T21n1255b_001:0260b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我(某甲)知摩利支天母有大神力。",
        "from": "T21n1255b_001:0260b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今歸命願護我身。",
        "from": "T21n1255b_001:0260b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能見我。",
        "from": "T21n1255b_001:0260b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能知我。",
        "from": "T21n1255b_001:0260b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能捉我。",
        "from": "T21n1255b_001:0260b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能縛我。",
        "from": "T21n1255b_001:0260b20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能害我。",
        "from": "T21n1255b_001:0260b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能欺誑我。",
        "from": "T21n1255b_001:0260b21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能債我財物。",
        "from": "T21n1255b_001:0260b21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能責罰我。",
        "from": "T21n1255b_001:0260b22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不為怨家能得其便。",
        "from": "T21n1255b_001:0260b22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊說陀羅尼曰。",
        "from": "T21n1255b_001:0260b23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨(引)囉怛曩(二合)怛囉(二合)夜野(一)怛儞也(二合)他(去聲引二)遏迦麼枲(鼻聲已下同三)沫迦麼枲(四)阿(上聲)度(引)麼枲(五)紫(精以切引)鉢囉麼枲(六)摩訶紫鉢囉麼枲(七)頞怛馱(引)曩麼枲(八)麼(鼻聲引)哩(引)紫野麼枲(九)曩謨(引)娑覩(二合)帝(十)囉(轉舌)乞灑(二合)囉乞灑(二合)[牟*含](引十一)薩嚩薩怛嚩(二合引)難(上聲)左(十二)薩嚩怛囉(二合十三)薩嚩婆(去聲)喻(引)鉢捺囉(二合)吠(引)毘藥(二合)娑嚩(二合引)賀(引十四)心真言曰。",
        "from": "T21n1255b_001:0260b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜莫三(去聲)滿多沒馱(引)南(一)唵(引二)摩(鼻聲引)哩(引)唧(引)娑嚩(二合)賀(引三)爾時世尊說此陀羅尼已。",
        "from": "T21n1255b_001:0260c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告諸苾芻。",
        "from": "T21n1255b_001:0260c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有受持此經法者。",
        "from": "T21n1255b_001:0260c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應作是願。",
        "from": "T21n1255b_001:0260c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王難中護我賊難中護我。",
        "from": "T21n1255b_001:0260c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行路難中護我於失道曠野中護我。",
        "from": "T21n1255b_001:0260c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水火難中護我刀兵軍陣難中護我。",
        "from": "T21n1255b_001:0260c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼神難中護我毒藥難中護我。",
        "from": "T21n1255b_001:0260c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡獸難中護我毒蟲難中護我。",
        "from": "T21n1255b_001:0260c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切怨家惡人難中護我。",
        "from": "T21n1255b_001:0260c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛實語護我法實語護我僧實語護我。",
        "from": "T21n1255b_001:0260c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天實語護我仙人實語護我。",
        "from": "T21n1255b_001:0260c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切處一切時。",
        "from": "T21n1255b_001:0260c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願常護我弟子(某甲)莎嚩(二合引)賀(引)。",
        "from": "T21n1255b_001:0260c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告諸苾芻若善男子善女人苾芻苾芻尼。",
        "from": "T21n1255b_001:0260c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鄔波索迦鄔波斯迦。",
        "from": "T21n1255b_001:0260c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國王大臣一切人等有諸難時。",
        "from": "T21n1255b_001:0260c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但當至心誦此摩利支陀羅尼。",
        "from": "T21n1255b_001:0260c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不待加功隨誦隨成。",
        "from": "T21n1255b_001:0260c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離諸難除不至心。",
        "from": "T21n1255b_001:0260c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持誦之時并結本印。",
        "from": "T21n1255b_001:0260c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以香塗手先結三部心印。",
        "from": "T21n1255b_001:0260c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉為拳並竪二大拇指。",
        "from": "T21n1255b_001:0260c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是一切如來心印真言曰。",
        "from": "T21n1255b_001:0260c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引一)爾(惹以切)曩爾迦(半音二)次結蓮部心印。",
        "from": "T21n1255b_001:0260c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前印以左大拇指屈入掌。",
        "from": "T21n1255b_001:0260c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直竪右大拇指真言曰。",
        "from": "T21n1255b_001:0260c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引一)阿(去聲引)嚕(引)力迦(半音二)次結金剛部心印。",
        "from": "T21n1255b_001:0260c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前印以右大拇指屈入掌直竪左大拇指真言曰。",
        "from": "T21n1255b_001:0260c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引一)嚩日囉(二合)地力(二合)迦(半音二)次結護身如來拳印。",
        "from": "T21n1255b_001:0260c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手屈大拇指橫於掌中。",
        "from": "T21n1255b_001:0261a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便以四指握大拇指為拳。",
        "from": "T21n1255b_001:0261a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此拳印加持自身五處。",
        "from": "T21n1255b_001:0261a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先額右肩左肩心喉每處各誦真言一遍真言曰。",
        "from": "T21n1255b_001:0261a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引一)僕(引)入嚩(二合)羅吽(引二)次結摩利支菩薩根本印。",
        "from": "T21n1255b_001:0261a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二小指二無名指。",
        "from": "T21n1255b_001:0261a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右押左內相叉。",
        "from": "T21n1255b_001:0261a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直竪二頭指相捻。",
        "from": "T21n1255b_001:0261a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二中指各繳頭指背。",
        "from": "T21n1255b_001:0261a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向前頭相拄。",
        "from": "T21n1255b_001:0261a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指並竪即成。",
        "from": "T21n1255b_001:0261a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結印當心。",
        "from": "T21n1255b_001:0261a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦前摩利支身陀羅尼及心各七遍。",
        "from": "T21n1255b_001:0261a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每遍屈二大拇指招之。",
        "from": "T21n1255b_001:0261a10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名迎請印。",
        "from": "T21n1255b_001:0261a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼以此印加持身五處。",
        "from": "T21n1255b_001:0261a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂上散印。",
        "from": "T21n1255b_001:0261a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結大三昧耶印。",
        "from": "T21n1255b_001:0261a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辟除結界。",
        "from": "T21n1255b_001:0261a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手大指捻小指甲上。",
        "from": "T21n1255b_001:0261a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘三指直竪如三股杵形。",
        "from": "T21n1255b_001:0261a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手作金剛拳按於心上。",
        "from": "T21n1255b_001:0261a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨誦真言以右手印於頂上。",
        "from": "T21n1255b_001:0261a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左轉三匝辟除一切作障難者。",
        "from": "T21n1255b_001:0261a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便右旋三匝并揮上下。",
        "from": "T21n1255b_001:0261a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成結十方界。",
        "from": "T21n1255b_001:0261a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切天龍人非人等不能附近真言曰。",
        "from": "T21n1255b_001:0261a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引一)商迦[口*(隸木+士)](輕舌二)摩訶(引)三(去聲)麼(鼻聲)琰娑嚩賀次結摩利支安怛袒那印(此言隱形)以左手虛掌作拳。",
        "from": "T21n1255b_001:0261a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指微捻頭指甲如環。",
        "from": "T21n1255b_001:0261a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已下三指握拳令密。",
        "from": "T21n1255b_001:0261a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又令掌中作孔。",
        "from": "T21n1255b_001:0261a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安自心前。",
        "from": "T21n1255b_001:0261a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想自身入此印孔中藏。",
        "from": "T21n1255b_001:0261a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手平掌。",
        "from": "T21n1255b_001:0261a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋摩此印便蓋孔上。",
        "from": "T21n1255b_001:0261a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想此印即是摩利支天菩薩身。",
        "from": "T21n1255b_001:0261a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我自身隱藏於摩利支天菩薩心中。",
        "from": "T21n1255b_001:0261a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心專注不間斷。",
        "from": "T21n1255b_001:0261a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦前根本及心真言。",
        "from": "T21n1255b_001:0261a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不限遍數但虔誠至心。",
        "from": "T21n1255b_001:0261a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必獲菩薩威神加護。",
        "from": "T21n1255b_001:0261a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切怨家惡人悉不能見。",
        "from": "T21n1255b_001:0261a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切災難皆得解脫。",
        "from": "T21n1255b_001:0261a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲供養摩利支菩薩者。",
        "from": "T21n1255b_001:0261b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應用金或銀或赤銅。",
        "from": "T21n1255b_001:0261b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或白檀香木或紫檀木等。",
        "from": "T21n1255b_001:0261b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刻作摩利支菩薩像。",
        "from": "T21n1255b_001:0261b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如天女形可長半寸。",
        "from": "T21n1255b_001:0261b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或一寸二寸已下。",
        "from": "T21n1255b_001:0261b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於蓮花上或立或坐。",
        "from": "T21n1255b_001:0261b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭冠瓔珞種種莊嚴極令端正。",
        "from": "T21n1255b_001:0261b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手把天扇。",
        "from": "T21n1255b_001:0261b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其扇如維摩詰前天女扇。",
        "from": "T21n1255b_001:0261b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手垂下揚掌向外。",
        "from": "T21n1255b_001:0261b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "展五指作與願勢。",
        "from": "T21n1255b_001:0261b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有二天女各執白拂侍立左右。",
        "from": "T21n1255b_001:0261b07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作此像成。",
        "from": "T21n1255b_001:0261b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戴於頂上或戴臂上或置衣中。",
        "from": "T21n1255b_001:0261b08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以菩薩威神之力不逢災難。",
        "from": "T21n1255b_001:0261b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於怨家處決定得勝。",
        "from": "T21n1255b_001:0261b09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼神惡人無得便。",
        "from": "T21n1255b_001:0261b10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲成驗願見摩利支天真身求勝願者。",
        "from": "T21n1255b_001:0261b10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此陀羅尼滿十萬遍。",
        "from": "T21n1255b_001:0261b11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依法建立曼荼羅。",
        "from": "T21n1255b_001:0261b12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫摩利支菩薩像。",
        "from": "T21n1255b_001:0261b12_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安置壇中種種供養。",
        "from": "T21n1255b_001:0261b13_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并作護摩火壇。",
        "from": "T21n1255b_001:0261b13_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩利支天女必現其身。",
        "from": "T21n1255b_001:0261b14_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求勝願決定成就除不至心。",
        "from": "T21n1255b_001:0261b14_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告諸苾芻。",
        "from": "T21n1255b_001:0261b15_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為當來惡世苦難恐怖有情。",
        "from": "T21n1255b_001:0261b15_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略說摩利支天法。",
        "from": "T21n1255b_001:0261b16_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此菩薩有大悲願。",
        "from": "T21n1255b_001:0261b16_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常於苦難恐怖之處。",
        "from": "T21n1255b_001:0261b17_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護諸有情不令天龍鬼神人及非人怨家惡獸所能為害。",
        "from": "T21n1255b_001:0261b17_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當受持廣宣流布饒益。",
        "from": "T21n1255b_001:0261b19_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有情諸苾芻等及天龍。",
        "from": "T21n1255b_001:0261b19_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八部一切大眾聞佛所說。",
        "from": "T21n1255b_001:0261b20_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆大歡喜信受奉行。",
        "from": "T21n1255b_001:0261b20_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說摩利支天經",
        "from": "T21n1255b_001:0261b22_0",
        "context_with_next_sentence": false
    }
]