    {
        "source_sentence": "得翻經大德佛授記寺主昌平縣開國公德感。",
        "from": "T55n2153_011:0442b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及翻經大德弘景等。",
        "from": "T55n2153_011:0442b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牒稱奉勅。",
        "from": "T55n2153_011:0442b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧德感等所奏涅槃經茶毘後分二卷。",
        "from": "T55n2153_011:0442b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛頂尊勝陀羅尼經一卷。",
        "from": "T55n2153_011:0442b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "占察經兩卷。",
        "from": "T55n2153_011:0442b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陀羅尼集經十二卷等。",
        "from": "T55n2153_011:0442b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於他國翻來。",
        "from": "T55n2153_011:0442b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於內外道場譯出。",
        "from": "T55n2153_011:0442b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸如此類未為經。",
        "from": "T55n2153_011:0442b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞奏宣行。",
        "from": "T55n2153_011:0442b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多無目錄者。",
        "from": "T55n2153_011:0442b07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宜付定經目錄。",
        "from": "T55n2153_011:0442b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧等子細類會。",
        "from": "T55n2153_011:0442b08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若是真經。",
        "from": "T55n2153_011:0442b08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即宜編入目錄者。",
        "from": "T55n2153_011:0442b09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定經目。",
        "from": "T55n2153_011:0442b09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧等准勅類會。",
        "from": "T55n2153_011:0442b09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並是實經編入錄訖。",
        "from": "T55n2153_011:0442b10_19",
        "context_with_next_sentence": true
    },
    {
