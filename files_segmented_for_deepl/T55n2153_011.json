        "source_sentence": "法門念佛三昧經一卷",
        "from": "T55n2153_011:0437b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "集法悅捨苦陀羅尼經一卷",
        "from": "T55n2153_011:0437b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天王所說陀羅尼經一卷",
        "from": "T55n2153_011:0437b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛十二使呪經一卷",
        "from": "T55n2153_011:0437b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "血氣神呪經一卷(一云取血氣經)",
        "from": "T55n2153_011:0437b20_0",
        "context_with_next_sentence": true
    },
