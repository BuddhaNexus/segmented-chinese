[
    {
        "source_sentence": "大周刊定眾經目錄卷第十一",
        "from": "T55n2153_011:0437a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大唐天后勅佛授記寺沙門明佺等撰",
        "from": "T55n2153_011:0437a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大小乘失譯經目一卷(四百二十四部六百三十六卷)右失譯經者。",
        "from": "T55n2153_011:0437a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "興廢遞更經全失主。",
        "from": "T55n2153_011:0437a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故今編之貽諸後代。",
        "from": "T55n2153_011:0437a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大乘居先小乘於後。",
        "from": "T55n2153_011:0437a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大乘失譯經",
        "from": "T55n2153_011:0437a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛名經一部三卷",
        "from": "T55n2153_011:0437a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛名經一部十卷",
        "from": "T55n2153_011:0437a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現在千佛名一卷",
        "from": "T55n2153_011:0437a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過去千佛名一卷",
        "from": "T55n2153_011:0437a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當來星宿劫千佛名一卷",
        "from": "T55n2153_011:0437a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同號佛名一卷",
        "from": "T55n2153_011:0437b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請金剛呪經一卷",
        "from": "T55n2153_011:0437b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛隨意所樂一切皆呪經一卷",
        "from": "T55n2153_011:0437b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛如所願一切勝呪經一卷",
        "from": "T55n2153_011:0437b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛結界呪經一卷",
        "from": "T55n2153_011:0437b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛小心除滅諸怨呪經一卷",
        "from": "T55n2153_011:0437b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛請夢呪經一卷",
        "from": "T55n2153_011:0437b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思益呪經一卷",
        "from": "T55n2153_011:0437b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方佛神呪經一卷",
        "from": "T55n2153_011:0437b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩善戒比丘藏經一卷",
        "from": "T55n2153_011:0437b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說菩薩戒經一卷",
        "from": "T55n2153_011:0437b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日出經一卷",
        "from": "T55n2153_011:0437b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雜要經一卷(一名菩薩要行經)",
        "from": "T55n2153_011:0437b13_0",
        "context_with_next_sentence": true
    },
    {
