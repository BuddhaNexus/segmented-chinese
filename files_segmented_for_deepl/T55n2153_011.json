        "from": "T55n2153_011:0437b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說分別觀經一卷",
        "from": "T55n2153_011:0437b15_0",
        "context_with_next_sentence": true
    },
