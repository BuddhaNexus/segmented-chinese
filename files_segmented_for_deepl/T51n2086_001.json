[
    {
        "source_sentence": "北魏僧惠生使西域記",
        "from": "T51n2086_001:0866c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魏神龜元年十一月冬。",
        "from": "T51n2086_001:0866c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大后遣崇立寺比邱惠生與敦煌人宋雲。",
        "from": "T51n2086_001:0866c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向西域取經。",
        "from": "T51n2086_001:0866c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡得百七十部。",
        "from": "T51n2086_001:0866c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆是大乘妙典。",
        "from": "T51n2086_001:0866c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初發京師。",
        "from": "T51n2086_001:0866c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西行四十日。",
        "from": "T51n2086_001:0866c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至赤嶺。",
        "from": "T51n2086_001:0866c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即國之西疆也。",
        "from": "T51n2086_001:0866c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "山無草木。",
        "from": "T51n2086_001:0866c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有鳥鼠。",
        "from": "T51n2086_001:0866c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同穴。",
        "from": "T51n2086_001:0866c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又西行二十日。",
        "from": "T51n2086_001:0866c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至吐谷渾國。",
        "from": "T51n2086_001:0866c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又西行三千五百里。",
        "from": "T51n2086_001:0866c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至鄯善城。",
        "from": "T51n2086_001:0866c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又西行千六百里。",
        "from": "T51n2086_001:0866c17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至且末城。",
        "from": "T51n2086_001:0866c17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有呂光代胡時所作佛菩薩像。",
        "from": "T51n2086_001:0866c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又西行千三百七十五里。",
        "from": "T51n2086_001:0866c18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至末城。",
        "from": "T51n2086_001:0866c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又西行二十二里。",
        "from": "T51n2086_001:0866c19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至捍&#;城。",
        "from": "T51n2086_001:0866c19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有于闐供佛之塔。",
        "from": "T51n2086_001:0866c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其旁小塔數千。",
        "from": "T51n2086_001:0866c20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縣幡萬計。",
        "from": "T51n2086_001:0866c20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又西行八百七十八里。",
        "from": "T51n2086_001:0866c21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至于闐國。",
        "from": "T51n2086_001:0866c21_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有國王所造覆盆浮圖一軀。",
        "from": "T51n2086_001:0866c21_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有辟支佛靴。",
        "from": "T51n2086_001:0866c22_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "于今不爛。",
        "from": "T51n2086_001:0866c22_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "于闐境東西三千里。",
        "from": "T51n2086_001:0866c23_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神龜二年七月二十九日。",
        "from": "T51n2086_001:0866c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入朱駒波國。",
        "from": "T51n2086_001:0866c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人民山居。",
        "from": "T51n2086_001:0866c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不立屠殺。",
        "from": "T51n2086_001:0866c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食自死肉。",
        "from": "T51n2086_001:0866c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風俗語言。",
        "from": "T51n2086_001:0866c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與于闐同。",
        "from": "T51n2086_001:0866c25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文學與婆羅門同。",
        "from": "T51n2086_001:0866c26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其國疆界。",
        "from": "T51n2086_001:0866c26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可五日行遍。",
        "from": "T51n2086_001:0866c26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八月入渴盤陀國界。",
        "from": "T51n2086_001:0866c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西行六百里。",
        "from": "T51n2086_001:0866c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "登葱嶺山。",
        "from": "T51n2086_001:0866c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復西行三日。",
        "from": "T51n2086_001:0866c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至鉢孟城。",
        "from": "T51n2086_001:0866c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三日至毒龍池。",
        "from": "T51n2086_001:0866c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為昔盤陀王以婆羅門呪呪之。",
        "from": "T51n2086_001:0866c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍徙葱嶺西。",
        "from": "T51n2086_001:0866c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "去此地二千餘里。",
        "from": "T51n2086_001:0866c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自發葱嶺步步漸高。",
        "from": "T51n2086_001:0867a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如此四日。",
        "from": "T51n2086_001:0867a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至嶺。",
        "from": "T51n2086_001:0867a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依約中下。",
        "from": "T51n2086_001:0867a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實天半矣。",
        "from": "T51n2086_001:0867a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "渴盤陀國。",
        "from": "T51n2086_001:0867a02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正在山頂。",
        "from": "T51n2086_001:0867a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自葱嶺已西。",
        "from": "T51n2086_001:0867a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水皆西流入西海。",
        "from": "T51n2086_001:0867a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世人云。",
        "from": "T51n2086_001:0867a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是天地之中。",
        "from": "T51n2086_001:0867a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九月中旬。",
        "from": "T51n2086_001:0867a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入鉢和國。",
        "from": "T51n2086_001:0867a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高山深谷。",
        "from": "T51n2086_001:0867a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "險道如常。",
        "from": "T51n2086_001:0867a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因山為城。",
        "from": "T51n2086_001:0867a05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "氈服窟居。",
        "from": "T51n2086_001:0867a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人畜相依。",
        "from": "T51n2086_001:0867a05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風雪勁切。",
        "from": "T51n2086_001:0867a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大雪山。",
        "from": "T51n2086_001:0867a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "望若玉峯。",
        "from": "T51n2086_001:0867a06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十月初旬入嚈噠國。",
        "from": "T51n2086_001:0867a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居無城郭。",
        "from": "T51n2086_001:0867a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨逐水草。",
        "from": "T51n2086_001:0867a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不識文字。",
        "from": "T51n2086_001:0867a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年無盈[門@壬]。",
        "from": "T51n2086_001:0867a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周十二月為一歲。",
        "from": "T51n2086_001:0867a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受諸國貢獻。",
        "from": "T51n2086_001:0867a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南至牒羅。",
        "from": "T51n2086_001:0867a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北盡勅勒。",
        "from": "T51n2086_001:0867a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東被于闐。",
        "from": "T51n2086_001:0867a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西及波斯。",
        "from": "T51n2086_001:0867a09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四十餘國。",
        "from": "T51n2086_001:0867a10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆來朝貢。",
        "from": "T51n2086_001:0867a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最為強大。",
        "from": "T51n2086_001:0867a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王帳周四十步。",
        "from": "T51n2086_001:0867a10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "器用七寶。",
        "from": "T51n2086_001:0867a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不信佛法。",
        "from": "T51n2086_001:0867a11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殺生血食。",
        "from": "T51n2086_001:0867a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見魏使拜受詔書。",
        "from": "T51n2086_001:0867a11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "去京師二萬餘里。",
        "from": "T51n2086_001:0867a12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十一月入波斯國。",
        "from": "T51n2086_001:0867a12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "境土甚狹。",
        "from": "T51n2086_001:0867a13_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七月行過。",
        "from": "T51n2086_001:0867a13_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人居山谷。",
        "from": "T51n2086_001:0867a13_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雪光耀日。",
        "from": "T51n2086_001:0867a13_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十一月中旬入賒彌國。",
        "from": "T51n2086_001:0867a14_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漸出葱嶺。",
        "from": "T51n2086_001:0867a14_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "磽角危峻。",
        "from": "T51n2086_001:0867a14_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人馬僅通。",
        "from": "T51n2086_001:0867a15_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鐵鎖懸度。",
        "from": "T51n2086_001:0867a15_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下不見底。",
        "from": "T51n2086_001:0867a15_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二月初旬入烏場國。",
        "from": "T51n2086_001:0867a15_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北接葱嶺。",
        "from": "T51n2086_001:0867a16_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南連天竺。",
        "from": "T51n2086_001:0867a16_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "土氣和暖。",
        "from": "T51n2086_001:0867a16_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "原田膴膴。",
        "from": "T51n2086_001:0867a17_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "民物殷阜。",
        "from": "T51n2086_001:0867a17_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國王菜食長齋。",
        "from": "T51n2086_001:0867a17_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晨夜禮佛。",
        "from": "T51n2086_001:0867a17_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日中以後。",
        "from": "T51n2086_001:0867a18_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始治國事。",
        "from": "T51n2086_001:0867a18_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鐘聲遍界。",
        "from": "T51n2086_001:0867a18_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "異花供養。",
        "from": "T51n2086_001:0867a18_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞魏使來。",
        "from": "T51n2086_001:0867a19_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "膜拜受詔。",
        "from": "T51n2086_001:0867a19_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國中有如來晒衣履石之處。",
        "from": "T51n2086_001:0867a19_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其餘佛跡。",
        "from": "T51n2086_001:0867a20_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所至炳然每一佛跡。",
        "from": "T51n2086_001:0867a20_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輒有寺塔履之。",
        "from": "T51n2086_001:0867a20_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比丘戒行清苦。",
        "from": "T51n2086_001:0867a21_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至正光元年四月中旬入乾陀羅國。",
        "from": "T51n2086_001:0867a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "土地與烏場國相似。",
        "from": "T51n2086_001:0867a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本名業波羅。",
        "from": "T51n2086_001:0867a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為嚈噠所滅。",
        "from": "T51n2086_001:0867a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂立勅勒為王。",
        "from": "T51n2086_001:0867a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國中人民。",
        "from": "T51n2086_001:0867a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉是婆羅門。",
        "from": "T51n2086_001:0867a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為嚈噠□典。",
        "from": "T51n2086_001:0867a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而國王好殺。",
        "from": "T51n2086_001:0867a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不信佛法。",
        "from": "T51n2086_001:0867a25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與罽賓爭境。",
        "from": "T51n2086_001:0867a25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "連年戰鬪。",
        "from": "T51n2086_001:0867a25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師老民怨。",
        "from": "T51n2086_001:0867a26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐受詔書。",
        "from": "T51n2086_001:0867a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凶慢無禮。",
        "from": "T51n2086_001:0867a26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "送使一寺。",
        "from": "T51n2086_001:0867a26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供給甚薄。",
        "from": "T51n2086_001:0867a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西行至新頭大河。",
        "from": "T51n2086_001:0867a27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復西行十三日。",
        "from": "T51n2086_001:0867a27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至佛沙伏城。",
        "from": "T51n2086_001:0867a28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "城郭端直。",
        "from": "T51n2086_001:0867a28_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "林泉茂盛。",
        "from": "T51n2086_001:0867a28_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "土饒珍寶。",
        "from": "T51n2086_001:0867a28_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風俗淳善。",
        "from": "T51n2086_001:0867a29_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名僧德泉道行高奇。",
        "from": "T51n2086_001:0867a29_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "石像莊嚴。",
        "from": "T51n2086_001:0867a29_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通身金箔。",
        "from": "T51n2086_001:0867b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有迦葉波佛跡。",
        "from": "T51n2086_001:0867b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復西行一日。",
        "from": "T51n2086_001:0867b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘舟渡一深水。",
        "from": "T51n2086_001:0867b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三百餘步。",
        "from": "T51n2086_001:0867b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復西南行六十里。",
        "from": "T51n2086_001:0867b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至乾陀羅城。",
        "from": "T51n2086_001:0867b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有佛涅槃後二百年。",
        "from": "T51n2086_001:0867b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國王迦尼迦所造雀離浮圖凡十二重。",
        "from": "T51n2086_001:0867b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "去地七百尺。",
        "from": "T51n2086_001:0867b04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "基廣三百餘步。",
        "from": "T51n2086_001:0867b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉用文石為陛。",
        "from": "T51n2086_001:0867b05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塔內佛事。",
        "from": "T51n2086_001:0867b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千變萬化。",
        "from": "T51n2086_001:0867b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金盤晃朗。",
        "from": "T51n2086_001:0867b06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶鐸和鳴。",
        "from": "T51n2086_001:0867b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西域浮圖。",
        "from": "T51n2086_001:0867b06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最為第一。",
        "from": "T51n2086_001:0867b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復西北行。",
        "from": "T51n2086_001:0867b07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "渡一大水。",
        "from": "T51n2086_001:0867b07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至那迦邏國。",
        "from": "T51n2086_001:0867b07_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有佛頂骨及佛手書梵字石塔銘。",
        "from": "T51n2086_001:0867b07_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡在烏場國二年。",
        "from": "T51n2086_001:0867b08_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至正光二年還闕。",
        "from": "T51n2086_001:0867b09_0",
        "context_with_next_sentence": false
    }
]