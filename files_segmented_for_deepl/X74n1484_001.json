[
    {
        "source_sentence": "消災延壽藥師懺法卷上",
        "from": "X74n1484_001:0571b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "熏修藥師懺儀",
        "from": "X74n1484_001:0571b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚴淨壇場",
        "from": "X74n1484_001:0571b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "楊枝淨水。",
        "from": "X74n1484_001:0571b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍洒三千。",
        "from": "X74n1484_001:0571b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性空八德利人天。",
        "from": "X74n1484_001:0571b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福慧廣增延。",
        "from": "X74n1484_001:0571b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅罪除愆。",
        "from": "X74n1484_001:0571b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火燄化紅蓮。",
        "from": "X74n1484_001:0571b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無清涼地菩薩摩訶薩(三稱)",
        "from": "X74n1484_001:0571b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無聖觀自在菩薩(三稱)　(眾持大悲等呪心經畢舉)　摩訶般若波羅蜜多(云云)。",
        "from": "X74n1484_001:0571b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵。",
        "from": "X74n1484_001:0571b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捺摩巴葛瓦帝。",
        "from": "X74n1484_001:0571b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿巴囉密沓。",
        "from": "X74n1484_001:0571b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿優哩阿納。",
        "from": "X74n1484_001:0571b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘇必你。",
        "from": "X74n1484_001:0571b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實執沓。",
        "from": "X74n1484_001:0571b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牒左囉宰也。",
        "from": "X74n1484_001:0571b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛塔哿達也。",
        "from": "X74n1484_001:0571b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿囉訶帝。",
        "from": "X74n1484_001:0571b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藥三不達也。",
        "from": "X74n1484_001:0571b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛你也塔。",
        "from": "X74n1484_001:0571b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵。",
        "from": "X74n1484_001:0571b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薩哩巴。",
        "from": "X74n1484_001:0571b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "桑斯葛哩。",
        "from": "X74n1484_001:0571b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "叭哩述沓。",
        "from": "X74n1484_001:0571b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "達囉馬帝。",
        "from": "X74n1484_001:0571b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "哿哿捺。",
        "from": "X74n1484_001:0571b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "桑馬兀哿帝。",
        "from": "X74n1484_001:0571b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莎巴瓦。",
        "from": "X74n1484_001:0571b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比述帝。",
        "from": "X74n1484_001:0571b16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬喝捺也。",
        "from": "X74n1484_001:0571b16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "叭哩瓦哩莎喝。",
        "from": "X74n1484_001:0571b17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願將以此勝功德。",
        "from": "X74n1484_001:0571b17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祝贊皇帝萬萬歲。",
        "from": "X74n1484_001:0571b17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖明君。",
        "from": "X74n1484_001:0571b18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸國來朝會。",
        "from": "X74n1484_001:0571b18_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無無量壽。",
        "from": "X74n1484_001:0571b18_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祝讚皇帝萬萬歲(再云)。",
        "from": "X74n1484_001:0571b18_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　以此經呪功德　　回向護法龍天",
        "from": "X74n1484_001:0571b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　三界岳瀆靈聰　　守護道場真宰",
        "from": "X74n1484_001:0571b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　祈福保安平善　　莊嚴無上菩提",
        "from": "X74n1484_001:0571b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　普願法界眾生　　共入毗盧性海",
        "from": "X74n1484_001:0571b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香讚",
        "from": "X74n1484_001:0571b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爐香乍爇。",
        "from": "X74n1484_001:0571c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法界蒙熏。",
        "from": "X74n1484_001:0571c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥師海會悉遙聞。",
        "from": "X74n1484_001:0571c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨處結祥雲。",
        "from": "X74n1484_001:0571c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誠意方殷。",
        "from": "X74n1484_001:0571c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛現全身。",
        "from": "X74n1484_001:0571c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香雲葢菩薩摩訶薩(三稱　跪誦藥師經一卷)　普賢王菩薩　　象駕光臨　有一菩薩　　結跏趺坐　　名曰普賢　身白玉色　　五十種光　　光五十種色　以為項光　　身諸毛孔　　流出金光　其金光端　　無量化佛　　諸化菩薩　以為眷屬　　安庠徐步　　雨大寶華　至行者前　　其象開口　　於象牙上　諸池玉女　　鼓樂絃歌　　其聲微妙　讚歎大乘　　一實之道　　行者見已　歡喜敬禮　　復更讀誦　　甚深經典　徧禮十方　　無量諸佛　　禮多寶佛塔　及釋迦牟尼　　并禮普賢　　諸大菩薩　發是誓願　　若我宿福　　應見普賢　願尊者徧吉　　示我色身普賢王菩薩摩訶薩(三聲)一切恭敬一心頂禮十方法界常住佛一心頂禮十方法界常住法一心頂禮十方法界常住僧是諸眾等各各胡跪嚴持香華如法供養。",
        "from": "X74n1484_001:0571c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　願此香華遍十方　　以為微妙光明臺　諸天音樂天寶香　　諸天肴饍天寶衣　不可思議妙法塵　　一一塵出一切塵　一一塵出一切法　　旋轉無礙互莊嚴　遍至十方三寶前　　十方法界三寶前　皆有我身脩供養　　一一皆悉徧法界　彼彼無雜無障礙　　盡未來際作佛事　普熏法界諸眾生　　蒙熏皆發菩提心　同入無生證佛智　願此香華雲　　遍滿十方界　　供養一切佛　尊法諸菩薩　　緣覺聲聞眾　　及一切天僊　以起光明臺　　過於無邊界　　無邊佛土中　受用作佛事　　普熏諸眾生　　皆發菩提心　容顏甚奇妙　　光明照十方　　我適曾供養　今復還親覲　　聖主天中王　　迦陵頻伽聲　哀愍眾生者　　我等今敬禮南無寶曇華菩薩摩訶薩(三稱)藥師如來(云)證明禮懺(述和　主白白云)。",
        "from": "X74n1484_001:0571c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　妄念成生滅　　真如不變遷　　總持難思議　無住對空宣仰白十方三寶。",
        "from": "X74n1484_001:0572a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥師如來。",
        "from": "X74n1484_001:0572a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剎海天龍。",
        "from": "X74n1484_001:0572a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切聖眾。",
        "from": "X74n1484_001:0572a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願賜慈悲。",
        "from": "X74n1484_001:0572a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俯垂證鑒。",
        "from": "X74n1484_001:0572a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有疏披宣。",
        "from": "X74n1484_001:0572a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏希諦聽。",
        "from": "X74n1484_001:0572a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(表白宣意畢)",
        "from": "X74n1484_001:0572a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上來情旨。",
        "from": "X74n1484_001:0572a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恭對披宣。",
        "from": "X74n1484_001:0572a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大圓鏡中。",
        "from": "X74n1484_001:0572a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諒垂印可。",
        "from": "X74n1484_001:0572a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是以脩香華供養。",
        "from": "X74n1484_001:0572a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燈燭莊嚴。",
        "from": "X74n1484_001:0572a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命我等流。",
        "from": "X74n1484_001:0572a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "代行懺法。",
        "from": "X74n1484_001:0572a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今則依仗如來大寂滅海。",
        "from": "X74n1484_001:0572a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸投普門大悲願王。",
        "from": "X74n1484_001:0572b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蕩罪業於無何有之鄉。",
        "from": "X74n1484_001:0572b01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剪芥蒂於不萌枝之地。",
        "from": "X74n1484_001:0572b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心純是法。",
        "from": "X74n1484_001:0572b02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與法相應。",
        "from": "X74n1484_001:0572b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "理事圓融。",
        "from": "X74n1484_001:0572b02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行願成就。",
        "from": "X74n1484_001:0572b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我既如是。",
        "from": "X74n1484_001:0572b03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛必哀憐。",
        "from": "X74n1484_001:0572b03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈叩洪慈。",
        "from": "X74n1484_001:0572b03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冥熏加被。",
        "from": "X74n1484_001:0572b03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　佛面猶如淨滿月　　亦如千日放光明　圓光普照於十方　　喜捨慈悲皆具足啟運藥師道場懺法今當歸命三世諸佛南無過去毗婆尸佛南無尸棄佛南無毗舍浮佛南無拘留孫佛南無拘那含牟尼佛南無迦葉佛南無本師釋迦牟尼佛南無當來彌勒尊佛南無藥師琉璃光如來南無藥師海會佛菩薩(三稱)開經偈　無上甚深微妙法　　百千萬劫難遭遇　我今見聞得受持　　願解如來懺法義消災延壽藥師懺法卷上一切諸佛。",
        "from": "X74n1484_001:0572b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愍念眾生。",
        "from": "X74n1484_001:0572b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為說藥師道場懺法。",
        "from": "X74n1484_001:0572b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "良以眾生垢重。",
        "from": "X74n1484_001:0572b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無明暗覆。",
        "from": "X74n1484_001:0572b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不諳因果。",
        "from": "X74n1484_001:0572b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不勤懺悔。",
        "from": "X74n1484_001:0572b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縱貪嗔癡。",
        "from": "X74n1484_001:0572b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "肆殺盜婬。",
        "from": "X74n1484_001:0572b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量無邊。",
        "from": "X74n1484_001:0572b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所作罪垢。",
        "from": "X74n1484_001:0572b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量無邊。",
        "from": "X74n1484_001:0572b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所結冤業。",
        "from": "X74n1484_001:0572b20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不覺不知。",
        "from": "X74n1484_001:0572b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日深日厚。",
        "from": "X74n1484_001:0572b21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以致促長壽而夭枉。",
        "from": "X74n1484_001:0572b21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黜官位而下賤。",
        "from": "X74n1484_001:0572b21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "削富饒而貧窮。",
        "from": "X74n1484_001:0572b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "折子女而孤獨。",
        "from": "X74n1484_001:0572b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "罹斯九橫。",
        "from": "X74n1484_001:0572b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "墜墮三塗。",
        "from": "X74n1484_001:0572b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "紛紛苦果。",
        "from": "X74n1484_001:0572b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自作自受。",
        "from": "X74n1484_001:0572b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種惡報。",
        "from": "X74n1484_001:0572b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或現或後。",
        "from": "X74n1484_001:0572b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纖毫無差。",
        "from": "X74n1484_001:0572b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遲速必報。",
        "from": "X74n1484_001:0572b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時。",
        "from": "X74n1484_001:0572b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥師琉璃光如來。",
        "from": "X74n1484_001:0572c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈悲拯濟。",
        "from": "X74n1484_001:0572c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是本願功德經。",
        "from": "X74n1484_001:0572c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令讀誦者。",
        "from": "X74n1484_001:0572c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造彼如來形像七軀。",
        "from": "X74n1484_001:0572c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一像前。",
        "from": "X74n1484_001:0572c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各置七燈。",
        "from": "X74n1484_001:0572c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一燈量。",
        "from": "X74n1484_001:0572c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大如車輪。",
        "from": "X74n1484_001:0572c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燃至四十九日。",
        "from": "X74n1484_001:0572c03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光明不絕。",
        "from": "X74n1484_001:0572c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是惟剎利。",
        "from": "X74n1484_001:0572c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居士大家。",
        "from": "X74n1484_001:0572c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多饒財寶。",
        "from": "X74n1484_001:0572c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倉庫盈溢之所堪為。",
        "from": "X74n1484_001:0572c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而緜力眾生。",
        "from": "X74n1484_001:0572c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心雖喜樂。",
        "from": "X74n1484_001:0572c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能如法莊嚴。",
        "from": "X74n1484_001:0572c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以是因緣。",
        "from": "X74n1484_001:0572c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於本願功德經中。",
        "from": "X74n1484_001:0572c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譯出消災延壽懺法。",
        "from": "X74n1484_001:0572c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便諸眾生。",
        "from": "X74n1484_001:0572c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "齋戒沐浴。",
        "from": "X74n1484_001:0572c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於寺院。",
        "from": "X74n1484_001:0572c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或在家庭。",
        "from": "X74n1484_001:0572c07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或延禪侶。",
        "from": "X74n1484_001:0572c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或偕善士。",
        "from": "X74n1484_001:0572c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洒掃熏修。",
        "from": "X74n1484_001:0572c07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香華燈燭。",
        "from": "X74n1484_001:0572c07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨分供養。",
        "from": "X74n1484_001:0572c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依科讚詠。",
        "from": "X74n1484_001:0572c08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虔誠敬禮。",
        "from": "X74n1484_001:0572c08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則無求不應。",
        "from": "X74n1484_001:0572c08_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無願不成。",
        "from": "X74n1484_001:0572c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經云。",
        "from": "X74n1484_001:0572c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說悔先罪。",
        "from": "X74n1484_001:0572c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨名所尚。",
        "from": "X74n1484_001:0572c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "改往修來。",
        "from": "X74n1484_001:0572c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洗浣身心。",
        "from": "X74n1484_001:0572c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷除眾惡。",
        "from": "X74n1484_001:0572c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誓不更造。",
        "from": "X74n1484_001:0572c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多劫罪愆。",
        "from": "X74n1484_001:0572c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨盡無餘。",
        "from": "X74n1484_001:0572c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨所樂求。",
        "from": "X74n1484_001:0572c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉令滿足。",
        "from": "X74n1484_001:0572c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(某等)志心頂禮。",
        "from": "X74n1484_001:0572c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求哀懺悔。",
        "from": "X74n1484_001:0572c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無毗盧遮那佛南無本師釋迦牟尼佛南無藥師琉璃光如來南無無量壽佛南無盡十方遍法界過去一切諸佛南無盡十方遍法界現在一切諸佛南無盡十方遍法界未來一切諸佛南無藥師琉璃光如來本願功德經南無日光遍照菩薩南無月光遍照菩薩南無文殊師利菩薩南無觀世音菩薩南無得大勢菩薩南無無盡意菩薩南無寶曇華菩薩南無藥王菩薩南無藥上菩薩南無彌勒菩薩南無消灾障菩薩南無增福壽菩薩南無樂音樹下三萬六千菩薩南無阿難尊者八千比丘諸大聖僧南無救脫菩薩禮諸佛已。",
        "from": "X74n1484_001:0572c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次復懺悔。",
        "from": "X74n1484_001:0573a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣念像法轉時。",
        "from": "X74n1484_001:0573a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若諸有情。",
        "from": "X74n1484_001:0573a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲利益安樂拔除一切業鄣。",
        "from": "X74n1484_001:0573a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建立殊勝功德者為欲滿修諸佛大願。",
        "from": "X74n1484_001:0573a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足受持諸佛名號。",
        "from": "X74n1484_001:0573a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及正法寶藏者。",
        "from": "X74n1484_001:0573a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲得阿耨多羅三藐三菩提。",
        "from": "X74n1484_001:0573a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十二相。",
        "from": "X74n1484_001:0573a06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八十隨形。",
        "from": "X74n1484_001:0573a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莊嚴其身者。",
        "from": "X74n1484_001:0573a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲得無量智慧方便。",
        "from": "X74n1484_001:0573a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸眾生安立大乘者。",
        "from": "X74n1484_001:0573a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲修行梵行。",
        "from": "X74n1484_001:0573a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得不缺戒。",
        "from": "X74n1484_001:0573a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具三聚戒。",
        "from": "X74n1484_001:0573a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "究竟清淨。",
        "from": "X74n1484_001:0573a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有毀犯者。",
        "from": "X74n1484_001:0573a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲得諸根完具。",
        "from": "X74n1484_001:0573a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無諸疾苦。",
        "from": "X74n1484_001:0573a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "家屬資具。",
        "from": "X74n1484_001:0573a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆豐足者。",
        "from": "X74n1484_001:0573a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲得破魔罥網。",
        "from": "X74n1484_001:0573a10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解脫一切外道纏縛拔種種惡見稠林者。",
        "from": "X74n1484_001:0573a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有女人。",
        "from": "X74n1484_001:0573a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲轉女成男。",
        "from": "X74n1484_001:0573a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具丈夫相者。",
        "from": "X74n1484_001:0573a11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲求男女而得男女。",
        "from": "X74n1484_001:0573a11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求長壽而得長壽者。",
        "from": "X74n1484_001:0573a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或當王法所加。",
        "from": "X74n1484_001:0573a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲解脫一切憂苦者。",
        "from": "X74n1484_001:0573a13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲得上妙飲食。",
        "from": "X74n1484_001:0573a13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飽足法味。",
        "from": "X74n1484_001:0573a13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及種種上妙衣服。",
        "from": "X74n1484_001:0573a13_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切寶莊嚴具者。",
        "from": "X74n1484_001:0573a14_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲行惠施。",
        "from": "X74n1484_001:0573a14_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切所有悉無貪惜。",
        "from": "X74n1484_001:0573a14_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施來求者。",
        "from": "X74n1484_001:0573a15_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲得正見精進。",
        "from": "X74n1484_001:0573a15_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善調意樂。",
        "from": "X74n1484_001:0573a15_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多聞聰利。",
        "from": "X74n1484_001:0573a15_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解甚深義。",
        "from": "X74n1484_001:0573a16_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒求勝法常遇善友者。",
        "from": "X74n1484_001:0573a16_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲受持學處。",
        "from": "X74n1484_001:0573a16_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願生西方極樂世界。",
        "from": "X74n1484_001:0573a17_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量壽佛所。",
        "from": "X74n1484_001:0573a17_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聽聞正法者。",
        "from": "X74n1484_001:0573a17_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有已毀之戒。",
        "from": "X74n1484_001:0573a17_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而欲還清淨。",
        "from": "X74n1484_001:0573a18_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已盡之命。",
        "from": "X74n1484_001:0573a18_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而欲復救續。",
        "from": "X74n1484_001:0573a18_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至國界人眾。",
        "from": "X74n1484_001:0573a18_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種諸橫。",
        "from": "X74n1484_001:0573a19_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種諸難。",
        "from": "X74n1484_001:0573a19_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而欲消伏隱沒順時歡樂者。",
        "from": "X74n1484_001:0573a19_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當如法莊嚴。",
        "from": "X74n1484_001:0573a20_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命。",
        "from": "X74n1484_001:0573a20_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥師琉璃光如來。",
        "from": "X74n1484_001:0573a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心精進。",
        "from": "X74n1484_001:0573a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修三摩地。",
        "from": "X74n1484_001:0573a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何。",
        "from": "X74n1484_001:0573a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼如來行菩薩道時。",
        "from": "X74n1484_001:0573a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所發十二微妙上願。",
        "from": "X74n1484_001:0573a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殊勝功德。",
        "from": "X74n1484_001:0573a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸聞者。",
        "from": "X74n1484_001:0573a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "業障消除。",
        "from": "X74n1484_001:0573a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切所求。",
        "from": "X74n1484_001:0573a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆得滿願故。",
        "from": "X74n1484_001:0573a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是唯一生所繫菩薩。",
        "from": "X74n1484_001:0573a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為能如實信解。",
        "from": "X74n1484_001:0573a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如說修行。",
        "from": "X74n1484_001:0573a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當以身命財。",
        "from": "X74n1484_001:0573a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不生恡惜。",
        "from": "X74n1484_001:0573b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竭力莊嚴。",
        "from": "X74n1484_001:0573b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必獲果遂。",
        "from": "X74n1484_001:0573b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故今日。",
        "from": "X74n1484_001:0573b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與現前(眾等)各各志心。",
        "from": "X74n1484_001:0573b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命頂禮。",
        "from": "X74n1484_001:0573b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無毗盧遮那佛南無本師釋迦牟尼佛南無藥師琉璃光如來南無無量壽佛南無盡十方徧法界過去一切諸佛南無盡十方徧法界現在一切諸佛南無盡十方徧法界未來一切諸佛南無藥師琉璃光如來本願功德經南無日光遍照菩薩南無月光遍照菩薩南無文殊師利菩薩南無觀世音菩薩南無得大勢菩薩南無無盡意菩薩南無寶曇華菩薩南無藥王菩薩南無藥上菩薩南無彌勒菩薩南無消灾障菩薩南無增福壽菩薩南無樂音樹下三萬六千菩薩南無阿難尊者八千比丘諸大聖僧南無救脫菩薩禮諸佛已。",
        "from": "X74n1484_001:0573b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "志心歸命。",
        "from": "X74n1484_001:0573b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願諸佛菩薩。",
        "from": "X74n1484_001:0573b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同運慈悲。",
        "from": "X74n1484_001:0573b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降臨道場。",
        "from": "X74n1484_001:0573b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切天龍八部。",
        "from": "X74n1484_001:0573b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥叉諸將。",
        "from": "X74n1484_001:0573b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉生哀愍。",
        "from": "X74n1484_001:0573b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來到道場。",
        "from": "X74n1484_001:0573b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸聖眾。",
        "from": "X74n1484_001:0573b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願悉證明。",
        "from": "X74n1484_001:0573b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我於今日。",
        "from": "X74n1484_001:0573b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲為十方一切六道眾生。",
        "from": "X74n1484_001:0573b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行無上菩提。",
        "from": "X74n1484_001:0573b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷除一切業障。",
        "from": "X74n1484_001:0573b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同入如來本大願海。",
        "from": "X74n1484_001:0573b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普現色身。",
        "from": "X74n1484_001:0573b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一念中。",
        "from": "X74n1484_001:0573b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養一切十方三寶。",
        "from": "X74n1484_001:0573b22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一念中。",
        "from": "X74n1484_001:0573b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普度一切六道眾生。",
        "from": "X74n1484_001:0573b23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令入平等大慧。",
        "from": "X74n1484_001:0573b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故於今日。",
        "from": "X74n1484_001:0573b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心精進。",
        "from": "X74n1484_001:0573b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如說修行。",
        "from": "X74n1484_001:0573b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願諸佛菩薩。",
        "from": "X74n1484_001:0573b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥師如來本願力故。",
        "from": "X74n1484_001:0573c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受我懺悔。",
        "from": "X74n1484_001:0573c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令我所行。",
        "from": "X74n1484_001:0573c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "決定破除罪障。",
        "from": "X74n1484_001:0573c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行願圓成。",
        "from": "X74n1484_001:0573c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如經所說。",
        "from": "X74n1484_001:0573c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願悉證明。",
        "from": "X74n1484_001:0573c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無藥師琉璃光如來。",
        "from": "X74n1484_001:0573c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應正等覺。",
        "from": "X74n1484_001:0573c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明行圓滿。",
        "from": "X74n1484_001:0573c03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光明廣大。",
        "from": "X74n1484_001:0573c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功德巍巍。",
        "from": "X74n1484_001:0573c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身善安住。",
        "from": "X74n1484_001:0573c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燄網莊嚴。",
        "from": "X74n1484_001:0573c04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過於日月。",
        "from": "X74n1484_001:0573c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幽冥眾生。",
        "from": "X74n1484_001:0573c04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉蒙開曉。",
        "from": "X74n1484_001:0573c04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨所樂求。",
        "from": "X74n1484_001:0573c04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切皆遂。",
        "from": "X74n1484_001:0573c05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有病苦。",
        "from": "X74n1484_001:0573c05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆消滅。",
        "from": "X74n1484_001:0573c05_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量菩薩。",
        "from": "X74n1484_001:0573c05_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行無量善巧方便。",
        "from": "X74n1484_001:0573c05_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量廣大願。",
        "from": "X74n1484_001:0573c06_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我若一劫。",
        "from": "X74n1484_001:0573c06_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一劫餘。",
        "from": "X74n1484_001:0573c06_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而廣說者。",
        "from": "X74n1484_001:0573c06_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劫可速盡。",
        "from": "X74n1484_001:0573c07_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼佛行願。",
        "from": "X74n1484_001:0573c07_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善巧方便。",
        "from": "X74n1484_001:0573c07_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有盡也。",
        "from": "X74n1484_001:0573c07_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故志心。",
        "from": "X74n1484_001:0573c07_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命頂禮。",
        "from": "X74n1484_001:0573c08_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無薄伽伐帝。",
        "from": "X74n1484_001:0573c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鞞殺社。",
        "from": "X74n1484_001:0573c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "窶嚕薜琉璃。",
        "from": "X74n1484_001:0573c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鉢囉婆。",
        "from": "X74n1484_001:0573c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喝喇闍也。",
        "from": "X74n1484_001:0573c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛他揭哆耶。",
        "from": "X74n1484_001:0573c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿囉訶帝。",
        "from": "X74n1484_001:0573c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藐三勃陀耶。",
        "from": "X74n1484_001:0573c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛姪他。",
        "from": "X74n1484_001:0573c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵。",
        "from": "X74n1484_001:0573c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鞞殺逝。",
        "from": "X74n1484_001:0573c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鞞殺逝。",
        "from": "X74n1484_001:0573c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鞞殺社。",
        "from": "X74n1484_001:0573c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三沒揭帝莎訶讚藥師如來。",
        "from": "X74n1484_001:0573c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世界琉璃。",
        "from": "X74n1484_001:0573c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈悲喜捨悉隨宜。",
        "from": "X74n1484_001:0573c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懺法度羣迷。",
        "from": "X74n1484_001:0573c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稽首歸依。",
        "from": "X74n1484_001:0573c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所願滿求祈。",
        "from": "X74n1484_001:0573c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥師佛藥師佛消灾延壽藥師佛",
        "from": "X74n1484_001:0573c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無消灾延壽藥師佛",
        "from": "X74n1484_001:0573c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(遶壇起佛偈念佛迴向)。",
        "from": "X74n1484_001:0573c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　稽首東方滿月界　　微妙正覺藥師尊　三祇果滿不思議　　十號名稱無等倫　二六願門因地發　　百千相好果中圓　慈悲海闊測難量　　功德山高讚莫盡南無東方淨琉璃世界藥師琉璃光如來南無消灾延壽藥師佛(或百聲千聲隨意迴向)消灾延壽藥師懺法卷上",
        "from": "X74n1484_001:0573c18_0",
        "context_with_next_sentence": false
    }
]