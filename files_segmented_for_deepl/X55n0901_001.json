[
    {
        "source_sentence": "六離合釋法式略解",
        "from": "X55n0901_001:0487a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蕅益智旭略解",
        "from": "X55n0901_001:0487a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方釋名。",
        "from": "X55n0901_001:0487a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有其六種。",
        "from": "X55n0901_001:0487a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一依主。",
        "from": "X55n0901_001:0487a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二持業。",
        "from": "X55n0901_001:0487a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三有財。",
        "from": "X55n0901_001:0487a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四相違。",
        "from": "X55n0901_001:0487a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五帶數。",
        "from": "X55n0901_001:0487a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六隣近。",
        "from": "X55n0901_001:0487a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此六種有離合故。",
        "from": "X55n0901_001:0487a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一具二。",
        "from": "X55n0901_001:0487a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若單一字名。",
        "from": "X55n0901_001:0487a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即非六釋。",
        "from": "X55n0901_001:0487a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以不得成離合相故。",
        "from": "X55n0901_001:0487a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已上總標。",
        "from": "X55n0901_001:0487a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已下別釋。",
        "from": "X55n0901_001:0487a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即為六也。",
        "from": "X55n0901_001:0487a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初依主者。",
        "from": "X55n0901_001:0487a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂所依為主。",
        "from": "X55n0901_001:0487a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如說眼識識依眼起。",
        "from": "X55n0901_001:0487a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即眼之識。",
        "from": "X55n0901_001:0487a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名眼識舉眼之主以表於識也。",
        "from": "X55n0901_001:0487a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名依士釋。",
        "from": "X55n0901_001:0487a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此即分取他名如眼識亦名為色",
        "from": "X55n0901_001:0487a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識耳識亦名為聲識等。",
        "from": "X55n0901_001:0487a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如子取父名名為依主。",
        "from": "X55n0901_001:0487a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父取子名。",
        "from": "X55n0901_001:0487a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即名依士。",
        "from": "X55n0901_001:0487a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所依劣故。",
        "from": "X55n0901_001:0487a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識如子。",
        "from": "X55n0901_001:0487b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "根如父。",
        "from": "X55n0901_001:0487b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故眼識等名為依主。",
        "from": "X55n0901_001:0487b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識如父。",
        "from": "X55n0901_001:0487b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塵如子。",
        "from": "X55n0901_001:0487b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故色識等名為依士。",
        "from": "X55n0901_001:0487b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓋主勝而士劣也。",
        "from": "X55n0901_001:0487b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘可知。",
        "from": "X55n0901_001:0487b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言離合。",
        "from": "X55n0901_001:0487b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相者。",
        "from": "X55n0901_001:0487b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離。",
        "from": "X55n0901_001:0487b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂眼者是根。",
        "from": "X55n0901_001:0487b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識者了別。",
        "from": "X55n0901_001:0487b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合。",
        "from": "X55n0901_001:0487b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂此二合名眼識。",
        "from": "X55n0901_001:0487b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘五離合。",
        "from": "X55n0901_001:0487b06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "準此應知。",
        "from": "X55n0901_001:0487b07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘五指下五釋皆有離合也。",
        "from": "X55n0901_001:0487b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘可知。",
        "from": "X55n0901_001:0487b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一依主釋竟。",
        "from": "X55n0901_001:0487b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言持業者如說藏識。",
        "from": "X55n0901_001:0487b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識者是體。",
        "from": "X55n0901_001:0487b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藏是業用。",
        "from": "X55n0901_001:0487b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用能顯體。",
        "from": "X55n0901_001:0487b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體能持業。",
        "from": "X55n0901_001:0487b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藏即識故。",
        "from": "X55n0901_001:0487b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為藏識。",
        "from": "X55n0901_001:0487b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名持業。",
        "from": "X55n0901_001:0487b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離之則藏是能藏所藏執藏之",
        "from": "X55n0901_001:0487b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義。",
        "from": "X55n0901_001:0487b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識是了別之義。",
        "from": "X55n0901_001:0487b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合之則識為體藏為用。",
        "from": "X55n0901_001:0487b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以用顯體。",
        "from": "X55n0901_001:0487b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體持于用。",
        "from": "X55n0901_001:0487b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名持業也。",
        "from": "X55n0901_001:0487b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名同依釋。",
        "from": "X55n0901_001:0487b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藏取含藏用。",
        "from": "X55n0901_001:0487b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識取了別用。",
        "from": "X55n0901_001:0487b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此二同一所依。",
        "from": "X55n0901_001:0487b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名同依也。",
        "from": "X55n0901_001:0487b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此以藏識二字俱約用釋。",
        "from": "X55n0901_001:0487c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而同依于第八心王之體。",
        "from": "X55n0901_001:0487c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故亦得名同依釋也。",
        "from": "X55n0901_001:0487c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二持業釋竟。",
        "from": "X55n0901_001:0487c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言有財者。",
        "from": "X55n0901_001:0487c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂從所有以得其名。",
        "from": "X55n0901_001:0487c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一如佛陀。",
        "from": "X55n0901_001:0487c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此云覺者。",
        "from": "X55n0901_001:0487c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即有覺之者。",
        "from": "X55n0901_001:0487c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為覺者。",
        "from": "X55n0901_001:0487c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此即分取他名。",
        "from": "X55n0901_001:0487c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離之。",
        "from": "X55n0901_001:0487c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則者字指人。",
        "from": "X55n0901_001:0487c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覺字指法合之。",
        "from": "X55n0901_001:0487c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則者字正指能有之人。",
        "from": "X55n0901_001:0487c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名之為自。",
        "from": "X55n0901_001:0487c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覺字乃指所有之財。",
        "from": "X55n0901_001:0487c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名之為他。",
        "from": "X55n0901_001:0487c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故為分取他名。",
        "from": "X55n0901_001:0487c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二如俱舍。",
        "from": "X55n0901_001:0487c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非對法藏。",
        "from": "X55n0901_001:0487c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對法藏者。",
        "from": "X55n0901_001:0487c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是本論名。",
        "from": "X55n0901_001:0487c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為依根本對法藏造。",
        "from": "X55n0901_001:0487c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故此亦名為對法藏論。",
        "from": "X55n0901_001:0487c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此全取他名。",
        "from": "X55n0901_001:0487c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名有財釋。",
        "from": "X55n0901_001:0487c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱舍論是自。",
        "from": "X55n0901_001:0487c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對法藏是他。",
        "from": "X55n0901_001:0487c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依對法藏之他。",
        "from": "X55n0901_001:0487c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造于俱舍論之自。",
        "from": "X55n0901_001:0487c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即名俱舍論。",
        "from": "X55n0901_001:0487c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為對法藏論。",
        "from": "X55n0901_001:0487c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此俱舍論。",
        "from": "X55n0901_001:0487c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能有對法藏之財。",
        "from": "X55n0901_001:0487c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是全取他以彰名也。",
        "from": "X55n0901_001:0487c18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三有財釋竟。",
        "from": "X55n0901_001:0488a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言相違者。",
        "from": "X55n0901_001:0488a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如說眼及耳等。",
        "from": "X55n0901_001:0488a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各別所詮。",
        "from": "X55n0901_001:0488a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆自為主。",
        "from": "X55n0901_001:0488a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不相隨順。",
        "from": "X55n0901_001:0488a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故曰。",
        "from": "X55n0901_001:0488a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相違。",
        "from": "X55n0901_001:0488a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為有及與二言。",
        "from": "X55n0901_001:0488a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非前二釋。",
        "from": "X55n0901_001:0488a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義通帶數有財。",
        "from": "X55n0901_001:0488a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若直云眼耳。",
        "from": "X55n0901_001:0488a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則離之各詮一物。",
        "from": "X55n0901_001:0488a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合之互不相攝。",
        "from": "X55n0901_001:0488a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名相違。",
        "from": "X55n0901_001:0488a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若言眼及耳等。",
        "from": "X55n0901_001:0488a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或言眼與耳等。",
        "from": "X55n0901_001:0488a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既有及字與字。",
        "from": "X55n0901_001:0488a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或借此以顯數。",
        "from": "X55n0901_001:0488a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即通帶數。",
        "from": "X55n0901_001:0488a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或以及與二字表於能有。",
        "from": "X55n0901_001:0488a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼耳等字表於所有。",
        "from": "X55n0901_001:0488a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即通有財。",
        "from": "X55n0901_001:0488a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然決非前依主持業之二釋也。",
        "from": "X55n0901_001:0488a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四相違釋竟。",
        "from": "X55n0901_001:0488a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言帶數者。",
        "from": "X55n0901_001:0488a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以數顯義。",
        "from": "X55n0901_001:0488a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通於三釋。",
        "from": "X55n0901_001:0488a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如五蘊二諦等。",
        "from": "X55n0901_001:0488a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五即是蘊。",
        "from": "X55n0901_001:0488a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二即是諦。",
        "from": "X55n0901_001:0488a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此用自為名。",
        "from": "X55n0901_001:0488a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即持業帶數。",
        "from": "X55n0901_001:0488a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五二皆數也。",
        "from": "X55n0901_001:0488a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘊以積聚為義諦以審實為義。",
        "from": "X55n0901_001:0488a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五皆積聚。",
        "from": "X55n0901_001:0488a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二皆審實。",
        "from": "X55n0901_001:0488a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故是持業帶數。",
        "from": "X55n0901_001:0488b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如眼等六識。",
        "from": "X55n0901_001:0488b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取自他為名。",
        "from": "X55n0901_001:0488b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即依主帶數。",
        "from": "X55n0901_001:0488b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識是能依之自。",
        "from": "X55n0901_001:0488b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼等是所依之他。",
        "from": "X55n0901_001:0488b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六則是數。",
        "from": "X55n0901_001:0488b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名依主帶數。",
        "from": "X55n0901_001:0488b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如說五逆為五無間。",
        "from": "X55n0901_001:0488b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無間是果。",
        "from": "X55n0901_001:0488b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即因談果。",
        "from": "X55n0901_001:0488b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此全取他名。",
        "from": "X55n0901_001:0488b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即有財帶數。",
        "from": "X55n0901_001:0488b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殺父等五逆罪。",
        "from": "X55n0901_001:0488b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃是無間之因。",
        "from": "X55n0901_001:0488b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無間地獄劇苦。",
        "from": "X55n0901_001:0488b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃是五逆之果。",
        "from": "X55n0901_001:0488b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今說五逆為五無間。",
        "from": "X55n0901_001:0488b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則是以果名因。",
        "from": "X55n0901_001:0488b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即因談果逆罪是能有之因。",
        "from": "X55n0901_001:0488b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為自。",
        "from": "X55n0901_001:0488b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無間是所有之財。",
        "from": "X55n0901_001:0488b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為他。",
        "from": "X55n0901_001:0488b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五則是數。",
        "from": "X55n0901_001:0488b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名有財帶數。",
        "from": "X55n0901_001:0488b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五帶數釋竟。",
        "from": "X55n0901_001:0488b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言隣近者。",
        "from": "X55n0901_001:0488b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從近為名。",
        "from": "X55n0901_001:0488b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如四念住。",
        "from": "X55n0901_001:0488b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以慧為體。",
        "from": "X55n0901_001:0488b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以慧近念。",
        "from": "X55n0901_001:0488b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名念住。",
        "from": "X55n0901_001:0488b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此先舉例。",
        "from": "X55n0901_001:0488b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以明隣近釋也。",
        "from": "X55n0901_001:0488b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四念住。",
        "from": "X55n0901_001:0488b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即四念處。",
        "from": "X55n0901_001:0488b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本是觀慧為體。",
        "from": "X55n0901_001:0488b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而念之與慧。",
        "from": "X55n0901_001:0488b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆是別境心所。",
        "from": "X55n0901_001:0488c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相隣近故。",
        "from": "X55n0901_001:0488c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為念住。",
        "from": "X55n0901_001:0488c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故是隣近釋也。",
        "from": "X55n0901_001:0488c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既是隣近。",
        "from": "X55n0901_001:0488c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不同自為名。",
        "from": "X55n0901_001:0488c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無持業義。",
        "from": "X55n0901_001:0488c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通餘二釋。",
        "from": "X55n0901_001:0488c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以自為名。",
        "from": "X55n0901_001:0488c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃有持業之義。",
        "from": "X55n0901_001:0488c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今既取隣近為名。",
        "from": "X55n0901_001:0488c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故不可作持業釋。",
        "from": "X55n0901_001:0488c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但通依主有財二釋也。",
        "from": "X55n0901_001:0488c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一依主隣近。",
        "from": "X55n0901_001:0488c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如有人近長安住有人問言。",
        "from": "X55n0901_001:0488c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為何處住。",
        "from": "X55n0901_001:0488c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答云。",
        "from": "X55n0901_001:0488c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長安住。",
        "from": "X55n0901_001:0488c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此人實非長安住。",
        "from": "X55n0901_001:0488c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以近長安。",
        "from": "X55n0901_001:0488c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故云長安住。",
        "from": "X55n0901_001:0488c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以分取他長安之名。",
        "from": "X55n0901_001:0488c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復是依主隣近。",
        "from": "X55n0901_001:0488c11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長安。",
        "from": "X55n0901_001:0488c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是所依之他。",
        "from": "X55n0901_001:0488c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住者。",
        "from": "X55n0901_001:0488c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是能依之自。",
        "from": "X55n0901_001:0488c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所依勝故。",
        "from": "X55n0901_001:0488c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為依主。",
        "from": "X55n0901_001:0488c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而近於長安。",
        "from": "X55n0901_001:0488c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即名為長安住。",
        "from": "X55n0901_001:0488c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故是依主隣近也。",
        "from": "X55n0901_001:0488c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二有財隣近。",
        "from": "X55n0901_001:0488c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如問何處人。",
        "from": "X55n0901_001:0488c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "X55n0901_001:0488c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長安。",
        "from": "X55n0901_001:0488c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以全取他處以標人名。",
        "from": "X55n0901_001:0488c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是有財。",
        "from": "X55n0901_001:0488c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以近長安。",
        "from": "X55n0901_001:0489a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復名隣近。",
        "from": "X55n0901_001:0489a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人是能有。",
        "from": "X55n0901_001:0489a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正報長安是所有依報。",
        "from": "X55n0901_001:0489a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故是有財。",
        "from": "X55n0901_001:0489a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "近長安即名為長安。",
        "from": "X55n0901_001:0489a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故是有財隣近也。",
        "from": "X55n0901_001:0489a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六隣近釋竟。",
        "from": "X55n0901_001:0489a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌曰。",
        "from": "X55n0901_001:0489a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用自。",
        "from": "X55n0901_001:0489a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及用他。",
        "from": "X55n0901_001:0489a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自他用。",
        "from": "X55n0901_001:0489a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱非。",
        "from": "X55n0901_001:0489a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通二通三種。",
        "from": "X55n0901_001:0489a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是六種釋。",
        "from": "X55n0901_001:0489a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持業釋用自。",
        "from": "X55n0901_001:0489a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有財釋用他。",
        "from": "X55n0901_001:0489a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依主釋自他雙用。",
        "from": "X55n0901_001:0489a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相違釋自他俱非。",
        "from": "X55n0901_001:0489a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隣近釋通於二種。",
        "from": "X55n0901_001:0489a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂依主隣近。",
        "from": "X55n0901_001:0489a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有財隣近。",
        "from": "X55n0901_001:0489a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶數釋通於三種。",
        "from": "X55n0901_001:0489a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂依主帶數。",
        "from": "X55n0901_001:0489a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有財帶數。",
        "from": "X55n0901_001:0489a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持業帶數也。",
        "from": "X55n0901_001:0489a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六離合釋法式略解(終)",
        "from": "X55n0901_001:0489a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寬文十二年(壬子)正月吉辰",
        "from": "X55n0901_001:0489b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二條通二王門町",
        "from": "X55n0901_001:0489b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長尾平兵衛雕梓",
        "from": "X55n0901_001:0489b03_0",
        "context_with_next_sentence": false
    }
]