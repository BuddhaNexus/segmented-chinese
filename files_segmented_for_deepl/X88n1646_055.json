[
    {
        "source_sentence": "居士傳五十五",
        "from": "X88n1646_055:0289a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周安士傳",
        "from": "X88n1646_055:0289a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周安士。",
        "from": "X88n1646_055:0289a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名夢顏。",
        "from": "X88n1646_055:0289a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名思仁。",
        "from": "X88n1646_055:0289a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "崑山諸生也。",
        "from": "X88n1646_055:0289a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "博通經藏。",
        "from": "X88n1646_055:0289a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深信因果。",
        "from": "X88n1646_055:0289a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居常念言。",
        "from": "X88n1646_055:0289a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生蚩蚩造無量罪。",
        "from": "X88n1646_055:0289a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婬殺二業實居大半。",
        "from": "X88n1646_055:0289a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "古今世典懲勸頗詳。",
        "from": "X88n1646_055:0289a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不斷其源川壅則潰。",
        "from": "X88n1646_055:0289a14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爰竭苦衷。",
        "from": "X88n1646_055:0289a14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深維經義。",
        "from": "X88n1646_055:0289a15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "抉微洞幽。",
        "from": "X88n1646_055:0289a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拔山搗穴。",
        "from": "X88n1646_055:0289a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勒成二書。",
        "from": "X88n1646_055:0289a15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戒淫與殺。",
        "from": "X88n1646_055:0289a15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其戒殺書言多深痛。",
        "from": "X88n1646_055:0289a16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大要云。",
        "from": "X88n1646_055:0289a16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀兵之難。",
        "from": "X88n1646_055:0289a16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在於人道。",
        "from": "X88n1646_055:0289a16_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或數十年。",
        "from": "X88n1646_055:0289a17_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或百餘年。",
        "from": "X88n1646_055:0289a17_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僅一二見。",
        "from": "X88n1646_055:0289a17_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至於畜生。",
        "from": "X88n1646_055:0289a17_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無日得脫。",
        "from": "X88n1646_055:0289a17_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普天之下一遇雞鳴。",
        "from": "X88n1646_055:0289a18_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量無邊狠心屠戶。",
        "from": "X88n1646_055:0289a18_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手執利刀奮向羣豕。",
        "from": "X88n1646_055:0289a18_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時羣豕自知難到。",
        "from": "X88n1646_055:0289a19_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大聲哀號救援不至。",
        "from": "X88n1646_055:0289a19_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各被彼人裂腹刺心抽腸拔肺。",
        "from": "X88n1646_055:0289a20_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "哀號未斷。",
        "from": "X88n1646_055:0289a20_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又投沸湯受大苦惱。",
        "from": "X88n1646_055:0289a20_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "片刻之間。",
        "from": "X88n1646_055:0289a21_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "閻浮世界萬萬生靈頭足異處。",
        "from": "X88n1646_055:0289a21_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "積羣豕骨過高山巔。",
        "from": "X88n1646_055:0289a22_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漂羣豕血赤江水流。",
        "from": "X88n1646_055:0289a22_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是惡因如是惡果。",
        "from": "X88n1646_055:0289a22_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詰其根由皆為我等口腹所致。",
        "from": "X88n1646_055:0289a23_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世人動稱我不作惡何必持齋。",
        "from": "X88n1646_055:0289a23_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈知爾等偃息在牀。",
        "from": "X88n1646_055:0289a24_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妻孥聚首。",
        "from": "X88n1646_055:0289a24_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即有素不相識之人先。",
        "from": "X88n1646_055:0289b01_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為汝等徧造惡業無量無邊。",
        "from": "X88n1646_055:0289b01_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛菩薩神通天眼。",
        "from": "X88n1646_055:0289b02_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見諸眾生輪迴六趣。",
        "from": "X88n1646_055:0289b02_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如旋火輪。",
        "from": "X88n1646_055:0289b02_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或為大身更相殺食。",
        "from": "X88n1646_055:0289b03_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或為細身更相殺食。",
        "from": "X88n1646_055:0289b03_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或為父母六親眷屬更相殺食。",
        "from": "X88n1646_055:0289b03_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發大悲心說真實語。",
        "from": "X88n1646_055:0289b04_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "導彼迷流開爾覺路。",
        "from": "X88n1646_055:0289b04_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我勸世人未能持齋先須斷殺。",
        "from": "X88n1646_055:0289b05_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孟子有言。",
        "from": "X88n1646_055:0289b05_54",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仁者人也。",
        "from": "X88n1646_055:0289b06_55",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人不信佛。",
        "from": "X88n1646_055:0289b06_56",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何不信人。",
        "from": "X88n1646_055:0289b06_57",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若信是人。",
        "from": "X88n1646_055:0289b06_58",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何不斷殺。",
        "from": "X88n1646_055:0289b06_59",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仁則不殺。",
        "from": "X88n1646_055:0289b07_60",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殺則非仁。",
        "from": "X88n1646_055:0289b07_61",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諦聽吾言。",
        "from": "X88n1646_055:0289b07_62",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "免入異類。",
        "from": "X88n1646_055:0289b07_63",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安士每過一切神祠。",
        "from": "X88n1646_055:0289b07_64",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必祝願云。",
        "from": "X88n1646_055:0289b08_65",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願尊神。",
        "from": "X88n1646_055:0289b08_66",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發出世心。",
        "from": "X88n1646_055:0289b08_67",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿受血食。",
        "from": "X88n1646_055:0289b08_68",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心常念阿彌陀佛。",
        "from": "X88n1646_055:0289b09_69",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求生淨土。",
        "from": "X88n1646_055:0289b09_70",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思仁自今二十四歲。",
        "from": "X88n1646_055:0289b09_71",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直至壽盡中間。",
        "from": "X88n1646_055:0289b10_72",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若殺一小魚蝦。",
        "from": "X88n1646_055:0289b10_73",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願尊神是糾是殛。",
        "from": "X88n1646_055:0289b10_74",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迅雷擊碎所著書板。",
        "from": "X88n1646_055:0289b11_75",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思仁自今二十四歲。",
        "from": "X88n1646_055:0289b11_76",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直至壽盡。",
        "from": "X88n1646_055:0289b11_77",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "家中眷屬若有一人傷一蚊蟻。",
        "from": "X88n1646_055:0289b12_78",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦同此誓。",
        "from": "X88n1646_055:0289b12_79",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思仁自今二十四歲。",
        "from": "X88n1646_055:0289b12_80",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直至壽盡。",
        "from": "X88n1646_055:0289b13_81",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中間若遇身及眷屬嬰諸重病。",
        "from": "X88n1646_055:0289b13_82",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忽一仙人來告之言。",
        "from": "X88n1646_055:0289b14_83",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "投一小蛤於沸湯中。",
        "from": "X88n1646_055:0289b14_84",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲之則生不然則死。",
        "from": "X88n1646_055:0289b14_85",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以貪生故竟從其言。",
        "from": "X88n1646_055:0289b15_86",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦同此誓。",
        "from": "X88n1646_055:0289b15_87",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思仁自今二十四歲。",
        "from": "X88n1646_055:0289b15_88",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直至壽盡。",
        "from": "X88n1646_055:0289b16_89",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倘有毒蛇嚙我垂斃。",
        "from": "X88n1646_055:0289b16_90",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "家眾憤憤欲殺此蛇。",
        "from": "X88n1646_055:0289b16_91",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而我不能極力救阻。",
        "from": "X88n1646_055:0289b17_92",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦同此誓。",
        "from": "X88n1646_055:0289b17_93",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思仁自今二十四歲。",
        "from": "X88n1646_055:0289b17_94",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直至壽盡。",
        "from": "X88n1646_055:0289b18_95",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨河見魚仰面見鳥。",
        "from": "X88n1646_055:0289b18_96",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不思救度反萌殺機。",
        "from": "X88n1646_055:0289b18_97",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦同此誓。",
        "from": "X88n1646_055:0289b19_98",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思仁自今二十四歲。",
        "from": "X88n1646_055:0289b19_99",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直至壽盡。",
        "from": "X88n1646_055:0289b19_100",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若夢寐中見人殺生。",
        "from": "X88n1646_055:0289b20_101",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能至心稱佛名號。",
        "from": "X88n1646_055:0289b20_102",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發救度心。",
        "from": "X88n1646_055:0289b20_103",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而反歡喜贊成其事。",
        "from": "X88n1646_055:0289b21_104",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦同此誓。",
        "from": "X88n1646_055:0289b21_105",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其戒淫書勸諸淫者。",
        "from": "X88n1646_055:0289b21_106",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先觀胎獄了種種苦。",
        "from": "X88n1646_055:0289b22_107",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是謂息淫原始方便。",
        "from": "X88n1646_055:0289b22_108",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫以偈曰。",
        "from": "X88n1646_055:0289b22_109",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父母未生時。",
        "from": "X88n1646_055:0289b23_110",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因業多掛帶。",
        "from": "X88n1646_055:0289b23_111",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一點無明根。",
        "from": "X88n1646_055:0289b23_112",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見慾生貪愛。",
        "from": "X88n1646_055:0289b23_113",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淫心纔一起。",
        "from": "X88n1646_055:0289b24_114",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忽被精血盖。",
        "from": "X88n1646_055:0289b24_115",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從此十月中。",
        "from": "X88n1646_055:0289b24_116",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次第還宿債。",
        "from": "X88n1646_055:0289b24_117",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母噉熱食時。",
        "from": "X88n1646_055:0289c01_118",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如受濩湯溉。",
        "from": "X88n1646_055:0289c01_119",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母飲冷水時。",
        "from": "X88n1646_055:0289c01_120",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若臥寒氷塊。",
        "from": "X88n1646_055:0289c01_121",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "腹中當飽滿。",
        "from": "X88n1646_055:0289c02_122",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂上懸鐵袋。",
        "from": "X88n1646_055:0289c02_123",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五內如飢虗。",
        "from": "X88n1646_055:0289c02_124",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空洞失依賴。",
        "from": "X88n1646_055:0289c02_125",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "會當出胎時。",
        "from": "X88n1646_055:0289c03_126",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翻身若旋岱。",
        "from": "X88n1646_055:0289c03_127",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "產母忽瞑眩。",
        "from": "X88n1646_055:0289c03_128",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眷屬盡惶駭。",
        "from": "X88n1646_055:0289c03_129",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身逼兩山中。",
        "from": "X88n1646_055:0289c04_130",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "熱窄不可耐。",
        "from": "X88n1646_055:0289c04_131",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "穩婆牽其頭。",
        "from": "X88n1646_055:0289c04_132",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "痛極心肝碎。",
        "from": "X88n1646_055:0289c04_133",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嫩頏扼麤手。",
        "from": "X88n1646_055:0289c05_134",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀劒無能賽。",
        "from": "X88n1646_055:0289c05_135",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放聲鳴其哀。",
        "from": "X88n1646_055:0289c05_136",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旁人乃稱快。",
        "from": "X88n1646_055:0289c05_137",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈知稱快人。",
        "from": "X88n1646_055:0289c06_138",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經過忘悔艾。",
        "from": "X88n1646_055:0289c06_139",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昧昧極思量。",
        "from": "X88n1646_055:0289c06_140",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咄咄咤災怪。",
        "from": "X88n1646_055:0289c06_141",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此言非臆說。",
        "from": "X88n1646_055:0289c07_142",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五王經所載。",
        "from": "X88n1646_055:0289c07_143",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡夫戀慾樂。",
        "from": "X88n1646_055:0289c07_144",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輾轉沉黑穢。",
        "from": "X88n1646_055:0289c07_145",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生生受煩惱。",
        "from": "X88n1646_055:0289c08_146",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曾見何人代。",
        "from": "X88n1646_055:0289c08_147",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲破煩惱軍。",
        "from": "X88n1646_055:0289c08_148",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智者擒其帥。",
        "from": "X88n1646_055:0289c08_149",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "主帥是淫魔。",
        "from": "X88n1646_055:0289c09_150",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魔死餘兵敗。",
        "from": "X88n1646_055:0289c09_151",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我將胎形慘。",
        "from": "X88n1646_055:0289c09_152",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈指陳其概。",
        "from": "X88n1646_055:0289c09_153",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "珍重世間人。",
        "from": "X88n1646_055:0289c10_154",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可一不可再。",
        "from": "X88n1646_055:0289c10_155",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次觀此身。",
        "from": "X88n1646_055:0289c10_156",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸蟲蝟集。",
        "from": "X88n1646_055:0289c10_157",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宛轉遊行。",
        "from": "X88n1646_055:0289c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食人腦髓。",
        "from": "X88n1646_055:0289c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為初開不淨方便。",
        "from": "X88n1646_055:0289c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫以偈曰。",
        "from": "X88n1646_055:0289c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "穢哉血肉軀。",
        "from": "X88n1646_055:0289c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量蟲所萃。",
        "from": "X88n1646_055:0289c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "潛伏四大中。",
        "from": "X88n1646_055:0289c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其形甚微細。",
        "from": "X88n1646_055:0289c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡夫不覺知。",
        "from": "X88n1646_055:0289c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天眼方能覷。",
        "from": "X88n1646_055:0289c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "靜氣一返觀。",
        "from": "X88n1646_055:0289c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "男女真兒戲。",
        "from": "X88n1646_055:0289c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次觀男女。",
        "from": "X88n1646_055:0289c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "膿血洟唾惡露中滿。",
        "from": "X88n1646_055:0289c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如溷廁糞穢所。",
        "from": "X88n1646_055:0289c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "都是為息淫對治方便。",
        "from": "X88n1646_055:0289c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫以偈曰。",
        "from": "X88n1646_055:0289c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間苦惱事。",
        "from": "X88n1646_055:0289c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆從愛慾生。",
        "from": "X88n1646_055:0289c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛心不能斷。",
        "from": "X88n1646_055:0289c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對色形神傾。",
        "from": "X88n1646_055:0289c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水從骨內出。",
        "from": "X88n1646_055:0289c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火自心中騰。",
        "from": "X88n1646_055:0289c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平時顧禮義。",
        "from": "X88n1646_055:0289c17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或復愛聲名。",
        "from": "X88n1646_055:0289c17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到此霎時間。",
        "from": "X88n1646_055:0289c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開眼投深阬。",
        "from": "X88n1646_055:0289c17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼神且不畏。",
        "from": "X88n1646_055:0289c18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名教豈能繩。",
        "from": "X88n1646_055:0289c18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "茫茫五濁世。",
        "from": "X88n1646_055:0289c18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獄訟滋繁興。",
        "from": "X88n1646_055:0289c18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幸有如來教。",
        "from": "X88n1646_055:0289c19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "暗室懸明燈。",
        "from": "X88n1646_055:0289c19_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "示以不淨觀。",
        "from": "X88n1646_055:0289c19_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慾火消為氷。",
        "from": "X88n1646_055:0289c19_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我從昔年來。",
        "from": "X88n1646_055:0289c20_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦復似摩登。",
        "from": "X88n1646_055:0289c20_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多方用強制。",
        "from": "X88n1646_055:0289c20_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對境還復萌。",
        "from": "X88n1646_055:0289c20_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自作此觀後。",
        "from": "X88n1646_055:0289c21_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結習日以輕。",
        "from": "X88n1646_055:0289c21_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今者稍覺悟。",
        "from": "X88n1646_055:0289c21_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命佛法僧。",
        "from": "X88n1646_055:0289c21_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誓於未來世。",
        "from": "X88n1646_055:0289c22_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "埽盡羣魔兵。",
        "from": "X88n1646_055:0289c22_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分身無量剎。",
        "from": "X88n1646_055:0289c22_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普利諸有情。",
        "from": "X88n1646_055:0289c22_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "氷菴張夫子誨我。",
        "from": "X88n1646_055:0289c23_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嘗叮嚀其言修觀法。",
        "from": "X88n1646_055:0289c23_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬諸學庖丁。",
        "from": "X88n1646_055:0289c23_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庖丁技成後。",
        "from": "X88n1646_055:0289c24_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不見全牛形。",
        "from": "X88n1646_055:0289c24_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用刀十九載。",
        "from": "X88n1646_055:0289c24_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如新發硎。",
        "from": "X88n1646_055:0289c24_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我此降魔劒。",
        "from": "X88n1646_055:0290a01_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百鍊剛所成。",
        "from": "X88n1646_055:0290a01_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斬盡妖容鬼。",
        "from": "X88n1646_055:0290a01_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髑髏莽縱橫。",
        "from": "X88n1646_055:0290a01_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西施接膝坐。",
        "from": "X88n1646_055:0290a02_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蔓草纏紫荊。",
        "from": "X88n1646_055:0290a02_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十六種物。",
        "from": "X88n1646_055:0290a02_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到鼻知羶腥。",
        "from": "X88n1646_055:0290a02_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此觀得成就。",
        "from": "X88n1646_055:0290a03_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬象都空明。",
        "from": "X88n1646_055:0290a03_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爰述為俚句。",
        "from": "X88n1646_055:0290a03_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞者應惕驚。",
        "from": "X88n1646_055:0290a03_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "齊心出苦海。",
        "from": "X88n1646_055:0290a04_54",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "濁世皎然清。",
        "from": "X88n1646_055:0290a04_55",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次設九想想。",
        "from": "X88n1646_055:0290a04_56",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初死人正直仰臥。",
        "from": "X88n1646_055:0290a04_57",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寒氣徹骨。",
        "from": "X88n1646_055:0290a05_58",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一無所知。",
        "from": "X88n1646_055:0290a05_59",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而我此身。",
        "from": "X88n1646_055:0290a05_60",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終亦如是。",
        "from": "X88n1646_055:0290a05_61",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又想死人。",
        "from": "X88n1646_055:0290a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一二日往。",
        "from": "X88n1646_055:0290a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黑氣騰溢。",
        "from": "X88n1646_055:0290a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉成青紫。",
        "from": "X88n1646_055:0290a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甚可怖畏。",
        "from": "X88n1646_055:0290a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而我此身。",
        "from": "X88n1646_055:0290a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終亦如是。",
        "from": "X88n1646_055:0290a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又想死人。",
        "from": "X88n1646_055:0290a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尸停既久。",
        "from": "X88n1646_055:0290a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黃水流出。",
        "from": "X88n1646_055:0290a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臭不可聞。",
        "from": "X88n1646_055:0290a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而我此身。",
        "from": "X88n1646_055:0290a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終亦如是。",
        "from": "X88n1646_055:0290a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又想死人。",
        "from": "X88n1646_055:0290a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徧體生蟲。",
        "from": "X88n1646_055:0290a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處處鑽囓。",
        "from": "X88n1646_055:0290a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "骨節之內。",
        "from": "X88n1646_055:0290a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆如蜂窠。",
        "from": "X88n1646_055:0290a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而我此身。",
        "from": "X88n1646_055:0290a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終亦如是。",
        "from": "X88n1646_055:0290a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又想死人。",
        "from": "X88n1646_055:0290a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皮肉已盡。",
        "from": "X88n1646_055:0290a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟筋在骨。",
        "from": "X88n1646_055:0290a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如繩束薪。",
        "from": "X88n1646_055:0290a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而我此身。",
        "from": "X88n1646_055:0290a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終亦如是。",
        "from": "X88n1646_055:0290a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又想死人。",
        "from": "X88n1646_055:0290a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "筋已爛盡。",
        "from": "X88n1646_055:0290a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "骨節縱橫。",
        "from": "X88n1646_055:0290a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不在一處。",
        "from": "X88n1646_055:0290a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而我此身。",
        "from": "X88n1646_055:0290a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終亦如是。",
        "from": "X88n1646_055:0290a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又想死人。",
        "from": "X88n1646_055:0290a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被火所燒。",
        "from": "X88n1646_055:0290a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "焦縮在地。",
        "from": "X88n1646_055:0290a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或熟或生。",
        "from": "X88n1646_055:0290a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而我此身。",
        "from": "X88n1646_055:0290a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終亦如是。",
        "from": "X88n1646_055:0290a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又想死人。",
        "from": "X88n1646_055:0290a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冡破骨出。",
        "from": "X88n1646_055:0290a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日暴雨侵。",
        "from": "X88n1646_055:0290a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其色轉白。",
        "from": "X88n1646_055:0290a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或復黃朽。",
        "from": "X88n1646_055:0290a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人獸踐踏。",
        "from": "X88n1646_055:0290a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而我此身。",
        "from": "X88n1646_055:0290a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終亦如是。",
        "from": "X88n1646_055:0290a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次想經要。",
        "from": "X88n1646_055:0290a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深自策發。",
        "from": "X88n1646_055:0290a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經言。",
        "from": "X88n1646_055:0290a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩修於死想。",
        "from": "X88n1646_055:0290a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀是壽命常為無量冤讎所繞。",
        "from": "X88n1646_055:0290a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念念損滅無有增長。",
        "from": "X88n1646_055:0290a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶山瀑水不得停住。",
        "from": "X88n1646_055:0290a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦如朝露勢不久停。",
        "from": "X88n1646_055:0290a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如囚趣市步步近死。",
        "from": "X88n1646_055:0290a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如牽牛羊詣於屠所。",
        "from": "X88n1646_055:0290a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫以偈曰。",
        "from": "X88n1646_055:0290a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人想死亡日。",
        "from": "X88n1646_055:0290a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慾火頓清凉。",
        "from": "X88n1646_055:0290a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愚人若聞此。",
        "from": "X88n1646_055:0290a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愁眉歎不祥。",
        "from": "X88n1646_055:0290a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "究竟百年後。",
        "from": "X88n1646_055:0290a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同入灰燼場。",
        "from": "X88n1646_055:0290a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩九想觀。",
        "from": "X88n1646_055:0290a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦海大津梁。",
        "from": "X88n1646_055:0290a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次念法華所說因緣。",
        "from": "X88n1646_055:0290a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生相滅相與不生滅。",
        "from": "X88n1646_055:0290a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為斷淫窮源方便。",
        "from": "X88n1646_055:0290a21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫以偈曰。",
        "from": "X88n1646_055:0290a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生畏苦果。",
        "from": "X88n1646_055:0290a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩斷惡因。",
        "from": "X88n1646_055:0290a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡因既已斷。",
        "from": "X88n1646_055:0290a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦果無遺根。",
        "from": "X88n1646_055:0290a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如猛獅子。",
        "from": "X88n1646_055:0290a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利鏃中其身。",
        "from": "X88n1646_055:0290a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "急顧發的處。",
        "from": "X88n1646_055:0290a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獵戶群波奔。",
        "from": "X88n1646_055:0290a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "犬若遭人擊。",
        "from": "X88n1646_055:0290a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但向棒頭呻。",
        "from": "X88n1646_055:0290a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一智視一愚。",
        "from": "X88n1646_055:0290a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不啻霄壤分。",
        "from": "X88n1646_055:0290a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又如止沸湯。",
        "from": "X88n1646_055:0290b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爨者去其薪。",
        "from": "X88n1646_055:0290b01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二因緣內。",
        "from": "X88n1646_055:0290b01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無明為之君。",
        "from": "X88n1646_055:0290b01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一斷一切斷。",
        "from": "X88n1646_055:0290b02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨埽鄰虗塵。",
        "from": "X88n1646_055:0290b02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當觀佛坐寶蓮華及諸種種莊嚴瑞相。",
        "from": "X88n1646_055:0290b02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發願往生西方極樂。",
        "from": "X88n1646_055:0290b03_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為究竟解脫方便。",
        "from": "X88n1646_055:0290b03_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫以偈曰。",
        "from": "X88n1646_055:0290b04_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行無別法。",
        "from": "X88n1646_055:0290b04_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出世為究竟。",
        "from": "X88n1646_055:0290b04_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出世有多途。",
        "from": "X88n1646_055:0290b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨土為捷徑。",
        "from": "X88n1646_055:0290b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "述此觀想法。",
        "from": "X88n1646_055:0290b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言言宗大乘。",
        "from": "X88n1646_055:0290b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "托質上品蓮。",
        "from": "X88n1646_055:0290b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永遠脫淫穽。",
        "from": "X88n1646_055:0290b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他諸辨難究析因果。",
        "from": "X88n1646_055:0290b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "語多造微。",
        "from": "X88n1646_055:0290b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能具載。",
        "from": "X88n1646_055:0290b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安士既歿。",
        "from": "X88n1646_055:0290b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其書漸湮。",
        "from": "X88n1646_055:0290b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "表而出之。",
        "from": "X88n1646_055:0290b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以勸來者(慾海回狂萬善先資)。",
        "from": "X88n1646_055:0290b07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知歸子曰。",
        "from": "X88n1646_055:0290b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "予讀安士書。",
        "from": "X88n1646_055:0290b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因樂訪安士生平行事。",
        "from": "X88n1646_055:0290b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "久之得其宗人言。",
        "from": "X88n1646_055:0290b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安士通世務。",
        "from": "X88n1646_055:0290b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "習知吳中田賦水利原委得失。",
        "from": "X88n1646_055:0290b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著書甚具。",
        "from": "X88n1646_055:0290b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "康熈三十八年仁皇帝南巡。",
        "from": "X88n1646_055:0290b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安士迎駕揚州九龍橋。",
        "from": "X88n1646_055:0290b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上疏請減蘇松浮賦事。",
        "from": "X88n1646_055:0290b12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖未遽行。",
        "from": "X88n1646_055:0290b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然安士之心至今猶可見也。",
        "from": "X88n1646_055:0290b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其後應巡撫張公聘。",
        "from": "X88n1646_055:0290b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "校錄宋元明先儒書。",
        "from": "X88n1646_055:0290b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "老於家。",
        "from": "X88n1646_055:0290b14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其大概如此。",
        "from": "X88n1646_055:0290b14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "張氷菴者。",
        "from": "X88n1646_055:0290b14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名立廉。",
        "from": "X88n1646_055:0290b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嘗序安士書。",
        "from": "X88n1646_055:0290b15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安士書尊稱之。",
        "from": "X88n1646_055:0290b15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "予訪之崑山。",
        "from": "X88n1646_055:0290b15_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人無知者。",
        "from": "X88n1646_055:0290b15_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汪大紳云。",
        "from": "X88n1646_055:0290b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悲猛極矣。",
        "from": "X88n1646_055:0290b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如從吾佛世尊金口宣揚。",
        "from": "X88n1646_055:0290b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而得六種為之震動。",
        "from": "X88n1646_055:0290b17_3",
        "context_with_next_sentence": false
    }
]