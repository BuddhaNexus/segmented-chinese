[
    {
        "source_sentence": "老子化胡經",
        "from": "T54n2139_001:1266a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□□□□□□□□□止開之則約□□□□□□□□□□之始吾欲書之非筆□□□□體之無形可擬飄乎無外或沈或□□□□□□豪翳餘止如響紀消若雲除□□□□□□捕魚比之於道不足稱無深慜□□□□□□為周柱史經九百年金身□□□□齒銀額有參午龍顏犀文耳高於頂日角月玄鼻有雙柱天中平填足蹈二五手把十文無極之際言歸崑崙化彼胡埏□□罽賓後及天竺於是遂遷文垂後世永乎弗泯。",
        "from": "T54n2139_001:1266a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "老子西昇化胡經序說第一",
        "from": "T54n2139_001:1266b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時太上老君。",
        "from": "T54n2139_001:1266b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以殷王湯甲庚申之歲建□之月。",
        "from": "T54n2139_001:1266b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從常道境。",
        "from": "T54n2139_001:1266b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "駕三氣雲。",
        "from": "T54n2139_001:1266b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘于曰精垂□九耀。",
        "from": "T54n2139_001:1266b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入於玉女玄妙口中。",
        "from": "T54n2139_001:1266b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寄胎為人。",
        "from": "T54n2139_001:1266b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庚辰□二月十五日誕生于毫。",
        "from": "T54n2139_001:1266b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九龍吐水灌洗其形。",
        "from": "T54n2139_001:1266b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化為九井。",
        "from": "T54n2139_001:1266b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時老君鬚髮皓白。",
        "from": "T54n2139_001:1266b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "登即能行。",
        "from": "T54n2139_001:1266b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "步生蓮花。",
        "from": "T54n2139_001:1266b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至于九。",
        "from": "T54n2139_001:1266b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手指天。",
        "from": "T54n2139_001:1266b16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手指地。",
        "from": "T54n2139_001:1266b16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而告人曰。",
        "from": "T54n2139_001:1266b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天上天下。",
        "from": "T54n2139_001:1266b17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯我獨尊。",
        "from": "T54n2139_001:1266b17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當開揚無上道法。",
        "from": "T54n2139_001:1266b17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普度一切動植眾生。",
        "from": "T54n2139_001:1266b18_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周遍十方及幽牢地獄。",
        "from": "T54n2139_001:1266b18_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應度未度咸悉度之。",
        "from": "T54n2139_001:1266b19_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隱顯人間。",
        "from": "T54n2139_001:1266b19_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為國師範。",
        "from": "T54n2139_001:1266b19_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "位登太極。",
        "from": "T54n2139_001:1266b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無上神仙。",
        "from": "T54n2139_001:1266b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時有自然天衣桂體。",
        "from": "T54n2139_001:1266b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神香滿室。",
        "from": "T54n2139_001:1266b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陽景重輝。",
        "from": "T54n2139_001:1266b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九日中。",
        "from": "T54n2139_001:1266b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身長九尺。",
        "from": "T54n2139_001:1266b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾咸驚議。",
        "from": "T54n2139_001:1266b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為聖人。",
        "from": "T54n2139_001:1266b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生有老容。",
        "from": "T54n2139_001:1266b22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故號為老子。",
        "from": "T54n2139_001:1266b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天神空裏。",
        "from": "T54n2139_001:1266b22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讚十號名。",
        "from": "T54n2139_001:1266b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所言十者。",
        "from": "T54n2139_001:1266b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "太上老君．圓神智無上尊．帝王師．大丈夫．大仙尊．天人父．無為上人．大悲仁者．元始天尊．此後老君。",
        "from": "T54n2139_001:1266b23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凝神混迹。",
        "from": "T54n2139_001:1266b25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教化天人。",
        "from": "T54n2139_001:1266b26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼說治身中外法。",
        "from": "T54n2139_001:1266b26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百有餘載。",
        "from": "T54n2139_001:1266b26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王道將衰。",
        "from": "T54n2139_001:1266b26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殺戮賢良。",
        "from": "T54n2139_001:1266b27_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "枉害無數。",
        "from": "T54n2139_001:1266b27_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忠臣切諫。",
        "from": "T54n2139_001:1266b27_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及被誅夷。",
        "from": "T54n2139_001:1266b27_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天降洪災。",
        "from": "T54n2139_001:1266b28_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曾無覺悟。",
        "from": "T54n2139_001:1266b28_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是數載。",
        "from": "T54n2139_001:1266b28_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為周所滅。",
        "from": "T54n2139_001:1266b28_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "康王之時。",
        "from": "T54n2139_001:1266b29_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歲在甲子。",
        "from": "T54n2139_001:1266b29_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦同俗官。",
        "from": "T54n2139_001:1266b29_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晦迹藏名。",
        "from": "T54n2139_001:1266b29_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為柱下史。",
        "from": "T54n2139_001:1266c01_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師輔王者。",
        "from": "T54n2139_001:1266c01_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至于照王。",
        "from": "T54n2139_001:1266c01_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其歲癸丑。",
        "from": "T54n2139_001:1266c01_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便即西邁。",
        "from": "T54n2139_001:1266c02_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過函谷關。",
        "from": "T54n2139_001:1266c02_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "授喜道德五千章句。",
        "from": "T54n2139_001:1266c02_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并說妙真西昇等經。",
        "from": "T54n2139_001:1266c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至太清上法三洞真文靈寶符圖太玄等法。",
        "from": "T54n2139_001:1266c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使其教授。",
        "from": "T54n2139_001:1266c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至精仁者羽化神仙。",
        "from": "T54n2139_001:1266c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令無斷絕。",
        "from": "T54n2139_001:1266c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便即西度。",
        "from": "T54n2139_001:1266c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經歷流沙。",
        "from": "T54n2139_001:1266c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至于闐國毘摩城所。",
        "from": "T54n2139_001:1266c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時老君舉如來節。",
        "from": "T54n2139_001:1266c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "招諸從人。",
        "from": "T54n2139_001:1266c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倏忽之間。",
        "from": "T54n2139_001:1266c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有赤松子．中黃丈人．元始天王．太一元君．六丁玉女．八卦神君．及龍虎君．功曹使者．金乘童子．惠光童子．天官地官．水官空官．日官月官．山官海官．陰官陽官．木官火官．金官土官．五岳四續諸神等君．天丁力士．遊羅將軍．飛天神王．仙人玉女。",
        "from": "T54n2139_001:1266c07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十萬餘眾。",
        "from": "T54n2139_001:1266c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘雲駕龍。",
        "from": "T54n2139_001:1266c13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "浮空而至。",
        "from": "T54n2139_001:1266c13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是老君處于玉帳。",
        "from": "T54n2139_001:1266c13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐七寶座。",
        "from": "T54n2139_001:1266c14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燌百和香。",
        "from": "T54n2139_001:1266c14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散眾名花。",
        "from": "T54n2139_001:1266c14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奏天鈞樂。",
        "from": "T54n2139_001:1266c14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天賢聖周匝圍遶。",
        "from": "T54n2139_001:1266c15_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復以神力召諸胡王。",
        "from": "T54n2139_001:1266c15_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無問遠近。",
        "from": "T54n2139_001:1266c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人士咸集。",
        "from": "T54n2139_001:1266c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "于闐國王乃至朱俱半王．渴叛陀王．護密多王．大月氏王．骨咄陀王．俱蜜王．解蘇國王．拔汗那王．久越得犍王．悒怛國王．烏拉喝王．失范延王．護時健王．多勒建王．罽賓國王．訶達羅支王．波斯國王．疎勒國王．碎葉國王．龜茲國王．拂林國王．大食國王．殖膩國王．數漫國王．恒沒國王．俱藥國王．嵯骨國王．曇陵國王．高昌國王．焉耆國王．弓月國王．石國王．瑟匿國王．康國王．史國王．米國王．似沒盤國王．曹國王．何國王．大小安國王．穆國王．烏那葛國王．尋勿國王．火尋國王．西女國王．大秦國王．舍衛國王．波羅柰國王．帝那忽國王．伽摩路王．乾陀羅王．烏長國王．迦葉彌羅國王．迦羅王．不路羅王．泥婆羅王．熱吒國王．師子國王．拘尸那揭羅王．毘舍離王．劫毘陀王．室羅伐王．瞻波羅國王．三摩咀吒王．烏荼國王．蘇剌吒國王．信度國王．烏刺尸王．扈利國王．狗頭國王．色伽栗王．漫吐[口*曼]王．泥拔國王．越底延王．奢彌國王．小人國王．軒渠國王．陀羅伊羅王．狼揭羅王．五天竺國王。",
        "from": "T54n2139_001:1266c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等八十餘國王。",
        "from": "T54n2139_001:1267a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及其妃后。",
        "from": "T54n2139_001:1267a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并其眷屬。",
        "from": "T54n2139_001:1267a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周匝圍遶。",
        "from": "T54n2139_001:1267a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆來聽法。",
        "from": "T54n2139_001:1267a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時老君告諸國王。",
        "from": "T54n2139_001:1267a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等心毒。",
        "from": "T54n2139_001:1267a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "好行殺害。",
        "from": "T54n2139_001:1267a09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯食血肉。",
        "from": "T54n2139_001:1267a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷眾生命。",
        "from": "T54n2139_001:1267a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今為汝說夜叉經。",
        "from": "T54n2139_001:1267a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令汝斷肉專食麥麨。",
        "from": "T54n2139_001:1267a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿為屠殺。",
        "from": "T54n2139_001:1267a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能斷者。",
        "from": "T54n2139_001:1267a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以自死肉。",
        "from": "T54n2139_001:1267a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "胡人佷戾。",
        "from": "T54n2139_001:1267a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不識親疎。",
        "from": "T54n2139_001:1267a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯好貪婬。",
        "from": "T54n2139_001:1267a12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一無恩義。",
        "from": "T54n2139_001:1267a12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬚髮拳鞠。",
        "from": "T54n2139_001:1267a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梳洗至難。",
        "from": "T54n2139_001:1267a13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性既羶腥。",
        "from": "T54n2139_001:1267a13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體多垢穢。",
        "from": "T54n2139_001:1267a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使其修道。",
        "from": "T54n2139_001:1267a14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煩惱行人。",
        "from": "T54n2139_001:1267a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故普令剔除鬚髮。",
        "from": "T54n2139_001:1267a14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨汝本俗而衣氈裘。",
        "from": "T54n2139_001:1267a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教汝小道。",
        "from": "T54n2139_001:1267a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令漸修學。",
        "from": "T54n2139_001:1267a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼持禁戒。",
        "from": "T54n2139_001:1267a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稍習慈悲。",
        "from": "T54n2139_001:1267a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每月十五日。",
        "from": "T54n2139_001:1267a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常須懺悔。",
        "from": "T54n2139_001:1267a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以神力為化佛形。",
        "from": "T54n2139_001:1267a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "騰空而來。",
        "from": "T54n2139_001:1267a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高丈六身。",
        "from": "T54n2139_001:1267a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體作金色。",
        "from": "T54n2139_001:1267a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面恒東向。",
        "from": "T54n2139_001:1267a18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "示不忘本。",
        "from": "T54n2139_001:1267a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以我東來故顯斯狀。",
        "from": "T54n2139_001:1267a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令其見者。",
        "from": "T54n2139_001:1267a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發慈善心。",
        "from": "T54n2139_001:1267a19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等國王。",
        "from": "T54n2139_001:1267a19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有朝拜一像吾面。",
        "from": "T54n2139_001:1267a19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東向政事。",
        "from": "T54n2139_001:1267a20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是不久。",
        "from": "T54n2139_001:1267a20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過蔥嶺山。",
        "from": "T54n2139_001:1267a20_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中有深池。",
        "from": "T54n2139_001:1267a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒龍居止。",
        "from": "T54n2139_001:1267a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五百商旅。",
        "from": "T54n2139_001:1267a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宿於池濱。",
        "from": "T54n2139_001:1267a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為龍所害。",
        "from": "T54n2139_001:1267a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竟不遺一。",
        "from": "T54n2139_001:1267a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我遺其國渴叛陀王傳祝與之。",
        "from": "T54n2139_001:1267a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就池行法。",
        "from": "T54n2139_001:1267a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍王恐怖。",
        "from": "T54n2139_001:1267a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃變為人。",
        "from": "T54n2139_001:1267a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謝過向王請移別住。",
        "from": "T54n2139_001:1267a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不復於此更損人民。",
        "from": "T54n2139_001:1267a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令後往來絕其傷害。",
        "from": "T54n2139_001:1267a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次即南出至于烏場。",
        "from": "T54n2139_001:1267a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍歷五天。",
        "from": "T54n2139_001:1267a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入摩竭國。",
        "from": "T54n2139_001:1267a25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我衣素服。",
        "from": "T54n2139_001:1267a26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手執空壺。",
        "from": "T54n2139_001:1267a26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置精舍中。",
        "from": "T54n2139_001:1267a26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立浮屠教。",
        "from": "T54n2139_001:1267a26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號清淨佛。",
        "from": "T54n2139_001:1267a27_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼剎利婆羅門等而奉事之。",
        "from": "T54n2139_001:1267a27_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以求無上正真之道。",
        "from": "T54n2139_001:1267a28_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歷年三八。",
        "from": "T54n2139_001:1267a28_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "穆王之時。",
        "from": "T54n2139_001:1267a28_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我還中夏。",
        "from": "T54n2139_001:1267a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使入東海。",
        "from": "T54n2139_001:1267a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至于蓬萊方丈等洲。",
        "from": "T54n2139_001:1267a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到於扶桑。",
        "from": "T54n2139_001:1267a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "暫過大帝之所。",
        "from": "T54n2139_001:1267b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "挍集仙品稱位高下。",
        "from": "T54n2139_001:1267b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又經八王二百餘載。",
        "from": "T54n2139_001:1267b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幽深演之時。",
        "from": "T54n2139_001:1267b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歲次辛酉。",
        "from": "T54n2139_001:1267b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三川震蕩。",
        "from": "T54n2139_001:1267b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王者將亡。",
        "from": "T54n2139_001:1267b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數遭百六。",
        "from": "T54n2139_001:1267b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非人可制。",
        "from": "T54n2139_001:1267b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我更西度。",
        "from": "T54n2139_001:1267b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教化諸國。",
        "from": "T54n2139_001:1267b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次入西海。",
        "from": "T54n2139_001:1267b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至于聚窟流麟等洲。",
        "from": "T54n2139_001:1267b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總召十方神仙大士及初得道地下主者并未授任遊散仙人至孝至忠適經歷度者。",
        "from": "T54n2139_001:1267b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等輩八萬餘人。",
        "from": "T54n2139_001:1267b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "挍量功德行業輕重。",
        "from": "T54n2139_001:1267b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "授其職位五等仙官二十七品仙真上聖岳瀆三天。",
        "from": "T54n2139_001:1267b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸悉補擬如是。",
        "from": "T54n2139_001:1267b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又經六十餘載。",
        "from": "T54n2139_001:1267b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "桓王之時。",
        "from": "T54n2139_001:1267b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歲次甲子一陰之月。",
        "from": "T54n2139_001:1267b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我令尹喜。",
        "from": "T54n2139_001:1267b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘彼月精。",
        "from": "T54n2139_001:1267b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降中天竺國入乎白淨夫人口中託廕而生。",
        "from": "T54n2139_001:1267b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號為悉達。",
        "from": "T54n2139_001:1267b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨太子位。",
        "from": "T54n2139_001:1267b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入山修道。",
        "from": "T54n2139_001:1267b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成無上道。",
        "from": "T54n2139_001:1267b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號為佛陀。",
        "from": "T54n2139_001:1267b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始建悉曇十二文字。",
        "from": "T54n2139_001:1267b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "展轉離合三萬餘言。",
        "from": "T54n2139_001:1267b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣說經誡。",
        "from": "T54n2139_001:1267b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求無上法。",
        "from": "T54n2139_001:1267b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又破九十六種邪道。",
        "from": "T54n2139_001:1267b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歷年七十。",
        "from": "T54n2139_001:1267b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "示人涅槃。",
        "from": "T54n2139_001:1267b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "襄王之時。",
        "from": "T54n2139_001:1267b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其歲乙酉。",
        "from": "T54n2139_001:1267b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我還中國。",
        "from": "T54n2139_001:1267b16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教化天人。",
        "from": "T54n2139_001:1267b16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃授孔丘仁義等法。",
        "from": "T54n2139_001:1267b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾後王誕六十年間。",
        "from": "T54n2139_001:1267b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分國從都。",
        "from": "T54n2139_001:1267b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王者無德。",
        "from": "T54n2139_001:1267b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我即上登崑崙。",
        "from": "T54n2139_001:1267b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飛昇紫微。",
        "from": "T54n2139_001:1267b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "布氣三界。",
        "from": "T54n2139_001:1267b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "含養一切。",
        "from": "T54n2139_001:1267b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後經四百五十餘年。",
        "from": "T54n2139_001:1267b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我乘自然光明道氣。",
        "from": "T54n2139_001:1267b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從真寂境飛入西那玉界蘇隣國中。",
        "from": "T54n2139_001:1267b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降誕王室。",
        "from": "T54n2139_001:1267b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "示為太子。",
        "from": "T54n2139_001:1267b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨家入道。",
        "from": "T54n2139_001:1267b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號末摩尼。",
        "from": "T54n2139_001:1267b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉大法輪。",
        "from": "T54n2139_001:1267b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說經誡律定慧等法。",
        "from": "T54n2139_001:1267b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至三際及二宗門。",
        "from": "T54n2139_001:1267b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教化天人。",
        "from": "T54n2139_001:1267b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令知本際。",
        "from": "T54n2139_001:1267b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上至明界。",
        "from": "T54n2139_001:1267b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下及幽塗。",
        "from": "T54n2139_001:1267b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有眾生。",
        "from": "T54n2139_001:1267b23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆由此度。",
        "from": "T54n2139_001:1267b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩尼之後。",
        "from": "T54n2139_001:1267b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年垂五九。",
        "from": "T54n2139_001:1267b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金氣將興。",
        "from": "T54n2139_001:1267b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我法當盛。",
        "from": "T54n2139_001:1267b25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方聖象。",
        "from": "T54n2139_001:1267b25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣彩自然。",
        "from": "T54n2139_001:1267b25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來入中洲是効也。",
        "from": "T54n2139_001:1267b25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當此之時。",
        "from": "T54n2139_001:1267b26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黃白氣合。",
        "from": "T54n2139_001:1267b26_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三教混齊。",
        "from": "T54n2139_001:1267b26_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同歸於我。",
        "from": "T54n2139_001:1267b26_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仁祠精舍。",
        "from": "T54n2139_001:1267b27_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "接棟連甍。",
        "from": "T54n2139_001:1267b27_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翻演後聖大明尊法。",
        "from": "T54n2139_001:1267b27_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中洲道士。",
        "from": "T54n2139_001:1267b28_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣說因緣。",
        "from": "T54n2139_001:1267b28_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為世舟航。",
        "from": "T54n2139_001:1267b28_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大弘法事。",
        "from": "T54n2139_001:1267b28_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "動植含氣。",
        "from": "T54n2139_001:1267b29_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普皆救度。",
        "from": "T54n2139_001:1267b29_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名總攝一切法",
        "from": "T54n2139_001:1267b29_32",
        "context_with_next_sentence": false
    }
]