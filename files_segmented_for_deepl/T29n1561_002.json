[
    {
        "source_sentence": "阿毘達磨俱舍論實義疏卷第二",
        "from": "T29n1561_002:0326a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊者安惠造",
        "from": "T29n1561_002:0326a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論如有頌言。",
        "from": "T29n1561_002:0326a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　諸佛出現樂　　演說正法樂",
        "from": "T29n1561_002:0326a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　僧眾和合樂　　同修勇進樂",
        "from": "T29n1561_002:0326a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論說。",
        "from": "T29n1561_002:0326a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北俱盧洲衣重一兩。",
        "from": "T29n1561_002:0326a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四天王眾天衣重半兩。",
        "from": "T29n1561_002:0326a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十三天衣重一銖。",
        "from": "T29n1561_002:0326a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜摩天衣重半銖。",
        "from": "T29n1561_002:0326a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覩史多天衣重一銖中四分之一。",
        "from": "T29n1561_002:0326a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樂變化天衣重一銖八分之一。",
        "from": "T29n1561_002:0326a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他化自在天衣重一銖中十六分之一。",
        "from": "T29n1561_002:0326a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已上。",
        "from": "T29n1561_002:0326a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色界天衣不可稱。",
        "from": "T29n1561_002:0326a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼界若有重何言不可稱。",
        "from": "T29n1561_002:0326a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色界諸天隨其身量所著衣服如光皎淨故不可言。",
        "from": "T29n1561_002:0326a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "表色相今次當說。",
        "from": "T29n1561_002:0326a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌曰。",
        "from": "T29n1561_002:0326a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　亂心無心等　　隨流淨不淨",
        "from": "T29n1561_002:0326a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　大種所造性　　由此說無表",
        "from": "T29n1561_002:0326a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六失惣說頌云。",
        "from": "T29n1561_002:0326a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　初闕相續及假是　　無心隨流表相雜　重置等言不簡別　　故此說中有六失(此是眼耳鼻舌身意也說諸心)。",
        "from": "T29n1561_002:0326a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六失中自異意故。",
        "from": "T29n1561_002:0326a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說頌云。",
        "from": "T29n1561_002:0326a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　作等餘心等　　及無心有記",
        "from": "T29n1561_002:0326a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無對所造性　　是名無表色",
        "from": "T29n1561_002:0326a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云地與地界有何差別。",
        "from": "T29n1561_002:0326a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論頌曰。",
        "from": "T29n1561_002:0326a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　地謂顯形色　　隨世想立名",
        "from": "T29n1561_002:0326a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　水火亦復然　　風即界亦爾",
        "from": "T29n1561_002:0326a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論故義品中作如說。",
        "from": "T29n1561_002:0326b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　趣求諸欲人　　常起於希望",
        "from": "T29n1561_002:0326b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　諸欲若不遂　　惱壞如箭中(說六識)當說論頌曰。",
        "from": "T29n1561_002:0326b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　受領納隨觸　　想取像為體",
        "from": "T29n1561_002:0326b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　四餘名行蘊　　如是受等三",
        "from": "T29n1561_002:0326b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　及無表無為　　名法處法界",
        "from": "T29n1561_002:0326b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三蘊七事。",
        "from": "T29n1561_002:0326b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如有頌云。",
        "from": "T29n1561_002:0326b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　三蘊與無表　　及三種無為",
        "from": "T29n1561_002:0326b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　此法處所攝　　法界亦如是",
        "from": "T29n1561_002:0326b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌曰。",
        "from": "T29n1561_002:0326b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　識為各了別　　此即名意處",
        "from": "T29n1561_002:0326b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　及七界應知　　六識轉為意",
        "from": "T29n1561_002:0326b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌曰。",
        "from": "T29n1561_002:0326b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　惣攝一切法　　由一蘊處界",
        "from": "T29n1561_002:0326b15_0",
        "context_with_next_sentence": false
    }
]