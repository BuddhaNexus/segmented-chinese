    },
    {
        "source_sentence": "云。",
        "from": "T55n2184_001:1167b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或執筆書紳。",
        "from": "T55n2184_001:1167b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懷鉛札葉。",
        "from": "T55n2184_001:1167b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "抄如結集。",
        "from": "T55n2184_001:1167b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "錄似載言。",
        "from": "T55n2184_001:1167b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是義門。",
        "from": "T55n2184_001:1167b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨弟子為目。",
        "from": "T55n2184_001:1167b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如云道身章是也。",
        "from": "T55n2184_001:1167b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或以處為名。",
        "from": "T55n2184_001:1167b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如云錐穴問答等云云。",
        "from": "T55n2184_001:1167b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但以當時集者。",
        "from": "T55n2184_001:1167b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未善文體。",
        "from": "T55n2184_001:1167b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂致章句鄙野。",
        "from": "T55n2184_001:1167b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雜以方言或是大教濫觴。",
        "from": "T55n2184_001:1167b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "務在隨機耳。",
        "from": "T55n2184_001:1167c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將來君子。",
        "from": "T55n2184_001:1167c01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宜加潤色。",
        "from": "T55n2184_001:1167c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋名章一卷　義融述",
        "from": "T55n2184_001:1167c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開定決疑三十卷",
        "from": "T55n2184_001:1167c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要決十二卷(或六卷)",
        "from": "T55n2184_001:1167c05_0",
        "context_with_next_sentence": true
    },
    {
