[
    {
        "source_sentence": "新編教藏總錄流衍序",
        "from": "T55n2184_001:1165b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫明月之珠。",
        "from": "T55n2184_001:1165b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "焰光之璧。",
        "from": "T55n2184_001:1165b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "納櫃而弗庸。",
        "from": "T55n2184_001:1165b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則有那益乎哉。",
        "from": "T55n2184_001:1165b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宓惟。",
        "from": "T55n2184_001:1165b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義天姓王氏。",
        "from": "T55n2184_001:1165b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高麗國。",
        "from": "T55n2184_001:1165b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文宗仁孝王第四之子也。",
        "from": "T55n2184_001:1165b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出家封祐世僧統。",
        "from": "T55n2184_001:1165b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元祐初。",
        "from": "T55n2184_001:1165b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入中華而上表。",
        "from": "T55n2184_001:1165b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請傳賢首教。",
        "from": "T55n2184_001:1165b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迺勅而見許。",
        "from": "T55n2184_001:1165b25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "肇往四明。",
        "from": "T55n2184_001:1165b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "事智師慈辨。",
        "from": "T55n2184_001:1165b26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而受天台教觀。",
        "from": "T55n2184_001:1165b26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "趨至靈芝。",
        "from": "T55n2184_001:1165b26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拜大智元照。",
        "from": "T55n2184_001:1165b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而學律藏。",
        "from": "T55n2184_001:1165b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從晉水淨源。",
        "from": "T55n2184_001:1165b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而聞賢首宗乘。",
        "from": "T55n2184_001:1165b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦遭圓照宗本禪師。",
        "from": "T55n2184_001:1165b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而問宗義大旨。",
        "from": "T55n2184_001:1165b28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曲如僧傳等。",
        "from": "T55n2184_001:1165b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "厥雷聲震于支那。",
        "from": "T55n2184_001:1165b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "動三韓矣。",
        "from": "T55n2184_001:1165b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五千四十八卷。",
        "from": "T55n2184_001:1165b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歠一口而不足。",
        "from": "T55n2184_001:1165c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦復搜索章疏教迹。",
        "from": "T55n2184_001:1165c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歷二十年。",
        "from": "T55n2184_001:1165c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而隨所獲編次。",
        "from": "T55n2184_001:1165c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以作三卷。",
        "from": "T55n2184_001:1165c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目新編諸宗教藏總錄也。",
        "from": "T55n2184_001:1165c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斯錄漸于本朝。",
        "from": "T55n2184_001:1165c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "垂于六百載。",
        "from": "T55n2184_001:1165c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛藏叢林。",
        "from": "T55n2184_001:1165c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而使為蟲蠹之宅。",
        "from": "T55n2184_001:1165c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頃日井上氏直屈起鏤梓。",
        "from": "T55n2184_001:1165c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以欲流衍天下。",
        "from": "T55n2184_001:1165c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "余於茲甜其志。",
        "from": "T55n2184_001:1165c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而俱出腕頭之力。",
        "from": "T55n2184_001:1165c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "的策此之懈。",
        "from": "T55n2184_001:1165c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云詎不急務諸。",
        "from": "T55n2184_001:1165c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時元祿歲次癸酉蒲月既望毘盧正宗西湖比丘白玄天龍書于三學山草堂新編諸宗教藏總錄序漢明夢感之後。",
        "from": "T55n2184_001:1165c07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "葉書繼至。",
        "from": "T55n2184_001:1165c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翻譯流通者。",
        "from": "T55n2184_001:1165c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無代無之。",
        "from": "T55n2184_001:1165c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而及貞觀。",
        "from": "T55n2184_001:1165c16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經論大備。",
        "from": "T55n2184_001:1165c16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繇是。",
        "from": "T55n2184_001:1165c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西聖之教。",
        "from": "T55n2184_001:1165c16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "霈然莫禦也。",
        "from": "T55n2184_001:1165c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自聶道真道安。",
        "from": "T55n2184_001:1165c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至于明佺宣律師。",
        "from": "T55n2184_001:1165c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各著目錄。",
        "from": "T55n2184_001:1165c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂之晉錄魏錄等。",
        "from": "T55n2184_001:1165c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然於同本異出舊目新名。",
        "from": "T55n2184_001:1165c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多惑異途。",
        "from": "T55n2184_001:1165c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真偽相亂。",
        "from": "T55n2184_001:1165c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或一經為兩本。",
        "from": "T55n2184_001:1165c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或支品為別翻。",
        "from": "T55n2184_001:1165c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四十餘家。",
        "from": "T55n2184_001:1165c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "紛然久矣開元中。",
        "from": "T55n2184_001:1165c20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始有大法師。",
        "from": "T55n2184_001:1165c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "厥號智昇。",
        "from": "T55n2184_001:1165c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刊落訛謬刪簡重複。",
        "from": "T55n2184_001:1165c21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總成一書。",
        "from": "T55n2184_001:1165c22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰開元釋教錄。",
        "from": "T55n2184_001:1165c22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡二十卷。",
        "from": "T55n2184_001:1165c22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最為精要。",
        "from": "T55n2184_001:1165c22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "議者以為。",
        "from": "T55n2184_001:1165c23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經法之譜。",
        "from": "T55n2184_001:1165c23_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無出昇之右矣。",
        "from": "T55n2184_001:1165c23_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住持遺教莫大焉。",
        "from": "T55n2184_001:1165c24_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "予甞竊謂。",
        "from": "T55n2184_001:1165c24_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經論雖備。",
        "from": "T55n2184_001:1165c24_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而章疏或廢。",
        "from": "T55n2184_001:1165c24_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則流衍無由矣。",
        "from": "T55n2184_001:1165c25_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輒効昇公護法之志。",
        "from": "T55n2184_001:1165c25_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "搜訪教迹以為己任。",
        "from": "T55n2184_001:1165c26_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孜孜不捨。",
        "from": "T55n2184_001:1165c26_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僅二十載于茲矣。",
        "from": "T55n2184_001:1165c26_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今以所得新舊製撰諸宗義章。",
        "from": "T55n2184_001:1165c27_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不敢私祕。",
        "from": "T55n2184_001:1165c27_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敘而出之。",
        "from": "T55n2184_001:1165c28_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後有所獲。",
        "from": "T55n2184_001:1165c28_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦欲隨而錄之。",
        "from": "T55n2184_001:1165c28_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脫或將來編次函帙。",
        "from": "T55n2184_001:1165c28_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與三藏正文。",
        "from": "T55n2184_001:1165c29_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "垂之無窮。",
        "from": "T55n2184_001:1165c29_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則吾願畢矣。",
        "from": "T55n2184_001:1165c29_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時後高麗十三葉。",
        "from": "T55n2184_001:1166a01_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在宥之八年歲次庚午八月初八日。",
        "from": "T55n2184_001:1166a01_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "海東傳華嚴大教沙門義天敘。",
        "from": "T55n2184_001:1166a02_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "新編諸宗教藏總錄卷第一",
        "from": "T55n2184_001:1166a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高麗沙門義天錄(此是草本俟後重廣如有漏略觀者恕之)海東有本見行錄上大華嚴經疏十卷又略疏四卷　已上　慧光(世稱光緣)述疏八卷(或四卷)(慧遠述辯相續修)疏二十二卷　智正述搜玄記五卷　智儼述探玄記二十卷略疏十二卷　已上　法藏述刊定記二十卷　慧苑述刊定記纂釋二十一卷(或十三卷)(法詵創造正覺再修)疏二十卷(本十九卷今開第二)　宗一述疏三十一卷　法銑述疏三十卷　神秀述疏二十卷(或開為四十卷)科七卷　已上　澄觀述科二十卷大科一卷　善來排定隨疏演義鈔四十卷(或開為六十卷徑山寫本八十卷)　澄觀述演義集玄記六卷演義逐難科一卷　已上　道弼述玄談鈔逐難科一卷　思孝述貞元疏十卷　澄觀述科三卷大科一卷　智昭述綸貫十五卷　宗密述疏十卷(本是八卷今開第五卷并宗要均作十卷也)　元曉述古迹記十卷(或五卷)　太賢述略注經一百卷(賢昱略清涼大疏注於經下)大疏注經一百二十卷(淨源移清涼大疏注於經下)科二十卷　淨源刊定論一百卷　靈辨述論四十卷　李通玄述合論一百二十卷(志寧將通玄論注於經下)合論音義十二卷　恒遂集大不思議論一百卷(前分四十卷流通訖餘未見)　德素述法界觀一卷(旋澓頌附)　法順(俗姓杜氏世稱杜順)述法界玄鏡一卷　澄觀述玄鏡議記二卷科一卷　德素述注法界觀門一卷　宗密注鈔四卷科一卷　守真述集要鈔三卷　從朗述集解五卷　有朋述鈔四卷科一卷　洪鑑述助脩記二卷科一卷　淨源述符真鈔四卷　元智述摭要鈔四卷　遵式述釋旋澓頌一卷　椎勁述略法界觀手記一卷三觀宗記一卷　已上　有誠述新注法界觀一卷　呂氏注疏通教觀十門論一卷法界十大觀論一卷釋起入法界觀四法明門一卷一相觀門三根判位章一卷普觀諸法相即在入不思議門論一卷心佛道交論一卷　已上　曇雅述十門實相觀一卷　杜順述還源觀一卷　法藏述疏鈔補解一卷科一卷　淨源述義綱一卷　可歸述三昧觀一卷普賢觀一卷色空觀一卷華藏世界海觀一卷　已上　法藏述華藏世界海主伴圖敘一卷　回濟述華藏世界海圖一卷　處恒述法界義海二卷(或一卷)　法藏述科一卷　淨源述十門看法觀一卷　義想述刊定別章二卷　慧苑述妙理圓成觀三卷　神秀述三聖圓融觀一卷五蘊觀一卷十二因緣觀一卷了義一卷(食肉有罪無罪附)心要一卷　已上　澄觀述注一卷　通義述自防遺忘集十卷開脈一卷　已上　文超述法界圖一卷　義想述指歸兩卷　佛陀三藏述廣釋義章一卷　光緣述章門雜孔目四卷要義問答二卷(世云五十要問答是)十玄章一卷(又有一本題云十玄無礙義者疑後人所述)六相章一卷(三性章附)　已上　智儼述教義分齊三卷　法藏述科一卷　淨源述綱目章一卷指歸一卷策林一卷華嚴雜章門一卷(三寶章流轉章法界緣起章圓音章法身章十世章玄義章)三寶別行記一卷金師子章一卷　已上　法藏述明鑑鈔二卷　祐田述注一卷　承遷述注一卷　昭昱述雲間類解一卷科一卷　淨源述一道章一卷大乘觀行一卷　已上　元曉述九會章一卷　慧苑述略策一卷　澄觀述修行次第決疑論四卷會釋兩卷略釋一卷十門玄義一卷十二緣生解述顯智成悲論一卷眼目論一卷　已上　李通玄述十門玄義一卷　亡名明難品疏一卷記一卷　已上　曇遷述入法界品鈔一卷　智儼述入法界品鈔記一卷　義想述綱要三卷　澄觀述錦冠鈔四卷(或二卷)　傳奧述經序別行崇福記一卷　紹詵述三品別行疏二卷(問明淨行賢首是也)三品隨疏演義鈔五卷科一卷行願品別行疏一卷　已上　澄觀述隨疏義記六卷科一卷又隨疏記義三卷科一卷又記義二卷科一卷　已上　宗密述行願品別行疏二卷(仲希移本疏注於經下)釋義鈔四卷科一卷(宗密述仲希治定)鈔六卷　從朗述發菩提心戒本一卷　澄觀述淨行品別行疏一卷鈔一卷科一卷　已上　從朗述隨好光明品解一卷　王氏述修慈分疏二卷略鈔一卷科一卷　已上　思孝述要義問答二卷(僧傳云錐穴問答是)　智通述一乘問答二卷(僧傳云道身章是)　道身述按大宋僧史義湘傳。",
        "from": "T55n2184_001:1166a08_0",
