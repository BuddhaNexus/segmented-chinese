[
    {
        "source_sentence": "心經說",
        "from": "X26n0546_001:0841c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雪浪釋　洪恩　述",
        "from": "X26n0546_001:0841c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "般若心經。",
        "from": "X26n0546_001:0841c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是世尊以人顯法。",
        "from": "X26n0546_001:0841c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照見蘊空。",
        "from": "X26n0546_001:0841c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而明甚深般若也。",
        "from": "X26n0546_001:0841c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恐人不解。",
        "from": "X26n0546_001:0841c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故自釋之曰。",
        "from": "X26n0546_001:0841c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照見五蘊皆空度一切苦厄。",
        "from": "X26n0546_001:0841c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何為蘊空。",
        "from": "X26n0546_001:0841c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以其色即是空。",
        "from": "X26n0546_001:0841c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空即是色。",
        "from": "X26n0546_001:0841c08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而受想行識。",
        "from": "X26n0546_001:0841c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫不皆空也。",
        "from": "X26n0546_001:0841c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恐人不知色即是空之旨。",
        "from": "X26n0546_001:0841c09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而復釋之曰。",
        "from": "X26n0546_001:0841c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸法空相。",
        "from": "X26n0546_001:0841c10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即五蘊之中。",
        "from": "X26n0546_001:0841c10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生而本自不生。",
        "from": "X26n0546_001:0841c10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅亦元無有滅。",
        "from": "X26n0546_001:0841c10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不垢不淨不增不減者。",
        "from": "X26n0546_001:0841c11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既此空相。",
        "from": "X26n0546_001:0841c11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖具五蘊諸法。",
        "from": "X26n0546_001:0841c11_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而原無生滅垢淨增減。",
        "from": "X26n0546_001:0841c12_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則無色無受想行識明矣。",
        "from": "X26n0546_001:0841c12_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為照見五蘊皆空。",
        "from": "X26n0546_001:0841c13_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五蘊既空。",
        "from": "X26n0546_001:0841c13_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則世之根塵識三。",
        "from": "X26n0546_001:0841c13_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出世之四諦十二緣。",
        "from": "X26n0546_001:0841c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以至能證所證。",
        "from": "X26n0546_001:0841c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫不皆空。",
        "from": "X26n0546_001:0841c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既無世之陰界。",
        "from": "X26n0546_001:0841c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則亦無出世智得也。",
        "from": "X26n0546_001:0841c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能所既忘。",
        "from": "X26n0546_001:0841c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五蘊何有。",
        "from": "X26n0546_001:0841c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故都無所得。",
        "from": "X26n0546_001:0841c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方名甚深般若。",
        "from": "X26n0546_001:0841c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩依此。",
        "from": "X26n0546_001:0841c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而心無罣礙。",
        "from": "X26n0546_001:0841c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恐怖遠離顛倒夢想。",
        "from": "X26n0546_001:0841c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能至究竟涅槃。",
        "from": "X26n0546_001:0841c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三世諸佛依此。",
        "from": "X26n0546_001:0841c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而阿耨菩提可得。",
        "from": "X26n0546_001:0841c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五住結盡。",
        "from": "X26n0546_001:0841c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二死俱忘。",
        "from": "X26n0546_001:0841c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何苦惱厄難而不度耶。",
        "from": "X26n0546_001:0841c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆由照見蘊空。",
        "from": "X26n0546_001:0841c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而能如是。",
        "from": "X26n0546_001:0841c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故讚嘆之曰。",
        "from": "X26n0546_001:0841c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而此蘊空甚深般若。",
        "from": "X26n0546_001:0841c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是大明呪神呪。",
        "from": "X26n0546_001:0841c20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以其能革凡成聖。",
        "from": "X26n0546_001:0841c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如果裸之呪螟蛉。",
        "from": "X26n0546_001:0841c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而不自知。",
        "from": "X26n0546_001:0841c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恐人不信。",
        "from": "X26n0546_001:0841c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復曰。",
        "from": "X26n0546_001:0841c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除一切苦。",
        "from": "X26n0546_001:0841c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真實不虗。",
        "from": "X26n0546_001:0841c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後說密呪。",
        "from": "X26n0546_001:0841c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使人斷言語息思想。",
        "from": "X26n0546_001:0841c22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而契此蘊空般若也。",
        "from": "X26n0546_001:0841c22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此則但離妄緣。",
        "from": "X26n0546_001:0841c23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即如如佛。",
        "from": "X26n0546_001:0841c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而觀世音能行此耳。",
        "from": "X26n0546_001:0841c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名自在。",
        "from": "X26n0546_001:0841c24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以至六百卷大經。",
        "from": "X26n0546_001:0841c24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此為心。",
        "from": "X26n0546_001:0841c24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如人之有心。",
        "from": "X26n0546_001:0841c24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為一身之主耳。",
        "from": "X26n0546_001:0842a01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等簡易直截明白。",
        "from": "X26n0546_001:0842a01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而世之裨販之徒。",
        "from": "X26n0546_001:0842a01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自衒自媒。",
        "from": "X26n0546_001:0842a02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妄談般若。",
        "from": "X26n0546_001:0842a02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[這言+(一/ㄙ)]橫生穿鑿。",
        "from": "X26n0546_001:0842a02_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牽枝引蔓。",
        "from": "X26n0546_001:0842a02_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "礙正知見。",
        "from": "X26n0546_001:0842a03_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一盲唱之於前。",
        "from": "X26n0546_001:0842a03_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百盲從而和之於後。",
        "from": "X26n0546_001:0842a03_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞎人眼目。",
        "from": "X26n0546_001:0842a04_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徒增業苦。",
        "from": "X26n0546_001:0842a04_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "予憫無聞後進。",
        "from": "X26n0546_001:0842a04_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蔓草難除。",
        "from": "X26n0546_001:0842a04_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛燈欲滅。",
        "from": "X26n0546_001:0842a04_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故不得已隨筆。",
        "from": "X26n0546_001:0842a05_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略記經之首尾之旨如是。",
        "from": "X26n0546_001:0842a05_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以俟知言。",
        "from": "X26n0546_001:0842a05_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大似揚聲止響。",
        "from": "X26n0546_001:0842a06_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "予欲無言可乎。",
        "from": "X26n0546_001:0842a06_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心經說(終)",
        "from": "X26n0546_001:0842a08_0",
        "context_with_next_sentence": false
    }
]