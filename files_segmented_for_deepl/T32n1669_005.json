[
    {
        "source_sentence": "大宗地玄文本論卷第五",
        "from": "T32n1669_005:0677a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬鳴菩薩造",
        "from": "T32n1669_005:0677a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真諦三藏譯",
        "from": "T32n1669_005:0677a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深里出興地藏大龍王道路大決擇分",
        "from": "T32n1669_005:0677a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第九",
        "from": "T32n1669_005:0677a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是已說深里出興地藏大龍王大決擇分。",
        "from": "T32n1669_005:0677a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說深里出興地藏大龍王道路大決擇分。",
        "from": "T32n1669_005:0677a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_005:0677a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_005:0677a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　龍王道路中　　總有二十法",
        "from": "T32n1669_005:0677a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　謂二本藏中　　各有十法故",
        "from": "T32n1669_005:0677a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_005:0677a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就深里出興地藏大龍王道路中。",
        "from": "T32n1669_005:0677a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總有二十中自在法。",
        "from": "T32n1669_005:0677a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何。",
        "from": "T32n1669_005:0677a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功德過患二種藏中各有十故。",
        "from": "T32n1669_005:0677a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈龍王道路中總有二十法。",
        "from": "T32n1669_005:0677a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂二本藏中各有十法故。",
        "from": "T32n1669_005:0677a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故彼二十法。",
        "from": "T32n1669_005:0677a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名字形相當如何耶。",
        "from": "T32n1669_005:0677a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_005:0677a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　功德本藏十　　常壞俱及非",
        "from": "T32n1669_005:0677a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　自他俱并非　　無礙兼一全",
        "from": "T32n1669_005:0677a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　過患本藏十　　如一違無有",
        "from": "T32n1669_005:0677a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　對利融上下　　如次應觀察",
        "from": "T32n1669_005:0677a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如是二十法　　一一皆各各",
        "from": "T32n1669_005:0677a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　圓滿廣大故　　與本存等量",
        "from": "T32n1669_005:0677a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_005:0677a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就功德本藏之體中。",
        "from": "T32n1669_005:0677a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則有十法。",
        "from": "T32n1669_005:0677a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能攝一切無量功德。",
        "from": "T32n1669_005:0677a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為十。",
        "from": "T32n1669_005:0677a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者本體自性決定常住不生不滅遠離流轉功德品。",
        "from": "T32n1669_005:0677a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者本體自性常恒移轉是生是滅流轉俱行功德品。",
        "from": "T32n1669_005:0677a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者決定常住常恒無常一時俱轉無前後違功德品。",
        "from": "T32n1669_005:0677b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者出常無常二事不攝自體本性離脫亡行功德品。",
        "from": "T32n1669_005:0677b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者十種自自攝一切法無有所餘獨一無二一種一行功德品。",
        "from": "T32n1669_005:0677b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六者無體無性從因緣起亦有亦無隨應變轉功德品。",
        "from": "T32n1669_005:0677b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七者無餘究竟俱轉俱行不相捨離功德品。",
        "from": "T32n1669_005:0677b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八者非自非他絕離有名住於非非自性決定功德品。",
        "from": "T32n1669_005:0677b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九者於一切法隨應無礙自體自性法爾道理性造如是功德品。",
        "from": "T32n1669_005:0677b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十者五根一一根五塵一一塵一切諸法亦復如是功德品。",
        "from": "T32n1669_005:0677b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為十。",
        "from": "T32n1669_005:0677b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈功德本藏十常壞俱及非自他俱并非無礙兼一全故。",
        "from": "T32n1669_005:0677b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就過患本藏之體中。",
        "from": "T32n1669_005:0677b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有十法。",
        "from": "T32n1669_005:0677b13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能攝一切無量過患。",
        "from": "T32n1669_005:0677b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為十。",
        "from": "T32n1669_005:0677b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者於一切法隨順如如如作逆事過患品。",
        "from": "T32n1669_005:0677b14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者造作諸法同一業用一作逆事過患品。",
        "from": "T32n1669_005:0677b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者治道起時無有定體違作逆事過患品。",
        "from": "T32n1669_005:0677b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者染淨諸法皆無所有無作逆事過患品。",
        "from": "T32n1669_005:0677b17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者一切諸法皆總有有有作逆事過患品。",
        "from": "T32n1669_005:0677b18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六者隨治同量如如現前對作逆事過患品。",
        "from": "T32n1669_005:0677b19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七者由治道力自類增益利作逆事過患品。",
        "from": "T32n1669_005:0677b20_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八者由治道息發起自用融作逆事過患品。",
        "from": "T32n1669_005:0677b21_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九者待上轉者方得起用上作逆事過患品。",
        "from": "T32n1669_005:0677b22_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十者以隱藏時方得起用下作逆事過患品。",
        "from": "T32n1669_005:0677b23_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為十。",
        "from": "T32n1669_005:0677b24_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如其次第住思止心。",
        "from": "T32n1669_005:0677b24_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "專心觀察其理故明。",
        "from": "T32n1669_005:0677b24_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈過患本藏十如一違無有對利融上下如次應觀察故。",
        "from": "T32n1669_005:0677b25_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是二十法。",
        "from": "T32n1669_005:0677b26_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與彼本方等無差別。",
        "from": "T32n1669_005:0677b26_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故有二十種本藏。",
        "from": "T32n1669_005:0677b27_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無相違過。",
        "from": "T32n1669_005:0677b27_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何。",
        "from": "T32n1669_005:0677b28_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其本藏中如是道理自然常有。",
        "from": "T32n1669_005:0677b28_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不從本藏長建立故。",
        "from": "T32n1669_005:0677b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何故處處皆存品字。",
        "from": "T32n1669_005:0677b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前所說二十種法。",
        "from": "T32n1669_005:0677b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各各有百眷屬類故。",
        "from": "T32n1669_005:0677c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故言品。",
        "from": "T32n1669_005:0677c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "例前應了。",
        "from": "T32n1669_005:0677c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈如是二十法一一皆各各圓滿廣大故與本存等量故。",
        "from": "T32n1669_005:0677c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是已說建立名字門。",
        "from": "T32n1669_005:0677c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說所詮義理門。",
        "from": "T32n1669_005:0677c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且治及障對量差別。",
        "from": "T32n1669_005:0677c04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當如何耶。",
        "from": "T32n1669_005:0677c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_005:0677c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如是二十法　　一一皆各各",
        "from": "T32n1669_005:0677c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一德治諸過　　諸過障一德",
        "from": "T32n1669_005:0677c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無有定次第　　而數品類等",
        "from": "T32n1669_005:0677c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無失對量過　　如理應觀察",
        "from": "T32n1669_005:0677c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如說本存法　　說品類亦爾",
        "from": "T32n1669_005:0677c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_005:0677c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前所說二十種法。",
        "from": "T32n1669_005:0677c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一各各一德治一切障。",
        "from": "T32n1669_005:0677c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切障障一德。",
        "from": "T32n1669_005:0677c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無別對量。",
        "from": "T32n1669_005:0677c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈如是二十法一一皆各各一德治諸過。",
        "from": "T32n1669_005:0677c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸過障一德無有定次第故。",
        "from": "T32n1669_005:0677c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若如是說。",
        "from": "T32n1669_005:0677c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此門中對量軌則混成雜亂。",
        "from": "T32n1669_005:0677c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖無別對量而有總對量。",
        "from": "T32n1669_005:0677c15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故無失。",
        "from": "T32n1669_005:0677c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈而數品類等無失對量過如理應觀察故。",
        "from": "T32n1669_005:0677c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如本品爾例前應了。",
        "from": "T32n1669_005:0677c17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈如說本存法說品類亦爾故。",
        "from": "T32n1669_005:0677c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是已說治障對量差別門。",
        "from": "T32n1669_005:0677c18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說安立金剛位地門。",
        "from": "T32n1669_005:0677c19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_005:0677c19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_005:0677c19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　此本法門中　　亦有金剛位",
        "from": "T32n1669_005:0677c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　依位有三門　　謂上中下門",
        "from": "T32n1669_005:0677c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_005:0677c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就此本法門中。",
        "from": "T32n1669_005:0677c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有五十一金剛位。",
        "from": "T32n1669_005:0677c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如上所說二十種法。",
        "from": "T32n1669_005:0677c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依位安立。",
        "from": "T32n1669_005:0677c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何安立。",
        "from": "T32n1669_005:0677c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂諸位中皆具二十。",
        "from": "T32n1669_005:0677c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有前後一時轉故。",
        "from": "T32n1669_005:0677c25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然則此中有三種門。",
        "from": "T32n1669_005:0677c25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為三。",
        "from": "T32n1669_005:0677c26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者一向上轉門。",
        "from": "T32n1669_005:0677c26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者一向下轉門。",
        "from": "T32n1669_005:0677c26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者一向中轉門。",
        "from": "T32n1669_005:0677c27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為三。",
        "from": "T32n1669_005:0677c27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是三門各各一一位中具足俱轉。",
        "from": "T32n1669_005:0677c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不待初後際故。",
        "from": "T32n1669_005:0677c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如說本法品類亦爾。",
        "from": "T32n1669_005:0677c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此小門廣大迴持。",
        "from": "T32n1669_005:0677c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應廣通達。",
        "from": "T32n1669_005:0678a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈此本法門中亦有金剛位依位有三門謂上中下門故。",
        "from": "T32n1669_005:0678a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大龍王重重廣海無盡大藏大決擇分第十如是已說深里出興地藏大龍王道路大決擇分。",
        "from": "T32n1669_005:0678a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說大龍王重重廣海無盡大藏大決擇分。",
        "from": "T32n1669_005:0678a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_005:0678a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_005:0678a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　廣海大藏中　　總有三種重",
        "from": "T32n1669_005:0678a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　謂初中後重　　初重有二十",
        "from": "T32n1669_005:0678a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　二億十方界　　量法門大海",
        "from": "T32n1669_005:0678a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　二十種本藏　　各生一萬量",
        "from": "T32n1669_005:0678a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　各別百眷屬　　各生一千故",
        "from": "T32n1669_005:0678a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　是故數圓滿　　住心應觀察",
        "from": "T32n1669_005:0678a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　中後重倍此　　應廣通達故",
        "from": "T32n1669_005:0678a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_005:0678a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就大龍王重重廣海無盡大藏之自家中。",
        "from": "T32n1669_005:0678a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總有三重。",
        "from": "T32n1669_005:0678a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能攝諸位。",
        "from": "T32n1669_005:0678a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為三。",
        "from": "T32n1669_005:0678a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者初第一有重。",
        "from": "T32n1669_005:0678a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者中安住居重。",
        "from": "T32n1669_005:0678a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者後建立轉重。",
        "from": "T32n1669_005:0678a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為三。",
        "from": "T32n1669_005:0678a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就第一重中。",
        "from": "T32n1669_005:0678a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則有二十二億十方世界之量種種勝妙法門大海。",
        "from": "T32n1669_005:0678a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此義云何。",
        "from": "T32n1669_005:0678a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂二十種本藏法中。",
        "from": "T32n1669_005:0678a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一各各出生一萬法門大海。",
        "from": "T32n1669_005:0678a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各各別別百眷屬中。",
        "from": "T32n1669_005:0678a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一各各出生一千法門海故。",
        "from": "T32n1669_005:0678a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此義故名字圓滿義理具足。",
        "from": "T32n1669_005:0678a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如其次第安住其心定止其思。",
        "from": "T32n1669_005:0678a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聰明觀察其數量理及所詮意現了分明。",
        "from": "T32n1669_005:0678a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈廣海大藏中總有三種重。",
        "from": "T32n1669_005:0678a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂初中後重。",
        "from": "T32n1669_005:0678a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初重有二十二億十方界量法門大海二十種本藏。",
        "from": "T32n1669_005:0678a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各生一萬量。",
        "from": "T32n1669_005:0678a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各別百眷屬各生一千故。",
        "from": "T32n1669_005:0678a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故數圓滿。",
        "from": "T32n1669_005:0678a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住心應觀察故。",
        "from": "T32n1669_005:0678a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次二種重。",
        "from": "T32n1669_005:0678a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如其次第倍前普布應廣通達。",
        "from": "T32n1669_005:0678a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈中後重倍此應廣通達故。",
        "from": "T32n1669_005:0678a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故大摩尼寶藏陀羅尼修集修多羅中作如是說。",
        "from": "T32n1669_005:0678a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍龍地地大無盡海藏中。",
        "from": "T32n1669_005:0678b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有多十方之量法門軌則品類。",
        "from": "T32n1669_005:0678b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最初名為轉大法論具足一海無極無盡引導光明現照地地本業本用出生增長軌則大海法門。",
        "from": "T32n1669_005:0678b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最後名為有性無性無我空理大利益廣光明亦離脫亦合轉具足具足無邊摩訶行種地本藏法雨騰門出興上味品類法門。",
        "from": "T32n1669_005:0678b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯取一界以為譬喻非取塵等。",
        "from": "T32n1669_005:0678b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至廣說故。",
        "from": "T32n1669_005:0678b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "◎大宗地玄文本論卷第五",
        "from": "T32n1669_005:0678b08_1",
        "context_with_next_sentence": false
    }
]