[
    {
        "source_sentence": "瑜伽論第三十一手記",
        "from": "T85n2802_001:0937c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "談述　福慧",
        "from": "T85n2802_001:0937c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言謂依緣性等者。",
        "from": "T85n2802_001:0937c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂因前名緣性後□□□緣起　言而起勝解者。",
        "from": "T85n2802_001:0937c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂能了知內外諸緣生法唯有因性及與緣性□有我人等□　言廣說如別者。",
        "from": "T85n2802_001:0937c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂無明有十九種。",
        "from": "T85n2802_001:0938a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行有三種等。",
        "from": "T85n2802_001:0938a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前尋思地生雜染中以廣分別　言不自在故者。",
        "from": "T85n2802_001:0938a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂我者是自在之義。",
        "from": "T85n2802_001:0938a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由無明中無自在我故不作是念。",
        "from": "T85n2802_001:0938a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我能生行。",
        "from": "T85n2802_001:0938a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘皆准此　言中間等者。",
        "from": "T85n2802_001:0938a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此顯空義。",
        "from": "T85n2802_001:0938a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂無明及行二法中間無有傳受之法故名空也　言如前廣說者。",
        "from": "T85n2802_001:0938a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說□諍有害等二世過患。",
        "from": "T85n2802_001:0938a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前不淨觀說　言諸業者。",
        "from": "T85n2802_001:0938a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂善等三業也　言一自體果者。",
        "from": "T85n2802_001:0938a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂情非情緣起通攝也　言受用境界果者。",
        "from": "T85n2802_001:0938a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂唯約有情而論也　言今世等者。",
        "from": "T85n2802_001:0938a09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂現世五趣果身生也　言六處者。",
        "from": "T85n2802_001:0938a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂六根也　言令生有芽者。",
        "from": "T85n2802_001:0938a11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂無明為因故發生三行芽等也。",
        "from": "T85n2802_001:0938a12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破展轉等也　言諸取者。",
        "from": "T85n2802_001:0938a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂我語取欲取戒禁取等也　言有內現證者。",
        "from": "T85n2802_001:0938a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂五位行人現見無明等順逆二法。",
        "from": "T85n2802_001:0938a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而能修行證上果故　言比度法者。",
        "from": "T85n2802_001:0938a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂比諸聖者等修順逆緣起故得彼聖果。",
        "from": "T85n2802_001:0938a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦比此修行證於上果故　言能正解了等者。",
        "from": "T85n2802_001:0938a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂地等六界種姓別故及能尋思　言種子義者。",
        "from": "T85n2802_001:0938a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂地等六法為種子故有五趣身也　言因義者。",
        "from": "T85n2802_001:0938a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂因地等六法為因故生五趣身也　言性義者。",
        "from": "T85n2802_001:0938a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂也等各各有堅等性故　言是其界義者。",
        "from": "T85n2802_001:0938a21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為上四義一□□　言一念等者。",
        "from": "T85n2802_001:0938a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂不了知身中有其六界故。",
        "from": "T85n2802_001:0938a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但起一我見者名一念相　言有正尋思等者。",
        "from": "T85n2802_001:0938a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂諸外道計我為因故得入母胎。",
        "from": "T85n2802_001:0938a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中不爾。",
        "from": "T85n2802_001:0938a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因六界故得入母胎也　言安住法性者。",
        "from": "T85n2802_001:0938a26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂地水等六界諸佛出世不出世法等有故名安住也　言長入息者。",
        "from": "T85n2802_001:0938a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂十息為一數等也　言都無持入息者等者。",
        "from": "T85n2802_001:0938a28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂出□□□□有蘊等諸行此中都無任持二見我人等故　言因者。",
        "from": "T85n2802_001:0938a29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "業煩惱也　言緣者。",
        "from": "T85n2802_001:0938b02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂父母精血等也　言諸行者。",
        "from": "T85n2802_001:0938b02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂果相也　言善巧等者。",
        "from": "T85n2802_001:0938b03_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中有問。",
        "from": "T85n2802_001:0938b04_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四緣之中淨行所緣所有六義如上所說。",
        "from": "T85n2802_001:0938b04_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其義已知。",
        "from": "T85n2802_001:0938b05_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三善巧所緣。",
        "from": "T85n2802_001:0938b05_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四淨惑所緣。",
        "from": "T85n2802_001:0938b05_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六相差別何故不說。",
        "from": "T85n2802_001:0938b06_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故論答云若依止善巧等也　言此中者。",
        "from": "T85n2802_001:0938b06_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂指前四慧行觀三門六事觀不淨而五觀破。",
        "from": "T85n2802_001:0938b07_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此終九種加行成前觀門故也。",
        "from": "T85n2802_001:0938b08_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故論文言九種等也　言得定者。",
        "from": "T85n2802_001:0938b09_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂五位行人由九種加行故速得五位本定故　言於所應往定等者。",
        "from": "T85n2802_001:0938b10_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂五位行人修九種加行故。",
        "from": "T85n2802_001:0938b11_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於初資糧位者速得加行位等也　言等分者。",
        "from": "T85n2802_001:0938b12_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂貪等五位煩惱不偏在一故名等分也　言薄塵者。",
        "from": "T85n2802_001:0938b13_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂五位行人上上位人也　言應隨所樂等者。",
        "from": "T85n2802_001:0938b14_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂等行等二人於五位煩惱中先何起者修對治門故。",
        "from": "T85n2802_001:0938b15_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故論文言舉緣一境性等也　言相應所緣等者。",
        "from": "T85n2802_001:0938b16_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂不淨等五觀門也　言不全臨入者。",
        "from": "T85n2802_001:0938b18_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨順等者。",
        "from": "T85n2802_001:0938b18_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂初中後三時心也　言由住如是等者。",
        "from": "T85n2802_001:0938b19_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂住前出等六法等三種加行不緩名不放逸也　言一時半時者。",
        "from": "T85n2802_001:0938b20_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂一更半更也　言法義者。",
        "from": "T85n2802_001:0938b21_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂所治能治法義之中無顛倒故　言九種相心住等者。",
        "from": "T85n2802_001:0938b22_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂內住等九種住心也　言無相者。",
        "from": "T85n2802_001:0938b23_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂無我及我所心也　言皆得清淨者。",
        "from": "T85n2802_001:0938b24_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂依止品破觀得成就二俱清淨故云皆等也　言所有加行者。",
        "from": "T85n2802_001:0938b25_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂止品此彼觀品之加行故也　言所熏習等者。",
        "from": "T85n2802_001:0938b26_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂觀為能熏故止為所熏。",
        "from": "T85n2802_001:0938b27_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此二法相依故皆得清淨也　言所有加行者。",
        "from": "T85n2802_001:0938b28_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂觀品是彼止品之加行故也　言謂由等者。",
        "from": "T85n2802_001:0938b29_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂六隨念等中隨取一者便名為舉　言顯示等者。",
        "from": "T85n2802_001:0938c01_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂若心沈沒時。",
        "from": "T85n2802_001:0938c02_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以淨妙相顯示其心自勸其心自慶其心　言隨順等者。",
        "from": "T85n2802_001:0938c03_54",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於不淨等五觀之中勤精進者是舉之相　言所緣等者。",
        "from": "T85n2802_001:0938c04_55",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於順情違情之境中心無染污心平等故。",
        "from": "T85n2802_001:0938c05_56",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於止觀中亦平等轉　言謂由所緣者。",
        "from": "T85n2802_001:0938c06_57",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於順違及止觀中心不住者名為捨相　言上捨者。",
        "from": "T85n2802_001:0938c07_58",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂取上上地之捨也　言謂於如是等者。",
        "from": "T85n2802_001:0938c08_59",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於止觀二法所有塵義。",
        "from": "T85n2802_001:0938c09_60",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如上所說中一一皆得善巧故　言善趣等者。",
        "from": "T85n2802_001:0938c10_61",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂初中後三心也　言或時棄捨者。",
        "from": "T85n2802_001:0938c11_62",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂若棄捨世出世定所有影像名為出定。",
        "from": "T85n2802_001:0938c12_63",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於化緣等說不定地作意思非亦名出定。",
        "from": "T85n2802_001:0938c13_64",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言於餘所作者。",
        "from": "T85n2802_001:0938c14_65",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於上上果所修行中而追求故　言謂於一切等者。",
        "from": "T85n2802_001:0938c14_66",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於所受戒學之中無違越者名不捨軛。",
        "from": "T85n2802_001:0938c15_67",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於無穿穴等八法之中而修習者名不違越　言於久所作等者。",
        "from": "T85n2802_001:0938c16_68",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於自所作身業及語意三業中。",
        "from": "T85n2802_001:0938c18_69",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸不善隨順不作。",
        "from": "T85n2802_001:0938c19_70",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於十善業隨順而作。",
        "from": "T85n2802_001:0938c19_71",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故論文言能因隨憶等也　言數起勝解者。",
        "from": "T85n2802_001:0938c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂能治道　言數正除遣者。",
        "from": "T85n2802_001:0938c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂所治法也。",
        "from": "T85n2802_001:0938c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵云毘呵婆耶者。",
        "from": "T85n2802_001:0938c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具含上二義。",
        "from": "T85n2802_001:0938c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一勝解義。",
        "from": "T85n2802_001:0938c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二除遣義。",
        "from": "T85n2802_001:0938c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此二皆是正加行故標此二名　言隨相行等者。",
        "from": "T85n2802_001:0938c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂二十賢善定中隨行可一皆具能治勝解及所治除遣。",
        "from": "T85n2802_001:0938c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中初修業者亦應如是。",
        "from": "T85n2802_001:0938c25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如久修觀行之人具修上二之行乃名正行人也　言當知此中等者。",
        "from": "T85n2802_001:0938c26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此等五門一一皆具因果二義。",
        "from": "T85n2802_001:0938c27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說者應知　言由於最初等者。",
        "from": "T85n2802_001:0938c28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂修不淨觀等者必背靜等顛倒之相　言由緣餘定者等。",
        "from": "T85n2802_001:0938c29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂若所修之定道力劣故不能除遣所治法者。",
        "from": "T85n2802_001:0939a01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當修餘定作其助伴成本定故　言由於一切等者。",
        "from": "T85n2802_001:0939a02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂由於諸淨相等中起染污者。",
        "from": "T85n2802_001:0939a03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應修不淨等觀除遣淨等諸相　言雖遍立等者。",
        "from": "T85n2802_001:0939a04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中有問。",
        "from": "T85n2802_001:0939a05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經中所說五種之相名除遣者意趣何等名為正除遣也。",
        "from": "T85n2802_001:0939a05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故論答云。",
        "from": "T85n2802_001:0939a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然此義中等也難有五種。",
        "from": "T85n2802_001:0939a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中但取初二之法立為除遣　言全不於所緣境繫縛心者。",
        "from": "T85n2802_001:0939a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂諸行人根性不等煩惱不同故。",
        "from": "T85n2802_001:0939a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初修業時若貪欲多者令於不淨觀繫縛其心。",
        "from": "T85n2802_001:0939a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若嗔多者以慈愍觀而繫其心。",
        "from": "T85n2802_001:0939a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘皆准此。",
        "from": "T85n2802_001:0939a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故論文言或復處等也　言唯作是念等者。",
        "from": "T85n2802_001:0939a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂如不淨觀等相中有淨等相者名有散亂。",
        "from": "T85n2802_001:0939a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有不淨等相者名無散亂。",
        "from": "T85n2802_001:0939a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初修業者應作是念。",
        "from": "T85n2802_001:0939a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我心云何得無散亂。",
        "from": "T85n2802_001:0939a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "意說我於何時得與不淨等無散亂法而相應也。",
        "from": "T85n2802_001:0939a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下之九種一一皆與唯作是念等相合說之　言無寂靜者。",
        "from": "T85n2802_001:0939a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂斷現行故　言猛寂靜者。",
        "from": "T85n2802_001:0939a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂斷種及現行故　言如是精勤等者。",
        "from": "T85n2802_001:0939a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與上十文一一相合　言即由如是等者。",
        "from": "T85n2802_001:0939a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂由內攝其心為因故。",
        "from": "T85n2802_001:0939a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五位行人於不淨等所緣境中不念淨等作意故也　言有相者。",
        "from": "T85n2802_001:0939a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂不淨相等也　言有分別者。",
        "from": "T85n2802_001:0939a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂不淨等有分別影像也　言謂由隨相行等者。",
        "from": "T85n2802_001:0939a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂行隨尋思等三種慧故　言彼境界者。",
        "from": "T85n2802_001:0939a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂依三種慧能觀不淨等五觀過患功德。",
        "from": "T85n2802_001:0939a25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而非但行不淨一觀者。",
        "from": "T85n2802_001:0939a26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故論文言而非一向等也　言還捨等者。",
        "from": "T85n2802_001:0939a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中有問。",
        "from": "T85n2802_001:0939a27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修五觀時唯用三慧為方便耶。",
        "from": "T85n2802_001:0939a27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為當處要餘法為方便耶。",
        "from": "T85n2802_001:0939a28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故論答云還相等也　言由於所緣等者。",
        "from": "T85n2802_001:0939a29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂若捨於止必當散亂故名為捨。",
        "from": "T85n2802_001:0939b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由九種住心止品攝故名為不捨　言即於所緣等者。",
        "from": "T85n2802_001:0939b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂由義等六事差別觀故。",
        "from": "T85n2802_001:0939b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於所治中不作其相不作我不起差別我我所心故。",
        "from": "T85n2802_001:0939b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名不取　言有於其中等者。",
        "from": "T85n2802_001:0939b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂由九種住心為因故。",
        "from": "T85n2802_001:0939b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而能安住不淨等觀　言取止行故等者。",
        "from": "T85n2802_001:0939b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂義等六事差別尋求不淨等故。",
        "from": "T85n2802_001:0939b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能為止品作其方便。",
        "from": "T85n2802_001:0939b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名止行能緣所知相也　言若於緣等者。",
        "from": "T85n2802_001:0939b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂勝解是觀。",
        "from": "T85n2802_001:0939b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除遣是止也　言由數勝解等者　言於狹小境等者。",
        "from": "T85n2802_001:0939b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂修不淨等五觀之時。",
        "from": "T85n2802_001:0939b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先觀指節名為狹小。",
        "from": "T85n2802_001:0939b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此漸次增長名為廣大。",
        "from": "T85n2802_001:0939b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後觀十方無量。",
        "from": "T85n2802_001:0939b12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故論文言廣大等也　言若諸法等者。",
        "from": "T85n2802_001:0939b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂青瘀等。",
        "from": "T85n2802_001:0939b13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十賢善定皆是有色影像也　言諸無色法等者。",
        "from": "T85n2802_001:0939b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂慈悲等四觀所有影像但是心法所變現故名非色也　言出離者。",
        "from": "T85n2802_001:0939b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂涅槃果也　言遠離者。",
        "from": "T85n2802_001:0939b16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂涅槃因也　言染污思慕者。",
        "from": "T85n2802_001:0939b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂貪著五欲等無出離心也　言不樂等者。",
        "from": "T85n2802_001:0939b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂涅槃因果法中無欣樂心生憂惱也　言下劣智見者。",
        "from": "T85n2802_001:0939b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂無間解脫道中證少分時便生高舉。",
        "from": "T85n2802_001:0939b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故論文言謂我能得等也　言廣說如前者。",
        "from": "T85n2802_001:0939b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯我能得等。",
        "from": "T85n2802_001:0939b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前出離地中已廣說故　言謂於隨順蓋等者。",
        "from": "T85n2802_001:0939b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂隨順五蓋處所法中。",
        "from": "T85n2802_001:0939b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非理作意名之為障。",
        "from": "T85n2802_001:0939b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尋思自舉准前說之。",
        "from": "T85n2802_001:0939b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此三皆由非理作意而生起故名因緣。",
        "from": "T85n2802_001:0939b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因緣者根本義也　言謂遍了知等者。",
        "from": "T85n2802_001:0939b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂由怯弱等四障為因緣故生起四種過患　言於其四種等者。",
        "from": "T85n2802_001:0939b26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一未證不證過患。",
        "from": "T85n2802_001:0939b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於無間解脫二道未證不證故也。",
        "from": "T85n2802_001:0939b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二退知失過患。",
        "from": "T85n2802_001:0939b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三壞加行過患。",
        "from": "T85n2802_001:0939b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四有染污等五法過患　言總用隨念等者。",
        "from": "T85n2802_001:0939b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂六隨念也　言用於精進等者。",
        "from": "T85n2802_001:0939c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂用於精進故治羸劣。",
        "from": "T85n2802_001:0939c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由平等故治身太過加行。",
        "from": "T85n2802_001:0939c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由通達故治初修過加行故　言用彼相違等者。",
        "from": "T85n2802_001:0939c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂蓋覆尋思自舉及不淨觀等五法對治故　言又遍了知等者。",
        "from": "T85n2802_001:0939c04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不了知諸障是障性者。",
        "from": "T85n2802_001:0939c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能修對治故。",
        "from": "T85n2802_001:0939c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以第一立遍知自性也　言遠離因緣者。",
        "from": "T85n2802_001:0939c07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂若不知諸障生起因緣者。",
        "from": "T85n2802_001:0939c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必不知自性也　言遠離因緣者。",
        "from": "T85n2802_001:0939c08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂若不知諸障生起因緣者。",
        "from": "T85n2802_001:0939c09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必不能修對治行故也　言隨順教者。",
        "from": "T85n2802_001:0939c10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂隨順觀所治法有多種故。",
        "from": "T85n2802_001:0939c11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能治之教亦有多種　故毘鉢舍那等者。",
        "from": "T85n2802_001:0939c11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於一一觀中一一止生故故言多也　言所知境者。",
        "from": "T85n2802_001:0939c12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂三門六事所觀察染淨品中因果義故　言身輕安等者。",
        "from": "T85n2802_001:0939c14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中有問。",
        "from": "T85n2802_001:0939c15_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五位行人於何時中如是二法得究竟耶。",
        "from": "T85n2802_001:0939c15_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故論答云若得轉依等也。",
        "from": "T85n2802_001:0939c16_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若阿羅漢轉染品依淨品時方得究竟　言所知事者。",
        "from": "T85n2802_001:0939c17_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂所治及能治道也　言能順貪欲等者。",
        "from": "T85n2802_001:0939c18_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂端正女色等所治法中。",
        "from": "T85n2802_001:0939c19_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初心不趣入。",
        "from": "T85n2802_001:0939c19_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中心不愛樂。",
        "from": "T85n2802_001:0939c20_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後心不信解　言乃至廣說者。",
        "from": "T85n2802_001:0939c20_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於隨順嗔恚違情境中。",
        "from": "T85n2802_001:0939c21_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初不趣入。",
        "from": "T85n2802_001:0939c21_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中不愛樂。",
        "from": "T85n2802_001:0939c22_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後不信解。",
        "from": "T85n2802_001:0939c22_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至齊此名為於慈愍觀。",
        "from": "T85n2802_001:0939c22_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已得究竟及未得究竟。",
        "from": "T85n2802_001:0939c23_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前不淨觀說也。",
        "from": "T85n2802_001:0939c23_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下三觀皆准此說　言隨順愚癡法者。",
        "from": "T85n2802_001:0939c24_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂三世緣起法中起常樂我四倒者順生愚癡法故　言非身聚相者。",
        "from": "T85n2802_001:0939c24_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂非從身中起一聚我想也　言戲論者。",
        "from": "T85n2802_001:0939c26_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂即多分別。",
        "from": "T85n2802_001:0939c27_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂欲覺等也　言法觀中等者。",
        "from": "T85n2802_001:0939c27_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂由等持為依止故。",
        "from": "T85n2802_001:0939c28_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於所緣境所治能治得緣境觀中增上慧轉　言普遍相者。",
        "from": "T85n2802_001:0939c28_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂不淨等五觀所緣也　言由此作意者。",
        "from": "T85n2802_001:0940a01_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂由不淨等五種作意為因故。",
        "from": "T85n2802_001:0940a02_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於淨常樂等五種可厭法中令心厭離也。",
        "from": "T85n2802_001:0940a02_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下三作意皆准此說　言於可忻尚法者。",
        "from": "T85n2802_001:0940a03_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂不淨等是最上法故　言由是緣等者。",
        "from": "T85n2802_001:0940a04_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂由不淨等五觀為因故。",
        "from": "T85n2802_001:0940a05_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能離身心所有淨相種子　言內心寂靜者。",
        "from": "T85n2802_001:0940a06_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂不淨等五觀也　言法觀者。",
        "from": "T85n2802_001:0940a07_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂五觀中所治能治增減相也　言漏者。",
        "from": "T85n2802_001:0940a07_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂淨等諸顛倒相也　言漏處法者。",
        "from": "T85n2802_001:0940a08_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂男女等可意及違情等境也　言生熱者。",
        "from": "T85n2802_001:0940a09_54",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於一分熱惱漏境中生厭離也　言等熱者。",
        "from": "T85n2802_001:0940a10_55",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂遍於諸漏熱惱境中生厭離相也。",
        "from": "T85n2802_001:0940a11_56",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下生厭等二句准此說云　言謂自衰損等者。",
        "from": "T85n2802_001:0940a12_57",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂五位行人若有自身及自眷屬喪失財利身遭衰損禍羅細等者。",
        "from": "T85n2802_001:0940a13_58",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名自衰損。",
        "from": "T85n2802_001:0940a15_59",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他亦同此。",
        "from": "T85n2802_001:0940a15_60",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是二境是修行人可厭處。",
        "from": "T85n2802_001:0940a15_61",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故論文言數思惟故等也。",
        "from": "T85n2802_001:0940a16_62",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂能思惟衰損之法而能自惱及惱他故　言若自興盛等者。",
        "from": "T85n2802_001:0940a16_63",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖然自身及與他有興盛事。",
        "from": "T85n2802_001:0940a18_64",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畢竟皆落謝之法盡法壞法滅法離散法變壞法故生厭離也　言津者。",
        "from": "T85n2802_001:0940a18_65",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂初與定相應也　言闊者。",
        "from": "T85n2802_001:0940a20_66",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂定漸潤次心也　言融適者。",
        "from": "T85n2802_001:0940a21_67",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後位也　言證淨者。",
        "from": "T85n2802_001:0940a21_68",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂身心輕安故也　言謂作是念等者。",
        "from": "T85n2802_001:0940a22_69",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此句與下二寶一一相合　言我今若德等者。",
        "from": "T85n2802_001:0940a23_70",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂由具戒德為伴呂故。",
        "from": "T85n2802_001:0940a24_71",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我若命終若殞沒當往善趣　言同梵行等者。",
        "from": "T85n2802_001:0940a24_72",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂顯六和敬如法　言有由前後等者。",
        "from": "T85n2802_001:0940a25_73",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂五位行人作是思惟。",
        "from": "T85n2802_001:0940a26_74",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我於不淨等五種觀中。",
        "from": "T85n2802_001:0940a27_75",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以精勤及後修習故。",
        "from": "T85n2802_001:0940a27_76",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必當獲得五觀之果。",
        "from": "T85n2802_001:0940a28_77",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故論文言由隨念法等也　言於能隨順等者。",
        "from": "T85n2802_001:0940a28_78",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂由調練作意為因故。",
        "from": "T85n2802_001:0940a29_79",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修不淨等觀之人。",
        "from": "T85n2802_001:0940b01_80",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於淨等漏法之中。",
        "from": "T85n2802_001:0940b01_81",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初不取向。",
        "from": "T85n2802_001:0940b02_82",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二能違逆三能喜樂。",
        "from": "T85n2802_001:0940b02_83",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四能離隔。",
        "from": "T85n2802_001:0940b02_84",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各住本定故也　言於可欣法等者。",
        "from": "T85n2802_001:0940b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂由不淨等可欣五觀悅潤心故。",
        "from": "T85n2802_001:0940b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於淨等顛倒之相。",
        "from": "T85n2802_001:0940b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初能出離。",
        "from": "T85n2802_001:0940b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二能令心取向。",
        "from": "T85n2802_001:0940b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三府舉。",
        "from": "T85n2802_001:0940b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四能喜樂和合。",
        "from": "T85n2802_001:0940b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此四與前不向等四法隨次對治　言於時時間等者。",
        "from": "T85n2802_001:0940b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂由生輕安為因故。",
        "from": "T85n2802_001:0940b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能發生四種慧行決擇染淨等諸法也　言用作業具者。",
        "from": "T85n2802_001:0940b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂用爐[皮*日*ㄆ]鉗錘等也　言如是勸修等者。",
        "from": "T85n2802_001:0940b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與下文一一相合　言彼彼等者。",
        "from": "T85n2802_001:0940b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於所修不淨等觀門之中。",
        "from": "T85n2802_001:0940b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一皆與止觀雙運故。",
        "from": "T85n2802_001:0940b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一能親府。",
        "from": "T85n2802_001:0940b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二能善和合。",
        "from": "T85n2802_001:0940b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三依定無轉謂其心不退。",
        "from": "T85n2802_001:0940b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四於定無動等也。",
        "from": "T85n2802_001:0940b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瑜伽論第三十一卷手記竟",
        "from": "T85n2802_001:0940b14_0",
        "context_with_next_sentence": false
    }
]