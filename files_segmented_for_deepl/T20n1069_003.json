[
    {
        "source_sentence": "十一面觀自在菩薩祕密心經",
        "from": "T20n1069_003:0146a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "語建立道場儀軌卷下",
        "from": "T20n1069_003:0146a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T20n1069_003:0146a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空謚大鑒",
        "from": "T20n1069_003:0146a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正號大廣智大興善寺三藏沙門",
        "from": "T20n1069_003:0146a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空奉　詔譯",
        "from": "T20n1069_003:0146a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今說成就處。",
        "from": "T20n1069_003:0146a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依教擇得地。",
        "from": "T20n1069_003:0146a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉日吉宿吉曜。",
        "from": "T20n1069_003:0146a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨其地離諸過患。",
        "from": "T20n1069_003:0146a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晨朝歡喜心攝受地作辟除法。",
        "from": "T20n1069_003:0146a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應作是言。",
        "from": "T20n1069_003:0146a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有於此地方障礙者。",
        "from": "T20n1069_003:0146a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應遠離。",
        "from": "T20n1069_003:0146a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "午時面南應作辟除法。",
        "from": "T20n1069_003:0146a27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒佉陀羅木。",
        "from": "T20n1069_003:0146a27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以芥子油投白芥子護摩。",
        "from": "T20n1069_003:0146a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用甘露軍荼利。",
        "from": "T20n1069_003:0146a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛心密言曰。",
        "from": "T20n1069_003:0146a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿(引)密哩(二合)帝吽發",
        "from": "T20n1069_003:0146b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復以濕嚩嚩訶密言。",
        "from": "T20n1069_003:0146b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持水七遍灑地。",
        "from": "T20n1069_003:0146b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜應作息災護摩。",
        "from": "T20n1069_003:0146b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面北用心中心密言一百八遍。",
        "from": "T20n1069_003:0146b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及用濕嚩嚩訶密言。",
        "from": "T20n1069_003:0146b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手按地。",
        "from": "T20n1069_003:0146b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦淨地密言一百八遍。",
        "from": "T20n1069_003:0146b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1069_003:0146b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵步(引)欠(平)",
        "from": "T20n1069_003:0146b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取地隨意大小。",
        "from": "T20n1069_003:0146b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或九肘十三肘或十六肘。",
        "from": "T20n1069_003:0146b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深掘齊膝。",
        "from": "T20n1069_003:0146b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除其地中過患。",
        "from": "T20n1069_003:0146b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平治地分為九分。",
        "from": "T20n1069_003:0146b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中央置七寶五穀藥等。",
        "from": "T20n1069_003:0146b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "好時日以印密言加持。",
        "from": "T20n1069_003:0146b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則應結十方界二手內相叉。",
        "from": "T20n1069_003:0146b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指頭指小指各申相合。",
        "from": "T20n1069_003:0146b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旋轉十方密言曰。",
        "from": "T20n1069_003:0146b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵入嚩(二合)里多路者儞吽",
        "from": "T20n1069_003:0146b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以佉陀羅木作橛。",
        "from": "T20n1069_003:0146b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持一百八遍釘四角。",
        "from": "T20n1069_003:0146b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佉陀羅木橛密言曰。",
        "from": "T20n1069_003:0146b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵虎魯唵虎魯吽泮",
        "from": "T20n1069_003:0146b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以波囉捨木燃火。",
        "from": "T20n1069_003:0146b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以本尊密言和三甜。",
        "from": "T20n1069_003:0146b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護摩一百八遍。",
        "from": "T20n1069_003:0146b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則於道場中全身舍利塔東面。",
        "from": "T20n1069_003:0146b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安本尊像。",
        "from": "T20n1069_003:0146b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "像面向西。",
        "from": "T20n1069_003:0146b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應習先行法。",
        "from": "T20n1069_003:0146b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦以了欲出道場。",
        "from": "T20n1069_003:0146b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持處所。",
        "from": "T20n1069_003:0146b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手作金剛拳。",
        "from": "T20n1069_003:0146b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪頭指旋轉十方。",
        "from": "T20n1069_003:0146b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則成堅固精室。",
        "from": "T20n1069_003:0146b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護摩儀軌品",
        "from": "T20n1069_003:0146b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　我今說護摩　　密言諸儀則",
        "from": "T20n1069_003:0146b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　普通令歡喜　　去念誦處所",
        "from": "T20n1069_003:0146b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　不近亦不遠　　對彼道場前",
        "from": "T20n1069_003:0146b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　護摩如契經　　先應獻部尊",
        "from": "T20n1069_003:0146b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　次供養本尊　　供養火天已",
        "from": "T20n1069_003:0146b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　然後依所求　　息災作圓爐",
        "from": "T20n1069_003:0146b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　增益應為方　　降伏應三角",
        "from": "T20n1069_003:0146b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　敬愛如蓮葉　　爐中應安置",
        "from": "T20n1069_003:0146b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　輪金剛獨古　　第四金剛鉤",
        "from": "T20n1069_003:0146c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　次第而建立",
        "from": "T20n1069_003:0146c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀自在菩薩通增益法護摩之時。",
        "from": "T20n1069_003:0146c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迎觀自在大勢至義成就大威德者。",
        "from": "T20n1069_003:0146c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安置於爐東邊。",
        "from": "T20n1069_003:0146c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并持明仙一切藥叉及吉祥天。",
        "from": "T20n1069_003:0146c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應置爐南邊。",
        "from": "T20n1069_003:0146c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又於爐北邊安佛并諸不退轉菩薩梵王并訶利底母。",
        "from": "T20n1069_003:0146c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求增益成就者。",
        "from": "T20n1069_003:0146c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當供養。",
        "from": "T20n1069_003:0146c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應迎觀自在密言曰。",
        "from": "T20n1069_003:0146c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵吠那勿(微一反)娑嚩(二合引)訶次大勢至密言曰。",
        "from": "T20n1069_003:0146c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵底瑟咤(二合)底瑟咤(二合下同)摩訶娑佗(二合引)麼吠誐三麼也麼努娑麼(二合)囉吽泮娑嚩(二合)訶(引)次義成就密言曰。",
        "from": "T20n1069_003:0146c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵悉[亭*夜]賀悉[亭*夜]娑(引)馱也娑嚩(二合引)訶(引)持明仙密言曰。",
        "from": "T20n1069_003:0146c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵訖哩(二合下同)拏尾訖哩(二合)拏尾訖哩(二合)尼多(引)也娑嚩(二合引)訶(引)藥叉眾密言曰。",
        "from": "T20n1069_003:0146c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥乞叉多(入)",
        "from": "T20n1069_003:0147a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切吉祥心密言曰。",
        "from": "T20n1069_003:0147a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨摩訶室哩(二合)夜(引)也唵止哩弭里膩曳娑嚩(二合引)訶梵王密言曰。",
        "from": "T20n1069_003:0147a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵鉢納麼(二合)喻曩曳娑嚩(二合引)訶(引)一切佛菩薩密言曰。",
        "from": "T20n1069_003:0147a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫薩嚩沒馱[曰/月]地薩怛嚩(二合引)南(引)阿(引)尾(引)囉吽欠(平)西邊應置白衣觀自在密言曰。",
        "from": "T20n1069_003:0147a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵濕吠(二合)帝濕吠(二合)帝半拏囉嚩(引)悉儞娑嚩(二合引)訶(引)訶唎底母密言曰。",
        "from": "T20n1069_003:0147a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵努努摩(引)里迦呬帝娑嚩(二合引)訶(引)如上建立名為增益儀軌。",
        "from": "T20n1069_003:0147a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　息災面向北　　南邊應置矟",
        "from": "T20n1069_003:0147a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　西安嚩素枳　　北置金剛印",
        "from": "T20n1069_003:0147a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　爐東邊應置　　三戟叉大印",
        "from": "T20n1069_003:0147a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　各以本密言　　呼召及發遣",
        "from": "T20n1069_003:0147a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "召三戟叉密言。",
        "from": "T20n1069_003:0147a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各以大指押小指甲。",
        "from": "T20n1069_003:0147a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散餘三指如叉。",
        "from": "T20n1069_003:0147a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便相合之。",
        "from": "T20n1069_003:0147a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨囉怛曩(二合)怛囉(二合)夜也曩莫室戰(二合)拏嚩日囉(二合)播拏曳摩訶藥乞叉(二合)細曩跛多曳．曀係曳呬摩訶藥乞叉(二合)嚕捺囉(二合下同)婆孕羯羅三麼焰鉢羅(二合)底播(引)攞也阿(引)蘖縒嚕捺羅(二合)麼麼曼拏禮嚩日囉(二合)三麼也歷努播攞也伊只枳弭里枳弭里娑嚩(二引引)訶發遣密言曰。",
        "from": "T20n1069_003:0147a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘖縒蘖縒嚕捺囉(二合)娑嚩(二合)娑嚩(二合)南補曩囉誐莽曩(引)夜(引)囉他(二合)悉馱曳止里枳娑嚩(二合引)訶召嚩蘇枳龍王。",
        "from": "T20n1069_003:0147b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰如常拳。",
        "from": "T20n1069_003:0147b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指頭指相捻如環。",
        "from": "T20n1069_003:0147b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1069_003:0147b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚩(引)蘇枳曩(引)誐囉惹嚩日囉(二合)三麼也麼努娑麼(二合)囉阿(引)蘖縒答多嚩(引)魯迦(引)婆也避多麼曼荼覽勢典虎魯虎魯阿(引)蘖縒娑嚩(二合引)訶(引)發遣密言曰。",
        "from": "T20n1069_003:0147b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孽縒孽縒娑嚩(二合引)婆嚩南曩(引)誐囉惹枳孕(二合)迦哩麼麼迦(引)麼(引)娑密哩(二合)[亭*夜]覩娑嚩(二合引)訶(引)請金剛杵密言。",
        "from": "T20n1069_003:0147b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉。",
        "from": "T20n1069_003:0147b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二中指相合。",
        "from": "T20n1069_003:0147b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二大指二小指屈。",
        "from": "T20n1069_003:0147b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指各附二中指背。",
        "from": "T20n1069_003:0147b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不相著。",
        "from": "T20n1069_003:0147b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿演嚩日囉(二合)麼訶具囉薩嚩咄瑟咤(二合)娑夜跛赦阿嚩地也(二合)薩嚩泥嚩(引)南(引)嚩日囉(二合)訶娑多(二合)婆孕羯囉阿(引)孽縒施仡囕(二合)麼麼迦(引)哩焰娑蜜哩(三合)[亭*夜]覩印捺囉(二合)娑訶(二合)娑囉(二合引)乞叉(二合)覩魯覩魯阿(引)孽縒娑嚩(二合引)訶(引)發遣金剛杵密言曰。",
        "from": "T20n1069_003:0147b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孽縒孽縒娑嚩(二合)婆嚩南嚩日囉(二合)訶娑多(二合)摩訶麼羅阿鉢囉(二合)地哩(二合)沙夜(二合)阿素(上)囕孽薩嚩(引)曩尾近曩(二合)覩娑那(引)婆嚩(引)跛囉爾儞娑嚩(二合引)訶(引)請梵天密言。",
        "from": "T20n1069_003:0147b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手虛心合掌。",
        "from": "T20n1069_003:0147c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開屈頭中名等六指。",
        "from": "T20n1069_003:0147c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如蓮葉。",
        "from": "T20n1069_003:0147c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比多(引)摩訶嚩覽劍麼攞嚩曩儞嚩(引)徙阿孽縒嚩日囉(二合)曼拏攞三麼也麼努播攞也鉢納弭(二合)儞那鉢納麼(二合)嚩悉儞(二合)娑嚩(二合引)訶(引)次用請毘紐天密言。",
        "from": "T20n1069_003:0147c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持茅為環。",
        "from": "T20n1069_003:0147c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安爐右。",
        "from": "T20n1069_003:0147c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發遣時應解請毘紐天密言。",
        "from": "T20n1069_003:0147c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手反相叉。",
        "from": "T20n1069_003:0147c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指頭相拄。",
        "from": "T20n1069_003:0147c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安右掌中。",
        "from": "T20n1069_003:0147c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吠(微愛反)濕嚩(二合)泥嚩摩賀鉢囉(二合)訖穰(二合)摩訶尾哩也(二合)跛囉訖囉(二合)麼阿孽縒孽嚕拏莽(引)嚕呬也(二合)斫訖[口*隸](二合)拏三麼也曼拏覽三麼也麼努播攞也娑嚩(二合引)訶(引)行者坐處右邊。",
        "from": "T20n1069_003:0147c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應置護摩支分。",
        "from": "T20n1069_003:0148a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酪蘇香花等。",
        "from": "T20n1069_003:0148a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一器中滿置。",
        "from": "T20n1069_003:0148a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種子應安右邊。",
        "from": "T20n1069_003:0148a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此水天密言加持水。",
        "from": "T20n1069_003:0148a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散灑。",
        "from": "T20n1069_003:0148a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T20n1069_003:0148a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿演嚩嚕拏烏(二合)曩(引)誐跛哩嚩(引)[口*路]曩(引)誐麼攞三麼儞庾(二合)底(入)冥儞儞(引寧反引)信者也娑吠(二合)底薩嚩烏那迦目佉(引)尾近曩(二合)莽(引)婆挽覩次取茅草密言曰。",
        "from": "T20n1069_003:0148a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伊冥矩鑠(引)儞尾夜(二合)室者(二合)布多(引)室者(二合)沒囉(二合)紇莽(二合)跛尾底[口*隸](二合)拏沒馱達磨僧伽囉多(引)北哩體(丁以反)尾散惹(引)多孽婆(引)莽(引)尾延南(二合)扇(引)蹉阿尾近南(二合)君(入)挽覩娑嚩(二合引)訶(引)　順敷吉祥茅　　東方為先首　南西最後北　　當以梢壓根　勿以根壓梢　　散布諸名花　遍嚴於茅上　　應用毘俱胝印密言加持曰。",
        "from": "T20n1069_003:0148a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫薩嚩怛他孽帝毘庾(二合)囉賀(二合)毘藥(二合)三藐三沒第毘藥(二合)唵婆也曩(引)捨儞怛囉(二合)娑儞怛囉(二合)細怛囉(二合)娑也毘[口*呂](二合)矩致多致吠多致吠多致濕吠(二合)帝惹致儞(二合)娑嚩(二合引)訶(引)印如前說。",
        "from": "T20n1069_003:0148a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以辦事密言灑火。",
        "from": "T20n1069_003:0148a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應然木依教然火密言曰。",
        "from": "T20n1069_003:0148a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵入嚩(二合)攞吽",
        "from": "T20n1069_003:0148a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或用濕嚩(二合)嚩訶密言。",
        "from": "T20n1069_003:0148a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灑火。",
        "from": "T20n1069_003:0148b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或用軍荼利。",
        "from": "T20n1069_003:0148b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依護摩儀則。",
        "from": "T20n1069_003:0148b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初中應用濕嚩嚩訶及軍荼利印密言。",
        "from": "T20n1069_003:0148b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以說。",
        "from": "T20n1069_003:0148b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以本明密言加持花。",
        "from": "T20n1069_003:0148b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀想投於爐中。",
        "from": "T20n1069_003:0148b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如教應思惟。",
        "from": "T20n1069_003:0148b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先投三莖木。",
        "from": "T20n1069_003:0148b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先應請火天以本印。",
        "from": "T20n1069_003:0148b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後作護摩。",
        "from": "T20n1069_003:0148b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手作施無畏。",
        "from": "T20n1069_003:0148b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微屈頭指招召。",
        "from": "T20n1069_003:0148b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指屈右掌中密言曰。",
        "from": "T20n1069_003:0148b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三滿多入嚩(二引合)攞摩訶入嚩(二合引)攞阿(引)儞底也(二合)三麼鉢囉(二合)婆娑惹(引)多吠那迦比羅慕(引)囉多曳阿孽縒曼拏覽沒度步佉者囉三娑嚩(二合引)訶(引)請已先以大杓三沃火。",
        "from": "T20n1069_003:0148b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後旋灑。",
        "from": "T20n1069_003:0148b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與火天漱口次灑淨。",
        "from": "T20n1069_003:0148b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則應以小杓供養本尊。",
        "from": "T20n1069_003:0148b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以木兩頭搵酥投火中獻。",
        "from": "T20n1069_003:0148b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次油麻次酪乳蜜等。",
        "from": "T20n1069_003:0148b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以後隨意。",
        "from": "T20n1069_003:0148b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸香藥應燒。",
        "from": "T20n1069_003:0148b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中以油麻粳米和酥燒為勝。",
        "from": "T20n1069_003:0148b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如上眾緣不具。",
        "from": "T20n1069_003:0148b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但燒酥密言後安娑嚩訶。",
        "from": "T20n1069_003:0148b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦得一切成就。",
        "from": "T20n1069_003:0148b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護摩已了。",
        "from": "T20n1069_003:0148b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火以水[瀟肅+即]灑。",
        "from": "T20n1069_003:0148b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令聖眾漱口。",
        "from": "T20n1069_003:0148b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為令聖眾歡喜故。",
        "from": "T20n1069_003:0148b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用本部心加持閼伽而獻之。",
        "from": "T20n1069_003:0148b17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定心合掌殷重心求悉地。",
        "from": "T20n1069_003:0148b18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以閼伽奉送依法以水灑火用辦事密言。",
        "from": "T20n1069_003:0148b19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有護摩殘物隨意樂供養外諸天。",
        "from": "T20n1069_003:0148b20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以前請火天印。",
        "from": "T20n1069_003:0148b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭指大指相捻。",
        "from": "T20n1069_003:0148b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發遣火天密言曰。",
        "from": "T20n1069_003:0148b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孽縒入嚩(二合引)攞娑那嘮捺囉(二合)阿鉢囉(二合)地唱(二合)史也(二合)蘇囉蘇囉孽縒娑嚩娑嚩南入嚩(二合引)攞三莽扇(引)井婆嚩底娑那(引)底儞枳抳嘮儞哩(二合)抳娑嚩(二合引)訶(引)獻閼伽奉送聖眾。",
        "from": "T20n1069_003:0148b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以前大護印密言左旋解界。",
        "from": "T20n1069_003:0148b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及寶輅印密言奉送聖眾。",
        "from": "T20n1069_003:0148b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　我今次第說　　護摩焰色相",
        "from": "T20n1069_003:0148c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　密語者由知　　速疾獲悉地",
        "from": "T20n1069_003:0148c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如虹霓白色　　珊瑚光莊嚴",
        "from": "T20n1069_003:0148c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　右旋妙滋潤　　煥爛若虹霓",
        "from": "T20n1069_003:0148c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　映赤吠瑠璃　　如護摩杓形",
        "from": "T20n1069_003:0148c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　三叉杵吉子　　商佉蓮拂形",
        "from": "T20n1069_003:0148c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　幢蓋羯囉捨　　娑嚩悉底迦",
        "from": "T20n1069_003:0148c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　其聲如笛鼓　　妙香極悅意",
        "from": "T20n1069_003:0148c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若見如是焰　　無垢離諸障",
        "from": "T20n1069_003:0148c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　行者應慇重　　應當求成就",
        "from": "T20n1069_003:0148c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　焰若一峯勝　　二峯為中相",
        "from": "T20n1069_003:0148c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　三峯下成就　　求成者應察",
        "from": "T20n1069_003:0148c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　次說不成相　　弊惡障嚴飾",
        "from": "T20n1069_003:0148c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　左旋極臭氣　　屍臭驢鳴聲",
        "from": "T20n1069_003:0148c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　數吐焰斷絕　　烟聚令怖聲",
        "from": "T20n1069_003:0148c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　糞器形乾濕　　焰散舐掠形",
        "from": "T20n1069_003:0148c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若見護摩相　　智者應審知",
        "from": "T20n1069_003:0149a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　密言者速疾　　辨事軍荼利",
        "from": "T20n1069_003:0149a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　以水應數灑　　能除不吉祥",
        "from": "T20n1069_003:0149a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　是故一切時　　應用甘露尊",
        "from": "T20n1069_003:0149a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時觀自在菩薩摩訶薩說此法已。",
        "from": "T20n1069_003:0149a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切大眾咸共讚言善哉善哉大士。",
        "from": "T20n1069_003:0149a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃能為欲利益安樂諸有情故。",
        "from": "T20n1069_003:0149a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此密語。",
        "from": "T20n1069_003:0149a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等隨喜亦願受持。",
        "from": "T20n1069_003:0149a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時大眾歡喜踊躍遶佛三匝作禮而去。",
        "from": "T20n1069_003:0149a08_5",
        "context_with_next_sentence": false
    }
]