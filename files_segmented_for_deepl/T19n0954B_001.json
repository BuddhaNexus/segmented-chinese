[
    {
        "source_sentence": "一字頂輪王念誦儀軌(依忉利天宮所說經譯)我今說無比力超勝世間出世間真言上上。",
        "from": "T19n0954B_001:0310c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切佛頂王佛頂輪王念誦儀則。",
        "from": "T19n0954B_001:0310c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者先當清淨。",
        "from": "T19n0954B_001:0310c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於淨處安本尊像。",
        "from": "T19n0954B_001:0311a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面西稽首禮。",
        "from": "T19n0954B_001:0311a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受三歸捨身說罪受戒。",
        "from": "T19n0954B_001:0311a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發菩提心隨喜勸請發願廻向已。",
        "from": "T19n0954B_001:0311a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應結佛部三昧耶印。",
        "from": "T19n0954B_001:0311a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四頂互內交。",
        "from": "T19n0954B_001:0311a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二輪並合竪。",
        "from": "T19n0954B_001:0311a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前附於蓋。",
        "from": "T19n0954B_001:0311a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名一切如來心印真言曰。",
        "from": "T19n0954B_001:0311a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引一)邇那匿(而翼切二)",
        "from": "T19n0954B_001:0311a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結蓮華三昧耶印。",
        "from": "T19n0954B_001:0311a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前印。",
        "from": "T19n0954B_001:0311a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左輪屈入掌中。",
        "from": "T19n0954B_001:0311a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右輪如前竪。",
        "from": "T19n0954B_001:0311a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名蓮華部心印真言曰。",
        "from": "T19n0954B_001:0311a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引一)阿嚕力(二)",
        "from": "T19n0954B_001:0311a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結金剛部三昧耶印。",
        "from": "T19n0954B_001:0311a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前蓮華部心印。",
        "from": "T19n0954B_001:0311a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右輪屈入掌中。",
        "from": "T19n0954B_001:0311a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左輪依前竪。",
        "from": "T19n0954B_001:0311a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名金剛部心印真言曰。",
        "from": "T19n0954B_001:0311a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引一)嚩日囉(二合)地力(二合二)次結甲冑印准根本印。",
        "from": "T19n0954B_001:0311a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋拄二光。",
        "from": "T19n0954B_001:0311a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "背印額右肩左肩心喉五處。",
        "from": "T19n0954B_001:0311a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0954B_001:0311a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引一)斫羯囉(二合)靺[口*栗]底(二合二)鉢囉(二合)賒弭多(三)囉捺囉(二合引)囉捺囉(二合引四)婆(去聲)娑摩(二合)車盧瑟尼(二合)沙(五)囉乞沙(二合)囉乞沙(二合)[牟*含](六)吽癹娑嚩(二合合)訶(引七)次結佛眼印。",
        "from": "T19n0954B_001:0311a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手合掌。",
        "from": "T19n0954B_001:0311a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋各拄光背。",
        "from": "T19n0954B_001:0311a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二輪屈入掌。",
        "from": "T19n0954B_001:0311a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印五處真言曰。",
        "from": "T19n0954B_001:0311a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南(一)唵(引二)嚕嚕薩怖(二合)嚕(三)入嚩(二合)攞底瑟姹(二合四)悉馱盧者寧(五)薩嚩喇他(二合)娑達尼娑嚩(二合引)訶(引六)次結大海印。",
        "from": "T19n0954B_001:0311a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉。",
        "from": "T19n0954B_001:0311a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仰掌擘開二輪。",
        "from": "T19n0954B_001:0311a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋三匝想大海真言曰。",
        "from": "T19n0954B_001:0311a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引一)微摩盧娜地娑嚩(二合引)訶(引二)次於大海中想須彌盧山。",
        "from": "T19n0954B_001:0311a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四寶所成。",
        "from": "T19n0954B_001:0311a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉急握豎拳真言曰。",
        "from": "T19n0954B_001:0311a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引一)阿者攞吽(二)",
        "from": "T19n0954B_001:0311b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次於須彌盧山上想寶樓閣。",
        "from": "T19n0954B_001:0311b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持寶樓閣印。",
        "from": "T19n0954B_001:0311b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印二手金剛掌真言曰。",
        "from": "T19n0954B_001:0311b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南(引一)唵薩嚩他欠(二)鄔娜哦(二合)帝薩頗(二合)囉呬(引)[牟*含](三)哦哦那檢娑嚩(二合)訶(引四)次結佛頂輪王印。",
        "from": "T19n0954B_001:0311b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二掌內相叉作拳。",
        "from": "T19n0954B_001:0311b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二光屈上節如劍。",
        "from": "T19n0954B_001:0311b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並竪二輪屈二蓋兩節。",
        "from": "T19n0954B_001:0311b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相拄於二輪上。",
        "from": "T19n0954B_001:0311b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印五處加護真言曰。",
        "from": "T19n0954B_001:0311b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南(一)步嚕唵(三合二)次結網橛印。",
        "from": "T19n0954B_001:0311b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0311b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋上節。",
        "from": "T19n0954B_001:0311b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "背不相著。",
        "from": "T19n0954B_001:0311b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二輪各壓。",
        "from": "T19n0954B_001:0311b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上下揮轉。",
        "from": "T19n0954B_001:0311b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成結上下界真言曰。",
        "from": "T19n0954B_001:0311b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南(一)阿鉢囉(二合)底呵多舍娑娜南(二)唵(引三)微枳囉拏微特防(去聲二合)娑尼(四)迦比羅貳縛里尼(五)怛囉(二合)娑耶(六)嚩日囉(二合)吠賒薩帝呶(引)囉特縛(二合七)能(上聲)瑟吒囉(三合)囉乞沙(二合)[牟*含]癹(八)次結牆印。",
        "from": "T19n0954B_001:0311b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0311b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋兩節相逼。",
        "from": "T19n0954B_001:0311b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平竪二輪附二蓋。",
        "from": "T19n0954B_001:0311b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋三匝即成。",
        "from": "T19n0954B_001:0311b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛牆界真言曰(歸命同前)。",
        "from": "T19n0954B_001:0311b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "噁(引一)莫壑(二)",
        "from": "T19n0954B_001:0311b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結車輅印。",
        "from": "T19n0954B_001:0311b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉。",
        "from": "T19n0954B_001:0311b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仰掌申二蓋。",
        "from": "T19n0954B_001:0311b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令甲側相拄。",
        "from": "T19n0954B_001:0311b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二輪拄蓋根下。",
        "from": "T19n0954B_001:0311b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想於他方世界奉迎本尊。",
        "from": "T19n0954B_001:0311b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0954B_001:0311b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引一)覩嚕覩嚕吽(二)",
        "from": "T19n0954B_001:0311b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結迎輅印。",
        "from": "T19n0954B_001:0311b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准車輅印。",
        "from": "T19n0954B_001:0311b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二輪撥光頭。",
        "from": "T19n0954B_001:0311b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向身三招。",
        "from": "T19n0954B_001:0311b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0954B_001:0311b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發遣除羯哩灑(二合)耶加尾薩哩惹耶娜謨薩底哩野(三合)地尾(二合)迦南(一)怛他孽哆南(二)唵(引三)嚩日啷(二合)儗(愚以切)儞也(二合)羯哩灑耶娑嚩(二合引)訶(引四)次結迎請印。",
        "from": "T19n0954B_001:0311b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0311c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈右蓋於光後。",
        "from": "T19n0954B_001:0311c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向前三招。",
        "from": "T19n0954B_001:0311c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0954B_001:0311c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨婆誐嚩帝(一)阿鉢囉(二合)底呵覩瑟尼(二合)沙耶(二)翳呬翳(二合)娑誐刎(三)達麼囉惹(平聲)鉢囉(二合)底車(鴟奚切)南(四)遏鉗巘談補瑟貶(逋諳切)度貶(同上)末隣儞半者(五)滿遮避洛乞叉(二合六)阿鉢囉(二合)底呵多(七)麼囉鉢囉(引)羯囉(二合)麼耶娑嚩(二合引)訶(引八)次結一切辨事佛頂印。",
        "from": "T19n0954B_001:0311c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉。",
        "from": "T19n0954B_001:0311c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二光如幢。",
        "from": "T19n0954B_001:0311c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸供養物及浴水洗淨土等。",
        "from": "T19n0954B_001:0311c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並用此真言。",
        "from": "T19n0954B_001:0311c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持辟除去垢結界皆用此真言。",
        "from": "T19n0954B_001:0311c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左旋辟除右旋結界真言曰。",
        "from": "T19n0954B_001:0311c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三滿多勃馱南(一)唵(引二)吒嚕嚕(二合三)滿馱娑嚩(二合引)訶(引四)次重結前網橛印。",
        "from": "T19n0954B_001:0311c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結上方界。",
        "from": "T19n0954B_001:0311c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結阿娑莽倪(移枳切)尼印。",
        "from": "T19n0954B_001:0311c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "側二手。",
        "from": "T19n0954B_001:0311c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左掩右。",
        "from": "T19n0954B_001:0311c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅二輪右轉一匝。",
        "from": "T19n0954B_001:0311c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成密縫真言曰。",
        "from": "T19n0954B_001:0311c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引一)阿娑莽倪尼吽癹(二)",
        "from": "T19n0954B_001:0311c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結獻閼伽印。",
        "from": "T19n0954B_001:0311c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0311c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋相拄。",
        "from": "T19n0954B_001:0311c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "附二光。",
        "from": "T19n0954B_001:0311c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二輪各附蓋根側真言曰。",
        "from": "T19n0954B_001:0311c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南(一)唵(引二)閼伽囉訶(三)閼伽必哩(二合)野(四)鉢囉(二合)底車(鴟奚切)娜末鉗娑嚩(二合引)訶(引五)次重結根本印。",
        "from": "T19n0954B_001:0311c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結獻師子座印。",
        "from": "T19n0954B_001:0311c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0311c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋於二輪甲側。",
        "from": "T19n0954B_001:0311c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0954B_001:0311c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引一)阿者囉尾囉耶娑嚩(二合引)訶(引二)次結塗香印。",
        "from": "T19n0954B_001:0312a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0312a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈右蓋。",
        "from": "T19n0954B_001:0312a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倚於右光下節真言曰。",
        "from": "T19n0954B_001:0312a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南(一)唵(引二)怛嚟(二合)盧枳也(二合)獻馱孽帝(三)吽吽癹癹娑嚩(二合引)訶(引四)次結獻華印。",
        "from": "T19n0954B_001:0312a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前塗香印。",
        "from": "T19n0954B_001:0312a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "改左蓋。",
        "from": "T19n0954B_001:0312a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倚右光下節真言曰。",
        "from": "T19n0954B_001:0312a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南(一)唵(引二)薩嚩盧迦補瑟波(二合)步哆耶(三)吽吽癹癹娑嚩(二合引)訶(引四)次結燒香印。",
        "from": "T19n0954B_001:0312a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0312a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋。",
        "from": "T19n0954B_001:0312a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各倚於光下節真言曰。",
        "from": "T19n0954B_001:0312a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南(一)唵(引二)尾囉孽哆微哦哆度麼耶(三)吽吽癹癹娑嚩(二合引)訶(引四)次結獻食印。",
        "from": "T19n0954B_001:0312a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0312a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋上節。",
        "from": "T19n0954B_001:0312a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各附於輪側真言曰。",
        "from": "T19n0954B_001:0312a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南(一)唵(引二)薩嚩盧迦麼里必哩(二合)夜耶(三)吽吽癹癹娑嚩(二合引)訶(引四)次結獻燈明印。",
        "from": "T19n0954B_001:0312a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0312a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋兩節令不相著二輪各附於蓋上真言曰。",
        "from": "T19n0954B_001:0312a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南(一)唵(引二)薩嚩盧迦珊捺囉(二合)捨娜耶(三)吽吽癹癹娑嚩(二合引)訶(引四)次結普供養加持印。",
        "from": "T19n0954B_001:0312a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手虛心合掌。",
        "from": "T19n0954B_001:0312a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五頂各兩節相交真言曰。",
        "from": "T19n0954B_001:0312a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨薩嚩勃馱冒地薩怛嚩(二合)南(一)唵(引二)薩嚩怛囉(二合)僧俱蘇弭多(三)避吉惹囉始那慕薩低娑嚩(二合引)訶(引四)次結遍照佛頂印。",
        "from": "T19n0954B_001:0312a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉合為拳。",
        "from": "T19n0954B_001:0312a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令二光節微起真言曰。",
        "from": "T19n0954B_001:0312a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南(一)噁(引二)莫含(呼頷切三)次結白傘蓋佛頂印。",
        "from": "T19n0954B_001:0312a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二輪各捻二高甲上。",
        "from": "T19n0954B_001:0312b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "側相合。",
        "from": "T19n0954B_001:0312b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二蓋屈如蓋形。",
        "from": "T19n0954B_001:0312b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二光微屈相合。",
        "from": "T19n0954B_001:0312b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二勝各竪相合真言曰。",
        "from": "T19n0954B_001:0312b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南(一)阿鉢囉(二合)底呵多舍娑娜南(二)唵(引三)麼麼麼吽匿(儞翼切四)次結光聚佛頂印。",
        "from": "T19n0954B_001:0312b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前白傘蓋印。",
        "from": "T19n0954B_001:0312b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坼開二蓋真言曰。",
        "from": "T19n0954B_001:0312b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南(一)阿鉢囉(二合)底呵多舍娑娜南(二)唵(引三)怛他蘖都瑟尼(二合)沙(四)阿娜嚩盧枳多慕[口*栗]馱(二合五)帝儒囉始吽(六)入嚩(二合)攞入嚩(二合)攞(七)馱迦馱迦(八)娜囉微娜囉(九)瞋那牝那吽吽癹癹娑嚩(二合引)訶(引十)次結高佛頂印。",
        "from": "T19n0954B_001:0312b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前白傘蓋印。",
        "from": "T19n0954B_001:0312b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋拄二光中節背真言曰。",
        "from": "T19n0954B_001:0312b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南(一)阿鉢囉(二合)底呵多舍娑娜南(二)唵(引三)儞[卑*也]儞卑渝(二合)娜哦都瑟尼(二合)沙(四)吽吽癹癹娑嚩(二合引)訶(引五)次結勝佛頂印。",
        "from": "T19n0954B_001:0312b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准高佛頂印。",
        "from": "T19n0954B_001:0312b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "移二蓋向上。",
        "from": "T19n0954B_001:0312b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩穬麥許真言曰。",
        "from": "T19n0954B_001:0312b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南(一)阿鉢囉(二合)底呵多舍娑娜南(二)唵(引三)入嚩(二合)羅惹踰瑟尼(二合)沙(四)吽癹娑嚩(二合引)訶(引五)次結摧毀佛頂(尾枳囉拏)印。",
        "from": "T19n0954B_001:0312b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉作拳。",
        "from": "T19n0954B_001:0312b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二光屈上節。",
        "from": "T19n0954B_001:0312b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右光拄左光面。",
        "from": "T19n0954B_001:0312b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令出半節許真言曰。",
        "from": "T19n0954B_001:0312b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南(一)阿鉢囉(二合)底呵多舍娑娜南(二)唵(引三)尾枳囉那(四)度那度那犢(引五)次結摧碎佛頂印。",
        "from": "T19n0954B_001:0312b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並准前。",
        "from": "T19n0954B_001:0312b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "改左光拄右光面。",
        "from": "T19n0954B_001:0312b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦出半節許。",
        "from": "T19n0954B_001:0312b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0954B_001:0312b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南(一)阿鉢囉(二合)底呵多舍娑娜南(二)唵(引二)阿鉢囉(二合)底呵都瑟尼(二合)沙耶(四)薩嚩尾伽那(二合)尾持望(二合)娑那迦囉耶(五)怛嚧(二合)吒耶娑嚩(二合引)訶(引六)次結輪王佛頂心印。",
        "from": "T19n0954B_001:0312c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0312c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋拄二光上節節上真言曰。",
        "from": "T19n0954B_001:0312c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南(一)阿鉢囉(二合)底呵多舍娑娜南(二)唵(引三)怛他薩那瑟尼(二合)沙那嚩盧枳多(四)沒馱尼斫羯囉(二合)靺[口*栗]底(二合五)吽入嚩(二合)羅入嚩(二合)羅(六)馱迦馱迦(七)度那微度那(八)怛囉(二合)娑耶麼囉逾瑳囉耶(九)呵那呵那(十)伴惹伴惹(十一)暗音嶪鉢嚨(二合十二)企尼君吒哩尼(十三)阿鉢囉爾多薩怛囉馱哩尼(十四)吽癹娑嚩(二合引)訶(引十五)次結心中印。",
        "from": "T19n0954B_001:0312c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0312c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋加於二光上節上真言曰。",
        "from": "T19n0954B_001:0312c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南(一)阿鉢囉(一合)底呵多舍娑娜南(二)唵(引三)阿鉢囉(引)爾多特(地翼切四)普通諸佛頂印。",
        "from": "T19n0954B_001:0312c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手虛心金剛合掌。",
        "from": "T19n0954B_001:0312c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如華在掌中。",
        "from": "T19n0954B_001:0312c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者若怱遽不能遍結諸佛頂印。",
        "from": "T19n0954B_001:0312c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但結此印誦諸佛頂真言。",
        "from": "T19n0954B_001:0312c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結頂印。",
        "from": "T19n0954B_001:0312c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0312c21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈右蓋竪。",
        "from": "T19n0954B_001:0312c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於右光後。",
        "from": "T19n0954B_001:0312c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令不相著真言曰。",
        "from": "T19n0954B_001:0312c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨三漫多勃馱南(一)阿鉢囉(二合)底呵多舍娑娜南(二)唵(引三)斫羯囉(二合)靺[口*栗]底(二合)唵吽(四)次結頭印。",
        "from": "T19n0954B_001:0312c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0312c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開二蓋各直竪於光後。",
        "from": "T19n0954B_001:0312c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令不相著微屈真言曰(歸命同前)。",
        "from": "T19n0954B_001:0312c26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引一)斫羯囉(二合)靺[口*栗]底(二合二)吽癹娑嚩(二合引)訶(引三)次又結根本印。",
        "from": "T19n0954B_001:0312c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結大三昧耶印。",
        "from": "T19n0954B_001:0313a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加護本尊。",
        "from": "T19n0954B_001:0313a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉。",
        "from": "T19n0954B_001:0313a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二光。",
        "from": "T19n0954B_001:0313a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋於光後如鉤。",
        "from": "T19n0954B_001:0313a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相去一穬麥許。",
        "from": "T19n0954B_001:0313a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二輪各附蓋根下。",
        "from": "T19n0954B_001:0313a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋三匝真言曰。",
        "from": "T19n0954B_001:0313a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引一)商羯哩(二)摩訶三昧鹽娑嚩(二合引)訶(引三)次誦一百八名讚歎。",
        "from": "T19n0954B_001:0313a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲念誦。",
        "from": "T19n0954B_001:0313a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以五支成本尊。",
        "from": "T19n0954B_001:0313a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或五相成本尊瑜伽。",
        "from": "T19n0954B_001:0313a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於處處(頂舌心也)想一字頂輪。",
        "from": "T19n0954B_001:0313a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成本尊坐八葉蓮華。",
        "from": "T19n0954B_001:0313a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一一葉上想七寶。",
        "from": "T19n0954B_001:0313a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前蓮華葉上想佛眼尊。",
        "from": "T19n0954B_001:0313a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應持珠(此依菩提場所說經)。",
        "from": "T19n0954B_001:0313a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次持珠合掌。",
        "from": "T19n0954B_001:0313a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捧珠誦清珠真言七遍真言曰。",
        "from": "T19n0954B_001:0313a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引一)阿娜步(二合)低尾惹曳(而矣切二)悉地悉馱囉梯娑嚩(二合引)訶(引三)次結持珠印。",
        "from": "T19n0954B_001:0313a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手各以輪捻高甲。",
        "from": "T19n0954B_001:0313a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直竪二光二勝。",
        "from": "T19n0954B_001:0313a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋於光後。",
        "from": "T19n0954B_001:0313a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令不相著(如毘盧遮那經半金剛杵印)真言曰。",
        "from": "T19n0954B_001:0313a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨婆誐嚩底(一)蘇悉第娑馱耶(二)悉馱囉梯娑嚩(二合引)訶(引三)　次應淨其心　　如法而念誦　持珠令當心　　繫心於鼻端　字句分明呼　　不緩亦不急　不頻伸噦嗌　　咳嗽與唾涕　染等心相應　　及心緣苦受　如是等過患　　皆不得成就當念誦時。",
        "from": "T19n0954B_001:0313a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身心不得疲怠。",
        "from": "T19n0954B_001:0313a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若勞倦即應結五供養印。",
        "from": "T19n0954B_001:0313a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦讚歎獻閼伽。",
        "from": "T19n0954B_001:0313a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦畢持珠頂上。",
        "from": "T19n0954B_001:0313a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結前密縫印。",
        "from": "T19n0954B_001:0313a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左轉一匝即成解界。",
        "from": "T19n0954B_001:0313a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結奉送印。",
        "from": "T19n0954B_001:0313a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954B_001:0313a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左蓋外擲。",
        "from": "T19n0954B_001:0313a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦迎請真言。",
        "from": "T19n0954B_001:0313a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除翳呬翳呬。",
        "from": "T19n0954B_001:0313a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加孽車孽車句。",
        "from": "T19n0954B_001:0313a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成奉送。",
        "from": "T19n0954B_001:0313a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應復結牆及網橛等印。",
        "from": "T19n0954B_001:0313a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加護處上下及所成就物。",
        "from": "T19n0954B_001:0313a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結計里枳里印。",
        "from": "T19n0954B_001:0313b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左輪壓勝甲上。",
        "from": "T19n0954B_001:0313b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘三指頂坼開直竪。",
        "from": "T19n0954B_001:0313b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如三股杵形。",
        "from": "T19n0954B_001:0313b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋轉三匝成結界真言曰。",
        "from": "T19n0954B_001:0313b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引一)枳里枳里嚩日囉(二合)吽癹(二)次結軍吒利印。",
        "from": "T19n0954B_001:0313b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二勝於掌中交。",
        "from": "T19n0954B_001:0313b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二高壓。",
        "from": "T19n0954B_001:0313b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二輪壓高上。",
        "from": "T19n0954B_001:0313b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二光相合。",
        "from": "T19n0954B_001:0313b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二蓋於光後。",
        "from": "T19n0954B_001:0313b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令一麥許不相著。",
        "from": "T19n0954B_001:0313b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋三匝即成結界真言曰。",
        "from": "T19n0954B_001:0313b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨囉怛那(二合)怛囉(二合)夜耶(一)娜謨室戰(二合)拏嚩日囉(二合)波拏曳(二)摩訶藥乞叉(二合)細那波多曳(三)娜謨室戰(二合)拏嚩日囉(二合)句[口*路]馱耶(四)唵(引五)虎嚕虎嚕(六)底瑟姹(二合)底瑟姹(二合七)滿馱滿馱(八)呵那呵那(九)阿蜜哩(二合)帝吽癹娑嚩(二合引)訶(引十)從一字真言。",
        "from": "T19n0954B_001:0313b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至於十五字。",
        "from": "T19n0954B_001:0313b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每計於字數。",
        "from": "T19n0954B_001:0313b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一字一洛叉。",
        "from": "T19n0954B_001:0313b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至三十字。",
        "from": "T19n0954B_001:0313b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應誦三十洛叉。",
        "from": "T19n0954B_001:0313b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應作先事法。",
        "from": "T19n0954B_001:0313b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十字已上。",
        "from": "T19n0954B_001:0313b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應誦一萬遍。",
        "from": "T19n0954B_001:0313b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一字頂輪王念誦儀軌◎",
        "from": "T19n0954B_001:0313b18_0",
        "context_with_next_sentence": false
    }
]