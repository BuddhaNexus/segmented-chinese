[
    {
        "source_sentence": "諸教決定名義論",
        "from": "T32n1658_001:0507b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖慈氏菩薩造",
        "from": "T32n1658_001:0507b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝奉大夫試鴻臚卿",
        "from": "T32n1658_001:0507b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳法大師臣施護奉　詔譯",
        "from": "T32n1658_001:0507b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　歸命一切佛世尊　　歸命所說三乘法　歸命一切和合眾　　歸命普賢法界理今當略說一切教中諸根本字。",
        "from": "T32n1658_001:0507b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼如實義此中云何。",
        "from": "T32n1658_001:0507b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂。",
        "from": "T32n1658_001:0507b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵字最為上首。",
        "from": "T32n1658_001:0507b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今頂禮此字清淨住不二相。",
        "from": "T32n1658_001:0507b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人能以此字舌上轉者。",
        "from": "T32n1658_001:0507b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼得真實諸慧根本。",
        "from": "T32n1658_001:0507b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此字於一切教中甚深祕密。",
        "from": "T32n1658_001:0507b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有三字。",
        "from": "T32n1658_001:0507b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂金剛吽字而為正因。",
        "from": "T32n1658_001:0507b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從是字中宣說一切正法儀軌。",
        "from": "T32n1658_001:0507b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼三界心此法如是。",
        "from": "T32n1658_001:0507b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡字為說相。",
        "from": "T32n1658_001:0507b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盎字即空性。",
        "from": "T32n1658_001:0507b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即彼惡字復為慧母。",
        "from": "T32n1658_001:0507b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若道非道皆從是說。",
        "from": "T32n1658_001:0507b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦即法界文字根本遍入空性。",
        "from": "T32n1658_001:0507b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初後相應由此正智而得成就。",
        "from": "T32n1658_001:0507b28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若法有說皆悉成就。",
        "from": "T32n1658_001:0507b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼無所說亦復如是。",
        "from": "T32n1658_001:0507b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故一切造作皆悉平等。",
        "from": "T32n1658_001:0507b29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於輪迴中隨順而轉。",
        "from": "T32n1658_001:0507c01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所言法界文字此復云何。",
        "from": "T32n1658_001:0507c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故今說。",
        "from": "T32n1658_001:0507c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂。",
        "from": "T32n1658_001:0507c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦(訖當切)佉(去當切)誐(宜當切)伽(其當切)左(左當切)蹉(悽當切)惹([口*尒]當切)　酇(齊當切)吒(知江切)姹(敕江切)拏(尼江切)　茶(直江切)多(低剛切)他(梯剛切)那(泥剛切)　馱(蹄剛切)波(卑剛切)頗(紕剛切)摩(縻剛切)　婆(步剛切)羊囉羅罔桑[元*欠](呼郎切)。",
        "from": "T32n1658_001:0507c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是諸字。",
        "from": "T32n1658_001:0507c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即如前說彼盎字攝。",
        "from": "T32n1658_001:0507c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此相今說是即空性。",
        "from": "T32n1658_001:0507c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出生甚深一切教法。",
        "from": "T32n1658_001:0507c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此復名為空空。",
        "from": "T32n1658_001:0507c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出現一切說相。",
        "from": "T32n1658_001:0507c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂迦佉誐伽左蹉惹酇吒姹拏茶多他那馱波頗摩婆耶囉羅嚩薩賀。",
        "from": "T32n1658_001:0507c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是諸字即如前說彼惡字攝。",
        "from": "T32n1658_001:0507c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此相今說。",
        "from": "T32n1658_001:0507c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂即一一文字。",
        "from": "T32n1658_001:0507c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆從一切智智所生。",
        "from": "T32n1658_001:0507c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初相應行我法二種皆悉平等。",
        "from": "T32n1658_001:0507c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛加持究竟安住。",
        "from": "T32n1658_001:0507c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次一切事業皆從金剛三昧出生。",
        "from": "T32n1658_001:0507c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂。",
        "from": "T32n1658_001:0507c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦(引)佉(引)誐(引)伽(引)左(引)蹉(引)惹(引)酇(引)吒(引)姹(引)拏(引)茶(引)多(引)他(引)那(引)馱(引)波(引)頗(引平)摩(引)婆(引)耶(引)囉(引)羅(引)嚩(引)薩(引)賀(引)。",
        "from": "T32n1658_001:0507c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是諸字皆阿(引)字攝。",
        "from": "T32n1658_001:0507c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此相今說。",
        "from": "T32n1658_001:0507c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂一切金剛事業。",
        "from": "T32n1658_001:0507c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次今說。",
        "from": "T32n1658_001:0507c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烏(烏功切)污(烏貢切)壹(伊靈切)翳(伊證切)哩(黎丁切下同)哩梨(里丁切下同)梨伊(依綾切)愛(依崗切)唵(烏籠切)奧(烏當切)。",
        "from": "T32n1658_001:0507c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中唵字相今如是說。",
        "from": "T32n1658_001:0507c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂即一切文字從彼出生。",
        "from": "T32n1658_001:0507c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中吽字相即如前說。",
        "from": "T32n1658_001:0507c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切事業皆從金剛三昧出生。",
        "from": "T32n1658_001:0507c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次如前所說諸字。",
        "from": "T32n1658_001:0507c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂即三身。",
        "from": "T32n1658_001:0507c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若性若相如實安住。",
        "from": "T32n1658_001:0507c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂。",
        "from": "T32n1658_001:0507c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吽字即法身。",
        "from": "T32n1658_001:0507c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿字即報身。",
        "from": "T32n1658_001:0507c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵字即化身。",
        "from": "T32n1658_001:0507c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是三字攝此三身。",
        "from": "T32n1658_001:0507c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼分別說三乘解脫道。",
        "from": "T32n1658_001:0508a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為正說因。",
        "from": "T32n1658_001:0508a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有聲聞緣覺及一切智智。",
        "from": "T32n1658_001:0508a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由是出現說一切法。",
        "from": "T32n1658_001:0508a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即彼三字。",
        "from": "T32n1658_001:0508a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦是金剛三業如實安住。",
        "from": "T32n1658_001:0508a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂。",
        "from": "T32n1658_001:0508a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)阿(引)吽(引)。",
        "from": "T32n1658_001:0508a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中唵字是名金剛身業。",
        "from": "T32n1658_001:0508a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿(引)字金剛語業。",
        "from": "T32n1658_001:0508a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吽字金剛心業。",
        "from": "T32n1658_001:0508a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復阿惡二字安住空性。",
        "from": "T32n1658_001:0508a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中惡字亦為正智。",
        "from": "T32n1658_001:0508a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中阿字是即正覺最上祕密。",
        "from": "T32n1658_001:0508a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復吽字而為心智。",
        "from": "T32n1658_001:0508a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覺了一切法。",
        "from": "T32n1658_001:0508a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如上所說一切文字。",
        "from": "T32n1658_001:0508a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知皆從盎阿(引)吽三字所生。",
        "from": "T32n1658_001:0508a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由是諸法起。",
        "from": "T32n1658_001:0508a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種相今當分別。",
        "from": "T32n1658_001:0508a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼一切法皆與盎阿(引)二字初後相攝。",
        "from": "T32n1658_001:0508a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中吽字出生一切。",
        "from": "T32n1658_001:0508a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於三界中出現眾色。",
        "from": "T32n1658_001:0508a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有天人龍阿修羅迦樓羅緊那羅乾闥婆。",
        "from": "T32n1658_001:0508a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就持明天吉祥天。",
        "from": "T32n1658_001:0508a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辯才天烏摩天。",
        "from": "T32n1658_001:0508a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝釋天梵王天。",
        "from": "T32n1658_001:0508a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那羅延天大自在天。",
        "from": "T32n1658_001:0508a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等天及天后。",
        "from": "T32n1658_001:0508a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有一切有情界中男子女人。",
        "from": "T32n1658_001:0508a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至諸佛菩薩等。",
        "from": "T32n1658_001:0508a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆從此吽字出生變化。",
        "from": "T32n1658_001:0508a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼一一心住此字相。",
        "from": "T32n1658_001:0508a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若心想此字時。",
        "from": "T32n1658_001:0508a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當住虛空出生無礙。",
        "from": "T32n1658_001:0508a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂三界心同此一心入。",
        "from": "T32n1658_001:0508a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入是心已。",
        "from": "T32n1658_001:0508a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此得名為現證菩提。",
        "from": "T32n1658_001:0508a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知此心無等無取無著無住無表無相。",
        "from": "T32n1658_001:0508a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是即虛空平等一切智。",
        "from": "T32n1658_001:0508a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無所得相應。",
        "from": "T32n1658_001:0508a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無自無他相應正行。",
        "from": "T32n1658_001:0508a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間所有旃陀羅等。",
        "from": "T32n1658_001:0508a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最下族類彼等諸行。",
        "from": "T32n1658_001:0508a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至傍生等類。",
        "from": "T32n1658_001:0508a25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼所行行種種差別。",
        "from": "T32n1658_001:0508a25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是諸行雖復差別。",
        "from": "T32n1658_001:0508a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆亦不離一切智智相應正行。",
        "from": "T32n1658_001:0508a26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次一切文字性非有說。",
        "from": "T32n1658_001:0508a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然彼一切智智。",
        "from": "T32n1658_001:0508a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸文字方便宣說。",
        "from": "T32n1658_001:0508a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中文字當云何義。",
        "from": "T32n1658_001:0508b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂虛空義。",
        "from": "T32n1658_001:0508b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空何義謂空性義空性何義謂戍室囉義。",
        "from": "T32n1658_001:0508b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戍室囉何義謂無說義。",
        "from": "T32n1658_001:0508b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無說何義謂無相義。",
        "from": "T32n1658_001:0508b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無相何義謂一切智義。",
        "from": "T32n1658_001:0508b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切智何義謂如意寶義如意寶何義謂即智義。",
        "from": "T32n1658_001:0508b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智復何義所謂心義。",
        "from": "T32n1658_001:0508b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心復何義謂彼三界大自在義。",
        "from": "T32n1658_001:0508b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三界大自在何義謂遍照義。",
        "from": "T32n1658_001:0508b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍照何義謂梵天義。",
        "from": "T32n1658_001:0508b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵天何義謂大力天義。",
        "from": "T32n1658_001:0508b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大力天何義謂自在天義。",
        "from": "T32n1658_001:0508b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自在天何義謂即佛義。",
        "from": "T32n1658_001:0508b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛者何義謂金剛薩埵義。",
        "from": "T32n1658_001:0508b09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛薩埵何義謂觀自在義。",
        "from": "T32n1658_001:0508b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀自在何義謂世間義。",
        "from": "T32n1658_001:0508b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間何義謂輪迴義。",
        "from": "T32n1658_001:0508b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輪迴何義謂涅槃義。",
        "from": "T32n1658_001:0508b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "涅槃何義謂不可數義。",
        "from": "T32n1658_001:0508b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可數何義謂不可知義。",
        "from": "T32n1658_001:0508b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可知何義謂無生義。",
        "from": "T32n1658_001:0508b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無生何義謂無滅義。",
        "from": "T32n1658_001:0508b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無滅何義謂無色義。",
        "from": "T32n1658_001:0508b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無色何義謂無聲義。",
        "from": "T32n1658_001:0508b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無聲何義謂無根本義。",
        "from": "T32n1658_001:0508b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無根本何義謂無長養義。",
        "from": "T32n1658_001:0508b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無長養何義謂無住義。",
        "from": "T32n1658_001:0508b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無住何義。",
        "from": "T32n1658_001:0508b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此無住者即無所有。",
        "from": "T32n1658_001:0508b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離諸有智思惟分別。",
        "from": "T32n1658_001:0508b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出過諸佛及佛菩提。",
        "from": "T32n1658_001:0508b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安住金剛吽字根本。",
        "from": "T32n1658_001:0508b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即此吽字復成蓮華火曼拏羅。",
        "from": "T32n1658_001:0508b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住空空性離塵法性。",
        "from": "T32n1658_001:0508b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二種根本最上清淨相應勝行。",
        "from": "T32n1658_001:0508b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是了知已。",
        "from": "T32n1658_001:0508b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於輪迴中勇猛精進。",
        "from": "T32n1658_001:0508b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普令獲得涅槃大樂。",
        "from": "T32n1658_001:0508b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若住此心。",
        "from": "T32n1658_001:0508b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為智者滅苦惱法。",
        "from": "T32n1658_001:0508b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二種根本皆悉平等。",
        "from": "T32n1658_001:0508b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為最上精進勝行。",
        "from": "T32n1658_001:0508b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就牟尼大安樂法。",
        "from": "T32n1658_001:0508b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安住正念。",
        "from": "T32n1658_001:0508b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如日光明普照世間。",
        "from": "T32n1658_001:0508b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心智平等相應安住。",
        "from": "T32n1658_001:0508b25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有一切甚深祕密如前所說。",
        "from": "T32n1658_001:0508b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼戍室囉及虛空分皆如是說。",
        "from": "T32n1658_001:0508b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是成就最上樂法。",
        "from": "T32n1658_001:0508b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得諸佛自性相應。",
        "from": "T32n1658_001:0508b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了知世間所有眾生一切生法。",
        "from": "T32n1658_001:0508b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喜愛二種和合相應當知無性。",
        "from": "T32n1658_001:0508b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼無常法畢竟空寂。",
        "from": "T32n1658_001:0508b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸教決定名義論",
        "from": "T32n1658_001:0508c01_0",
        "context_with_next_sentence": false
    }
]