[
    {
        "source_sentence": "愚菴和尚語錄卷第四",
        "from": "X71n1421_004:0673b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徑山興聖萬壽禪寺語錄",
        "from": "X71n1421_004:0673b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "門人　希顏　等編",
        "from": "X71n1421_004:0673b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師於至正辛丑十月十七日。",
        "from": "X71n1421_004:0673b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "承奉行宣政院官疏請。",
        "from": "X71n1421_004:0673b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入寺。",
        "from": "X71n1421_004:0673b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "指山門云。",
        "from": "X71n1421_004:0673b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天下徑山。",
        "from": "X71n1421_004:0673b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天下龍門。",
        "from": "X71n1421_004:0673b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吐吞日月。",
        "from": "X71n1421_004:0673b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "函盖乾坤。",
        "from": "X71n1421_004:0673b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛殿。",
        "from": "X71n1421_004:0673b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頋左右云。",
        "from": "X71n1421_004:0673b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "插一莖草建瓊樓玉殿。",
        "from": "X71n1421_004:0673b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈一莖草作丈六金身。",
        "from": "X71n1421_004:0673b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "山上有鯉魚。",
        "from": "X71n1421_004:0673b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "井底有塳塵。",
        "from": "X71n1421_004:0673b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陞座祝贊。",
        "from": "X71n1421_004:0673b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問答罷。",
        "from": "X71n1421_004:0673b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃云。",
        "from": "X71n1421_004:0673b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通上徹下。",
        "from": "X71n1421_004:0673b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迥絕羅籠。",
        "from": "X71n1421_004:0673b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "匝地普天。",
        "from": "X71n1421_004:0673b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了無眹跡。",
        "from": "X71n1421_004:0673b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識不可識。",
        "from": "X71n1421_004:0673b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智莫能知。",
        "from": "X71n1421_004:0673b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一自拈花微咲。",
        "from": "X71n1421_004:0673b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面壁安心。",
        "from": "X71n1421_004:0673b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便見五葉流芳。",
        "from": "X71n1421_004:0673b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千燈續焰。",
        "from": "X71n1421_004:0673b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體究將來。",
        "from": "X71n1421_004:0673b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總是強生節目。",
        "from": "X71n1421_004:0673b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直饒上無扳仰。",
        "from": "X71n1421_004:0673b16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下絕己躬。",
        "from": "X71n1421_004:0673b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常光見前。",
        "from": "X71n1421_004:0673b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壁立萬仞。",
        "from": "X71n1421_004:0673b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何異癩馬繫枯樁。",
        "from": "X71n1421_004:0673b17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黑牛臥死水。",
        "from": "X71n1421_004:0673b17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "借使斬新日月。",
        "from": "X71n1421_004:0673b17_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逸格風規。",
        "from": "X71n1421_004:0673b17_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冰稜上度過九[阿可+尻]。",
        "from": "X71n1421_004:0673b18_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劒刃上拾得全身。",
        "from": "X71n1421_004:0673b18_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正好朝打三千。",
        "from": "X71n1421_004:0673b18_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫打八百。",
        "from": "X71n1421_004:0673b19_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈不見雲門大師道。",
        "from": "X71n1421_004:0673b19_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡乾坤大地。",
        "from": "X71n1421_004:0673b19_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無纖毫過患。",
        "from": "X71n1421_004:0673b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶是轉句。",
        "from": "X71n1421_004:0673b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不見一色。",
        "from": "X71n1421_004:0673b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始是半提。",
        "from": "X71n1421_004:0673b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須知更有全提時節。",
        "from": "X71n1421_004:0673b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖然一向與麼去。",
        "from": "X71n1421_004:0673b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法堂上何啻草深一丈。",
        "from": "X71n1421_004:0673b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "官不容針。",
        "from": "X71n1421_004:0673b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "私通車馬。",
        "from": "X71n1421_004:0673b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二峯突兀。",
        "from": "X71n1421_004:0673b22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "双徑回環。",
        "from": "X71n1421_004:0673b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大覺國師願力宏深。",
        "from": "X71n1421_004:0673b22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孚佑龍王淨緣際會。",
        "from": "X71n1421_004:0673b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "維禪林之首剎。",
        "from": "X71n1421_004:0673b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實天下之名山。",
        "from": "X71n1421_004:0673b23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不妨與諸人高掛鉢囊。",
        "from": "X71n1421_004:0673b24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "抝折拄杖。",
        "from": "X71n1421_004:0673b24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直得八方寧謐。",
        "from": "X71n1421_004:0673b24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬姓歌謠。",
        "from": "X71n1421_004:0673c01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鴉鳴鵲噪。",
        "from": "X71n1421_004:0673c01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總契真源。",
        "from": "X71n1421_004:0673c01_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風動塵飛。",
        "from": "X71n1421_004:0673c01_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆明佛事。",
        "from": "X71n1421_004:0673c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "揚不揚之正化。",
        "from": "X71n1421_004:0673c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "報不報之深恩。",
        "from": "X71n1421_004:0673c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正與麼時。",
        "from": "X71n1421_004:0673c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且開堂祝聖一句如何舉似。",
        "from": "X71n1421_004:0673c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擊拂子。",
        "from": "X71n1421_004:0673c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黎元頓息三災劫。",
        "from": "X71n1421_004:0673c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "睿算延洪億萬春。",
        "from": "X71n1421_004:0673c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復舉。",
        "from": "X71n1421_004:0673c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧問當山諲禪師。",
        "from": "X71n1421_004:0673c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掩息如灰時如何。",
        "from": "X71n1421_004:0673c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諲云。",
        "from": "X71n1421_004:0673c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶是時人功幹。",
        "from": "X71n1421_004:0673c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧云。",
        "from": "X71n1421_004:0673c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幹後如何。",
        "from": "X71n1421_004:0673c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諲云。",
        "from": "X71n1421_004:0673c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畊人田不種。",
        "from": "X71n1421_004:0673c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧云。",
        "from": "X71n1421_004:0673c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畢竟如何。",
        "from": "X71n1421_004:0673c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諲云。",
        "from": "X71n1421_004:0673c07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禾熟不臨場。",
        "from": "X71n1421_004:0673c07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師云。",
        "from": "X71n1421_004:0673c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諲禪師與麼答話。",
        "from": "X71n1421_004:0673c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勞而無功。",
        "from": "X71n1421_004:0673c07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "這僧若是個漢。",
        "from": "X71n1421_004:0673c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "決不依他作解。",
        "from": "X71n1421_004:0673c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後來應菴和尚道。",
        "from": "X71n1421_004:0673c08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳳閤香沈。",
        "from": "X71n1421_004:0673c09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雪巢夜冷。",
        "from": "X71n1421_004:0673c09_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "半窓明月。",
        "from": "X71n1421_004:0673c09_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和氣藹然。",
        "from": "X71n1421_004:0673c09_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正與麼時。",
        "from": "X71n1421_004:0673c09_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且道。",
        "from": "X71n1421_004:0673c10_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸宗與徑山。",
        "from": "X71n1421_004:0673c10_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還有相見分也無。",
        "from": "X71n1421_004:0673c10_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喝。",
        "from": "X71n1421_004:0673c10_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直饒相見。",
        "from": "X71n1421_004:0673c10_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "也是蝦蟆窟裏作活計。",
        "from": "X71n1421_004:0673c11_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要見四方八面逸格變通。",
        "from": "X71n1421_004:0673c11_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別著個眼始得。",
        "from": "X71n1421_004:0673c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "新徑山莫有長處麼。",
        "from": "X71n1421_004:0673c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拍禪床云。",
        "from": "X71n1421_004:0673c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敲出鳳凰五色髓。",
        "from": "X71n1421_004:0673c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擊碎驪龍明月珠。",
        "from": "X71n1421_004:0673c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千古華山山脚下。",
        "from": "X71n1421_004:0673c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "只應潘閬倒騎驢。",
        "from": "X71n1421_004:0673c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "久立珍重。",
        "from": "X71n1421_004:0673c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上堂。",
        "from": "X71n1421_004:0673c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉睦州道。",
        "from": "X71n1421_004:0673c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "揑聚也在我。",
        "from": "X71n1421_004:0673c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "裂開也在我。",
        "from": "X71n1421_004:0673c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有僧便問。",
        "from": "X71n1421_004:0673c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如何是裂開。",
        "from": "X71n1421_004:0673c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "州云。",
        "from": "X71n1421_004:0673c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩提涅槃。",
        "from": "X71n1421_004:0673c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真如解脫。",
        "from": "X71n1421_004:0673c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三九二十七。",
        "from": "X71n1421_004:0673c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即心即佛。",
        "from": "X71n1421_004:0673c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我且與麼道。",
        "from": "X71n1421_004:0673c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "你又作麼生。",
        "from": "X71n1421_004:0673c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧云。",
        "from": "X71n1421_004:0673c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "某甲不與麼道。",
        "from": "X71n1421_004:0673c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "州云。",
        "from": "X71n1421_004:0673c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "琖子撲落地。",
        "from": "X71n1421_004:0673c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "楪子成七片。",
        "from": "X71n1421_004:0673c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧云。",
        "from": "X71n1421_004:0673c18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如何是揑聚。",
        "from": "X71n1421_004:0673c18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "州乃斂手而坐。",
        "from": "X71n1421_004:0673c19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師去。",
        "from": "X71n1421_004:0673c19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大小睦州。",
        "from": "X71n1421_004:0673c19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擬將織蒲鞋手。",
        "from": "X71n1421_004:0673c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "籠罩古今。",
        "from": "X71n1421_004:0673c19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自謂。",
        "from": "X71n1421_004:0673c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "揑聚放開。",
        "from": "X71n1421_004:0673c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乾坤把斷。",
        "from": "X71n1421_004:0673c20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要且無合殺。",
        "from": "X71n1421_004:0673c20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "者僧當時若是皮下有血。",
        "from": "X71n1421_004:0673c20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見它與麼道。",
        "from": "X71n1421_004:0673c21_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但低聲向它耳邊下一轉語。",
        "from": "X71n1421_004:0673c21_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教他揑聚也不得。",
        "from": "X71n1421_004:0673c22_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放開也不得。",
        "from": "X71n1421_004:0673c22_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瑞浴主火。",
        "from": "X71n1421_004:0673c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅固法身。",
        "from": "X71n1421_004:0673c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "膿滴滴地。",
        "from": "X71n1421_004:0673c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火不能燒。",
        "from": "X71n1421_004:0673c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水不能洗。",
        "from": "X71n1421_004:0673c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擲火云。",
        "from": "X71n1421_004:0673c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我見燈明佛。",
        "from": "X71n1421_004:0673c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本光瑞如此。",
        "from": "X71n1421_004:0673c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上堂。",
        "from": "X71n1421_004:0674a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧問。",
        "from": "X71n1421_004:0674a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不起一念時如何。",
        "from": "X71n1421_004:0674a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師云。",
        "from": "X71n1421_004:0674a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道者合如是。",
        "from": "X71n1421_004:0674a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進云。",
        "from": "X71n1421_004:0674a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與麼則依而行之也。",
        "from": "X71n1421_004:0674a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師云。",
        "from": "X71n1421_004:0674a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虗生浪死漢。",
        "from": "X71n1421_004:0674a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又僧問。",
        "from": "X71n1421_004:0674a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父母非我親。",
        "from": "X71n1421_004:0674a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誰是最親者。",
        "from": "X71n1421_004:0674a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師云。",
        "from": "X71n1421_004:0674a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識得不為冤。",
        "from": "X71n1421_004:0674a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進云。",
        "from": "X71n1421_004:0674a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛非我道。",
        "from": "X71n1421_004:0674a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誰是最道者。",
        "from": "X71n1421_004:0674a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師云。",
        "from": "X71n1421_004:0674a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闍黎合自知。",
        "from": "X71n1421_004:0674a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧擬議。",
        "from": "X71n1421_004:0674a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師便喝。",
        "from": "X71n1421_004:0674a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃云。",
        "from": "X71n1421_004:0674a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今朝又是十一月。",
        "from": "X71n1421_004:0674a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卷地北風吹觱發。",
        "from": "X71n1421_004:0674a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臘月三十便到來。",
        "from": "X71n1421_004:0674a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勸君各要知時節。",
        "from": "X71n1421_004:0674a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人漸老水長流。",
        "from": "X71n1421_004:0674a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "君子可八。",
        "from": "X71n1421_004:0674a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知得便[但日+木]。",
        "from": "X71n1421_004:0674a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上堂。",
        "from": "X71n1421_004:0674a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈拄杖。",
        "from": "X71n1421_004:0674a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冬至一陽生。",
        "from": "X71n1421_004:0674a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拄杖長丈二。",
        "from": "X71n1421_004:0674a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "石笋暗抽條。",
        "from": "X71n1421_004:0674a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千花生碓觜。",
        "from": "X71n1421_004:0674a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且衲僧分上。",
        "from": "X71n1421_004:0674a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成得個甚麼邊事。",
        "from": "X71n1421_004:0674a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫有道得底麼。",
        "from": "X71n1421_004:0674a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有則不妨出來對眾道看。",
        "from": "X71n1421_004:0674a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無。",
        "from": "X71n1421_004:0674a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徑山自道去也。",
        "from": "X71n1421_004:0674a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卓拄杖。",
        "from": "X71n1421_004:0674a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上堂。",
        "from": "X71n1421_004:0674a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧問。",
        "from": "X71n1421_004:0674a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨濟道。",
        "from": "X71n1421_004:0674a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一句中具三玄。",
        "from": "X71n1421_004:0674a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一玄中具三要。",
        "from": "X71n1421_004:0674a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如何是一句中具三玄。",
        "from": "X71n1421_004:0674a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師云。",
        "from": "X71n1421_004:0674a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬仞峯頭駕鐵船。",
        "from": "X71n1421_004:0674a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進云。",
        "from": "X71n1421_004:0674a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如何是一玄中具三要。",
        "from": "X71n1421_004:0674a13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師云。",
        "from": "X71n1421_004:0674a14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼裏瞳人吹木呌。",
        "from": "X71n1421_004:0674a14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進云。",
        "from": "X71n1421_004:0674a14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如何是和尚家風。",
        "from": "X71n1421_004:0674a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師云。",
        "from": "X71n1421_004:0674a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吹折門前一株松。",
        "from": "X71n1421_004:0674a15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進云。",
        "from": "X71n1421_004:0674a15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "學人不問這個風。",
        "from": "X71n1421_004:0674a15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師云。",
        "from": "X71n1421_004:0674a16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝問甚麼風。",
        "from": "X71n1421_004:0674a16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進云家風。",
        "from": "X71n1421_004:0674a16_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師云。",
        "from": "X71n1421_004:0674a16_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我這裏大功不竪賞。",
        "from": "X71n1421_004:0674a16_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃舉仰山示眾云。",
        "from": "X71n1421_004:0674a17_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等諸人。",
        "from": "X71n1421_004:0674a17_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各自回光返照。",
        "from": "X71n1421_004:0674a17_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫記吾言。",
        "from": "X71n1421_004:0674a18_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝無始劫來。",
        "from": "X71n1421_004:0674a18_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "背明投暗。",
        "from": "X71n1421_004:0674a18_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妄想根深。",
        "from": "X71n1421_004:0674a18_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卒難頓拔。",
        "from": "X71n1421_004:0674a18_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以假設方便。",
        "from": "X71n1421_004:0674a19_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奪汝麤識。",
        "from": "X71n1421_004:0674a19_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如將黃葉止啼。",
        "from": "X71n1421_004:0674a19_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有什麼是處。",
        "from": "X71n1421_004:0674a19_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師云。",
        "from": "X71n1421_004:0674a20_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡十方世界。",
        "from": "X71n1421_004:0674a20_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸人自己。",
        "from": "X71n1421_004:0674a20_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡十方世界。",
        "from": "X71n1421_004:0674a20_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸人本有光明。",
        "from": "X71n1421_004:0674a21_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為我拈針鋒許。",
        "from": "X71n1421_004:0674a21_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妄想麤識來看。",
        "from": "X71n1421_004:0674a21_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喝一喝。",
        "from": "X71n1421_004:0674a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下座。",
        "from": "X71n1421_004:0674a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上堂。",
        "from": "X71n1421_004:0674a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庚申好日逢元旦。",
        "from": "X71n1421_004:0674a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四海歌謠樂太平。",
        "from": "X71n1421_004:0674a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖德如天何以報。",
        "from": "X71n1421_004:0674a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卓拄杖云。",
        "from": "X71n1421_004:0674a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬年松上一枝藤。",
        "from": "X71n1421_004:0674a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上堂。",
        "from": "X71n1421_004:0674b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉。",
        "from": "X71n1421_004:0674b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧問長慶。",
        "from": "X71n1421_004:0674b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有問有答。",
        "from": "X71n1421_004:0674b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賓主歷然。",
        "from": "X71n1421_004:0674b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無問無答時如何。",
        "from": "X71n1421_004:0674b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慶云。",
        "from": "X71n1421_004:0674b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怕爛却那。",
        "from": "X71n1421_004:0674b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧又問睦州。",
        "from": "X71n1421_004:0674b02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "州云。",
        "from": "X71n1421_004:0674b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相逢盡道[但日+木]官去。",
        "from": "X71n1421_004:0674b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "林下何曾見一人。",
        "from": "X71n1421_004:0674b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師云。",
        "from": "X71n1421_004:0674b03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長慶放頑。",
        "from": "X71n1421_004:0674b03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "睦州檐板。",
        "from": "X71n1421_004:0674b03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "等是有問有答。",
        "from": "X71n1421_004:0674b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要且未會無問無答底道理在。",
        "from": "X71n1421_004:0674b04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今日忽有問徑山。",
        "from": "X71n1421_004:0674b04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有問。",
        "from": "X71n1421_004:0674b05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有答。",
        "from": "X71n1421_004:0674b05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賓主歷然。",
        "from": "X71n1421_004:0674b05_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無問無答時如何。",
        "from": "X71n1421_004:0674b05_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "只向道。",
        "from": "X71n1421_004:0674b05_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一曲自歌山自綠。",
        "from": "X71n1421_004:0674b06_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此情不與白雲知。",
        "from": "X71n1421_004:0674b06_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愚菴和尚語錄卷第四",
        "from": "X71n1421_004:0674b08_0",
