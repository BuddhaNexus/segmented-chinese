[
    {
        "source_sentence": "佛祖綱目卷第十二(昃字號)",
        "from": "X85n1594_012:0576b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甲子(周威烈王九年起)庚辰(安王元年)丙午(烈王元年)癸丑(顯王元年)癸亥(周顯王十一年止)(戊戌)十一祖富那夜奢傳法馬鳴富那夜奢。",
        "from": "X85n1594_012:0576b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華氏國人。",
        "from": "X85n1594_012:0576b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姓瞿曇氏。",
        "from": "X85n1594_012:0576b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父寶身。",
        "from": "X85n1594_012:0576b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有子七人。",
        "from": "X85n1594_012:0576b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜奢最少。",
        "from": "X85n1594_012:0576b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幼即與諸兄異尚。",
        "from": "X85n1594_012:0576b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嘗曰。",
        "from": "X85n1594_012:0576b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遇大士。",
        "from": "X85n1594_012:0576b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐於道場。",
        "from": "X85n1594_012:0576b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我則詣彼。",
        "from": "X85n1594_012:0576b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "親近隨喜。",
        "from": "X85n1594_012:0576b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及遇脇尊者。",
        "from": "X85n1594_012:0576b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂授法。",
        "from": "X85n1594_012:0576b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜奢教被無量得果者。",
        "from": "X85n1594_012:0576b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且五百矣。",
        "from": "X85n1594_012:0576b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後至波羅柰國。",
        "from": "X85n1594_012:0576b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有一長者。",
        "from": "X85n1594_012:0576b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來趣其會。",
        "from": "X85n1594_012:0576b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜奢謂眾曰。",
        "from": "X85n1594_012:0576b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等識此來者耶。",
        "from": "X85n1594_012:0576b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛記。",
        "from": "X85n1594_012:0576b13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖者馬鳴紹吾法者也。",
        "from": "X85n1594_012:0576b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是馬鳴致禮問曰。",
        "from": "X85n1594_012:0576b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我欲識佛。",
        "from": "X85n1594_012:0576b14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何者即是。",
        "from": "X85n1594_012:0576b14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜奢曰。",
        "from": "X85n1594_012:0576b15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝欲識佛。",
        "from": "X85n1594_012:0576b15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不識者是。",
        "from": "X85n1594_012:0576b15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬鳴曰。",
        "from": "X85n1594_012:0576b15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛既不識焉知是乎。",
        "from": "X85n1594_012:0576b15_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜奢曰。",
        "from": "X85n1594_012:0576b16_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既不識佛。",
        "from": "X85n1594_012:0576b16_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "焉知不是。",
        "from": "X85n1594_012:0576b16_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬鳴曰。",
        "from": "X85n1594_012:0576b16_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是鋸義。",
        "from": "X85n1594_012:0576b16_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜奢曰。",
        "from": "X85n1594_012:0576b17_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼是木義。",
        "from": "X85n1594_012:0576b17_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃問鋸義者何。",
        "from": "X85n1594_012:0576b17_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰與師平出。",
        "from": "X85n1594_012:0576b17_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬鳴却問木義者何。",
        "from": "X85n1594_012:0576b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰汝被我解。",
        "from": "X85n1594_012:0576b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬鳴豁然省悟。",
        "from": "X85n1594_012:0576b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂求剃度。",
        "from": "X85n1594_012:0576b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜奢謂眾曰。",
        "from": "X85n1594_012:0576b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大士者。",
        "from": "X85n1594_012:0576b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔為毗舍利國王。",
        "from": "X85n1594_012:0576b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以其國有一類裸人。",
        "from": "X85n1594_012:0576b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如馬裸露。",
        "from": "X85n1594_012:0576b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王運神通。",
        "from": "X85n1594_012:0576b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分身為蠶。",
        "from": "X85n1594_012:0576b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼乃得衣。",
        "from": "X85n1594_012:0576b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後復生中印度。",
        "from": "X85n1594_012:0576b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬人感戀悲鳴。",
        "from": "X85n1594_012:0576b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故號馬鳴。",
        "from": "X85n1594_012:0576b21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來懸記。",
        "from": "X85n1594_012:0576b21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅度後六百年。",
        "from": "X85n1594_012:0576b22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬鳴當於波羅柰國。",
        "from": "X85n1594_012:0576b22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摧伏異道。",
        "from": "X85n1594_012:0576b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繼吾傳化。",
        "from": "X85n1594_012:0576b23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今正是時。",
        "from": "X85n1594_012:0576b23_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂告之曰。",
        "from": "X85n1594_012:0576b23_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來大法眼藏。",
        "from": "X85n1594_012:0576b23_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今付於汝。",
        "from": "X85n1594_012:0576b23_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說偈曰。",
        "from": "X85n1594_012:0576b24_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迷悟如隱顯。",
        "from": "X85n1594_012:0576b24_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明暗不相離。",
        "from": "X85n1594_012:0576b24_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今付隱顯法。",
        "from": "X85n1594_012:0576b24_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非一亦非二。",
        "from": "X85n1594_012:0576c01_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "付法已。",
        "from": "X85n1594_012:0576c01_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即現神變。",
        "from": "X85n1594_012:0576c01_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "湛然圓寂。",
        "from": "X85n1594_012:0576c01_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾興寶塔。",
        "from": "X85n1594_012:0576c01_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以閟全身。",
        "from": "X85n1594_012:0576c02_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當此土周安王十九年戊戌歲也○宗鏡錄云。",
        "from": "X85n1594_012:0576c02_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天韻陀山中。",
        "from": "X85n1594_012:0576c03_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有一羅漢。",
        "from": "X85n1594_012:0576c03_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名富樓那。",
        "from": "X85n1594_012:0576c03_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬鳴往見。",
        "from": "X85n1594_012:0576c03_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端坐林中。",
        "from": "X85n1594_012:0576c04_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "志氣渺然。",
        "from": "X85n1594_012:0576c04_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不可測。",
        "from": "X85n1594_012:0576c04_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬鳴語之曰。",
        "from": "X85n1594_012:0576c04_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙門說之。",
        "from": "X85n1594_012:0576c04_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敢有所明。",
        "from": "X85n1594_012:0576c05_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要必屈汝。",
        "from": "X85n1594_012:0576c05_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我若不勝。",
        "from": "X85n1594_012:0576c05_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刎頸以謝。",
        "from": "X85n1594_012:0576c05_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅漢默然。",
        "from": "X85n1594_012:0576c05_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "容無負色。",
        "from": "X85n1594_012:0576c06_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦無勝顏。",
        "from": "X85n1594_012:0576c06_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "扣之數四。",
        "from": "X85n1594_012:0576c06_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曾無應情。",
        "from": "X85n1594_012:0576c06_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳴退自思惟。",
        "from": "X85n1594_012:0576c06_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我負矣。",
        "from": "X85n1594_012:0576c07_54",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼勝矣。",
        "from": "X85n1594_012:0576c07_55",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼安無言。",
        "from": "X85n1594_012:0576c07_56",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故無可屈。",
        "from": "X85n1594_012:0576c07_57",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾以言之。",
        "from": "X85n1594_012:0576c07_58",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖如言有可屈。",
        "from": "X85n1594_012:0576c08_59",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自吾未免於言。",
        "from": "X85n1594_012:0576c08_60",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真可媿矣。",
        "from": "X85n1594_012:0576c08_61",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂投出家。",
        "from": "X85n1594_012:0576c08_62",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此說與傳燈不同。",
        "from": "X85n1594_012:0576c09_63",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并識之。",
        "from": "X85n1594_012:0576c09_64",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛祖綱目卷第十二",
        "from": "X85n1594_012:0576c11_0",
        "context_with_next_sentence": false
    }
]