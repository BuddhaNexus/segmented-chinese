        "from": "T18n0879_001:0338a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南慕一切如來常普照金剛燈菩薩等出生。",
        "from": "T18n0879_001:0338a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡虛空遍法界一切波羅蜜菩薩摩訶薩。",
        "from": "T18n0879_001:0338a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南慕一切如來戒清涼金剛塗香菩薩等出生。",
        "from": "T18n0879_001:0338a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡虛空遍法界一切波羅蜜菩薩摩訶薩。",
        "from": "T18n0879_001:0338a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南慕一切如來四攝智金剛鉤菩薩等出生。",
        "from": "T18n0879_001:0338a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡虛空遍法界同一體性金剛界生身一切成辦波羅蜜菩薩摩訶薩。",
        "from": "T18n0879_001:0338a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南慕一切如來善巧智金剛索菩薩等出生。",
        "from": "T18n0879_001:0338a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡虛空遍法界同一體性金剛界生身一切奉教波羅蜜菩薩摩訶薩。",
        "from": "T18n0879_001:0338a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南慕一切如來堅固智金剛鎖菩薩等出生。",
        "from": "T18n0879_001:0338a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡虛空遍法界同一體性金剛界生身一切如來使者波羅蜜菩薩摩訶薩。",
        "from": "T18n0879_001:0338a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南慕一切如來歡樂智金剛鈴菩薩等出生。",
        "from": "T18n0879_001:0338a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡虛空遍法界同一體性金剛界生身一切如來隨順波羅蜜菩薩摩訶薩。",
        "from": "T18n0879_001:0338a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普為梵釋四王天龍八部帝主人王師僧父母。",
        "from": "T18n0879_001:0338a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及善知識道場眾等法界有情。",
        "from": "T18n0879_001:0338a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並願斷除諸障。",
        "from": "T18n0879_001:0338a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命懺悔。",
        "from": "T18n0879_001:0338a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至心懺悔。",
        "from": "T18n0879_001:0338a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弟子眾等。",
        "from": "T18n0879_001:0338a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自從無始曠大劫來。",
        "from": "T18n0879_001:0338a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至于今日。",
        "from": "T18n0879_001:0338a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迷無我覺。",
        "from": "T18n0879_001:0338a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "計有我人。",
        "from": "T18n0879_001:0338a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我許既興。",
        "from": "T18n0879_001:0338a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常緣我所。",
        "from": "T18n0879_001:0338a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "根塵浩繞。",
        "from": "T18n0879_001:0338a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識陰奔波。",
        "from": "T18n0879_001:0338a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擊動身心。",
        "from": "T18n0879_001:0338a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如電轉。",
        "from": "T18n0879_001:0338a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清淨眼耳鼻舌身意。",
        "from": "T18n0879_001:0338a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一念不覺。",
        "from": "T18n0879_001:0338a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翻作六師。",
        "from": "T18n0879_001:0338a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偷法王財。",
        "from": "T18n0879_001:0338a25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供邊見賊。",
        "from": "T18n0879_001:0338a25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賊既熾盛。",
        "from": "T18n0879_001:0338a25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破涅槃城。",
        "from": "T18n0879_001:0338a25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殘害法身。",
        "from": "T18n0879_001:0338a26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "焚燒慧命。",
        "from": "T18n0879_001:0338a26_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如此等罪。",
        "from": "T18n0879_001:0338a26_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數越塵沙。",
        "from": "T18n0879_001:0338a26_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從迷至迷。",
        "from": "T18n0879_001:0338a27_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫測終始。",
        "from": "T18n0879_001:0338a27_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今始覺悟。",
        "from": "T18n0879_001:0338a27_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深悔自慚。",
        "from": "T18n0879_001:0338a27_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曉夜驚惶。",
        "from": "T18n0879_001:0338a28_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身心戰慄。",
        "from": "T18n0879_001:0338a28_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永斷迷覺。",
        "from": "T18n0879_001:0338a28_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貪愛我人。",
        "from": "T18n0879_001:0338a28_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "投涅槃城。",
        "from": "T18n0879_001:0338a29_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸安樂國。",
        "from": "T18n0879_001:0338a29_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以無我覺。",
        "from": "T18n0879_001:0338a29_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降伏六師。",
        "from": "T18n0879_001:0338a29_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "收法王財。",
        "from": "T18n0879_001:0338b01_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "納三堅藏。",
        "from": "T18n0879_001:0338b01_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "資給慧命。",
        "from": "T18n0879_001:0338b01_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增益法身。",
        "from": "T18n0879_001:0338b01_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然法性燈。",
        "from": "T18n0879_001:0338b02_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常照無盡。",
        "from": "T18n0879_001:0338b02_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行願理事。",
        "from": "T18n0879_001:0338b02_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塵界不違。",
        "from": "T18n0879_001:0338b02_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三寶三乘。",
        "from": "T18n0879_001:0338b03_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誓當弘護。",
        "from": "T18n0879_001:0338b03_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迷覺之罪。",
        "from": "T18n0879_001:0338b03_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨懺消滅。",
        "from": "T18n0879_001:0338b03_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懺悔迴向已至心歸命禮三寶。",
        "from": "T18n0879_001:0338b04_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至心發願。",
        "from": "T18n0879_001:0338b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弟子眾等。",
        "from": "T18n0879_001:0338b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及法界有情。",
        "from": "T18n0879_001:0338b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始從今日乃至無上菩提。",
        "from": "T18n0879_001:0338b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念念堅固。",
        "from": "T18n0879_001:0338b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念念勝進。",
        "from": "T18n0879_001:0338b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身心自在。",
        "from": "T18n0879_001:0338b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辯說無礙。",
        "from": "T18n0879_001:0338b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一念之中。",
        "from": "T18n0879_001:0338b07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足一切種智。",
        "from": "T18n0879_001:0338b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須知諸法畢竟空寂。",
        "from": "T18n0879_001:0338b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而常度脫一切眾生。",
        "from": "T18n0879_001:0338b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同證涅槃。",
        "from": "T18n0879_001:0338b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不以涅槃為證。",
        "from": "T18n0879_001:0338b09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發願已至心歸命禮三寶。",
        "from": "T18n0879_001:0338b09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛頂瑜伽三十七尊禮",
        "from": "T18n0879_001:0338b10_0",
        "context_with_next_sentence": false
    }
]