[
    {
        "source_sentence": "金剛頂瑜伽三十七尊禮",
        "from": "T18n0879_001:0337a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐特進試鴻臚卿三藏沙門",
        "from": "T18n0879_001:0337a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大廣智不空奉　詔譯",
        "from": "T18n0879_001:0337a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南慕清淨法身毘盧遮那佛。",
        "from": "T18n0879_001:0337a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南慕金剛堅固自性身阿閦佛。",
        "from": "T18n0879_001:0337b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南慕功德莊嚴聚身寶生佛。",
        "from": "T18n0879_001:0337b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南慕受用智慧身阿彌陀佛。",
        "from": "T18n0879_001:0337b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南慕作變化身不空成就佛。",
        "from": "T18n0879_001:0337b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南慕大圓鏡智金剛波羅蜜出生。",
        "from": "T18n0879_001:0337b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡虛空遍法界一切波羅蜜菩薩摩訶薩。",
        "from": "T18n0879_001:0337b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南慕平等性智寶波羅蜜出生。",
        "from": "T18n0879_001:0337b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡虛空遍法界一切波羅蜜菩薩摩訶薩。",
        "from": "T18n0879_001:0337b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南慕妙觀察智法波羅蜜出生。",
        "from": "T18n0879_001:0337b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡虛空遍法界一切波羅蜜菩薩摩訶薩。",
        "from": "T18n0879_001:0337b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南慕成所作智業波羅蜜出生。",
        "from": "T18n0879_001:0337b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡虛空遍法界一切波羅蜜菩薩摩訶薩。",
        "from": "T18n0879_001:0337b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南慕一切如來菩提心金剛薩埵菩薩等。",
        "from": "T18n0879_001:0337b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡虛空遍法界同一體性金剛界生身一切菩薩摩訶薩。",
        "from": "T18n0879_001:0337b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南慕一切如來菩提心金剛王菩薩等盡虛空遍法界同一體性金剛界生身一切菩薩摩訶薩。",
        "from": "T18n0879_001:0337b16_0",
        "context_with_next_sentence": true
    },
    {
