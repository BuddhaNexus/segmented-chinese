[
    {
        "source_sentence": "孔雀經真言等梵本",
        "from": "T19n0983B_001:0441b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孔雀經上",
        "from": "T19n0983B_001:0441b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "         三十六句宛",
        "from": "T19n0983B_001:0442a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二行即界二十四降可得也。",
        "from": "T19n0983B_001:0442a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "         宛二",
        "from": "T19n0983B_001:0442b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二行即界四十四降可得也。",
        "from": "T19n0983B_001:0442b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(去)           ",
        "from": "T19n0983B_001:0442b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "       文二十行即界四",
        "from": "T19n0983B_001:0442c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十降也。",
        "from": "T19n0983B_001:0442c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "     (引)    ",
        "from": "T19n0983B_001:0442c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "       文三十八行界",
        "from": "T19n0983B_001:0443a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即可得七十六降也加漢語八十六降也。",
        "from": "T19n0983B_001:0443a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孔雀經中",
        "from": "T19n0983B_001:0443c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "        此真",
        "from": "T19n0983B_001:0444b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言七也",
        "from": "T19n0983B_001:0444b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "        得百二降也。",
        "from": "T19n0983B_001:0445a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孔雀經下",
        "from": "T19n0983B_001:0445a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孔雀經一部真言",
        "from": "T19n0983B_001:0446b13_0",
