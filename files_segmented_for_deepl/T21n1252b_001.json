[
    {
        "source_sentence": "佛說大吉祥天女十二名號經",
        "from": "T21n1252b_001:0252c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐三藏沙門大廣智不空譯",
        "from": "T21n1252b_001:0252c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T21n1252b_001:0252c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時薄伽梵。",
        "from": "T21n1252b_001:0252c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住極樂世界。",
        "from": "T21n1252b_001:0252c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與無量大菩薩眾。",
        "from": "T21n1252b_001:0252c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後圍繞而為說法。",
        "from": "T21n1252b_001:0252c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時觀自在菩薩摩訶薩。",
        "from": "T21n1252b_001:0252c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大吉祥天女菩薩摩訶薩等。",
        "from": "T21n1252b_001:0252c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆從座起詣世尊所。",
        "from": "T21n1252b_001:0252c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭面禮足各坐一面。",
        "from": "T21n1252b_001:0252c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊為欲利益薄福貧窮諸有情故。",
        "from": "T21n1252b_001:0252c22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告觀自在菩薩言。",
        "from": "T21n1252b_001:0252c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子若有苾芻苾芻尼近事男近事女諸有情類。",
        "from": "T21n1252b_001:0252c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知此大吉祥天女十二名號。",
        "from": "T21n1252b_001:0252c24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能受持讀誦修習供養為他宣說。",
        "from": "T21n1252b_001:0252c25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除一切貧窮業障。",
        "from": "T21n1252b_001:0252c26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲大富貴豐饒財寶。",
        "from": "T21n1252b_001:0252c26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時會中天龍八部。",
        "from": "T21n1252b_001:0252c27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "異口同音咸作是言。",
        "from": "T21n1252b_001:0252c27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如世尊說真實不虛。",
        "from": "T21n1252b_001:0252c28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等願聞十二名號。",
        "from": "T21n1252b_001:0252c28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願世尊大悲演說。",
        "from": "T21n1252b_001:0252c29_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言汝當善聽今為汝說所謂吉慶　吉祥蓮華嚴飾　具財　白色　大名稱　蓮華眼大光曜　施食者　施飲者　寶光　大吉祥　是為十二名號。",
        "from": "T21n1252b_001:0252c29_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當受持。",
        "from": "T21n1252b_001:0253a04_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今復說大吉祥陀羅尼曰。",
        "from": "T21n1252b_001:0253a04_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也(二合)他(去聲引一)室哩(二合)抳室哩(二合)抳(二)薩嚩迦(引)哩野(二合)娑(去聲引)馱[寧*頁](三)悉[寧*頁]悉[寧*頁](四)[寧*頁][寧*頁][寧*頁][寧*頁](五)阿(上聲)洛乞史茗(三合)曩(引)捨野娑嚩(二合引)賀(引六)爾時世尊說是陀羅尼已。",
        "from": "T21n1252b_001:0253a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告觀自在菩薩言。",
        "from": "T21n1252b_001:0253a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大吉祥陀羅尼及十二名號。",
        "from": "T21n1252b_001:0253a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除貧窮一切不祥。",
        "from": "T21n1252b_001:0253a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有願求皆得圓滿。",
        "from": "T21n1252b_001:0253a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能晝夜三時讀誦此經每時三遍。",
        "from": "T21n1252b_001:0253a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或常受持不間。",
        "from": "T21n1252b_001:0253a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作饒益心隨力虔誠供養大吉祥天女菩薩。",
        "from": "T21n1252b_001:0253a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速獲一切財寶豐饒吉祥安樂。",
        "from": "T21n1252b_001:0253a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時觀自在菩薩摩訶薩。",
        "from": "T21n1252b_001:0253a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸大眾天龍八部。",
        "from": "T21n1252b_001:0253a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從佛聞說十二名號及陀羅尼。",
        "from": "T21n1252b_001:0253a16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歎末曾有。",
        "from": "T21n1252b_001:0253a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆大歡喜信受奉行。",
        "from": "T21n1252b_001:0253a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說大吉祥天女十二名號經",
        "from": "T21n1252b_001:0253a19_0",
        "context_with_next_sentence": false
    }
]