[
    {
        "source_sentence": "四家語錄序",
        "from": "X69n1320_001:0001a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "達摩大師西來。",
        "from": "X69n1320_001:0001a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不立文字。",
        "from": "X69n1320_001:0001a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直指人心。",
        "from": "X69n1320_001:0001a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見性成佛。",
        "from": "X69n1320_001:0001a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心心相印。",
        "from": "X69n1320_001:0001a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以迨六祖。",
        "from": "X69n1320_001:0001a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六祖以下。",
        "from": "X69n1320_001:0001a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分為南嶽青原。",
        "from": "X69n1320_001:0001a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而南嶽最盛。",
        "from": "X69n1320_001:0001a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南嶽又分為臨濟溈仰。",
        "from": "X69n1320_001:0001a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而臨濟最盛。",
        "from": "X69n1320_001:0001a07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正所傳四家語錄者。",
        "from": "X69n1320_001:0001a07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃南嶽以下。",
        "from": "X69n1320_001:0001a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬祖百丈黃檗臨濟四尊宿。",
        "from": "X69n1320_001:0001a08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應機接人語也。",
        "from": "X69n1320_001:0001a08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多者萬言。",
        "from": "X69n1320_001:0001a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少者亦不下數千言。",
        "from": "X69n1320_001:0001a09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果文字乎。",
        "from": "X69n1320_001:0001a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非文字乎。",
        "from": "X69n1320_001:0001a10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西來之意。",
        "from": "X69n1320_001:0001a10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果不出此乎。",
        "from": "X69n1320_001:0001a10_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嗟夫。",
        "from": "X69n1320_001:0001a10_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "指月示迷。",
        "from": "X69n1320_001:0001a10_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迷者覩指遺月。",
        "from": "X69n1320_001:0001a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以盤喻日。",
        "from": "X69n1320_001:0001a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盲者自盤之鍾。",
        "from": "X69n1320_001:0001a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種差別。",
        "from": "X69n1320_001:0001a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "胡可勝言。",
        "from": "X69n1320_001:0001a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸尊宿。",
        "from": "X69n1320_001:0001a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其能忘言邪夫。",
        "from": "X69n1320_001:0001a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈不欲直指人心。",
        "from": "X69n1320_001:0001a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令其頓悟。",
        "from": "X69n1320_001:0001a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬之寫照。",
        "from": "X69n1320_001:0001a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以形求形。",
        "from": "X69n1320_001:0001a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僅肖十一。",
        "from": "X69n1320_001:0001a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟此妙明。",
        "from": "X69n1320_001:0001a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非可形求。",
        "from": "X69n1320_001:0001a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非可意度。",
        "from": "X69n1320_001:0001a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "況可言顯。",
        "from": "X69n1320_001:0001a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即令可言。",
        "from": "X69n1320_001:0001a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顧從門而入。",
        "from": "X69n1320_001:0001a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恐非家珍。",
        "from": "X69n1320_001:0001a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因言而明。",
        "from": "X69n1320_001:0001a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不離道聽。",
        "from": "X69n1320_001:0001a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸尊宿。",
        "from": "X69n1320_001:0001a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其忍直言邪。",
        "from": "X69n1320_001:0001a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不忍直言。",
        "from": "X69n1320_001:0001a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不容易言。",
        "from": "X69n1320_001:0001a16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又不能忘言。",
        "from": "X69n1320_001:0001a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸尊宿之心。",
        "from": "X69n1320_001:0001a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其若之何。",
        "from": "X69n1320_001:0001a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或謂。",
        "from": "X69n1320_001:0001a17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人具此心。",
        "from": "X69n1320_001:0001a17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自有鑑覺。",
        "from": "X69n1320_001:0001a17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何煩諸尊宿之喋喋。",
        "from": "X69n1320_001:0001a17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不知情欲萌生。",
        "from": "X69n1320_001:0001a18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知識竝出。",
        "from": "X69n1320_001:0001a18_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前塵分別。",
        "from": "X69n1320_001:0001a18_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悞謂妙明。",
        "from": "X69n1320_001:0001a18_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "趨妄背真。",
        "from": "X69n1320_001:0001a18_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愈求愈遠。",
        "from": "X69n1320_001:0001a19_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儻執定相。",
        "from": "X69n1320_001:0001a19_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輒墮黑山鬼窟。",
        "from": "X69n1320_001:0001a19_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聽其納草。",
        "from": "X69n1320_001:0001a19_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且至犯人禾稼。",
        "from": "X69n1320_001:0001a20_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "參承既遠。",
        "from": "X69n1320_001:0001a20_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘緒茫茫。",
        "from": "X69n1320_001:0001a20_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不有格言。",
        "from": "X69n1320_001:0001a20_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "胡尋正脉。",
        "from": "X69n1320_001:0001a20_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心傅公深入三昧。",
        "from": "X69n1320_001:0001a21_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙脫言詮。",
        "from": "X69n1320_001:0001a21_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸尊宿。",
        "from": "X69n1320_001:0001a21_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有深契焉。",
        "from": "X69n1320_001:0001a21_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂入道者。",
        "from": "X69n1320_001:0001a22_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非此無階。",
        "from": "X69n1320_001:0001a22_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂法施者。",
        "from": "X69n1320_001:0001a22_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非梓莫廣。",
        "from": "X69n1320_001:0001a22_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "會東安解君靜山寧。",
        "from": "X69n1320_001:0001a22_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宿植靈根。",
        "from": "X69n1320_001:0001a23_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深培善力。",
        "from": "X69n1320_001:0001a23_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相與捐貲。",
        "from": "X69n1320_001:0001a23_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竟為繕刻。",
        "from": "X69n1320_001:0001a23_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心公。",
        "from": "X69n1320_001:0001a24_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶恐觀者滯于文字。",
        "from": "X69n1320_001:0001a24_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屬余序其意於首。",
        "from": "X69n1320_001:0001a24_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "余不能深知諸尊宿之心。",
        "from": "X69n1320_001:0001b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甞聞聖人之訓矣。",
        "from": "X69n1320_001:0001b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰予欲無言。",
        "from": "X69n1320_001:0001b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然今之希聖者。",
        "from": "X69n1320_001:0001b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍聖言又奚識焉。",
        "from": "X69n1320_001:0001b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要知善會者。",
        "from": "X69n1320_001:0001b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "侈然無間。",
        "from": "X69n1320_001:0001b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總是一嘿。",
        "from": "X69n1320_001:0001b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不善會者。",
        "from": "X69n1320_001:0001b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離言絕句。",
        "from": "X69n1320_001:0001b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉隔萬塵。",
        "from": "X69n1320_001:0001b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四尊宿之語。",
        "from": "X69n1320_001:0001b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其為文字邪。",
        "from": "X69n1320_001:0001b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非文字邪。",
        "from": "X69n1320_001:0001b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直指邪。",
        "from": "X69n1320_001:0001b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非直指邪。",
        "from": "X69n1320_001:0001b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必有能辨之者。",
        "from": "X69n1320_001:0001b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　毗陵唐鶴徵書讀四家語錄引解君。",
        "from": "X69n1320_001:0001b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刻是錄畢。",
        "from": "X69n1320_001:0001b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有客扣予曰。",
        "from": "X69n1320_001:0001b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但形文字。",
        "from": "X69n1320_001:0001b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即屬言教。",
        "from": "X69n1320_001:0001b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烏得謂之教外別傳邪。",
        "from": "X69n1320_001:0001b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "予曰。",
        "from": "X69n1320_001:0001b11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非也。",
        "from": "X69n1320_001:0001b11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有言固教。",
        "from": "X69n1320_001:0001b11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而無言亦教也。",
        "from": "X69n1320_001:0001b11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至非有言。",
        "from": "X69n1320_001:0001b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非無言。",
        "from": "X69n1320_001:0001b12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非非有言。",
        "from": "X69n1320_001:0001b12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非非無言。",
        "from": "X69n1320_001:0001b12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆教也。",
        "from": "X69n1320_001:0001b12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂離四句絕百非。",
        "from": "X69n1320_001:0001b13_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "客又曰。",
        "from": "X69n1320_001:0001b13_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "靈龜曳尾。",
        "from": "X69n1320_001:0001b13_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而其迹彌彰。",
        "from": "X69n1320_001:0001b13_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四尊宿之語。",
        "from": "X69n1320_001:0001b14_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不亦曳尾乎哉。",
        "from": "X69n1320_001:0001b14_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "予曰。",
        "from": "X69n1320_001:0001b14_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非也。",
        "from": "X69n1320_001:0001b14_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言四尊宿有意泯迹。",
        "from": "X69n1320_001:0001b14_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非教外也。",
        "from": "X69n1320_001:0001b15_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無意泯迹。",
        "from": "X69n1320_001:0001b15_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦非教外也。",
        "from": "X69n1320_001:0001b15_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至非有意。",
        "from": "X69n1320_001:0001b15_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非無意。",
        "from": "X69n1320_001:0001b16_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非非有意。",
        "from": "X69n1320_001:0001b16_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非非無意。",
        "from": "X69n1320_001:0001b16_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具非教外也。",
        "from": "X69n1320_001:0001b16_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "客曰。",
        "from": "X69n1320_001:0001b16_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然則孰謂為別傳哉。",
        "from": "X69n1320_001:0001b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "予曰。",
        "from": "X69n1320_001:0001b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "待覷破下文。",
        "from": "X69n1320_001:0001b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "試與汝道。",
        "from": "X69n1320_001:0001b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬曆丁未秋八一日　　　荊溪釋正傳書刻四家語錄跋客有以四家語錄示者。",
        "from": "X69n1320_001:0001b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "予終日持誦。",
        "from": "X69n1320_001:0001c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能去手。",
        "from": "X69n1320_001:0001c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因出與一心師共讀之。",
        "from": "X69n1320_001:0001c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師請壽諸梓。",
        "from": "X69n1320_001:0001c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "予曰。",
        "from": "X69n1320_001:0001c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸方語錄。",
        "from": "X69n1320_001:0001c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流布不少。",
        "from": "X69n1320_001:0001c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "柰何復侈茲舉。",
        "from": "X69n1320_001:0001c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師曰。",
        "from": "X69n1320_001:0001c07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不然。",
        "from": "X69n1320_001:0001c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大約我祖家說話。",
        "from": "X69n1320_001:0001c07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端的要人妙悟。",
        "from": "X69n1320_001:0001c07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不容擬議商量。",
        "from": "X69n1320_001:0001c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教伊立地得徹去。",
        "from": "X69n1320_001:0001c08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當下便了去。",
        "from": "X69n1320_001:0001c08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詎圖陳露布。",
        "from": "X69n1320_001:0001c09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "益葛藤乎。",
        "from": "X69n1320_001:0001c09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藉風颺塵。",
        "from": "X69n1320_001:0001c09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而眯人目乎。",
        "from": "X69n1320_001:0001c09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且夫奮大機顯大用。",
        "from": "X69n1320_001:0001c10_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何似馬祖黃檗老婆心最切。",
        "from": "X69n1320_001:0001c10_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其承稟。",
        "from": "X69n1320_001:0001c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則奚如百丈臨濟賣弄處偏奇。",
        "from": "X69n1320_001:0001c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾何忍置此等事。",
        "from": "X69n1320_001:0001c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不一提撕。",
        "from": "X69n1320_001:0001c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以痛快千古邪。",
        "from": "X69n1320_001:0001c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "予曰。",
        "from": "X69n1320_001:0001c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師更欲搊人鼻孔。",
        "from": "X69n1320_001:0001c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令其知痛邪。",
        "from": "X69n1320_001:0001c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又將施棒於眾。",
        "from": "X69n1320_001:0001c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲我還拳邪。",
        "from": "X69n1320_001:0001c13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師笑而無語。",
        "from": "X69n1320_001:0001c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "予於是邀諸明公發心。",
        "from": "X69n1320_001:0001c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捐俸梓行。",
        "from": "X69n1320_001:0001c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庶留心斯道者。",
        "from": "X69n1320_001:0001c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得此馳求頓歇。",
        "from": "X69n1320_001:0001c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "默爾知歸。",
        "from": "X69n1320_001:0001c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相與四尊宿。",
        "from": "X69n1320_001:0001c15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可謂千載一遇者也。",
        "from": "X69n1320_001:0001c15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾師勉是刻。",
        "from": "X69n1320_001:0001c16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈小補哉。",
        "from": "X69n1320_001:0001c16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時",
        "from": "X69n1320_001:0001c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬曆己丑臘八日。",
        "from": "X69n1320_001:0001c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東安靜山居士　解寧　識",
        "from": "X69n1320_001:0001c18_1",
        "context_with_next_sentence": false
    }
]