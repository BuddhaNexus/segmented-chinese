[
    {
        "source_sentence": "毘沙門天王經",
        "from": "T21n1244_001:0215a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅",
        "from": "T21n1244_001:0215a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國公食邑三千戶贈司空諡大鑒",
        "from": "T21n1244_001:0215a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正號大廣智大興善寺三藏法師",
        "from": "T21n1244_001:0215a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空奉　詔譯",
        "from": "T21n1244_001:0215a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時毘沙門天王。",
        "from": "T21n1244_001:0215a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在於佛前合掌。",
        "from": "T21n1244_001:0215a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊。",
        "from": "T21n1244_001:0215a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為未來諸有情等利益安樂。",
        "from": "T21n1244_001:0215a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豐饒財寶護持國界故。",
        "from": "T21n1244_001:0215a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說自真言。",
        "from": "T21n1244_001:0215a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我此真言。",
        "from": "T21n1244_001:0215a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如真多摩尼寶王能滿眾願。",
        "from": "T21n1244_001:0215a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊聽許我說。",
        "from": "T21n1244_001:0215a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言善哉善哉天王。",
        "from": "T21n1244_001:0215a12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝能愍念為諸有情。",
        "from": "T21n1244_001:0215a13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恣汝意說。",
        "from": "T21n1244_001:0215a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時毘沙門天王歡喜無量即於佛前說心真言曰。",
        "from": "T21n1244_001:0215a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨囉怛曩(二合)怛囉(二合)夜(引)野(一)曩謨吠室囉(二合)麼拏(鼻引)野(二)摩賀(引)囉(引)惹(引)野(三)薩嚩薩怛嚩(二合)曩(引)麼(鼻引四)舍(引)跛哩布(引)囉拏(鼻引)野(五)悉地迦囉(引)野(六)蘇(上)騫娜娜(引)野(七)怛娑每(二合引)曩莫塞訖哩(三合)怛嚩(二合八)伊(上)[牟*含](引)吠(引)室囉(二合)麼拏(鼻引)紇哩(二合)乃野(九)麼(引)襪多以灑(引)弭(十)薩嚩薩怛嚩(二合)蘇(上)佉(去引)嚩憾(十一)怛儞也(二合)他(去引十二)唵悉地悉地(十三)蘇(上)母蘇(上)母(十四)左(上)左(上)左(上)左(上十五)左囉左囉(十六)娑囉娑囉(十七)羯囉羯囉(十八)抧里抧里(十九)矩嚕矩嚕(二十)母嚕母嚕(二十一)主嚕主嚕(二十二)娑(去引)馱野遏貪麼麼(二十三)[寧*頁]底也(二合)末他弩(鼻引)婆(去)嚩娑嚩(二合)賀(二十四)吠室囉(二合)麼拏(鼻引)野娑嚩(二合引)賀(二十五)馱曩娜(引)野娑嚩(二合)賀(引二十六)麼(鼻)拏(鼻引)囉他(二十七)跛哩布(引)囉迦(引)野娑嚩(二合)賀(引二十八)爾時毘沙門天王說此真言已。",
        "from": "T21n1244_001:0215a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊。",
        "from": "T21n1244_001:0215b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今說受持真言法。",
        "from": "T21n1244_001:0215b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先取安悉香白檀香龍腦香多櫱囉香薰陸香蘇合香。",
        "from": "T21n1244_001:0215b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和合此香。",
        "from": "T21n1244_001:0215b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養我毘沙門天王。",
        "from": "T21n1244_001:0215b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若迎請時結根本印。",
        "from": "T21n1244_001:0215b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二頭指向身三招。",
        "from": "T21n1244_001:0215b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦真言七遍頂上散印。",
        "from": "T21n1244_001:0215b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也(二合)他(去引一)曩謨吠(引)室囉(二合)麼(鼻音呼)拏(鼻音引)野(二)曩謨(引)馱曩娜(引)野(三)馱寗(寧定反引)濕嚩(二合)囉(引)野(四)阿(去引)櫱(言羯反)蹉(去引)櫱蹉(五)阿跛哩弭多馱甯(引)濕嚩(二合)囉(六)鉢囉麼(鼻)迦(引)嚕抳迦(七)薩嚩薩答嚩(二合)呬(馨異反)多(上)唧多(八)麼麼馱曩麼(鼻音)拏(鼻音)鉢囉(二合)拽(延結反)蹉(九)娑嚩(二合)琰麼(鼻音引)蘗嗟娑嚩(二合)賀(引十)行者念誦常無間斷。",
        "from": "T21n1244_001:0215b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至毘沙門天王子赦儞娑。",
        "from": "T21n1244_001:0215b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現童子形告持誦者言。",
        "from": "T21n1244_001:0215b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝有何事請召我父。",
        "from": "T21n1244_001:0215b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持誦者言。",
        "from": "T21n1244_001:0215b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為供養三寶受與我財寶。",
        "from": "T21n1244_001:0215b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "童子赦儞娑於須臾頃。",
        "from": "T21n1244_001:0215b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還至毘沙門天王所。",
        "from": "T21n1244_001:0215b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告父王言。",
        "from": "T21n1244_001:0215b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持誦者求諸財寶。",
        "from": "T21n1244_001:0215b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為供養故利益有情。",
        "from": "T21n1244_001:0215b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘沙門天王告童子赦儞娑言。",
        "from": "T21n1244_001:0215b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝日日與金錢一百乃至壽終。",
        "from": "T21n1244_001:0215b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其童子赦儞娑。",
        "from": "T21n1244_001:0215b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日日送金錢一百。",
        "from": "T21n1244_001:0215b22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與持誦者安於頭邊。",
        "from": "T21n1244_001:0215b23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其金錢異種香氣。",
        "from": "T21n1244_001:0215b23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先願所得之者。",
        "from": "T21n1244_001:0215b24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除自受用外應行捨施。",
        "from": "T21n1244_001:0215b24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應貯積而懷慳悋。",
        "from": "T21n1244_001:0215b25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常於一切有情起大慈心勿生嗔恚。",
        "from": "T21n1244_001:0215b25_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以殊勝香花飲食燈明。",
        "from": "T21n1244_001:0215b26_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於寂靜處如法供養佛法僧寶。",
        "from": "T21n1244_001:0215b26_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼復思惟而無間斷。",
        "from": "T21n1244_001:0215b27_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為毘沙門天王并諸眷屬。",
        "from": "T21n1244_001:0215b28_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念恩德故常應誦吉祥讚。",
        "from": "T21n1244_001:0215b28_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼天王獲諸吉慶。",
        "from": "T21n1244_001:0215b29_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願毘沙門天王男女眷屬內外親姻輔弼。",
        "from": "T21n1244_001:0215b29_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至使者及諸營從。",
        "from": "T21n1244_001:0215b30_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國界有情佛所稱讚十種福利悉皆獲得。",
        "from": "T21n1244_001:0215c01_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂一者淨信二者戒。",
        "from": "T21n1244_001:0215c02_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者聞四者捨。",
        "from": "T21n1244_001:0215c02_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者受六者慧。",
        "from": "T21n1244_001:0215c03_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七者形貌八者力。",
        "from": "T21n1244_001:0215c03_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九者辯十者色聲香味觸富貴自在。",
        "from": "T21n1244_001:0215c03_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於佛法中而開法眼。",
        "from": "T21n1244_001:0215c04_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證得聖果獲得甘露妙法。",
        "from": "T21n1244_001:0215c05_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦得三十七品助佛道法。",
        "from": "T21n1244_001:0215c05_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持誦者每日作如是發願。",
        "from": "T21n1244_001:0215c06_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘沙門天王即生歡喜告自營從眷屬。",
        "from": "T21n1244_001:0215c06_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等觀彼持誦者於我深生恭敬。",
        "from": "T21n1244_001:0215c07_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復告子赦儞娑言。",
        "from": "T21n1244_001:0215c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持誦者希望。",
        "from": "T21n1244_001:0215c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲見我毘沙門藥叉王欲閉惡趣門。",
        "from": "T21n1244_001:0215c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所思勝願皆令滿足。",
        "from": "T21n1244_001:0215c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽無量百千歲。",
        "from": "T21n1244_001:0215c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲得如意寶飛騰虛空。",
        "from": "T21n1244_001:0215c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安怛那及得伏藏。",
        "from": "T21n1244_001:0215c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若男若女及囉惹皆令敬愛。",
        "from": "T21n1244_001:0215c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦解一切禽獸語言。",
        "from": "T21n1244_001:0215c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令得豐財永離貧匱。",
        "from": "T21n1244_001:0215c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼持誦者當於白月八日及十五日。",
        "from": "T21n1244_001:0215c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令畫人受八戒澡浴著新淨衣。",
        "from": "T21n1244_001:0215c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取不截白[疊*毛]畫像。",
        "from": "T21n1244_001:0215c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其彩色中不用皮膠。",
        "from": "T21n1244_001:0215c15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中心畫釋迦牟尼佛作說法相。",
        "from": "T21n1244_001:0215c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛右邊畫吉祥天女形。",
        "from": "T21n1244_001:0215c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼目廣長顏貌寂靜。",
        "from": "T21n1244_001:0215c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "首戴天冠瓔珞臂釧莊嚴其身。",
        "from": "T21n1244_001:0215c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手作施願手左手執開敷蓮花。",
        "from": "T21n1244_001:0215c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫像得已於清淨處。",
        "from": "T21n1244_001:0215c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安像供養以塗香花鬘燒香飲食燈明。",
        "from": "T21n1244_001:0215c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以供養佛及吉祥天女。",
        "from": "T21n1244_001:0215c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持者不應以下劣心而生恐怖。",
        "from": "T21n1244_001:0215c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應以決定心。",
        "from": "T21n1244_001:0215c21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如法念誦此吉祥天女真言曰。",
        "from": "T21n1244_001:0215c22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫室哩(二合)伽(去)曩(引)野曩謨吠(引)室囉(二合)摩拏(上引)野(二)摩賀(引)藥乞灑(二合)囉(引)惹(引)地囉(引)惹野(三)曩莫室哩(二合)夜(引)&#;(引四)摩賀(引)禰(引)吠(鼻引五)怛儞也(二合)他(去引六)唵怛囉怛囉(七)咄嚕咄嚕(八)蘇瑟貙(二合)蘇瑟貙(二合下貙勅數反上同九)麼抳迦曩迦(十)嚩日嚩(二合)吠(引)女(拏數反引)哩也(二合十一)穆訖多(二合)曩(引)麼(引)稜(去)訖[口*呂](二合)多(十二)僕(引十三)薩嚩薩怛嚩(二合十四)呬多迦(引)麼(十五)吠(引)室囉(二合)摩拏(十六)室哩(二合)野泥(上)尾(引十七)末臘毘(去十八)曀醯(去引)呬(十九)具囉拏(二合鼻)具囉拏(二合鼻二十)麼娑麼娑(二十一)捺囉(二合)捨野悉地(二十二)娜娜(引)呬銘(二十三)捺囉(二合)捨曩迦(引)麼寫(二十四)捺囉(二合)捨南(二十五)鉢囉(二合)賀攞(二合)賀攞(二合引)娜野麼諾娑縛(二合引)賀(引二十六)爾時毘沙門天王。",
        "from": "T21n1244_001:0215c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見持誦此真言及供養如來。",
        "from": "T21n1244_001:0216a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愍念行者。",
        "from": "T21n1244_001:0216a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則為現身作童子形或居士形。",
        "from": "T21n1244_001:0216a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持如意寶左手持金篋。",
        "from": "T21n1244_001:0216a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顏貌寂靜來至像前。",
        "from": "T21n1244_001:0216a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮佛像已告行者言。",
        "from": "T21n1244_001:0216a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今於我欲求何願。",
        "from": "T21n1244_001:0216a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為入修羅窟耶。",
        "from": "T21n1244_001:0216a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為求伏藏耶。",
        "from": "T21n1244_001:0216a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為求伏火水銀耶。",
        "from": "T21n1244_001:0216a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為求安怛那囉惹敬愛耶。",
        "from": "T21n1244_001:0216a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雄黃成就耶安膳那藥成就耶。",
        "from": "T21n1244_001:0216a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持明成就耶飛騰虛空耶受命一大劫耶。",
        "from": "T21n1244_001:0216a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等願悉能成就。",
        "from": "T21n1244_001:0216a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持誦者白毘沙門天王言。",
        "from": "T21n1244_001:0216a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願我一切處通達。",
        "from": "T21n1244_001:0216a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲得金銀無盡名稱福德。",
        "from": "T21n1244_001:0216a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽命無量劫飛騰虛空。",
        "from": "T21n1244_001:0216a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變化種種瑜伽自在。",
        "from": "T21n1244_001:0216a21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘沙門言隨汝所願。",
        "from": "T21n1244_001:0216a22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時毘沙門天王。",
        "from": "T21n1244_001:0216a22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲重明其義而說偈言。",
        "from": "T21n1244_001:0216a23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　假使有日月　　從空墮於地",
        "from": "T21n1244_001:0216a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　或大地傾覆　　寧有如是事",
        "from": "T21n1244_001:0216a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　不應生少疑　　此法易成就",
        "from": "T21n1244_001:0216a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　不假於齋戒　　利益貧匱者",
        "from": "T21n1244_001:0216a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一切人恭敬　　乃至壽命盡",
        "from": "T21n1244_001:0216a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　毘沙門加持　　遠離諸厄難",
        "from": "T21n1244_001:0216a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　藥叉將衛護　　常隨受持者",
        "from": "T21n1244_001:0216b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若能持是教　　諸願悉成就",
        "from": "T21n1244_001:0216b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　迅疾如射箭　　諸王敬彼人",
        "from": "T21n1244_001:0216b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　獲得無盡寶　　千俱胝藥叉",
        "from": "T21n1244_001:0216b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　衛護持誦者　　能滿諸勝願",
        "from": "T21n1244_001:0216b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　解脫諸惡趣　　若見毘沙門",
        "from": "T21n1244_001:0216b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　俱尾羅財施　　獲得大智慧",
        "from": "T21n1244_001:0216b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　乃至天眼通　　壽命俱胝歲",
        "from": "T21n1244_001:0216b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若人殷重心　　愛敬此教法",
        "from": "T21n1244_001:0216b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　應當求成就　　決定無有疑",
        "from": "T21n1244_001:0216b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　今此護身法　　多聞天所說",
        "from": "T21n1244_001:0216b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　由此加持故　　真言上悉地",
        "from": "T21n1244_001:0216b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　即誦護身明",
        "from": "T21n1244_001:0216b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨囉怛曩(二合)怛囉(二合)野(引上聲)野(一)曩謨吠(引)室囉(二合)麼(鼻)拏(鼻引)野(二)摩賀(引)囉(引)惹(引)野(三)怛儞也(二合)他(去引)唵(四)[口*朗](轉舌)誐[口*朗](准上)誐(五)赦(吒簡反下同)拏赦拏(六)齲(區宇反下文同)拏(拏數反鼻音數字上聲)齲弩(准上音七)摩賀(引)囉(引)惹(八)灑乞灑(二合)灑乞灑(二合)[牟*含](莫感反引鼻九)薩冒(毛保反引)鉢捺囉(二合)吠(引)毘藥(二合)娑嚩(二合引)賀(引十)我今說根本印。",
        "from": "T21n1244_001:0216b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手右押左內相叉。",
        "from": "T21n1244_001:0216b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二名指頭相合。",
        "from": "T21n1244_001:0216b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指如鉤。",
        "from": "T21n1244_001:0216b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若迎請時向身招。",
        "from": "T21n1244_001:0216b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若發遣時向外撥。",
        "from": "T21n1244_001:0216b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦時結印當心誦七遍即頂上散。",
        "from": "T21n1244_001:0216b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後取念珠專注念誦。",
        "from": "T21n1244_001:0216b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說吉祥天女身印。",
        "from": "T21n1244_001:0216b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手虛心合掌。",
        "from": "T21n1244_001:0216b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開二頭指二中指二無名指。",
        "from": "T21n1244_001:0216b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈如蓮華形。",
        "from": "T21n1244_001:0216b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指二小指竪合。",
        "from": "T21n1244_001:0216b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若念誦時當心結誦真言七遍頂上散。",
        "from": "T21n1244_001:0216b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘沙門天王呪曰。",
        "from": "T21n1244_001:0216b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那謨裴鑠囉皤拏寫(一)摩訶曷囉闍寫施鞞(二)娑婆呵(三)施皤跋跌犁娑婆訶若呪淨油七遍用塗臥所。",
        "from": "T21n1244_001:0216b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乞財物等得如所願。",
        "from": "T21n1244_001:0216c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘沙門天王經",
        "from": "T21n1244_001:0216c04_0",
        "context_with_next_sentence": false
    }
]