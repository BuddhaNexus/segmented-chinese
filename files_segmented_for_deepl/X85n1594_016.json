[
    {
        "source_sentence": "佛祖綱目卷第十六(張字號)",
        "from": "X85n1594_016:0578c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甲子(漢文帝三年起)乙酉(景帝元年)辛丑(武帝建元元年年號自此始)丁未(改元元光)癸丑(改元元朔)己未(改元元狩)癸亥(漢武帝元狩五年止)(庚辰)十五祖迦那提婆傳法羅睺羅多迦那提婆。",
        "from": "X85n1594_016:0578c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南天竺人。",
        "from": "X85n1594_016:0578c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姓毗舍羅。",
        "from": "X85n1594_016:0578c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初求福業。",
        "from": "X85n1594_016:0578c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼樂辯論。",
        "from": "X85n1594_016:0578c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後謁龍樹。",
        "from": "X85n1594_016:0578c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將及門。",
        "from": "X85n1594_016:0578c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍樹知是智人。",
        "from": "X85n1594_016:0578c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先遣侍者。",
        "from": "X85n1594_016:0578c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以滿鉢水。",
        "from": "X85n1594_016:0578c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置於座前。",
        "from": "X85n1594_016:0578c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提婆覩之。",
        "from": "X85n1594_016:0578c21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以一鍼投之而進。",
        "from": "X85n1594_016:0578c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欣然契會。",
        "from": "X85n1594_016:0578c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍樹即為說法。",
        "from": "X85n1594_016:0578c22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不起於座。",
        "from": "X85n1594_016:0578c22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現月輪相。",
        "from": "X85n1594_016:0578c22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提婆既得法已。",
        "from": "X85n1594_016:0578c22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名震五天。",
        "from": "X85n1594_016:0578c23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然猶以人不信用其言為憂。",
        "from": "X85n1594_016:0578c23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時天竺有大自在天人。",
        "from": "X85n1594_016:0578c23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身真金色。",
        "from": "X85n1594_016:0578c24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高二丈。",
        "from": "X85n1594_016:0578c24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人有所求。",
        "from": "X85n1594_016:0578c24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆如所願。",
        "from": "X85n1594_016:0578c24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提婆造廟見之。",
        "from": "X85n1594_016:0579a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬眾隨入。",
        "from": "X85n1594_016:0579a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "像果瞬視若怒。",
        "from": "X85n1594_016:0579a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提婆曰。",
        "from": "X85n1594_016:0579a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神則神矣。",
        "from": "X85n1594_016:0579a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何其小哉。",
        "from": "X85n1594_016:0579a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正當以威靈感人。",
        "from": "X85n1594_016:0579a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智德化物。",
        "from": "X85n1594_016:0579a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而假金為軀。",
        "from": "X85n1594_016:0579a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玻璃為目。",
        "from": "X85n1594_016:0579a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以妖世。",
        "from": "X85n1594_016:0579a03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非所望也。",
        "from": "X85n1594_016:0579a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即梯其肩。",
        "from": "X85n1594_016:0579a03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鑿出目睛。",
        "from": "X85n1594_016:0579a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀者疑之曰。",
        "from": "X85n1594_016:0579a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大自在天。",
        "from": "X85n1594_016:0579a04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃為一小婆羅門所折困耶。",
        "from": "X85n1594_016:0579a04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提婆曰。",
        "from": "X85n1594_016:0579a05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神明遠大。",
        "from": "X85n1594_016:0579a05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故以近事試我。",
        "from": "X85n1594_016:0579a05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我得其心。",
        "from": "X85n1594_016:0579a05_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故敢爾也。",
        "from": "X85n1594_016:0579a05_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是辦供。",
        "from": "X85n1594_016:0579a06_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是夜大自在天降以受之曰。",
        "from": "X85n1594_016:0579a06_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝得我心。",
        "from": "X85n1594_016:0579a06_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人得我形。",
        "from": "X85n1594_016:0579a07_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝以心供。",
        "from": "X85n1594_016:0579a07_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人以質饋。",
        "from": "X85n1594_016:0579a07_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知而敬我者汝。",
        "from": "X85n1594_016:0579a07_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畏而誣我者人。",
        "from": "X85n1594_016:0579a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然汝供甚美。",
        "from": "X85n1594_016:0579a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但乏我所欲。",
        "from": "X85n1594_016:0579a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提婆曰。",
        "from": "X85n1594_016:0579a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神須何物。",
        "from": "X85n1594_016:0579a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大自在天人曰。",
        "from": "X85n1594_016:0579a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我缺左目。",
        "from": "X85n1594_016:0579a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能施我乎。",
        "from": "X85n1594_016:0579a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提婆笑即出自己目與之。",
        "from": "X85n1594_016:0579a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愈出而愈不竭。",
        "from": "X85n1594_016:0579a10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自旦及暮。",
        "from": "X85n1594_016:0579a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出目睛數萬。",
        "from": "X85n1594_016:0579a10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神讚曰。",
        "from": "X85n1594_016:0579a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉摩衲。",
        "from": "X85n1594_016:0579a11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真上施也。",
        "from": "X85n1594_016:0579a11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲何所求。",
        "from": "X85n1594_016:0579a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提婆曰。",
        "from": "X85n1594_016:0579a11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我稟明於心。",
        "from": "X85n1594_016:0579a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不假外也。",
        "from": "X85n1594_016:0579a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後至迦毗羅國。",
        "from": "X85n1594_016:0579a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼有長者。",
        "from": "X85n1594_016:0579a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰梵摩淨德。",
        "from": "X85n1594_016:0579a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日園樹生耳。",
        "from": "X85n1594_016:0579a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如菌。",
        "from": "X85n1594_016:0579a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "味甚美。",
        "from": "X85n1594_016:0579a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟長者與第二子羅睺羅多。",
        "from": "X85n1594_016:0579a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取而食之。",
        "from": "X85n1594_016:0579a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取已隨長。",
        "from": "X85n1594_016:0579a14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡而復生。",
        "from": "X85n1594_016:0579a14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自餘親屬皆不能見。",
        "from": "X85n1594_016:0579a14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提婆知其宿因。",
        "from": "X85n1594_016:0579a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂至其家。",
        "from": "X85n1594_016:0579a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長者乃問其故。",
        "from": "X85n1594_016:0579a15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提婆曰。",
        "from": "X85n1594_016:0579a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝家昔曾供養一比丘。",
        "from": "X85n1594_016:0579a16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然此比丘道眼未明。",
        "from": "X85n1594_016:0579a16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以虗沾信施。",
        "from": "X85n1594_016:0579a17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故報為木菌。",
        "from": "X85n1594_016:0579a17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟汝與子。",
        "from": "X85n1594_016:0579a17_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精勤供養。",
        "from": "X85n1594_016:0579a17_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得以享之。",
        "from": "X85n1594_016:0579a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘則否矣。",
        "from": "X85n1594_016:0579a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又問。",
        "from": "X85n1594_016:0579a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長者年多少。",
        "from": "X85n1594_016:0579a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "X85n1594_016:0579a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七十有九。",
        "from": "X85n1594_016:0579a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提婆乃說偈曰。",
        "from": "X85n1594_016:0579a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入道不通理。",
        "from": "X85n1594_016:0579a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復身還信施。",
        "from": "X85n1594_016:0579a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝年八十一。",
        "from": "X85n1594_016:0579a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此樹不生耳。",
        "from": "X85n1594_016:0579a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長者聞偈已。",
        "from": "X85n1594_016:0579a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深加嘆伏。",
        "from": "X85n1594_016:0579a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且曰。",
        "from": "X85n1594_016:0579a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弟子衰老。",
        "from": "X85n1594_016:0579a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能事師。",
        "from": "X85n1594_016:0579a21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願捨次子。",
        "from": "X85n1594_016:0579a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨師出家。",
        "from": "X85n1594_016:0579a21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提婆曰。",
        "from": "X85n1594_016:0579a21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔如來記此子。",
        "from": "X85n1594_016:0579a21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當第二五百年。",
        "from": "X85n1594_016:0579a22_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為大教主。",
        "from": "X85n1594_016:0579a22_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今之相遇。",
        "from": "X85n1594_016:0579a22_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓋符宿因。",
        "from": "X85n1594_016:0579a22_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即與剃髮。",
        "from": "X85n1594_016:0579a23_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至巴連弗城。",
        "from": "X85n1594_016:0579a23_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞諸外道欲障佛法。",
        "from": "X85n1594_016:0579a23_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "計之既久。",
        "from": "X85n1594_016:0579a23_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提婆乃執長旛。",
        "from": "X85n1594_016:0579a24_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入彼眾中。",
        "from": "X85n1594_016:0579a24_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼問曰。",
        "from": "X85n1594_016:0579a24_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝何不前。",
        "from": "X85n1594_016:0579a24_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提婆曰。",
        "from": "X85n1594_016:0579a24_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝何不後。",
        "from": "X85n1594_016:0579b01_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼曰。",
        "from": "X85n1594_016:0579b01_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝似賤人。",
        "from": "X85n1594_016:0579b01_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提婆曰。",
        "from": "X85n1594_016:0579b01_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝似良人。",
        "from": "X85n1594_016:0579b01_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼曰汝解何法。",
        "from": "X85n1594_016:0579b01_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提婆曰。",
        "from": "X85n1594_016:0579b02_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝百不解。",
        "from": "X85n1594_016:0579b02_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼曰。",
        "from": "X85n1594_016:0579b02_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我欲得佛。",
        "from": "X85n1594_016:0579b02_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提婆曰。",
        "from": "X85n1594_016:0579b02_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我灼然得佛。",
        "from": "X85n1594_016:0579b03_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼曰。",
        "from": "X85n1594_016:0579b03_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝不合得。",
        "from": "X85n1594_016:0579b03_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提婆曰。",
        "from": "X85n1594_016:0579b03_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元道我得。",
        "from": "X85n1594_016:0579b03_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝實不得。",
        "from": "X85n1594_016:0579b03_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼曰。",
        "from": "X85n1594_016:0579b04_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝既不得。",
        "from": "X85n1594_016:0579b04_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何言得。",
        "from": "X85n1594_016:0579b04_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提婆曰。",
        "from": "X85n1594_016:0579b04_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝有我故。",
        "from": "X85n1594_016:0579b04_54",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以不得。",
        "from": "X85n1594_016:0579b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我無我故。",
        "from": "X85n1594_016:0579b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我自當得。",
        "from": "X85n1594_016:0579b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼詞既屈。",
        "from": "X85n1594_016:0579b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃問提婆曰。",
        "from": "X85n1594_016:0579b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝名何等。",
        "from": "X85n1594_016:0579b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰我名迦那提婆。",
        "from": "X85n1594_016:0579b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼既夙聞提婆名。",
        "from": "X85n1594_016:0579b06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃悔過致謝。",
        "from": "X85n1594_016:0579b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時眾中猶互興問難。",
        "from": "X85n1594_016:0579b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提婆折以無礙之辨。",
        "from": "X85n1594_016:0579b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繇是歸伏。",
        "from": "X85n1594_016:0579b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃告上足羅睺羅多。",
        "from": "X85n1594_016:0579b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而付法眼。",
        "from": "X85n1594_016:0579b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "X85n1594_016:0579b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本對傳法人。",
        "from": "X85n1594_016:0579b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為說解脫理。",
        "from": "X85n1594_016:0579b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於法實無證。",
        "from": "X85n1594_016:0579b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無終亦無始。",
        "from": "X85n1594_016:0579b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說偈已。",
        "from": "X85n1594_016:0579b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入奮迅定。",
        "from": "X85n1594_016:0579b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身放八光。",
        "from": "X85n1594_016:0579b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而歸寂滅。",
        "from": "X85n1594_016:0579b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "學眾興塔供養。",
        "from": "X85n1594_016:0579b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當此土漢文帝十九年庚辰歲也○漢武帝元狩二年。",
        "from": "X85n1594_016:0579b10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "驃騎將軍霍去病討匈奴。",
        "from": "X85n1594_016:0579b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過居延。",
        "from": "X85n1594_016:0579b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擒休屠王。",
        "from": "X85n1594_016:0579b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲其金人。",
        "from": "X85n1594_016:0579b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "率長丈餘。",
        "from": "X85n1594_016:0579b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝以為大神。",
        "from": "X85n1594_016:0579b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "列於甘泉宮。",
        "from": "X85n1594_016:0579b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不祭祀。",
        "from": "X85n1594_016:0579b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但燒香禮拜而已。",
        "from": "X85n1594_016:0579b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此佛道流通之漸也。",
        "from": "X85n1594_016:0579b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及開西域。",
        "from": "X85n1594_016:0579b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遣張騫使大夏。",
        "from": "X85n1594_016:0579b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還云。",
        "from": "X85n1594_016:0579b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身毒國有浮屠之教云。",
        "from": "X85n1594_016:0579b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元狩三年。",
        "from": "X85n1594_016:0579b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "京師掘昆明池。",
        "from": "X85n1594_016:0579b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得黑灰。",
        "from": "X85n1594_016:0579b16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以問東方朔。",
        "from": "X85n1594_016:0579b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朔曰。",
        "from": "X85n1594_016:0579b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可問西域胡道人。",
        "from": "X85n1594_016:0579b16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及後永明中。",
        "from": "X85n1594_016:0579b17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩騰至。",
        "from": "X85n1594_016:0579b17_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有問之者。",
        "from": "X85n1594_016:0579b17_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "騰曰。",
        "from": "X85n1594_016:0579b17_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此劫灰也。",
        "from": "X85n1594_016:0579b17_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛祖綱目卷第十六",
        "from": "X85n1594_016:0579b19_0",
        "context_with_next_sentence": false
    }
]