[
    {
        "source_sentence": "佛說寶帶陀羅尼經",
        "from": "T21n1377_001:0898c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝奉大夫試光祿卿",
        "from": "T21n1377_001:0898c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳法大師賜紫臣施護奉　詔譯",
        "from": "T21n1377_001:0898c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T21n1377_001:0898c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在釋種住處迦毘羅城尼拘律陀樹園。",
        "from": "T21n1377_001:0898c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大眾俱。",
        "from": "T21n1377_001:0898c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時羅睺羅童子居止異處。",
        "from": "T21n1377_001:0898c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於夜分中忽為大惡羅剎所怖。",
        "from": "T21n1377_001:0898c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時羅睺羅即於是夜。",
        "from": "T21n1377_001:0898c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與諸童子眾前後圍繞。",
        "from": "T21n1377_001:0899a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出迦毘羅城往詣佛所。",
        "from": "T21n1377_001:0899a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到已頭面禮佛雙足。",
        "from": "T21n1377_001:0899a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃於佛前涕泣而住。",
        "from": "T21n1377_001:0899a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊知是事已。",
        "from": "T21n1377_001:0899a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即告尊者阿難言。",
        "from": "T21n1377_001:0899a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難當知我有大明。",
        "from": "T21n1377_001:0899a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名曰寶帶有大功德。",
        "from": "T21n1377_001:0899a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能為羅睺羅童子而作擁護。",
        "from": "T21n1377_001:0899a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今宣說汝當受持。",
        "from": "T21n1377_001:0899a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而此大明若諸天眾龍眾。",
        "from": "T21n1377_001:0899a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜叉眾羅剎眾。",
        "from": "T21n1377_001:0899a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿修羅眾迦樓羅眾。",
        "from": "T21n1377_001:0899a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乾闥婆眾緊那羅眾。",
        "from": "T21n1377_001:0899a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必舍左眾必隷多眾。",
        "from": "T21n1377_001:0899a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳩盤茶眾布單那眾。",
        "from": "T21n1377_001:0899a09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羯吒布單那眾諸宿曜眾。",
        "from": "T21n1377_001:0899a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十八大夜叉將。",
        "from": "T21n1377_001:0899a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四大夜叉主四大天王。",
        "from": "T21n1377_001:0899a10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸離欲仙眾雪山住者。",
        "from": "T21n1377_001:0899a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至十方一切諸部多眾并羅剎女眾。",
        "from": "T21n1377_001:0899a11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦儞里布寫那哩計吒羯蘭拏蘇摩那摩賀訶哩等諸母鬼眾。",
        "from": "T21n1377_001:0899a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三母訥誐彌羯臘波大羅剎。",
        "from": "T21n1377_001:0899a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及東方訶哩大羅剎女。",
        "from": "T21n1377_001:0899a14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南方虞拏西大羅剎女。",
        "from": "T21n1377_001:0899a15_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方計舍哩大羅剎女。",
        "from": "T21n1377_001:0899a15_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方珠髻大羅剎女。",
        "from": "T21n1377_001:0899a16_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此四大羅剎女各與六十眷屬俱。",
        "from": "T21n1377_001:0899a16_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等眾若聞宣說寶帶大明。",
        "from": "T21n1377_001:0899a17_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切處能為一切眾生而作擁護。",
        "from": "T21n1377_001:0899a18_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊即說寶帶大明曰。",
        "from": "T21n1377_001:0899a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竭儞(引)屹囉(二合引一句)竭那鉢底(二)瞿彌(引)屹囉(二合引三)瞿摩鉢底(四)鉢哩(引)迦(五)竭哩尼(六)竭囉鉢底(七)娑(引)囉鉢底(八)達那鉢底(九)蘇囉鉢底(十)阿梨波(十一)阿梨波鉢底(十二)摩鉢底(十三)阿鉢底(十四)末虎鉢底(十五)虎羅(十六)呬梨(十七)虎盧(十八)底梨(十九)呬梨(二十)唧致(二十一)梨鉢多(二合二十二)鉢跓(引)吒吒吠(引二十三)唵(引二十四)藍尾(引二十五)摩尾薩哩(引二十六)摩賀(引)呬梨(二十七)虎虎(二十八)三滿多薩薩哩(引二十九)阿三滿多薩囉鉢梨(引三十)珂薩哩(引三十一)阿(引)嚩哩多(二合三十二)摩賀(引)嚩哩多(二合三十三)竭儞(引三十四)彌(引)珂梨(引三十五)佛言阿難。",
        "from": "T21n1377_001:0899a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我此宣說寶帶大明有大功德。",
        "from": "T21n1377_001:0899b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有持是明者。",
        "from": "T21n1377_001:0899b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨諸所求皆得圓滿。",
        "from": "T21n1377_001:0899b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有持明天眾部多眾。",
        "from": "T21n1377_001:0899b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吠多拏眾大力天眾。",
        "from": "T21n1377_001:0899b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸神通精進仙眾。",
        "from": "T21n1377_001:0899b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於持明者不能為障。",
        "from": "T21n1377_001:0899b06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難當知今此寶帶大明。",
        "from": "T21n1377_001:0899b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛世尊共所宣說。",
        "from": "T21n1377_001:0899b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普為一切眾生利益安樂。",
        "from": "T21n1377_001:0899b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至於一切處密作擁護。",
        "from": "T21n1377_001:0899b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難大地所有大惡難調攝人威光者極惡業者。",
        "from": "T21n1377_001:0899b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若方若隅乃至一切處。",
        "from": "T21n1377_001:0899b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞是寶帶大明時皆大驚怖。",
        "from": "T21n1377_001:0899b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此大明周匝十由旬內。",
        "from": "T21n1377_001:0899b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能為一切眾生而作擁護。",
        "from": "T21n1377_001:0899b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救諸難事攝持護念。",
        "from": "T21n1377_001:0899b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸眾生息災安樂。",
        "from": "T21n1377_001:0899b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為杖打不為刀傷不為毒中。",
        "from": "T21n1377_001:0899b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切惡毒自然消滅。",
        "from": "T21n1377_001:0899b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡友遠離一切惡者。",
        "from": "T21n1377_001:0899b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作禁縛事自然解脫。",
        "from": "T21n1377_001:0899b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人及非人。",
        "from": "T21n1377_001:0899b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲於一切眾生作惱害者。",
        "from": "T21n1377_001:0899b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當依法建立壇場。",
        "from": "T21n1377_001:0899b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以諸香華供養如來。",
        "from": "T21n1377_001:0899b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依法結界沐浴清淨。",
        "from": "T21n1377_001:0899b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦是寶帶大明。",
        "from": "T21n1377_001:0899b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼諸惡者自然攝伏不能為害。",
        "from": "T21n1377_001:0899b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至一切病苦亦得息除。",
        "from": "T21n1377_001:0899b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說大明曰。",
        "from": "T21n1377_001:0899b19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛[寧*也](切身下同)他(引一句)儞黎儞黎(二)尾摩梨(引三)多摩梨(引四)虎唧虎唧(五)悉哩(六)底哩(七)薩梨(引)囉(八)復次阿難又此大明有大功德。",
        "from": "T21n1377_001:0899b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是大明時。",
        "from": "T21n1377_001:0899b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若鬼眾聞者驚怖戰掉。",
        "from": "T21n1377_001:0899b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若天眾聞者悉來集會。",
        "from": "T21n1377_001:0899b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若龍王若部多王聞者亦悉來集。",
        "from": "T21n1377_001:0899b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若梵王帝釋大自在天童子天等。",
        "from": "T21n1377_001:0899b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞是大明者皆從異處咸來集此。",
        "from": "T21n1377_001:0899b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱讚隨喜寶帶大明。",
        "from": "T21n1377_001:0899b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時慈氏菩薩摩訶薩。",
        "from": "T21n1377_001:0899b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊。",
        "from": "T21n1377_001:0899b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦為羅睺羅童子及一切眾生。",
        "from": "T21n1377_001:0899b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作擁護法救諸難事。",
        "from": "T21n1377_001:0899b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝持護念息災安樂。",
        "from": "T21n1377_001:0899b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀杖諸毒不能為害。",
        "from": "T21n1377_001:0899b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡友禁縛自然遠離。",
        "from": "T21n1377_001:0899c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以是利益故隨喜宣說寶帶大明曰。",
        "from": "T21n1377_001:0899c01_1",
        "context_with_next_sentence": true
    },
