[
    {
        "source_sentence": "佛說寶藏神大明曼拏羅儀軌經",
        "from": "T21n1283_001:0343b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卷上",
        "from": "T21n1283_001:0343b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試鴻臚少卿",
        "from": "T21n1283_001:0343b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳教大師臣法天奉　詔譯",
        "from": "T21n1283_001:0343b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞一時佛在楞伽國。",
        "from": "T21n1283_001:0343b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彼國中有尾鼻瑟拏羅叉王月軍羅叉王等。",
        "from": "T21n1283_001:0343b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於佛法中皆得不退。",
        "from": "T21n1283_001:0343b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有住大福德菩薩摩訶薩恭敬圍繞。",
        "from": "T21n1283_001:0343b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時彼世尊運神通力。",
        "from": "T21n1283_001:0343b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上往虛空至吠覽婆界。",
        "from": "T21n1283_001:0343b28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從空而下。",
        "from": "T21n1283_001:0343b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於曩尼嚕閉祖摩里那樹下安詳而坐。",
        "from": "T21n1283_001:0343b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時寶藏神。",
        "from": "T21n1283_001:0343c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜叉王阿脩羅王。",
        "from": "T21n1283_001:0343c01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母唧隷那龍王。",
        "from": "T21n1283_001:0343c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五通瓶五髻乾達婆王子。",
        "from": "T21n1283_001:0343c02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉隷枳羅羅叉王。",
        "from": "T21n1283_001:0343c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火頂緊曩羅王五髻緊曩羅王。",
        "from": "T21n1283_001:0343c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大樹緊曩羅王財主夜叉王。",
        "from": "T21n1283_001:0343c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有住不退地菩薩摩訶薩。",
        "from": "T21n1283_001:0343c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心大歡喜異口同音。",
        "from": "T21n1283_001:0343c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而白佛言。",
        "from": "T21n1283_001:0343c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "希有世尊希有善逝。",
        "from": "T21n1283_001:0343c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有過去如來大功德正法得未曾聞。",
        "from": "T21n1283_001:0343c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而諸眾生苦惱無量。",
        "from": "T21n1283_001:0343c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無主無宰無救無歸。",
        "from": "T21n1283_001:0343c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此正法能為主宰而能救拔。",
        "from": "T21n1283_001:0343c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊利益安樂天上人間一切眾生。",
        "from": "T21n1283_001:0343c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此祕密曼拏羅儀軌心真言。",
        "from": "T21n1283_001:0343c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生若聞行布施波羅蜜。",
        "from": "T21n1283_001:0343c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得阿耨多羅三藐三菩提安樂圓滿。",
        "from": "T21n1283_001:0343c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時復有夜叉等。",
        "from": "T21n1283_001:0343c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從座而起偏袒右肩。",
        "from": "T21n1283_001:0343c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右膝著地合掌恭敬。",
        "from": "T21n1283_001:0343c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白世尊言曼拏羅儀軌。",
        "from": "T21n1283_001:0343c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當為宣說願樂欲聞。",
        "from": "T21n1283_001:0343c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告彼夜叉王等大會之眾汝今諦聽。",
        "from": "T21n1283_001:0343c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子此正法。",
        "from": "T21n1283_001:0343c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是大夜叉王曼拏羅儀軌心真言。",
        "from": "T21n1283_001:0343c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真實祕密得未曾有。",
        "from": "T21n1283_001:0343c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來同所宣說。",
        "from": "T21n1283_001:0343c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有半尼所(二合)天子。",
        "from": "T21n1283_001:0343c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "末羅賀天子酥摩天子。",
        "from": "T21n1283_001:0343c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普酥摩天子惹野天子等。",
        "from": "T21n1283_001:0343c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其晨朝俱來佛所頭面禮足。",
        "from": "T21n1283_001:0343c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繞佛三匝住立一面。",
        "from": "T21n1283_001:0343c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時彼菩薩摩訶薩以其神力。",
        "from": "T21n1283_001:0343c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令曩尼囉閉祖摩里那樹下。",
        "from": "T21n1283_001:0343c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起大光明普照一切。",
        "from": "T21n1283_001:0343c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告彼菩薩摩訶薩言。",
        "from": "T21n1283_001:0343c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今欲說大富貴未曾有圓滿大法海。",
        "from": "T21n1283_001:0343c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等當受持信順奉行。",
        "from": "T21n1283_001:0343c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時寶藏神夜叉王。",
        "from": "T21n1283_001:0343c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從座而起偏袒右肩。",
        "from": "T21n1283_001:0343c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右膝著地合掌恭敬。",
        "from": "T21n1283_001:0343c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊。",
        "from": "T21n1283_001:0343c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願慈悲說此真實八大富貴未曾有圓滿大法海。",
        "from": "T21n1283_001:0343c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等受持奉行。",
        "from": "T21n1283_001:0344a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊告言如是。",
        "from": "T21n1283_001:0344a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大腹蓮華色一切莊嚴大夜叉王祕密法。",
        "from": "T21n1283_001:0344a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於寶藏神大夜叉王兩邊有夜叉。",
        "from": "T21n1283_001:0344a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名吉隷二名摩隷。",
        "from": "T21n1283_001:0344a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒傾寶藏。",
        "from": "T21n1283_001:0344a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方名舍也(二合)摩夜叉妙滿夜叉滿賢夜叉。",
        "from": "T21n1283_001:0344a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東方獲財夜叉大財夜叉。",
        "from": "T21n1283_001:0344a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦傾寶藏大腹一切莊嚴大夜叉王。",
        "from": "T21n1283_001:0344a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如童子相守賢聖堂門。",
        "from": "T21n1283_001:0344a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦人應觀想此八大夜叉王。",
        "from": "T21n1283_001:0344a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說八夜叉眷屬真言。",
        "from": "T21n1283_001:0344a09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)馱曩(引)那野娑嚩(二合引)賀摩賀(引)馱曩(引)那(引)野娑嚩(二合引)賀摩尼婆捺囉(二合引)野娑嚩(二合引)賀布囉拏(二合)婆捺囉(二合引)野娑嚩(二合引)賀惹敢(二合)婆羅母契孕(二合)捺囉(二合引)野娑嚩(二合引)賀惹敢(二合)婆羅左隣捺囉(二合引)野娑嚩(二合引)賀唵(引)唧尾軍拏隷儞(引)娑嚩(二合引)賀唵(引)枳(引)隷摩(引)隷儞(引)娑嚩(二合引)賀此八眷屬夜叉王東方滿賢南方多聞。",
        "from": "T21n1283_001:0344a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方獲財北方寶賢等真言。",
        "from": "T21n1283_001:0344a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王一切天人阿脩羅。",
        "from": "T21n1283_001:0344a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆生恭敬信受奉行。",
        "from": "T21n1283_001:0344a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此八大夜叉王。",
        "from": "T21n1283_001:0344a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居八地自在大菩薩位。",
        "from": "T21n1283_001:0344a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其利生善能取捨。",
        "from": "T21n1283_001:0344a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安住三界一切財寶。",
        "from": "T21n1283_001:0344a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於寶藏神右邊。",
        "from": "T21n1283_001:0344a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安置清淨寶瓶。",
        "from": "T21n1283_001:0344a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及吉隷夜叉摩隷夜叉。",
        "from": "T21n1283_001:0344a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此二是寶藏神兄弟。",
        "from": "T21n1283_001:0344a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦居最上菩薩位。",
        "from": "T21n1283_001:0344a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一住西南角。",
        "from": "T21n1283_001:0344a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誓願度脫一切眾生。",
        "from": "T21n1283_001:0344a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一住東北方。",
        "from": "T21n1283_001:0344a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具大精進所見真實發歡喜誓願。",
        "from": "T21n1283_001:0344a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若念名者所求皆得。",
        "from": "T21n1283_001:0344a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言我於過去塵墨劫前深信佛法。",
        "from": "T21n1283_001:0344a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大夜叉鬼王。",
        "from": "T21n1283_001:0344a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於迦尸國請我受灌頂王位。",
        "from": "T21n1283_001:0344a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我於彼時得入曼拏羅修祕密行。",
        "from": "T21n1283_001:0344a27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸眾生說十善法。",
        "from": "T21n1283_001:0344a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼有情俱行十善。",
        "from": "T21n1283_001:0344a28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各各能捨一切財物。",
        "from": "T21n1283_001:0344a29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我行布施波羅蜜多清淨圓滿。",
        "from": "T21n1283_001:0344a29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入歡喜地成菩薩位。",
        "from": "T21n1283_001:0344b01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後斷除一切眾生貧病苦惱。",
        "from": "T21n1283_001:0344b01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求一切智。",
        "from": "T21n1283_001:0344b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願入如來應供正等正覺得佛智慧。",
        "from": "T21n1283_001:0344b02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過去諸佛亦復如是。",
        "from": "T21n1283_001:0344b03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依真言教求成就法已。",
        "from": "T21n1283_001:0344b04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後得成阿耨多羅三藐三菩提。",
        "from": "T21n1283_001:0344b04_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故汝寶藏神大夜叉王法。",
        "from": "T21n1283_001:0344b05_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為一切如來父母一切如來涅盤相。",
        "from": "T21n1283_001:0344b06_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝即如來能化夜叉形。",
        "from": "T21n1283_001:0344b06_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而為利益一切眾生。",
        "from": "T21n1283_001:0344b07_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今說彼大夜叉王真言義曼拏羅儀軌。",
        "from": "T21n1283_001:0344b07_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若彼誦人無財者令得財。",
        "from": "T21n1283_001:0344b08_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無吉祥者令得吉祥。",
        "from": "T21n1283_001:0344b09_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切所欲之事令得成就。",
        "from": "T21n1283_001:0344b09_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我此曼拏羅法若見若聞。",
        "from": "T21n1283_001:0344b10_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有過去多劫多生愚癡所作。",
        "from": "T21n1283_001:0344b11_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切重罪皆悉消除。",
        "from": "T21n1283_001:0344b11_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復能於寶藏神大明王菩薩同住親近。",
        "from": "T21n1283_001:0344b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不久速得不退之地。",
        "from": "T21n1283_001:0344b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離一切貧窮下賤諸苦煩惱。",
        "from": "T21n1283_001:0344b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身心適悅。",
        "from": "T21n1283_001:0344b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有一切空居宿曜。",
        "from": "T21n1283_001:0344b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及天龍夜叉乾闥婆阿脩羅迦樓羅緊那羅摩睺羅伽人非人等。",
        "from": "T21n1283_001:0344b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸惡心者皆不能為害而作擁護。",
        "from": "T21n1283_001:0344b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此八種大富貴夜叉王大法海廣大圓滿。",
        "from": "T21n1283_001:0344b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子等發深信心。",
        "from": "T21n1283_001:0344b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持此未曾有法修行梵行。",
        "from": "T21n1283_001:0344b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所得功德如前無異。",
        "from": "T21n1283_001:0344b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時寶藏神大夜叉王。",
        "from": "T21n1283_001:0344b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊。",
        "from": "T21n1283_001:0344b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此八種富貴未曾有法。",
        "from": "T21n1283_001:0344b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今受持願作擁護息災吉祥。",
        "from": "T21n1283_001:0344b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我有心真言。",
        "from": "T21n1283_001:0344b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而能利益安樂一切眾生。",
        "from": "T21n1283_001:0344b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其世間悲愍饒益。",
        "from": "T21n1283_001:0344b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願佛納受。",
        "from": "T21n1283_001:0344b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我今知此正法。",
        "from": "T21n1283_001:0344b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於過去世羯句忖那如來。",
        "from": "T21n1283_001:0344b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羯諾迦牟尼如來。",
        "from": "T21n1283_001:0344b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及迦葉佛等互相傳說。",
        "from": "T21n1283_001:0344b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此心真言我今欲說。",
        "from": "T21n1283_001:0344b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告寶藏神大夜叉王。",
        "from": "T21n1283_001:0344b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝宜速說今正是時。",
        "from": "T21n1283_001:0344b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶藏神大夜叉王。",
        "from": "T21n1283_001:0344b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛言已即說因緣。",
        "from": "T21n1283_001:0344b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大夜叉王種種變化。",
        "from": "T21n1283_001:0344c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如大火能燒一切煩惱薪。",
        "from": "T21n1283_001:0344c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦如人中王為第一。",
        "from": "T21n1283_001:0344c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾流之中海為第一。",
        "from": "T21n1283_001:0344c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "星宿之中月為第一。",
        "from": "T21n1283_001:0344c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此心真言亦復如是。",
        "from": "T21n1283_001:0344c03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有四維上下世間出世間中。",
        "from": "T21n1283_001:0344c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行求福功德富貴最為第一。",
        "from": "T21n1283_001:0344c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及彼天上人間。",
        "from": "T21n1283_001:0344c05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正等正覺二足世尊之中亦為第一。",
        "from": "T21n1283_001:0344c05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時寶藏神大夜叉王。",
        "from": "T21n1283_001:0344c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此真言。",
        "from": "T21n1283_001:0344c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨(引)囉怛曩(二合)怛囉(二合)夜(引)野曩謨(引)摩(引)尼婆捺囉(二合引)野摩賀(引)藥乞叉(二合)細(引)曩(引)鉢哆曳(引)唵(引)惹敢(二合)婆羅惹隣(引)捺囉(二合引)野娑嚩(二合)賀爾時寶藏神大夜叉王。",
        "from": "T21n1283_001:0344c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此真言已。",
        "from": "T21n1283_001:0344c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三千大千一切世界六種震動。",
        "from": "T21n1283_001:0344c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有一切夜叉大將以其二手結印。",
        "from": "T21n1283_001:0344c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此印相以頭指與大指。",
        "from": "T21n1283_001:0344c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相捻如鎖。",
        "from": "T21n1283_001:0344c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘指並舒。",
        "from": "T21n1283_001:0344c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此名衣印用此真言。",
        "from": "T21n1283_001:0344c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)地里地里摩賀(引)三母捺覽(二合引)摩囉馱(引)囉迦(引)野娑嚩(二合引)賀次結莊嚴印。",
        "from": "T21n1283_001:0344c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手大指捻餘三指甲。",
        "from": "T21n1283_001:0344c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩頭各如環。",
        "from": "T21n1283_001:0344c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更相向上旋轉。",
        "from": "T21n1283_001:0344c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次復相交。",
        "from": "T21n1283_001:0344c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是莊嚴印用此真言。",
        "from": "T21n1283_001:0344c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)彌(引)伽婆囉拏惹羅惹藍母伽曩娑嚩(二合)賀次拂印。",
        "from": "T21n1283_001:0344c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手作拳左手叉腰。",
        "from": "T21n1283_001:0344c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是拂印用此真言。",
        "from": "T21n1283_001:0344c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)賀囉賀囉囉惹計(二合)誐囉(二合)賀囉(二合)囉拏也(二合引)野娑嚩(二合)賀次結曼拏羅印。",
        "from": "T21n1283_001:0344c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手合掌。",
        "from": "T21n1283_001:0344c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開其指頭勿令相著。",
        "from": "T21n1283_001:0344c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "垂手指向下。",
        "from": "T21n1283_001:0344c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是曼拏羅印。",
        "from": "T21n1283_001:0344c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及塗壇同用。",
        "from": "T21n1283_001:0344c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言。",
        "from": "T21n1283_001:0344c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)俱胝羅瑜誐(引)嚩里哆(二合)必里(二合)夜(引)娑嚩(二合)賀次結五色粉印。",
        "from": "T21n1283_001:0345a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將粉曼拏羅時。",
        "from": "T21n1283_001:0345a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以五指平正。",
        "from": "T21n1283_001:0345a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下覆於地。",
        "from": "T21n1283_001:0345a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是五色粉印。",
        "from": "T21n1283_001:0345a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用此真言。",
        "from": "T21n1283_001:0345a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呬(引)呬(引)譬(引)曩末隷必里(二合)野輸(引)婆(引)野娑嚩(二合)賀次結香印。",
        "from": "T21n1283_001:0345a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手作合掌。",
        "from": "T21n1283_001:0345a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令無名指中指頭指入於掌內。",
        "from": "T21n1283_001:0345a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小指相合如針。",
        "from": "T21n1283_001:0345a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是香印用此真言。",
        "from": "T21n1283_001:0345a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)羅羅迦(引)羅(引)酥巘馱巘馱(引)野娑嚩(二合)賀次結花印。",
        "from": "T21n1283_001:0345a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手大指並竪。",
        "from": "T21n1283_001:0345a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此印香華同用真言亦爾。",
        "from": "T21n1283_001:0345a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此儀軌。",
        "from": "T21n1283_001:0345a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛如來亦乃略而說之。",
        "from": "T21n1283_001:0345a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有供養此馱羅尼者。",
        "from": "T21n1283_001:0345a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如供諸佛而無有異。",
        "from": "T21n1283_001:0345a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來應供正等正覺。",
        "from": "T21n1283_001:0345a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒自攝受恒自長養。",
        "from": "T21n1283_001:0345a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若諸眾生依法持誦。",
        "from": "T21n1283_001:0345a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速入十地修十波羅蜜成就十力。",
        "from": "T21n1283_001:0345a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊於其座上。",
        "from": "T21n1283_001:0345a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀察大眾告寶藏神大夜叉王。",
        "from": "T21n1283_001:0345a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝寶藏神曼拏羅儀軌。",
        "from": "T21n1283_001:0345a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祕密真言及彼印法。",
        "from": "T21n1283_001:0345a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來同所宣說。",
        "from": "T21n1283_001:0345a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘鉢尸如來尸棄如來。",
        "from": "T21n1283_001:0345a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘舍浮如來羯句忖那如來。",
        "from": "T21n1283_001:0345a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羯諾迦牟尼如來迦葉波如來。",
        "from": "T21n1283_001:0345a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今我釋迦如來亦皆宣說。",
        "from": "T21n1283_001:0345a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉寶藏神。",
        "from": "T21n1283_001:0345a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今諦聽善思念之。",
        "from": "T21n1283_001:0345a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為利益天上人間一切有情增長善根。",
        "from": "T21n1283_001:0345a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說於往昔求法之事。",
        "from": "T21n1283_001:0345a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我於過去無數劫中。",
        "from": "T21n1283_001:0345a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常往山中採柴為活。",
        "from": "T21n1283_001:0345a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忽於一日於其山野見一病比丘。",
        "from": "T21n1283_001:0345a26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲採於柴無力可為。",
        "from": "T21n1283_001:0345a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我自生悲愍心而施彼柴。",
        "from": "T21n1283_001:0345a27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燃火造食供養看承。",
        "from": "T21n1283_001:0345a28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經一日間而彼比丘。",
        "from": "T21n1283_001:0345a28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身獲安樂心甚歡喜。",
        "from": "T21n1283_001:0345a29_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即為於我受三歸五戒令發菩提心。",
        "from": "T21n1283_001:0345a29_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後說此曼拏羅祕密三昧儀軌。",
        "from": "T21n1283_001:0345b01_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時我得是真言王。",
        "from": "T21n1283_001:0345b02_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心觀想依法受持。",
        "from": "T21n1283_001:0345b02_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而能成就聖寶藏神一切勝利之事。",
        "from": "T21n1283_001:0345b03_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作護摩三遍復作水護摩。",
        "from": "T21n1283_001:0345b03_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後獻閼伽一日三時。",
        "from": "T21n1283_001:0345b04_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "專心讀誦楞伽經。",
        "from": "T21n1283_001:0345b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前作於觀想日日求法。",
        "from": "T21n1283_001:0345b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就八種眷屬恒自擁護。",
        "from": "T21n1283_001:0345b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復書真言戴於頸上。",
        "from": "T21n1283_001:0345b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得寶藏神擁護。",
        "from": "T21n1283_001:0345b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人誦此真言一洛叉。",
        "from": "T21n1283_001:0345b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或二洛叉或三洛叉。",
        "from": "T21n1283_001:0345b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於舍利塔前安置幢幡受持齋戒。",
        "from": "T21n1283_001:0345b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸夜叉三夜出生供獻所求成就。",
        "from": "T21n1283_001:0345b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得最上神通具大神力。",
        "from": "T21n1283_001:0345b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若水護摩儀則。",
        "from": "T21n1283_001:0345b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令眾生獲大財寶。",
        "from": "T21n1283_001:0345b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於江河寂靜之處先結齋戒。",
        "from": "T21n1283_001:0345b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然次入水內令水至臍。",
        "from": "T21n1283_001:0345b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以手施水而作護摩。",
        "from": "T21n1283_001:0345b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復用塗香誦寶賢真言。",
        "from": "T21n1283_001:0345b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦人觀想。",
        "from": "T21n1283_001:0345b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶賢安頭指上。",
        "from": "T21n1283_001:0345b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿賢安中指上。",
        "from": "T21n1283_001:0345b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲財安無名指上。",
        "from": "T21n1283_001:0345b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘沙門安手掌中。",
        "from": "T21n1283_001:0345b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "計[口*(隸-木+士)]麼尼曼弟目契捺囉(二合)室囉(二合)室左(二合)哩(引)捺囉(二合)安大指上。",
        "from": "T21n1283_001:0345b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是賢聖互相現見能為擁護。",
        "from": "T21n1283_001:0345b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說夜叉曼拏羅儀軌。",
        "from": "T21n1283_001:0345b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有如是功德。",
        "from": "T21n1283_001:0345b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況智者。",
        "from": "T21n1283_001:0345b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦人信佛重法。",
        "from": "T21n1283_001:0345b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持戒多聞一心清淨。",
        "from": "T21n1283_001:0345b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作最上曼拏羅福不可量。",
        "from": "T21n1283_001:0345b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲作此曼拏羅。",
        "from": "T21n1283_001:0345b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須依清淨地位有花果樹林處。",
        "from": "T21n1283_001:0345b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掘地至膝廣闊十二肘。",
        "from": "T21n1283_001:0345b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用五淨水灑結淨。",
        "from": "T21n1283_001:0345b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後實築平正如手掌。",
        "from": "T21n1283_001:0345b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦夜叉王真言加持。",
        "from": "T21n1283_001:0345b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以種種香花而為供養。",
        "from": "T21n1283_001:0345b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作法時須於白月吉日。",
        "from": "T21n1283_001:0345b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿闍梨志心清淨。",
        "from": "T21n1283_001:0345b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以五種淨水作自結淨。",
        "from": "T21n1283_001:0345b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心誦真言王作於擁護。",
        "from": "T21n1283_001:0345b26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次於上下十方結界。",
        "from": "T21n1283_001:0345b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦寶藏神真言加持香水依次灑淨。",
        "from": "T21n1283_001:0345b27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用五色線令童女合造。",
        "from": "T21n1283_001:0345b28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後以供俱摩香水裛過。",
        "from": "T21n1283_001:0345b28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用憂曇鉢木為橛。",
        "from": "T21n1283_001:0345b29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拼線作曼拏羅。",
        "from": "T21n1283_001:0345b29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先於東方起手。",
        "from": "T21n1283_001:0345c01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作四方四角開四門。",
        "from": "T21n1283_001:0345c01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中方安如來一切智王諸相圓滿。",
        "from": "T21n1283_001:0345c01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向下安寶藏神。",
        "from": "T21n1283_001:0345c02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色如青雲具足諸相。",
        "from": "T21n1283_001:0345c03_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以種種寶莊嚴。",
        "from": "T21n1283_001:0345c03_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於師子座上結跏趺坐其師子座以眾寶莊嚴。",
        "from": "T21n1283_001:0345c03_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放大光明如是殊妙。",
        "from": "T21n1283_001:0345c04_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手作施願相。",
        "from": "T21n1283_001:0345c05_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掌有菴摩勒果。",
        "from": "T21n1283_001:0345c05_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持寶囊滿種種寶。",
        "from": "T21n1283_001:0345c06_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復於寶藏神兩邊。",
        "from": "T21n1283_001:0345c06_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安置二金色夜叉王。",
        "from": "T21n1283_001:0345c07_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名左隣捺囉二名目契捺囉。",
        "from": "T21n1283_001:0345c07_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身相相等。",
        "from": "T21n1283_001:0345c08_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於寶藏神下面。",
        "from": "T21n1283_001:0345c08_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安夜叉女七子母九子母。",
        "from": "T21n1283_001:0345c08_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於壇東方安夜叉母。",
        "from": "T21n1283_001:0345c09_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東南角安摩尼曩迦。",
        "from": "T21n1283_001:0345c09_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南方安曩誐曩婆迦。",
        "from": "T21n1283_001:0345c10_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方安暗婆曩婆。",
        "from": "T21n1283_001:0345c10_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西南方安儞羅曩婆。",
        "from": "T21n1283_001:0345c11_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方安毘沙門。",
        "from": "T21n1283_001:0345c11_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西北方安大力大富伊舍曩天。",
        "from": "T21n1283_001:0345c12_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東北方安嚩嚕拏大力天夜叉王。",
        "from": "T21n1283_001:0345c12_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次有惹敢(二合)婆羅夜叉護嚕護羅夜叉。",
        "from": "T21n1283_001:0345c13_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酥細拏夜叉娑野夜叉。",
        "from": "T21n1283_001:0345c14_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惹敢(二合)婆迦夜叉目吒迦夜叉。",
        "from": "T21n1283_001:0345c14_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "囉訖哆(二合)[口*爾]嚩夜叉摩護那羅夜叉。",
        "from": "T21n1283_001:0345c15_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安曼拏羅外第三重。",
        "from": "T21n1283_001:0345c16_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次安置夜叉女。",
        "from": "T21n1283_001:0345c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並須一一色相端嚴。",
        "from": "T21n1283_001:0345c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尾布羅夜叉女尾舍羅夜叉女。",
        "from": "T21n1283_001:0345c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "贊捺囉輸末帝夜叉女誐嚩夜叉女。",
        "from": "T21n1283_001:0345c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酥路左曩夜叉女鉢納摩薩曩夜叉女。",
        "from": "T21n1283_001:0345c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酥嚩尾夜叉女摩賀摩野夜叉女。",
        "from": "T21n1283_001:0345c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酥吉尸夜叉女馱曩麼羅夜叉女。",
        "from": "T21n1283_001:0345c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "布室隷夜叉女拏嚕摩隷迦夜叉女。",
        "from": "T21n1283_001:0345c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手作施一切施願相。",
        "from": "T21n1283_001:0345c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及摩拏惹嚩迦摩嚕閉酥嚕哆嚩吉也(二合)酥捺里舍儞摩護誐囉天帝釋等。",
        "from": "T21n1283_001:0345c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或二十或一百。",
        "from": "T21n1283_001:0345c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於曼拏羅外依其方位一一安置。",
        "from": "T21n1283_001:0345c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次金剛杵金剛劍金剛棒螺羂索幢輪三股叉。",
        "from": "T21n1283_001:0345c25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外第一重安結界網。",
        "from": "T21n1283_001:0345c26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二重復安金剛杵。",
        "from": "T21n1283_001:0345c27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三重安金剛熾焰並令周匝次略明粉壇請召供養儀軌。",
        "from": "T21n1283_001:0345c27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須用青黃赤白黑五色之粉。",
        "from": "T21n1283_001:0346a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先粉八方位地作八熾焰等如前所說。",
        "from": "T21n1283_001:0346a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次捻閼伽鉢滿盛香水及好花。",
        "from": "T21n1283_001:0346a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦寶藏神心真言作於召請。",
        "from": "T21n1283_001:0346a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持誦者須自虔誠勿生疑惑。",
        "from": "T21n1283_001:0346a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若夜叉女等。",
        "from": "T21n1283_001:0346a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須各各念本真言召請。",
        "from": "T21n1283_001:0346a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令於有德智者。",
        "from": "T21n1283_001:0346a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請召天中天大明寶藏神。",
        "from": "T21n1283_001:0346a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既請召已。",
        "from": "T21n1283_001:0346a06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然以摩羅帝花鬘色妙蓮華。",
        "from": "T21n1283_001:0346a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憂鉢羅花迦難摩花。",
        "from": "T21n1283_001:0346a07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱覽吒華緊輸迦華。",
        "from": "T21n1283_001:0346a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗香燒香燃燈設食。",
        "from": "T21n1283_001:0346a08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切所須之物依次供獻。",
        "from": "T21n1283_001:0346a08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次作護摩。",
        "from": "T21n1283_001:0346a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用紫礦安悉香龍腦香及白檀香。",
        "from": "T21n1283_001:0346a09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於寶賢面前。",
        "from": "T21n1283_001:0346a10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作護摩一千專心供養。",
        "from": "T21n1283_001:0346a10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於曼拏羅四角安閼伽瓶。",
        "from": "T21n1283_001:0346a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入五穀五藥滿盛香水。",
        "from": "T21n1283_001:0346a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復於瓶口內插樹枝。",
        "from": "T21n1283_001:0346a12_2",
        "context_with_next_sentence": true
    },
