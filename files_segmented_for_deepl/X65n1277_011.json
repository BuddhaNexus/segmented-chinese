[
    {
        "source_sentence": "高峰龍泉院因師集賢語錄卷之十一",
        "from": "X65n1277_011:0041c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總願碎語門",
        "from": "X65n1277_011:0041c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總願全段",
        "from": "X65n1277_011:0041c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上祝　當今皇帝道踰堯舜之君。",
        "from": "X65n1277_011:0041c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在位大臣德邁皐夔之列。",
        "from": "X65n1277_011:0041c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "霧掃胡塵而干戈盡偃。",
        "from": "X65n1277_011:0041c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月沉邊燧而文軌皆同。",
        "from": "X65n1277_011:0041c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有民皆樂於春臺。",
        "from": "X65n1277_011:0041c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無俗不躋於壽域。",
        "from": "X65n1277_011:0041c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次異鄉閭寧。",
        "from": "X65n1277_011:0041c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謐家室康安。",
        "from": "X65n1277_011:0041c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "免逢山水之蒙。",
        "from": "X65n1277_011:0041c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常遇地天之泰。",
        "from": "X65n1277_011:0041c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鑿井耕田而各安生業。",
        "from": "X65n1277_011:0041c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "含哺鼓腹而咸樂天真。",
        "from": "X65n1277_011:0041c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "商歌廛而物阜財豐。",
        "from": "X65n1277_011:0041c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "士修學而功成名遂。",
        "from": "X65n1277_011:0042a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蠶桑春富家家繅白雪之絲。",
        "from": "X65n1277_011:0042a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禾稼秋成處處刈黃雲之穗。",
        "from": "X65n1277_011:0042a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜絕覆梁之輩。",
        "from": "X65n1277_011:0042a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時聞擊壤之童。",
        "from": "X65n1277_011:0042a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牛羊茁壯滿千山。",
        "from": "X65n1277_011:0042a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雞犬相聞達四境。",
        "from": "X65n1277_011:0042a03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獄囚永息。",
        "from": "X65n1277_011:0042a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疫癘潛消。",
        "from": "X65n1277_011:0042a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓滿一切願心。",
        "from": "X65n1277_011:0042a04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就無邊福利。",
        "from": "X65n1277_011:0042a04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為官者",
        "from": "X65n1277_011:0042a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居官無恙。",
        "from": "X65n1277_011:0042a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "理事如神。",
        "from": "X65n1277_011:0042a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人思召伯之甘棠。",
        "from": "X65n1277_011:0042a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世競郭公之竹馬。",
        "from": "X65n1277_011:0042a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "早錫千鍾之祿。",
        "from": "X65n1277_011:0042a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "榮膺一尺之書。",
        "from": "X65n1277_011:0042a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "享台筭以綿長。",
        "from": "X65n1277_011:0042a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "履仕途而遷擢。",
        "from": "X65n1277_011:0042a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修文者",
        "from": "X65n1277_011:0042a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舒卷韓窻之編簡。",
        "from": "X65n1277_011:0042a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "富饒鄴架之詩書。",
        "from": "X65n1277_011:0042a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禹門三級願先登。",
        "from": "X65n1277_011:0042a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "郄桂一枝祈早折。",
        "from": "X65n1277_011:0042a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "茂延百福。",
        "from": "X65n1277_011:0042a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永殄千災。",
        "from": "X65n1277_011:0042a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "習武者",
        "from": "X65n1277_011:0042a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六篇明豹路。",
        "from": "X65n1277_011:0042a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三尺撫龍泉。",
        "from": "X65n1277_011:0042a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孫吳之事業益精。",
        "from": "X65n1277_011:0042a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衛霍之勳名早遂。",
        "from": "X65n1277_011:0042a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彎弓馳射功収百步之穿楊。",
        "from": "X65n1277_011:0042a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣錦榮歸堂慶双親之戲綵。",
        "from": "X65n1277_011:0042a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "治家者",
        "from": "X65n1277_011:0042a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "理家有道。",
        "from": "X65n1277_011:0042a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙筭無遺。",
        "from": "X65n1277_011:0042a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "產業山崇還如王愷。",
        "from": "X65n1277_011:0042a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貨財泉溢有若陶朱椿老。",
        "from": "X65n1277_011:0042a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "桂芳益顯竇家之陰德。",
        "from": "X65n1277_011:0042a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘭森玉立茂揚謝砌之休聲。",
        "from": "X65n1277_011:0042a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "事公者",
        "from": "X65n1277_011:0042a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為吏事官多享祿。",
        "from": "X65n1277_011:0042a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "招財遇貴得安身。",
        "from": "X65n1277_011:0042a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結案疑而無錯無差。",
        "from": "X65n1277_011:0042a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覆官員而允順允聽。",
        "from": "X65n1277_011:0042a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常願保全之美。",
        "from": "X65n1277_011:0042a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永無過失之虞。",
        "from": "X65n1277_011:0042a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "家道榮華。",
        "from": "X65n1277_011:0042a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公庭清吉。",
        "from": "X65n1277_011:0042a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "務農者",
        "from": "X65n1277_011:0042a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水耨敢辭頻。",
        "from": "X65n1277_011:0042b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火耕當作苦。",
        "from": "X65n1277_011:0042b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "春帶一犂江上雨。",
        "from": "X65n1277_011:0042b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "秋収千畒隴頭雲。",
        "from": "X65n1277_011:0042b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大田常獲豐登。",
        "from": "X65n1277_011:0042b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高廩益充委積。",
        "from": "X65n1277_011:0042b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "犬無吠夜。",
        "from": "X65n1277_011:0042b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吏不登門。",
        "from": "X65n1277_011:0042b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "養蚕者",
        "from": "X65n1277_011:0042b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝釋密扶。",
        "from": "X65n1277_011:0042b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬鳴默佑。",
        "from": "X65n1277_011:0042b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "採空五畒無餘葉。",
        "from": "X65n1277_011:0042b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繰就千機有細絲。",
        "from": "X65n1277_011:0042b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縑帛豐盈。",
        "from": "X65n1277_011:0042b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貨財富衍。",
        "from": "X65n1277_011:0042b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "工匠者",
        "from": "X65n1277_011:0042b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性天曉了。",
        "from": "X65n1277_011:0042b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心匠玲瓏。",
        "from": "X65n1277_011:0042b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "胸中自有規摹。",
        "from": "X65n1277_011:0042b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手內多施巧妙。",
        "from": "X65n1277_011:0042b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "動止常居吉地。",
        "from": "X65n1277_011:0042b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "工夫每遇貴人。",
        "from": "X65n1277_011:0042b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倍収日至之財。",
        "from": "X65n1277_011:0042b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行見天書之詔。",
        "from": "X65n1277_011:0042b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "商賈者",
        "from": "X65n1277_011:0042b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貿遷如意。",
        "from": "X65n1277_011:0042b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出塗収鉅萬之財。",
        "from": "X65n1277_011:0042b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斡運隨心。",
        "from": "X65n1277_011:0042b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藏市獲倍蓰之利。",
        "from": "X65n1277_011:0042b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "錢流地上。",
        "from": "X65n1277_011:0042b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貨衍日中。",
        "from": "X65n1277_011:0042b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世業增崇。",
        "from": "X65n1277_011:0042b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "家聲赫奕。",
        "from": "X65n1277_011:0042b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攻醫者",
        "from": "X65n1277_011:0042b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳不傳之秘方。",
        "from": "X65n1277_011:0042b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "治難治之篤疾。",
        "from": "X65n1277_011:0042b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "活人之功為甚愽。",
        "from": "X65n1277_011:0042b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "濟世之道願益行。",
        "from": "X65n1277_011:0042b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福德藏王常密扶於動履。",
        "from": "X65n1277_011:0042b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安樂菩薩更茂介於禎祥。",
        "from": "X65n1277_011:0042b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尅擇者",
        "from": "X65n1277_011:0042b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水法深明宗妙。",
        "from": "X65n1277_011:0042b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "山形洞曉玄機。",
        "from": "X65n1277_011:0042b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "胷包造化之工。",
        "from": "X65n1277_011:0042b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數衍星辰之妙。",
        "from": "X65n1277_011:0042b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常遇貴人持掣。",
        "from": "X65n1277_011:0042b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頻蒙富室招延。",
        "from": "X65n1277_011:0042b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "錢財衣食如願如心。",
        "from": "X65n1277_011:0042b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "富貴榮華從謀從望。",
        "from": "X65n1277_011:0042b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漁釣者",
        "from": "X65n1277_011:0042b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數尺絲綸長在手。",
        "from": "X65n1277_011:0042b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一簑煙雨任平生。",
        "from": "X65n1277_011:0042b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "江魚美可求。",
        "from": "X65n1277_011:0042b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "湘水清堪汲。",
        "from": "X65n1277_011:0042b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽比岡陵之久。",
        "from": "X65n1277_011:0042b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福如江海之深。",
        "from": "X65n1277_011:0042b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開酒肆者",
        "from": "X65n1277_011:0042c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬相如之滌器。",
        "from": "X65n1277_011:0042c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卓文君之當壚。",
        "from": "X65n1277_011:0042c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "挑錢之客常來。",
        "from": "X65n1277_011:0042c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盜飲之人已去。",
        "from": "X65n1277_011:0042c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "息収萬倍。",
        "from": "X65n1277_011:0042c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願應千般。",
        "from": "X65n1277_011:0042c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開店肆者",
        "from": "X65n1277_011:0042c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百神擁護。",
        "from": "X65n1277_011:0042c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬聖維持。",
        "from": "X65n1277_011:0042c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "店舍興隆長似孟甞門下。",
        "from": "X65n1277_011:0042c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "資財進入常如王愷藏中。",
        "from": "X65n1277_011:0042c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "官非口舌逐時消。",
        "from": "X65n1277_011:0042c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福祿吉祥隨日至。",
        "from": "X65n1277_011:0042c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為屠戶者",
        "from": "X65n1277_011:0042c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "割肉固殊於方朔。",
        "from": "X65n1277_011:0042c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "操刀濫學於庖丁。",
        "from": "X65n1277_011:0042c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殺生之罪業冰消。",
        "from": "X65n1277_011:0042c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "潤屋之貨財山積。",
        "from": "X65n1277_011:0042c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為愽弈者",
        "from": "X65n1277_011:0042c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙筭隨心。",
        "from": "X65n1277_011:0042c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取財遂意。",
        "from": "X65n1277_011:0042c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但要見機喝采。",
        "from": "X65n1277_011:0042c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終須得手盈錢。",
        "from": "X65n1277_011:0042c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從教一擲之時。",
        "from": "X65n1277_011:0042c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自獲百萬之利。",
        "from": "X65n1277_011:0042c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "官非不惹。",
        "from": "X65n1277_011:0042c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盜賊無侵。",
        "from": "X65n1277_011:0042c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行藏消不測之災。",
        "from": "X65n1277_011:0042c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "動止納新來之福。",
        "from": "X65n1277_011:0042c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為蒿工者",
        "from": "X65n1277_011:0042c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運篙持漿永無波浪之憂。",
        "from": "X65n1277_011:0042c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遇險臨灘常有龍神之護。",
        "from": "X65n1277_011:0042c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡來往滄波之上。",
        "from": "X65n1277_011:0042c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如安行平陸之中。",
        "from": "X65n1277_011:0042c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "事事亨通。",
        "from": "X65n1277_011:0042c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭頭利濟。",
        "from": "X65n1277_011:0042c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為僧行者",
        "from": "X65n1277_011:0042c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天龍外護。",
        "from": "X65n1277_011:0042c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵行內清。",
        "from": "X65n1277_011:0042c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行住坐臥而百福資扶。",
        "from": "X65n1277_011:0042c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "春夏秋冬而千災殄滅。",
        "from": "X65n1277_011:0042c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為道士者",
        "from": "X65n1277_011:0042c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "星冠离洞府。",
        "from": "X65n1277_011:0042c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "霞帔下煙霄。",
        "from": "X65n1277_011:0042c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六匣風雷藏寶劒。",
        "from": "X65n1277_011:0042c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一壺天地養靈丹。",
        "from": "X65n1277_011:0042c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "茨福駢臻。",
        "from": "X65n1277_011:0042c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "椿齡有永。",
        "from": "X65n1277_011:0042c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為尼姑者",
        "from": "X65n1277_011:0043a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眉攢柳綠栖身眾妙之門。",
        "from": "X65n1277_011:0043a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臉映桃紅勵志自然之教。",
        "from": "X65n1277_011:0043a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無為洞達。",
        "from": "X65n1277_011:0043a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大道勤行。",
        "from": "X65n1277_011:0043a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弘持清淨之規。",
        "from": "X65n1277_011:0043a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "延續神仙之壽。",
        "from": "X65n1277_011:0043a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為師巫者",
        "from": "X65n1277_011:0043a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救四百四病之苦。",
        "from": "X65n1277_011:0043a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有百發百中之功。",
        "from": "X65n1277_011:0043a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "靈符鎻斷鬼門關。",
        "from": "X65n1277_011:0043a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "巾斗飜開人世界。",
        "from": "X65n1277_011:0043a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三元尊聖時時大降吉祥。",
        "from": "X65n1277_011:0043a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸部吏兵日日勤加護衛。",
        "from": "X65n1277_011:0043a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相士論命者",
        "from": "X65n1277_011:0043a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "術妙高古。",
        "from": "X65n1277_011:0043a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藝精動人。",
        "from": "X65n1277_011:0043a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論吉凶則究五星五行之奧旨。",
        "from": "X65n1277_011:0043a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀貴賤則有百發百中之奇功。",
        "from": "X65n1277_011:0043a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何須西蜀問君平。",
        "from": "X65n1277_011:0043a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不待江南尋郭璞。",
        "from": "X65n1277_011:0043a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "笑傲壺中之日月。",
        "from": "X65n1277_011:0043a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "優淤物外之煙霞。",
        "from": "X65n1277_011:0043a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牙儈者",
        "from": "X65n1277_011:0043a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持心正直。",
        "from": "X65n1277_011:0043a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "秤物公平。",
        "from": "X65n1277_011:0043a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "會四遠之經商。",
        "from": "X65n1277_011:0043a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合兩家之交易。",
        "from": "X65n1277_011:0043a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居多財貨只憑辯口之懸河。",
        "from": "X65n1277_011:0043a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輻湊舟車常見滿門而成市。",
        "from": "X65n1277_011:0043a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娼妓者",
        "from": "X65n1277_011:0043a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雲水之生涯日進。",
        "from": "X65n1277_011:0043a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煙花之活計風流。",
        "from": "X65n1277_011:0043a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朝陪四海豪奢。",
        "from": "X65n1277_011:0043a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "暮伴五陵年少。",
        "from": "X65n1277_011:0043a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "綺羅叢裏潛消口舌官非。",
        "from": "X65n1277_011:0043a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "絃管聲中常獲金珠璧玉。",
        "from": "X65n1277_011:0043a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高峰龍泉院因師集賢語錄卷之十一",
        "from": "X65n1277_011:0043a21_0",
        "context_with_next_sentence": false
    }
]