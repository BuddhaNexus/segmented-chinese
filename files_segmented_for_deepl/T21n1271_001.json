[
    {
        "source_sentence": "摩訶毘盧遮那如來定惠均等入",
        "from": "T21n1271_001:0305c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三昧耶身雙身大聖歡喜天菩薩",
        "from": "T21n1271_001:0305c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行祕密法儀軌",
        "from": "T21n1271_001:0305c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鴻盧卿大廣智不空三藏奉　勅譯",
        "from": "T21n1271_001:0305c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有佛弟子。",
        "from": "T21n1271_001:0305c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比丘比丘尼優婆塞優婆夷四種弟子。",
        "from": "T21n1271_001:0305c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人等。",
        "from": "T21n1271_001:0305c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲得修行此聖天菩薩法。",
        "from": "T21n1271_001:0305c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令成就所求所願隨心如意者。",
        "from": "T21n1271_001:0305c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先須盡珍寶入勞傳法師並阿闍梨。",
        "from": "T21n1271_001:0305c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為令得歡喜心。",
        "from": "T21n1271_001:0305c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百味飲食。",
        "from": "T21n1271_001:0305c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "資具資財。",
        "from": "T21n1271_001:0305c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣服座具。",
        "from": "T21n1271_001:0305c13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨身之資具。",
        "from": "T21n1271_001:0305c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養勞令得歡喜心後。",
        "from": "T21n1271_001:0305c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其微細祕密甚悉旨宗趣。",
        "from": "T21n1271_001:0305c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問求具習覺。",
        "from": "T21n1271_001:0305c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應修行祕密法。",
        "from": "T21n1271_001:0305c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要道成就法奇捨身求道。",
        "from": "T21n1271_001:0305c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡珍寶入勞。",
        "from": "T21n1271_001:0305c16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂資身衣服也。",
        "from": "T21n1271_001:0305c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "續命飲食。",
        "from": "T21n1271_001:0305c17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以自身可被著衣服之料。",
        "from": "T21n1271_001:0305c17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自可食飲食之料。",
        "from": "T21n1271_001:0305c18_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養傳法師知法阿闍梨。",
        "from": "T21n1271_001:0305c18_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養此盡珍寶入勞。",
        "from": "T21n1271_001:0305c19_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂先須以火一升胡麻油溫。",
        "from": "T21n1271_001:0305c19_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "炭瓫火網白銅多羅加持。",
        "from": "T21n1271_001:0305c20_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大真言百八返。",
        "from": "T21n1271_001:0305c20_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且溫且加持。",
        "from": "T21n1271_001:0305c21_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但印三股鉾也。",
        "from": "T21n1271_001:0305c21_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "根本大真言曰　身呪。",
        "from": "T21n1271_001:0305c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨尾那翼迦(二合)瀉賀悉底(二合)母佉瀉怛儞也(二合引)他唵娜(去聲下同)翼迦(二合)娜翼迦(二合)尾娜翼迦(二合)尾娜翼迦(二合)怛囉(二合)翼迦(二合)簸哩(二合)怛囉(二合)翼迦(二合)餉(引)佉賀悉底(二合)餉(引生反)佉迦只多扇(上)底迦囉(二合)娑嚩(二合引)賀(引)灌沐日中三返。",
        "from": "T21n1271_001:0305c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平日旦四返。",
        "from": "T21n1271_001:0305c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灌沐限七日為期。",
        "from": "T21n1271_001:0305c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乞得成就悉地。",
        "from": "T21n1271_001:0305c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以少心呪加持供物。",
        "from": "T21n1271_001:0305c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其供物數蘿福根歡喜團餅時菓子等。",
        "from": "T21n1271_001:0305c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯現身賢聖供養奉。",
        "from": "T21n1271_001:0306a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七日之內必驗得。",
        "from": "T21n1271_001:0306a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少心呪曰。",
        "from": "T21n1271_001:0306a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵只里虐娑婆賀(引二合)",
        "from": "T21n1271_001:0306a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每供養奉畢。",
        "from": "T21n1271_001:0306a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦讚歎頌曰。",
        "from": "T21n1271_001:0306a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵誐娜婆底洲底娑婆悉底摩訶誐馱轉底洲底迦羅耶娑婆賀(引二合)成就所求所願如意圓滿具足故。",
        "from": "T21n1271_001:0306a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可令傳法阿闍梨歡喜。",
        "from": "T21n1271_001:0306a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後一七日可修行。",
        "from": "T21n1271_001:0306a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得四種成就悉地。",
        "from": "T21n1271_001:0306a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "息增愛調。",
        "from": "T21n1271_001:0306a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此謂四種成就悉地法也。",
        "from": "T21n1271_001:0306a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘那夜經說。",
        "from": "T21n1271_001:0306a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂此聖天菩薩供養奉。",
        "from": "T21n1271_001:0306a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有三種果報。",
        "from": "T21n1271_001:0306a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上品供養奉。",
        "from": "T21n1271_001:0306a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令得國王威德。",
        "from": "T21n1271_001:0306a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中品供養奉者。",
        "from": "T21n1271_001:0306a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令具七寶富饒令得下品供養奉。",
        "from": "T21n1271_001:0306a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣食不乏飽滿具足自然也。",
        "from": "T21n1271_001:0306a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上品供養奉者。",
        "from": "T21n1271_001:0306a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令得傳法人歡喜之心。",
        "from": "T21n1271_001:0306a13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祕密甚深□旨被教授。",
        "from": "T21n1271_001:0306a14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如法供養奉。",
        "from": "T21n1271_001:0306a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中品供養奉者儀軌可見不悟具次第要道略供養奉。",
        "from": "T21n1271_001:0306a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下品供養奉者。",
        "from": "T21n1271_001:0306a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不灌沐油。",
        "from": "T21n1271_001:0306a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "只別殿令坐。",
        "from": "T21n1271_001:0306a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所得每初分供養奉。",
        "from": "T21n1271_001:0306a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此名為下品。",
        "from": "T21n1271_001:0306a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "像末佛弟子。",
        "from": "T21n1271_001:0306a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自之心懷慳貪我慢愛惜之心。",
        "from": "T21n1271_001:0306a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不成得傳法知道阿闍梨得歡喜悅賀之心。",
        "from": "T21n1271_001:0306a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "委曲要深趣不問悟。",
        "from": "T21n1271_001:0306a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦自得疎略供養修行。",
        "from": "T21n1271_001:0306a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誹謗深祕勝道。",
        "from": "T21n1271_001:0306a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "招取貧窮無福種子。",
        "from": "T21n1271_001:0306a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天台智者阿闍梨止觀心要記云。",
        "from": "T21n1271_001:0306a22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂有福無智。",
        "from": "T21n1271_001:0306a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此愚人不殊龍身鬼(云云)有智無福。",
        "from": "T21n1271_001:0306a23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此橫惑者。",
        "from": "T21n1271_001:0306a23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非賢聖之心。",
        "from": "T21n1271_001:0306a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間甚自無福能無勝者。",
        "from": "T21n1271_001:0306a24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世中操者福德自莊嚴無過哉。",
        "from": "T21n1271_001:0306a25_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂所以此聖天菩薩供養奉　修行時。",
        "from": "T21n1271_001:0306a25_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必令圓滿福智也。",
        "from": "T21n1271_001:0306a26_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定者福也。",
        "from": "T21n1271_001:0306a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惠均等三摩身者。",
        "from": "T21n1271_001:0306a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂此法初終有兩時。",
        "from": "T21n1271_001:0306a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者平旦。",
        "from": "T21n1271_001:0306a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者日中。",
        "from": "T21n1271_001:0306a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此兩時此平旦增益時也。",
        "from": "T21n1271_001:0306a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日中吉祥時也。",
        "from": "T21n1271_001:0306a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以此聖天供養奉人者。",
        "from": "T21n1271_001:0306a29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福智圓滿具足耳。",
        "from": "T21n1271_001:0306b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦毘尼宗有□所權教人者。",
        "from": "T21n1271_001:0306b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百世無平等(云云)此法以酒供養附子。",
        "from": "T21n1271_001:0306b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此毒物服醫師得之除病安身藥。",
        "from": "T21n1271_001:0306b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和合人合得悅賀。",
        "from": "T21n1271_001:0306b03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酒此歡喜水妙藥也。",
        "from": "T21n1271_001:0306b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "田夫令悅。",
        "from": "T21n1271_001:0306b04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此酒也。",
        "from": "T21n1271_001:0306b04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能飲人藥成。",
        "from": "T21n1271_001:0306b05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡飲人毒成。",
        "from": "T21n1271_001:0306b05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此天菩薩人心令得歡喜故。",
        "from": "T21n1271_001:0306b05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以酒供養此名歡喜水耳。",
        "from": "T21n1271_001:0306b06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現生國王貴君。",
        "from": "T21n1271_001:0306b06_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如供養奉時。",
        "from": "T21n1271_001:0306b07_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必獲得靈驗。",
        "from": "T21n1271_001:0306b07_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所供遺呪師服無咎。",
        "from": "T21n1271_001:0306b07_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲法所制也。",
        "from": "T21n1271_001:0306b08_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此法無制。",
        "from": "T21n1271_001:0306b08_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "像末佛弟子發誠心信念。",
        "from": "T21n1271_001:0306b08_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至心慇懃供養奉。",
        "from": "T21n1271_001:0306b09_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可生起疑心。",
        "from": "T21n1271_001:0306b09_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳法知法者問求具習悟信受奉行。",
        "from": "T21n1271_001:0306b10_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可疎略。",
        "from": "T21n1271_001:0306b11_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次第法阿闍梨含受具不載。",
        "from": "T21n1271_001:0306b11_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儀軌文能次第法受習覺悟供養修行信受奉行皆大歡喜。",
        "from": "T21n1271_001:0306b12_0",
