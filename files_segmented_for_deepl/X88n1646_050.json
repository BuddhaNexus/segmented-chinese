[
    {
        "source_sentence": "居士傳五十",
        "from": "X88n1646_050:0279b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬邦良徐成民傳",
        "from": "X88n1646_050:0279b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬邦良者富陽人。",
        "from": "X88n1646_050:0279b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬歷中登進士第。",
        "from": "X88n1646_050:0279b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知丹徒縣。",
        "from": "X88n1646_050:0279b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仁慈正直。",
        "from": "X88n1646_050:0279b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一方推為神君。",
        "from": "X88n1646_050:0279b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既去。",
        "from": "X88n1646_050:0279b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立祠祀之。",
        "from": "X88n1646_050:0279b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後官甘肅。",
        "from": "X88n1646_050:0279b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行太僕。",
        "from": "X88n1646_050:0279b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以憂歸居慈山僧舍。",
        "from": "X88n1646_050:0279b17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日禮阿彌陀佛求生西方。",
        "from": "X88n1646_050:0279b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過蘇州將謁座主王荊石。",
        "from": "X88n1646_050:0279b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至一寺禮佛稱佛名。",
        "from": "X88n1646_050:0279b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悲泪不止。",
        "from": "X88n1646_050:0279b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時舉人黃鍾與一友謁之。",
        "from": "X88n1646_050:0279b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就坐問曰。",
        "from": "X88n1646_050:0279b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公信佛念佛一何真實若此。",
        "from": "X88n1646_050:0279b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "邦良曰。",
        "from": "X88n1646_050:0279b20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人身難得。",
        "from": "X88n1646_050:0279b20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正法難逢。",
        "from": "X88n1646_050:0279b20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛可不信耶。",
        "from": "X88n1646_050:0279b20_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念佛可不真實耶。",
        "from": "X88n1646_050:0279b21_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛法利益人天。",
        "from": "X88n1646_050:0279b21_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非宿植善根多生疑謗。",
        "from": "X88n1646_050:0279b21_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一失人身受苦無量。",
        "from": "X88n1646_050:0279b22_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每一思之痛徹心骨。",
        "from": "X88n1646_050:0279b22_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩人曰。",
        "from": "X88n1646_050:0279b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公言人身難得。",
        "from": "X88n1646_050:0279b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉目便是。",
        "from": "X88n1646_050:0279b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三人併諸左右非人耶。",
        "from": "X88n1646_050:0279b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時方仲夏。",
        "from": "X88n1646_050:0279b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "邦良以扇一揮。",
        "from": "X88n1646_050:0279b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羣蠅四飛。",
        "from": "X88n1646_050:0279b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詰曰。",
        "from": "X88n1646_050:0279b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蠅多與。",
        "from": "X88n1646_050:0279b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人多與。",
        "from": "X88n1646_050:0279c01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩人憮然久之。",
        "from": "X88n1646_050:0279c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "邦良曰。",
        "from": "X88n1646_050:0279c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始吾為諸生時。",
        "from": "X88n1646_050:0279c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉上帝旨。",
        "from": "X88n1646_050:0279c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掌第五殿閻羅事。",
        "from": "X88n1646_050:0279c02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每夜坐殿上。",
        "from": "X88n1646_050:0279c02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "簿書山積。",
        "from": "X88n1646_050:0279c02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亡者三尸自陳身口意業。",
        "from": "X88n1646_050:0279c02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "據律定案。",
        "from": "X88n1646_050:0279c03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千百人中不失人身者纔一二耳。",
        "from": "X88n1646_050:0279c03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且陽世極刑不過一死。",
        "from": "X88n1646_050:0279c04_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陰司受罪痛極則死。",
        "from": "X88n1646_050:0279c04_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "死已復生。",
        "from": "X88n1646_050:0279c05_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是作。",
        "from": "X88n1646_050:0279c05_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是受。",
        "from": "X88n1646_050:0279c05_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "絕無適輕適重於其間。",
        "from": "X88n1646_050:0279c05_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟信佛念佛往生極樂者不入冥塗。",
        "from": "X88n1646_050:0279c06_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然則佛可不信耶。",
        "from": "X88n1646_050:0279c06_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念佛可不真實耶。",
        "from": "X88n1646_050:0279c07_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因言殿西有二楹。",
        "from": "X88n1646_050:0279c07_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養雲棲．紫栢二大師。",
        "from": "X88n1646_050:0279c07_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而雲棲香火為尤盛。",
        "from": "X88n1646_050:0279c08_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其後領鄉薦。",
        "from": "X88n1646_050:0279c08_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "判事漸稀。",
        "from": "X88n1646_050:0279c08_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至釋褐而謝事。",
        "from": "X88n1646_050:0279c09_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二人聞已毛髮竦豎。",
        "from": "X88n1646_050:0279c09_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳其事以告鄉里焉(淨土晨鐘．活閻君紀略)。",
        "from": "X88n1646_050:0279c09_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徐成民名坤。",
        "from": "X88n1646_050:0279c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小名佛舍。",
        "from": "X88n1646_050:0279c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "江南太倉人。",
        "from": "X88n1646_050:0279c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父驥生。",
        "from": "X88n1646_050:0279c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縣學生。",
        "from": "X88n1646_050:0279c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成民為人長者長齋奉佛。",
        "from": "X88n1646_050:0279c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年十八。",
        "from": "X88n1646_050:0279c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當崇禎六年六月十二日。",
        "from": "X88n1646_050:0279c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜夢被帝召至忉利天。",
        "from": "X88n1646_050:0279c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敕示夙世因緣。",
        "from": "X88n1646_050:0279c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命攝第五殿閻羅王事百日。",
        "from": "X88n1646_050:0279c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自此每夜坐堂中。",
        "from": "X88n1646_050:0279c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作呼叱聲。",
        "from": "X88n1646_050:0279c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若官府決獄狀。",
        "from": "X88n1646_050:0279c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "驥生起而瞷之。",
        "from": "X88n1646_050:0279c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見一王者南面坐。",
        "from": "X88n1646_050:0279c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼判獄卒猙獰旁列。",
        "from": "X88n1646_050:0279c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則大驚諦視之。",
        "from": "X88n1646_050:0279c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而王者乃成民也。",
        "from": "X88n1646_050:0279c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遲明詢其故。",
        "from": "X88n1646_050:0279c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成民以實告。",
        "from": "X88n1646_050:0279c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即令移居寺中。",
        "from": "X88n1646_050:0279c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸昆弟及遠近好事者多携筆札伏壁後記其判語。",
        "from": "X88n1646_050:0279c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得九十餘條。",
        "from": "X88n1646_050:0279c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "決斷精嚴。",
        "from": "X88n1646_050:0279c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見者身毛為竪。",
        "from": "X88n1646_050:0279c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成民嘗自言。",
        "from": "X88n1646_050:0279c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冥中罪囚合四天下。",
        "from": "X88n1646_050:0279c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "動以萬計。",
        "from": "X88n1646_050:0279c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除付所司分治外。",
        "from": "X88n1646_050:0279c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其親鞫者猶夕以千計。",
        "from": "X88n1646_050:0279c20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獄有十八。",
        "from": "X88n1646_050:0279c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刑分萬條。",
        "from": "X88n1646_050:0279c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塞耳呼號。",
        "from": "X88n1646_050:0279c21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉目愁慘。",
        "from": "X88n1646_050:0279c21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛菩薩及歷代祖師時來救拔。",
        "from": "X88n1646_050:0279c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然非宿有善根者。",
        "from": "X88n1646_050:0279c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖面與開示惛然不知。",
        "from": "X88n1646_050:0279c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟七月之望為佛歡喜日。",
        "from": "X88n1646_050:0279c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普天下盡演瑜伽施食法。",
        "from": "X88n1646_050:0279c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大士親領眾囚赴食。",
        "from": "X88n1646_050:0279c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法力所屆諸罪末減。",
        "from": "X88n1646_050:0279c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過此則與人世遠隔矣。",
        "from": "X88n1646_050:0280a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是歲七月望夕。",
        "from": "X88n1646_050:0280a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成民謂獄官曰。",
        "from": "X88n1646_050:0280a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "好勸眾囚各稱阿彌陀佛。",
        "from": "X88n1646_050:0280a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能稱阿彌陀佛一聲者。",
        "from": "X88n1646_050:0280a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合獄都出罪。",
        "from": "X88n1646_050:0280a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其不能者。",
        "from": "X88n1646_050:0280a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但令持佛字。",
        "from": "X88n1646_050:0280a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復詔四門各樹一牌。",
        "from": "X88n1646_050:0280a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庭中樹三十六幡。",
        "from": "X88n1646_050:0280a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各各大書阿彌陀佛以示眾囚。",
        "from": "X88n1646_050:0280a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復詔眾囚前各書佛字於胸。",
        "from": "X88n1646_050:0280a05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而命之曰念之念之。",
        "from": "X88n1646_050:0280a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慎莫忘佛。",
        "from": "X88n1646_050:0280a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛在爾心。",
        "from": "X88n1646_050:0280a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今告爾等。",
        "from": "X88n1646_050:0280a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有惡鬼銅狗犯爾。",
        "from": "X88n1646_050:0280a06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀劍刺爾。",
        "from": "X88n1646_050:0280a07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自火逼爾。",
        "from": "X88n1646_050:0280a07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾但稱佛名。",
        "from": "X88n1646_050:0280a07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切銷滅。",
        "from": "X88n1646_050:0280a07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮花現前。",
        "from": "X88n1646_050:0280a08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我若誑爾。",
        "from": "X88n1646_050:0280a08_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與爾同罪。",
        "from": "X88n1646_050:0280a08_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成民自受事後。",
        "from": "X88n1646_050:0280a08_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偶言生死事輙驗。",
        "from": "X88n1646_050:0280a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自以徐氏先世有負罪縶獄者。",
        "from": "X88n1646_050:0280a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "率諸宗黨禮梁皇懺。",
        "from": "X88n1646_050:0280a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周而復始得末減乃已。",
        "from": "X88n1646_050:0280a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頃之至九月十四日成民上表謝事。",
        "from": "X88n1646_050:0280a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是修持益力。",
        "from": "X88n1646_050:0280a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日誦準提呪回向淨土以終其身其。",
        "from": "X88n1646_050:0280a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "判語二集。",
        "from": "X88n1646_050:0280a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "里人揚時泰．崑山戴衮序之。",
        "from": "X88n1646_050:0280a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名曰活閻羅斷案。",
        "from": "X88n1646_050:0280a13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刻行於世(活閻羅斷案)。",
        "from": "X88n1646_050:0280a13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知歸子曰。",
        "from": "X88n1646_050:0280a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "予讀文昌化書言。",
        "from": "X88n1646_050:0280a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "張孝仲日應世務。",
        "from": "X88n1646_050:0280a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜治幽冥。",
        "from": "X88n1646_050:0280a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為異。",
        "from": "X88n1646_050:0280a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及觀馬邦良．徐成民事。",
        "from": "X88n1646_050:0280a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃知其事世多有之。",
        "from": "X88n1646_050:0280a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀二人所言。",
        "from": "X88n1646_050:0280a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善惡報應理有固然。",
        "from": "X88n1646_050:0280a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無足怪者。",
        "from": "X88n1646_050:0280a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詩云。",
        "from": "X88n1646_050:0280a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "視爾夢夢。",
        "from": "X88n1646_050:0280a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我心慘慘。",
        "from": "X88n1646_050:0280a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又云。",
        "from": "X88n1646_050:0280a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "胡不相畏。",
        "from": "X88n1646_050:0280a17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不畏於天。",
        "from": "X88n1646_050:0280a17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嗚呼。",
        "from": "X88n1646_050:0280a17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可不念哉。",
        "from": "X88n1646_050:0280a18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汪大紳云。",
        "from": "X88n1646_050:0280a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為甚要念佛。",
        "from": "X88n1646_050:0280a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要腔子裏光明故也。",
        "from": "X88n1646_050:0280a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "腔子裏香淨故也。",
        "from": "X88n1646_050:0280a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一分光明得念佛一分力。",
        "from": "X88n1646_050:0280a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一分香淨得念佛一分力。",
        "from": "X88n1646_050:0280a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到得滿腔子光明。",
        "from": "X88n1646_050:0280a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿腔子香淨。",
        "from": "X88n1646_050:0280a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "這便是極樂世界。",
        "from": "X88n1646_050:0280a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如今人要長要短。",
        "from": "X88n1646_050:0280a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弄得腔子裏又黑又臭。",
        "from": "X88n1646_050:0280a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若到得滿腔子黑臭。",
        "from": "X88n1646_050:0280a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "這便是地獄。",
        "from": "X88n1646_050:0280a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛也救他不得。",
        "from": "X88n1646_050:0280a23_13",
        "context_with_next_sentence": false
    }
]