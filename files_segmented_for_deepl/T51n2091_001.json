[
    {
        "source_sentence": "燉煌錄",
        "from": "T51n2091_001:0997c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "効穀城本是漁澤。",
        "from": "T51n2091_001:0997c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漢孝帝時。",
        "from": "T51n2091_001:0997c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "崔不意教人力田得穀。",
        "from": "T51n2091_001:0997c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因名。",
        "from": "T51n2091_001:0997c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後為縣。",
        "from": "T51n2091_001:0997c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貳師泉去沙城東三程。",
        "from": "T51n2091_001:0997c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漢時李廣利軍行渴乏。",
        "from": "T51n2091_001:0997c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祝山神以劍劄山。",
        "from": "T51n2091_001:0997c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因之水下。",
        "from": "T51n2091_001:0997c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流向西數十里黃章泊。",
        "from": "T51n2091_001:0997c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後有將。",
        "from": "T51n2091_001:0997c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "渴甚飲水。",
        "from": "T51n2091_001:0997c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "泉側而終。",
        "from": "T51n2091_001:0997c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水遂不流。",
        "from": "T51n2091_001:0997c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祇及平地。",
        "from": "T51n2091_001:0997c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後來若人多即水多。",
        "from": "T51n2091_001:0997c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人少即水少。",
        "from": "T51n2091_001:0997c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若郡眾大噉水則猛下。",
        "from": "T51n2091_001:0997c10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至今如然。",
        "from": "T51n2091_001:0997c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其二師廟在路傍。",
        "from": "T51n2091_001:0997c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "久廢。",
        "from": "T51n2091_001:0997c11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但有積石。",
        "from": "T51n2091_001:0997c11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "駝馬行人祈福之所。",
        "from": "T51n2091_001:0997c12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次東入瓜州界。",
        "from": "T51n2091_001:0997c12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "州南有莫高窟。",
        "from": "T51n2091_001:0997c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "去州二十五里。",
        "from": "T51n2091_001:0997c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中過石磧。",
        "from": "T51n2091_001:0997c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶山坡至彼。",
        "from": "T51n2091_001:0997c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斗下谷中。",
        "from": "T51n2091_001:0997c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其東即三危山。",
        "from": "T51n2091_001:0997c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西即鳴沙山。",
        "from": "T51n2091_001:0997c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中有自南流水。",
        "from": "T51n2091_001:0997c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名之宕泉。",
        "from": "T51n2091_001:0997c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "古寺僧舍絕多。",
        "from": "T51n2091_001:0997c15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有洪鐘。",
        "from": "T51n2091_001:0997c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其谷南北兩頭有天王堂及神祠。",
        "from": "T51n2091_001:0997c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壁畫吐蕃贊普部從。",
        "from": "T51n2091_001:0997c17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其山西壁南北二里。",
        "from": "T51n2091_001:0997c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並是鐫鑿高大沙窟。",
        "from": "T51n2091_001:0997c18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塑畫佛像。",
        "from": "T51n2091_001:0997c18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每窟動計費稅百萬。",
        "from": "T51n2091_001:0997c18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前設樓閣數層。",
        "from": "T51n2091_001:0997c19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大像堂殿。",
        "from": "T51n2091_001:0997c19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其像長一百六十尺。",
        "from": "T51n2091_001:0997c19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其小龕無數。",
        "from": "T51n2091_001:0997c20_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉有虛檻通。",
        "from": "T51n2091_001:0997c20_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "連巡禮遊覽之景。",
        "from": "T51n2091_001:0997c20_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次南山有觀音菩薩會現之處。",
        "from": "T51n2091_001:0997c21_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "郡人每詣彼。",
        "from": "T51n2091_001:0997c22_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必徒行來往。",
        "from": "T51n2091_001:0997c22_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其恭敬如是。",
        "from": "T51n2091_001:0997c22_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳴沙山去州十里。",
        "from": "T51n2091_001:0997c22_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其山東西八十里。",
        "from": "T51n2091_001:0997c23_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南北四十里。",
        "from": "T51n2091_001:0997c23_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高處五百尺。",
        "from": "T51n2091_001:0997c24_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉純沙聚起。",
        "from": "T51n2091_001:0997c24_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此山神異。",
        "from": "T51n2091_001:0997c24_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "峯如削成。",
        "from": "T51n2091_001:0997c24_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其間有井。",
        "from": "T51n2091_001:0997c25_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙不能蔽。",
        "from": "T51n2091_001:0997c25_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盛夏自鳴。",
        "from": "T51n2091_001:0997c25_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人馬踐之聲振數十里。",
        "from": "T51n2091_001:0997c25_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風俗端午日。",
        "from": "T51n2091_001:0997c26_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "城中子女皆躋高峯。",
        "from": "T51n2091_001:0997c26_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一齊蹙下。",
        "from": "T51n2091_001:0997c27_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其沙聲吼如雷。",
        "from": "T51n2091_001:0997c27_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至曉看之。",
        "from": "T51n2091_001:0997c27_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "峭崿如舊。",
        "from": "T51n2091_001:0997c27_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "古號鳴沙。",
        "from": "T51n2091_001:0997c28_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神沙而祠焉。",
        "from": "T51n2091_001:0997c28_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "近南有甘泉。",
        "from": "T51n2091_001:0997c28_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自沙山南。",
        "from": "T51n2091_001:0997c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其上源出大雪山。",
        "from": "T51n2091_001:0997c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於西南壽昌縣界入燉煌。",
        "from": "T51n2091_001:0997c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以其沃潤之功俗號甘泉。",
        "from": "T51n2091_001:0998a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金鞍山在沙山西南。",
        "from": "T51n2091_001:0998a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經夏常有雪。",
        "from": "T51n2091_001:0998a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "山中有神祠。",
        "from": "T51n2091_001:0998a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甚靈。",
        "from": "T51n2091_001:0998a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人不敢近。",
        "from": "T51n2091_001:0998a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每歲土主望祀獻駿馬。",
        "from": "T51n2091_001:0998a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "驅入山中。",
        "from": "T51n2091_001:0998a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稍近立致雷電風雹之患。",
        "from": "T51n2091_001:0998a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "州西南有李先王廟。",
        "from": "T51n2091_001:0998a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即西涼昭王先世之廟。",
        "from": "T51n2091_001:0998a05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乾封年。",
        "from": "T51n2091_001:0998a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廟側得瑞石。",
        "from": "T51n2091_001:0998a05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其色翠碧。",
        "from": "T51n2091_001:0998a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有赤文古字云。",
        "from": "T51n2091_001:0998a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卜世三十。",
        "from": "T51n2091_001:0998a06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卜年七百。",
        "from": "T51n2091_001:0998a07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今人呼為李廟。",
        "from": "T51n2091_001:0998a07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "州西有陽關。",
        "from": "T51n2091_001:0998a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即故玉門關。",
        "from": "T51n2091_001:0998a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因沙州刺史陽明詔追拒命奔出此關。",
        "from": "T51n2091_001:0998a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後人呼為陽關。",
        "from": "T51n2091_001:0998a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "接鄯善城。",
        "from": "T51n2091_001:0998a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "險阻乏水草。",
        "from": "T51n2091_001:0998a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不通人行。",
        "from": "T51n2091_001:0998a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其關後移州東。",
        "from": "T51n2091_001:0998a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "城西八十五里。",
        "from": "T51n2091_001:0998a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有玉女泉。",
        "from": "T51n2091_001:0998a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人傳頗有虛。",
        "from": "T51n2091_001:0998a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每歲此郡率童男女各一人。",
        "from": "T51n2091_001:0998a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "充祭湫神。",
        "from": "T51n2091_001:0998a12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年則順成。",
        "from": "T51n2091_001:0998a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不爾損苗。",
        "from": "T51n2091_001:0998a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父母雖苦生離兒女。",
        "from": "T51n2091_001:0998a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為神所錄。",
        "from": "T51n2091_001:0998a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡然携手而沒神龍中。",
        "from": "T51n2091_001:0998a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刺史張孝嵩下車求。",
        "from": "T51n2091_001:0998a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "郡人告之。",
        "from": "T51n2091_001:0998a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大守怒曰。",
        "from": "T51n2091_001:0998a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈有川源妖怪害我生靈。",
        "from": "T51n2091_001:0998a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂設壇備性泉側曰。",
        "from": "T51n2091_001:0998a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願見本身。",
        "from": "T51n2091_001:0998a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲親享。",
        "from": "T51n2091_001:0998a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神乃化為一龍從水而出。",
        "from": "T51n2091_001:0998a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "太守應弦中喉。",
        "from": "T51n2091_001:0998a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拔劍斬首。",
        "from": "T51n2091_001:0998a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "親詣闕進上。",
        "from": "T51n2091_001:0998a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玄宗嘉稱再三。",
        "from": "T51n2091_001:0998a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂賜龍舌。",
        "from": "T51n2091_001:0998a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勅號龍舌。",
        "from": "T51n2091_001:0998a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "張氏編在簡書。",
        "from": "T51n2091_001:0998a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "郡城西北一里有寺。",
        "from": "T51n2091_001:0998a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "古木陰森。",
        "from": "T51n2091_001:0998a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中有小堡。",
        "from": "T51n2091_001:0998a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上設廊殿。",
        "from": "T51n2091_001:0998a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具體而徵。",
        "from": "T51n2091_001:0998a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先有沙倅張球。",
        "from": "T51n2091_001:0998a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已邁從心。",
        "from": "T51n2091_001:0998a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寓止于此。",
        "from": "T51n2091_001:0998a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖非博學。",
        "from": "T51n2091_001:0998a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦甚苦心。",
        "from": "T51n2091_001:0998a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓋經亂年多。",
        "from": "T51n2091_001:0998a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "習業人少。",
        "from": "T51n2091_001:0998a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂集後進。",
        "from": "T51n2091_001:0998a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以闡大猷。",
        "from": "T51n2091_001:0998a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天不愍遺。",
        "from": "T51n2091_001:0998a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "民受其賜。",
        "from": "T51n2091_001:0998a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "石膏山在州北二百五十六里。",
        "from": "T51n2091_001:0998a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烏山烽山石間出其膏。",
        "from": "T51n2091_001:0998a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開皇十九年。",
        "from": "T51n2091_001:0998a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烏山變白。",
        "from": "T51n2091_001:0998a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中驗不虛遣道士皇甫德琮等七人。",
        "from": "T51n2091_001:0998a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祭醮。",
        "from": "T51n2091_001:0998a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自後望如雪峯。",
        "from": "T51n2091_001:0998a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "河倉城州西北二百三十里。",
        "from": "T51n2091_001:0998a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "古時軍儲在彼。",
        "from": "T51n2091_001:0998a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長城在州北。",
        "from": "T51n2091_001:0998a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其城六十三里。",
        "from": "T51n2091_001:0998a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正西入磧。",
        "from": "T51n2091_001:0998a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前漢所卜置。",
        "from": "T51n2091_001:0998a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北入伊州界。",
        "from": "T51n2091_001:0998b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燉煌錄一卷",
        "from": "T51n2091_001:0998b02_0",
        "context_with_next_sentence": false
    }
]