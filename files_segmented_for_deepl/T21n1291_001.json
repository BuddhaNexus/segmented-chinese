[
    {
        "source_sentence": "深沙大將儀軌",
        "from": "T21n1291_001:0376b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大興善寺三藏沙門大廣智",
        "from": "T21n1291_001:0376b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空奉　詔譯",
        "from": "T21n1291_001:0376b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時觀世音菩薩。",
        "from": "T21n1291_001:0376b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在大眾會座中而起。",
        "from": "T21n1291_001:0376b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五體投地禮佛雙足。",
        "from": "T21n1291_001:0376b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而白佛言。",
        "from": "T21n1291_001:0376b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我有一真言一印三使者真言廣大成就之法。",
        "from": "T21n1291_001:0376b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願為利益安樂閻浮提諸善男子善女人故。",
        "from": "T21n1291_001:0376b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願世尊哀愍聽許我今說之。",
        "from": "T21n1291_001:0376b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1291_001:0376b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那謨囉怛那(一)跢囉夜耶(二)那謨阿利耶婆嚧枳帝(三)跢侄他(四)闍曳闍曳(二合)闍夜(五)婆醯儞闍榆跢唎(六)迦囉迦囉麼囉麼囉者羅器拏(八)薩婆羯摩跢羅拏儞(九)迷婆底沙訶誦此呪已惡業消滅。",
        "from": "T21n1291_001:0376b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦至七遍五逆罪滅。",
        "from": "T21n1291_001:0376b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若滿千遍已罪無不滅。",
        "from": "T21n1291_001:0376b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿十萬遍面見觀音種種莊嚴者。",
        "from": "T21n1291_001:0376b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七日之中初作法時。",
        "from": "T21n1291_001:0376b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯得食乳糜酥。",
        "from": "T21n1291_001:0376b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最後一日勿食。",
        "from": "T21n1291_001:0376b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十五日夜佛前念誦無定限數。",
        "from": "T21n1291_001:0376b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見像捊出聲唱言。",
        "from": "T21n1291_001:0376b29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉放光明曜。",
        "from": "T21n1291_001:0376b29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復以真珠寶物安呪師頂上。",
        "from": "T21n1291_001:0376c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即知成驗。",
        "from": "T21n1291_001:0376c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面見觀音已。",
        "from": "T21n1291_001:0376c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得滿一切善願。",
        "from": "T21n1291_001:0376c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又於睡眠中夢見觀音種種莊嚴者。",
        "from": "T21n1291_001:0376c02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切善事成就。",
        "from": "T21n1291_001:0376c03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切惡業消滅。",
        "from": "T21n1291_001:0376c03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真轉轉者得滅罪。",
        "from": "T21n1291_001:0376c04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說一印亦名利益眾生。",
        "from": "T21n1291_001:0376c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以一印三十二及種種成辦。",
        "from": "T21n1291_001:0376c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手交背相。",
        "from": "T21n1291_001:0376c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二母指押二頭指為劍。",
        "from": "T21n1291_001:0376c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印使向虛空為天蓋。",
        "from": "T21n1291_001:0376c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "境界十方天上隨處成結界。",
        "from": "T21n1291_001:0376c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向本尊為普供養。",
        "from": "T21n1291_001:0376c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作法為普禮。",
        "from": "T21n1291_001:0376c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後時為迴向。",
        "from": "T21n1291_001:0376c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又為救發遣若欲降伏惡人者。",
        "from": "T21n1291_001:0376c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作手印無二以瞋心誦一百八遍。",
        "from": "T21n1291_001:0376c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切外道所作滅　若人心病者加持黃土塗於心除差。",
        "from": "T21n1291_001:0376c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人患頭病。",
        "from": "T21n1291_001:0376c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "土火。",
        "from": "T21n1291_001:0376c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人患眼者。",
        "from": "T21n1291_001:0376c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持杏子之油塗除差。",
        "from": "T21n1291_001:0376c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人患腫者。",
        "from": "T21n1291_001:0376c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[米/曰]夫加持一百八遍塗除愈。",
        "from": "T21n1291_001:0376c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲令遠去魔事。",
        "from": "T21n1291_001:0376c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持若木八遍其人遠去。",
        "from": "T21n1291_001:0376c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次使者三師二十八部總攝。",
        "from": "T21n1291_001:0376c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手掌平向天上。",
        "from": "T21n1291_001:0376c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大指來去。",
        "from": "T21n1291_001:0376c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天上使者。",
        "from": "T21n1291_001:0376c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "費迦羅亦名淨滿。",
        "from": "T21n1291_001:0376c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密語曰。",
        "from": "T21n1291_001:0376c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "費迦羅翳醯[口*皿]娑嚩呵",
        "from": "T21n1291_001:0376c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手合二地來去虛空使者。",
        "from": "T21n1291_001:0376c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大仙亦名嚩斯密語曰。",
        "from": "T21n1291_001:0376c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉儞也娑婆呵",
        "from": "T21n1291_001:0376c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手向虛空。",
        "from": "T21n1291_001:0376c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大來去下地使者。",
        "from": "T21n1291_001:0376c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水火雷自在亦名大羅惹。",
        "from": "T21n1291_001:0376c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密語曰。",
        "from": "T21n1291_001:0376c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵闍曳闍曳娑嚩呵",
        "from": "T21n1291_001:0376c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此三種使者。",
        "from": "T21n1291_001:0376c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能滅三災能滅三種執。",
        "from": "T21n1291_001:0376c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能滅生生世世業障。",
        "from": "T21n1291_001:0376c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生生世世。",
        "from": "T21n1291_001:0376c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉地成就。",
        "from": "T21n1291_001:0376c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此三種使者無相爭。",
        "from": "T21n1291_001:0376c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "力頓生滅轉鉢結縛木合空鉢。",
        "from": "T21n1291_001:0376c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人滿呪功者。",
        "from": "T21n1291_001:0377a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘空界為仙人。",
        "from": "T21n1291_001:0377a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "召集天龍藥叉鬼神等。",
        "from": "T21n1291_001:0377a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "任心發遣尤法力也。",
        "from": "T21n1291_001:0377a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿迦陀密",
        "from": "T21n1291_001:0377a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一印千類千轉三使者成就法一卷",
        "from": "T21n1291_001:0377a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深沙大將菩薩儀軌要一首",
        "from": "T21n1291_001:0377a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運錄云。",
        "from": "T21n1291_001:0377a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩醯首羅法一卷　睿錄云。",
        "from": "T21n1291_001:0377a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩醯首羅天化生呪法一卷(無人名。",
        "from": "T21n1291_001:0377a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三紙。",
        "from": "T21n1291_001:0377a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "策子說愛法降伏怨法病法又不論淨不淨)。",
        "from": "T21n1291_001:0377a08_4",
        "context_with_next_sentence": false
    }
]