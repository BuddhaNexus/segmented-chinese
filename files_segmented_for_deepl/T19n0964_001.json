[
    {
        "source_sentence": "佛說大威德金輪佛頂熾盛光如",
        "from": "T19n0964_001:0338b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來消除一切災難陀羅尼經",
        "from": "T19n0964_001:0338b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐代失譯",
        "from": "T19n0964_001:0338b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時釋迦牟尼佛。",
        "from": "T19n0964_001:0338b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住淨居天宮。",
        "from": "T19n0964_001:0338b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告文殊師利菩薩摩訶薩。",
        "from": "T19n0964_001:0338b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸四眾八部遊空大天九執七曜十二宮神二十八星日月諸宿。",
        "from": "T19n0964_001:0338b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我昔於過去娑羅樹王佛所。",
        "from": "T19n0964_001:0338b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受此大威德金輪佛頂熾盛光如來消除一切災難陀羅尼法。",
        "from": "T19n0964_001:0338b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於未來世中若有國界。",
        "from": "T19n0964_001:0338b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日月五星羅睺計都彗孛妖怪惡星。",
        "from": "T19n0964_001:0338b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照臨所屬本命宮宿及諸星位。",
        "from": "T19n0964_001:0338b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或臨帝座於國於家并分野處。",
        "from": "T19n0964_001:0338b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陵逼之時或進或入作諸災難者。",
        "from": "T19n0964_001:0338b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應於清淨處置立道場。",
        "from": "T19n0964_001:0338b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "志心持是陀羅尼經一百八遍或一千八十遍。",
        "from": "T19n0964_001:0338b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一日二日乃至七日。",
        "from": "T19n0964_001:0338b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依法修治壇場受持讀誦。",
        "from": "T19n0964_001:0338b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切災難自然消滅不能為害。",
        "from": "T19n0964_001:0338b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若大白火星入於南斗。",
        "from": "T19n0964_001:0338b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於國於家及分野處作災難者。",
        "from": "T19n0964_001:0338b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應於一忿怒尊像前。",
        "from": "T19n0964_001:0338b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫彼設覩嚕形。",
        "from": "T19n0964_001:0338b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念此真言加持。",
        "from": "T19n0964_001:0338b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其災即散移於不順王命悖逆人身上。",
        "from": "T19n0964_001:0338b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說陀羅尼真言曰。",
        "from": "T19n0964_001:0338b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喃無(去)三滿哆沒馱喃阿鉢囉(二合)底(丁以反)賀哆舍薩曩喃(引)唵(引)佉佉佉呬佉呬吽(半音)吽入嚩(二合)囉入嚩(二合)囉鉢囉(二合)入嚩(二合)囉鉢囉(二合)入嚩(二合)囉底瑟吒(二合)底瑟吒(二合)瑟緻(二合)哩瑟緻(二合)哩薩發(二合)吒薩發(二合)吒扇底迦室哩(二合)曳娑嚩(二合)賀佛言若國界分野及男子女人。",
        "from": "T19n0964_001:0338b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被諸天星辰所臨身形。",
        "from": "T19n0964_001:0338b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但書寫此經志心受持讀誦。",
        "from": "T19n0964_001:0338c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常須護淨。",
        "from": "T19n0964_001:0338c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此陀羅尼一切如來同共宣說。",
        "from": "T19n0964_001:0338c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能成就八萬種大吉祥事。",
        "from": "T19n0964_001:0338c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復能滅除八萬種大不吉祥事。",
        "from": "T19n0964_001:0338c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有國王大臣及諸眷屬一切庶民。",
        "from": "T19n0964_001:0338c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或被五星羅睺計都彗孛怪惡諸宿。",
        "from": "T19n0964_001:0338c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陵逼帝座於國於家并分野處所屬宮宿。",
        "from": "T19n0964_001:0338c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "災難競起。",
        "from": "T19n0964_001:0338c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或土星侵陵或進或退。",
        "from": "T19n0964_001:0338c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及宿世怨家欲相謀害。",
        "from": "T19n0964_001:0338c07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸惡橫事口舌厭禱呪詛符書以為災難。",
        "from": "T19n0964_001:0338c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸眾生依法受持。",
        "from": "T19n0964_001:0338c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切災禍不能為害。",
        "from": "T19n0964_001:0338c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變災為福皆得吉祥。",
        "from": "T19n0964_001:0338c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我說此真言不可思議功德無比。",
        "from": "T19n0964_001:0338c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祕密受持勿妄宣傳。",
        "from": "T19n0964_001:0338c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告大眾若有國界不安災難起時。",
        "from": "T19n0964_001:0338c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及男子女人災詳變禍。",
        "from": "T19n0964_001:0338c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但請僧眾如法建立道場安置佛像潔戒護持。",
        "from": "T19n0964_001:0338c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香花燈燭隨分供養。",
        "from": "T19n0964_001:0338c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸眾生獲福無量其災即除。",
        "from": "T19n0964_001:0338c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時如來復告大眾。",
        "from": "T19n0964_001:0338c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人行年被金木水火土五星。",
        "from": "T19n0964_001:0338c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及羅睺計都日月諸宿臨身。",
        "from": "T19n0964_001:0338c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "災難競起。",
        "from": "T19n0964_001:0338c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我有大吉祥真言名破宿曜。",
        "from": "T19n0964_001:0338c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能受持志心憶念。",
        "from": "T19n0964_001:0338c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其災自滅變禍為福。",
        "from": "T19n0964_001:0338c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說真言曰。",
        "from": "T19n0964_001:0338c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)薩嚩諾剎咀囉(二合)糝摩曳室哩曳扇底迦呴嚧薩婆訶爾時如來說是經時。",
        "from": "T19n0964_001:0338c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊師利菩薩摩訶薩。",
        "from": "T19n0964_001:0338c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸四眾遊空大天諸星辰等一切聖眾。",
        "from": "T19n0964_001:0338c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸依佛勅頂禮奉持各還本宮。",
        "from": "T19n0964_001:0338c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天龍八部等聞佛所說。",
        "from": "T19n0964_001:0338c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆大歡喜信受奉行。",
        "from": "T19n0964_001:0338c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說熾盛光消災經",
        "from": "T19n0964_001:0338c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九曜真言曰。",
        "from": "T19n0964_001:0338c27_0",
        "context_with_next_sentence": true
    },
