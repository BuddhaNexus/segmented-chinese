[
    {
        "source_sentence": "心經小談",
        "from": "X26n0556_001:0880b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明　觀衡　述",
        "from": "X26n0556_001:0880b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩訶般若波羅蜜多心經。",
        "from": "X26n0556_001:0880b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此十字乃一經之題目。",
        "from": "X26n0556_001:0880b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩訶是梵語。",
        "from": "X26n0556_001:0880b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐言大。",
        "from": "X26n0556_001:0880b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "般若是梵語。",
        "from": "X26n0556_001:0880b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐言智慧。",
        "from": "X26n0556_001:0880b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "波羅蜜多是梵語。",
        "from": "X26n0556_001:0880b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐言彼岸到。",
        "from": "X26n0556_001:0880b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即到彼岸。",
        "from": "X26n0556_001:0880b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言眾生無智慧。",
        "from": "X26n0556_001:0880b08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迷淪生死苦海。",
        "from": "X26n0556_001:0880b08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛菩薩有大智慧。",
        "from": "X26n0556_001:0880b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照破無明。",
        "from": "X26n0556_001:0880b09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能超生死苦海。",
        "from": "X26n0556_001:0880b09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到涅槃彼岸。",
        "from": "X26n0556_001:0880b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故云摩訶般若波羅蜜多。",
        "from": "X26n0556_001:0880b10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心之一字。",
        "from": "X26n0556_001:0880b10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念在上即般若心。",
        "from": "X26n0556_001:0880b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若念在下即此本經為六百卷之心也。",
        "from": "X26n0556_001:0880b11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經者梵語修多羅。",
        "from": "X26n0556_001:0880b12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此云契經。",
        "from": "X26n0556_001:0880b12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "契理合機之教也。",
        "from": "X26n0556_001:0880b12_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此方從略。",
        "from": "X26n0556_001:0880b13_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故但云經。",
        "from": "X26n0556_001:0880b13_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故云摩訶般若波羅蜜多心經(此經多不讀摩訶。",
        "from": "X26n0556_001:0880b13_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "據文中云深般若。",
        "from": "X26n0556_001:0880b14_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "題中亦應云大般若)。",
        "from": "X26n0556_001:0880b14_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀自在菩薩。",
        "from": "X26n0556_001:0880b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即能行般若之人。",
        "from": "X26n0556_001:0880b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉是人以顯般若之法。",
        "from": "X26n0556_001:0880b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀自在三字。",
        "from": "X26n0556_001:0880b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是此菩薩別名。",
        "from": "X26n0556_001:0880b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩二字是通號。",
        "from": "X26n0556_001:0880b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何名觀自在。",
        "from": "X26n0556_001:0880b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言此菩薩。",
        "from": "X26n0556_001:0880b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行深般若波羅蜜多時。",
        "from": "X26n0556_001:0880b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照見五蘊皆空。",
        "from": "X26n0556_001:0880b17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "度一切苦厄。",
        "from": "X26n0556_001:0880b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名觀自在。",
        "from": "X26n0556_001:0880b18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照見五蘊皆空。",
        "from": "X26n0556_001:0880b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即觀。",
        "from": "X26n0556_001:0880b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "度一切苦厄。",
        "from": "X26n0556_001:0880b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即自在也。",
        "from": "X26n0556_001:0880b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上略破三字大意。",
        "from": "X26n0556_001:0880b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下講明其義。",
        "from": "X26n0556_001:0880b19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利子。",
        "from": "X26n0556_001:0880b20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是尊者名。",
        "from": "X26n0556_001:0880b20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此尊者智慧第一。",
        "from": "X26n0556_001:0880b20_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故呼其名而授之。",
        "from": "X26n0556_001:0880b20_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂上言照見五蘊皆空。",
        "from": "X26n0556_001:0880b21_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如何照見。",
        "from": "X26n0556_001:0880b21_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如何得空。",
        "from": "X26n0556_001:0880b21_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利子當如是觀照。",
        "from": "X26n0556_001:0880b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色不異空。",
        "from": "X26n0556_001:0880b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空不異色。",
        "from": "X26n0556_001:0880b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色即是空。",
        "from": "X26n0556_001:0880b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空即是色。",
        "from": "X26n0556_001:0880b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是觀照。",
        "from": "X26n0556_001:0880b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即空也。",
        "from": "X26n0556_001:0880b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此四句。",
        "from": "X26n0556_001:0880b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上二句。",
        "from": "X26n0556_001:0880b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先照色空體一。",
        "from": "X26n0556_001:0880b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下二句。",
        "from": "X26n0556_001:0880b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方照見色性本空。",
        "from": "X26n0556_001:0880b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言不異。",
        "from": "X26n0556_001:0880b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要成下一即字。",
        "from": "X26n0556_001:0880b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總之在色即是空一句為正。",
        "from": "X26n0556_001:0880c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘助成耳。",
        "from": "X26n0556_001:0880c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色一法如是觀照。",
        "from": "X26n0556_001:0880c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受想行識亦如是觀照。",
        "from": "X26n0556_001:0880c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故云亦復如是。",
        "from": "X26n0556_001:0880c02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上講明五蘊皆空一句。",
        "from": "X26n0556_001:0880c02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下講度一切苦厄。",
        "from": "X26n0556_001:0880c03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂五蘊諸法若有。",
        "from": "X26n0556_001:0880c03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即有生相。",
        "from": "X26n0556_001:0880c04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有滅相。",
        "from": "X26n0556_001:0880c04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有垢相。",
        "from": "X26n0556_001:0880c04_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有淨相。",
        "from": "X26n0556_001:0880c04_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有增相。",
        "from": "X26n0556_001:0880c04_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有減相。",
        "from": "X26n0556_001:0880c04_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆苦相也。",
        "from": "X26n0556_001:0880c05_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若諸法空相。",
        "from": "X26n0556_001:0880c05_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既是空相。",
        "from": "X26n0556_001:0880c05_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有何生。",
        "from": "X26n0556_001:0880c05_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有何滅。",
        "from": "X26n0556_001:0880c05_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有何垢。",
        "from": "X26n0556_001:0880c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有何淨。",
        "from": "X26n0556_001:0880c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有何增。",
        "from": "X26n0556_001:0880c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有何減。",
        "from": "X26n0556_001:0880c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既無生滅垢淨等。",
        "from": "X26n0556_001:0880c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即涅槃真樂也。",
        "from": "X26n0556_001:0880c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故復告舍利子。",
        "from": "X26n0556_001:0880c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸法空相。",
        "from": "X26n0556_001:0880c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不生不滅。",
        "from": "X26n0556_001:0880c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不垢不淨。",
        "from": "X26n0556_001:0880c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不增不減。",
        "from": "X26n0556_001:0880c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此一節。",
        "from": "X26n0556_001:0880c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "講明上度一切苦厄一句也。",
        "from": "X26n0556_001:0880c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上既講明照見五蘊皆空度一切苦厄之義。",
        "from": "X26n0556_001:0880c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下廣為開演。",
        "from": "X26n0556_001:0880c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故空中一句。",
        "from": "X26n0556_001:0880c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "承上諸法空相之中。",
        "from": "X26n0556_001:0880c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言此空中不唯五蘊皆空。",
        "from": "X26n0556_001:0880c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二處亦空。",
        "from": "X26n0556_001:0880c11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十八界亦空。",
        "from": "X26n0556_001:0880c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無眼耳鼻舌身意。",
        "from": "X26n0556_001:0880c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無色聲香味觸法。",
        "from": "X26n0556_001:0880c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此空十二處也。",
        "from": "X26n0556_001:0880c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無眼界乃至無意識界。",
        "from": "X26n0556_001:0880c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此空十八界也。",
        "from": "X26n0556_001:0880c13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此空世間法。",
        "from": "X26n0556_001:0880c13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下空出世間法。",
        "from": "X26n0556_001:0880c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無無明乃至無無明盡。",
        "from": "X26n0556_001:0880c14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無老死乃至無老死盡。",
        "from": "X26n0556_001:0880c14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此空緣覺法也。",
        "from": "X26n0556_001:0880c15_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無苦集滅道。",
        "from": "X26n0556_001:0880c15_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無智亦無得。",
        "from": "X26n0556_001:0880c15_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此空聲聞法也。",
        "from": "X26n0556_001:0880c15_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言此空中世間出世間皆空也。",
        "from": "X26n0556_001:0880c16_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此開演上照見五蘊皆空一句。",
        "from": "X26n0556_001:0880c16_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即一觀字也。",
        "from": "X26n0556_001:0880c17_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以無所得故一句。",
        "from": "X26n0556_001:0880c17_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "承上起下之辭也。",
        "from": "X26n0556_001:0880c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言世間法出世間法都無所得。",
        "from": "X26n0556_001:0880c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以無所得故。",
        "from": "X26n0556_001:0880c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛菩薩。",
        "from": "X26n0556_001:0880c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依之斷煩惱。",
        "from": "X26n0556_001:0880c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證菩提。",
        "from": "X26n0556_001:0880c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出生死。",
        "from": "X26n0556_001:0880c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證涅槃。",
        "from": "X26n0556_001:0880c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故云菩提薩埵。",
        "from": "X26n0556_001:0880c20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依般若波羅蜜多故。",
        "from": "X26n0556_001:0880c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心無罣礙。",
        "from": "X26n0556_001:0880c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無罣礙故。",
        "from": "X26n0556_001:0880c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有恐怖。",
        "from": "X26n0556_001:0880c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離顛倒夢想。",
        "from": "X26n0556_001:0880c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "究竟涅槃。",
        "from": "X26n0556_001:0880c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此言菩薩依般若離苦得樂也。",
        "from": "X26n0556_001:0880c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三世諸佛。",
        "from": "X26n0556_001:0880c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依般若波羅蜜多故。",
        "from": "X26n0556_001:0880c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得阿耨多羅三藐三菩提。",
        "from": "X26n0556_001:0880c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此言諸佛依般若離苦得樂也。",
        "from": "X26n0556_001:0880c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自以無所得。",
        "from": "X26n0556_001:0880c24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至阿耨菩提。",
        "from": "X26n0556_001:0880c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此一節。",
        "from": "X26n0556_001:0880c24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開演引證上度一切苦厄一句。",
        "from": "X26n0556_001:0880c24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即自在二字也。",
        "from": "X26n0556_001:0881a01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上既破解講明開演引證。",
        "from": "X26n0556_001:0881a01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明知照見五蘊皆空度一切苦厄實處。",
        "from": "X26n0556_001:0881a02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故下結成。",
        "from": "X26n0556_001:0881a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有二。",
        "from": "X26n0556_001:0881a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初結顯般若之功。",
        "from": "X26n0556_001:0881a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故云故知般若波羅蜜多。",
        "from": "X26n0556_001:0881a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是大神呪。",
        "from": "X26n0556_001:0881a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(轉凡成聖神妙難測)是大明呪。",
        "from": "X26n0556_001:0881a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(慧日當空破無明暗)是無上呪。",
        "from": "X26n0556_001:0881a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(無過之前無加之上)是無等等呪。",
        "from": "X26n0556_001:0881a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(不唯無上亦無齊等)上結顯般若神功。",
        "from": "X26n0556_001:0881a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二結成破題。",
        "from": "X26n0556_001:0881a06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因上破云照見五蘊皆空度一切苦厄。",
        "from": "X26n0556_001:0881a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如何見得。",
        "from": "X26n0556_001:0881a07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既講明開演引證了然明白。",
        "from": "X26n0556_001:0881a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故此結云能除一切苦真實不虗。",
        "from": "X26n0556_001:0881a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此即自在也。",
        "from": "X26n0556_001:0881a08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總之一經只觀自在三字盡矣。",
        "from": "X26n0556_001:0881a08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上顯說。",
        "from": "X26n0556_001:0881a09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下密說。",
        "from": "X26n0556_001:0881a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故說般若波羅蜜多呪。",
        "from": "X26n0556_001:0881a09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "X26n0556_001:0881a10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "揭諦揭諦　波羅揭諦　波羅僧揭諦　菩提娑訶上顯說令人依義生慧。",
        "from": "X26n0556_001:0881a10_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密說令人持誦生定。",
        "from": "X26n0556_001:0881a11_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定慧均等。",
        "from": "X26n0556_001:0881a12_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脩行備矣。",
        "from": "X26n0556_001:0881a12_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心經小談(終)",
        "from": "X26n0556_001:0881a14_0",
        "context_with_next_sentence": false
    }
]