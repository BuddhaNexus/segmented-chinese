[
    {
        "source_sentence": "大吉祥天女十二契一百八名無",
        "from": "T21n1253_001:0253b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "垢大乘經",
        "from": "T21n1253_001:0253b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T21n1253_001:0253b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空諡大鑒",
        "from": "T21n1253_001:0253b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正號大廣智大興善寺三藏沙門",
        "from": "T21n1253_001:0253b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空奉　詔譯",
        "from": "T21n1253_001:0253b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T21n1253_001:0253b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時薄伽梵。",
        "from": "T21n1253_001:0253b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住安樂世界。",
        "from": "T21n1253_001:0253b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大菩薩眾。",
        "from": "T21n1253_001:0253b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂觀自在菩薩。",
        "from": "T21n1253_001:0253b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得大勢菩薩。",
        "from": "T21n1253_001:0253b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除一切蓋障菩薩。",
        "from": "T21n1253_001:0253b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地藏菩薩。",
        "from": "T21n1253_001:0253b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普光菩薩。",
        "from": "T21n1253_001:0253b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空藏菩薩。",
        "from": "T21n1253_001:0253b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛手菩薩除一切怖畏菩薩。",
        "from": "T21n1253_001:0253b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持一切清淨吉祥菩薩。",
        "from": "T21n1253_001:0253b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持一切福相菩薩。",
        "from": "T21n1253_001:0253b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持日月三世菩薩。",
        "from": "T21n1253_001:0253b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊師利菩薩。",
        "from": "T21n1253_001:0253b16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等菩薩摩訶薩而為上首。",
        "from": "T21n1253_001:0253b16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時觀自在菩薩摩訶薩。",
        "from": "T21n1253_001:0253b17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往詣世尊所頭面禮足退坐一面。",
        "from": "T21n1253_001:0253b17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時大吉祥天女亦往詣佛所頭面禮足。",
        "from": "T21n1253_001:0253b18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圍繞。",
        "from": "T21n1253_001:0253b19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量百千匝及禮一切安樂世界所住菩薩退坐一面。",
        "from": "T21n1253_001:0253b19_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊見吉祥天女。",
        "from": "T21n1253_001:0253b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有無量百千福莊嚴。",
        "from": "T21n1253_001:0253b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱胝如來圍繞。",
        "from": "T21n1253_001:0253b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切釋梵護世讚揚稱歎。",
        "from": "T21n1253_001:0253b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大梵音告觀自在菩薩摩訶薩言。",
        "from": "T21n1253_001:0253b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀自在菩薩若有國王王子比丘比丘尼優婆塞優婆夷婆羅門剎利毘舍首陀。",
        "from": "T21n1253_001:0253b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若受持大吉祥天女十二契一百八名無垢讚歎。",
        "from": "T21n1253_001:0253b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其王剎利國界所有眾生。",
        "from": "T21n1253_001:0253b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切怖畏逼惱並皆消除。",
        "from": "T21n1253_001:0253b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切怨賊人非人怖亦不為害。",
        "from": "T21n1253_001:0253b27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切財穀皆悉豐饒。",
        "from": "T21n1253_001:0253b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉祥天女於彼王剎利宅中常所居止。",
        "from": "T21n1253_001:0253b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時彼菩薩摩訶薩說如是言。",
        "from": "T21n1253_001:0253b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉世尊妙說此語。",
        "from": "T21n1253_001:0253c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有持吉祥天女名號。",
        "from": "T21n1253_001:0253c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼獲如是福利。",
        "from": "T21n1253_001:0253c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時無畏觀自在菩薩摩訶薩。",
        "from": "T21n1253_001:0253c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即白佛言世尊。",
        "from": "T21n1253_001:0253c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉祥天女曾於何處種植善根。",
        "from": "T21n1253_001:0253c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言彼於恒河沙如來應供正遍知處。",
        "from": "T21n1253_001:0253c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種植善根。",
        "from": "T21n1253_001:0253c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無畏觀自在菩薩我念過去世。",
        "from": "T21n1253_001:0253c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於寶生世界。",
        "from": "T21n1253_001:0253c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶花功德海吠琉璃金山金光明吉祥如來應供正遍知。",
        "from": "T21n1253_001:0253c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出興於世。",
        "from": "T21n1253_001:0253c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大吉祥天女於彼種植善根。",
        "from": "T21n1253_001:0253c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及餘多如來所。",
        "from": "T21n1253_001:0253c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由稱如是如來名號。",
        "from": "T21n1253_001:0253c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大吉祥天女作成就善根。",
        "from": "T21n1253_001:0253c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此諸如來常隨逐大吉祥天女。",
        "from": "T21n1253_001:0253c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能脫一切罪。",
        "from": "T21n1253_001:0253c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除滅一切煩惱。",
        "from": "T21n1253_001:0253c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令一切身作無垢。",
        "from": "T21n1253_001:0253c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "召集增益一切財穀。",
        "from": "T21n1253_001:0253c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除貧窮。",
        "from": "T21n1253_001:0253c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能攝召一切天龍藥叉羅剎乾闥婆阿脩羅迦樓羅緊那羅摩睺羅伽。",
        "from": "T21n1253_001:0253c13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能息一切逼惱諍訟鬪戰。",
        "from": "T21n1253_001:0253c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能成辦六波羅蜜所謂。",
        "from": "T21n1253_001:0253c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無吉祥密如來",
        "from": "T21n1253_001:0253c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無恒河一切津口吉慶吉祥如來",
        "from": "T21n1253_001:0253c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無栴檀花威星光吉祥如來",
        "from": "T21n1253_001:0253c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無普遍照曜勝鬪戰吉祥如來",
        "from": "T21n1253_001:0253c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無功德海照曜曼茶羅吉祥如來",
        "from": "T21n1253_001:0253c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無法神通幢進吉祥如來",
        "from": "T21n1253_001:0253c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無曜寂靜香照曜吉祥如來",
        "from": "T21n1253_001:0253c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無眾生意樂寂靜身吉祥如來",
        "from": "T21n1253_001:0253c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無願海吉祥如來",
        "from": "T21n1253_001:0253c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無妙遍稱讚名號吉祥如來",
        "from": "T21n1253_001:0253c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無不退輪寶處吉祥如來",
        "from": "T21n1253_001:0253c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無日輪照曜踴起吉祥如來",
        "from": "T21n1253_001:0253c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無無數精進妙住吉祥如來",
        "from": "T21n1253_001:0253c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無無量善住吉祥如來",
        "from": "T21n1253_001:0254a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無音聲支分吉祥如來",
        "from": "T21n1253_001:0254a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無般若燈無數光幢吉祥如來",
        "from": "T21n1253_001:0254a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無那羅延禁戒甲冑吉祥如來",
        "from": "T21n1253_001:0254a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無梵吉祥如來",
        "from": "T21n1253_001:0254a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無摩醯首羅吉祥如來",
        "from": "T21n1253_001:0254a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無日月吉祥如來",
        "from": "T21n1253_001:0254a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無甚深法光王吉祥如來",
        "from": "T21n1253_001:0254a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無虛空燈現喜吉祥如來",
        "from": "T21n1253_001:0254a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無日光幢吉祥如來",
        "from": "T21n1253_001:0254a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無香燈吉祥如來",
        "from": "T21n1253_001:0254a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無海藏生吉祥如來",
        "from": "T21n1253_001:0254a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無變化雲妙聲吉祥如來",
        "from": "T21n1253_001:0254a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無一切照曜嚴吉祥如來",
        "from": "T21n1253_001:0254a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無樹王增長吉祥如來",
        "from": "T21n1253_001:0254a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無寶焰山吉祥如來",
        "from": "T21n1253_001:0254a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無智焰海吉祥如來",
        "from": "T21n1253_001:0254a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無大願進吉祥如來",
        "from": "T21n1253_001:0254a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無大雲吉祥如來",
        "from": "T21n1253_001:0254a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無念幢王吉祥如來",
        "from": "T21n1253_001:0254a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無帝幢幡王吉祥如來",
        "from": "T21n1253_001:0254a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無鉤召一切財穀吉祥如來",
        "from": "T21n1253_001:0254a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無鉤召寂靜吉祥如來",
        "from": "T21n1253_001:0254a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無鉤召吉慶吉祥如來",
        "from": "T21n1253_001:0254a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是如來名號。",
        "from": "T21n1253_001:0254a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有恭敬受持讀誦者。",
        "from": "T21n1253_001:0254a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼善男子善女人。",
        "from": "T21n1253_001:0254a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得發生甚多福聚。",
        "from": "T21n1253_001:0254a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來授記大吉祥天女。",
        "from": "T21n1253_001:0254a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當於吉祥寶莊嚴世界成等正覺。",
        "from": "T21n1253_001:0254a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號吉祥摩尼寶生如來應供正遍知。",
        "from": "T21n1253_001:0254a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其世界種種天寶以為莊嚴。",
        "from": "T21n1253_001:0254a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彼世界。",
        "from": "T21n1253_001:0254a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯此如來作光明。",
        "from": "T21n1253_001:0254a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼菩薩眾於彼佛世界中。",
        "from": "T21n1253_001:0254b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然光明壽命無量。",
        "from": "T21n1253_001:0254b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從空演出佛法僧音聲。",
        "from": "T21n1253_001:0254b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有菩薩於彼佛世界生者。",
        "from": "T21n1253_001:0254b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切皆蓮華臺化生。",
        "from": "T21n1253_001:0254b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何十二契一百八名無垢讚歎。",
        "from": "T21n1253_001:0254b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無畏觀自在汝今諦聽。",
        "from": "T21n1253_001:0254b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂。",
        "from": "T21n1253_001:0254b05_2",
        "context_with_next_sentence": true
    },
