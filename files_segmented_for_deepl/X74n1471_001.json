[
    {
        "source_sentence": "華嚴清涼國師禮讚文(并敘)",
        "from": "X74n1471_001:0360c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長水法孫(智肱)述",
        "from": "X74n1471_001:0360c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾祖賢首國師。",
        "from": "X74n1471_001:0360c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開宗判教。",
        "from": "X74n1471_001:0360c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經窮一代。",
        "from": "X74n1471_001:0360c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義絕百家。",
        "from": "X74n1471_001:0360c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解釋唐則天聖后朝。",
        "from": "X74n1471_001:0360c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所譯華嚴。",
        "from": "X74n1471_001:0360c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至十九卷。",
        "from": "X74n1471_001:0360c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大願未終。",
        "from": "X74n1471_001:0360c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奄然真寂。",
        "from": "X74n1471_001:0360c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清凉始登賢首法席。",
        "from": "X74n1471_001:0360c11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳習起信等義。",
        "from": "X74n1471_001:0360c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念學無常師。",
        "from": "X74n1471_001:0360c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂人詢北禮。",
        "from": "X74n1471_001:0360c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法倣南求。",
        "from": "X74n1471_001:0360c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "負笈四方。",
        "from": "X74n1471_001:0360c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "撗經三學。",
        "from": "X74n1471_001:0360c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復趨賢首絳紗。",
        "from": "X74n1471_001:0360c12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偶師遷化。",
        "from": "X74n1471_001:0360c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師之門徒。",
        "from": "X74n1471_001:0360c13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖高甲宗一慧苑慧英等輩。",
        "from": "X74n1471_001:0360c13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "續其餘藁。",
        "from": "X74n1471_001:0360c14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆不繫先古。",
        "from": "X74n1471_001:0360c14_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清涼遽發慨然之歎。",
        "from": "X74n1471_001:0360c14_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故別製新經疏二十卷(或開為四十卷)。",
        "from": "X74n1471_001:0360c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨經演義鈔四十卷。",
        "from": "X74n1471_001:0360c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(或開六十卷或開八十卷)其疏鈔皆不違先師之意。",
        "from": "X74n1471_001:0360c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢首門生。",
        "from": "X74n1471_001:0360c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實若星繁。",
        "from": "X74n1471_001:0360c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白眉青眼。",
        "from": "X74n1471_001:0360c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拔萃超群。",
        "from": "X74n1471_001:0360c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道契緣符。",
        "from": "X74n1471_001:0360c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心傳口授。",
        "from": "X74n1471_001:0360c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獨清涼一蟾孤朗。",
        "from": "X74n1471_001:0360c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大宋高僧傳。",
        "from": "X74n1471_001:0360c18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義解科中。",
        "from": "X74n1471_001:0360c18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藏師傳末云。",
        "from": "X74n1471_001:0360c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華嚴一宗。",
        "from": "X74n1471_001:0360c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "付授澄觀(國師諱也)。",
        "from": "X74n1471_001:0360c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又師撰隨文手鑑一百卷。",
        "from": "X74n1471_001:0360c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華嚴綱要三卷。",
        "from": "X74n1471_001:0360c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法界玄鏡一卷。",
        "from": "X74n1471_001:0360c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鏡燈說文一卷。",
        "from": "X74n1471_001:0360c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三聖圓融觀一卷。",
        "from": "X74n1471_001:0360c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華嚴圓覺四分中觀等經律關脈三十餘部。",
        "from": "X74n1471_001:0360c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華嚴七處九會華藏界圖。",
        "from": "X74n1471_001:0360c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心鏡說文一卷。",
        "from": "X74n1471_001:0360c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大經了義備要三卷。",
        "from": "X74n1471_001:0360c22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐聖誕晨對御談經文。",
        "from": "X74n1471_001:0360c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼一家祖述。",
        "from": "X74n1471_001:0360c23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詩表牋章。",
        "from": "X74n1471_001:0360c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總八十餘卷。",
        "from": "X74n1471_001:0360c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斐相國云。",
        "from": "X74n1471_001:0360c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "古錦純金。",
        "from": "X74n1471_001:0360c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨器任用。",
        "from": "X74n1471_001:0360c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "良以投機授法。",
        "from": "X74n1471_001:0361a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "海應多波。",
        "from": "X74n1471_001:0361a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奧義雅談。",
        "from": "X74n1471_001:0361a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其歸一揆。",
        "from": "X74n1471_001:0361a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歷九宗聖世。",
        "from": "X74n1471_001:0361a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為七帝門師。",
        "from": "X74n1471_001:0361a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俗壽一百八。",
        "from": "X74n1471_001:0361a02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧臈八十三。",
        "from": "X74n1471_001:0361a02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形長九尺四寸。",
        "from": "X74n1471_001:0361a02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "垂手過膝。",
        "from": "X74n1471_001:0361a03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目夜發光。",
        "from": "X74n1471_001:0361a03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晝仍不瞬。",
        "from": "X74n1471_001:0361a03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言論清雅。",
        "from": "X74n1471_001:0361a03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "動止作則。",
        "from": "X74n1471_001:0361a04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "才供二筆。",
        "from": "X74n1471_001:0361a04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡著述見流傳者。",
        "from": "X74n1471_001:0361a04_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四百餘卷。",
        "from": "X74n1471_001:0361a04_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(云云)以國師地位高深。",
        "from": "X74n1471_001:0361a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解行幽邃。",
        "from": "X74n1471_001:0361a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "力扶大教。",
        "from": "X74n1471_001:0361a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智燭宏宗。",
        "from": "X74n1471_001:0361a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縱浩博淵微之學。",
        "from": "X74n1471_001:0361a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畢一代佛言者。",
        "from": "X74n1471_001:0361a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢首之後。",
        "from": "X74n1471_001:0361a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "襲有我師。",
        "from": "X74n1471_001:0361a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功歸清涼也。",
        "from": "X74n1471_001:0361a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每歲仲冬。",
        "from": "X74n1471_001:0361a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸方咸以賢首忌晨。",
        "from": "X74n1471_001:0361a07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "專致讚供。",
        "from": "X74n1471_001:0361a07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸祖附列而已。",
        "from": "X74n1471_001:0361a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嘻推功祖德清涼當克獨祭近得甌越。",
        "from": "X74n1471_001:0361a08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "的本傳神。",
        "from": "X74n1471_001:0361a09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宗密僧睿二法師。",
        "from": "X74n1471_001:0361a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拱侍香几。",
        "from": "X74n1471_001:0361a09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冰姿高朗。",
        "from": "X74n1471_001:0361a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風神超邁。",
        "from": "X74n1471_001:0361a10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "巍峩若存。",
        "from": "X74n1471_001:0361a10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覿者畏愛。",
        "from": "X74n1471_001:0361a10_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "特輪長利。",
        "from": "X74n1471_001:0361a10_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覓工傳寫。",
        "from": "X74n1471_001:0361a10_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "適三月初六日化旦。",
        "from": "X74n1471_001:0361a11_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聊陳薄禴。",
        "from": "X74n1471_001:0361a11_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雅多同學。",
        "from": "X74n1471_001:0361a11_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸扣余曰。",
        "from": "X74n1471_001:0361a11_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盛致餚饌。",
        "from": "X74n1471_001:0361a12_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "缺如讚章。",
        "from": "X74n1471_001:0361a12_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請從屬文。",
        "from": "X74n1471_001:0361a12_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貴參古式。",
        "from": "X74n1471_001:0361a12_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃不揆檮昧。",
        "from": "X74n1471_001:0361a12_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略伸讚詠。",
        "from": "X74n1471_001:0361a13_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳習華嚴疏鈔。",
        "from": "X74n1471_001:0361a13_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祖述清涼章句者。",
        "from": "X74n1471_001:0361a13_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "推功企恩。",
        "from": "X74n1471_001:0361a14_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "力行祭享。",
        "from": "X74n1471_001:0361a14_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不亦宜乎。",
        "from": "X74n1471_001:0361a14_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每位用義龍為稱者。",
        "from": "X74n1471_001:0361a14_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢首初見雲華儼師。",
        "from": "X74n1471_001:0361a15_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能設數問。",
        "from": "X74n1471_001:0361a15_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆出意表。",
        "from": "X74n1471_001:0361a15_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儼師嗟曰。",
        "from": "X74n1471_001:0361a15_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此來義龍輩。",
        "from": "X74n1471_001:0361a16_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尚罕扣斯端。",
        "from": "X74n1471_001:0361a16_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何計仁賢。",
        "from": "X74n1471_001:0361a16_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發明耳目。",
        "from": "X74n1471_001:0361a16_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又高僧慧榮自志曰。",
        "from": "X74n1471_001:0361a16_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "學習三十秊。",
        "from": "X74n1471_001:0361a17_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不號義龍。",
        "from": "X74n1471_001:0361a17_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誓不歸迹。",
        "from": "X74n1471_001:0361a17_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後果聲振彌遠。",
        "from": "X74n1471_001:0361a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又清涼五臺華嚴疏。",
        "from": "X74n1471_001:0361a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就夢身為龍。",
        "from": "X74n1471_001:0361a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭枕南臺。",
        "from": "X74n1471_001:0361a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尾蟠北臺。",
        "from": "X74n1471_001:0361a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化多分照而去。",
        "from": "X74n1471_001:0361a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流通之吉兆也。",
        "from": "X74n1471_001:0361a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又斐相國。",
        "from": "X74n1471_001:0361a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉勑作清涼妙覺塔銘。",
        "from": "X74n1471_001:0361a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "銘曰。",
        "from": "X74n1471_001:0361a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勑俾圖形。",
        "from": "X74n1471_001:0361a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相即無相。",
        "from": "X74n1471_001:0361a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "海印大龍。",
        "from": "X74n1471_001:0361a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蟠居方丈。",
        "from": "X74n1471_001:0361a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此以義龍稱耳。",
        "from": "X74n1471_001:0361a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若考清涼菩薩。",
        "from": "X74n1471_001:0361a21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功德難思。",
        "from": "X74n1471_001:0361a22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義龍何啻。",
        "from": "X74n1471_001:0361a22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初學無疑。",
        "from": "X74n1471_001:0361a22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(行者入道場敷具禮",
        "from": "X74n1471_001:0361a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三拜互跪秉爐唱云)。",
        "from": "X74n1471_001:0361a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恭以。",
        "from": "X74n1471_001:0361b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通方妙術。",
        "from": "X74n1471_001:0361b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終古沖規。",
        "from": "X74n1471_001:0361b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍七處九會。",
        "from": "X74n1471_001:0361b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以常宣。",
        "from": "X74n1471_001:0361b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具六相十玄。",
        "from": "X74n1471_001:0361b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而廣說。",
        "from": "X74n1471_001:0361b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義海之波瀾浩瀚。",
        "from": "X74n1471_001:0361b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一極唱高。",
        "from": "X74n1471_001:0361b02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教門之詮旨圓融。",
        "from": "X74n1471_001:0361b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二乘絕聽。",
        "from": "X74n1471_001:0361b03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毛分不移。",
        "from": "X74n1471_001:0361b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而撗該法界。",
        "from": "X74n1471_001:0361b03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剎那無動。",
        "from": "X74n1471_001:0361b03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而竪貫長時。",
        "from": "X74n1471_001:0361b04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竭性相之洪源。",
        "from": "X74n1471_001:0361b04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徹果因之坦道。",
        "from": "X74n1471_001:0361b04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "超眾典。",
        "from": "X74n1471_001:0361b04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比山推於峻嶽。",
        "from": "X74n1471_001:0361b05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝群經。",
        "from": "X74n1471_001:0361b05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶川注於窮溟。",
        "from": "X74n1471_001:0361b05_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朗金烏。",
        "from": "X74n1471_001:0361b05_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奪眾景之光。",
        "from": "X74n1471_001:0361b05_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皎玉兔。",
        "from": "X74n1471_001:0361b06_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掩餘星之彩。",
        "from": "X74n1471_001:0361b06_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幽深匹測。",
        "from": "X74n1471_001:0361b06_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祕密難思。",
        "from": "X74n1471_001:0361b06_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非下地之能知。",
        "from": "X74n1471_001:0361b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必上根而可入。",
        "from": "X74n1471_001:0361b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爰自龍宮選妙鳳闕翻靈。",
        "from": "X74n1471_001:0361b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雜華爛漫於西乾。",
        "from": "X74n1471_001:0361b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具葉芬芳於東夏。",
        "from": "X74n1471_001:0361b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無私駕說。",
        "from": "X74n1471_001:0361b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五天迓惠於真風。",
        "from": "X74n1471_001:0361b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不禦弘宣。",
        "from": "X74n1471_001:0361b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬國其[雨/旁]於法雨。",
        "from": "X74n1471_001:0361b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓明主伴。",
        "from": "X74n1471_001:0361b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "代不乏人。",
        "from": "X74n1471_001:0361b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應感聖凡。",
        "from": "X74n1471_001:0361b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時新後哲。",
        "from": "X74n1471_001:0361b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢首始歌于白雪。",
        "from": "X74n1471_001:0361b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清涼繼唱于陽春。",
        "from": "X74n1471_001:0361b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本雄詮。",
        "from": "X74n1471_001:0361b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開五教之蹊。",
        "from": "X74n1471_001:0361b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體大法。",
        "from": "X74n1471_001:0361b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "導十宗之徑。",
        "from": "X74n1471_001:0361b11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "示形禹族。",
        "from": "X74n1471_001:0361b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降迹唐朝。",
        "from": "X74n1471_001:0361b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慕花中瞻蔔之香。",
        "from": "X74n1471_001:0361b12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羨林內旃檀之貴。",
        "from": "X74n1471_001:0361b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剃除鬚髮。",
        "from": "X74n1471_001:0361b13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拌弃衣簪。",
        "from": "X74n1471_001:0361b13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曠然出三界之家。",
        "from": "X74n1471_001:0361b13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豁爾窹四生之夢。",
        "from": "X74n1471_001:0361b14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辯才天縱。",
        "from": "X74n1471_001:0361b14_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "學問生知。",
        "from": "X74n1471_001:0361b14_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為七帝之門師。",
        "from": "X74n1471_001:0361b14_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歷九宗之聖世。",
        "from": "X74n1471_001:0361b15_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "德雲靉靆。",
        "from": "X74n1471_001:0361b15_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名翼翩飜。",
        "from": "X74n1471_001:0361b15_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "綜緇侶以孤標。",
        "from": "X74n1471_001:0361b15_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "統僧倫而獨步。",
        "from": "X74n1471_001:0361b16_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化緣寂處。",
        "from": "X74n1471_001:0361b16_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將花殘柳謝之春。",
        "from": "X74n1471_001:0361b16_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能事畢時。",
        "from": "X74n1471_001:0361b16_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僅鸎囀鷰來之序。",
        "from": "X74n1471_001:0361b17_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諱晨旦偶祭享宜陳略聲巴俚之詞。",
        "from": "X74n1471_001:0361b17_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聊薦藻蘋之供。",
        "from": "X74n1471_001:0361b18_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薄酬法乳。",
        "from": "X74n1471_001:0361b18_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微報鴻恩。",
        "from": "X74n1471_001:0361b18_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "希道眼以洞明。",
        "from": "X74n1471_001:0361b18_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冀他心而昭格。",
        "from": "X74n1471_001:0361b19_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師資感應。",
        "from": "X74n1471_001:0361b19_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水月光臨。",
        "from": "X74n1471_001:0361b19_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鑒我虔誠。",
        "from": "X74n1471_001:0361b19_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乞垂迃降。",
        "from": "X74n1471_001:0361b20_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(禮一拜起互",
        "from": "X74n1471_001:0361b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "跪秉爐請云)。",
        "from": "X74n1471_001:0361b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請。",
        "from": "X74n1471_001:0361b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華嚴教海賢首義龍。",
        "from": "X74n1471_001:0361b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六祖師清涼尊者(三請三拜然後說偈)。",
        "from": "X74n1471_001:0361b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　稽首華嚴菩薩海　　普賢悲願久修心　四依德行妙通依　　五地功熏難勝地　三觀已深超解脫　　十身將證入圓融　分形普化寄唐朝　　說法遍霑生禹族　聲震寰中咸軌範　　名聞世表盡宗師　經龍律虎間生才　　僧統國師曇蕚瑞(至此陳白不然則修一疏宣之)。",
        "from": "X74n1471_001:0361c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心頂禮。",
        "from": "X74n1471_001:0361c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華嚴教海賢首義龍。",
        "from": "X74n1471_001:0361c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出胎分慶時身清涼尊者(從清涼字和之)。",
        "from": "X74n1471_001:0361c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　四依應跡生王族　　十月形成出聖胎　祥瑞希奇動眾心　　光明煥赫驚鄰室　照世星眸藏襁褓　　懸河海辯擁衣衾　善才法爾又童真　　性德重開珍寶藏故我一心歸命頂禮(下皆倣此)。",
        "from": "X74n1471_001:0361c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心頂禮。",
        "from": "X74n1471_001:0361c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華嚴教海賢首義龍。",
        "from": "X74n1471_001:0361c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嬉童入寺時身清涼尊者。",
        "from": "X74n1471_001:0361c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　福擬百童嚴塔像　　嬉遊眾藝聚泥沙　差無白馬詣青山　　悅有寶林逾雲嶺　掃灑六塵求解脫　　躋攀四智學菩提　一周歲曜發靈機　　三藏微言皆曉達一心頂禮。",
        "from": "X74n1471_001:0361c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華嚴教海賢首義龍。",
        "from": "X74n1471_001:0361c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉恩落[髟/采]時身清涼尊者。",
        "from": "X74n1471_001:0361c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　祝髮染緇除俗態　　奉恩遵禁入僧倫　金刀剪剪綠絲飄　　玉璽輝輝黃勑鎮　窣堵福超功德重　　周羅髮盡垢塵輕　形權心實妙難思　　便是德雲從定起一心頂禮。",
        "from": "X74n1471_001:0361c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華嚴教海賢首義龍。",
        "from": "X74n1471_001:0362a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "群經注演時身清涼尊者。",
        "from": "X74n1471_001:0362a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　調理筌[(ㄇ@(企止))/弟]遊智刃　　從容科節銳詞鋒　楞嚴圓覺洞能詮　　般若涅槃明所解　文錦江心垂玉釣　　義花浪底擲金鈎　洪濤本意擷鯨鰲　　十四輕罾方便設一心頂禮。",
        "from": "X74n1471_001:0362a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華嚴教海賢首義龍。",
        "from": "X74n1471_001:0362a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精傳諸論時身清涼尊者。",
        "from": "X74n1471_001:0362a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　宗釋覈(上革切)研咸秉志　　深探慧學不師心　十城封賞徤螢牕　　五日馳名工雪几　起信瑜伽親指授　　因明寶性力師承　縱橫九論鼓聲雄　　剛助肇公談四絕一心頂禮。",
        "from": "X74n1471_001:0362a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華嚴教海賢首義龍。",
        "from": "X74n1471_001:0362a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "優游觀法時身清涼尊者。",
        "from": "X74n1471_001:0362a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　導達大方闚佛眼　　澄渟衍行逈禪心　蓮花止觀去情遊　　法界色空亡相入　台嶺明明藏智地　　終山歷歷現心地　還源深寂洗塵勞　　忽證普光平等理一心頂禮。",
        "from": "X74n1471_001:0362a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華嚴教海賢首義龍。",
        "from": "X74n1471_001:0362a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓明眾戒時身清涼尊者。",
        "from": "X74n1471_001:0362a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　本期補特皆成佛　　纔淨尸羅福施生　聲聞軌轍究端倪　　菩薩車秉重控御　律悟開遮偏妙處　　戒登樓閣最高層　十弘要(平聲)誓緊浮囊　　三聚妍媸懸寶鑑一心頂禮。",
        "from": "X74n1471_001:0362a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華嚴教海賢首義龍。",
        "from": "X74n1471_001:0362b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無名師證時身清涼尊者。",
        "from": "X74n1471_001:0362b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　法界湛然超語迹　　宗門離念以心傳　教窮源本達枝流　　禪悟濬(私閏切)深參骨髓　虗寂觀中空寂趣　　無名師表離名求　洞明說默兩鎔融　　豈比筭沙徒自困一心頂禮。",
        "from": "X74n1471_001:0362b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華嚴教海賢首義龍。",
        "from": "X74n1471_001:0362b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞經能演時身清涼尊者。",
        "from": "X74n1471_001:0362b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　盡詫涅槃心易曉　　誰醒(平聲)差別智難明　不乘圓滿大因輪　　曷到汪洋真果海　無盡指歸觀掌上　　難思法術印胸中　再周能演動詵言　　法界洪規今在汝一心頂禮。",
        "from": "X74n1471_001:0362b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華嚴教海賢首義龍。",
        "from": "X74n1471_001:0362b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五峯祈請時身清涼尊者。",
        "from": "X74n1471_001:0362b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　聖智冥加祈寶積　　威神與力藉龍尊　十年麟筆貫花心　　五頂鳳牋標月志　真侶懇求敷創義　　法師開拓演新文　赤髭白足兩弘真　　瓶瀉雲興良可喻一心頂禮。",
        "from": "X74n1471_001:0362b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華嚴教誨賢首義龍。",
        "from": "X74n1471_001:0362b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通儒助釋時身清涼尊者。",
        "from": "X74n1471_001:0362b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　俗學無裨雖佛誡　　道儒會釋始通方　本操五教作權衡　　兼握九經為斗赤　道德逍遙莊老志　　詩書仁義孔顏心　四圍章句五明宗　　十翼百家咸顧習一心頂禮。",
        "from": "X74n1471_001:0362b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華嚴教海賢首義龍。",
        "from": "X74n1471_001:0362c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疏成感夢時身清涼尊者。",
        "from": "X74n1471_001:0362c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　法食慶文猶吐鳳　　傳芳吉兆夢成龍　輝空鱗鬣亘雙臺　　耀世光明逾皦日　奮迅侃梭昭變化　　分飛雷劒讖流通　百川入海味同醎　　製錦操刀功不淺一心頂禮。",
        "from": "X74n1471_001:0362c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華嚴教海賢首義龍。",
        "from": "X74n1471_001:0362c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "箋經普化時身清涼尊者。",
        "from": "X74n1471_001:0362c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　五色雲凝天上瑞　　八方人擁地中仙　九州慶溢扇慈風　　四海遐飛霑法雨　絳帳揮松傳百眾　　黌堂談柄烈千徒　名曹大手盡樞衣　　殉命奔趨求發藥一心頂禮。",
        "from": "X74n1471_001:0362c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華嚴教海賢首義龍。",
        "from": "X74n1471_001:0362c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "群英雜述時身清涼尊者。",
        "from": "X74n1471_001:0362c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　述義屬文遺輔相　　摛毫麗藻惠公卿　七家貴宦笥珠珍　　十寺名僧囊錦繡　二要五篇三聖觀　　諸關雙鏡九經圖　蟠根聳本雜花功　　布葉垂陰章句力一心頂禮。",
        "from": "X74n1471_001:0362c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華嚴教海賢首義龍。",
        "from": "X74n1471_001:0362c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "新經譯贊時身清涼尊者。",
        "from": "X74n1471_001:0362c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　增輝妙理因烏茶　　聖叡同翻預譯場　講贊何煩夢易(入聲)頭　　弘通不假仙呼藥　花開錦上重重媚　　雲起長空片片新　聖法高談悟帝心　　國師仍賜清涼號一心頂禮。",
        "from": "X74n1471_001:0362c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華嚴教海賢首義龍。",
        "from": "X74n1471_001:0363a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "談真歷遇時身清涼尊者。",
        "from": "X74n1471_001:0363a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　壽永九宗天子世　　德高七帝國王師　四方卓犖拜先生　　三教清英推首坐　葵藿心傾光俗諦　　桑榆日晚暢真猷　詩章牋表動嚴宸　　對御談經欣慶會一心頂禮。",
        "from": "X74n1471_001:0363a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華嚴教海賢首義龍。",
        "from": "X74n1471_001:0363a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "韜光順寂時身清涼尊者。",
        "from": "X74n1471_001:0363a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　緣盡四心同有滅　　韜光三月示無常　重臣縞素愴賢心　　皇帝輟朝悽聖意　石室葬龕蒙國禮　　興唐塑皃動天文　闍維舌固朵紅蓮　　舍利數千光色潤(至此復跪秉爐運想披陳五悔)。",
        "from": "X74n1471_001:0363a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普為四恩。",
        "from": "X74n1471_001:0363a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三有法界眾生。",
        "from": "X74n1471_001:0363a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉願斷除三障。",
        "from": "X74n1471_001:0363a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命懺悔(或運逆順十心於理無爽)。",
        "from": "X74n1471_001:0363a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至心懺悔",
        "from": "X74n1471_001:0363a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　忽忘廣大毗盧性　　妄躍無涯苦海津　波蕩千華本佛心　　漚成五蘊眾生相　十身妙好拘形殻　　十智光明墮惑情　一法纔迷萬法迷　　一塵暫起諸塵起　頓差三德成三毒　　豈達凡心即聖心　十剎常光瞽不窺　　十方遍說聾無聽　雖登毗奈如狂象　　已屬摩羅若啞羊　六相全虧朗智燈　　十玄鎮缺懸明鏡　百非四句增輕謗　　萬劫千生結重愆　我體真空觀慧門　　全修全性圓融懺懺悔已至心歸命三寶(下皆例此)。",
        "from": "X74n1471_001:0363a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至心勸請",
        "from": "X74n1471_001:0363b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　毛孔微塵容佛處　　剎那劫海說經時　鏡光帝網道場中　　願轉法輪窮後際至心隨喜　十方三世微塵佛　　八相圓成眾善根　六道諸乘行願因　　隨喜皆修功德海至心回向　願學普賢深信解　　圓修萬行等虗空　菩提實際與眾生　　回向湛然真法界至心發願　不捨千輪功德海　　常持十本大華嚴　普招群品剖塵經　　遍拉眾生刳識藏　在果在因能說法　　居凡居聖善修心　即於普眼見遮那　　不離普賢觀妙德　三聖圓融歸當念　　十身無礙應塵方　未來劫剎道場中　　等願法王稱長子自歸於佛。",
        "from": "X74n1471_001:0363b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當願眾生。",
        "from": "X74n1471_001:0363b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體解大道。",
        "from": "X74n1471_001:0363b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發無上意。",
        "from": "X74n1471_001:0363b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自歸於法。",
        "from": "X74n1471_001:0363c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當願眾生。",
        "from": "X74n1471_001:0363c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深入經藏。",
        "from": "X74n1471_001:0363c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智慧如海。",
        "from": "X74n1471_001:0363c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自歸於僧。",
        "from": "X74n1471_001:0363c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當願眾生。",
        "from": "X74n1471_001:0363c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "統理大眾。",
        "from": "X74n1471_001:0363c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切無礙。",
        "from": "X74n1471_001:0363c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華嚴清涼國師禮讚文一卷",
        "from": "X74n1471_001:0363c04_0",
        "context_with_next_sentence": false
    }
]