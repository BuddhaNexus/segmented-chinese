[
    {
        "source_sentence": "祈願文",
        "from": "T85n2847_001:1298c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即娑婆之界誓心利物故立方便之門□□□□□惠建八極安祥樂音競走[敲-高+壹]來迎梵□□□□前引意謂佃役難却妖分鬼魅遠千里□□□□於地戶總斯多善莫限良緣先用□□□□四王龍天八部伏願威光熾盛福力彌增興□□□□救人護國又持勝福次用莊嚴我河西節□。",
        "from": "T85n2847_001:1298c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使□□大王貴位。",
        "from": "T85n2847_001:1298c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願南□□□北極標尊長。",
        "from": "T85n2847_001:1298c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為菩薩之人王。",
        "from": "T85n2847_001:1298c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永應如來之付囑。",
        "from": "T85n2847_001:1298c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又持勝福次用莊嚴。",
        "from": "T85n2847_001:1298c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我河西都僧統和尚貴位。",
        "from": "T85n2847_001:1298c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願敫楊政述鎮遏□□色力堅於丘山。",
        "from": "T85n2847_001:1298c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惠命延於遐□□□勝福次用□□□指撝尚書貴位。",
        "from": "T85n2847_001:1298c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願金柯永茂玉葉時芳盤石增薰唯城作鎮。",
        "from": "T85n2847_001:1298c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又持勝福次用莊嚴。",
        "from": "T85n2847_001:1298c24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "閻都衙薰都衙諸都頭貴位。",
        "from": "T85n2847_001:1298c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願奇才出眾無藝召輪歸懷戍物之能共助明王之道。",
        "from": "T85n2847_001:1298c25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後天下定海內清。",
        "from": "T85n2847_001:1298c26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無聞征戰之名。",
        "from": "T85n2847_001:1298c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有賴擁為之□。",
        "from": "T85n2847_001:1298c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩訶般若利樂無邊。",
        "from": "T85n2847_001:1298c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大眾虔誠一切普誦。",
        "from": "T85n2847_001:1298c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□□□□　　　　　　　　道□□記",
        "from": "T85n2847_001:1298c29_0",
        "context_with_next_sentence": false
    }
]