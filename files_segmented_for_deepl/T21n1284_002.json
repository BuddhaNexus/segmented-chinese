[
    {
        "source_sentence": "佛說聖寶藏神儀軌經卷下",
        "from": "T21n1284_002:0352a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試鴻臚少卿",
        "from": "T21n1284_002:0352a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳教大師臣法天奉　詔譯",
        "from": "T21n1284_002:0352a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次曼拏羅內所用供獻之物。",
        "from": "T21n1284_002:0352a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須具二十件。",
        "from": "T21n1284_002:0352a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每日三時供獻。",
        "from": "T21n1284_002:0352a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辰時供物。",
        "from": "T21n1284_002:0352a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "午時酉時不得再用。",
        "from": "T21n1284_002:0352a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須別換新者。",
        "from": "T21n1284_002:0352a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦者力所不及。",
        "from": "T21n1284_002:0352a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "許納價直迴贖。",
        "from": "T21n1284_002:0352a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於第二時第三時再將供獻。",
        "from": "T21n1284_002:0352a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每日三時皆然。",
        "from": "T21n1284_002:0352a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直候得壇法。",
        "from": "T21n1284_002:0352a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求成就日。",
        "from": "T21n1284_002:0352a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速納前件一切價直。",
        "from": "T21n1284_002:0352a15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若供獻時。",
        "from": "T21n1284_002:0352a16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結幢印誦幢真言曰。",
        "from": "T21n1284_002:0352a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "囉怛曩(二合)婆捺囉(二合)曩帝覩瑟也(二合)迦羅娑特嚩(二合)惹必里(二合)焰特嚩(二合)惹布惹迦娑嚩(二合引)賀復說衣物儀則。",
        "from": "T21n1284_002:0352a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以無蟲匹帛造白衣二對。",
        "from": "T21n1284_002:0352a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於衣上畫金彩莊嚴塔相。",
        "from": "T21n1284_002:0352a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是言我今獻衣物。",
        "from": "T21n1284_002:0352a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以兩手大指各捻頭指屈如環。",
        "from": "T21n1284_002:0352a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩環相鉤。",
        "from": "T21n1284_002:0352a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘指磔開作衣物印。",
        "from": "T21n1284_002:0352a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復誦衣物真言七遍。",
        "from": "T21n1284_002:0352a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1284_002:0352a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "囉怛曩(二合)婆捺囉(二合)曩帝地里地里摩賀(引)三母捺覽(二合)麼囉馱娑嚩(二合引)賀復說扇儀則。",
        "from": "T21n1284_002:0352a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用無蟲匹帛作扇。",
        "from": "T21n1284_002:0352a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其扇白色制度合儀。",
        "from": "T21n1284_002:0352a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "扇柄端直以金寶莊畫二柄。",
        "from": "T21n1284_002:0352a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是言我今獻扇。",
        "from": "T21n1284_002:0352a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以左手屈小指於掌內。",
        "from": "T21n1284_002:0352a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪臂搖手作扇印。",
        "from": "T21n1284_002:0352a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復誦扇真言七遍真言曰。",
        "from": "T21n1284_002:0352b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "囉怛曩(二合)婆捺囉(二合)曩帝吠(引)覽婆嚩(引)喻馱(引)囉迦迦羅娑嚩(二合)賀復說拂儀則。",
        "from": "T21n1284_002:0352b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作白拂如法莊畫拂柄。",
        "from": "T21n1284_002:0352b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用拂真言加持七遍。",
        "from": "T21n1284_002:0352b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後於東北方安置曩婆夜叉王。",
        "from": "T21n1284_002:0352b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方安毘沙門。",
        "from": "T21n1284_002:0352b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西北方安摩賀曩婆。",
        "from": "T21n1284_002:0352b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次安阿嚕尼所(二合)大力夜叉王。",
        "from": "T21n1284_002:0352b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次安鉢捺摩護努護努迦。",
        "from": "T21n1284_002:0352b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及酥細拏贊捺嚩昝婆句謨吒迦囉訖哆[口*爾]麼摩護捺囉。",
        "from": "T21n1284_002:0352b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次安尾布羅左夜叉女。",
        "from": "T21n1284_002:0352b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此夜叉女最上端嚴。",
        "from": "T21n1284_002:0352b09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次安尾布覽　左　贊捺隣(二合引)輸摩呬摩呬誐挽(引)　左　酥路(引)左曩(引)　左　鉢納摩(二合)娑囉(引)　酥部嚕(二合)嚩尾　左　夜叉尼　摩賀(引)麼曳　酥枳細　左　嚩曩麼(引)羅(引)　左　必里(二合引)濕嚩(二合)梨　捺嚕(二合)摩麼黎迦　左　薩里嚩(二合)迦(引)摩頗羅鉢羅(二合)那(引)　及摩努(引)惹嚩(引)迦摩嚕閉　酥嚕(二合)哆嚩(引)吉也(二合)　酥[木*奈]里舍(二合)儞　摩護(引)誐囉(二合引)　唧(引)帝尾契也(二合)哆(引)等二十。",
        "from": "T21n1284_002:0352b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或一百依次第安置於外方位。",
        "from": "T21n1284_002:0352b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因陀羅等賢聖前。",
        "from": "T21n1284_002:0352b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作金剛網輪槍杖螺羂索幢三股叉。",
        "from": "T21n1284_002:0352b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及於東門安戟金剛焰等。",
        "from": "T21n1284_002:0352b20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是可及百種已上。",
        "from": "T21n1284_002:0352b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以摩羅帝花摩梨迦花。",
        "from": "T21n1284_002:0352b21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "裛五色粉。",
        "from": "T21n1284_002:0352b21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獻塗香燒沈香。",
        "from": "T21n1284_002:0352b21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦心真言請召寶藏神及諸賢聖。",
        "from": "T21n1284_002:0352b22_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦本真言請召夜叉女。",
        "from": "T21n1284_002:0352b23_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次獻花鬘摩羅帝花殊妙蓮花。",
        "from": "T21n1284_002:0352b23_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "優鉢羅花迦難麼花。",
        "from": "T21n1284_002:0352b24_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱魯嚩花及緊輸迦花。",
        "from": "T21n1284_002:0352b24_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切賢聖依次第獻。",
        "from": "T21n1284_002:0352b25_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以紫礦安悉香龍腦香白檀香同作護摩。",
        "from": "T21n1284_002:0352b25_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於寶藏賢聖前。",
        "from": "T21n1284_002:0352b26_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置種種花種種供養之物。",
        "from": "T21n1284_002:0352b27_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或聞大惡聲如得破壞。",
        "from": "T21n1284_002:0352b27_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是祥瑞不得恐怖。",
        "from": "T21n1284_002:0352b28_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求之事決定成就。",
        "from": "T21n1284_002:0352b28_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當得寶藏神與大富貴。",
        "from": "T21n1284_002:0352b29_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此則曼拏羅儀則。",
        "from": "T21n1284_002:0352b29_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及所誦真言。",
        "from": "T21n1284_002:0352c01_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)惹敢(二合)婆羅惹隣(引)捺羅(二合引)野娑嚩(二合引)賀　阿地鉢帝摩賀(引)夜叉囉(引)惹地摩呬(引)擔努(引)夜叉鉢囉(二合)冒(引)捺夜夜叉娑(引)尾怛里(二合引)唵(引)夜乞叉(二合)娑嚩(二合引)賀次心真言。",
        "from": "T21n1284_002:0352c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿南哆鉢捺摩(二合引)野娑嚩(二合引)賀　夜乞叉(二合)尾儞也(二合引)野娑嚩(二合引)賀次蓮華藏真言。",
        "from": "T21n1284_002:0352c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿喃哆商佉(引)野娑嚩(二合)賀次螺藏真言。",
        "from": "T21n1284_002:0352c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "囉惹哆曩(引)婆野馱曩(引)地鉢哆曳(引)娑嚩(二合引)賀　酥曩(引)婆(引)野馱曩(引)地鉢哆曳(引)娑嚩(二合引)賀　摩尼曩(引)婆(引)野馱曩(引)地鉢哆曳(引)娑嚩(二合引)賀　囉怛曩(二合)曩(引)婆(引)野馱曩(引)地鉢哆曳(引)娑嚩(二合引)賀　酥曩婆野馱曩地鉢哆曳(引)娑嚩(二合引)賀　暗沒曩(引)婆(引)野馱曩(引)地鉢哆曳(引)娑嚩(二合引)賀　儞羅曩(引)婆(引)野馱曩地鉢哆曳(引)娑嚩(二合引)賀　吠(引)室囉(二合)摩拏野馱曩地鉢哆曳(引)娑嚩(二合引)賀　摩賀(引)曩(引)婆(引)野馱曩(引)地鉢哆曳(引)娑嚩(二合引)賀　賀囉賀囉惹敢(二合)婆迦吽[口*癹]吒娑嚩(二合引)賀　惹敢(二合)婆迦野羼捺囉(二合引)曳(引)娑嚩(二合引)賀　護嚕護嚕喃(引)曩(引)酥史拏(引)野娑嚩(二合引)賀　賀囉賀囉贊捺囉(二合)細(引)曩野娑嚩(二合引)賀　惹敢(二合)婆曩(引)野娑嚩(二合引)賀　摩護(引)捺囉(引)野娑嚩(二合引)賀　尾摩羅尾摩羅捺里舍(二合)儞(引)娑嚩(二合)賀　尾摩羅尾摩隷(引)尾舍(引)羅曩野儞(引)冐馱野娑嚩(二合引)賀贊捺覽(二合引)輸摩儞數布即以(二合)帝數布即以(二合)帝(引)贊捺里(二合引)贊捺囉(二合)摩帝娑嚩(二合)賀　贊捺覽(二合引)輸摩帝娑嚩(二合引)賀　阿嚩(引)賀儞酥路(引)左儞也鉢捺彌(二合引)娑嚩(二合引)賀　鉢捺摩(二合)娑囉(引)酥部嚕(二合)摩努(引)囉彌(引)娑嚩(二合引)賀　酥部嚕(二合)酥部嚕(二合)嚩(引)護鉢囉(二合)怛也(二合)曳(引)娑嚩(二合引)賀　嚩呬尾(二合)摩隷波致謨(引)致娑嚩(二合引)賀　摩賀(引)彌(引)佉酥吉(引)舍摩(引)隷酥吉(引)舍摩(引)隷吉(引)舍嚩囉尼娑嚩(二合引)賀　曩嚩摩(引)隷儞娑嚩(二合引)賀(引)烏骨(二合)底瑟姹(二合)布室左(二合)隷(引)娑嚩(二合引)賀(引)　捺嚕(二合)摩麼(引)隷迦(引)頗隷捺嚕(二合)摩枳隷枳隷娑嚩(二合引)賀迦摩摩隷尾羅細儞娑嚩(二合)賀薩里嚩(二合)誐囉(二合)賀(引)摩努(引)惹味(引)嚩誐嚩帝娑嚩(二合引)賀(引)　迦(引)摩嚕閉婆(引)尾儞娑嚩(二合引)賀酥嚕(二合)哆嚩(引)吉也(二合)緊迦哩娑嚩(二合引)賀　酥捺里舍(二合)儞(引)迦(引)彌儞娑嚩(二合引)賀　摩護(引)誐里(二合引)婆野迦里娑嚩(二合引)賀　印捺囉(二合引)野禰(引)嚩(引)地鉢哆曳(引)娑嚩(二合引)賀　阿誐曩(二合)曳(引)賀尾也(二合)地鉢哆曳(引)娑嚩(二合引)賀　夜摩(引)野必里(二合引)哆地鉢哆曳(引)娑嚩(二合引)賀此等真言。",
        "from": "T21n1284_002:0352c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令一切天龍夜叉乾闥婆阿脩羅迦樓羅緊那羅摩睺羅伽必里哆毘舍左人非人等恭敬讚歎皆大歡喜。",
        "from": "T21n1284_002:0353a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此真言利益一切眾生如如意寶如彼賢瓶如彼劫樹。",
        "from": "T21n1284_002:0353a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸有所求皆得成就。",
        "from": "T21n1284_002:0353a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今說此真言之法令彼持誦。",
        "from": "T21n1284_002:0353a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若彼不種善根眾生暫聞此法。",
        "from": "T21n1284_002:0353a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼人永不墮於惡趣之難。",
        "from": "T21n1284_002:0353a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有種善根者眾生之類。",
        "from": "T21n1284_002:0353b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樂聞書寫受持讀誦。",
        "from": "T21n1284_002:0353b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所得功德如佛如來功德無異。",
        "from": "T21n1284_002:0353b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人受持心真言者。",
        "from": "T21n1284_002:0353b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人得入阿毘跋致。",
        "from": "T21n1284_002:0353b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不久當成阿耨多羅三藐三菩提。",
        "from": "T21n1284_002:0353b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永離惡道。",
        "from": "T21n1284_002:0353b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人不貧不老無病無苦。",
        "from": "T21n1284_002:0353b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種之事俱得圓滿。",
        "from": "T21n1284_002:0353b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復得一切有情愛樂尊重恭敬供養。",
        "from": "T21n1284_002:0353b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今略說持此真言所獲功德。",
        "from": "T21n1284_002:0353b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "假使一切賢聖及天龍夜叉乾闥婆阿脩羅迦樓羅緊那羅等。",
        "from": "T21n1284_002:0353b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能知此功德之量。",
        "from": "T21n1284_002:0353b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說不能盡。",
        "from": "T21n1284_002:0353b09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我等及彼諸佛如來應供正等正覺。",
        "from": "T21n1284_002:0353b09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此真言功德亦不能盡。",
        "from": "T21n1284_002:0353b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況菩薩摩訶薩聲聞辟支佛及諸天人。",
        "from": "T21n1284_002:0353b11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知此功德之量而說盡耶。",
        "from": "T21n1284_002:0353b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來一切智智功德無邊。",
        "from": "T21n1284_002:0353b12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大力寶藏神所有功德。",
        "from": "T21n1284_002:0353b13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦復如是。",
        "from": "T21n1284_002:0353b14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此真言王一切如來真實之心。",
        "from": "T21n1284_002:0353b14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能為一切世間主宰。",
        "from": "T21n1284_002:0353b15_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救諸眾生惡心者。",
        "from": "T21n1284_002:0353b15_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "調伏眾生苦惱者。",
        "from": "T21n1284_002:0353b15_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而能出生一切智慧。",
        "from": "T21n1284_002:0353b16_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若求寶藏神。",
        "from": "T21n1284_002:0353b16_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當於如來右邊畫神像。",
        "from": "T21n1284_002:0353b17_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白色手執拂子儀相圓滿。",
        "from": "T21n1284_002:0353b17_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神像右邊畫金剛手菩薩。",
        "from": "T21n1284_002:0353b18_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如青優鉢羅花色。",
        "from": "T21n1284_002:0353b18_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以真珠莊嚴儀相圓滿。",
        "from": "T21n1284_002:0353b19_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩前畫夜叉大悲寶賢子。",
        "from": "T21n1284_002:0353b19_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如清水色身相圓滿。",
        "from": "T21n1284_002:0353b20_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面帶善顏手執毘惹布羅果藏。",
        "from": "T21n1284_002:0353b20_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著黃衣服大頭冠破損座垂左足。",
        "from": "T21n1284_002:0353b21_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是誦人從頭至足。",
        "from": "T21n1284_002:0353b22_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一觀察儀軌具足。",
        "from": "T21n1284_002:0353b22_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是正觀經一洛叉時。",
        "from": "T21n1284_002:0353b23_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起心別作供養觀行。",
        "from": "T21n1284_002:0353b23_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想於水中所生諸花。",
        "from": "T21n1284_002:0353b24_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陸地諸花種種好香。",
        "from": "T21n1284_002:0353b24_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切珍寶殊妙衣服。",
        "from": "T21n1284_002:0353b25_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及飲食燈鬘等而用供獻。",
        "from": "T21n1284_002:0353b25_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得一切願圓滿。",
        "from": "T21n1284_002:0353b26_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是專心觀想已。",
        "from": "T21n1284_002:0353b26_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦人復自白言。",
        "from": "T21n1284_002:0353b27_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我依三寶儀則信受奉行。",
        "from": "T21n1284_002:0353b27_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後依天龍儀則作護摩法。",
        "from": "T21n1284_002:0353b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀想寶賢大將。",
        "from": "T21n1284_002:0353b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執持火來著護摩爐內。",
        "from": "T21n1284_002:0353b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿賢大將爐中燃火。",
        "from": "T21n1284_002:0353b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲財大將作清淨法。",
        "from": "T21n1284_002:0353c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多聞大將鋪吉祥草座。",
        "from": "T21n1284_002:0353c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來儀則觀想成就。",
        "from": "T21n1284_002:0353c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次想頭真言燃火。",
        "from": "T21n1284_002:0353c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口真言請召。",
        "from": "T21n1284_002:0353c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "足真言作擁護求於成就。",
        "from": "T21n1284_002:0353c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即作護摩滿。",
        "from": "T21n1284_002:0353c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手擲三度。",
        "from": "T21n1284_002:0353c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言曰。",
        "from": "T21n1284_002:0353c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿誐曩(二合)曳(引)娑嚩(二合引)賀即結吉祥等印。",
        "from": "T21n1284_002:0353c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手作合掌。",
        "from": "T21n1284_002:0353c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中間安置賢聖。",
        "from": "T21n1284_002:0353c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀想誦真言。",
        "from": "T21n1284_002:0353c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經須臾之間如風吹火。",
        "from": "T21n1284_002:0353c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀想前所說乳汁作護摩一洛叉先獻八大明王作護摩一洛叉。",
        "from": "T21n1284_002:0353c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次獻寶賢大明王。",
        "from": "T21n1284_002:0353c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別作護摩一洛叉得大富貴。",
        "from": "T21n1284_002:0353c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二洛叉得子孫興盛。",
        "from": "T21n1284_002:0353c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三洛叉有大罪之者。",
        "from": "T21n1284_002:0353c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦得成就之法。",
        "from": "T21n1284_002:0353c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是儀則專心恒作。",
        "from": "T21n1284_002:0353c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以水作護摩。",
        "from": "T21n1284_002:0353c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用花裛水。",
        "from": "T21n1284_002:0353c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以香水一千合作護摩獻寶藏神。",
        "from": "T21n1284_002:0353c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒令得財恒獲快樂。",
        "from": "T21n1284_002:0353c13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復作一千護摩得增長法。",
        "from": "T21n1284_002:0353c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依如是儀則。",
        "from": "T21n1284_002:0353c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前觀想作護摩滿。",
        "from": "T21n1284_002:0353c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獻三合水家得增長。",
        "from": "T21n1284_002:0353c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作護摩五洛叉。",
        "from": "T21n1284_002:0353c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令幀像隱身及令伏藏出現。",
        "from": "T21n1284_002:0353c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受用無盡獲最上快樂。",
        "from": "T21n1284_002:0353c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復作足真言口真言等。",
        "from": "T21n1284_002:0353c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令一切所求之事成就。",
        "from": "T21n1284_002:0353c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能禁怨家得過魔界永離深苦。",
        "from": "T21n1284_002:0353c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行住坐臥所受福德與天帝釋等。",
        "from": "T21n1284_002:0353c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其法如是。",
        "from": "T21n1284_002:0353c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說聖寶藏神儀軌經卷下",
        "from": "T21n1284_002:0353c21_0",
        "context_with_next_sentence": false
    }
]