[
    {
        "source_sentence": "佛說持句神呪經",
        "from": "T21n1351_001:0864c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吳月支優婆塞支謙譯",
        "from": "T21n1351_001:0864c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞如是。",
        "from": "T21n1351_001:0864c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在舍衛國祇樹給孤獨園。",
        "from": "T21n1351_001:0864c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大比丘眾千二百五十人菩薩萬人。",
        "from": "T21n1351_001:0864c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時從是佛土過億百千佛國。",
        "from": "T21n1351_001:0864c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有世界名無量華。",
        "from": "T21n1351_001:0864c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其佛號鄧尊王如來無所著至真等正覺。",
        "from": "T21n1351_001:0864c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今現在說法。",
        "from": "T21n1351_001:0864c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼佛遣二菩薩。",
        "from": "T21n1351_001:0864c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名無量光明二名大光明。",
        "from": "T21n1351_001:0864c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時二菩薩來至釋迦文佛所。",
        "from": "T21n1351_001:0864c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稽首佛足却住一面。",
        "from": "T21n1351_001:0864c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前白佛言。",
        "from": "T21n1351_001:0864c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯然世尊從是佛土過億百千佛國。",
        "from": "T21n1351_001:0864c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有世界名無量華。",
        "from": "T21n1351_001:0864c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其佛號鄧尊王如來。",
        "from": "T21n1351_001:0864c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遣我等來。",
        "from": "T21n1351_001:0864c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊如常不其眾安隱無他耶。",
        "from": "T21n1351_001:0864c20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天龍鬼神餓鬼女鬼鳩桓。",
        "from": "T21n1351_001:0864c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人非人虎狼毒獸。",
        "from": "T21n1351_001:0864c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得無嬈亂人民。",
        "from": "T21n1351_001:0864c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用是故佛遣是持句呪來。",
        "from": "T21n1351_001:0864c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "哀念眾生令安。",
        "from": "T21n1351_001:0864c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉善名聞威神得力。",
        "from": "T21n1351_001:0864c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是。",
        "from": "T21n1351_001:0864c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闍梨摩訶　闍梨羅尼　優佉目佉　沙波提　摩訶沙波提佛告阿難。",
        "from": "T21n1351_001:0864c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受是持句呪執持誦說。",
        "from": "T21n1351_001:0864c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛世難值。",
        "from": "T21n1351_001:0864c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持句呪亦為難遇。",
        "from": "T21n1351_001:0864c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子善女人。",
        "from": "T21n1351_001:0864c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受是持句呪。",
        "from": "T21n1351_001:0864c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諷誦讀識七世宿命。",
        "from": "T21n1351_001:0864c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持者一切浮陀鬼神若人非人。",
        "from": "T21n1351_001:0864c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得嬈近。",
        "from": "T21n1351_001:0865a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒蛇不敢螫。",
        "from": "T21n1351_001:0865a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒藥自然除。",
        "from": "T21n1351_001:0865a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀亦不能傷。",
        "from": "T21n1351_001:0865a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王亦不能害。",
        "from": "T21n1351_001:0865a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵亦不瞋之。",
        "from": "T21n1351_001:0865a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是持句呪。",
        "from": "T21n1351_001:0865a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七十七億佛所說。",
        "from": "T21n1351_001:0865a03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "犯是呪者當獲重罪。",
        "from": "T21n1351_001:0865a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有菩薩彌勒等八十人。",
        "from": "T21n1351_001:0865a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告賢者阿難。",
        "from": "T21n1351_001:0865a04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦當說持句呪。",
        "from": "T21n1351_001:0865a05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "哀念眾生令安。",
        "from": "T21n1351_001:0865a05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉善名聞威神得力。",
        "from": "T21n1351_001:0865a06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是。",
        "from": "T21n1351_001:0865a06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿知和知　吒佉羅羅　里彌喜利　彌利提盧　留彌勒佛告阿難。",
        "from": "T21n1351_001:0865a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受是持句呪執持誦說。",
        "from": "T21n1351_001:0865a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛世難值。",
        "from": "T21n1351_001:0865a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持句呪諷誦讀識。",
        "from": "T21n1351_001:0865a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十四世宿命。",
        "from": "T21n1351_001:0865a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持者一切浮陀鬼神。",
        "from": "T21n1351_001:0865a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人非人不得嬈近。",
        "from": "T21n1351_001:0865a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒蛇不敢螫。",
        "from": "T21n1351_001:0865a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒藥自然除。",
        "from": "T21n1351_001:0865a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀亦不能傷。",
        "from": "T21n1351_001:0865a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王亦不敢害。",
        "from": "T21n1351_001:0865a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵亦不瞋之。",
        "from": "T21n1351_001:0865a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是持句呪八十四億佛所說。",
        "from": "T21n1351_001:0865a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謗是呪者當獲重罪。",
        "from": "T21n1351_001:0865a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告阿難。",
        "from": "T21n1351_001:0865a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦當說持句呪。",
        "from": "T21n1351_001:0865a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "哀念眾生令安。",
        "from": "T21n1351_001:0865a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉善名聞威神得力。",
        "from": "T21n1351_001:0865a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是。",
        "from": "T21n1351_001:0865a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿知和知　那知鳩那知　提梨　沙羅波提　阿那波提　波那提　波那迦和尼　摩訶迦和尼佛告阿難。",
        "from": "T21n1351_001:0865a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受是持句呪執持誦說。",
        "from": "T21n1351_001:0865a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛世難值。",
        "from": "T21n1351_001:0865a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持句呪亦為難遇。",
        "from": "T21n1351_001:0865a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子善女人。",
        "from": "T21n1351_001:0865a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受是持句呪諷誦讀。",
        "from": "T21n1351_001:0865a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識無數世宿命。",
        "from": "T21n1351_001:0865a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持者一切浮陀鬼神。",
        "from": "T21n1351_001:0865a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人非人不敢嬈近。",
        "from": "T21n1351_001:0865a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒蛇不敢螫。",
        "from": "T21n1351_001:0865a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒藥自然除。",
        "from": "T21n1351_001:0865a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀亦不敢傷。",
        "from": "T21n1351_001:0865a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王亦不敢害。",
        "from": "T21n1351_001:0865a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵亦不瞋之。",
        "from": "T21n1351_001:0865a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是持句呪無數億佛所說。",
        "from": "T21n1351_001:0865a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "犯是呪者當獲重罪。",
        "from": "T21n1351_001:0865a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告阿難。",
        "from": "T21n1351_001:0865a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是持句呪。",
        "from": "T21n1351_001:0865a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行道中當念之。",
        "from": "T21n1351_001:0865a25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若至縣官若行賊中。",
        "from": "T21n1351_001:0865a25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若蠱道若毒若刀刃中。",
        "from": "T21n1351_001:0865a26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人非人中當念之。",
        "from": "T21n1351_001:0865a26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是持句呪於枯樹令生葉華實。",
        "from": "T21n1351_001:0865a27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況為人結縷也。",
        "from": "T21n1351_001:0865a28_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當使吉百病消除。",
        "from": "T21n1351_001:0865a28_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然安隱辟除凶害。",
        "from": "T21n1351_001:0865a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無佛令呪皆得從願。",
        "from": "T21n1351_001:0865a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說經已皆大歡喜。",
        "from": "T21n1351_001:0865b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說持句神呪經",
        "from": "T21n1351_001:0865b02_0",
        "context_with_next_sentence": false
    }
]