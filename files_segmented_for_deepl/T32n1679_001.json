        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無無相亦無老。",
        "from": "T32n1679_001:0758a14_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無無我無二執。",
        "from": "T32n1679_001:0758a14_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無無怨無戲論。",
        "from": "T32n1679_001:0758a14_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無無業無怖。",
        "from": "T32n1679_001:0758a15_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無施願無畏。",
        "from": "T32n1679_001:0758a15_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無法師子二足尊。",
        "from": "T32n1679_001:0758a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無調伏除煩惱。",
        "from": "T32n1679_001:0758a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無最上意清淨。",
