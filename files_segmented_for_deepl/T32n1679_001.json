        "from": "T32n1679_001:0758a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無應供。",
        "from": "T32n1679_001:0758a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無善逝。",
        "from": "T32n1679_001:0758a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無世尊。",
        "from": "T32n1679_001:0758a03_7",
