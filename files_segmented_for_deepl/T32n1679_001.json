        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無六根清淨眼。",
        "from": "T32n1679_001:0758a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無第一六神通。",
        "from": "T32n1679_001:0758a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無六趣海到彼岸。",
        "from": "T32n1679_001:0758a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無無師自然覺。",
        "from": "T32n1679_001:0758a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無善逝德成就。",
        "from": "T32n1679_001:0758a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無一切世間為愛樂。",
        "from": "T32n1679_001:0758a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無無等大智慧。",
