[
    {
        "source_sentence": "佛一百八名讚",
        "from": "T32n1679_001:0757c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試鴻臚",
        "from": "T32n1679_001:0757c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卿傳教大師臣法天奉　詔譯",
        "from": "T32n1679_001:0757c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　歸命一切智　　一切世間師",
        "from": "T32n1679_001:0757c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　牟尼大法王　　一百八名號",
        "from": "T32n1679_001:0757c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無邊功德海　　具足眾吉祥",
        "from": "T32n1679_001:0757c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　能滅諸有情　　罪業諸煩惱",
        "from": "T32n1679_001:0757c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　我今歸命禮　　一切大吉祥",
        "from": "T32n1679_001:0757c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　救度諸群生　　令得大安樂",
        "from": "T32n1679_001:0757c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　我今歸命禮　　悲愍二足尊",
        "from": "T32n1679_001:0757c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　圓滿於眾生　　一切吉祥事",
        "from": "T32n1679_001:0757c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　我今歸命禮　　無相無上尊",
        "from": "T32n1679_001:0757c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　成就天中天　　祕密大明義",
        "from": "T32n1679_001:0757c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如來正等覺　　利樂於世間",
        "from": "T32n1679_001:0757c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　最上百八名　　我今集彼說",
        "from": "T32n1679_001:0757c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無一切義成就。",
        "from": "T32n1679_001:0757c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無正等覺。",
        "from": "T32n1679_001:0757c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無一切智。",
        "from": "T32n1679_001:0757c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無大釋子。",
        "from": "T32n1679_001:0758a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無一切法自在無畏。",
