[
    {
        "source_sentence": "大宗地玄文本論卷第十一",
        "from": "T32n1669_011:0684c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬鳴菩薩造",
        "from": "T32n1669_011:0684c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真諦三藏譯",
        "from": "T32n1669_011:0684c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "◎[玨/覞]尸梵諾本王道路大決擇分第二十一如是已說[(王*昍)/(王*屾)][囗@日]陀尸梵迦諾本王本地大決擇分。",
        "from": "T32n1669_011:0684c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說[玨/覞]尸梵諾本王道路大決擇分。",
        "from": "T32n1669_011:0684c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_011:0684c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_011:0684c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　本王道路中　　有三種千變",
        "from": "T32n1669_011:0684c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　名如前說量　　各初變如次",
        "from": "T32n1669_011:0684c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　增百千萬億　　各後一切變",
        "from": "T32n1669_011:0684c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如次倍數轉　　住心應觀察",
        "from": "T32n1669_011:0684c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_011:0684c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就[玨/覞]尸梵諾本王道路分中。",
        "from": "T32n1669_011:0684c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則有三種千變修行。",
        "from": "T32n1669_011:0684c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其名次第同前所說。",
        "from": "T32n1669_011:0684c26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈本王道路中有三種千變名如前說量故。",
        "from": "T32n1669_011:0684c26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是三種。",
        "from": "T32n1669_011:0684c27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一變中如其次第。",
        "from": "T32n1669_011:0684c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增百億千億萬億數。",
        "from": "T32n1669_011:0684c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上下一量漸漸轉故。",
        "from": "T32n1669_011:0684c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈各初變如次增百千萬億故。",
        "from": "T32n1669_011:0684c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各後諸變如其次第增倍數轉。",
        "from": "T32n1669_011:0685a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應審思擇。",
        "from": "T32n1669_011:0685a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈各後一切變如次倍數轉住心應觀察故。",
        "from": "T32n1669_011:0685a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則修多羅中。",
        "from": "T32n1669_011:0685a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說。",
        "from": "T32n1669_011:0685a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來藏佛上下門中。",
        "from": "T32n1669_011:0685a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有三種修行。",
        "from": "T32n1669_011:0685a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數以千為量。",
        "from": "T32n1669_011:0685a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是三品。",
        "from": "T32n1669_011:0685a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以多億轉出生增長廣大周遍法門海藏。",
        "from": "T32n1669_011:0685a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至廣說故。",
        "from": "T32n1669_011:0685a06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩訶[玨/覞]尸梵諾母原主天王大決擇分第二十二如是已說[玨/覞]尸梵諾本王道路大決擇分。",
        "from": "T32n1669_011:0685a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說摩訶[玨/覞]尸梵諾母原主天王大決擇分。",
        "from": "T32n1669_011:0685a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_011:0685a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_011:0685a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　摩訶天王中　　有三種億變",
        "from": "T32n1669_011:0685a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　名次第如前　　各初變如次",
        "from": "T32n1669_011:0685a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一二三十方　　世界數量轉",
        "from": "T32n1669_011:0685a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　所餘一切變　　如次倍數轉",
        "from": "T32n1669_011:0685a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_011:0685a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就摩訶主天王體中。",
        "from": "T32n1669_011:0685a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則有三種億變修行。",
        "from": "T32n1669_011:0685a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名字次第同前所說。",
        "from": "T32n1669_011:0685a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈摩訶天王中有三種億變名次第如前故。",
        "from": "T32n1669_011:0685a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是三種。",
        "from": "T32n1669_011:0685a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一變中如其次第。",
        "from": "T32n1669_011:0685a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增一十方二十方三十方數轉。",
        "from": "T32n1669_011:0685a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈各初變如次一二三十方世界數量轉故。",
        "from": "T32n1669_011:0685a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘一切變如其次第倍數轉故如偈所餘一切變如次倍數轉故。",
        "from": "T32n1669_011:0685a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總字法轉大輪修多羅中。",
        "from": "T32n1669_011:0685a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說。",
        "from": "T32n1669_011:0685a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛陀摩訶本藏王地地中。",
        "from": "T32n1669_011:0685a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則有三行。",
        "from": "T32n1669_011:0685a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為三。",
        "from": "T32n1669_011:0685a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者下億轉行。",
        "from": "T32n1669_011:0685a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者中億轉行。",
        "from": "T32n1669_011:0685a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者上億轉行。",
        "from": "T32n1669_011:0685a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初行出生一大方界量下眷屬海。",
        "from": "T32n1669_011:0685a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中行出生二大方界量等俱轉海。",
        "from": "T32n1669_011:0685a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後行出生三大方界量上眷屬海。",
        "from": "T32n1669_011:0685a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至廣說故。",
        "from": "T32n1669_011:0685a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "◎大宗地玄文本論卷第十一",
        "from": "T32n1669_011:0685a27_5",
        "context_with_next_sentence": false
    }
]