[
    {
        "source_sentence": "文殊師利菩薩無相十禮",
        "from": "T85n2844_001:1296b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清涼山中大聖文殊師利菩薩。",
        "from": "T85n2844_001:1296b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端坐政看心。",
        "from": "T85n2844_001:1296b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心亦不可得。",
        "from": "T85n2844_001:1296b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至心歸命敬禮。",
        "from": "T85n2844_001:1296b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真如法身佛。",
        "from": "T85n2844_001:1296b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無色無形像。",
        "from": "T85n2844_001:1296b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無根無住處。",
        "from": "T85n2844_001:1296b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不生不滅故。",
        "from": "T85n2844_001:1296b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬禮無數觀。",
        "from": "T85n2844_001:1296b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不來亦不去。",
        "from": "T85n2844_001:1296b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不取亦不捨。",
        "from": "T85n2844_001:1296b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離六入故。",
        "from": "T85n2844_001:1296b12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出過於三界。",
        "from": "T85n2844_001:1296b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "等如虛空。",
        "from": "T85n2844_001:1296b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸慾不染故。",
        "from": "T85n2844_001:1296b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸威儀中。",
        "from": "T85n2844_001:1296b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "去來及睡悟。",
        "from": "T85n2844_001:1296b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常在三昧故。",
        "from": "T85n2844_001:1296b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "去來悉平等。",
        "from": "T85n2844_001:1296b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已住於平等。",
        "from": "T85n2844_001:1296b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不壞平等故。",
        "from": "T85n2844_001:1296b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入諸無相定。",
        "from": "T85n2844_001:1296b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見諸法寂淨。",
        "from": "T85n2844_001:1296b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離諸寂淨故。",
        "from": "T85n2844_001:1296b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛虛空相。",
        "from": "T85n2844_001:1296b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空亦無相。",
        "from": "T85n2844_001:1296b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離諸因果故。",
        "from": "T85n2844_001:1296b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空無中邊。",
        "from": "T85n2844_001:1296b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛身亦然。",
        "from": "T85n2844_001:1296b17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心同虛空故。",
        "from": "T85n2844_001:1296b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛常在世間。",
        "from": "T85n2844_001:1296b18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而不染勢法。",
        "from": "T85n2844_001:1296b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不分別世間故。",
        "from": "T85n2844_001:1296b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸法由如幻。",
        "from": "T85n2844_001:1296b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如幻不可得。",
        "from": "T85n2844_001:1296b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離諸幻法故。",
        "from": "T85n2844_001:1296b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切平等禮。",
        "from": "T85n2844_001:1296b20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無禮無不禮。",
        "from": "T85n2844_001:1296b20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一禮遍含識。",
        "from": "T85n2844_001:1296b20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同歸實相體。",
        "from": "T85n2844_001:1296b20_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普為四恩三有及法界眾生同如如。",
        "from": "T85n2844_001:1296b21_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命懺悔至心懺悔。",
        "from": "T85n2844_001:1296b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我於三時求罪性。",
        "from": "T85n2844_001:1296b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內外中間心實無。",
        "from": "T85n2844_001:1296b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已無心故諸法寂。",
        "from": "T85n2844_001:1296b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三毒四倒悉皆如。",
        "from": "T85n2844_001:1296b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懺悔已歸命禮法身如來至心勸請。",
        "from": "T85n2844_001:1296b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸法本不生。",
        "from": "T85n2844_001:1296b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已無生故何有滅。",
        "from": "T85n2844_001:1296b25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不生不滅性常住。",
        "from": "T85n2844_001:1296b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願諸佛莫涅槃。",
        "from": "T85n2844_001:1296b26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸眾生照本性。",
        "from": "T85n2844_001:1296b26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然遊戲涅槃城。",
        "from": "T85n2844_001:1296b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者但能照五蘊。",
        "from": "T85n2844_001:1296b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無我無人兩邊空。",
        "from": "T85n2844_001:1296b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勸請已歸命禮法身如來。",
        "from": "T85n2844_001:1296b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至心隨喜。",
        "from": "T85n2844_001:1296b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法本不貪亦不畏。",
        "from": "T85n2844_001:1296b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿我不一亦不異。",
        "from": "T85n2844_001:1296b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同觀一實證無生。",
        "from": "T85n2844_001:1296b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無緣等觀盡隨喜。",
        "from": "T85n2844_001:1296c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀空照有如如性。",
        "from": "T85n2844_001:1296c01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願眾生勤照心。",
        "from": "T85n2844_001:1296c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心體由來性清淨。",
        "from": "T85n2844_001:1296c02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妄色虛空同智真。",
        "from": "T85n2844_001:1296c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨喜已歸命禮法身如來至心迴向。",
        "from": "T85n2844_001:1296c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迷於一室隨明想。",
        "from": "T85n2844_001:1296c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執明想故我塵生。",
        "from": "T85n2844_001:1296c04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今照我塵無自性。",
        "from": "T85n2844_001:1296c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迴向無住涅槃城。",
        "from": "T85n2844_001:1296c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五法包含於政智。",
        "from": "T85n2844_001:1296c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八識清淨淨心王。",
        "from": "T85n2844_001:1296c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迴此蔭身成佛道。",
        "from": "T85n2844_001:1296c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四儀一向現前行。",
        "from": "T85n2844_001:1296c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迴向已歸命禮法身如來至心發願。",
        "from": "T85n2844_001:1296c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸眾生妨六賊。",
        "from": "T85n2844_001:1296c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悲智二照現前行。",
        "from": "T85n2844_001:1296c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不斷不常離無量。",
        "from": "T85n2844_001:1296c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非空非有惶了行。",
        "from": "T85n2844_001:1296c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四智三身緣彼體。",
        "from": "T85n2844_001:1296c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五眼常照浪三明。",
        "from": "T85n2844_001:1296c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三眾意生無障礙。",
        "from": "T85n2844_001:1296c10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩提樹下度群萠。",
        "from": "T85n2844_001:1296c11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發願已歸命禮法身如來一切恭敬。",
        "from": "T85n2844_001:1296c11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸佛德菩提。",
        "from": "T85n2844_001:1296c12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道心恒不退。",
        "from": "T85n2844_001:1296c12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願共諸眾生。",
        "from": "T85n2844_001:1296c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同入真如體。",
        "from": "T85n2844_001:1296c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸法薩般若。",
        "from": "T85n2844_001:1296c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得大總持門。",
        "from": "T85n2844_001:1296c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願共諸眾生。",
        "from": "T85n2844_001:1296c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同入真如海。",
        "from": "T85n2844_001:1296c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸僧息諍論。",
        "from": "T85n2844_001:1296c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同入和合海。",
        "from": "T85n2844_001:1296c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願諸眾生等。",
        "from": "T85n2844_001:1296c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉發菩提心。",
        "from": "T85n2844_001:1296c15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三業恒清淨。",
        "from": "T85n2844_001:1296c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和南眾法身佛。",
        "from": "T85n2844_001:1296c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸眾等聽說寅朝清淨偈。",
        "from": "T85n2844_001:1296c16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲求寂滅樂。",
        "from": "T85n2844_001:1296c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當學沙門法。",
        "from": "T85n2844_001:1296c17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣食之身命。",
        "from": "T85n2844_001:1296c17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精麁隨眾等。",
        "from": "T85n2844_001:1296c18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸眾等今日寅朝清淨上中下坐各記六念。",
        "from": "T85n2844_001:1296c18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸眾等聽說午時無常偈。",
        "from": "T85n2844_001:1296c19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人生不精進。",
        "from": "T85n2844_001:1296c19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□若樹無根。",
        "from": "T85n2844_001:1296c20_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "葉花至日終。",
        "from": "T85n2844_001:1296c20_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能得已時新。",
        "from": "T85n2844_001:1296c20_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "花亦不久鮮。",
        "from": "T85n2844_001:1296c21_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色亦非常好。",
        "from": "T85n2844_001:1296c21_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人命如剎那。",
        "from": "T85n2844_001:1296c21_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須臾難可報。",
        "from": "T85n2844_001:1296c22_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今勸諸眾等。",
        "from": "T85n2844_001:1296c22_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懃修無上道。",
        "from": "T85n2844_001:1296c22_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸眾等聽說黃昏無常偈。",
        "from": "T85n2844_001:1296c22_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此日已過命。",
        "from": "T85n2844_001:1296c23_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即隨減",
        "from": "T85n2844_001:1296c23_31",
        "context_with_next_sentence": false
    }
]