[
    {
        "source_sentence": "大宗地玄文本論卷第十二",
        "from": "T32n1669_012:0685b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬鳴菩薩造",
        "from": "T32n1669_012:0685b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真諦三藏譯",
        "from": "T32n1669_012:0685b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "◎一種功德純純無雜大圓滿地道路大決擇分第二十三如是已說摩訶[玨/覞]尸梵諾母原主天王大決擇分。",
        "from": "T32n1669_012:0685b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說一種功德純純無雜大圓滿地道路大決擇分。",
        "from": "T32n1669_012:0685b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_012:0685b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_012:0685b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　功德道路中　　有二千五百",
        "from": "T32n1669_012:0685b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　五十法門海　　五十一位中",
        "from": "T32n1669_012:0685b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一一皆各各　　具五十位故",
        "from": "T32n1669_012:0685b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　亦一一位中　　有性相本末",
        "from": "T32n1669_012:0685b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一萬二百數　　修多羅中說",
        "from": "T32n1669_012:0685b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　總一萬二千　　七百五十數",
        "from": "T32n1669_012:0685b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　取彼總本數　　如法應觀察",
        "from": "T32n1669_012:0685b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　以如是數量　　為道路分界",
        "from": "T32n1669_012:0685b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_012:0685b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就一種功德純純無雜大圓滿地道路分中。",
        "from": "T32n1669_012:0685b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總有二千五百五十法門大海。",
        "from": "T32n1669_012:0685b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甚深甚深廣大廣大。",
        "from": "T32n1669_012:0685b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈功德道路中有二千五百五十法門海故。",
        "from": "T32n1669_012:0685b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以何義故數如是。",
        "from": "T32n1669_012:0685b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成應可了。",
        "from": "T32n1669_012:0685b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知所謂五十一種位中。",
        "from": "T32n1669_012:0685b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一各各具五十故。",
        "from": "T32n1669_012:0685b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此義云何。",
        "from": "T32n1669_012:0685b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂五十信心五十念心。",
        "from": "T32n1669_012:0685b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至五十如來地各差別故。",
        "from": "T32n1669_012:0685b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈五十一位中一一皆各各具五十位故。",
        "from": "T32n1669_012:0685b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故亦一一位位各各具足性相本末之四法故。",
        "from": "T32n1669_012:0685b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一萬二百數成立而已。",
        "from": "T32n1669_012:0685b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是四事云何差別。",
        "from": "T32n1669_012:0685b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂如其次第不可思議說故。",
        "from": "T32n1669_012:0685b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明了現覺說故。",
        "from": "T32n1669_012:0685b29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能生長因說故。",
        "from": "T32n1669_012:0685b29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所生長果說故。",
        "from": "T32n1669_012:0685b29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈亦一一位中有性相本末一萬二百數故。",
        "from": "T32n1669_012:0685c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若爾此方說文云何通耶。",
        "from": "T32n1669_012:0685c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂金剛等地一行三昧修多羅中。",
        "from": "T32n1669_012:0685c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說。",
        "from": "T32n1669_012:0685c03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無雜無亂一一同同非惡非患吉祥地地軌則門中。",
        "from": "T32n1669_012:0685c03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總有一萬二千七百五十法門。",
        "from": "T32n1669_012:0685c05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼修多羅中如是說者。",
        "from": "T32n1669_012:0685c05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總別總故。",
        "from": "T32n1669_012:0685c06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈修多羅中說總一萬二千七百五十數取彼總本數如法應觀察故。",
        "from": "T32n1669_012:0685c06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此道路以之為量有別法門。",
        "from": "T32n1669_012:0685c08_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯以此數為其極量無別法門。",
        "from": "T32n1669_012:0685c08_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈以如是數量為道路分界故。",
        "from": "T32n1669_012:0685c09_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一種功德摩訶本地明白離惡品藏大決擇分第二十四如是已說一種功德純純無雜大圓滿地道路大決擇分。",
        "from": "T32n1669_012:0685c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說一種功德摩訶本地明白離惡品藏大決擇分。",
        "from": "T32n1669_012:0685c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_012:0685c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_012:0685c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　本地品藏中　　有一億三萬",
        "from": "T32n1669_012:0685c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　七千五百數　　廣大法門海",
        "from": "T32n1669_012:0685c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　前說諸位中　　一一皆各各",
        "from": "T32n1669_012:0685c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　攝別五十位　　法門如是廣",
        "from": "T32n1669_012:0685c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　性相本末四　　例此廣通達",
        "from": "T32n1669_012:0685c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_012:0685c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就一種功德摩訶本地明白離惡品藏中。",
        "from": "T32n1669_012:0685c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總有一億三萬七千五百數廣大法門海。",
        "from": "T32n1669_012:0685c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甚深甚深周遍周遍。",
        "from": "T32n1669_012:0685c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈本地品藏中有一億三萬七千五百數廣大法門海故。",
        "from": "T32n1669_012:0685c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以何義故數如是成。",
        "from": "T32n1669_012:0685c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前所說一切位中。",
        "from": "T32n1669_012:0685c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一各各攝差別轉五十位故。",
        "from": "T32n1669_012:0685c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法門之海如是廣大。",
        "from": "T32n1669_012:0685c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈前說諸位中一一皆各各攝別五十位法門如是廣故。",
        "from": "T32n1669_012:0685c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性相本末四種事中。",
        "from": "T32n1669_012:0685c28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以例配當。",
        "from": "T32n1669_012:0685c28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更復增數轉勝廣大。",
        "from": "T32n1669_012:0685c28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應審思擇如偈性相本末四例此廣通達故。",
        "from": "T32n1669_012:0685c29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大宗地玄文本論卷第十二",
        "from": "T32n1669_012:0686a02_0",
        "context_with_next_sentence": false
    }
]