[
    {
        "source_sentence": "佛治身經",
        "from": "T17n0795_001:0739c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "失譯人名今附西晉錄",
        "from": "T17n0795_001:0739c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言：「當學。",
        "from": "T17n0795_001:0739c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "工語不？",
        "from": "T17n0795_001:0739c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙門法行，獨一處坐，但當正意。",
        "from": "T17n0795_001:0739c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一處一臥一行不？",
        "from": "T17n0795_001:0740a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中正，一為曉身。",
        "from": "T17n0795_001:0740a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受經亦獨居。",
        "from": "T17n0795_001:0740a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還使千人一人勝，千軍中能得勝；一人能勝身，是為勝。",
        "from": "T17n0795_001:0740a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "軍中勝，勝勝者，不如勝餘勝；己勝身，常為勝。",
        "from": "T17n0795_001:0740a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無力亦不勝，摩汎藍亦不能己勝，不能作不勝，比丘勝乃不勝。",
        "from": "T17n0795_001:0740a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先為自身定正教，後教餘，為諸輩如我。",
        "from": "T17n0795_001:0740a05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自身上頭隨法行，便教餘人，如是易教。",
        "from": "T17n0795_001:0740a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "己身如是，如教他人，已得身教意，不復難教餘。",
        "from": "T17n0795_001:0740a07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲教餘，先自教，還使自教，從身教得黠。",
        "from": "T17n0795_001:0740a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為自身他故，多人亦莫犯。",
        "from": "T17n0795_001:0740a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有極自身，為自念無有極。",
        "from": "T17n0795_001:0740a10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自歸無有過身自歸，不如自歸諦。",
        "from": "T17n0795_001:0740a10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教身已黠，得歸已黠，便得法已黠，便得戒已黠。",
        "from": "T17n0795_001:0740a11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昆弟中不復憂已黠，便得樂已黠，不復憂自身，自歸身亦得歸他人。",
        "from": "T17n0795_001:0740a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "己身無有極，教一切從苦得脫。」",
        "from": "T17n0795_001:0740a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰：「何等為工語？」",
        "from": "T17n0795_001:0740a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師曰：「說三十七品經為工語；謂所說不亂人意，亦為工語。」",
        "from": "T17n0795_001:0740a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「何等為法行？」",
        "from": "T17n0795_001:0740a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「謂不離二百五十戒，是為法行。」",
        "from": "T17n0795_001:0740a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「何等為一為曉身？」",
        "from": "T17n0795_001:0740a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「謂知分別身中事。」",
        "from": "T17n0795_001:0740a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「何等為軍中人能得勝？」",
        "from": "T17n0795_001:0740a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「謂人不能勝惡。」",
        "from": "T17n0795_001:0740a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「何等為一人能勝身？」",
        "from": "T17n0795_001:0740a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「謂意念惡，能自制斷，是為勝身。」",
        "from": "T17n0795_001:0740a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「何等為身意教？」",
        "from": "T17n0795_001:0740a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「謂身受七戒，意受三戒。」",
        "from": "T17n0795_001:0740a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「何等為多人亦不犯？」",
        "from": "T17n0795_001:0740a21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "「謂等心使人不墮結。」",
        "from": "T17n0795_001:0740a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛治身經",
        "from": "T17n0795_001:0740a23_0",
        "context_with_next_sentence": false
    }
]