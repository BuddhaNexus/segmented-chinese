[
    {
        "source_sentence": "維摩詰所說經卷六",
        "from": "X19n0347_006:0561b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不思議品",
        "from": "X19n0347_006:0561b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時舍利弗。",
        "from": "X19n0347_006:0561b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見此室中。",
        "from": "X19n0347_006:0561b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有床座。",
        "from": "X19n0347_006:0561b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是念。",
        "from": "X19n0347_006:0561b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斯諸菩薩。",
        "from": "X19n0347_006:0561b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大弟子眾。",
        "from": "X19n0347_006:0561c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當於何坐。",
        "from": "X19n0347_006:0561c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長者維摩詰。",
        "from": "X19n0347_006:0561c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知其意。",
        "from": "X19n0347_006:0561c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "語舍利弗言。",
        "from": "X19n0347_006:0561c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何仁者。",
        "from": "X19n0347_006:0561c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為法來耶。",
        "from": "X19n0347_006:0561c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求床座耶。",
        "from": "X19n0347_006:0561c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利弗言。",
        "from": "X19n0347_006:0561c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為法來。",
        "from": "X19n0347_006:0561c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非為牀座。",
        "from": "X19n0347_006:0561c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "維摩詰言。",
        "from": "X19n0347_006:0561c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯舍利弗。",
        "from": "X19n0347_006:0561c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫求法者。",
        "from": "X19n0347_006:0561c04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不貪軀命。",
        "from": "X19n0347_006:0561c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況牀座。",
        "from": "X19n0347_006:0561c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫求法者非有色受想行識之求。",
        "from": "X19n0347_006:0561c04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非有界入之求。",
        "from": "X19n0347_006:0561c05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非有欲色無色之求(即無三界之求)。",
        "from": "X19n0347_006:0561c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯舍利弗。",
        "from": "X19n0347_006:0561c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫求法者。",
        "from": "X19n0347_006:0561c06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不著佛求。",
        "from": "X19n0347_006:0561c06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不著法求。",
        "from": "X19n0347_006:0561c06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不著眾求。",
        "from": "X19n0347_006:0561c07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫求法者。",
        "from": "X19n0347_006:0561c07_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無見苦求。",
        "from": "X19n0347_006:0561c07_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無斷集求。",
        "from": "X19n0347_006:0561c07_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無造盡證修道之求。",
        "from": "X19n0347_006:0561c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何。",
        "from": "X19n0347_006:0561c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法無戲論。",
        "from": "X19n0347_006:0561c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若言我當見苦斷集證滅修道。",
        "from": "X19n0347_006:0561c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是則戲論。",
        "from": "X19n0347_006:0561c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非求法也。",
        "from": "X19n0347_006:0561c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯舍利弗。",
        "from": "X19n0347_006:0561c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法名寂滅。",
        "from": "X19n0347_006:0561c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若行生滅。",
        "from": "X19n0347_006:0561c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是求生滅。",
        "from": "X19n0347_006:0561c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非求法也。",
        "from": "X19n0347_006:0561c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法名無染。",
        "from": "X19n0347_006:0561c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若染於法乃至涅槃。",
        "from": "X19n0347_006:0561c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是則染著。",
        "from": "X19n0347_006:0561c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非求法也。",
        "from": "X19n0347_006:0561c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法無行處。",
        "from": "X19n0347_006:0561c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若行於法。",
        "from": "X19n0347_006:0561c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是則行處。",
        "from": "X19n0347_006:0561c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非求法也。",
        "from": "X19n0347_006:0561c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法無取捨。",
        "from": "X19n0347_006:0562a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若取捨法。",
        "from": "X19n0347_006:0562a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是則取捨。",
        "from": "X19n0347_006:0562a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非求法也。",
        "from": "X19n0347_006:0562a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法無處所。",
        "from": "X19n0347_006:0562a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若著處所。",
        "from": "X19n0347_006:0562a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是則著處。",
        "from": "X19n0347_006:0562a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非求法也。",
        "from": "X19n0347_006:0562a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法名無相。",
        "from": "X19n0347_006:0562a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若隨相識。",
        "from": "X19n0347_006:0562a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是則求相。",
        "from": "X19n0347_006:0562a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非求法也。",
        "from": "X19n0347_006:0562a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法不可住。",
        "from": "X19n0347_006:0562a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若住於法。",
        "from": "X19n0347_006:0562a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是則住法。",
        "from": "X19n0347_006:0562a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非求法也。",
        "from": "X19n0347_006:0562a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法不可見聞覺知。",
        "from": "X19n0347_006:0562a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若行見聞覺知。",
        "from": "X19n0347_006:0562a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是則見聞覺知。",
        "from": "X19n0347_006:0562a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非求法也。",
        "from": "X19n0347_006:0562a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法名無為。",
        "from": "X19n0347_006:0562a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若行有為。",
        "from": "X19n0347_006:0562a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是求有為。",
        "from": "X19n0347_006:0562a06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非求法也。",
        "from": "X19n0347_006:0562a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故舍利弗。",
        "from": "X19n0347_006:0562a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若求法者。",
        "from": "X19n0347_006:0562a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切法。",
        "from": "X19n0347_006:0562a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應無所求。",
        "from": "X19n0347_006:0562a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是語時。",
        "from": "X19n0347_006:0562a07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五百天子。",
        "from": "X19n0347_006:0562a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸法中。",
        "from": "X19n0347_006:0562a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得法眼淨。",
        "from": "X19n0347_006:0562a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時長者維摩詰。",
        "from": "X19n0347_006:0562a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問文殊師利。",
        "from": "X19n0347_006:0562a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仁者遊於無量千萬億阿僧祇國。",
        "from": "X19n0347_006:0562a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等佛土。",
        "from": "X19n0347_006:0562a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有好上妙功德成就師子之座。",
        "from": "X19n0347_006:0562a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊師利言居士。",
        "from": "X19n0347_006:0562a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東方度三十六恒河沙國。",
        "from": "X19n0347_006:0562a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有世界名須彌相。",
        "from": "X19n0347_006:0562a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其佛號須彌燈王。",
        "from": "X19n0347_006:0562a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今現在。",
        "from": "X19n0347_006:0562a12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼佛身長八萬四千由旬。",
        "from": "X19n0347_006:0562a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其師子座。",
        "from": "X19n0347_006:0562a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高八萬四千由旬。",
        "from": "X19n0347_006:0562a13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚴飾第一。",
        "from": "X19n0347_006:0562a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是長者維摩詰。",
        "from": "X19n0347_006:0562a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現神通力。",
        "from": "X19n0347_006:0562a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即時彼佛。",
        "from": "X19n0347_006:0562a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遣三萬二千師子之座。",
        "from": "X19n0347_006:0562a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高廣嚴淨。",
        "from": "X19n0347_006:0562a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來入維摩詰室。",
        "from": "X19n0347_006:0562a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸菩薩大弟子。",
        "from": "X19n0347_006:0562a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋梵四天王等。",
        "from": "X19n0347_006:0562a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔所未見。",
        "from": "X19n0347_006:0562a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其室廣博。",
        "from": "X19n0347_006:0562a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆包容。",
        "from": "X19n0347_006:0562a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三萬二千師子座。",
        "from": "X19n0347_006:0562a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無所妨礙於毗耶離城。",
        "from": "X19n0347_006:0562a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及閻浮提四天下。",
        "from": "X19n0347_006:0562a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不迫迮。",
        "from": "X19n0347_006:0562a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉見如故。",
        "from": "X19n0347_006:0562a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時維摩詰。",
        "from": "X19n0347_006:0562a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "語文殊師利。",
        "from": "X19n0347_006:0562a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就師子座。",
        "from": "X19n0347_006:0562a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與諸菩薩上人俱坐。",
        "from": "X19n0347_006:0562a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當自立身如彼座像。",
        "from": "X19n0347_006:0562a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其得神通菩薩。",
        "from": "X19n0347_006:0562a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即自變形。",
        "from": "X19n0347_006:0562a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為四萬二千由旬。",
        "from": "X19n0347_006:0562a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐師子座。",
        "from": "X19n0347_006:0562a21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸新發意菩薩。",
        "from": "X19n0347_006:0562a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及大弟子。",
        "from": "X19n0347_006:0562a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆不能昇。",
        "from": "X19n0347_006:0562a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時維摩詰語舍利弗。",
        "from": "X19n0347_006:0562a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就師子座。",
        "from": "X19n0347_006:0562a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利弗言居士。",
        "from": "X19n0347_006:0562a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此座高廣。",
        "from": "X19n0347_006:0562a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾不能昇。",
        "from": "X19n0347_006:0562a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "維摩詰言唯。",
        "from": "X19n0347_006:0562a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利弗。",
        "from": "X19n0347_006:0562a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為須彌燈王如來作禮。",
        "from": "X19n0347_006:0562a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃可得坐。",
        "from": "X19n0347_006:0562b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是新發意菩薩。",
        "from": "X19n0347_006:0562b01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及大弟子。",
        "from": "X19n0347_006:0562b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即為須彌燈王如來作禮。",
        "from": "X19n0347_006:0562b02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便得坐師子座。",
        "from": "X19n0347_006:0562b02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利弗言居士。",
        "from": "X19n0347_006:0562b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未曾有也。",
        "from": "X19n0347_006:0562b03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是小室。",
        "from": "X19n0347_006:0562b03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃容受此高廣之座。",
        "from": "X19n0347_006:0562b03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於毗耶離城無所妨礙。",
        "from": "X19n0347_006:0562b04_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又於閻浮提。",
        "from": "X19n0347_006:0562b04_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聚落城邑。",
        "from": "X19n0347_006:0562b04_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及四天下。",
        "from": "X19n0347_006:0562b05_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天龍王鬼神宮殿。",
        "from": "X19n0347_006:0562b05_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不迫迮。",
        "from": "X19n0347_006:0562b05_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "維摩詰言。",
        "from": "X19n0347_006:0562b06_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯舍利弗。",
        "from": "X19n0347_006:0562b06_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛菩薩。",
        "from": "X19n0347_006:0562b06_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有解脫。",
        "from": "X19n0347_006:0562b06_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名不可思議。",
        "from": "X19n0347_006:0562b06_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若菩薩住是解脫者。",
        "from": "X19n0347_006:0562b07_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以須彌之高廣。",
        "from": "X19n0347_006:0562b07_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內芥子中。",
        "from": "X19n0347_006:0562b07_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無所增減。",
        "from": "X19n0347_006:0562b08_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須彌山王。",
        "from": "X19n0347_006:0562b08_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本相如故。",
        "from": "X19n0347_006:0562b08_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而四天王忉利諸天。",
        "from": "X19n0347_006:0562b08_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不覺不知己之所入。",
        "from": "X19n0347_006:0562b09_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯應度者。",
        "from": "X19n0347_006:0562b09_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃見須彌入芥子中。",
        "from": "X19n0347_006:0562b09_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名不可思議解脫法門。",
        "from": "X19n0347_006:0562b10_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以四大海水。",
        "from": "X19n0347_006:0562b10_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入一毛孔。",
        "from": "X19n0347_006:0562b11_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不嬈魚鼈黿鼉水性之屬。",
        "from": "X19n0347_006:0562b11_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而彼大海本性如故。",
        "from": "X19n0347_006:0562b12_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸龍鬼神阿修羅等。",
        "from": "X19n0347_006:0562b12_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不覺不知己之所入。",
        "from": "X19n0347_006:0562b12_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此眾生。",
        "from": "X19n0347_006:0562b13_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦無所嬈。",
        "from": "X19n0347_006:0562b13_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又舍利弗。",
        "from": "X19n0347_006:0562b13_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住不可思議解脫菩薩。",
        "from": "X19n0347_006:0562b14_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷取三千大千世界。",
        "from": "X19n0347_006:0562b14_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如陶家輪。",
        "from": "X19n0347_006:0562b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著右掌中。",
        "from": "X19n0347_006:0562b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擲過恒沙世界之外。",
        "from": "X19n0347_006:0562b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其中眾生。",
        "from": "X19n0347_006:0562b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不覺不知己之所往。",
        "from": "X19n0347_006:0562b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復還置本處。",
        "from": "X19n0347_006:0562b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "都不使人有往來想。",
        "from": "X19n0347_006:0562b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而此世界本相如故。",
        "from": "X19n0347_006:0562b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又舍利弗。",
        "from": "X19n0347_006:0562b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有眾生。",
        "from": "X19n0347_006:0562b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樂久住世而可度者。",
        "from": "X19n0347_006:0562b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩即演七日以為一劫。",
        "from": "X19n0347_006:0562b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼眾生謂之一劫。",
        "from": "X19n0347_006:0562b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有眾生。",
        "from": "X19n0347_006:0562b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不樂久住而可度者。",
        "from": "X19n0347_006:0562b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩即促一劫以為七日。",
        "from": "X19n0347_006:0562b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼眾生謂之七日。",
        "from": "X19n0347_006:0562b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又舍利弗。",
        "from": "X19n0347_006:0562b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住不可思議解脫菩薩。",
        "from": "X19n0347_006:0562b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以一切佛土嚴飾之事。",
        "from": "X19n0347_006:0562b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "集在一國。",
        "from": "X19n0347_006:0562b22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "示於眾生。",
        "from": "X19n0347_006:0562b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又菩薩。",
        "from": "X19n0347_006:0562b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以一切佛土眾生。",
        "from": "X19n0347_006:0562b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置之右掌。",
        "from": "X19n0347_006:0562b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飛到十方。",
        "from": "X19n0347_006:0562b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍示一切。",
        "from": "X19n0347_006:0562b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而不動本處。",
        "from": "X19n0347_006:0562b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又舍利弗。",
        "from": "X19n0347_006:0562b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方眾生。",
        "from": "X19n0347_006:0562b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養諸佛之具。",
        "from": "X19n0347_006:0562b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩於一毛孔。",
        "from": "X19n0347_006:0562c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆令得見。",
        "from": "X19n0347_006:0562c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又十方國土所有日月星宿。",
        "from": "X19n0347_006:0562c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一毛孔。",
        "from": "X19n0347_006:0562c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普使見之。",
        "from": "X19n0347_006:0562c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又舍利弗。",
        "from": "X19n0347_006:0562c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方世界所有諸風。",
        "from": "X19n0347_006:0562c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩悉能吸著口中。",
        "from": "X19n0347_006:0562c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而身不損。",
        "from": "X19n0347_006:0562c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外諸樹木。",
        "from": "X19n0347_006:0562c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不摧折。",
        "from": "X19n0347_006:0562c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又十方世界。",
        "from": "X19n0347_006:0562c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劫盡燒時。",
        "from": "X19n0347_006:0562c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以一切火內於腹中。",
        "from": "X19n0347_006:0562c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火事如故。",
        "from": "X19n0347_006:0562c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而不為害。",
        "from": "X19n0347_006:0562c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又於下方。",
        "from": "X19n0347_006:0562c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過恒河沙等諸佛世界。",
        "from": "X19n0347_006:0562c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取一佛土。",
        "from": "X19n0347_006:0562c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉著上方過恒河沙無數世界。",
        "from": "X19n0347_006:0562c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如持針鋒舉一棗葉。",
        "from": "X19n0347_006:0562c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而無所嬈。",
        "from": "X19n0347_006:0562c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又舍利弗。",
        "from": "X19n0347_006:0562c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住不可思議解脫菩薩。",
        "from": "X19n0347_006:0562c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能以神通現作佛身。",
        "from": "X19n0347_006:0562c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或現辟支佛身。",
        "from": "X19n0347_006:0562c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或現聲聞身。",
        "from": "X19n0347_006:0562c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或現帝釋身。",
        "from": "X19n0347_006:0562c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或現梵王身。",
        "from": "X19n0347_006:0562c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或現世主身。",
        "from": "X19n0347_006:0562c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或現轉輪聖王身。",
        "from": "X19n0347_006:0562c10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又十方世界。",
        "from": "X19n0347_006:0562c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有眾聲上中下音。",
        "from": "X19n0347_006:0562c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆能變之。",
        "from": "X19n0347_006:0562c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令作佛聲。",
        "from": "X19n0347_006:0562c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "演出無常苦空無我之音。",
        "from": "X19n0347_006:0562c12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及十方諸佛所說種種之法。",
        "from": "X19n0347_006:0563a01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆於其中普令得聞。",
        "from": "X19n0347_006:0563a01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利弗。",
        "from": "X19n0347_006:0563a02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今略說菩薩不可思議解脫之力。",
        "from": "X19n0347_006:0563a02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若廣說者。",
        "from": "X19n0347_006:0563a03_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "窮劫不盡。",
        "from": "X19n0347_006:0563a03_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時大迦葉。",
        "from": "X19n0347_006:0563a03_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞說菩薩不可思議解脫法門。",
        "from": "X19n0347_006:0563a03_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歎未曾有。",
        "from": "X19n0347_006:0563a04_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂舍利弗。",
        "from": "X19n0347_006:0563a04_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如有人。",
        "from": "X19n0347_006:0563a04_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於盲者前。",
        "from": "X19n0347_006:0563a05_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現眾色像。",
        "from": "X19n0347_006:0563a05_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非彼所見。",
        "from": "X19n0347_006:0563a05_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切聲聞。",
        "from": "X19n0347_006:0563a05_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞是不可思議解脫法門。",
        "from": "X19n0347_006:0563a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能解了。",
        "from": "X19n0347_006:0563a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為若此也。",
        "from": "X19n0347_006:0563a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智者聞是。",
        "from": "X19n0347_006:0563a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其誰不發阿耨多羅三藐三菩提心。",
        "from": "X19n0347_006:0563a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等何為永斷其根。",
        "from": "X19n0347_006:0563a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此大乘。",
        "from": "X19n0347_006:0563a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已如敗種。",
        "from": "X19n0347_006:0563a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切聲聞。",
        "from": "X19n0347_006:0563a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞是不可思議解脫法門。",
        "from": "X19n0347_006:0563a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆應號泣。",
        "from": "X19n0347_006:0563a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聲震三千大千世界。",
        "from": "X19n0347_006:0563a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切菩薩應大欣慶。",
        "from": "X19n0347_006:0563a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂受此法。",
        "from": "X19n0347_006:0563a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有菩薩。",
        "from": "X19n0347_006:0563a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信解不可思議解脫法門者。",
        "from": "X19n0347_006:0563a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切魔眾。",
        "from": "X19n0347_006:0563a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無如之何。",
        "from": "X19n0347_006:0563a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大迦葉說此語時。",
        "from": "X19n0347_006:0563a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三萬二千天子。",
        "from": "X19n0347_006:0563a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆發阿耨多羅三藐三菩提心。",
        "from": "X19n0347_006:0563a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時維摩詰語大迦葉。",
        "from": "X19n0347_006:0563a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仁者。",
        "from": "X19n0347_006:0563a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方無量阿僧祇世界中。",
        "from": "X19n0347_006:0563a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作魔王者。",
        "from": "X19n0347_006:0563a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多是住不可思議解脫菩薩。",
        "from": "X19n0347_006:0563a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以方便力故。",
        "from": "X19n0347_006:0563a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教化眾生。",
        "from": "X19n0347_006:0563a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現作魔王。",
        "from": "X19n0347_006:0563a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又迦葉。",
        "from": "X19n0347_006:0563a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方無量菩薩。",
        "from": "X19n0347_006:0563a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有人。",
        "from": "X19n0347_006:0563a16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從乞手足耳鼻。",
        "from": "X19n0347_006:0563a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭目髓腦。",
        "from": "X19n0347_006:0563a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "血肉皮骨。",
        "from": "X19n0347_006:0563a17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聚落城邑。",
        "from": "X19n0347_006:0563a17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妻子奴婢。",
        "from": "X19n0347_006:0563a18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "象馬車乘。",
        "from": "X19n0347_006:0563a18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金銀瑠璃。",
        "from": "X19n0347_006:0563a18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "硨磲碼碯。",
        "from": "X19n0347_006:0563a18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "珊瑚琥珀。",
        "from": "X19n0347_006:0563a18_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真珠珂貝。",
        "from": "X19n0347_006:0563a19_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣服飲食。",
        "from": "X19n0347_006:0563a19_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如此乞者。",
        "from": "X19n0347_006:0563a19_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多是住不可思議解脫菩薩。",
        "from": "X19n0347_006:0563a19_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以方便力。",
        "from": "X19n0347_006:0563a20_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而往試之。",
        "from": "X19n0347_006:0563a20_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令其堅固。",
        "from": "X19n0347_006:0563a20_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何。",
        "from": "X19n0347_006:0563a21_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住不可思議解脫菩薩。",
        "from": "X19n0347_006:0563a21_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有威德力。",
        "from": "X19n0347_006:0563a21_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故行逼迫。",
        "from": "X19n0347_006:0563a22_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "示諸眾生如是難事。",
        "from": "X19n0347_006:0563a22_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡夫下劣。",
        "from": "X19n0347_006:0563a22_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有力勢。",
        "from": "X19n0347_006:0563a22_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能如是逼迫菩薩。",
        "from": "X19n0347_006:0563a23_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如龍象蹴踏非驢所堪。",
        "from": "X19n0347_006:0563a23_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名住不可思議解脫菩薩智慧方便之門。",
        "from": "X19n0347_006:0563a24_37",
        "context_with_next_sentence": false
    }
]