[
    {
        "source_sentence": "轉法輪菩薩摧魔怨敵法",
        "from": "T20n1150_001:0609b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大興善寺三藏沙門大廣智",
        "from": "T20n1150_001:0609b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空奉　詔譯",
        "from": "T20n1150_001:0609b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時摧魔怨菩薩。",
        "from": "T20n1150_001:0609b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊。",
        "from": "T20n1150_001:0609b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸菩薩等各各已說祕密真言教法。",
        "from": "T20n1150_001:0609b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我今為欲未來末法之時。",
        "from": "T20n1150_001:0609b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護持國王及護國界諸有情故。",
        "from": "T20n1150_001:0609b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說祕蜜祕勝最密中祕密摧魔怨敵法。",
        "from": "T20n1150_001:0609b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊哀愍聽許。",
        "from": "T20n1150_001:0609b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言便說。",
        "from": "T20n1150_001:0609b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊若有隣國擾侵國界。",
        "from": "T20n1150_001:0609b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或自國內軍眾寡少。",
        "from": "T20n1150_001:0609b12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或復怯弱。",
        "from": "T20n1150_001:0609b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有不臣起惡叛逆。",
        "from": "T20n1150_001:0609b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即應取苦揀木而作一幢。",
        "from": "T20n1150_001:0609b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長十二指周圓八指削令極圓。",
        "from": "T20n1150_001:0609b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如世尊所勅。",
        "from": "T20n1150_001:0609b14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切天龍八部。",
        "from": "T20n1150_001:0609b14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令護諸國護諸國王。",
        "from": "T20n1150_001:0609b15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及護國界一切有情。",
        "from": "T20n1150_001:0609b15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為除災禍令得安樂。",
        "from": "T20n1150_001:0609b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若難起時。",
        "from": "T20n1150_001:0609b16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆圖畫彼護國土神於幢上。",
        "from": "T20n1150_001:0609b16_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如此大唐護國土者。",
        "from": "T20n1150_001:0609b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂毘首羯磨藥叉。",
        "from": "T20n1150_001:0609b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劫比羅藥叉。",
        "from": "T20n1150_001:0609b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法護藥叉。",
        "from": "T20n1150_001:0609b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "肩目藥叉。",
        "from": "T20n1150_001:0609b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣目藥叉。",
        "from": "T20n1150_001:0609b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護軍藥叉。",
        "from": "T20n1150_001:0609b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "珠賢藥叉。",
        "from": "T20n1150_001:0609b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿賢藥叉。",
        "from": "T20n1150_001:0609b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持明藥叉。",
        "from": "T20n1150_001:0609b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿吒縛俱藥叉(已上十大藥叉)嚩蘇枳龍王。",
        "from": "T20n1150_001:0609b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘇摩那龍王。",
        "from": "T20n1150_001:0609b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "補沙毘摩大龍王(已上三大龍王)訶利帝大天后。",
        "from": "T20n1150_001:0609b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翳囉嚩蹉大天后。",
        "from": "T20n1150_001:0609b23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雙目大天后(已上三大天后)各有五千神將以為眷屬。",
        "from": "T20n1150_001:0609b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所應畫者皆畫本形。",
        "from": "T20n1150_001:0609b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執持幖幟圖於幢上。",
        "from": "T20n1150_001:0609b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又於幢頂上書無礙王十字佛頂真言。",
        "from": "T20n1150_001:0609b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "末後吒字在輪臍中。",
        "from": "T20n1150_001:0609b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有怨敵叛逆主帥。",
        "from": "T20n1150_001:0609b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當書彼名於輪輻間。",
        "from": "T20n1150_001:0609b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於幢底圖畫一輪。",
        "from": "T20n1150_001:0609b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足八輻。",
        "from": "T20n1150_001:0609b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如上吒字置輪臍中。",
        "from": "T20n1150_001:0609b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若災難起時。",
        "from": "T20n1150_001:0609b29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應請一解法之人。",
        "from": "T20n1150_001:0609b29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此教中曾稟受者。",
        "from": "T20n1150_001:0609c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其中夜圖一方壇。",
        "from": "T20n1150_001:0609c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於方壇上重畫三角壇。",
        "from": "T20n1150_001:0609c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立於此幢在壇中央。",
        "from": "T20n1150_001:0609c02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於壇四面著種種食飲并二閼伽。",
        "from": "T20n1150_001:0609c03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒安悉香及然酥燈。",
        "from": "T20n1150_001:0609c03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其解法者面南而坐。",
        "from": "T20n1150_001:0609c04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結一切如來鉤印　以二手內相叉作拳。",
        "from": "T20n1150_001:0609c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右頭指為鉤。",
        "from": "T20n1150_001:0609c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及誦真言稱名。",
        "from": "T20n1150_001:0609c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請召十六大護及其眷屬大威德諸神將等壇中供養。",
        "from": "T20n1150_001:0609c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來鉤真言曰。",
        "from": "T20n1150_001:0609c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩　莫　薩　滿　多　沒　馱　喃　噁　娑　嚩　哆羅(二合引)鉢囉(二合)　底　賀　哆　怛　佗(引)　蘖　擔　俱舍　冐　地　左　哩野　播　哩　布　囉　迦　婆嚩(二合)賀(引)行者次當入三世勝金剛忿怒三摩地。",
        "from": "T20n1150_001:0609c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀威德熾盛四面八臂。",
        "from": "T20n1150_001:0609c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左蹈塢摩天后。",
        "from": "T20n1150_001:0609c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右脚蹈摩醯首羅天。",
        "from": "T20n1150_001:0609c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則結三世勝印　以二手金剛拳。",
        "from": "T20n1150_001:0609c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "檀慧互相鉤。",
        "from": "T20n1150_001:0609c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直竪於進力。",
        "from": "T20n1150_001:0609c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成已誦真言三五或七遍。",
        "from": "T20n1150_001:0609c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1150_001:0609c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　遜　婆　儞　遜　婆　吽　發　吒　吃哩(二合)　賀拏(二合)吃哩(二合)　賀拏(二合)　吽　發　吒　吃哩(二合)賀拏(二合)　跛　野　吽　發　吒　阿(引)　娜　野　斛(引)婆　誐　鑁　嚩　日囉(二合)　吽　發　吒復次即結上方諸天教勅印　以二手各作金剛拳。",
        "from": "T20n1150_001:0609c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指互相旋繞。",
        "from": "T20n1150_001:0610a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結印成已而教勅言。",
        "from": "T20n1150_001:0610a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等十六大護大藥叉將等。",
        "from": "T20n1150_001:0610a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受佛付囑護持國王。",
        "from": "T20n1150_001:0610a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及護國界一切有情。",
        "from": "T20n1150_001:0610a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令得安樂辟除災禍。",
        "from": "T20n1150_001:0610a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今我國境有某難事。",
        "from": "T20n1150_001:0610a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等大護助我軍陣摧破怨敵。",
        "from": "T20n1150_001:0610a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等當令彼軍被縛迷悶。",
        "from": "T20n1150_001:0610a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令某荒亂。",
        "from": "T20n1150_001:0610a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使我軍兵強盛得勝。",
        "from": "T20n1150_001:0610a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是勅已即誦真言三遍曰(已下召請諸天並同此結本印)。",
        "from": "T20n1150_001:0610a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吽　跛　里(入)　底　娜嚩(二合)　里(入)　多(入)　嚩　日囉(二合)羯　灑　唅　若復次當結遊虛空諸天教勅印　以二手金剛縛。",
        "from": "T20n1150_001:0610a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指以甲相背。",
        "from": "T20n1150_001:0610a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並竪二空押於二風。",
        "from": "T20n1150_001:0610a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結印成已。",
        "from": "T20n1150_001:0610a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一依前文而教。",
        "from": "T20n1150_001:0610a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勅已。",
        "from": "T20n1150_001:0610a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即三遍誦真言曰。",
        "from": "T20n1150_001:0610a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　嚩　日囉(二合)　蘖囉(二合)　跛　拏　野　嚩　日囉(二合)吽復次當結住虛空諸天教勅印　如降三世印。",
        "from": "T20n1150_001:0610a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二食相繞。",
        "from": "T20n1150_001:0610a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印安於頂上即是結印已。",
        "from": "T20n1150_001:0610a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一依前文而教勅言。",
        "from": "T20n1150_001:0610a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦真言三遍曰。",
        "from": "T20n1150_001:0610a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　嚩　日囉(二合)　摩　囉　吃囉(二合)　鑁復次當結地居諸天教勅印　以二手金剛縛。",
        "from": "T20n1150_001:0610a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舒二風頭相合。",
        "from": "T20n1150_001:0610a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如金剛護菩薩印即是。",
        "from": "T20n1150_001:0610a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結印已。",
        "from": "T20n1150_001:0610a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一依前文而教勅言。",
        "from": "T20n1150_001:0610a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即三遍誦真言曰。",
        "from": "T20n1150_001:0610a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　嚩　日囉(二合)　滿　馱　憾次結地底諸天教勅印　以二手各展令掌向外。",
        "from": "T20n1150_001:0610a30_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二空頭相捻。",
        "from": "T20n1150_001:0610b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二風相鉤。",
        "from": "T20n1150_001:0610b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二火以頭相拄。",
        "from": "T20n1150_001:0610b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印安於眉間。",
        "from": "T20n1150_001:0610b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一依前文而教勅言。",
        "from": "T20n1150_001:0610b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即三遍誦真言曰。",
        "from": "T20n1150_001:0610b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　賀　嚕　迦　嚩　日囉(二合)　娑　摩　野　娑　嚩努　瑟吒(二合)　娑　麼　野　母　捺囉(二合)　鉢囉(二合)憾　惹　迦　吽　發　吒既啟告已。",
        "from": "T20n1150_001:0610b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則結慧手作金剛拳。",
        "from": "T20n1150_001:0610b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "按於幢上誦十字佛頂真言一百八遍。",
        "from": "T20n1150_001:0610b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲誦此十字佛頂真言。",
        "from": "T20n1150_001:0610b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先應結印誦之七遍　印以二手內相叉為拳。",
        "from": "T20n1150_001:0610b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二空入於掌中。",
        "from": "T20n1150_001:0610b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二風各屈拄二空背即是。",
        "from": "T20n1150_001:0610b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言曰。",
        "from": "T20n1150_001:0610b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　嚩　日囉(二合)　薩　怛舞(二合)　瑟抳(二合)　灑　吽發　吒念誦已即更焚香獻花。",
        "from": "T20n1150_001:0610b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即捧閼伽奉。",
        "from": "T20n1150_001:0610b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為帝王并及百姓。",
        "from": "T20n1150_001:0610b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發廣大利益安樂殊勝之願。",
        "from": "T20n1150_001:0610b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結金剛縛竪合二火。",
        "from": "T20n1150_001:0610b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成。",
        "from": "T20n1150_001:0610b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦金剛解脫真言三遍。",
        "from": "T20n1150_001:0610b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "送聖眾各還本宮。",
        "from": "T20n1150_001:0610b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1150_001:0610b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　嚩　日囉(二合)　穆",
        "from": "T20n1150_001:0610b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法事已訖。",
        "from": "T20n1150_001:0610b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則收幢於淨函或篋安置。",
        "from": "T20n1150_001:0610b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養食飲等。",
        "from": "T20n1150_001:0610b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依法送往河流之中。",
        "from": "T20n1150_001:0610b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摧魔怨菩薩破他陳敵法",
        "from": "T20n1150_001:0610b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　　　　發軍時節依宿曜日量",
        "from": "T20n1150_001:0610b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貞享三年九月十九日一校正了",
        "from": "T20n1150_001:0610c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　　河南延命沙門淨嚴(四十有八)",
        "from": "T20n1150_001:0610c02_0",
        "context_with_next_sentence": false
    }
]