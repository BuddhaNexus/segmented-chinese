[
    {
        "source_sentence": "大般涅槃經集解卷第十四",
        "from": "T37n1763_014:0435a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　雜廣解脫義　釋一等四執義",
        "from": "T37n1763_014:0435a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四相品之第四",
        "from": "T37n1763_014:0435a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又解脫者名曰甚深(至)真解脫者即是如來。",
        "from": "T37n1763_014:0435a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明駿案。",
        "from": "T37n1763_014:0435a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前已廣上八味竟也。",
        "from": "T37n1763_014:0435a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此下更舉別義。",
        "from": "T37n1763_014:0435a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是二乘所不能入者也。",
        "from": "T37n1763_014:0435a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此總歎甚深不可見也。",
        "from": "T37n1763_014:0435a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又解脫者名無舍宅(至)真解脫者即是如來。",
        "from": "T37n1763_014:0435a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明駿案。",
        "from": "T37n1763_014:0435a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雜舉眾德。",
        "from": "T37n1763_014:0435a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨所樂入而解釋也。",
        "from": "T37n1763_014:0435a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又解脫者名斷一切(至)無漏善法斷塞諸道。",
        "from": "T37n1763_014:0435a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_014:0435a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_014:0435a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷邪見諸道也。",
        "from": "T37n1763_014:0435a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂若我無我非我非無我。",
        "from": "T37n1763_014:0435a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_014:0435a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_014:0435a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我者常見。",
        "from": "T37n1763_014:0435a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無我者斷見也。",
        "from": "T37n1763_014:0435a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非我非無我者。",
        "from": "T37n1763_014:0435a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此第四家計也。",
        "from": "T37n1763_014:0435a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闕第三家。",
        "from": "T37n1763_014:0435a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三家云。",
        "from": "T37n1763_014:0435a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦我則不同第一家。",
        "from": "T37n1763_014:0435a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦非我復不同第二家。",
        "from": "T37n1763_014:0435a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所言不同者。",
        "from": "T37n1763_014:0435a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂不全同。",
        "from": "T37n1763_014:0435a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃片同耳。",
        "from": "T37n1763_014:0435a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今第四家。",
        "from": "T37n1763_014:0435a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復異第三家也。",
        "from": "T37n1763_014:0435a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言非我者。",
        "from": "T37n1763_014:0435a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不同第三家斥我也。",
        "from": "T37n1763_014:0435a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言非無我者。",
        "from": "T37n1763_014:0435a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不同第三家斥無我也。",
        "from": "T37n1763_014:0435a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖有此言。",
        "from": "T37n1763_014:0435a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及其撿覈。",
        "from": "T37n1763_014:0435a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不得異第三家之也。",
        "from": "T37n1763_014:0435a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯斷取著不斷我見(至)真解脫者即是如來。",
        "from": "T37n1763_014:0435a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_014:0435a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_014:0435a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生死非我橫生我心。",
        "from": "T37n1763_014:0435a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是取著應斷也。",
        "from": "T37n1763_014:0435a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛性是理。",
        "from": "T37n1763_014:0435a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不斷此也。",
        "from": "T37n1763_014:0435a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又解脫者名不空空。",
        "from": "T37n1763_014:0435a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_014:0435a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_014:0435a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常樂我淨是一實。",
        "from": "T37n1763_014:0435a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又解脫眾苦是二實。",
        "from": "T37n1763_014:0435a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名不空空。",
        "from": "T37n1763_014:0435a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空空者名無所有(至)實無解脫故名空空。",
        "from": "T37n1763_014:0435a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_014:0435a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_014:0435b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尼揵所計解脫。",
        "from": "T37n1763_014:0435b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無常樂我淨一空。",
        "from": "T37n1763_014:0435b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又非解脫二空也。",
        "from": "T37n1763_014:0435b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名無所有者。",
        "from": "T37n1763_014:0435b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體是無常。",
        "from": "T37n1763_014:0435b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又非解脫。",
        "from": "T37n1763_014:0435b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無所有者也。",
        "from": "T37n1763_014:0435b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真解脫者則不如是(至)而復無有水酒等實。",
        "from": "T37n1763_014:0435b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_014:0435b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_014:0435b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水酒瓶者。",
        "from": "T37n1763_014:0435b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為不空空譬也。",
        "from": "T37n1763_014:0435b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃可無水而器不空。",
        "from": "T37n1763_014:0435b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明涅槃乃空無生死耳。",
        "from": "T37n1763_014:0435b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而常果不空也。",
        "from": "T37n1763_014:0435b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解脫亦爾不可說色(至)誰受是常樂我淨者。",
        "from": "T37n1763_014:0435b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_014:0435b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_014:0435b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "垂形六道處。",
        "from": "T37n1763_014:0435b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處見色也。",
        "from": "T37n1763_014:0435b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以是義故不可說空及以不空(至)如來即是涅槃。",
        "from": "T37n1763_014:0435b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_014:0435b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_014:0435b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本未釋之。",
        "from": "T37n1763_014:0435b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貪是結本。",
        "from": "T37n1763_014:0435b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相名貪境也。",
        "from": "T37n1763_014:0435b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貪與所貪。",
        "from": "T37n1763_014:0435b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫縛三有。",
        "from": "T37n1763_014:0435b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切煩惱。",
        "from": "T37n1763_014:0435b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由之而起。",
        "from": "T37n1763_014:0435b12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故生死之數。",
        "from": "T37n1763_014:0435b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因果無窮。",
        "from": "T37n1763_014:0435b13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永斷貪故。",
        "from": "T37n1763_014:0435b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次第而滅。",
        "from": "T37n1763_014:0435b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切解脫。",
        "from": "T37n1763_014:0435b14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生怖畏生死(至)阿耨多羅三藐三菩提。",
        "from": "T37n1763_014:0435b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_014:0435b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_014:0435b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上廣說解脫。",
        "from": "T37n1763_014:0435b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令人歸也。",
        "from": "T37n1763_014:0435b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法相有三。",
        "from": "T37n1763_014:0435b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何者。",
        "from": "T37n1763_014:0435b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法相常定。",
        "from": "T37n1763_014:0435b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貴其先覺。",
        "from": "T37n1763_014:0435b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先覺者歸佛也。",
        "from": "T37n1763_014:0435b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所覺者。",
        "from": "T37n1763_014:0435b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙歸法也。",
        "from": "T37n1763_014:0435b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非法不得名覺也。",
        "from": "T37n1763_014:0435b18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小覺諸法非我。",
        "from": "T37n1763_014:0435b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "假用境智合用。",
        "from": "T37n1763_014:0435b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生死可除。",
        "from": "T37n1763_014:0435b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識其合用歸僧也。",
        "from": "T37n1763_014:0435b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸說行始。",
        "from": "T37n1763_014:0435b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此明其終。",
        "from": "T37n1763_014:0435b20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終得盡苦。",
        "from": "T37n1763_014:0435b20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名解脫也。",
        "from": "T37n1763_014:0435b20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉菩薩白佛言世尊(至)非是聲聞緣覺所知。",
        "from": "T37n1763_014:0435b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_014:0435b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_014:0435b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "涅槃一法具二也。",
        "from": "T37n1763_014:0435b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二言俱在當也。",
        "from": "T37n1763_014:0435b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛法僧常者。",
        "from": "T37n1763_014:0435b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常即涅槃。",
        "from": "T37n1763_014:0435b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常名是一。",
        "from": "T37n1763_014:0435b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而三事即涅槃。",
        "from": "T37n1763_014:0435b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_014:0435b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如三歸之異者。",
        "from": "T37n1763_014:0435b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知涅槃。",
        "from": "T37n1763_014:0435b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "似不一體。",
        "from": "T37n1763_014:0435b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲令如來釋會三歸。",
        "from": "T37n1763_014:0435b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以體一而義異之旨也。",
        "from": "T37n1763_014:0435b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛以眾生畏生死故。",
        "from": "T37n1763_014:0435b27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為說三名。",
        "from": "T37n1763_014:0435b27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非所以別體也。",
        "from": "T37n1763_014:0435b27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有法名一義異者。",
        "from": "T37n1763_014:0435b28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此更開釋所以得有施三名之竟也。",
        "from": "T37n1763_014:0435b28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫施設名義。",
        "from": "T37n1763_014:0435b29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自有多塗。",
        "from": "T37n1763_014:0435b29_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如佛法僧常。",
        "from": "T37n1763_014:0435c01_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名一而三義是異。",
        "from": "T37n1763_014:0435c01_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛以覺為義也。",
        "from": "T37n1763_014:0435c01_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法名不覺者。",
        "from": "T37n1763_014:0435c02_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法以軌為義。",
        "from": "T37n1763_014:0435c02_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非覺義也。",
        "from": "T37n1763_014:0435c02_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "涅槃名解脫者。",
        "from": "T37n1763_014:0435c03_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是無累。",
        "from": "T37n1763_014:0435c03_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼自有釋義也。",
        "from": "T37n1763_014:0435c03_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今解脫者。",
        "from": "T37n1763_014:0435c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是翻彼義釋也。",
        "from": "T37n1763_014:0435c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空名非善者非善。",
        "from": "T37n1763_014:0435c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非惡也。",
        "from": "T37n1763_014:0435c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此證名義俱異者矣。",
        "from": "T37n1763_014:0435c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我昔告彼提者。",
        "from": "T37n1763_014:0435c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "引證三歸名義不應一也。",
        "from": "T37n1763_014:0435c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就別體法僧。",
        "from": "T37n1763_014:0435c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則體義俱異。",
        "from": "T37n1763_014:0435c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今就佛上。",
        "from": "T37n1763_014:0435c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則其體雖一。",
        "from": "T37n1763_014:0435c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逐義為三。",
        "from": "T37n1763_014:0435c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可以體一故。",
        "from": "T37n1763_014:0435c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使義不得三也。",
        "from": "T37n1763_014:0435c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂義有三。",
        "from": "T37n1763_014:0435c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲令體亦別也。",
        "from": "T37n1763_014:0435c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說一為三者。",
        "from": "T37n1763_014:0435c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨方設化。",
        "from": "T37n1763_014:0435c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說法不同。",
        "from": "T37n1763_014:0435c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三歸亦爾也。",
        "from": "T37n1763_014:0435c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉復言如佛所說(至)真解脫者即是如來。",
        "from": "T37n1763_014:0435c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_014:0435c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_014:0435c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生滅聚積名身也。",
        "from": "T37n1763_014:0435c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分別苦樂名智也。",
        "from": "T37n1763_014:0435c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "涅槃無此。",
        "from": "T37n1763_014:0435c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名捨也。",
        "from": "T37n1763_014:0435c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誰受安樂者。",
        "from": "T37n1763_014:0435c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上說種種樂。",
        "from": "T37n1763_014:0435c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明非受樂。",
        "from": "T37n1763_014:0435c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故發問也。",
        "from": "T37n1763_014:0435c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身得安樂者。",
        "from": "T37n1763_014:0435c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有食有身。",
        "from": "T37n1763_014:0435c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有識分別。",
        "from": "T37n1763_014:0435c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為苦也。",
        "from": "T37n1763_014:0435c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無食無身。",
        "from": "T37n1763_014:0435c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不生分別。",
        "from": "T37n1763_014:0435c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為樂也。",
        "from": "T37n1763_014:0435c16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無受樂者。",
        "from": "T37n1763_014:0435c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無受理圓。",
        "from": "T37n1763_014:0435c16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可以方事喻也。",
        "from": "T37n1763_014:0435c16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中諸譬。",
        "from": "T37n1763_014:0435c17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆是少分耳。",
        "from": "T37n1763_014:0435c17_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_014:0435c17_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食已悶者。",
        "from": "T37n1763_014:0435c17_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行地之時。",
        "from": "T37n1763_014:0435c18_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貪嗜五欲。",
        "from": "T37n1763_014:0435c18_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬之於食。",
        "from": "T37n1763_014:0435c18_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為惑所蔽。",
        "from": "T37n1763_014:0435c18_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如心悶也。",
        "from": "T37n1763_014:0435c19_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出外欲吐者。",
        "from": "T37n1763_014:0435c19_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "標心離苦義言出耳。",
        "from": "T37n1763_014:0435c19_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲除煩惱。",
        "from": "T37n1763_014:0435c20_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吐已還者。",
        "from": "T37n1763_014:0435c20_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修道斷結。",
        "from": "T37n1763_014:0435c20_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功登果就。",
        "from": "T37n1763_014:0435c20_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俯入生死。",
        "from": "T37n1763_014:0435c21_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如還也。",
        "from": "T37n1763_014:0435c21_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同伴問者。",
        "from": "T37n1763_014:0435c21_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如迦葉今所問也。",
        "from": "T37n1763_014:0435c21_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若言差者。",
        "from": "T37n1763_014:0435c22_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來解釋果地之妙極也。",
        "from": "T37n1763_014:0435c22_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯別昔曰捨身捨智。",
        "from": "T37n1763_014:0435c23_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言虛不實也。",
        "from": "T37n1763_014:0435c23_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉復言不生不滅(至)如來之性即是解脫。",
        "from": "T37n1763_014:0435c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_014:0435c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧朗曰。",
        "from": "T37n1763_014:0435c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉因釋解脫。",
        "from": "T37n1763_014:0435c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有此句也。",
        "from": "T37n1763_014:0435c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故以為難。",
        "from": "T37n1763_014:0435c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請重釋也。",
        "from": "T37n1763_014:0435c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告迦葉善男子是事不然(至)不可以譬烏鵲之音。",
        "from": "T37n1763_014:0435c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_014:0435c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_014:0435c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以鳥譬鳥。",
        "from": "T37n1763_014:0435c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以聲譬聲。",
        "from": "T37n1763_014:0435c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍取同鳥。",
        "from": "T37n1763_014:0435c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得相比也。",
        "from": "T37n1763_014:0435c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分取可譬佛聲也。",
        "from": "T37n1763_014:0435c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛讚迦葉善哉善哉(至)知諸法性皆亦如是。",
        "from": "T37n1763_014:0436a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_014:0436a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_014:0436a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以是因緣者。",
        "from": "T37n1763_014:0436a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少分因緣也。",
        "from": "T37n1763_014:0436a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無能匹者。",
        "from": "T37n1763_014:0436a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非遍喻也。",
        "from": "T37n1763_014:0436a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為喻者少分也。",
        "from": "T37n1763_014:0436a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉復言云何如來(至)成就滿故名大般涅槃。",
        "from": "T37n1763_014:0436a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_014:0436a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_014:0436a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡聖不可相譬。",
        "from": "T37n1763_014:0436a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若分取可借凡以明聖也。",
        "from": "T37n1763_014:0436a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖不具足者。",
        "from": "T37n1763_014:0436a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逆罪有二種。",
        "from": "T37n1763_014:0436a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者但以心重。",
        "from": "T37n1763_014:0436a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為不具足。",
        "from": "T37n1763_014:0436a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者以心重事重。",
        "from": "T37n1763_014:0436a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為具足。",
        "from": "T37n1763_014:0436a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉菩薩白佛言世尊(至)當作如是護持正法。",
        "from": "T37n1763_014:0436a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_014:0436a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智秀曰。",
        "from": "T37n1763_014:0436a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉領旨。",
        "from": "T37n1763_014:0436a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛讚述也。",
        "from": "T37n1763_014:0436a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大般涅槃經集解卷第十四",
        "from": "T37n1763_014:0436a10_0",
        "context_with_next_sentence": false
    }
]