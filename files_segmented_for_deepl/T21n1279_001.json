[
    {
        "source_sentence": "摩醯首羅天法要",
        "from": "T21n1279_001:0339c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時摩醯首羅天。",
        "from": "T21n1279_001:0339c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於天上與諸天女遊戲化樂作諸伎樂。",
        "from": "T21n1279_001:0339c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忽然於髮中化出一天女。",
        "from": "T21n1279_001:0339c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "容顏端正伎藝第一。",
        "from": "T21n1279_001:0339c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天之眾無能過者。",
        "from": "T21n1279_001:0340a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而說呪曰。",
        "from": "T21n1279_001:0340a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨欝爾灑摩毘藥(二合)尸佉地毘也(二合)鉢囉(二合)鉢底(丁以反)野式伽羅者僧(去)林僧(去)林但儞也(二合)他濕嚩(二合)諾底兼跋囉麼諾哩儞吽泮吒(半音)持誦此呪者。",
        "from": "T21n1279_001:0340a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不問淨與不淨。",
        "from": "T21n1279_001:0340a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有妻無妻。",
        "from": "T21n1279_001:0340a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食五辛酒肉與不食。",
        "from": "T21n1279_001:0340a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常誦持者。",
        "from": "T21n1279_001:0340a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切善願悉得成就。",
        "from": "T21n1279_001:0340a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "況三業清淨人早成悉地。",
        "from": "T21n1279_001:0340a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更有何疑。",
        "from": "T21n1279_001:0340a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人日沒之時。",
        "from": "T21n1279_001:0340a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此呪一百八反。",
        "from": "T21n1279_001:0340a10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即有効驗。",
        "from": "T21n1279_001:0340a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次畫像法　先畫摩醯首羅天像。",
        "from": "T21n1279_001:0340a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "容顏奇特端正殊妙。",
        "from": "T21n1279_001:0340a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從髮際化出一天女。",
        "from": "T21n1279_001:0340a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端正第一垂妙瓔珞。",
        "from": "T21n1279_001:0340a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩手絡環釧。",
        "from": "T21n1279_001:0340a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手捧天花。",
        "from": "T21n1279_001:0340a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與天王相視喜媚之容。",
        "from": "T21n1279_001:0340a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手向下捻裙之勢。",
        "from": "T21n1279_001:0340a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於左膝上坐摩登伽天。",
        "from": "T21n1279_001:0340a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說手印相。",
        "from": "T21n1279_001:0340a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二羽外相叉。",
        "from": "T21n1279_001:0340a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左押右成縛。",
        "from": "T21n1279_001:0340a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "檀惠進力竪禪智申相合禪庾為請召。",
        "from": "T21n1279_001:0340a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1279_001:0340a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵尾跢羅娑(上)柅跛羅摩哩馱抳瞋馱抳瞋馱抳頻馱尼頻馱抳娑嚩(二合)賀若有人日日受持此印真言。",
        "from": "T21n1279_001:0340a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養摩醯首羅天者。",
        "from": "T21n1279_001:0340a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種得驗。",
        "from": "T21n1279_001:0340a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦每作是印呪。",
        "from": "T21n1279_001:0340a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養一切諸佛。",
        "from": "T21n1279_001:0340a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛生歡喜。",
        "from": "T21n1279_001:0340a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼療一切諸病有驗。",
        "from": "T21n1279_001:0340a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦滿一千反。",
        "from": "T21n1279_001:0340a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令婦人生歡喜。",
        "from": "T21n1279_001:0340a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦二千反。",
        "from": "T21n1279_001:0340a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令一切人生歡喜。",
        "from": "T21n1279_001:0340a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦三千反。",
        "from": "T21n1279_001:0340a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得非人愛樂。",
        "from": "T21n1279_001:0340a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦四千反。",
        "from": "T21n1279_001:0340a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切毘那多羅鬼皆歡喜。",
        "from": "T21n1279_001:0340a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦五千反。",
        "from": "T21n1279_001:0340a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切龍女生歡喜心。",
        "from": "T21n1279_001:0340a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至誦一萬反。",
        "from": "T21n1279_001:0340a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切天龍藥叉五通神仙皆悉歡喜。",
        "from": "T21n1279_001:0340a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲見本身者。",
        "from": "T21n1279_001:0340a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取燒死人。",
        "from": "T21n1279_001:0340a28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殘柴誦呪一百八反招之。",
        "from": "T21n1279_001:0340a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得見本身。",
        "from": "T21n1279_001:0340a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若能降伏怨家者。",
        "from": "T21n1279_001:0340a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取赤土畫其形。",
        "from": "T21n1279_001:0340a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書其姓名。",
        "from": "T21n1279_001:0340b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左脚踏之。",
        "from": "T21n1279_001:0340b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦呪一百八反。",
        "from": "T21n1279_001:0340b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不論遠近彼人自然來向所降伏。",
        "from": "T21n1279_001:0340b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲令夫婦相愛。",
        "from": "T21n1279_001:0340b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫其形書姓名。",
        "from": "T21n1279_001:0340b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前法踏之。",
        "from": "T21n1279_001:0340b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即相愛敬。",
        "from": "T21n1279_001:0340b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲欲得國王大臣一切諸人敬愛者。",
        "from": "T21n1279_001:0340b04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前法作之。",
        "from": "T21n1279_001:0340b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若療鬼病者。",
        "from": "T21n1279_001:0340b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取伽陀木。",
        "from": "T21n1279_001:0340b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "截作二十一段。",
        "from": "T21n1279_001:0340b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪二十一反。",
        "from": "T21n1279_001:0340b06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一呪一燒病差。",
        "from": "T21n1279_001:0340b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若神病。",
        "from": "T21n1279_001:0340b07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取阿魏安悉香雄黃青木香苦練葉。",
        "from": "T21n1279_001:0340b07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各等分擣篩和水為丸。",
        "from": "T21n1279_001:0340b08_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦呪燒之病即差。",
        "from": "T21n1279_001:0340b08_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若狐媚病。",
        "from": "T21n1279_001:0340b09_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取雄黃安悉香芎藭獨頭萩犀角羝羊尿白馬懸蹄驢夜等。",
        "from": "T21n1279_001:0340b09_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各分此八味為朱燒之。",
        "from": "T21n1279_001:0340b10_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薰入鼻孔及塗身上。",
        "from": "T21n1279_001:0340b11_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲成結界者。",
        "from": "T21n1279_001:0340b11_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以水呪七反。",
        "from": "T21n1279_001:0340b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散灑四方。",
        "from": "T21n1279_001:0340b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意遠近如法成結界。",
        "from": "T21n1279_001:0340b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩醯首羅天王法要",
        "from": "T21n1279_001:0340b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此法中摩醯首羅者東北伊舍那天也。",
        "from": "T21n1279_001:0340b15_0",
        "context_with_next_sentence": false
    }
]