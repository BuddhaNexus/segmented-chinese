[
    {
        "source_sentence": "摩尼光佛教法儀略一卷",
        "from": "T54n2141A_001:1279c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開元十九年六月八日大德",
        "from": "T54n2141A_001:1279c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拂多誕奉詔集賢院　譯",
        "from": "T54n2141A_001:1279c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "託化國土名號宗教第一",
        "from": "T54n2141A_001:1279c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛夷瑟德烏盧詵者(本國梵音也)譯云光明使者。",
        "from": "T54n2141A_001:1279c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又號具智法王。",
        "from": "T54n2141A_001:1279c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦謂摩尼光佛。",
        "from": "T54n2141A_001:1279c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即我光明大慧無上醫王應化法身之異號也。",
        "from": "T54n2141A_001:1279c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當欲出世。",
        "from": "T54n2141A_001:1279c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二耀降靈分光三體。",
        "from": "T54n2141A_001:1279c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大慈愍故應敵魔軍。",
        "from": "T54n2141A_001:1279c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "親受明尊清淨教命。",
        "from": "T54n2141A_001:1279c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後化誕故云光明使者。",
        "from": "T54n2141A_001:1279c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精真洞慧堅疑克辯故曰具智法王。",
        "from": "T54n2141A_001:1279c24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛應靈聖覺觀究竟故號摩尼光佛。",
        "from": "T54n2141A_001:1279c25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光明所以徹內外。",
        "from": "T54n2141A_001:1279c26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大慧所以極人天。",
        "from": "T54n2141A_001:1279c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無上所以位高尊。",
        "from": "T54n2141A_001:1279c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "醫王所以布法藥。",
        "from": "T54n2141A_001:1279c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則老君託孕太陽流其晶釋迦受胎日輪叶其象資靈本本三聖亦何殊成性存存一貫皆悟道。",
        "from": "T54n2141A_001:1279c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "按彼波斯婆毘長曆自開闢初有十二辰掌分年代。",
        "from": "T54n2141A_001:1280a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至第十一辰名訥管代二百二十七年。",
        "from": "T54n2141A_001:1280a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋迦出現。",
        "from": "T54n2141A_001:1280a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至第十二辰名魔謝管代五百二十七年。",
        "from": "T54n2141A_001:1280a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩尼光佛誕蘇隣國跋帝王宮金薩健種夫人滿艶之所生也。",
        "from": "T54n2141A_001:1280a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婆毘長曆當漢献帝建安十三年二月八日而生。",
        "from": "T54n2141A_001:1280a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "泯然懸合矣。",
        "from": "T54n2141A_001:1280a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至若資稟天符而受胎。",
        "from": "T54n2141A_001:1280a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "齋戒嚴潔而懷孕者。",
        "from": "T54n2141A_001:1280a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本清淨也。",
        "from": "T54n2141A_001:1280a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自胸前化誕。",
        "from": "T54n2141A_001:1280a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卓世殊倫。",
        "from": "T54n2141A_001:1280a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神驗九。",
        "from": "T54n2141A_001:1280a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徵靈瑞五應者生非凡也。",
        "from": "T54n2141A_001:1280a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以三願四寂五真八種無畏眾德圓備。",
        "from": "T54n2141A_001:1280a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其可勝言。",
        "from": "T54n2141A_001:1280a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自天及人拔苦與樂諛德而論矣。",
        "from": "T54n2141A_001:1280a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不然者曷有身誕王宮神凝道慧。",
        "from": "T54n2141A_001:1280a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明宗真本智謀特正。",
        "from": "T54n2141A_001:1280a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體質孤秀。",
        "from": "T54n2141A_001:1280a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "量包乾坤。",
        "from": "T54n2141A_001:1280a13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識洞日月。",
        "from": "T54n2141A_001:1280a13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開兩元大義。",
        "from": "T54n2141A_001:1280a13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "示自性各殊。",
        "from": "T54n2141A_001:1280a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "演三際深文。",
        "from": "T54n2141A_001:1280a14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辯因緣瓦合。",
        "from": "T54n2141A_001:1280a14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誅耶祐正。",
        "from": "T54n2141A_001:1280a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "激濁揚清。",
        "from": "T54n2141A_001:1280a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其詞簡。",
        "from": "T54n2141A_001:1280a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其理直。",
        "from": "T54n2141A_001:1280a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其行正。",
        "from": "T54n2141A_001:1280a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其證真。",
        "from": "T54n2141A_001:1280a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六十年內開示方便。",
        "from": "T54n2141A_001:1280a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "感四聖以為威力。",
        "from": "T54n2141A_001:1280a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "騰七部以作舟航。",
        "from": "T54n2141A_001:1280a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應三宮而建三尊。",
        "from": "T54n2141A_001:1280a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法五明而列五級。",
        "from": "T54n2141A_001:1280a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙門殊特。",
        "from": "T54n2141A_001:1280a18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福被存亡也。",
        "from": "T54n2141A_001:1280a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "按摩訶摩耶經云。",
        "from": "T54n2141A_001:1280a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛滅度後一千三百年。",
        "from": "T54n2141A_001:1280a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "袈裟變白。",
        "from": "T54n2141A_001:1280a19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不受染色。",
        "from": "T54n2141A_001:1280a20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀佛三昧海經云。",
        "from": "T54n2141A_001:1280a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩尼光佛出現世時。",
        "from": "T54n2141A_001:1280a20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常施光明。",
        "from": "T54n2141A_001:1280a21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以作佛事。",
        "from": "T54n2141A_001:1280a21_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "老子化胡經云。",
        "from": "T54n2141A_001:1280a21_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我乘自然光明道氣。",
        "from": "T54n2141A_001:1280a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飛入西那王界蘇隣國中。",
        "from": "T54n2141A_001:1280a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "示為太子。",
        "from": "T54n2141A_001:1280a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨家入道。",
        "from": "T54n2141A_001:1280a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號曰摩尼轉大法輪。",
        "from": "T54n2141A_001:1280a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說經戒律定慧等法。",
        "from": "T54n2141A_001:1280a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至三際。",
        "from": "T54n2141A_001:1280a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及二宗門。",
        "from": "T54n2141A_001:1280a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上從明界下及幽塗。",
        "from": "T54n2141A_001:1280a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有眾生皆由此度。",
        "from": "T54n2141A_001:1280a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩尼之後。",
        "from": "T54n2141A_001:1280a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年垂五九。",
        "from": "T54n2141A_001:1280a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我法當盛者。",
        "from": "T54n2141A_001:1280a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五九四十五。",
        "from": "T54n2141A_001:1280a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四百五十年。",
        "from": "T54n2141A_001:1280a26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教合傳於中國。",
        "from": "T54n2141A_001:1280a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至晉太始二年正月四日乃息化。",
        "from": "T54n2141A_001:1280a27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身還歸真寂。",
        "from": "T54n2141A_001:1280a28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教流諸國。",
        "from": "T54n2141A_001:1280a28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "接化蒼生。",
        "from": "T54n2141A_001:1280a28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從晉太始至今開十九歲計四百六十年。",
        "from": "T54n2141A_001:1280a29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證記合同。",
        "from": "T54n2141A_001:1280b01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖跡照著。",
        "from": "T54n2141A_001:1280b01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教闡明宗用除暗惑。",
        "from": "T54n2141A_001:1280b01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法開兩性分別為門。",
        "from": "T54n2141A_001:1280b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故釋經云。",
        "from": "T54n2141A_001:1280b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人捨分別。",
        "from": "T54n2141A_001:1280b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是則滅諸法。",
        "from": "T54n2141A_001:1280b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如有修行人。",
        "from": "T54n2141A_001:1280b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應共其住。",
        "from": "T54n2141A_001:1280b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又云。",
        "from": "T54n2141A_001:1280b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烏歸虛空。",
        "from": "T54n2141A_001:1280b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獸歸林藪。",
        "from": "T54n2141A_001:1280b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義歸分別。",
        "from": "T54n2141A_001:1280b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道歸涅槃。",
        "from": "T54n2141A_001:1280b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不覈宗本。",
        "from": "T54n2141A_001:1280b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將何歸趣。",
        "from": "T54n2141A_001:1280b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行門真實。",
        "from": "T54n2141A_001:1280b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果證三宮。",
        "from": "T54n2141A_001:1280b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性離無明。",
        "from": "T54n2141A_001:1280b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為一相。",
        "from": "T54n2141A_001:1280b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此教中。",
        "from": "T54n2141A_001:1280b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是稱解脫。",
        "from": "T54n2141A_001:1280b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略舉微分。",
        "from": "T54n2141A_001:1280b06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以表進修。",
        "from": "T54n2141A_001:1280b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵本頗具。",
        "from": "T54n2141A_001:1280b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此未繁載。",
        "from": "T54n2141A_001:1280b07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形相儀第二",
        "from": "T54n2141A_001:1280b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩尼光佛。",
        "from": "T54n2141A_001:1280b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂圓十二光王勝相。",
        "from": "T54n2141A_001:1280b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體備大明無量祕義。",
        "from": "T54n2141A_001:1280b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙形特絕。",
        "from": "T54n2141A_001:1280b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人天無比。",
        "from": "T54n2141A_001:1280b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "串以素帔。",
        "from": "T54n2141A_001:1280b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倣四淨法身。",
        "from": "T54n2141A_001:1280b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其居白座。",
        "from": "T54n2141A_001:1280b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "像五金剛地二界合離。",
        "from": "T54n2141A_001:1280b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初後旨趣。",
        "from": "T54n2141A_001:1280b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宛在真容。",
        "from": "T54n2141A_001:1280b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀之可曉。",
        "from": "T54n2141A_001:1280b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸有靈相百千勝妙。",
        "from": "T54n2141A_001:1280b12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寔難備陳。",
        "from": "T54n2141A_001:1280b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經圖儀第三凡七部并圖一。",
        "from": "T54n2141A_001:1280b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一大應輪部　譯云徹盡萬法根源智經第二尋提賀部　譯云淨命寶藏經第三泥萬部　譯云律藏經亦稱藥藏經第四阿羅瓚部　譯云祕密法藏經第五鉢迦摩帝夜部　譯云證明過去教經第六俱緩部　譯云大力士經第七阿拂胤部　譯云讚願經大門荷翼圖一　譯云大二宗圖右七部大經及圖。",
        "from": "T54n2141A_001:1280b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩尼光佛當欲降代。",
        "from": "T54n2141A_001:1280b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾聖贊助。",
        "from": "T54n2141A_001:1280b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出應有緣。",
        "from": "T54n2141A_001:1280b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置法之日。",
        "from": "T54n2141A_001:1280b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳受五級。",
        "from": "T54n2141A_001:1280b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其餘六十年間宣說正法。",
        "from": "T54n2141A_001:1280b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸弟子等隨事記錄。",
        "from": "T54n2141A_001:1280b25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此不載列。",
        "from": "T54n2141A_001:1280b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五級儀第四",
        "from": "T54n2141A_001:1280b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一十二慕闍　譯云承法教道者",
        "from": "T54n2141A_001:1280b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二七十二薩波塞　譯云侍法者亦號拂多誕第三三百六十默奚悉德　譯云法堂主第四阿羅緩　譯云一切純善人第五耨沙喭　譯云一切淨信聽者右阿羅緩已上並素冠服。",
        "from": "T54n2141A_001:1280b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯耨沙喭一位聽仍舊服。",
        "from": "T54n2141A_001:1280c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是五位稟受相依。",
        "from": "T54n2141A_001:1280c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸遵教命。",
        "from": "T54n2141A_001:1280c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅持禁戒。",
        "from": "T54n2141A_001:1280c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名解脫路。",
        "from": "T54n2141A_001:1280c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若慕闍犯戒。",
        "from": "T54n2141A_001:1280c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即不得承其教命。",
        "from": "T54n2141A_001:1280c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "假使精通七部才辯卓然。",
        "from": "T54n2141A_001:1280c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為有愆違。",
        "from": "T54n2141A_001:1280c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五位不攝如樹滋茂皆因其根。",
        "from": "T54n2141A_001:1280c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "根若憊者樹必乾枯。",
        "from": "T54n2141A_001:1280c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿羅緩犯戒。",
        "from": "T54n2141A_001:1280c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "視之如死。",
        "from": "T54n2141A_001:1280c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "表白眾知逐令出法。",
        "from": "T54n2141A_001:1280c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "海雖至廣不宿死屍。",
        "from": "T54n2141A_001:1280c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有覆藏。",
        "from": "T54n2141A_001:1280c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還同破戒。",
        "from": "T54n2141A_001:1280c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寺宇儀第五",
        "from": "T54n2141A_001:1280c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經圖堂一　齋講堂一　禮懺堂一　教授堂一　病僧堂一右置五堂。",
        "from": "T54n2141A_001:1280c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法眾共居。",
        "from": "T54n2141A_001:1280c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精修善業。",
        "from": "T54n2141A_001:1280c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得別立私室厨庫。",
        "from": "T54n2141A_001:1280c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每日齋食。",
        "from": "T54n2141A_001:1280c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儼然待施。",
        "from": "T54n2141A_001:1280c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無施者。",
        "from": "T54n2141A_001:1280c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乞丐以充。",
        "from": "T54n2141A_001:1280c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯使聽人勿畜奴婢及六畜等非法之具。",
        "from": "T54n2141A_001:1280c18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每寺尊首詮蕳三人。",
        "from": "T54n2141A_001:1280c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一阿拂胤薩　譯云讚願首專知法事。",
        "from": "T54n2141A_001:1280c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二呼嚧喚　譯云教道首專知獎勸。",
        "from": "T54n2141A_001:1280c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三遏換健塞波塞。",
        "from": "T54n2141A_001:1280c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譯云月直。",
        "from": "T54n2141A_001:1280c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "專知供施。",
        "from": "T54n2141A_001:1280c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆須依命。",
        "from": "T54n2141A_001:1280c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得擅意。",
        "from": "T54n2141A_001:1280c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出家儀第六",
        "from": "T54n2141A_001:1280c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初辯二宗。",
        "from": "T54n2141A_001:1280c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求出家者。",
        "from": "T54n2141A_001:1280c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須知明暗各宗。",
        "from": "T54n2141A_001:1280c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性情懸隔。",
        "from": "T54n2141A_001:1280c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不辯識。",
        "from": "T54n2141A_001:1280c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以修為。",
        "from": "T54n2141A_001:1280c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次明三際。",
        "from": "T54n2141A_001:1281a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一初際　二中際　三後際",
        "from": "T54n2141A_001:1281a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初際者。",
        "from": "T54n2141A_001:1281a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未有天地。",
        "from": "T54n2141A_001:1281a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但殊明暗。",
        "from": "T54n2141A_001:1281a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明性智慧。",
        "from": "T54n2141A_001:1281a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "暗性愚癡。",
        "from": "T54n2141A_001:1281a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸所動靜。",
        "from": "T54n2141A_001:1281a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無不相背。",
        "from": "T54n2141A_001:1281a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中際者。",
        "from": "T54n2141A_001:1281a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "暗既侵明。",
        "from": "T54n2141A_001:1281a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恣情馳逐。",
        "from": "T54n2141A_001:1281a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明來入暗。",
        "from": "T54n2141A_001:1281a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "委質推移。",
        "from": "T54n2141A_001:1281a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大患厭離。",
        "from": "T54n2141A_001:1281a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於形體火宅。",
        "from": "T54n2141A_001:1281a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願求於出離。",
        "from": "T54n2141A_001:1281a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勞身救性。",
        "from": "T54n2141A_001:1281a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖教固然。",
        "from": "T54n2141A_001:1281a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即妄為真。",
        "from": "T54n2141A_001:1281a07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孰敢聞命。",
        "from": "T54n2141A_001:1281a07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "事須辯折。",
        "from": "T54n2141A_001:1281a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求解脫緣。",
        "from": "T54n2141A_001:1281a08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後際者。",
        "from": "T54n2141A_001:1281a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教化事畢。",
        "from": "T54n2141A_001:1281a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真妄歸根。",
        "from": "T54n2141A_001:1281a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明既歸於大明。",
        "from": "T54n2141A_001:1281a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "暗亦歸於積暗。",
        "from": "T54n2141A_001:1281a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二宗各復。",
        "from": "T54n2141A_001:1281a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩者交歸。",
        "from": "T54n2141A_001:1281a10_2",
        "context_with_next_sentence": false
    }
]