[
    {
        "source_sentence": "梁京寺記",
        "from": "T51n2094_001:1024a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闕名",
        "from": "T51n2094_001:1024a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小莊嚴寺",
        "from": "T51n2094_001:1024a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梁小莊嚴寺。",
        "from": "T51n2094_001:1024a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在建業定陰里。",
        "from": "T51n2094_001:1024a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本是晉零陵王廟地。",
        "from": "T51n2094_001:1024a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天監六年度禪師起造。",
        "from": "T51n2094_001:1024a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時有邵文立者。",
        "from": "T51n2094_001:1024a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世以烹屠為業。",
        "from": "T51n2094_001:1024a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嘗欲殺一鹿。",
        "from": "T51n2094_001:1024a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鹿跪而流淚。",
        "from": "T51n2094_001:1024a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為不祥。",
        "from": "T51n2094_001:1024a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鹿懷一麑。",
        "from": "T51n2094_001:1024a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尋當產育。",
        "from": "T51n2094_001:1024a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就庖哀切。",
        "from": "T51n2094_001:1024a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同被刳割。",
        "from": "T51n2094_001:1024a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因斯患疾。",
        "from": "T51n2094_001:1024a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眉鬚皆落。",
        "from": "T51n2094_001:1024a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身瘡並壞。",
        "from": "T51n2094_001:1024a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後乃深起悔責求道。",
        "from": "T51n2094_001:1024a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "度禪師發大誓願。",
        "from": "T51n2094_001:1024a25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "罄捨家資迴買此地。",
        "from": "T51n2094_001:1024a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為立伽藍。",
        "from": "T51n2094_001:1024a26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同泰寺",
        "from": "T51n2094_001:1024a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梁武帝改年號大同。",
        "from": "T51n2094_001:1024a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起同泰寺。",
        "from": "T51n2094_001:1024a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在臺城內。",
        "from": "T51n2094_001:1024a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "窮竭帑藏。",
        "from": "T51n2094_001:1024a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造大佛閣七層。",
        "from": "T51n2094_001:1024a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為火所焚。",
        "from": "T51n2094_001:1024a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "武帝捨身施財。",
        "from": "T51n2094_001:1024a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以祈佛福。",
        "from": "T51n2094_001:1024b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自大通以後。",
        "from": "T51n2094_001:1024b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無年不幸。",
        "from": "T51n2094_001:1024b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "興國禪寺",
        "from": "T51n2094_001:1024b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梁武帝天監十三年。",
        "from": "T51n2094_001:1024b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以錢二十萬。",
        "from": "T51n2094_001:1024b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "易定林前前岡獨龍阜。",
        "from": "T51n2094_001:1024b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以葬誌公。",
        "from": "T51n2094_001:1024b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永定公主以湯沐之資。",
        "from": "T51n2094_001:1024b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造浮圖五級於其上。",
        "from": "T51n2094_001:1024b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十四年。",
        "from": "T51n2094_001:1024b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即塔前建開善寺。",
        "from": "T51n2094_001:1024b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昇元寺",
        "from": "T51n2094_001:1024b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昇元寺即瓦棺寺也。",
        "from": "T51n2094_001:1024b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在城西隅瞰江面。",
        "from": "T51n2094_001:1024b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後踞崇岡最為古跡。",
        "from": "T51n2094_001:1024b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "累經兵火。",
        "from": "T51n2094_001:1024b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略無彷彿。",
        "from": "T51n2094_001:1024b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "李王時昇元閣猶在。",
        "from": "T51n2094_001:1024b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃梁朝故物。",
        "from": "T51n2094_001:1024b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大愛敬寺",
        "from": "T51n2094_001:1024b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梁武帝普通元年造。",
        "from": "T51n2094_001:1024b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在蔣山之北高峯上。",
        "from": "T51n2094_001:1024b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法寶寺",
        "from": "T51n2094_001:1024b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梁同泰寺基之半也。",
        "from": "T51n2094_001:1024b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建康剎錄。",
        "from": "T51n2094_001:1024b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梁武帝大通元年。",
        "from": "T51n2094_001:1024b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "創同泰寺。",
        "from": "T51n2094_001:1024b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寺處宮後。",
        "from": "T51n2094_001:1024b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別開一門。",
        "from": "T51n2094_001:1024b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名大通門。",
        "from": "T51n2094_001:1024b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝晨夕講議。",
        "from": "T51n2094_001:1024b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多遊此門。",
        "from": "T51n2094_001:1024b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法光寺",
        "from": "T51n2094_001:1024b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即梁之蕭帝寺。",
        "from": "T51n2094_001:1024b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舊傳。",
        "from": "T51n2094_001:1024b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天監十三年造。",
        "from": "T51n2094_001:1024b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元絳寺記云。",
        "from": "T51n2094_001:1024b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不知從昔之名。",
        "from": "T51n2094_001:1024b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故後人以帝氏目之。",
        "from": "T51n2094_001:1024b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶林寺",
        "from": "T51n2094_001:1024b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梁天監中。",
        "from": "T51n2094_001:1024b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "武帝與寶公。",
        "from": "T51n2094_001:1024b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同遊此山。",
        "from": "T51n2094_001:1024b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見林巒殊勝。",
        "from": "T51n2094_001:1024b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命建精藍。",
        "from": "T51n2094_001:1024b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長于寺",
        "from": "T51n2094_001:1024b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建康南五里有山岡。",
        "from": "T51n2094_001:1024b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其間平地。",
        "from": "T51n2094_001:1024b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庶民雜居。",
        "from": "T51n2094_001:1024b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大長于．小長于．東長于．並是地名。",
        "from": "T51n2094_001:1024b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小長于在瓦棺寺南。",
        "from": "T51n2094_001:1024b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "巷西頭出大江。",
        "from": "T51n2094_001:1024b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梁初起長于寺。",
        "from": "T51n2094_001:1024b26_6",
        "context_with_next_sentence": false
    }
]