    {
        "source_sentence": "即從座起頂禮佛足而白佛言。",
        "from": "X01n0010_002:0357a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "X01n0010_002:0357a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我於恒河沙劫聞佛真言。",
        "from": "X01n0010_002:0357a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從三慧入圓通。",
        "from": "X01n0010_002:0357a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今復聞如是勝義。",
        "from": "X01n0010_002:0357a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願因佛旨以拔濟眾生。",
        "from": "X01n0010_002:0357a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有能誦是經及神呪者。",
        "from": "X01n0010_002:0357a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾即護持令彼成就。",
        "from": "X01n0010_002:0357a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言。",
        "from": "X01n0010_002:0357a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉。",
        "from": "X01n0010_002:0357a07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀世音菩薩即說六字大明神呪及大陀羅尼神呪曰。",
        "from": "X01n0010_002:0357a07_11",
        "context_with_next_sentence": true
    },
    {
