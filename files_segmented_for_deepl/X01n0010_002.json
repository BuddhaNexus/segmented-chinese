        "from": "X01n0010_002:0358a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛說是經義神呪。",
        "from": "X01n0010_002:0358b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸菩薩各說神呪贊揚佛道。",
        "from": "X01n0010_002:0358b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利弗合掌恭敬白佛言。",
        "from": "X01n0010_002:0358c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若徧滿十方恒河沙世界。",
        "from": "X01n0010_002:0358c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持誦此經及神呪者得福多不。",
        "from": "X01n0010_002:0358c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告舍利弗。",
        "from": "X01n0010_002:0358c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若恒河沙世界諸有學。",
        "from": "X01n0010_002:0358c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲進修正覺妙明自性斷緣空諦。",
        "from": "X01n0010_002:0358c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一聞勝義諸漏虗盡。",
        "from": "X01n0010_002:0358c04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人。",
        "from": "X01n0010_002:0358c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比丘比丘尼。",
        "from": "X01n0010_002:0358c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "優婆塞優婆夷。",
        "from": "X01n0010_002:0358c05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精心好道堅持戒律。",
        "from": "X01n0010_002:0358c05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦是第一希有大功德經及神呪即得成就。",
        "from": "X01n0010_002:0358c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生罹諸苦難及諸煩惱冤業宿障。",
        "from": "X01n0010_002:0358c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得罪惡消除灾障潛釋安隱快樂。",
        "from": "X01n0010_002:0358c07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遇水火盜賊。",
        "from": "X01n0010_002:0358c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水不能溺火不能燒。",
        "from": "X01n0010_002:0358c08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黑風大海盜賊兵戈。",
        "from": "X01n0010_002:0358c08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虎狼蛇蟲魘蠱毒藥呪詛俱不能害。",
        "from": "X01n0010_002:0358c09_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人。",
        "from": "X01n0010_002:0358c10_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書寫此經及神呪受持佩誦。",
        "from": "X01n0010_002:0358c10_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以華香纓絡幢旛繒蓋眾樂燈燭起塔造寺種種供養。",
        "from": "X01n0010_002:0358c10_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於末劫中開示未學勝於七寶布施。",
        "from": "X01n0010_002:0358c11_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九世先靈咸獲超度。",
        "from": "X01n0010_002:0358c12_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切幽魂滯魄十種異類竝得超生。",
        "from": "X01n0010_002:0358c12_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已獲福德無量無邊。",
        "from": "X01n0010_002:0358c13_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無嗣續生智慧男子。",
        "from": "X01n0010_002:0358c14_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如性癡愚即成大辯才生一切種智。",
        "from": "X01n0010_002:0358c14_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽命不永即得延生。",
        "from": "X01n0010_002:0358c15_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復得身榮家盛不犯刑憲。",
        "from": "X01n0010_002:0358c15_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其有貪欲恚癡不實妄想塵勞所汙悉得清淨。",
        "from": "X01n0010_002:0358c16_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永不墮無間地獄受諸輪迴苦趣。",
        "from": "X01n0010_002:0358c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人持誦是經及神呪。",
        "from": "X01n0010_002:0358c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三千大千世界光明圓滿合成一界。",
        "from": "X01n0010_002:0358c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其有墮諸鑊湯洋銅銕城刀山劒樹斧鉞鎗鋸黑闇寒冰。",
        "from": "X01n0010_002:0358c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種地獄悉皆破壞。",
        "from": "X01n0010_002:0358c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸際光明即得超脫。",
        "from": "X01n0010_002:0358c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切火蛇火狗馬頭羅剎自相遠離。",
        "from": "X01n0010_002:0358c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人發千百億善念。",
        "from": "X01n0010_002:0358c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此經及神呪。",
        "from": "X01n0010_002:0358c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我即千百億化身。",
        "from": "X01n0010_002:0358c22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼不惑外道皆成佛果。",
        "from": "X01n0010_002:0358c22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人持誦此經一句一偈及一神呪。",
        "from": "X01n0010_002:0358c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所獲福德無量。",
        "from": "X01n0010_002:0358c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知是人非於一佛二佛種諸善根。",
        "from": "X01n0010_002:0358c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是乃至盡百千萬億那由他一切佛所。",
        "from": "X01n0010_002:0359a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種諸福慧故聞此經教。",
        "from": "X01n0010_002:0359a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時舍利弗聞佛說是經功德不可思議。",
        "from": "X01n0010_002:0359a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即頂禮恭敬而說偈曰。",
        "from": "X01n0010_002:0359a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如來大興慈愍心　　示我第一希有義　廣大勝妙不思議　　是故覺悟一切人　充周法界等河沙　　惟是密因最上品　無量功德不可說　　不可說轉受佛恩　大雄威力大神通　　要令種種成佛道　眾生聞者隨所願　　有所得者由善根　我今頂禮妙覺尊　　永斷無明諸惑業　誓願將身於歷劫　　護持法藏顯真如佛說是經已。",
        "from": "X01n0010_002:0359a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙法圓音徧十方界。",
        "from": "X01n0010_002:0359a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即時天雨百寶妙華。",
        "from": "X01n0010_002:0359a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五色紛糅徧滿十方虗空。",
        "from": "X01n0010_002:0359a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方微塵剎土皆成寶色。",
        "from": "X01n0010_002:0359a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡世界所未有異品妙香馥郁凝聚。",
        "from": "X01n0010_002:0359a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "河沙佛國悉皆熏聞。",
        "from": "X01n0010_002:0359a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸大菩薩摩訶薩。",
        "from": "X01n0010_002:0359a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸無漏大阿羅漢。",
        "from": "X01n0010_002:0359a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切世間天人阿脩羅。",
        "from": "X01n0010_002:0359a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆踊躍歡喜信受奉行。",
        "from": "X01n0010_002:0359a16_10",
        "context_with_next_sentence": false
    }
]