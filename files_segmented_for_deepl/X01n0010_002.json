    {
        "source_sentence": "亦從座起頂禮佛足而白佛言。",
        "from": "X01n0010_002:0357c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "X01n0010_002:0357c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我最初成道聞是勝義及神呪。",
        "from": "X01n0010_002:0357c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "承佛威旨廣度眾生而得成就。",
        "from": "X01n0010_002:0357c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "X01n0010_002:0357c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵斡吉杓囉蒙(含口誦)",
        "from": "X01n0010_002:0357c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捺謨蠻租釋哩(二合)孤麻囉蒲答耶菩提薩埵耶馬曷薩埵耶馬曷葛嚕聶葛耶答的牙(二合)塔唵啞囉接月囉接述銕月述銕杓塔尼月杓塔尼杓塔耶月杓塔耶啞麻列月麻列你兒(二合)麻列拶耶斡哩拶耶咓曷尼嚧嚧咂棃吽吽吽癹吒(二合)癹吒(二合)癹吒(二合)莎曷爾時金剛手菩薩。",
        "from": "X01n0010_002:0357c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即從座起頂禮佛足右繞三帀而白佛言。",
        "from": "X01n0010_002:0358a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大悲世尊。",
        "from": "X01n0010_002:0358a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣為諸眾生開發蒙昧宣揚妙旨。",
        "from": "X01n0010_002:0358a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作大方便闡祕密藏。",
        "from": "X01n0010_002:0358a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為最上了義大乘。",
        "from": "X01n0010_002:0358a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等幸承慈誨。",
        "from": "X01n0010_002:0358a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持是經及神呪。",
        "from": "X01n0010_002:0358a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願覺悟一切有情及諸末世一切眾生。",
        "from": "X01n0010_002:0358a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永斷疑悔得成正覺。",
        "from": "X01n0010_002:0358a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "X01n0010_002:0358a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵斡資囉(二合)叭呢吽",
        "from": "X01n0010_002:0358a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵斡資囉(二合)[口/(午干+尢)/貝](則干切)劄馬曷嚧[煞(烈列)+山](音煞)拿吽吽吽癹吒(二合)癹吒(二合)癹吒(二合)爾時摩利支天菩薩。",
        "from": "X01n0010_002:0358a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即從座起頂禮佛足右繞三帀。",
        "from": "X01n0010_002:0358a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而白佛言。",
        "from": "X01n0010_002:0358a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大悲世尊。",
        "from": "X01n0010_002:0358a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我於如來得大神通自在力。",
        "from": "X01n0010_002:0358a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常行日月天前所不能見我則能見。",
        "from": "X01n0010_002:0358a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能見無人能知。",
        "from": "X01n0010_002:0358a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能執無人能縛。",
        "from": "X01n0010_002:0358a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無人能害無人能欺誑。",
        "from": "X01n0010_002:0358a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人常憶念者應亦如是。",
        "from": "X01n0010_002:0358a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願常於恐怖苦難之處護諸有情。",
        "from": "X01n0010_002:0358a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若盜賊水火刀兵毒藥疾病饑饉天龍鬼神人及非人冤家惡獸不令為害。",
        "from": "X01n0010_002:0358a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一切眾生有能持誦此經及神呪者。",
        "from": "X01n0010_002:0358a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當守護令其成就。",
        "from": "X01n0010_002:0358a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永無灾障遠離諸難。",
        "from": "X01n0010_002:0358a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身垢清淨速證聖果。",
        "from": "X01n0010_002:0358a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說陀羅尼曰。",
        "from": "X01n0010_002:0358a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵馬哩唧茫莎曷",
        "from": "X01n0010_002:0358a19_0",
        "context_with_next_sentence": true
    },
