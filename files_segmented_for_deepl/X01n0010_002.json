[
    {
        "source_sentence": "大明仁孝皇后夢感佛說第一希有大功德經卷下爾時尊勝佛母。",
        "from": "X01n0010_002:0356a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及救度佛母。",
        "from": "X01n0010_002:0356a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙斡哩佛母。",
        "from": "X01n0010_002:0356a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞說是經及神呪功德不可思議。",
        "from": "X01n0010_002:0356a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即從座起合掌恭敬而白佛言。",
        "from": "X01n0010_002:0356a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我速疾勇猛能除怖畏。",
        "from": "X01n0010_002:0356a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能授諸勝義殊勝威光。",
        "from": "X01n0010_002:0356a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照見三世施精勤行得超彼岸。",
        "from": "X01n0010_002:0356a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能攝無餘諸藥叉眾於他加行摧壞一切自身熾盛。",
        "from": "X01n0010_002:0356a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鎮世攝伏度脫有情。",
        "from": "X01n0010_002:0356a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現勝妙光普徧喜悅。",
        "from": "X01n0010_002:0356a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能破七險能消灾禍令離諸怨。",
        "from": "X01n0010_002:0356a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能動三處能滅諸毒能解鬬諍。",
        "from": "X01n0010_002:0356a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善除瘟疫善靜威力種種具足。",
        "from": "X01n0010_002:0356a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願施此力護持經教。",
        "from": "X01n0010_002:0356a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於世間眾生作種種方便。",
        "from": "X01n0010_002:0356a21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永得皈依佛道不入輪迴。",
        "from": "X01n0010_002:0356a22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是尊勝佛母。",
        "from": "X01n0010_002:0356a22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及救度佛母沙斡哩佛母。",
        "from": "X01n0010_002:0356a22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各說無量大陀羅尼神呪曰。",
        "from": "X01n0010_002:0356a23_19",
        "context_with_next_sentence": true
    },
