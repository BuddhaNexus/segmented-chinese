[
    {
        "source_sentence": "傳教大師將來目錄",
        "from": "T55n2159_001:1055a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刻根本大師將來錄序",
        "from": "T55n2159_001:1055a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大師入唐求法目錄有二本。",
        "from": "T55n2159_001:1055a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其一則記於台州所得經疏名目。",
        "from": "T55n2159_001:1055a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂之圓宗錄。",
        "from": "T55n2159_001:1055a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其一則記於越州所受密典名數。",
        "from": "T55n2159_001:1055a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂之越府錄。",
        "from": "T55n2159_001:1055a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "越府錄有元祿年間印本。",
        "from": "T55n2159_001:1055a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而文字紕訛不正。",
        "from": "T55n2159_001:1055a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今因就橫川松禪院所藏大師真筆。",
        "from": "T55n2159_001:1055a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以摸出之。",
        "from": "T55n2159_001:1055a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其圓宗錄元龜之亂散失。",
        "from": "T55n2159_001:1055a10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不知所在。",
        "from": "T55n2159_001:1055a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故就正二位前權大納言宗建卿所藏寫本。",
        "from": "T55n2159_001:1055a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以錄出之。",
        "from": "T55n2159_001:1055a12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且挍以東寺觀智院所藏寬治五年寫本。",
        "from": "T55n2159_001:1055a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂并上木。",
        "from": "T55n2159_001:1055a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翼傳之萬世。",
        "from": "T55n2159_001:1055a14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以備法門盛典云爾。",
        "from": "T55n2159_001:1055a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文政四年辛巳大師千年遠忌之初夏",
        "from": "T55n2159_001:1055a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "叡嶽新探題權僧正真超識",
        "from": "T55n2159_001:1055a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進官錄上表",
        "from": "T55n2159_001:1055a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙門最澄言。",
        "from": "T55n2159_001:1055a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最澄聞。",
        "from": "T55n2159_001:1055a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六爻探頤局於生滅之場。",
        "from": "T55n2159_001:1055a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百物正名未涉真如之境。",
        "from": "T55n2159_001:1055a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈若隨他權教開三乘於機門。",
        "from": "T55n2159_001:1055a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨自實教示一乘於道場哉。",
        "from": "T55n2159_001:1055a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然則。",
        "from": "T55n2159_001:1055a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓教難說。",
        "from": "T55n2159_001:1055a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "演其義者天台。",
        "from": "T55n2159_001:1055a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙法難傳。",
        "from": "T55n2159_001:1055a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "暢其道者聖帝。",
        "from": "T55n2159_001:1055a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏惟陛下。",
        "from": "T55n2159_001:1055a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纂靈出震。",
        "from": "T55n2159_001:1055a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "撫運登極。",
        "from": "T55n2159_001:1055a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北蕃來朝請賀正於每年。",
        "from": "T55n2159_001:1055a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東夷北首知歸德於先年。",
        "from": "T55n2159_001:1055a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是。",
        "from": "T55n2159_001:1055a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屬想圓宗緬懷一乘。",
        "from": "T55n2159_001:1055a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "紹宣妙法以為大訓。",
        "from": "T55n2159_001:1055a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由是。",
        "from": "T55n2159_001:1055a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙圓極教。",
        "from": "T55n2159_001:1055a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應聖機而興顯。",
        "from": "T55n2159_001:1055a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灌頂祕法。",
        "from": "T55n2159_001:1055a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "感皇緣而圓滿。",
        "from": "T55n2159_001:1055a26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最澄奉使求法。",
        "from": "T55n2159_001:1055a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠尋靈蹤。",
        "from": "T55n2159_001:1055a26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往登台嶺。",
        "from": "T55n2159_001:1055a27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "躬寫教迹。",
        "from": "T55n2159_001:1055a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所獲經并疏及記等。",
        "from": "T55n2159_001:1055a27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總二百三十部四百六十卷。",
        "from": "T55n2159_001:1055a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且見進經一十卷。",
        "from": "T55n2159_001:1055a28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名曰金字妙法蓮華經七卷。",
        "from": "T55n2159_001:1055a29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金字金剛般若經一卷。",
        "from": "T55n2159_001:1055a29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金字菩薩戒經一卷。",
        "from": "T55n2159_001:1055b01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金字觀無量壽經一卷。",
        "from": "T55n2159_001:1055b01_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及天台智者大師靈應圖一張。",
        "from": "T55n2159_001:1055b02_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天台大師禪鎮一頭。",
        "from": "T55n2159_001:1055b03_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天台山香爐峯送檉及柏木文釋四枚。",
        "from": "T55n2159_001:1055b03_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說法白角如意一抦。",
        "from": "T55n2159_001:1055b04_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謹遣弟子藏經奉進。",
        "from": "T55n2159_001:1055b04_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但聖鑑照明二門圓滿。",
        "from": "T55n2159_001:1055b05_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不任誠懇之至。",
        "from": "T55n2159_001:1055b05_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉表戰慄謹言。",
        "from": "T55n2159_001:1055b06_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "延曆二十四年七月十五日　沙門最澄上表傳教大師將來台州錄日本國求法僧最澄等勘定合疏記等壹佰貳部貳佰肆拾卷之中(別絹圖壹卷但疏紙陸行捌佰玖拾漆紙)妙法蓮華經玄義十卷(智者大師出)(二百七十二紙)妙法蓮華經玄義釋十卷(荊溪和尚撰)(三百四十三紙)妙法蓮華經文句疏十卷(智者大師出)(三百三十七紙)妙法蓮華經文句疏記十卷(荊溪和尚撰)(五百一十五紙)妙法蓮華經觀音品義一卷(智者大師出)(四十紙)妙法蓮華經觀音品義疏二卷(智者大師出)(三十八紙)妙法蓮華經科文二卷(左溪和尚撰)(八十五紙)妙法蓮華經觀音品偈科文一卷(一紙)妙法蓮華經大意一卷(荊溪明曠座主述)(一十紙)妙法蓮華經文句序一卷(會稽沙門神迴述)(三紙)妙法蓮華經懺法一卷(或名三昧行法)(智者大師出)(一十八紙)妙法蓮華經三昧補助儀一卷(荊溪和上撰)(三紙)妙法蓮華經大意一卷(五紙)妙法蓮華經玄義科文一卷(七紙)妙法蓮華經文句疏科文一卷(六紙)已上一十一部一十三卷同帙　法華部(一千六百八十三紙)。",
        "from": "T55n2159_001:1055b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩訶止觀十卷(智者大師出)(三百七十紙)摩訶止觀補行傳弘決十卷(荊溪和尚撰)(七百四十七紙)摩訶止觀文句二卷(荊溪和尚撰)(七十八紙)摩訶止觀義例二卷(荊溪和尚撰)(三十一紙)摩訶止觀心要一卷(八紙)摩訶止觀音一卷(八紙)摩訶止觀略音一卷(一十五紙)摩訶止觀大意一卷(荊溪和尚撰)(一十紙)摩訶止觀八教大意一卷(荊溪沙門明曠述)(一十五紙)摩訶止觀科文一卷(八紙)摩訶止觀三德圖一張(六紙)已上九部一十卷同帙　止觀部(一千二百九十六紙)。",
