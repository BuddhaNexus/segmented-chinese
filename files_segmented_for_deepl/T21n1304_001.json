[
    {
        "source_sentence": "宿曜儀軌一卷",
        "from": "T21n1304_001:0422b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一行撰",
        "from": "T21n1304_001:0422b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空藏印　合掌縛水指。",
        "from": "T21n1304_001:0422b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風指如寶形。",
        "from": "T21n1304_001:0422b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1304_001:0422b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿伽沙(一)迦羅波伊野(二)唵麼哩(三)伽摩哩(四)母梨(五)娑嚩賀若人欲求福智。",
        "from": "T21n1304_001:0422b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當歸依此菩薩。",
        "from": "T21n1304_001:0422b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日月星。",
        "from": "T21n1304_001:0422b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆虛空藏所變也。",
        "from": "T21n1304_001:0422b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊印　虛空合掌。",
        "from": "T21n1304_001:0422b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大指各押二水甲。",
        "from": "T21n1304_001:0422b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二風屈捻空端。",
        "from": "T21n1304_001:0422b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1304_001:0422b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)阿(引入惡聲)味(引)羅吽佉左洛　若求息(災)當欲殄滅七種災難。",
        "from": "T21n1304_001:0422b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂日月薄喰五星。",
        "from": "T21n1304_001:0422b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "違失常度兵賊競起。",
        "from": "T21n1304_001:0422b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水旱不時風雨失度。",
        "from": "T21n1304_001:0422b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡臣背逆損害國民。",
        "from": "T21n1304_001:0422b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "武狼惡獸食噉眾生。",
        "from": "T21n1304_001:0422b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五穀不登。",
        "from": "T21n1304_001:0422b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是災禍急厄官府王難死厄怖等。",
        "from": "T21n1304_001:0422b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆當消散。",
        "from": "T21n1304_001:0422b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普賢印　虛心合掌真言曰。",
        "from": "T21n1304_001:0422b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "支波啄(一決定)毘尼波啄(二斷結)鳥蘇波啄(三盡生死)此呪平旦誦七遍夜七遍。",
        "from": "T21n1304_001:0422b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "厭魅野道蟲毒皆悉消滅。",
        "from": "T21n1304_001:0422b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能得身心慧三解脫。",
        "from": "T21n1304_001:0422b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後生不受生死之身。",
        "from": "T21n1304_001:0422b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲得法身。",
        "from": "T21n1304_001:0422b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內外國土怨賊。",
        "from": "T21n1304_001:0422b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切惡人一切鬼神一切盜賊虎狼師子毒蟲惡獸聞此呪聲。",
        "from": "T21n1304_001:0422b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆口噤閉不相惱亂。",
        "from": "T21n1304_001:0422c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天變致厄惡夢災殃。",
        "from": "T21n1304_001:0422c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百鳥怪異自然消滅。",
        "from": "T21n1304_001:0422c03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此呪功能不可說盡。",
        "from": "T21n1304_001:0422c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "延命印　左手捻火指空指。",
        "from": "T21n1304_001:0422c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如持蓮花。",
        "from": "T21n1304_001:0422c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手直申如垂。",
        "from": "T21n1304_001:0422c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1304_001:0422c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)摩折羅喻碎(某甲)娑嚩賀若誦此呪延命除(災)。",
        "from": "T21n1304_001:0422c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝釋天印　右手作拳安腰。",
        "from": "T21n1304_001:0422c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手直豎相著。",
        "from": "T21n1304_001:0422c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地水二指屈中節。",
        "from": "T21n1304_001:0422c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以風著火背。",
        "from": "T21n1304_001:0422c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈空中節。",
        "from": "T21n1304_001:0422c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1304_001:0422c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵因捺羅野娑嚩賀若誦此呪授與官位。",
        "from": "T21n1304_001:0422c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿足所求。",
        "from": "T21n1304_001:0422c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘沙門天印　虛心合掌。",
        "from": "T21n1304_001:0422c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "交二空並竪。",
        "from": "T21n1304_001:0422c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二風付二火側。",
        "from": "T21n1304_001:0422c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如鉤端相去一寸不著。",
        "from": "T21n1304_001:0422c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1304_001:0422c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵吠室羅縛拏野娑嚩賀若誦此呪得大富饒。",
        "from": "T21n1304_001:0422c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晝夜守護。",
        "from": "T21n1304_001:0422c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日天真言曰。",
        "from": "T21n1304_001:0422c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨羅怛曩(二合)怛羅夜(引)野曩莫索里野薩嚩曩乞灑(二合)怛羅(二合)羅若野唵(引)阿謨伽烏(名位)設底娑嚩(二合)賀月天真言曰。",
        "from": "T21n1304_001:0422c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵戰(上)怛羅(二合)曩乞灑(二合引)怛羅(二合)羅惹野(名位)設底娑嚩(二合)賀火星真言曰。",
        "from": "T21n1304_001:0422c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿誐羅嚕儗野(名位)莎訶",
        "from": "T21n1304_001:0422c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水星真言曰。",
        "from": "T21n1304_001:0422c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵母馱曩乞殺(二合)怛羅(二合)弭曩(名位)契努摩莎賀木星真言曰。",
        "from": "T21n1304_001:0422c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵婆羅(二合)訶薩鉢(二合)底曩摩比跢嚩曩師(名位)摩攞嚩羅(引)馱(二合)寧娑嚩賀金星真言曰。",
        "from": "T21n1304_001:0423a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵戌羯羅誐馱嚩(二合)羅惹野(名位)室里(二合)迦里娑嚩賀土星真言曰。",
        "from": "T21n1304_001:0423a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)捨泥殺作(二合)羅曩乞殺(二合)怛羅(二合)跛羅(二合)訶摩曩嚕波野(名位)普瑟底(二合)迦里莎訶羅睺星真言曰。",
        "from": "T21n1304_001:0423a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵羅戶曩阿素羅(二合)邏惹野塞摩捨都曩野(名位)扇底迦里娑嚩賀計都星真言曰。",
        "from": "T21n1304_001:0423a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉(二合)計都曩乞殺(二合)怛羅(二合)邏惹野(名位)娑嚩賀九執曜天印　堅固合掌。",
        "from": "T21n1304_001:0423a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二空相並直竪。",
        "from": "T21n1304_001:0423a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二風舒。",
        "from": "T21n1304_001:0423a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名本開立來去。",
        "from": "T21n1304_001:0423a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1304_001:0423a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)蘖羅(二合)係濕嚩(二合二)里耶(三)鉢羅鉢多(四)珠底(五)摩野(六)娑嚩賀二十八宿印　堅固合掌。",
        "from": "T21n1304_001:0423a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並二空直立當心。",
        "from": "T21n1304_001:0423a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二空召之。",
        "from": "T21n1304_001:0423a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1304_001:0423a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵諾乞灑(二合)怛羅(二合)爼(寧吉反)蘇那儞曳莎訶先奉供虛空藏文殊普賢延命帝釋毘沙門等菩薩天等。",
        "from": "T21n1304_001:0423a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後勸請九曜二十八宿天北斗之中本命屬星及以泰山府君司命司祿供之。",
        "from": "T21n1304_001:0423a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈乞除災延命消除厄害之事。",
        "from": "T21n1304_001:0423a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耳。",
        "from": "T21n1304_001:0423a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能吉祥真言曰。",
        "from": "T21n1304_001:0423a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨羅怛曩(二合)怛羅(二合)夜野(一)曩莫素摩薩羅嚩(二合)諾訖灑(二合)怛羅(二合)邏(引)惹(上)野者者都地波阿(去引)路迦羅野怛儞也(意)他努摩底跛努摩底薩賓(上)儞佉(上)細娑嚩賀誦此九曜息災大白衣觀音陀羅尼。",
        "from": "T21n1304_001:0423b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若日月在人本命宮中。",
        "from": "T21n1304_001:0423b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及五星在本命宮鬪戰失度。",
        "from": "T21n1304_001:0423b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可立大白衣觀音或文殊八字熾盛光佛頂等道場。",
        "from": "T21n1304_001:0423b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各依本法念誦。",
        "from": "T21n1304_001:0423b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切災難自然消散。",
        "from": "T21n1304_001:0423b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切曜不吉祥誦此真言成能吉祥。",
        "from": "T21n1304_001:0423b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北斗七星真言曰。",
        "from": "T21n1304_001:0423b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵颯跢曩而(齒)曩野伴惹密野(二合)染普他摩娑嚩(二合)弭曩(名位)羅訖山婆嚩都莎訶若羅睺計都暗行人本命星宮。",
        "from": "T21n1304_001:0423b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須誦此北斗真言。",
        "from": "T21n1304_001:0423b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來說。",
        "from": "T21n1304_001:0423b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破一切宿曜障吉祥。",
        "from": "T21n1304_001:0423b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1304_001:0423b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩羅嚩諾剎怛羅(二合)三摩曳室里曳扇底迦俱嚕娑嚩賀金剛宿成就經。",
        "from": "T21n1304_001:0423b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉祥成就品曰。",
        "from": "T21n1304_001:0423b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今宣說世間成就妙祕法。",
        "from": "T21n1304_001:0423b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是諸曜運行虛空。",
        "from": "T21n1304_001:0423b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一若二三四五等。",
        "from": "T21n1304_001:0423b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨入眾生命宿對衝宿遷移宿大殺業宿安宿薄相宿奴婢宿。",
        "from": "T21n1304_001:0423b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作諸厄害。",
        "from": "T21n1304_001:0423b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四大惡曜。",
        "from": "T21n1304_001:0423b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂火曜土曜羅睺計都最重眾生。",
        "from": "T21n1304_001:0423b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時修諸福業廣施仁慈。",
        "from": "T21n1304_001:0423b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或依文殊八字真言。",
        "from": "T21n1304_001:0423b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或依熾盛光佛頂。",
        "from": "T21n1304_001:0423b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或依被葉衣觀音。",
        "from": "T21n1304_001:0423b25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或依一字王佛頂。",
        "from": "T21n1304_001:0423b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立大息災護摩壇場。",
        "from": "T21n1304_001:0423b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各依本法念誦供養。",
        "from": "T21n1304_001:0423b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切災難自然消滅。",
        "from": "T21n1304_001:0423b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作成就法於心宿直日柳宿直日昴宿直日牛宿直日。",
        "from": "T21n1304_001:0423b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不簡日月吉凶。",
        "from": "T21n1304_001:0423b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但於此宿直日於一日之中。",
        "from": "T21n1304_001:0423b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不食念誦滿一千八十遍。",
        "from": "T21n1304_001:0423c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有心願應時便渴大悉地。",
        "from": "T21n1304_001:0423c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若厄難時。",
        "from": "T21n1304_001:0423c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於本生日最相應。",
        "from": "T21n1304_001:0423c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作木曜成就法者。",
        "from": "T21n1304_001:0423c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月十六日最為相應。",
        "from": "T21n1304_001:0423c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作火曜成就法者。",
        "from": "T21n1304_001:0423c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即彼直日最為相應。",
        "from": "T21n1304_001:0423c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作土曜成就法者。",
        "from": "T21n1304_001:0423c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每月七日最為相應。",
        "from": "T21n1304_001:0423c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作金曜成就法者。",
        "from": "T21n1304_001:0423c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨所見方平旦最為相應。",
        "from": "T21n1304_001:0423c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作水曜成就法者依水曜直日而作成就。",
        "from": "T21n1304_001:0423c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作羅睺計都法者。",
        "from": "T21n1304_001:0423c07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依本生日以為相應。",
        "from": "T21n1304_001:0423c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復供養本生宿者。",
        "from": "T21n1304_001:0423c08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於本生年月。",
        "from": "T21n1304_001:0423c09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若本生日而可取成就。",
        "from": "T21n1304_001:0423c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若惡宿生每月供養。",
        "from": "T21n1304_001:0423c10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若三日七日供養直轉成吉祥直。",
        "from": "T21n1304_001:0423c10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祕勿令知俗也。",
        "from": "T21n1304_001:0423c11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宿曜儀軌一卷",
        "from": "T21n1304_001:0423c12_0",
        "context_with_next_sentence": false
    }
]