        "from": "T20n1054_001:0069b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有受持讀誦此聖觀自在菩薩一百八名祕密明者。",
        "from": "T20n1054_001:0070b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知是人世世生生。",
        "from": "T20n1054_001:0070b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒得覩見聖觀自在。",
        "from": "T20n1054_001:0070b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若恒受持得大富貴。",
        "from": "T20n1054_001:0070b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲得聰明獲得勇猛。",
        "from": "T20n1054_001:0070b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲得端嚴獲得妙聲。",
        "from": "T20n1054_001:0070b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲得辯才。",
        "from": "T20n1054_001:0070b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲得恒知一切法義入曼拏羅。",
        "from": "T20n1054_001:0070b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡有所祈一切真言悉地成就。",
        "from": "T20n1054_001:0070b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "早晨課念永無病苦疥癩氣疾。",
        "from": "T20n1054_001:0070b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨命終時往生西方極樂世界。",
        "from": "T20n1054_001:0070b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告梵王。",
        "from": "T20n1054_001:0070b27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言若人受持六十二億恒河沙數諸佛名號。",
        "from": "T20n1054_001:0070b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復能盡形四事供養。",
        "from": "T20n1054_001:0070b28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人所獲果報多不。",
        "from": "T20n1054_001:0070c01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵王白言甚多世尊甚多善逝。",
        "from": "T20n1054_001:0070c01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言若人受持聖觀自在菩薩一百八名。",
        "from": "T20n1054_001:0070c02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至須臾禮拜供養。",
        "from": "T20n1054_001:0070c03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二人果報正等無異。",
        "from": "T20n1054_001:0070c03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵王當知受持讀誦聖觀自在菩薩一百八名。",
        "from": "T20n1054_001:0070c04_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲得如是無量無邊福報之利。",
        "from": "T20n1054_001:0070c05_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況盡形受持讀誦。",
        "from": "T20n1054_001:0070c05_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所獲功德世世生生不可窮盡。",
        "from": "T20n1054_001:0070c06_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊說是語已。",
        "from": "T20n1054_001:0070c07_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大梵天王及天帝釋。",
        "from": "T20n1054_001:0070c07_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天龍八部一切大眾。",
        "from": "T20n1054_001:0070c08_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛所說信受奉行。",
        "from": "T20n1054_001:0070c08_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖觀自在菩薩一百八名經",
        "from": "T20n1054_001:0070c09_0",
        "context_with_next_sentence": false
    }
]