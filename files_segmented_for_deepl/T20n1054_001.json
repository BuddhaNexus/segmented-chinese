[
    {
        "source_sentence": "聖觀自在菩薩一百八名經",
        "from": "T20n1054_001:0069b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宋西天中印度惹爛馱羅國三藏",
        "from": "T20n1054_001:0069b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明教大師天息災奉　詔譯",
        "from": "T20n1054_001:0069b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T20n1054_001:0069b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在補怛落迦山聖觀自在菩薩宮。",
        "from": "T20n1054_001:0069b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其山峯峙眾寶所成。",
        "from": "T20n1054_001:0069b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無垢清淨閻浮檀金。",
        "from": "T20n1054_001:0069b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩尼寶王種種珍寶。",
        "from": "T20n1054_001:0069b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙色光明常普照曜復有如意天劫波樹。",
        "from": "T20n1054_001:0069b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒時流出阿僧祇數。",
        "from": "T20n1054_001:0069b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘇羅鼻香栴檀沈水。",
        "from": "T20n1054_001:0069b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱蘇摩華柔軟適意。",
        "from": "T20n1054_001:0069b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙色芬芳處處嚴飾。",
        "from": "T20n1054_001:0069b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有無量百千萬億那由他數。",
        "from": "T20n1054_001:0069b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天龍夜叉乾闥婆阿修羅迦樓羅緊那羅摩睺羅迦人非人等往詣佛所頭面作禮。",
        "from": "T20n1054_001:0069b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養恭敬尊重讚歎。",
        "from": "T20n1054_001:0069b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心合掌寂然聽法。",
        "from": "T20n1054_001:0069b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告梵王言。",
        "from": "T20n1054_001:0069b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來說法初中後善。",
        "from": "T20n1054_001:0069b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其義深遠其語巧妙。",
        "from": "T20n1054_001:0069b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "純一無雜圓滿清淨梵行之相。",
        "from": "T20n1054_001:0069b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨宜說法利益眾生。",
        "from": "T20n1054_001:0069b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是聖觀自在菩薩一百八名。",
        "from": "T20n1054_001:0069b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有聞者百千萬億無數劫中不墮惡趣。",
        "from": "T20n1054_001:0069b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼人若有五無間業盡得消除。",
        "from": "T20n1054_001:0069b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世世生生得宿命智。",
        "from": "T20n1054_001:0069b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖觀自在菩薩一百八名祕密明曰。",
        "from": "T20n1054_001:0069b21_11",
        "context_with_next_sentence": true
    },
