[
    {
        "source_sentence": "佛說蓮華眼陀羅尼經",
        "from": "T21n1411_001:0926b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試鴻臚少卿",
        "from": "T21n1411_001:0926b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳法大師臣施護奉　詔譯",
        "from": "T21n1411_001:0926b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨(引)囉怛曩(二合)怛囉(二合)夜(引)野曩麼阿(引)哩也(二合)鉢納摩(二合)儞(引)怛囉(二合引)野怛他(引)誐哆(引)野曩謨薩哩嚩(二合)儞嚩囉拏尾瑟迦(二合)鼻尼冐地薩怛嚩(二合)野摩賀(引)薩怛嚩(二合)野怛儞也(二合)他(引)唵(引)哆囉哆囉底哩底哩覩嚕覩嚕迦羅迦羅吉隷吉隷俱嚕俱嚕婆囉婆囉鼻哩鼻哩部嚕部嚕曩野曩野俱嚕俱嚕謨乞叉(二合引)播野娑嚩(二合引)賀(引)入嚩(二合)羅曩嚩呬儞(二合)儞鼻(引)娑嚩(二合引)賀(引)薩哩嚩(二合)怛他(引)誐哆(引)底瑟恥(二合)帝(引)娑嚩(二合引)賀麼麼薩哩嚩(二合)薩怛嚩(二合)難左娑嚩(二合)賀(引)此蓮華眼陀羅尼。",
        "from": "T21n1411_001:0926b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人於其晨朝恭敬供養。",
        "from": "T21n1411_001:0926b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及念此陀羅尼一百八遍。",
        "from": "T21n1411_001:0926b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日二日乃至三七日志心持誦。",
        "from": "T21n1411_001:0926b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切罪障悉皆除滅。",
        "from": "T21n1411_001:0926b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不患其眼病耳病鼻舌身病。",
        "from": "T21n1411_001:0926b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心不邪亂。",
        "from": "T21n1411_001:0926b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復得五種眼清淨五種鼻清淨。",
        "from": "T21n1411_001:0926b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五種舌清淨五種意清淨。",
        "from": "T21n1411_001:0926b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身相端直心離垢染。",
        "from": "T21n1411_001:0926b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切生中恒發菩提心。",
        "from": "T21n1411_001:0926b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與諸聖眾常得見佛。",
        "from": "T21n1411_001:0926b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而聽妙法直至菩提道場圓滿正覺。",
        "from": "T21n1411_001:0926b27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說蓮華眼陀羅尼經",
        "from": "T21n1411_001:0926b29_0",
        "context_with_next_sentence": false
    }
]