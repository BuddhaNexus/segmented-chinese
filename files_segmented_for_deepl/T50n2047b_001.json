[
    {
        "source_sentence": "龍樹菩薩傳",
        "from": "T50n2047b_001:0185b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姚秦三藏法師鳩摩羅什譯",
        "from": "T50n2047b_001:0185b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大師名龍樹菩薩者。",
        "from": "T50n2047b_001:0185b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出南天竺梵志種也。",
        "from": "T50n2047b_001:0185b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天聰奇悟事不再告。",
        "from": "T50n2047b_001:0185b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在乳哺之中。",
        "from": "T50n2047b_001:0185b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞諸梵志誦四韋陀典各四萬偈。",
        "from": "T50n2047b_001:0185b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈有四十二字。",
        "from": "T50n2047b_001:0185b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "背誦其文而領其義。",
        "from": "T50n2047b_001:0185b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弱冠馳名獨步諸國。",
        "from": "T50n2047b_001:0185b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世學藝能天文地理圖緯祕讖。",
        "from": "T50n2047b_001:0185b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸道術無不悉練。",
        "from": "T50n2047b_001:0185b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "契友三人亦是一時之傑。",
        "from": "T50n2047b_001:0185b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相與議曰天下義理可以開神明悟幽旨者。",
        "from": "T50n2047b_001:0185b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾等盡之矣。",
        "from": "T50n2047b_001:0185b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復欲何以自娛騁情極欲。",
        "from": "T50n2047b_001:0185b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最是一生之樂。",
        "from": "T50n2047b_001:0185b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然諸梵志道士勢非王公何由得之。",
        "from": "T50n2047b_001:0185b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯有隱身之術斯樂可辦。",
        "from": "T50n2047b_001:0185b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四人相視莫逆於心。",
        "from": "T50n2047b_001:0185b22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱至術家求隱身法。",
        "from": "T50n2047b_001:0185b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "術師念曰。",
        "from": "T50n2047b_001:0185b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此四梵志擅名一世。",
        "from": "T50n2047b_001:0185b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "草芥群生今以術故屈辱就我。",
        "from": "T50n2047b_001:0185b24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我若呪法授之。",
        "from": "T50n2047b_001:0185b25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此人才明絕世所不知者唯此賤法。",
        "from": "T50n2047b_001:0185b25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若得之便去不復可屈。",
        "from": "T50n2047b_001:0185b26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且與其藥使日用而不知。",
        "from": "T50n2047b_001:0185b27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥盡必來求。",
        "from": "T50n2047b_001:0185b27_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可以術屈為我弟子。",
        "from": "T50n2047b_001:0185b28_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各與青藥一丸告之曰。",
        "from": "T50n2047b_001:0185b28_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝於靜處用水磨之以塗眼瞼。",
        "from": "T50n2047b_001:0185b29_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則無有人能見汝形者。",
        "from": "T50n2047b_001:0185b29_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍樹菩薩磨藥聞氣。",
        "from": "T50n2047b_001:0185c01_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便盡知藥名分數。",
        "from": "T50n2047b_001:0185c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多少錙銖無失。",
        "from": "T50n2047b_001:0185c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨其氣勢龍樹識之。",
        "from": "T50n2047b_001:0185c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還語術師此藥有七十種。",
        "from": "T50n2047b_001:0185c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分數多少盡如其方。",
        "from": "T50n2047b_001:0185c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥師問曰汝何由知。",
        "from": "T50n2047b_001:0185c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰藥自有氣何以不知。",
        "from": "T50n2047b_001:0185c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師即歎伏。",
        "from": "T50n2047b_001:0185c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顧斯人者聞之猶難。",
        "from": "T50n2047b_001:0185c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而況相學。",
        "from": "T50n2047b_001:0185c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我之賤術何足惜耶。",
        "from": "T50n2047b_001:0185c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即具授。",
        "from": "T50n2047b_001:0185c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其四人得術隱身自在入王宮中。",
        "from": "T50n2047b_001:0185c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宮中美人皆被侵陵百餘日。",
        "from": "T50n2047b_001:0185c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後宮中人有懷妊者以事白王。",
        "from": "T50n2047b_001:0185c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王大不悅。",
        "from": "T50n2047b_001:0185c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此何不祥為怪乃爾。",
        "from": "T50n2047b_001:0185c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "召諸智臣以謀此事。",
        "from": "T50n2047b_001:0185c10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有舊老者言凡如此事應有二種。",
        "from": "T50n2047b_001:0185c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或鬼或術。",
        "from": "T50n2047b_001:0185c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可以細土置諸門中。",
        "from": "T50n2047b_001:0185c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令有司守之斷諸術者。",
        "from": "T50n2047b_001:0185c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若是術人足跡自現可以兵除。",
        "from": "T50n2047b_001:0185c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若其是鬼則無跡也。",
        "from": "T50n2047b_001:0185c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼可呪除人可刀殺。",
        "from": "T50n2047b_001:0185c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "備法試之見四人跡。",
        "from": "T50n2047b_001:0185c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即閉諸門令數百力士。",
        "from": "T50n2047b_001:0185c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "揮刀空斫斫殺三人。",
        "from": "T50n2047b_001:0185c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯有龍樹斂身屏氣依王頭側。",
        "from": "T50n2047b_001:0185c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王頭側七尺刀所不至。",
        "from": "T50n2047b_001:0185c17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時始悟欲為苦本。",
        "from": "T50n2047b_001:0185c17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "厭欲心生發出家願。",
        "from": "T50n2047b_001:0185c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若我得脫。",
        "from": "T50n2047b_001:0185c18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當詣沙門求出家法。",
        "from": "T50n2047b_001:0185c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既而得出入山詣佛塔出家受戒。",
        "from": "T50n2047b_001:0185c19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九十日中誦三藏。",
        "from": "T50n2047b_001:0185c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡通諸深義。",
        "from": "T50n2047b_001:0185c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更求諸經都無得處。",
        "from": "T50n2047b_001:0185c21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雪山中深遠處有佛塔。",
        "from": "T50n2047b_001:0185c21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塔中有一老比丘。",
        "from": "T50n2047b_001:0185c22_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以摩訶衍經與之。",
        "from": "T50n2047b_001:0185c22_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦受愛樂雖知實義未得通利。",
        "from": "T50n2047b_001:0185c23_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周遊諸國更求餘經。",
        "from": "T50n2047b_001:0185c23_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於閻浮提中遍求不得。",
        "from": "T50n2047b_001:0185c24_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外道論師沙門義宗咸皆摧伏。",
        "from": "T50n2047b_001:0185c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即起憍慢心。",
        "from": "T50n2047b_001:0185c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自念言世界法中津塗甚多。",
        "from": "T50n2047b_001:0186a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛經雖妙以理推之故未盡。",
        "from": "T50n2047b_001:0186a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未盡之中可推。",
        "from": "T50n2047b_001:0186a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而說之以悟後學。",
        "from": "T50n2047b_001:0186a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於理不違於事無失。",
        "from": "T50n2047b_001:0186a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斯有何咎。",
        "from": "T50n2047b_001:0186a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思此事已。",
        "from": "T50n2047b_001:0186a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即欲行之立師教誡更造衣服。",
        "from": "T50n2047b_001:0186a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今附佛法所別為異。",
        "from": "T50n2047b_001:0186a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方欲以無所推屈表一切智相。",
        "from": "T50n2047b_001:0186a05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擇日選時當與諸弟子。",
        "from": "T50n2047b_001:0186a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受新戒著新衣便欲行之。",
        "from": "T50n2047b_001:0186a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獨在靜室水精地房。",
        "from": "T50n2047b_001:0186a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大龍菩薩見其如此。",
        "from": "T50n2047b_001:0186a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惜而愍之即接入海。",
        "from": "T50n2047b_001:0186a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於宮殿中開七寶藏發七寶函。",
        "from": "T50n2047b_001:0186a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以諸方等深奧經典無上妙法授之龍樹。",
        "from": "T50n2047b_001:0186a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍樹受讀九十日中通練甚多。",
        "from": "T50n2047b_001:0186a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其心深入體得實利。",
        "from": "T50n2047b_001:0186a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍知其心而問之曰。",
        "from": "T50n2047b_001:0186a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "看經遍未。",
        "from": "T50n2047b_001:0186a12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答言汝諸函中經甚多無量不可盡也。",
        "from": "T50n2047b_001:0186a12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我所讀者已十倍閻浮提。",
        "from": "T50n2047b_001:0186a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍言如我宮中所有經典。",
        "from": "T50n2047b_001:0186a14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸處此比復不可知。",
        "from": "T50n2047b_001:0186a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍樹即得諸經一箱。",
        "from": "T50n2047b_001:0186a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深入無生三忍具足。",
        "from": "T50n2047b_001:0186a16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍還送出。",
        "from": "T50n2047b_001:0186a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時南天竺王甚邪見承事外道毀謗正法。",
        "from": "T50n2047b_001:0186a16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍樹菩薩為化彼故躬持赤旛在王前行。",
        "from": "T50n2047b_001:0186a17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經歷七年王始怪問。",
        "from": "T50n2047b_001:0186a18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是何人在我前行。",
        "from": "T50n2047b_001:0186a19_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰我是一切智人。",
        "from": "T50n2047b_001:0186a19_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王聞是已甚大驚愕而問之言。",
        "from": "T50n2047b_001:0186a20_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切智人曠代不有。",
        "from": "T50n2047b_001:0186a21_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝自言是何以驗之。",
        "from": "T50n2047b_001:0186a21_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答言欲知智在說王當見問。",
        "from": "T50n2047b_001:0186a22_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王即自念我為智主大論議師。",
        "from": "T50n2047b_001:0186a22_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問之能屈猶不足名。",
        "from": "T50n2047b_001:0186a23_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一旦不如此非小事。",
        "from": "T50n2047b_001:0186a24_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若其不問便是一屈遲疑良久。",
        "from": "T50n2047b_001:0186a24_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得已而問之。",
        "from": "T50n2047b_001:0186a25_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天今何為耶。",
        "from": "T50n2047b_001:0186a25_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍樹言天今與阿脩羅戰。",
        "from": "T50n2047b_001:0186a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王聞此言譬如人噎。",
        "from": "T50n2047b_001:0186a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既不得吐又不得咽。",
        "from": "T50n2047b_001:0186a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲非其言復無以證之。",
        "from": "T50n2047b_001:0186a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲是其事無事可明。",
        "from": "T50n2047b_001:0186a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未言之間龍樹復言。",
        "from": "T50n2047b_001:0186a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此非虛論求勝之談。",
        "from": "T50n2047b_001:0186a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王小待之須臾有驗。",
        "from": "T50n2047b_001:0186a29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言訖空中便有干戈兵器相係而落。",
        "from": "T50n2047b_001:0186b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王言干戈矛戟雖是戰器。",
        "from": "T50n2047b_001:0186b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝何必知是天與阿脩羅戰。",
        "from": "T50n2047b_001:0186b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍樹言搆之虛言不如校以實事。",
        "from": "T50n2047b_001:0186b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言已阿脩羅手足指及其耳鼻從空而下。",
        "from": "T50n2047b_001:0186b04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又令王及臣民婆羅門眾。",
        "from": "T50n2047b_001:0186b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見空中清除兩陣相對。",
        "from": "T50n2047b_001:0186b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王乃稽首伏其法化。",
        "from": "T50n2047b_001:0186b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殿上有萬婆羅門。",
        "from": "T50n2047b_001:0186b06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆棄束髮受成就戒。",
        "from": "T50n2047b_001:0186b07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時龍樹．於南天竺大弘佛教。",
        "from": "T50n2047b_001:0186b07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摧伏外道廣明摩訶衍。",
        "from": "T50n2047b_001:0186b08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作優波提舍十萬偈。",
        "from": "T50n2047b_001:0186b09_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又作莊嚴佛道論五千偈。",
        "from": "T50n2047b_001:0186b09_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大慈方便論五十偈。",
        "from": "T50n2047b_001:0186b10_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令摩訶衍教大行於天竺。",
        "from": "T50n2047b_001:0186b10_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又造無畏論十萬偈。",
        "from": "T50n2047b_001:0186b11_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於無畏中出中論也。",
        "from": "T50n2047b_001:0186b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時有婆羅門善知呪術。",
        "from": "T50n2047b_001:0186b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲以所能與龍樹諍勝告天竺國王。",
        "from": "T50n2047b_001:0186b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我能伏此比丘王當驗之。",
        "from": "T50n2047b_001:0186b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王言汝大愚人。",
        "from": "T50n2047b_001:0186b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此菩薩者明與日月爭光。",
        "from": "T50n2047b_001:0186b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智與聖心並照。",
        "from": "T50n2047b_001:0186b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝何不遜敢不推敬。",
        "from": "T50n2047b_001:0186b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婆羅門言。",
        "from": "T50n2047b_001:0186b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王為智人何不以理驗之。",
        "from": "T50n2047b_001:0186b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而抑斷一切。",
        "from": "T50n2047b_001:0186b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王見言至為請龍樹。",
        "from": "T50n2047b_001:0186b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清旦共坐政德殿上。",
        "from": "T50n2047b_001:0186b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婆羅門後至便於殿前呪作大池。",
        "from": "T50n2047b_001:0186b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣長清淨中有千葉蓮華。",
        "from": "T50n2047b_001:0186b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自坐其上而訶龍樹。",
        "from": "T50n2047b_001:0186b20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝在地坐如畜生無異。",
        "from": "T50n2047b_001:0186b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而欲與我清淨華上大德智人抗言論議。",
        "from": "T50n2047b_001:0186b21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時龍樹亦以呪術化作一六牙白象。",
        "from": "T50n2047b_001:0186b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行池水上趣其華坐以鼻繳拔高舉擲地。",
        "from": "T50n2047b_001:0186b23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婆羅門傷腰委頓。",
        "from": "T50n2047b_001:0186b24_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命龍樹我不自量毀辱大師。",
        "from": "T50n2047b_001:0186b24_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願哀受我啟其愚蒙。",
        "from": "T50n2047b_001:0186b25_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有一小乘法師。",
        "from": "T50n2047b_001:0186b25_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常懷忿嫉。",
        "from": "T50n2047b_001:0186b26_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍樹問之言汝樂我久住世不。",
        "from": "T50n2047b_001:0186b26_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答言實不願也。",
        "from": "T50n2047b_001:0186b27_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "退入閑室經日不出。",
        "from": "T50n2047b_001:0186b27_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弟子破戶看之遂蟬蛻而去。",
        "from": "T50n2047b_001:0186b28_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "去世已來始過百歲。",
        "from": "T50n2047b_001:0186b28_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南天竺諸國。",
        "from": "T50n2047b_001:0186b29_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為其立廟敬奉如佛。",
        "from": "T50n2047b_001:0186b29_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其母樹下生之。",
        "from": "T50n2047b_001:0186c01_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因字阿周陀那。",
        "from": "T50n2047b_001:0186c01_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿周陀那樹名也。",
        "from": "T50n2047b_001:0186c01_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以龍成其道故以龍配字號曰龍樹也(依付法藏經即第十三祖三百餘年任持佛法)。",
        "from": "T50n2047b_001:0186c02_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍樹菩薩傳(終)",
        "from": "T50n2047b_001:0186c04_0",
        "context_with_next_sentence": false
    }
]