[
    {
        "source_sentence": "大乘集菩薩學論卷第十五",
        "from": "T32n1636_015:0118a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫",
        "from": "T32n1636_015:0118a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "試鴻臚少卿宣梵大師賜紫",
        "from": "T32n1636_015:0118a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙門臣日稱等奉　詔譯",
        "from": "T32n1636_015:0118a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "治心品第十二之二(亦名禪定波羅蜜多品)復有佛緣菩薩緣聲聞獨覺緣眾生緣等。",
        "from": "T32n1636_015:0118a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼眾生緣者。",
        "from": "T32n1636_015:0118a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂先樂與利益安樂。",
        "from": "T32n1636_015:0118a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以禪定意與彼慈故。",
        "from": "T32n1636_015:0118a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有知識客寄近住。",
        "from": "T32n1636_015:0118a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以己聚落如他聚落。",
        "from": "T32n1636_015:0118a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至於此一方解脫。",
        "from": "T32n1636_015:0118a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是汝緣十方佛等亦無厭怠。",
        "from": "T32n1636_015:0118a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如金剛幢迴向中說。",
        "from": "T32n1636_015:0118a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足修習菩薩行者。",
        "from": "T32n1636_015:0118a28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若所見色對治於愛。",
        "from": "T32n1636_015:0118a29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是乃至聲香味觸法對治於愛。",
        "from": "T32n1636_015:0118a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則無罪犯。",
        "from": "T32n1636_015:0118b01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是淨是善。",
        "from": "T32n1636_015:0118b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最上光潔超諸妙樂。",
        "from": "T32n1636_015:0118b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生適悅意起淨信樂。",
        "from": "T32n1636_015:0118b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發生踊躍安住極喜。",
        "from": "T32n1636_015:0118b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於分別心不生退惱。",
        "from": "T32n1636_015:0118b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心意調柔諸根猛利常受妙樂。",
        "from": "T32n1636_015:0118b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是迴向一切諸佛。",
        "from": "T32n1636_015:0118b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉復迴向諸佛般若。",
        "from": "T32n1636_015:0118b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而得具足不可思議佛妙樂行。",
        "from": "T32n1636_015:0118b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然未同佛得善攝受三摩地樂。",
        "from": "T32n1636_015:0118b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由是力能展轉得佛無量妙樂。",
        "from": "T32n1636_015:0118b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足無量佛解脫樂。",
        "from": "T32n1636_015:0118b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝受無量佛神通樂。",
        "from": "T32n1636_015:0118b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得善攝受無數諸佛無著行樂。",
        "from": "T32n1636_015:0118b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得佛自在如彼牛王所至遠近速疾妙樂。",
        "from": "T32n1636_015:0118b09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畢竟獲得無量佛力了知一切寂靜無生無滅等樂。",
        "from": "T32n1636_015:0118b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於無著行而常等引得諸如來不二集行無瞋妙樂。",
        "from": "T32n1636_015:0118b11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為菩薩常以如是善根迴向如來。",
        "from": "T32n1636_015:0118b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迴向菩薩者。",
        "from": "T32n1636_015:0118b13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於意樂未圓滿者。",
        "from": "T32n1636_015:0118b13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迴向圓滿於一切智。",
        "from": "T32n1636_015:0118b14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未清淨者。",
        "from": "T32n1636_015:0118b14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深心清淨一切波羅蜜多。",
        "from": "T32n1636_015:0118b14_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未成辦者。",
        "from": "T32n1636_015:0118b15_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為令成辦發菩提心。",
        "from": "T32n1636_015:0118b15_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶若金剛令一切智鎧無有退屈。",
        "from": "T32n1636_015:0118b16_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若令一切智鎧無退屈者。",
        "from": "T32n1636_015:0118b16_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則於菩提妙樂隨所輕安。",
        "from": "T32n1636_015:0118b17_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸善道無所退向。",
        "from": "T32n1636_015:0118b18_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平等安住一切世間。",
        "from": "T32n1636_015:0118b18_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓滿大願修習一切菩薩之行。",
        "from": "T32n1636_015:0118b19_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了知菩薩利根神通。",
        "from": "T32n1636_015:0118b19_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由斯善根證一切智。",
        "from": "T32n1636_015:0118b20_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以如是善根義故。",
        "from": "T32n1636_015:0118b20_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迴向菩薩。",
        "from": "T32n1636_015:0118b21_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又若學佛教者。",
        "from": "T32n1636_015:0118b21_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切聲聞辟支佛等。",
        "from": "T32n1636_015:0118b21_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦以善根如是迴向。",
        "from": "T32n1636_015:0118b22_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生一彈指頃聞佛法聲。",
        "from": "T32n1636_015:0118b22_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及賢聖眾為作尊重。",
        "from": "T32n1636_015:0118b23_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此善根迴向阿耨多羅三藐三菩提。",
        "from": "T32n1636_015:0118b23_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂念佛圓滿迴向。",
        "from": "T32n1636_015:0118b24_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念法方便迴向。",
        "from": "T32n1636_015:0118b25_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊重聖眾迴向。",
        "from": "T32n1636_015:0118b25_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不離見佛迴向。",
        "from": "T32n1636_015:0118b25_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清淨心迴向。",
        "from": "T32n1636_015:0118b26_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通達佛法迴向。",
        "from": "T32n1636_015:0118b26_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所修無量功德迴向。",
        "from": "T32n1636_015:0118b26_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清淨一切神通善根迴向。",
        "from": "T32n1636_015:0118b27_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷諸法疑迴向。",
        "from": "T32n1636_015:0118b27_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為於聲聞辟支佛等學佛教者之所迴向。",
        "from": "T32n1636_015:0118b28_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又彼菩薩亦以善根迴向一切眾生。",
        "from": "T32n1636_015:0118b29_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂遠離地獄道迴向。",
        "from": "T32n1636_015:0118c01_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷除畜生道迴向。",
        "from": "T32n1636_015:0118c01_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與琰摩羅界妙樂迴向。",
        "from": "T32n1636_015:0118c02_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷除墮諸惡趣迴向。",
        "from": "T32n1636_015:0118c02_54",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼眾生樂欲增長無上菩提迴向。",
        "from": "T32n1636_015:0118c03_55",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為令深心得一切智心迴向。",
        "from": "T32n1636_015:0118c03_56",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不謗一切佛法迴向。",
        "from": "T32n1636_015:0118c04_57",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畢竟成就一切智地迴向。",
        "from": "T32n1636_015:0118c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畢竟清淨一切眾生迴向。",
        "from": "T32n1636_015:0118c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以無量智迴向一切眾生。",
        "from": "T32n1636_015:0118c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至所有飲食衣服臥具病緣醫藥。",
        "from": "T32n1636_015:0118c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於身承事去來坐立。",
        "from": "T32n1636_015:0118c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種作業住律儀道。",
        "from": "T32n1636_015:0118c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於律儀道不生瞋恚。",
        "from": "T32n1636_015:0118c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身語意業如善修作。",
        "from": "T32n1636_015:0118c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密緻諸根而或自身消息。",
        "from": "T32n1636_015:0118c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "按摩等事。",
        "from": "T32n1636_015:0118c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾味飲食。",
        "from": "T32n1636_015:0118c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若開若合。",
        "from": "T32n1636_015:0118c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若觀不觀。",
        "from": "T32n1636_015:0118c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若臥若覺。",
        "from": "T32n1636_015:0118c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而復自身詣彼承事。",
        "from": "T32n1636_015:0118c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩於一切智緣平等相應者無不迴向。",
        "from": "T32n1636_015:0118c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼一切智心樂利樂一切眾生。",
        "from": "T32n1636_015:0118c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至意常救護一切世間。",
        "from": "T32n1636_015:0118c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發起善根遠離憍逸。",
        "from": "T32n1636_015:0118c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以要言之。",
        "from": "T32n1636_015:0118c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸煩惱不復現前。",
        "from": "T32n1636_015:0118c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸菩薩之所守護。",
        "from": "T32n1636_015:0118c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "決定勤求一切智道。",
        "from": "T32n1636_015:0118c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛樂親近諸有智者。",
        "from": "T32n1636_015:0118c16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至而能修習一切善根。",
        "from": "T32n1636_015:0118c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速得圓滿相續諸行。",
        "from": "T32n1636_015:0118c17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以要言之。",
        "from": "T32n1636_015:0118c17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼畜生趣者。",
        "from": "T32n1636_015:0118c17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或以少飯而能施與。",
        "from": "T32n1636_015:0118c18_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼一切得生善趣。",
        "from": "T32n1636_015:0118c18_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是利益解脫迴向。",
        "from": "T32n1636_015:0118c19_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼畜生者。",
        "from": "T32n1636_015:0118c19_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為苦海。",
        "from": "T32n1636_015:0118c19_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是苦所取。",
        "from": "T32n1636_015:0118c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為苦蘊。",
        "from": "T32n1636_015:0118c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名苦受。",
        "from": "T32n1636_015:0118c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為苦聚。",
        "from": "T32n1636_015:0118c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是苦遷流。",
        "from": "T32n1636_015:0118c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是苦邊際。",
        "from": "T32n1636_015:0118c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是苦根本。",
        "from": "T32n1636_015:0118c21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是苦依處。",
        "from": "T32n1636_015:0118c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彼眾生不退迴向。",
        "from": "T32n1636_015:0118c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又彼一切眾生緣者。",
        "from": "T32n1636_015:0118c22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於眾生現前作意。",
        "from": "T32n1636_015:0118c22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由先所作善根發菩提心迴向一切智故。",
        "from": "T32n1636_015:0118c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不墮輪迴離諸險難得佛妙樂現前無礙超輪迴海永斷相續照明佛法起大慈故。",
        "from": "T32n1636_015:0118c24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如最勝金光明經慈悲藏伽陀云。",
        "from": "T32n1636_015:0118c26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　是金光明　　最勝金鼓　　所出妙聲　遍三千界　　能息地獄　　及焰摩羅　乃至人中　　貧窮苦等　　又此金鼓　滅除世間　　一切障惱　　亦願眾生　得除恐怖　　如牟尼尊　　寂靜無畏　如諸聖人　　處生死海　　修一切智　如彼所得　　勝慧等持　　及菩提分　功德大海　　又此鼓音　　願諸眾生　獲斯梵響　　如佛證已　　勝妙菩提　轉淨法輪　　住無數劫　　說法利生　滅貪瞋癡　　煩惱眾苦　　若有眾生　住於惡道　　為火所燒　　身常洞然　聞斯鼓音　　如教誨言　　令歸佛寶　又令眾生　　得宿住念　　於百千億　俱胝生中　　念牢尼尊　　聞甚深教　又此鼓音　　常得近佛　　淨脩善行　遠離罪惡　　乃至偈云　　又諸剎土　一切眾生　　息除世間　　所有諸苦　又若眾生　　身分下劣　　根缺減者　普得諸根　　悉皆圓滿　　若疾病者　身形怯弱　　隨其方處　　無所依怙　解脫眾病　　得獲輕安　　根力充實　又若眾生　　王法所加　　生種種怖　及多憂惱　　彼諸眾生　　苦難若至　最極惡事　　百種怖畏　　悉令解脫　或復捶打　　枷鎖繫縛　　種種苦具　逼切其身　　又復無量　　百千種類　憂畏愁歎　　間擾其心　　牢獄繫閉　打撻楚毒　　一切解脫　　被臨刑者　遽活軀命　　眾苦雖至　　得無怖懼　又若眾生　　飢渴逼惱　　令得上味　美膳飲食　　盲者備覩　　眾妙色相　聾者得聞　　適意音響　　彼裸形者　得細軟衣　　貧窶眾生　　得眾伏藏　倉庫豐饒　　眾寶嚴飾　　一切眾生　獲斯妙樂　　無一眾生　　受諸苦報　諸相端嚴　　人所樂見　　飲膳豐饒　福德具足　　常得受用　　無量妙樂　箜篌簫笛　　眾妙音聲　　隨彼心念　應時現前　　念水即現　　清淨池沼　金色蓮華　　及優鉢羅　　遍覆其上　隨彼心念　　應時現前　　乃至偈云　塗香花鬘　　和合末香　　每日三時　從樹而雨　　眾生取已　　咸生喜悅　供養十方　　不可思議　　一切如來　菩提妙法　　諸大菩薩　　遠塵離垢　諸聲聞眾　　得離一切　　卑下種族　及八難處　　常值無難　　最勝中王　常得親近　　諸佛如來　　乃至偈云　願諸女人　　得為男子　　剛健勇猛　智慧聰利　　而常修行　　諸菩提道　六波羅蜜　　見十方佛　　在寶樹下　處琉璃座　　安隱快樂　　圍繞恭敬　聞說妙法論曰。",
        "from": "T32n1636_015:0118c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此略明慈行對治於瞋。",
        "from": "T32n1636_015:0119b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以緣生觀對治於癡。",
        "from": "T32n1636_015:0119b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今當說是中緣生義。",
        "from": "T32n1636_015:0119b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有稻稈經說。",
        "from": "T32n1636_015:0119b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何內緣生法繫屬於因。",
        "from": "T32n1636_015:0119b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂無明緣行乃至生緣老死若無無明則知無有行。",
        "from": "T32n1636_015:0119b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至若無有生則知無有老死。",
        "from": "T32n1636_015:0119b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又若實無明滅則行滅。",
        "from": "T32n1636_015:0119b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是乃至生滅則老死滅。",
        "from": "T32n1636_015:0119b27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又彼無明不念我從行起。",
        "from": "T32n1636_015:0119b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行亦不念我從無明得起。",
        "from": "T32n1636_015:0119b29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是乃至生亦不念我從老死得起。",
        "from": "T32n1636_015:0119b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "老死亦不念我從生得起若實無明起者。",
        "from": "T32n1636_015:0119c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則行起不可得。",
        "from": "T32n1636_015:0119c02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是乃至生起則老死起亦不可得。",
        "from": "T32n1636_015:0119c02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應知是為內緣生法繫屬於因。",
        "from": "T32n1636_015:0119c03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復何內緣生法繫屬於緣。",
        "from": "T32n1636_015:0119c04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂地水火風空識等界。",
        "from": "T32n1636_015:0119c04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和合於內繫屬於緣。",
        "from": "T32n1636_015:0119c05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何地界。",
        "from": "T32n1636_015:0119c05_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂令此身聚集而轉。",
        "from": "T32n1636_015:0119c06_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅硬為性。",
        "from": "T32n1636_015:0119c06_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說為地界。",
        "from": "T32n1636_015:0119c06_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又若令身攝持造作。",
        "from": "T32n1636_015:0119c07_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說為水界。",
        "from": "T32n1636_015:0119c07_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又若令身煖性現前飲食成熟。",
        "from": "T32n1636_015:0119c07_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說為火界。",
        "from": "T32n1636_015:0119c08_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又若於身造出入息。",
        "from": "T32n1636_015:0119c08_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說為風界。",
        "from": "T32n1636_015:0119c09_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又若於身中有竅隙。",
        "from": "T32n1636_015:0119c09_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說為空界。",
        "from": "T32n1636_015:0119c09_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂此名色猶如交蘆。",
        "from": "T32n1636_015:0119c10_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令五識身相應繫屬。",
        "from": "T32n1636_015:0119c10_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有漏意識。",
        "from": "T32n1636_015:0119c11_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說為識界。",
        "from": "T32n1636_015:0119c11_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若非此緣者。",
        "from": "T32n1636_015:0119c11_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則身不轉。",
        "from": "T32n1636_015:0119c11_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若內不闕地界。",
        "from": "T32n1636_015:0119c12_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是水火風空識界亦無闕減。",
        "from": "T32n1636_015:0119c12_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼彼和合則身定轉故。",
        "from": "T32n1636_015:0119c13_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然此地界不念我能令身堅聚。",
        "from": "T32n1636_015:0119c13_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水界不念我能令身攝持造作。",
        "from": "T32n1636_015:0119c14_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火界不念我能令身溫煖飲食成熟。",
        "from": "T32n1636_015:0119c15_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風界不念我能於身作出入息。",
        "from": "T32n1636_015:0119c15_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空界不念我能於身中成竅隙。",
        "from": "T32n1636_015:0119c16_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識界不念我為緣生由是緣故身定得轉。",
        "from": "T32n1636_015:0119c17_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此地界無我無人無眾生無壽者無意生無儒童非男非女亦非非男亦非非女無自在無主宰無此無彼及無所有。",
        "from": "T32n1636_015:0119c18_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是水火風空識界亦復無我無人無眾生無壽者無意生無儒童非男非女亦非非男亦非非女無自在無主宰無此無彼及無所有。",
        "from": "T32n1636_015:0119c20_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次無明者。",
        "from": "T32n1636_015:0119c23_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是義云何。",
        "from": "T32n1636_015:0119c24_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於此六界之中。",
        "from": "T32n1636_015:0119c24_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起一想合想堅固想常樂我淨想眾生壽者意生儒童想自在主宰等想。",
        "from": "T32n1636_015:0119c24_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是種種無智。",
        "from": "T32n1636_015:0119c26_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說為無明。",
        "from": "T32n1636_015:0119c26_54",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此無明謂實有境起貪瞋癡。",
        "from": "T32n1636_015:0119c27_55",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即彼貪瞋癡境。",
        "from": "T32n1636_015:0119c27_56",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此說為行。",
        "from": "T32n1636_015:0119c28_57",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於事表了。",
        "from": "T32n1636_015:0119c28_58",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此說為識。",
        "from": "T32n1636_015:0119c28_59",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識俱生時彼四取蘊。",
        "from": "T32n1636_015:0119c28_60",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說為名色。",
        "from": "T32n1636_015:0119c29_61",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此名色所依諸根。",
        "from": "T32n1636_015:0119c29_62",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說為六處。",
        "from": "T32n1636_015:0120a01_63",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三法和合。",
        "from": "T32n1636_015:0120a01_64",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此說為觸。",
        "from": "T32n1636_015:0120a01_65",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由觸領納。",
        "from": "T32n1636_015:0120a01_66",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此說為受。",
        "from": "T32n1636_015:0120a02_67",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受耽著故。",
        "from": "T32n1636_015:0120a02_68",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此說名愛。",
        "from": "T32n1636_015:0120a02_69",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛深廣故。",
        "from": "T32n1636_015:0120a02_70",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說名為取。",
        "from": "T32n1636_015:0120a03_71",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取能生有。",
        "from": "T32n1636_015:0120a03_72",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有復生因。",
        "from": "T32n1636_015:0120a03_73",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由是蘊起名生。",
        "from": "T32n1636_015:0120a03_74",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘊熟為老。",
        "from": "T32n1636_015:0120a04_75",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘊滅名死。",
        "from": "T32n1636_015:0120a04_76",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "癡瞋染著畏死名憂。",
        "from": "T32n1636_015:0120a04_77",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "追感傷歎名悲。",
        "from": "T32n1636_015:0120a05_78",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五識相應不常領納名苦。",
        "from": "T32n1636_015:0120a05_79",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作意和合極苦名惱。",
        "from": "T32n1636_015:0120a06_80",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是引生隨煩惱等。",
        "from": "T32n1636_015:0120a06_81",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至總略。",
        "from": "T32n1636_015:0120a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大乘集菩薩學論卷第十五",
        "from": "T32n1636_015:0120a08_0",
        "context_with_next_sentence": false
    }
]