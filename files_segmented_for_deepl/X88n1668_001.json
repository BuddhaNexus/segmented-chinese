[
    {
        "source_sentence": "摘錄聖武記之卷五溯查西藏剌麻",
        "from": "X88n1668_001:0567b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來源",
        "from": "X88n1668_001:0567b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吳中沙門　守一　編輯",
        "from": "X88n1668_001:0567b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "按西藏。",
        "from": "X88n1668_001:0567b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在於甘肅嘉峪關外。",
        "from": "X88n1668_001:0567b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繞東之南。",
        "from": "X88n1668_001:0567b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有祁連山。",
        "from": "X88n1668_001:0567b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "山之南有蒙古之青海。",
        "from": "X88n1668_001:0567b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "青海之西南。",
        "from": "X88n1668_001:0567b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即藏之東北界。",
        "from": "X88n1668_001:0567b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四川雅州之西。",
        "from": "X88n1668_001:0567b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "松潘廳之打箭爐。",
        "from": "X88n1668_001:0567b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出塞千數百里。",
        "from": "X88n1668_001:0567b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即藏之東界。",
        "from": "X88n1668_001:0567b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為官長往來之正驛。",
        "from": "X88n1668_001:0567b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此官站之大道也。",
        "from": "X88n1668_001:0567b12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後兩藏。",
        "from": "X88n1668_001:0567b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東西六千餘里。",
        "from": "X88n1668_001:0567b13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南北三千餘里。",
        "from": "X88n1668_001:0567b13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "距京師一萬四千餘里。",
        "from": "X88n1668_001:0567b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藏之西北界大戈璧。",
        "from": "X88n1668_001:0567b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東南界雲南金沙江。",
        "from": "X88n1668_001:0567b14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南界緬甸。",
        "from": "X88n1668_001:0567b14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西南界廓爾噶國。",
        "from": "X88n1668_001:0567b15_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自漢唐即有烏斯藏。",
        "from": "X88n1668_001:0567b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即今西藏。",
        "from": "X88n1668_001:0567b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名異地同。",
        "from": "X88n1668_001:0567b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藏之西南界外。",
        "from": "X88n1668_001:0567b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為廓爾喀國。",
        "from": "X88n1668_001:0567b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廓爾喀之西。",
        "from": "X88n1668_001:0567b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即為東印度國。",
        "from": "X88n1668_001:0567b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東印度之西。",
        "from": "X88n1668_001:0567b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃中印度。",
        "from": "X88n1668_001:0567b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即古佛國也。",
        "from": "X88n1668_001:0567b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剌麻者。",
        "from": "X88n1668_001:0567b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃西域之尊稱。",
        "from": "X88n1668_001:0567b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譯華言為無上二字。",
        "from": "X88n1668_001:0567b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比中國之稱和尚。",
        "from": "X88n1668_001:0567b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為力生二字也。",
        "from": "X88n1668_001:0567b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自唐太宗時。",
        "from": "X88n1668_001:0567b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文成公主。",
        "from": "X88n1668_001:0567b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下嫁與烏斯藏汗。",
        "from": "X88n1668_001:0567b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "贊普為妃。",
        "from": "X88n1668_001:0567b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "專尚佛教。",
        "from": "X88n1668_001:0567b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "興建大塔寺廟數百坐。",
        "from": "X88n1668_001:0567b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "延請印度高行剌麻到藏焚修教化。",
        "from": "X88n1668_001:0567b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歷宋元明清。",
        "from": "X88n1668_001:0567b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "均有高行剌麻。",
        "from": "X88n1668_001:0567b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故元世祖封剌麻八思巴為帝師。",
        "from": "X88n1668_001:0567b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大寶法王。",
        "from": "X88n1668_001:0567b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後嗣世襲其號。",
        "from": "X88n1668_001:0567b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為紅教之宗。",
        "from": "X88n1668_001:0567c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至明永樂成祖。",
        "from": "X88n1668_001:0567c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "封剌麻哈立麻。",
        "from": "X88n1668_001:0567c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為大寶法王。",
        "from": "X88n1668_001:0567c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天大善自在佛。",
        "from": "X88n1668_001:0567c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其徒三人皆封國師。",
        "from": "X88n1668_001:0567c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又封大乘法王。",
        "from": "X88n1668_001:0567c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大德法王。",
        "from": "X88n1668_001:0567c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又封闡化　闡教　輔教　護教　贊善五法王。",
        "from": "X88n1668_001:0567c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又授西天佛子者(二)。",
        "from": "X88n1668_001:0567c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灌頂大國師者(九)。",
        "from": "X88n1668_001:0567c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灌頂國師者(十八)。",
        "from": "X88n1668_001:0567c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法王等命終。",
        "from": "X88n1668_001:0567c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其徒輒自相承襲。",
        "from": "X88n1668_001:0567c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漸後變為旁門。",
        "from": "X88n1668_001:0567c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爭名奪地。",
        "from": "X88n1668_001:0567c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而諸法王之後。",
        "from": "X88n1668_001:0567c07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先娶妻生子。",
        "from": "X88n1668_001:0567c07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "再落髮為剌麻。",
        "from": "X88n1668_001:0567c07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "候前剌麻命終。",
        "from": "X88n1668_001:0567c08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則其子頂替。",
        "from": "X88n1668_001:0567c08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "私襲其位。",
        "from": "X88n1668_001:0567c08_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與所娶之妻。",
        "from": "X88n1668_001:0567c08_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每逢朔望相見。",
        "from": "X88n1668_001:0567c09_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘日則不相會。",
        "from": "X88n1668_001:0567c09_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖戒再淫之說。",
        "from": "X88n1668_001:0567c09_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未知能恪遵否。",
        "from": "X88n1668_001:0567c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而剌麻自印度來藏。",
        "from": "X88n1668_001:0567c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身搭紅衣。",
        "from": "X88n1668_001:0567c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名紅教。",
        "from": "X88n1668_001:0567c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後紅教繁盛。",
        "from": "X88n1668_001:0567c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又分花帽紅帽者。",
        "from": "X88n1668_001:0567c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名目甚多。",
        "from": "X88n1668_001:0567c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黃教者。",
        "from": "X88n1668_001:0567c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自永樂十五年間。",
        "from": "X88n1668_001:0567c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有青海之西寧衛。",
        "from": "X88n1668_001:0567c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生一人名宗喀巴者為剌麻。",
        "from": "X88n1668_001:0567c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先入藏投紅教。",
        "from": "X88n1668_001:0567c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "授經持呪。",
        "from": "X88n1668_001:0567c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見該教之人。",
        "from": "X88n1668_001:0567c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "均鍊神通。",
        "from": "X88n1668_001:0567c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "專事吞刀吐火。",
        "from": "X88n1668_001:0567c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衒奇惑眾。",
        "from": "X88n1668_001:0567c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不修戒定慧之正道。",
        "from": "X88n1668_001:0567c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故捨去而入藏內甘丹寺。",
        "from": "X88n1668_001:0567c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "習定得道該寺。",
        "from": "X88n1668_001:0567c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至成化十四年示寂。",
        "from": "X88n1668_001:0567c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宗喀巴初修道時。",
        "from": "X88n1668_001:0567c16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即改其衣冠為黃色。",
        "from": "X88n1668_001:0567c16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不屑同於紅教之人。",
        "from": "X88n1668_001:0567c17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宗喀巴臨終。",
        "from": "X88n1668_001:0567c17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遺囑二大弟子。",
        "from": "X88n1668_001:0567c17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世世以呼畢勒罕。",
        "from": "X88n1668_001:0567c18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應身轉世。",
        "from": "X88n1668_001:0567c18_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不迷本性。",
        "from": "X88n1668_001:0567c18_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能知夙世。",
        "from": "X88n1668_001:0567c18_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還來為坐牀掌教之師。",
        "from": "X88n1668_001:0567c19_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大弟子者。",
        "from": "X88n1668_001:0567c19_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名達賴。",
        "from": "X88n1668_001:0567c19_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名班禪。",
        "from": "X88n1668_001:0567c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至乾隆中已第六次轉生矣。",
        "from": "X88n1668_001:0567c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又有第一世達賴之大弟子。",
        "from": "X88n1668_001:0567c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名哲卜尊丹巴者。",
        "from": "X88n1668_001:0567c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一支住持蒙古庫倫。",
        "from": "X88n1668_001:0567c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能仿達賴班禪。",
        "from": "X88n1668_001:0567c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以呼畢勒罕。",
        "from": "X88n1668_001:0567c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不迷本性。",
        "from": "X88n1668_001:0567c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世世轉生者。",
        "from": "X88n1668_001:0567c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "康熙中。",
        "from": "X88n1668_001:0567c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又有第五世達賴之弟子。",
        "from": "X88n1668_001:0567c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名章佳呼土克圖者。",
        "from": "X88n1668_001:0567c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為雍正時國師大剌麻。",
        "from": "X88n1668_001:0568a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能以呼畢勒罕。",
        "from": "X88n1668_001:0568a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不迷本性。",
        "from": "X88n1668_001:0568a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉世再來。",
        "from": "X88n1668_001:0568a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常住京都口外。",
        "from": "X88n1668_001:0568a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大清太宗崇德七年。",
        "from": "X88n1668_001:0568a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "延請第四世達賴剌麻至盛京。",
        "from": "X88n1668_001:0568a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱為金剛大士。",
        "from": "X88n1668_001:0568a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還住藏中。",
        "from": "X88n1668_001:0568a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "順治初遣使迓達賴。",
        "from": "X88n1668_001:0568a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九年至京師。",
        "from": "X88n1668_001:0568a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "封西天大善自在佛。",
        "from": "X88n1668_001:0568a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "領天下釋教。",
        "from": "X88n1668_001:0568a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又封為普通鄂濟大剌麻。",
        "from": "X88n1668_001:0568a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "達賴班禪。",
        "from": "X88n1668_001:0568a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分主前後兩藏。",
        "from": "X88n1668_001:0568a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乾隆時理藩院造冊。",
        "from": "X88n1668_001:0568a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至第六世達賴。",
        "from": "X88n1668_001:0568a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所轄寺廟。",
        "from": "X88n1668_001:0568a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "計有三千一百五十餘所。",
        "from": "X88n1668_001:0568a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共計剌麻三十萬二千五百有奇　班禪所轄寺廟三百二十七所。",
        "from": "X88n1668_001:0568a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共計剌麻一萬三千七百有奇。",
        "from": "X88n1668_001:0568a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "布魯特二部紅教。",
        "from": "X88n1668_001:0568a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有寺廟一百二十。",
        "from": "X88n1668_001:0568a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剌麻二萬五千。",
        "from": "X88n1668_001:0568a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "紅黃二教。",
        "from": "X88n1668_001:0568a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共計現有剌麻三十四萬一千二百有奇。",
        "from": "X88n1668_001:0568a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可謂盛之極矣。",
        "from": "X88n1668_001:0568a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其他剌麻學道。",
        "from": "X88n1668_001:0568a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能明心見性。",
        "from": "X88n1668_001:0568a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由達賴班禪證明。",
        "from": "X88n1668_001:0568a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方能稱呼土克圖。",
        "from": "X88n1668_001:0568a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能不迷本性。",
        "from": "X88n1668_001:0568a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉世再來。",
        "from": "X88n1668_001:0568a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方稱呼畢勒罕。",
        "from": "X88n1668_001:0568a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "綜計剌麻之能。",
        "from": "X88n1668_001:0568a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以神識轉生於世。",
        "from": "X88n1668_001:0568a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出呼畢勒罕。",
        "from": "X88n1668_001:0568a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入理藩院冊者。",
        "from": "X88n1668_001:0568a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西藏稱呼土克圖者十八人。",
        "from": "X88n1668_001:0568a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱沙布隆十有二。",
        "from": "X88n1668_001:0568a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漠北蒙古十有九。",
        "from": "X88n1668_001:0568a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漠南蒙古五十有七。",
        "from": "X88n1668_001:0568a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "青海番地三十有五。",
        "from": "X88n1668_001:0568a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四川察木多番地五。",
        "from": "X88n1668_001:0568a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "駐京呼土克圖十四。",
        "from": "X88n1668_001:0568a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共計呼畢勒罕一百六十。",
        "from": "X88n1668_001:0568a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其能得呼土克圖。",
        "from": "X88n1668_001:0568a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方可管轄番民。",
        "from": "X88n1668_001:0568a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如同土司。",
        "from": "X88n1668_001:0568a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧多於民。",
        "from": "X88n1668_001:0568a21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不設官吏。",
        "from": "X88n1668_001:0568a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "君并於師。",
        "from": "X88n1668_001:0568a21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "介於出家在家之間。",
        "from": "X88n1668_001:0568a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其秩有國師。",
        "from": "X88n1668_001:0568a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪師。",
        "from": "X88n1668_001:0568a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及大剌麻。",
        "from": "X88n1668_001:0568a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "扎薩克等。",
        "from": "X88n1668_001:0568a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分住各番。",
        "from": "X88n1668_001:0568a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及蒙古部落。",
        "from": "X88n1668_001:0568a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆受達賴班禪。",
        "from": "X88n1668_001:0568a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頒領贈秩。",
        "from": "X88n1668_001:0568a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其兵刑稅賦。",
        "from": "X88n1668_001:0568a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則有剌麻為第巴。",
        "from": "X88n1668_001:0568a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有噶布倫。",
        "from": "X88n1668_001:0568a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有戴琫。",
        "from": "X88n1668_001:0568a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有堪布。",
        "from": "X88n1668_001:0568b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有商上等職。",
        "from": "X88n1668_001:0568b01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掌司其事。",
        "from": "X88n1668_001:0568b01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嘉慶八年第八世呼畢勒罕。",
        "from": "X88n1668_001:0568b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉身之達賴剌麻。",
        "from": "X88n1668_001:0568b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "靈徵眾著。",
        "from": "X88n1668_001:0568b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由駐藏大臣奏聞。",
        "from": "X88n1668_001:0568b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請旨坐牀掌教。",
        "from": "X88n1668_001:0568b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至今又數轉",
        "from": "X88n1668_001:0568b03_5",
        "context_with_next_sentence": false
    }
]