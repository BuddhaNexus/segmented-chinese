        "from": "T21n1233_001:0172c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為於世間作最上修行圓滿。",
        "from": "T21n1233_001:0173a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成佛事業獲得菩提。",
        "from": "T21n1233_001:0173a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降伏魔軍轉於法輪。",
        "from": "T21n1233_001:0173a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有千俱胝淨居天眾來。",
        "from": "T21n1233_001:0173a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於我前發甚深意。",
        "from": "T21n1233_001:0173a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以柔軟音請轉法輪。",
        "from": "T21n1233_001:0173a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告舍利子言。",
        "from": "T21n1233_001:0173a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諦聽舍利子。",
        "from": "T21n1233_001:0173a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此無能勝明王利益安樂一切有情。",
        "from": "T21n1233_001:0173a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利子昔過去之世。",
        "from": "T21n1233_001:0173a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天與阿蘇羅兩相鬪戰。",
        "from": "T21n1233_001:0173a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時天眾退散阿蘇羅得勝。",
        "from": "T21n1233_001:0173b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時帝釋天主。",
        "from": "T21n1233_001:0173b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以此無能勝陀羅尼。",
        "from": "T21n1233_001:0173b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書於金葉安旗幟頭。",
        "from": "T21n1233_001:0173b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復與相戰。",
        "from": "T21n1233_001:0173b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時天眾得勝。",
        "from": "T21n1233_001:0173b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿蘇羅軍尋即退散。",
        "from": "T21n1233_001:0173b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1233_001:0173b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛(上)儞也(二合)他(去引一)沒素謎(二)嚩素迦囉帝(三)曩(引)虞(上)那哩佐(四)迦播哩抳(五)娑嚩(二合引)賀(引六)佛告舍利子。",
        "from": "T21n1233_001:0173b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我不見有諸天及魔羅等。",
        "from": "T21n1233_001:0173b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至人間沙門婆羅門。",
        "from": "T21n1233_001:0173b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并阿蘇羅一切鬼魅。",
        "from": "T21n1233_001:0173b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿跛薩麼囉乾闥婆。",
        "from": "T21n1233_001:0173b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥叉囉乞叉蘖路荼緊那羅。",
        "from": "T21n1233_001:0173b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尾那夜迦布旦曩供盤拏。",
        "from": "T21n1233_001:0173b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羯吒布多曩食生者等。",
        "from": "T21n1233_001:0173b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此無能勝大明王無敢違犯。",
        "from": "T21n1233_001:0173b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是大明王陀羅尼。",
        "from": "T21n1233_001:0173b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不見不聞者。",
        "from": "T21n1233_001:0173b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃是過去見在業所障故。",
        "from": "T21n1233_001:0173b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊說是陀羅尼一切成就已。",
        "from": "T21n1233_001:0173b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時阿難陀舍利子天人阿蘇羅等。",
        "from": "T21n1233_001:0173b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心受持歡喜奉行。",
        "from": "T21n1233_001:0173b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說無能勝大明王陀羅尼經",
        "from": "T21n1233_001:0173b18_0",
        "context_with_next_sentence": false
    }
]