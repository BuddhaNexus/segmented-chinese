[
    {
        "source_sentence": "楞伽阿跋多羅寶經疏卷上",
        "from": "X17n0322_001:0116b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法於是下文則云。",
        "from": "X17n0322_001:0116b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼妄想自性建立二自性。",
        "from": "X17n0322_001:0116b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云云。",
        "from": "X17n0322_001:0116b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可也。",
        "from": "X17n0322_001:0116b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂圓成實性。",
        "from": "X17n0322_001:0116b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離有無也。",
        "from": "X17n0322_001:0116b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何有二性哉。",
        "from": "X17n0322_001:0116b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以彼妄想分別。",
        "from": "X17n0322_001:0116b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃有無二性。",
        "from": "X17n0322_001:0116b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為建立有無二性也。",
        "from": "X17n0322_001:0116b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以建立之故。",
        "from": "X17n0322_001:0116b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃見種種虗妄現於清淨聖境之中。",
        "from": "X17n0322_001:0116b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "○辯頌之一。",
        "from": "X17n0322_001:0116b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "○平箋。",
        "from": "X17n0322_001:0116b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妄想一性。",
        "from": "X17n0322_001:0116b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能生緣起圓成二性。",
        "from": "X17n0322_001:0116b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故。",
        "from": "X17n0322_001:0116b10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妄想迷相。",
        "from": "X17n0322_001:0116b11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是緣起性。",
        "from": "X17n0322_001:0116b11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妄想覺相。",
        "from": "X17n0322_001:0116b11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是圓成性。",
        "from": "X17n0322_001:0116b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依之而言。",
        "from": "X17n0322_001:0116b11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若即妄想種種現相。",
        "from": "X17n0322_001:0116b12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善觀察即清淨境界。",
        "from": "X17n0322_001:0116b12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　妄想如畫色",
        "from": "X17n0322_001:0116b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妄想如畫。",
        "from": "X17n0322_001:0116b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂意匠經營。",
        "from": "X17n0322_001:0116b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後畫出。",
        "from": "X17n0322_001:0116b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　緣起",
        "from": "X17n0322_001:0116b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非究竟。",
        "from": "X17n0322_001:0116b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "X17n0322_001:0116b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上以梵王。",
        "from": "X17n0322_001:0116b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為天乘也。",
        "from": "X17n0322_001:0116b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此及字。",
        "from": "X17n0322_001:0116b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "似分二種。",
        "from": "X17n0322_001:0116b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "X17n0322_001:0116b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵王者。",
        "from": "X17n0322_001:0116b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是外道所宗。",
        "from": "X17n0322_001:0116b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今以梵乘。",
        "from": "X17n0322_001:0116b17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為人乘也。",
        "from": "X17n0322_001:0116b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而此三乘與諸經異。",
        "from": "X17n0322_001:0116b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "束分五乘。",
        "from": "X17n0322_001:0116b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為三乘也。",
        "from": "X17n0322_001:0116b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初句人天兩乘。",
        "from": "X17n0322_001:0116b18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次二句是三乘。",
        "from": "X17n0322_001:0116b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰。",
        "from": "X17n0322_001:0116b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘乘且隨。",
        "from": "X17n0322_001:0116b19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛如來乘。",
        "from": "X17n0322_001:0116b19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何非究竟。",
        "from": "X17n0322_001:0116b19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰。",
        "from": "X17n0322_001:0116b20_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二乘對故。",
        "from": "X17n0322_001:0116b20_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰。",
        "from": "X17n0322_001:0116b20_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人天兩乘。",
        "from": "X17n0322_001:0116b20_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸宗取舍不一。",
        "from": "X17n0322_001:0116b20_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或不立乘。",
        "from": "X17n0322_001:0116b20_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今何取乎。",
        "from": "X17n0322_001:0116b21_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰。",
        "from": "X17n0322_001:0116b21_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經旨趣。",
        "from": "X17n0322_001:0116b21_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外道二乘。",
        "from": "X17n0322_001:0116b21_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸名相法。",
        "from": "X17n0322_001:0116b21_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說為真如。",
        "from": "X17n0322_001:0116b22_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈人天乘之取舍乎。",
        "from": "X17n0322_001:0116b22_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "○智箋。",
        "from": "X17n0322_001:0116b22_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成作。",
        "from": "X17n0322_001:0116b22_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五乘合說。",
        "from": "X17n0322_001:0116b22_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成作之智。",
        "from": "X17n0322_001:0116b23_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若彼心滅盡(心量滅除)　無乘(乘云法也)及乘者(人也)　無有乘建立(謂化儀也)　我說為一乘正頌。",
        "from": "X17n0322_001:0116b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "○平箋。",
        "from": "X17n0322_001:0116c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘法乘人及建立相。",
        "from": "X17n0322_001:0116c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三法無有。",
        "from": "X17n0322_001:0116c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是即名為楞伽一乘。",
        "from": "X17n0322_001:0116c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "X17n0322_001:0116c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼心滅盡。",
        "from": "X17n0322_001:0116c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無乘建立。",
        "from": "X17n0322_001:0116c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "似乎相重。",
        "from": "X17n0322_001:0116c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "X17n0322_001:0116c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心滅因中。",
        "from": "X17n0322_001:0116c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無建果上。",
        "from": "X17n0322_001:0116c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　引導眾生故　　分別說諸乘",
        "from": "X17n0322_001:0116c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "簡頌。",
        "from": "X17n0322_001:0116c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "○平箋。",
        "from": "X17n0322_001:0116c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來分說。",
        "from": "X17n0322_001:0116c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三五諸乘。",
        "from": "X17n0322_001:0116c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "引導眾生。",
        "from": "X17n0322_001:0116c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "權說建立。",
        "from": "X17n0322_001:0116c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大凡一乘。",
        "from": "X17n0322_001:0116c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃有三種。",
        "from": "X17n0322_001:0116c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一寂靜一乘。",
        "from": "X17n0322_001:0116c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二三乘一乘。",
        "from": "X17n0322_001:0116c08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三三無一乘。",
        "from": "X17n0322_001:0116c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來境界。",
        "from": "X17n0322_001:0116c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "X17n0322_001:0116c09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法華一乘。",
        "from": "X17n0322_001:0116c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三中當何。",
        "from": "X17n0322_001:0116c09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "X17n0322_001:0116c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三乘一乘。",
        "from": "X17n0322_001:0116c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰。",
        "from": "X17n0322_001:0116c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼經中云。",
        "from": "X17n0322_001:0116c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一佛乘。",
        "from": "X17n0322_001:0116c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分別說三。",
        "from": "X17n0322_001:0116c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今偈所謂引導眾生。",
        "from": "X17n0322_001:0116c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分說諸乘。",
        "from": "X17n0322_001:0116c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "意義相同於一佛乘。",
        "from": "X17n0322_001:0116c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是如來境。",
        "from": "X17n0322_001:0116c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今何却云菩薩境耶。",
        "from": "X17n0322_001:0116c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰圓機所被。",
        "from": "X17n0322_001:0116c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何非佛乘。",
        "from": "X17n0322_001:0116c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此經說外道二乘妄想皆是。",
        "from": "X17n0322_001:0116c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然建立及有心轉。",
        "from": "X17n0322_001:0116c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故自成階耳。",
        "from": "X17n0322_001:0116c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼經所謂大白牛車。",
        "from": "X17n0322_001:0116c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈非建立。",
        "from": "X17n0322_001:0116c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘此寶乘直至道場。",
        "from": "X17n0322_001:0116c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘至二法。",
        "from": "X17n0322_001:0116c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈非心轉。",
        "from": "X17n0322_001:0116c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知乘外。",
        "from": "X17n0322_001:0116c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別有道場。",
        "from": "X17n0322_001:0116c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有道場。",
        "from": "X17n0322_001:0116c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘為途中。",
        "from": "X17n0322_001:0116c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼白牛車。",
        "from": "X17n0322_001:0116c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈非途乘。",
        "from": "X17n0322_001:0116c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經不爾。",
        "from": "X17n0322_001:0116c17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘及乘者。",
        "from": "X17n0322_001:0116c17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并建立法。",
        "from": "X17n0322_001:0116c17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三種共無。",
        "from": "X17n0322_001:0116c17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三無之所。",
        "from": "X17n0322_001:0116c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非道場何。",
        "from": "X17n0322_001:0116c18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故我今言彼一乘者。",
        "from": "X17n0322_001:0116c18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "途路一乘。",
        "from": "X17n0322_001:0116c18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩乘也。",
        "from": "X17n0322_001:0116c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此一乘者。",
        "from": "X17n0322_001:0116c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道場一乘。",
        "from": "X17n0322_001:0116c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來乘也。",
        "from": "X17n0322_001:0116c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰。",
        "from": "X17n0322_001:0116c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然何彼經云一佛乘。",
        "from": "X17n0322_001:0116c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰。",
        "from": "X17n0322_001:0116c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼一佛乘。",
        "from": "X17n0322_001:0116c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經所謂佛如來乘。",
        "from": "X17n0322_001:0116c20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而佛微旨。",
        "from": "X17n0322_001:0116c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至乎下句。",
        "from": "X17n0322_001:0116c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃有非究竟言。",
        "from": "X17n0322_001:0116c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是以彼之三乘一乘。",
        "from": "X17n0322_001:0116c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非本一乘。",
        "from": "X17n0322_001:0116c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又菩薩乘。",
        "from": "X17n0322_001:0116c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "混佛。",
        "from": "X17n0322_001:0116c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愚。",
        "from": "X17n0322_001:0116c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而又味著醉倒於三昧之樂。",
        "from": "X17n0322_001:0116c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安於無漏。",
        "from": "X17n0322_001:0116c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆非究竟。",
        "from": "X17n0322_001:0116c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖亦不退。",
        "from": "X17n0322_001:0116c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復作凡夫。",
        "from": "X17n0322_001:0116c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然以得諸三昧。",
        "from": "X17n0322_001:0116c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如長壽天。",
        "from": "X17n0322_001:0116c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至長劫而不覺悟。",
        "from": "X17n0322_001:0117a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如昏醉人。",
        "from": "X17n0322_001:0117a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　酒消然後覺　　彼覺法亦然　　得佛無上身喻頌。",
        "from": "X17n0322_001:0117a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "○平箋。",
        "from": "X17n0322_001:0117a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沈酒醉人。",
        "from": "X17n0322_001:0117a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酒力消後。",
        "from": "X17n0322_001:0117a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有覺悟也。",
        "from": "X17n0322_001:0117a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二乘亦然。",
        "from": "X17n0322_001:0117a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三昧樂酒。",
        "from": "X17n0322_001:0117a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長時昏醉。",
        "from": "X17n0322_001:0117a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經八萬劫。",
        "from": "X17n0322_001:0117a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必得覺悟。",
        "from": "X17n0322_001:0117a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出定回心。",
        "from": "X17n0322_001:0117a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漸得佛身。",
        "from": "X17n0322_001:0117a05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "○智箋。",
        "from": "X17n0322_001:0117a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓鏡。",
        "from": "X17n0322_001:0117a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酒消得覺。",
        "from": "X17n0322_001:0117a05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成如來身。",
        "from": "X17n0322_001:0117a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是佛地。",
        "from": "X17n0322_001:0117a06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鏡智像相。",
        "from": "X17n0322_001:0117a06_17",
        "context_with_next_sentence": false
    }
]