[
    {
        "source_sentence": "佛說高王觀世音經",
        "from": "T85n2898_001:1425b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀世音菩薩。",
        "from": "T85n2898_001:1425b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無佛。",
        "from": "T85n2898_001:1425b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無法。",
        "from": "T85n2898_001:1425b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無僧。",
        "from": "T85n2898_001:1425b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛國有緣。",
        "from": "T85n2898_001:1425b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛法相因。",
        "from": "T85n2898_001:1425b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常樂我淨。",
        "from": "T85n2898_001:1425b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有緣佛法。",
        "from": "T85n2898_001:1425b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無摩訶般若波羅蜜。",
        "from": "T85n2898_001:1425b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是大神呪。",
        "from": "T85n2898_001:1425b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無摩訶般若波羅蜜。",
        "from": "T85n2898_001:1425b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是大明呪。",
        "from": "T85n2898_001:1425b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無摩訶般若波羅蜜。",
        "from": "T85n2898_001:1425b11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是無上呪。",
        "from": "T85n2898_001:1425b11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無摩訶般若波羅蜜。",
        "from": "T85n2898_001:1425b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是無等等呪。",
        "from": "T85n2898_001:1425b12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無淨光祕密佛。",
        "from": "T85n2898_001:1425b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法藏佛。",
        "from": "T85n2898_001:1425b13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師子吼神足由王佛。",
        "from": "T85n2898_001:1425b13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告須彌登王佛。",
        "from": "T85n2898_001:1425b14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法護佛。",
        "from": "T85n2898_001:1425b14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛藏師子游戲佛。",
        "from": "T85n2898_001:1425b14_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶勝佛。",
        "from": "T85n2898_001:1425b15_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神通佛。",
        "from": "T85n2898_001:1425b15_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥師琉璃光王佛。",
        "from": "T85n2898_001:1425b15_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普光功德山王佛。",
        "from": "T85n2898_001:1425b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善住功德寶王佛。",
        "from": "T85n2898_001:1425b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過去七佛。",
        "from": "T85n2898_001:1425b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未來賢劫千佛。",
        "from": "T85n2898_001:1425b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千五百佛。",
        "from": "T85n2898_001:1425b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬五千佛。",
        "from": "T85n2898_001:1425b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五百花勝佛。",
        "from": "T85n2898_001:1425b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百億金剛藏佛。",
        "from": "T85n2898_001:1425b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定光佛。",
        "from": "T85n2898_001:1425b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六方六佛名號。",
        "from": "T85n2898_001:1425b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東方寶光月殿月妙尊音王佛。",
        "from": "T85n2898_001:1425b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南方樹根花王佛。",
        "from": "T85n2898_001:1425b19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方皂王神通焰花王佛。",
        "from": "T85n2898_001:1425b20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方月殿清淨佛。",
        "from": "T85n2898_001:1425b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上方無數精進寶首佛。",
        "from": "T85n2898_001:1425b21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下方善寂月音王佛。",
        "from": "T85n2898_001:1425b21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量諸佛。",
        "from": "T85n2898_001:1425b22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多寶佛。",
        "from": "T85n2898_001:1425b22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋迦牟尼佛。",
        "from": "T85n2898_001:1425b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彌勒佛。",
        "from": "T85n2898_001:1425b22_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿閦佛。",
        "from": "T85n2898_001:1425b23_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彌陀佛。",
        "from": "T85n2898_001:1425b23_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中央一切眾生。",
        "from": "T85n2898_001:1425b23_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在佛世界中者。",
        "from": "T85n2898_001:1425b23_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行住於地上。",
        "from": "T85n2898_001:1425b24_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及在虛空中。",
        "from": "T85n2898_001:1425b24_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈憂於一切眾生。",
        "from": "T85n2898_001:1425b24_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各令安穩休息。",
        "from": "T85n2898_001:1425b25_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晝夜修持。",
        "from": "T85n2898_001:1425b25_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心常求誦此經。",
        "from": "T85n2898_001:1425b25_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能滅生死苦。",
        "from": "T85n2898_001:1425b26_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "消伏於毒害。",
        "from": "T85n2898_001:1425b26_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那摩大明觀世音。",
        "from": "T85n2898_001:1425b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀明觀世音。",
        "from": "T85n2898_001:1425b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高明觀世音。",
        "from": "T85n2898_001:1425b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開明觀世音。",
        "from": "T85n2898_001:1425b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥王菩薩。",
        "from": "T85n2898_001:1425b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥上菩薩。",
        "from": "T85n2898_001:1425b28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊師利菩薩。",
        "from": "T85n2898_001:1425b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普賢菩薩。",
        "from": "T85n2898_001:1425b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空藏菩薩。",
        "from": "T85n2898_001:1425b29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地藏菩薩。",
        "from": "T85n2898_001:1425b29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清涼寶山億萬菩薩。",
        "from": "T85n2898_001:1425b30_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普光如來化勝菩薩。",
        "from": "T85n2898_001:1425b30_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念念誦此偈。",
        "from": "T85n2898_001:1425c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七佛世尊。",
        "from": "T85n2898_001:1425c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T85n2898_001:1425c01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離波離波帝。",
        "from": "T85n2898_001:1425c01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求訶求訶帝。",
        "from": "T85n2898_001:1425c02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陀羅尼帝。",
        "from": "T85n2898_001:1425c02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尼訶羅帝。",
        "from": "T85n2898_001:1425c02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘離尼帝。",
        "from": "T85n2898_001:1425c03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩訶迦帝。",
        "from": "T85n2898_001:1425c03_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真靈虔帝。",
        "from": "T85n2898_001:1425c03_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娑婆訶。",
        "from": "T85n2898_001:1425c03_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　十方觀世音　　一切諸菩薩　　誓願救眾生　稱名悉解脫　　若有智慧者　　慇懃為解脫　但是有因緣　　讀誦口不綴　　誦經滿千遍　念念心不絕　　火焰不能傷　　刀兵立摧折　恚怒生歡喜　　死者變成活　　莫言此是虛　諸佛不妄說佛說高王觀世音經　高王觀世音　　能救諸苦危　　臨危急難中　死者變成活　　諸佛語不虛　　是故應頂禮　持誦滿千遍　　重罪皆消滅　　薄福不信者　專貢受持經念八大菩薩名號。",
        "from": "T85n2898_001:1425c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無觀世音菩薩摩訶薩。",
        "from": "T85n2898_001:1425c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無彌勒菩薩摩訶薩。",
        "from": "T85n2898_001:1425c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無虛空藏菩薩摩訶薩。",
        "from": "T85n2898_001:1425c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無普賢菩薩摩訶薩。",
        "from": "T85n2898_001:1425c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無金剛手菩薩摩訶薩。",
        "from": "T85n2898_001:1426a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無妙吉祥菩薩摩訶薩。",
        "from": "T85n2898_001:1426a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無除蓋障菩薩摩訶薩。",
        "from": "T85n2898_001:1426a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無地藏王菩薩摩訶薩。",
        "from": "T85n2898_001:1426a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無諸尊菩薩摩訶薩。",
        "from": "T85n2898_001:1426a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　願以此功德　　普及於一切",
        "from": "T85n2898_001:1426a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　誦滿一千遍　　重罪皆消滅",
        "from": "T85n2898_001:1426a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說高王觀世音經(終)",
        "from": "T85n2898_001:1426a07_0",
        "context_with_next_sentence": false
    }
]