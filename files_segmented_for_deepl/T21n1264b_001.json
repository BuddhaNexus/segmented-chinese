[
    {
        "source_sentence": "佛說穰麌梨童女經",
        "from": "T21n1264b_001:0293a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐三藏沙門大廣智不空奉　詔譯",
        "from": "T21n1264b_001:0293a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T21n1264b_001:0293a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時薄伽梵。",
        "from": "T21n1264b_001:0293a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住舍衛國祇樹給孤獨園。",
        "from": "T21n1264b_001:0293a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大苾芻眾千二百五十人俱。",
        "from": "T21n1264b_001:0293a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有無量菩薩摩訶薩。",
        "from": "T21n1264b_001:0293a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸天龍藥叉乾闥婆阿修羅迦樓羅緊那羅摩睺羅伽人非人等皆來集會。",
        "from": "T21n1264b_001:0293a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告諸苾芻。",
        "from": "T21n1264b_001:0293a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我念往昔。",
        "from": "T21n1264b_001:0293a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住雪山北遊香醉山。",
        "from": "T21n1264b_001:0293a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見一童女。",
        "from": "T21n1264b_001:0293a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百福相好莊嚴其身。",
        "from": "T21n1264b_001:0293a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鹿皮為衣以諸毒蛇而為瓔珞。",
        "from": "T21n1264b_001:0293a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將諸毒蟲蚖蝮之類。",
        "from": "T21n1264b_001:0293a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後圍繞常為伴戲。",
        "from": "T21n1264b_001:0293a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲毒漿食毒果。",
        "from": "T21n1264b_001:0293a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼女見我謂言。",
        "from": "T21n1264b_001:0293a19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仁者當知聽我宣說穰麌梨真言。",
        "from": "T21n1264b_001:0293a19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除世間一切諸毒。",
        "from": "T21n1264b_001:0293a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人聞此真言。",
        "from": "T21n1264b_001:0293a21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及持我名者。",
        "from": "T21n1264b_001:0293a21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不被一切諸毒所害。",
        "from": "T21n1264b_001:0293a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時童女為我宣說。",
        "from": "T21n1264b_001:0293a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我從彼女聞已。",
        "from": "T21n1264b_001:0293a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常持此法饒益有情。",
        "from": "T21n1264b_001:0293a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今當說真言曰。",
        "from": "T21n1264b_001:0293a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也(二合)他(去聲引)唵(引)壹里蜜帝(引一)底(丁以切)里蜜帝(引二)壹里底里蜜帝(引三)努(鼻)迷努(鼻)麼(引)里(引)曳(平聲引四)訥泚訥蹉(去聲引)里(引)曳(平聲五)得羯囉抳(尼呈切鼻六)嚩(無博切)羯囉抳(鼻上同七)羯濕弭(二合引)[口*(隸-木+士)](引八)羯濕弭(二合引)囉穆訖帝(二合引九)惡祇(祇曳切十)惡伽(去聲)寧(十一)惡伽(去聲)曩伽寧(十二)壹里曳(十三)壹里壹里(引)曳(十四)阿(上聲)佉(去聲引)夜(引)曳(平聲十五)姶(烏合切)播(引)夜(引)曳(平聲十六)濕吠(二合引)帝(引十七)濕吠(二合引)多頓嬭(引十八)阿(上)曩(引)努(鼻)囉乞曬(二合引)娑嚩(二合引)賀(十九)佛告諸苾芻。",
        "from": "T21n1264b_001:0293a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人一切聞陀羅尼。",
        "from": "T21n1264b_001:0293b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "却後七年遠離一切諸毒。",
        "from": "T21n1264b_001:0293b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若常受持。",
        "from": "T21n1264b_001:0293b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切毒蟲及諸毒藥悉不能害。",
        "from": "T21n1264b_001:0293b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有毒蛇來嚙此人者。",
        "from": "T21n1264b_001:0293b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭破作七分猶如蘭香[卄/梢]。",
        "from": "T21n1264b_001:0293b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其有受持此真言法。",
        "from": "T21n1264b_001:0293b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應以白物先供養師。",
        "from": "T21n1264b_001:0293b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後受持必獲成就。",
        "from": "T21n1264b_001:0293b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苾芻當知。",
        "from": "T21n1264b_001:0293b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿於蛇前稱誦此明。",
        "from": "T21n1264b_001:0293b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其蛇必死。",
        "from": "T21n1264b_001:0293b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當以此真言。",
        "from": "T21n1264b_001:0293b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持死蛇令其蘇息。",
        "from": "T21n1264b_001:0293b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1264b_001:0293b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也(二合)他(去聲引一)唵(引)壹羅(引二)尾羅(引三)斫句(引)嚩句(去聲引四)句(引)拏(引)句嬭(引)底(丁以切五)報(引)拏(引)報(引)嬭(引)底(同前)[寧*頁]矩嚕拏(引六)[寧*頁]矩魯嬭(引)底(同前七)普吒囉蒵(引)普吒囉蒵(引八)普吒赧(讁簡切)拏囉蒵(引九)薩跛囉蒵(引十)薩跛吒赧(准上音)拏囉蒵(引十一)曩(引)誐囉蒵(引十二)曩(引)誐吒赧(同前)拏囉蒵(引十三)阿(上)泚(引)蹉禮(引十四)揆攞尾曬(引十五)試(引)帝(引)試(引)多嚩寧(引十六)滿跢(引)黎(引十七)伊(上)里黎(引十八)賀黎賀黎(十九)旦嬭(引)旦嬭(引二十)怛[齒*來](二十一)娑普(二合)吒娑普(二合)吒娑嚩(二合)賀(引二十二)爾時世尊說此陀羅尼已。",
        "from": "T21n1264b_001:0293b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告諸苾芻。",
        "from": "T21n1264b_001:0293b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我此真言能解世間一切諸毒。",
        "from": "T21n1264b_001:0293b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蟲毒魅毒蠱毒藥毒等不能為害。",
        "from": "T21n1264b_001:0293b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有被毒中者。",
        "from": "T21n1264b_001:0293b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此真言加持皆得消滅。",
        "from": "T21n1264b_001:0293b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸苾芻此穰麌梨陀羅尼。",
        "from": "T21n1264b_001:0293c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切如來大會中說。",
        "from": "T21n1264b_001:0293c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真實不虛不顛倒語。",
        "from": "T21n1264b_001:0293c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如語不異語諸有藥毒呪毒蟲毒魅毒。",
        "from": "T21n1264b_001:0293c03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲來相害。",
        "from": "T21n1264b_001:0293c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令却著本所與者。",
        "from": "T21n1264b_001:0293c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令使諸毒入水入火入柱入壁。",
        "from": "T21n1264b_001:0293c04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦令入地。",
        "from": "T21n1264b_001:0293c05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有諸毒不令成毒。",
        "from": "T21n1264b_001:0293c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉能除滅。",
        "from": "T21n1264b_001:0293c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人受持此經日誦一遍。",
        "from": "T21n1264b_001:0293c06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非但滅世間諸毒。",
        "from": "T21n1264b_001:0293c07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能除滅身中三毒。",
        "from": "T21n1264b_001:0293c07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊。",
        "from": "T21n1264b_001:0293c08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說穰麌梨童女隨心真言及成就法。",
        "from": "T21n1264b_001:0293c08_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行者欲成就此法。",
        "from": "T21n1264b_001:0293c09_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先斷五辛亦不食鹽不食油。",
        "from": "T21n1264b_001:0293c09_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷語於一淨處。",
        "from": "T21n1264b_001:0293c10_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三時澡浴三時換衣。",
        "from": "T21n1264b_001:0293c10_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結印誦隨心真言滿一萬遍。",
        "from": "T21n1264b_001:0293c11_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則行法成就。",
        "from": "T21n1264b_001:0293c12_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復作一切事必獲成就隨心真言曰。",
        "from": "T21n1264b_001:0293c12_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)阿(上聲)枲(思以切)爾賀(二合引一)戌(引)攞爾賀吠(二合引)嚩囉(二合)迦(引)臾(引三)仡囉(三合)娑仡囉(二合)娑(四)入嚩(二合)羅入嚩(二合)羅(五)摩賀(引)迦(引)里(六)摩賀(引)喻祇(霓夷切引七)濕嚩(二合)里(引)曳(引八)唵(引)頗[齒*來](引九)普吒囉奚(引)娑嚩(二合引)賀(引十)吽癹吒(上半聲)娑嚩(二合引)賀(引十一)我今復說穰虞梨印及觀行法。",
        "from": "T21n1264b_001:0293c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其根本印。",
        "from": "T21n1264b_001:0293c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手相搏如掬物勢。",
        "from": "T21n1264b_001:0293c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二小指相並。",
        "from": "T21n1264b_001:0293c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘八指各散開微曲即成。",
        "from": "T21n1264b_001:0293c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結此印誦前根本真言。",
        "from": "T21n1264b_001:0293c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持自身五處。",
        "from": "T21n1264b_001:0293c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂右肩左肩心喉額等項上散印。",
        "from": "T21n1264b_001:0293c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結隨心印。",
        "from": "T21n1264b_001:0293c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手五指散開。",
        "from": "T21n1264b_001:0293c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微屈如師子爪形即成。",
        "from": "T21n1264b_001:0293c25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結此印誦隨心真言七遍加持五處。",
        "from": "T21n1264b_001:0293c25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行者作先行成就法已。",
        "from": "T21n1264b_001:0293c26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲作法除毒之時。",
        "from": "T21n1264b_001:0293c27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀想自身為穰麌梨童女。",
        "from": "T21n1264b_001:0293c27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身綠色狀如龍女。",
        "from": "T21n1264b_001:0293c28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足七頭項有圓光。",
        "from": "T21n1264b_001:0293c28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應想四臂。",
        "from": "T21n1264b_001:0294a01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右第一手持三戟叉第二手執三五莖孔雀尾。",
        "from": "T21n1264b_001:0294a01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左第一手把一黑蛇第二手施無畏。",
        "from": "T21n1264b_001:0294a02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又想七寶瓔珞耳璫環釧。",
        "from": "T21n1264b_001:0294a03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臂脚釧莊嚴其身。",
        "from": "T21n1264b_001:0294a04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并以諸蛇用為瓔珞。",
        "from": "T21n1264b_001:0294a04_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想從一一毛孔流出火焰。",
        "from": "T21n1264b_001:0294a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作此觀已於被螫人前。",
        "from": "T21n1264b_001:0294a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結根本印及隨心印。",
        "from": "T21n1264b_001:0294a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持自身五處。",
        "from": "T21n1264b_001:0294a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取一熟銅椀盛水。",
        "from": "T21n1264b_001:0294a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦隨心真言加持七遍。",
        "from": "T21n1264b_001:0294a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手掬水打被螫人心上。",
        "from": "T21n1264b_001:0294a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有毒氣漸漸銷除。",
        "from": "T21n1264b_001:0294a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於真言句中增加此句。",
        "from": "T21n1264b_001:0294a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂。",
        "from": "T21n1264b_001:0294a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左攞娑嚩(二合引)賀(引)尾鑠(疏覺切)娑縛(二合引)賀(引)即取淨土加持七遍。",
        "from": "T21n1264b_001:0294a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周匝圍繞被螫之人。",
        "from": "T21n1264b_001:0294a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸毒應時銷滅。",
        "from": "T21n1264b_001:0294a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法於被螫人前。",
        "from": "T21n1264b_001:0294a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結本印數誦吽字一字真言。",
        "from": "T21n1264b_001:0294a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或誦撲字一字真言加持。",
        "from": "T21n1264b_001:0294a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆能除毒。",
        "from": "T21n1264b_001:0294a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若常受持此穰麌梨法。",
        "from": "T21n1264b_001:0294a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能滅世間一切諸毒。",
        "from": "T21n1264b_001:0294a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求無不遂心。",
        "from": "T21n1264b_001:0294a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊說此經已。",
        "from": "T21n1264b_001:0294a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時彼大會天龍藥叉人非人等。",
        "from": "T21n1264b_001:0294a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛所說皆大歡喜信受奉行。",
        "from": "T21n1264b_001:0294a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說穰麌梨童女經",
        "from": "T21n1264b_001:0294a19_0",
        "context_with_next_sentence": false
    }
]