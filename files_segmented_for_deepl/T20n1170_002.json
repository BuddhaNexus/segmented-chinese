        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於佛族中猶如明月。",
        "from": "T20n1170_002:0697c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能破一切諸惡黑暗。",
        "from": "T20n1170_002:0697c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復能擁護佛法離諸障難。",
        "from": "T20n1170_002:0697c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次說大金剛香菩薩成就儀法。",
        "from": "T20n1170_002:0697c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前持誦大明八千遍精熟已。",
        "from": "T20n1170_002:0697c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後誦明加持安息香。",
        "from": "T20n1170_002:0697c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "焚此香煙凡所觸人。",
        "from": "T20n1170_002:0697c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並得入寤使說善惡。",
        "from": "T20n1170_002:0697c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有天人及藥叉等所魅者悉能解除。",
        "from": "T20n1170_002:0697c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至不淨鬼及作熱病瘧病小小鬼等所執持者。",
        "from": "T20n1170_002:0697c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纔誦大明速得除解。",
        "from": "T20n1170_002:0697c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲令彼一切入寤者。",
        "from": "T20n1170_002:0697c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦須結界擁護己身。",
        "from": "T20n1170_002:0697c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及擁護入寤者。",
        "from": "T20n1170_002:0697c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有諸惡星曜作災害者。",
        "from": "T20n1170_002:0697c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此大明亦速退解。",
        "from": "T20n1170_002:0697c15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能成己事能斷他呪。",
        "from": "T20n1170_002:0697c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有一切難事皆得成就。",
        "from": "T20n1170_002:0697c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是大明力悉得隨意。",
        "from": "T20n1170_002:0697c16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次若有行人。",
        "from": "T20n1170_002:0697c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲作諸成就法者。",
        "from": "T20n1170_002:0697c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行人潔淨常食赤色飲食。",
        "from": "T20n1170_002:0697c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後備種種香花。",
        "from": "T20n1170_002:0697c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養佛及本尊。",
        "from": "T20n1170_002:0697c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用白月一日起首。",
        "from": "T20n1170_002:0697c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿七晝夜誦大明滿八千遍。",
        "from": "T20n1170_002:0697c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作先行已。",
        "from": "T20n1170_002:0697c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此後一切所作皆得成就。",
        "from": "T20n1170_002:0697c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲作敬愛者。",
        "from": "T20n1170_002:0697c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當稱彼名誦大明即獲敬愛。",
        "from": "T20n1170_002:0697c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦大明八千遍加持白芥子。",
        "from": "T20n1170_002:0697c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用芥子觸者皆得敬愛。",
        "from": "T20n1170_002:0697c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦大明八千遍加持麥。",
        "from": "T20n1170_002:0697c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觸剎帝利者則獲敬愛。",
        "from": "T20n1170_002:0697c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲婆羅門敬愛者。",
        "from": "T20n1170_002:0697c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦大明八千遍加持炭灰。",
        "from": "T20n1170_002:0697c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散婆羅門彼自敬愛。",
        "from": "T20n1170_002:0697c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦大明八千遍加持油。",
        "from": "T20n1170_002:0697c26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與毘舍者彼自敬愛。",
        "from": "T20n1170_002:0697c27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦大明八千遍加持鹽。",
        "from": "T20n1170_002:0697c27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與首陀者彼自敬愛。",
        "from": "T20n1170_002:0697c28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有貴族夫妻不睦者。",
        "from": "T20n1170_002:0698a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦大明八千遍。",
        "from": "T20n1170_002:0698a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持白芥子用觸身者。",
        "from": "T20n1170_002:0698a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得和睦互相敬愛。",
        "from": "T20n1170_002:0698a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦大明加持柳枝。",
        "from": "T20n1170_002:0698a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作齒木用至一年者。",
        "from": "T20n1170_002:0698a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得一切所言誠實。",
        "from": "T20n1170_002:0698a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言無謇訥。",
        "from": "T20n1170_002:0698a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若軍人誦大明七遍。",
        "from": "T20n1170_002:0698a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持頭髮者入陣得勝。",
        "from": "T20n1170_002:0698a05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦大明加持白芥子作護摩者。",
        "from": "T20n1170_002:0698a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得一切敬愛。",
        "from": "T20n1170_002:0698a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦大明加持脂麻作護摩者。",
        "from": "T20n1170_002:0698a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得一切增益。",
        "from": "T20n1170_002:0698a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦大明加持酥作護摩者。",
        "from": "T20n1170_002:0698a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當息滅一切災。",
        "from": "T20n1170_002:0698a08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦大明加持乳作護摩者。",
        "from": "T20n1170_002:0698a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當令牛馬及諸畜等無諸瘴疫。",
        "from": "T20n1170_002:0698a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次說摩摩枳菩薩印相。",
        "from": "T20n1170_002:0698a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有一切宿曜。",
        "from": "T20n1170_002:0698a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸眾生作災害者。",
        "from": "T20n1170_002:0698a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持誦行人立身作舞勢想。",
        "from": "T20n1170_002:0698a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如釘金剛橛。",
        "from": "T20n1170_002:0698a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手執金剛杵安於腰側成印。",
        "from": "T20n1170_002:0698a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此印一切宿曜見者。",
        "from": "T20n1170_002:0698a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所作災害皆悉退散。",
        "from": "T20n1170_002:0698a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又不改前舞勢。",
        "from": "T20n1170_002:0698a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手頭指作鉤舒左足。",
        "from": "T20n1170_002:0698a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名一切叉印。",
        "from": "T20n1170_002:0698a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此印能成就一切事。",
        "from": "T20n1170_002:0698a15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次說金剛香菩薩。",
        "from": "T20n1170_002:0698a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀想根本大明字相安諸身分。",
        "from": "T20n1170_002:0698a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行人欲觀字相。",
        "from": "T20n1170_002:0698a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先立身作右舞勢心住三昧。",
        "from": "T20n1170_002:0698a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手作金剛鉤安於心上。",
        "from": "T20n1170_002:0698a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是金剛香菩薩常相印。",
        "from": "T20n1170_002:0698a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲觀根本大明字莊嚴者。",
        "from": "T20n1170_002:0698a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當觀金剛香菩薩。",
        "from": "T20n1170_002:0698a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或觀自身如彼金剛香菩薩。",
        "from": "T20n1170_002:0698a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身得現前已。",
        "from": "T20n1170_002:0698a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即觀賀賀二字現於二足。",
        "from": "T20n1170_002:0698a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次觀係係二字現於二膝。",
        "from": "T20n1170_002:0698a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次觀吽吽二字現於心上。",
        "from": "T20n1170_002:0698a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次觀呬呬二字現於口門。",
        "from": "T20n1170_002:0698a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次觀護嘷二字現於頂上。",
        "from": "T20n1170_002:0698a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次觀憾郝二字現於二手。",
        "from": "T20n1170_002:0698a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次觀憾賀二字為笑容。",
        "from": "T20n1170_002:0698a26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次觀吽吽二字能令他來作於承事。",
        "from": "T20n1170_002:0698a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次觀護嘷二字能化為種種形相已復誦根本大明曰。",
        "from": "T20n1170_002:0698a28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)吽(引)吽(引)吽(引)吽(引)吽(引)郝曩莫又觀智法。",
        "from": "T20n1170_002:0698b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀護嘷二字現於心。",
        "from": "T20n1170_002:0698b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀憾一字現於兩眼。",
        "from": "T20n1170_002:0698b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "係一字現於頭上。",
        "from": "T20n1170_002:0698b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吽一字現於頂上。",
        "from": "T20n1170_002:0698b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復觀憾字為甲冑。",
        "from": "T20n1170_002:0698b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "郝字為器仗。",
        "from": "T20n1170_002:0698b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此觀智法門。",
        "from": "T20n1170_002:0698b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡諸持誦行人欲持誦求諸成就者。",
        "from": "T20n1170_002:0698b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先須於此二種觀智習令精熟。",
        "from": "T20n1170_002:0698b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然誦根本大明。",
        "from": "T20n1170_002:0698b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿一洛叉而為先行。",
        "from": "T20n1170_002:0698b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦數滿已。",
        "from": "T20n1170_002:0698b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後可作種種成就之法。",
        "from": "T20n1170_002:0698b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲成就一切事作護摩者。",
        "from": "T20n1170_002:0698b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用迦囉尾囉木柴燃火。",
        "from": "T20n1170_002:0698b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用酥八百合誦大明。",
        "from": "T20n1170_002:0698b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作於護摩所作成就。",
        "from": "T20n1170_002:0698b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此法可使三界之內一切皆令入寤。",
        "from": "T20n1170_002:0698b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來住虛空之中。",
        "from": "T20n1170_002:0698b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說於過去未來現在之事。",
        "from": "T20n1170_002:0698b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又或以瞿摩夷塗四方曼拏羅。",
        "from": "T20n1170_002:0698b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於四隅安鉢出生。",
        "from": "T20n1170_002:0698b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中心安諸形像。",
        "from": "T20n1170_002:0698b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或閼伽瓶及鈴等。",
        "from": "T20n1170_002:0698b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "磨赤檀塗像身及鈴等已。",
        "from": "T20n1170_002:0698b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後燒安息香誦大明千遍。",
        "from": "T20n1170_002:0698b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼鈴等即入寤。",
        "from": "T20n1170_002:0698b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後更結金剛印作忿怒相。",
        "from": "T20n1170_002:0698b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高聲誦吽字。",
        "from": "T20n1170_002:0698b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即時虛空中有聲說三世之事。",
        "from": "T20n1170_002:0698b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若燒覩嚕瑟迦香。",
        "from": "T20n1170_002:0698b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦根本大明千遍。",
        "from": "T20n1170_002:0698b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至於江河大山。",
        "from": "T20n1170_002:0698b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦可振搖現入寤相。",
        "from": "T20n1170_002:0698b18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於曼拏羅中安髑髏。",
        "from": "T20n1170_002:0698b19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行人誦大明滿千遍。",
        "from": "T20n1170_002:0698b19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒安息香作忿怒相高聲誦吽字。",
        "from": "T20n1170_002:0698b20_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髑髏即入寤起離曼拏羅。",
        "from": "T20n1170_002:0698b20_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住虛空中說善惡事。",
        "from": "T20n1170_002:0698b21_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復於有伏藏處。",
        "from": "T20n1170_002:0698b21_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以不墮地瞿摩夷塗曼拏羅。",
        "from": "T20n1170_002:0698b22_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於上面中心燒安息香。",
        "from": "T20n1170_002:0698b22_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦大明滿一千遍。",
        "from": "T20n1170_002:0698b23_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行人作忿怒相結忿怒印。",
        "from": "T20n1170_002:0698b23_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以金剛杵擊伏藏之地伏藏自現。",
        "from": "T20n1170_002:0698b24_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說金剛香菩薩大明成就儀軌經卷中",
        "from": "T20n1170_002:0698b26_0",
        "context_with_next_sentence": false
    }
]