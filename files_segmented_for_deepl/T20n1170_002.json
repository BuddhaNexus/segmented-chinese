[
    {
        "source_sentence": "佛說金剛香菩薩大明成就儀軌",
        "from": "T20n1170_002:0695a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經卷中",
        "from": "T20n1170_002:0695a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試鴻臚",
        "from": "T20n1170_002:0695a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卿傳法大師臣施護奉　詔譯",
        "from": "T20n1170_002:0695a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次作禁伏法。",
        "from": "T20n1170_002:0695a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先畫金剛香菩薩。",
        "from": "T20n1170_002:0695a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作圓滿相身黃色。",
        "from": "T20n1170_002:0695a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放大光明照虛空界。",
        "from": "T20n1170_002:0695a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住大山頂足踏冤家像。",
        "from": "T20n1170_002:0695a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者於此像前作觀想。",
        "from": "T20n1170_002:0695a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得現前已。",
        "from": "T20n1170_002:0695a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切冤家皆悉禁縛。",
        "from": "T20n1170_002:0695a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法於幀上。",
        "from": "T20n1170_002:0695a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同前畫金剛香菩薩。",
        "from": "T20n1170_002:0695a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以上妙香花而作供養。",
        "from": "T20n1170_002:0695a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用雌黃及淨土與水和埿。",
        "from": "T20n1170_002:0695a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作冤家像。",
        "from": "T20n1170_002:0695a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者於菩薩幀前。",
        "from": "T20n1170_002:0695a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手執五股金剛杵。",
        "from": "T20n1170_002:0695a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作忿怒相起踏冤家。",
        "from": "T20n1170_002:0695a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦大明一千遍。",
        "from": "T20n1170_002:0695a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切冤家皆悉禁伏。",
        "from": "T20n1170_002:0695a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法用黃臘作冤家像。",
        "from": "T20n1170_002:0695a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者以左足踏復作觀想。",
        "from": "T20n1170_002:0695a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼冤家在百千由旬內。",
        "from": "T20n1170_002:0695a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復觀聖賢遍滿虛空。",
        "from": "T20n1170_002:0695a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得此觀現前已。",
        "from": "T20n1170_002:0695a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼冤自然如癡不能為害。",
        "from": "T20n1170_002:0695a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此成就法若向諸佛作者。",
        "from": "T20n1170_002:0695a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛亦不欲違。",
        "from": "T20n1170_002:0695a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況一切聖賢及人非人等。",
        "from": "T20n1170_002:0695a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說降伏大明曰。",
        "from": "T20n1170_002:0695a28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引一句)曩謨婆誐嚩帝(引二)摩賀(引)嚩日囉(二合)巘(引)馱(引)哩摩賀(引)末攞(三)塞儞也(二合)娑擔(二合)婆儞(引四)攞攞攞攞娑擔(二合)婆野薩哩嚩(二合)設咄嚨(二合五)曩(引)舍薩野(六)娑普(二合引)吒野(七)尾捺囉(二合引)鉢野(八)薩哩嚩(二合)曩(引)舍野(九)薩哩嚩(二合)訥瑟吒(二合引)儞伽(引)怛野(十)摩摩薩哩嚩(二合)迦(引)哩也(二合)抳(十一)矩嚕(十二)矩嚕(十三)虎盧(十四)虎盧(十五)祖盧(十六)祖盧(十七)母盧(十八)母盧(十九)達摩(二十)達摩(二十一)囕誐(二十二)囕誐(二十三)尾捺囉(二合引)鉢野(二十四)尾捺囉(二合引)鉢野(二十五)薩哩嚩(二合)迦(引)哩也(二合引)抳彌(引二十六)禰哩(二合引)茶矩嚕(二十七)薩哩嚩(二合)悉地孕(二合)彌(引二十八)鉢囉(二合)野蹉(二十九)伊呬曳(二合引)呬婆誐嚩帝(引三十)摩賀(引)嚩日囉(二合)巘(引)馱(引)哩(引三十一)郝(入)郝郝郝吽(引)發吒(半音)娑嚩(二合引)賀(引三十二)由誦此明能成就一切事。",
        "from": "T20n1170_002:0695b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能破一切冤家。",
        "from": "T20n1170_002:0695b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說成就法。",
        "from": "T20n1170_002:0695b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於月一日或初七日初八日。",
        "from": "T20n1170_002:0695b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者食赤色食著潔淨衣。",
        "from": "T20n1170_002:0695b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦大明千遍或百遍。",
        "from": "T20n1170_002:0695b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若得身毛喜竪。",
        "from": "T20n1170_002:0695b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於夜分得見種種吉夢已。",
        "from": "T20n1170_002:0695b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此後當依金剛香菩薩作法。",
        "from": "T20n1170_002:0695b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切皆得成就。",
        "from": "T20n1170_002:0695b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲令婆羅門敬愛者。",
        "from": "T20n1170_002:0695b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用生酥作婆羅門像。",
        "from": "T20n1170_002:0695b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然誦大明八百遍。",
        "from": "T20n1170_002:0695b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持白芥子稱婆羅門名。",
        "from": "T20n1170_002:0695b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以白芥子打像彼則敬愛。",
        "from": "T20n1170_002:0695b22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法用鹽加持八百遍。",
        "from": "T20n1170_002:0695b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當稱毘舍名。",
        "from": "T20n1170_002:0695b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每一稱名一誦大明。",
        "from": "T20n1170_002:0695b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用鹽一打彼像。",
        "from": "T20n1170_002:0695b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼毘舍則敬愛。",
        "from": "T20n1170_002:0695b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法用黑豆誦大明加持八百遍。",
        "from": "T20n1170_002:0695b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用炭灰作首陀像。",
        "from": "T20n1170_002:0695b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後一誦大明一稱彼名。",
        "from": "T20n1170_002:0695b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以豆一擲彼像滿八百遍。",
        "from": "T20n1170_002:0695b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼自來敬愛。",
        "from": "T20n1170_002:0695c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次說金剛香菩薩成就根本大明印。",
        "from": "T20n1170_002:0695c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手各作金剛拳。",
        "from": "T20n1170_002:0695c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二小指相交。",
        "from": "T20n1170_002:0695c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指相交如鉤。",
        "from": "T20n1170_002:0695c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二拇指展舒成印。",
        "from": "T20n1170_002:0695c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此印有大威力。",
        "from": "T20n1170_002:0695c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能成就一切最上事。",
        "from": "T20n1170_002:0695c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能破一切冤家。",
        "from": "T20n1170_002:0695c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切天人乃至宿曜。",
        "from": "T20n1170_002:0695c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見此印已驚怖欲死。",
        "from": "T20n1170_002:0695c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯稱念佛以求覆護。",
        "from": "T20n1170_002:0695c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說根本大明曰。",
        "from": "T20n1170_002:0695c07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)曩謨婆誐嚩帝(引一句)摩賀(引)嚩日囉(二合)巘(引)馱(引)哩(引二)入嚩(二合)攞(三)入嚩(二合)攞(四)阿(引)迦哩沙(二合)野(五)阿(引)迦哩沙(二合)野(引六)娑擔(二合)婆野(七)娑擔(二合)婆野(八)悉馱路左[寧*頁](引九)吽(引)發吒(半音十)復說句召印。",
        "from": "T20n1170_002:0695c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不解前印。",
        "from": "T20n1170_002:0695c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二頭指如鉤動搖成印。",
        "from": "T20n1170_002:0695c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此印能句召一切天龍藥叉囉叉娑。",
        "from": "T20n1170_002:0695c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及一切鬼神并宿曜等。",
        "from": "T20n1170_002:0695c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆來可為僮僕。",
        "from": "T20n1170_002:0695c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至或有諸菩薩。",
        "from": "T20n1170_002:0695c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞此大明句召之者。",
        "from": "T20n1170_002:0695c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於剎那之間即至。",
        "from": "T20n1170_002:0695c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說句召大明曰。",
        "from": "T20n1170_002:0695c17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)曩謨婆誐嚩帝(引一句)摩賀(引)嚩日囉(二合)巘(引)馱(引)哩(引二)伊呬曳(引二合)呬阿(引)誐蹉(二合三)阿(引)誐蹉(四)摩摩三摩野(五)摩耨播(引)攞野(六)悉馱室贊(二合)拏嚩日囉(二合)播(引)抳(引七)囉(引)倪鉢野底(八)紇哩(二合引九)郝郝郝郝郝(十)攞攞攞攞斛(十一)若有行人。",
        "from": "T20n1170_002:0695c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為作種種饒益之事。",
        "from": "T20n1170_002:0695c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作曼拏羅已。",
        "from": "T20n1170_002:0695c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲請本尊金剛香菩薩者。",
        "from": "T20n1170_002:0695c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當誦此明結印句召菩薩。",
        "from": "T20n1170_002:0695c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行人復於曼拏羅前。",
        "from": "T20n1170_002:0695c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞑目作觀想菩薩。",
        "from": "T20n1170_002:0695c25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於剎那頃菩薩即來。",
        "from": "T20n1170_002:0695c26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩來時現忿怒相。",
        "from": "T20n1170_002:0695c26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而有六面利牙外出。",
        "from": "T20n1170_002:0695c27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種嚴身光焰熾盛如日初出。",
        "from": "T20n1170_002:0695c27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而有百臂手中各各執於器仗。",
        "from": "T20n1170_002:0695c28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲破一切冤家之相。",
        "from": "T20n1170_002:0695c28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行人審知菩薩來已。",
        "from": "T20n1170_002:0695c29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即想奉上眾寶蓮華之座。",
        "from": "T20n1170_002:0695c29_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩坐已。",
        "from": "T20n1170_002:0696a01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀知行人具真實意。",
        "from": "T20n1170_002:0696a01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即時為說諸佛微妙三昧。",
        "from": "T20n1170_002:0696a02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行人聞已歡喜禮謝。",
        "from": "T20n1170_002:0696a02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次說三昧印。",
        "from": "T20n1170_002:0696a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手作金剛合掌。",
        "from": "T20n1170_002:0696a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舒二拇指二小指相交。",
        "from": "T20n1170_002:0696a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指如環成印。",
        "from": "T20n1170_002:0696a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若結此印三昧現前。",
        "from": "T20n1170_002:0696a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說沐浴印。",
        "from": "T20n1170_002:0696a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手合掌屈二頭指。",
        "from": "T20n1170_002:0696a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二拇指捻二頭指甲如環成印。",
        "from": "T20n1170_002:0696a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每沐浴諸佛賢聖。",
        "from": "T20n1170_002:0696a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結此印即誦明曰。",
        "from": "T20n1170_002:0696a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)曩謨(引)婆誐嚩帝(引一)蘇嚕(二)蘇嚕(三)捺囉(二合)嚩(四)捺囉(二合)嚩(五)娑嚩(二合)賀(引六)復說獻香印。",
        "from": "T20n1170_002:0696a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手合掌屈二頭指如月。",
        "from": "T20n1170_002:0696a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二拇指成印。",
        "from": "T20n1170_002:0696a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每獻香時結此印。",
        "from": "T20n1170_002:0696a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦明曰。",
        "from": "T20n1170_002:0696a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)曩謨(引)婆誐嚩帝(引一)摩賀(引)嚩日囉(二合)巘(引)馱(引)哩(引二)娑囉(三)娑囉(四)尾娑囉(五)尾娑囉(六)尾捺踰(二合引)多野(七)尾捺踰(二合引)多野(八)娑摩(引)迦(引)舍嚩底娑嚩(二合引)賀(引九)復說獻花印。",
        "from": "T20n1170_002:0696a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手合掌舒二拇指。",
        "from": "T20n1170_002:0696a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倒舒二無名指如針成印。",
        "from": "T20n1170_002:0696a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每獻花時結此印。",
        "from": "T20n1170_002:0696a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復誦明曰。",
        "from": "T20n1170_002:0696a19_4",
        "context_with_next_sentence": true
    },
    {
