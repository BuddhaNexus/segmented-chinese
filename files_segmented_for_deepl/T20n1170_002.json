        "from": "T20n1170_002:0696a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手並作拳。",
        "from": "T20n1170_002:0696a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二拇指成印。",
        "from": "T20n1170_002:0696a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每獻燈時結此印。",
        "from": "T20n1170_002:0696a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此明曰。",
        "from": "T20n1170_002:0696a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵曩謨(引)婆誐嚩帝(引)摩賀(引)嚩日囉(二合)巘(引)馱(引)哩(引)入嚩(二合)羅入嚩(二合)羅唧致唧致娑嚩(二合)賀(引)復說塗香印。",
        "from": "T20n1170_002:0696a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手作堅固合掌。",
        "from": "T20n1170_002:0696a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指二中指中節相交。",
        "from": "T20n1170_002:0696a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二拇指按二頭指節成印。",
        "from": "T20n1170_002:0696b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每獻塗香結此印。",
        "from": "T20n1170_002:0696b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復誦明曰。",
        "from": "T20n1170_002:0696b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)曩謨(引)婆誐嚩帝(引一)三滿多巘馱娑嚩(二合)賀(引二)復說出生印。",
        "from": "T20n1170_002:0696b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手作金剛縛。",
        "from": "T20n1170_002:0696b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指竪如針。",
        "from": "T20n1170_002:0696b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指。",
        "from": "T20n1170_002:0696b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二拇指相按。",
        "from": "T20n1170_002:0696b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指附中指成印。",
        "from": "T20n1170_002:0696b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每出生時結此印。",
        "from": "T20n1170_002:0696b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復誦明曰。",
        "from": "T20n1170_002:0696b07_7",
        "context_with_next_sentence": true
    },
