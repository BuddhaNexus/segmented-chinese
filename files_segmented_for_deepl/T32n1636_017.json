[
    {
        "source_sentence": "大乘集菩薩學論卷第十七",
        "from": "T32n1636_017:0122a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫",
        "from": "T32n1636_017:0122a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "試鴻臚少卿宣梵大師賜紫",
        "from": "T32n1636_017:0122a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙門臣日稱等奉　詔譯",
        "from": "T32n1636_017:0122a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念處品第十三之餘",
        "from": "T32n1636_017:0122a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1636_017:0122a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此略說受念處。",
        "from": "T32n1636_017:0122a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次明心念處者。",
        "from": "T32n1636_017:0122a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如寶積經說。",
        "from": "T32n1636_017:0122a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言。",
        "from": "T32n1636_017:0122a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉波。",
        "from": "T32n1636_017:0122a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於此心或生愛樂或起厭患或多封著。",
        "from": "T32n1636_017:0122a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彼三際云何伺察。",
        "from": "T32n1636_017:0122a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若過去則已滅。",
        "from": "T32n1636_017:0122a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現在不住。",
        "from": "T32n1636_017:0122a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未來未至。",
        "from": "T32n1636_017:0122a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非內非外亦非中間。",
        "from": "T32n1636_017:0122a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆不可得。",
        "from": "T32n1636_017:0122a12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此心者不可色見。",
        "from": "T32n1636_017:0122a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非所詮表亦非對治。",
        "from": "T32n1636_017:0122a12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非觀非照無住無著。",
        "from": "T32n1636_017:0122a13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然此心者一切如來尚無所見。",
        "from": "T32n1636_017:0122a13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘何能觀。",
        "from": "T32n1636_017:0122a14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無別境界唯法想轉。",
        "from": "T32n1636_017:0122a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉波。",
        "from": "T32n1636_017:0122a15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是心如幻遍計不實。",
        "from": "T32n1636_017:0122a15_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由所取故種種得生。",
        "from": "T32n1636_017:0122a15_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心如虛空為諸煩惱及隨煩惱客塵所覆。",
        "from": "T32n1636_017:0122a16_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心如河流生滅不住。",
        "from": "T32n1636_017:0122a17_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心如燈光因緣所起。",
        "from": "T32n1636_017:0122a17_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心如擊電剎那不住。",
        "from": "T32n1636_017:0122a18_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心如惡友能生諸苦。",
        "from": "T32n1636_017:0122a18_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心如漁師苦為樂想。",
        "from": "T32n1636_017:0122a19_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心如鬼魅作諸嬈惱。",
        "from": "T32n1636_017:0122a19_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心如藥叉伺噉精氣。",
        "from": "T32n1636_017:0122a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心如狂賊壞諸善根。",
        "from": "T32n1636_017:0122a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心如燈蛾常眄其色。",
        "from": "T32n1636_017:0122a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心如鼙鼓唯警鬪戰。",
        "from": "T32n1636_017:0122a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心如婢僕貪嗜殘味。",
        "from": "T32n1636_017:0122a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是心如蠅觸羶膩器。",
        "from": "T32n1636_017:0122a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是心如猪於不淨中謂為香潔。",
        "from": "T32n1636_017:0122a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言。",
        "from": "T32n1636_017:0122a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉波。",
        "from": "T32n1636_017:0122a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求是心者了不可得。",
        "from": "T32n1636_017:0122a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由不可得故無所得。",
        "from": "T32n1636_017:0122a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於過現未來皆不可得。",
        "from": "T32n1636_017:0122a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則能超越三世有非有等。",
        "from": "T32n1636_017:0122a25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又寶髻經云。",
        "from": "T32n1636_017:0122a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然此心者非外所有。",
        "from": "T32n1636_017:0122a26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故不可見。",
        "from": "T32n1636_017:0122a26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於蘊處界亦不可見。",
        "from": "T32n1636_017:0122a26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是尋求不可見者。",
        "from": "T32n1636_017:0122a27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由何所緣數數得起。",
        "from": "T32n1636_017:0122a27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂彼彼心緣如是事。",
        "from": "T32n1636_017:0122a28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何說為心不可見。",
        "from": "T32n1636_017:0122a28_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應知是心如利劍刃何能自斷。",
        "from": "T32n1636_017:0122a29_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復此心亦如指端指豈能說。",
        "from": "T32n1636_017:0122a29_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心不可見亦復如是。",
        "from": "T32n1636_017:0122b01_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至如人遠行其身輕轉迅疾如風。",
        "from": "T32n1636_017:0122b02_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所至境界無能障礙。",
        "from": "T32n1636_017:0122b02_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子。",
        "from": "T32n1636_017:0122b03_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人於此六處境界自他繫屬。",
        "from": "T32n1636_017:0122b03_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心無愛著身不散亂。",
        "from": "T32n1636_017:0122b04_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則於奢摩他心一境性得無障礙。",
        "from": "T32n1636_017:0122b04_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為心念處。",
        "from": "T32n1636_017:0122b05_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又無盡意經說。",
        "from": "T32n1636_017:0122b05_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以相應行修習莊嚴。",
        "from": "T32n1636_017:0122b06_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於法性心而不減失。",
        "from": "T32n1636_017:0122b06_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何莊嚴彼法性心同於幻化。",
        "from": "T32n1636_017:0122b07_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂若自捨一切所有。",
        "from": "T32n1636_017:0122b07_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而能回向彼法性心。",
        "from": "T32n1636_017:0122b08_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為嚴淨諸佛剎土。",
        "from": "T32n1636_017:0122b08_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法念處者。",
        "from": "T32n1636_017:0122b09_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦如彼說。",
        "from": "T32n1636_017:0122b09_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩於法不觀法行法無可觀故。",
        "from": "T32n1636_017:0122b09_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若非佛法非菩提道。",
        "from": "T32n1636_017:0122b10_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則一切法皆非出離。",
        "from": "T32n1636_017:0122b10_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了知此已得大悲三摩地。",
        "from": "T32n1636_017:0122b11_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切法非有煩惱非無煩惱。",
        "from": "T32n1636_017:0122b11_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得三輪無想。",
        "from": "T32n1636_017:0122b12_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何。",
        "from": "T32n1636_017:0122b12_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了諸法性無有二相。",
        "from": "T32n1636_017:0122b13_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸煩惱無積聚性。",
        "from": "T32n1636_017:0122b13_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無貪性無瞋性無癡性。",
        "from": "T32n1636_017:0122b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能如是悟菩提者。",
        "from": "T32n1636_017:0122b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了煩惱性即菩提性。",
        "from": "T32n1636_017:0122b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為法念處。",
        "from": "T32n1636_017:0122b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又寶髻經云。",
        "from": "T32n1636_017:0122b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子。",
        "from": "T32n1636_017:0122b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩以法觀法念處。",
        "from": "T32n1636_017:0122b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若法起即生。",
        "from": "T32n1636_017:0122b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法謝即滅。",
        "from": "T32n1636_017:0122b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於法爾我人眾生壽者養者士夫補特伽羅意生儒童生老死等。",
        "from": "T32n1636_017:0122b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若集即集行。",
        "from": "T32n1636_017:0122b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不集即不集行。",
        "from": "T32n1636_017:0122b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善不善及不動行。",
        "from": "T32n1636_017:0122b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未有少法無其因緣而得生起。",
        "from": "T32n1636_017:0122b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至於是法處深所伺察。",
        "from": "T32n1636_017:0122b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不捨一切智菩提心故。",
        "from": "T32n1636_017:0122b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如大戲樂經云。",
        "from": "T32n1636_017:0122b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此有為行唯心造作。",
        "from": "T32n1636_017:0122b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶若坯瓶無常散壞。",
        "from": "T32n1636_017:0122b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行如空城為雨沮壞。",
        "from": "T32n1636_017:0122b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "泥塗不堅漸當磨滅。",
        "from": "T32n1636_017:0122b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦如河岸積沙所成自性怯弱為水流蕩。",
        "from": "T32n1636_017:0122b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行如風燈生滅不住。",
        "from": "T32n1636_017:0122b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行如聚沫不可撮持。",
        "from": "T32n1636_017:0122b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行如芭蕉中無有實。",
        "from": "T32n1636_017:0122b25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復如空拳誑示愚騃。",
        "from": "T32n1636_017:0122b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至或草葛等搓撚為繩。",
        "from": "T32n1636_017:0122b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依瓶轆轤而能汲引。",
        "from": "T32n1636_017:0122b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未嘗獨一有彼作用。",
        "from": "T32n1636_017:0122b27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切有支互相依止聚集而轉。",
        "from": "T32n1636_017:0122b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是前後際亦不可得。",
        "from": "T32n1636_017:0122b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如人鑽火二手與木勤劬不息火乃得生。",
        "from": "T32n1636_017:0122b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾緣若離火勢隨滅。",
        "from": "T32n1636_017:0122c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於如是行印所印已。",
        "from": "T32n1636_017:0122c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若自若他即能超越斷常等行。",
        "from": "T32n1636_017:0122c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如智商人於諸險道。",
        "from": "T32n1636_017:0122c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所至方隅而得通達。",
        "from": "T32n1636_017:0122c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1636_017:0122c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由無明愛等煩惱業緣之所招集成蘊處界。",
        "from": "T32n1636_017:0122c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於勝義中皆不可得。",
        "from": "T32n1636_017:0122c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自性清淨品第十四",
        "from": "T32n1636_017:0122c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1636_017:0122c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說念處已。",
        "from": "T32n1636_017:0122c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次復入解補特伽羅。",
        "from": "T32n1636_017:0122c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "決定成就相應空性斷根本已。",
        "from": "T32n1636_017:0122c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘煩惱等不復集行。",
        "from": "T32n1636_017:0122c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又如來祕密經云。",
        "from": "T32n1636_017:0122c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言。",
        "from": "T32n1636_017:0122c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寂慧。",
        "from": "T32n1636_017:0122c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如有樹名鉢羅奢。",
        "from": "T32n1636_017:0122c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若斷其根即一切枝葉悉皆枯瘁。",
        "from": "T32n1636_017:0122c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寂慧。",
        "from": "T32n1636_017:0122c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此亦如是。",
        "from": "T32n1636_017:0122c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若斷身見即能除滅一切煩惱。",
        "from": "T32n1636_017:0122c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1636_017:0122c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分別空性有無量行相。",
        "from": "T32n1636_017:0122c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如月燈經偈云。",
        "from": "T32n1636_017:0122c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若人信解如來教　　於佛戒學無毀犯　悉能遠離諸女人　　知法自性常空寂　拔除一切憂苦箭　　或施醫藥令安穩　速得成斯二足尊　　知法自性常空寂　若於身命斷支節　　杖木捶打無恚惱　忍力最上人中月　　知法自性常空寂　設百生中墮惡道　　常得任持妙色相　亦復獲是五神通　　而常安住諸佛所又般若經云。",
        "from": "T32n1636_017:0122c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次舍利子。",
        "from": "T32n1636_017:0122c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩摩訶薩欲成就佛身三十二大人相及八十種好者。",
        "from": "T32n1636_017:0122c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切生中當念菩提心無令損壞。",
        "from": "T32n1636_017:0122c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於菩薩行得無忘失。",
        "from": "T32n1636_017:0122c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離惡知識及諸過咎。",
        "from": "T32n1636_017:0122c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "親近一切佛菩薩眾善知識等。",
        "from": "T32n1636_017:0122c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲降伏諸天魔眾。",
        "from": "T32n1636_017:0122c25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨諸業障。",
        "from": "T32n1636_017:0122c25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切法得無障礙。",
        "from": "T32n1636_017:0122c26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當學般若波羅蜜多。",
        "from": "T32n1636_017:0122c26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次舍利子。",
        "from": "T32n1636_017:0122c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩摩訶薩如是發一念心。",
        "from": "T32n1636_017:0122c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉能超越東方殑伽沙等世界一切方所當學般若波羅蜜多。",
        "from": "T32n1636_017:0122c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次舍利子。",
        "from": "T32n1636_017:0122c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩摩訶薩設住十方佛剎當學般若波羅蜜多亦如是說。",
        "from": "T32n1636_017:0122c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至廣如諸法無我。",
        "from": "T32n1636_017:0123a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅業障縛則見諸法自性無滅。",
        "from": "T32n1636_017:0123a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如父子合集經云。",
        "from": "T32n1636_017:0123a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言。",
        "from": "T32n1636_017:0123a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大王。",
        "from": "T32n1636_017:0123a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是六界及六觸處十八意所伺察。",
        "from": "T32n1636_017:0123a03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是補嚕沙為緣得生。",
        "from": "T32n1636_017:0123a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何六界。",
        "from": "T32n1636_017:0123a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂地界水界火界風界空界識界。",
        "from": "T32n1636_017:0123a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何六觸處。",
        "from": "T32n1636_017:0123a06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂眼觸處而見於色。",
        "from": "T32n1636_017:0123a06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若耳觸處得聞其聲。",
        "from": "T32n1636_017:0123a06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若鼻觸處能嗅於香。",
        "from": "T32n1636_017:0123a07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若舌觸處悉嘗於味。",
        "from": "T32n1636_017:0123a07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若身觸處親覺其觸。",
        "from": "T32n1636_017:0123a08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若意觸處則知於法。",
        "from": "T32n1636_017:0123a08_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何十八意所伺察。",
        "from": "T32n1636_017:0123a09_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂眼見色已。",
        "from": "T32n1636_017:0123a09_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若生適悅若生憂惱若住於捨。",
        "from": "T32n1636_017:0123a10_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是六根各各緣彼適悅等三。",
        "from": "T32n1636_017:0123a10_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名十八意所伺察。",
        "from": "T32n1636_017:0123a11_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大王。",
        "from": "T32n1636_017:0123a11_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為內地界。",
        "from": "T32n1636_017:0123a12_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於內身所生硬澁髮毛爪齒等若內地界不生亦無有滅則無集行。",
        "from": "T32n1636_017:0123a12_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大王。",
        "from": "T32n1636_017:0123a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若時女人而於內身如所思惟彼補嚕沙。",
        "from": "T32n1636_017:0123a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼補嚕沙亦生愛樂。",
        "from": "T32n1636_017:0123a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由二和合羯邏藍生。",
        "from": "T32n1636_017:0123a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復如所思惟相似和合而得生者。",
        "from": "T32n1636_017:0123a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有是處。",
        "from": "T32n1636_017:0123a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若二女人無有是處。",
        "from": "T32n1636_017:0123a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二補嚕沙亦無是處。",
        "from": "T32n1636_017:0123a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若彼彼思惟而得生者。",
        "from": "T32n1636_017:0123a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦無是處。",
        "from": "T32n1636_017:0123a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自體無實非相應故。",
        "from": "T32n1636_017:0123a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何說此為堅硬性。",
        "from": "T32n1636_017:0123a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大王。",
        "from": "T32n1636_017:0123a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此堅硬性相似而立。",
        "from": "T32n1636_017:0123a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畢竟此身潰爛散滅。",
        "from": "T32n1636_017:0123a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯塚壙間是所歸趣。",
        "from": "T32n1636_017:0123a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼堅硬性從何所來。",
        "from": "T32n1636_017:0123a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦非四方上下而去。",
        "from": "T32n1636_017:0123a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大王。",
        "from": "T32n1636_017:0123a22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此內地界應如是知。",
        "from": "T32n1636_017:0123a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大乘集菩薩學論卷第十七",
        "from": "T32n1636_017:0123a23_0",
        "context_with_next_sentence": false
    }
]