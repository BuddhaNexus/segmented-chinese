[
    {
        "source_sentence": "御製序",
        "from": "X68n1319_006:0527c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顧著曰鑒。",
        "from": "X68n1319_006:0527c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擬問即咦。",
        "from": "X68n1319_006:0527c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "揚眉眨眼。",
        "from": "X68n1319_006:0527c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敗闕如斯。",
        "from": "X68n1319_006:0527c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又道古來老宿為慈悲之故。",
        "from": "X68n1319_006:0527c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有落草之談。",
        "from": "X68n1319_006:0527c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是鑒咦。",
        "from": "X68n1319_006:0527c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "落草也未。",
        "from": "X68n1319_006:0527c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大慈大悲。",
        "from": "X68n1319_006:0527c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那顧喪身失命。",
        "from": "X68n1319_006:0527c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祇這註破。",
        "from": "X68n1319_006:0527c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即今早已落草了也。",
        "from": "X68n1319_006:0527c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛印元曰。",
        "from": "X68n1319_006:0527c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雲門說法如雲雨。",
        "from": "X68n1319_006:0527c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "絕不喜人記錄。",
        "from": "X68n1319_006:0527c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見必罵逐曰。",
        "from": "X68n1319_006:0527c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝口不用。",
        "from": "X68n1319_006:0527c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "反記吾語。",
        "from": "X68n1319_006:0527c13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今室中對機錄。",
        "from": "X68n1319_006:0527c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆香林明教以紙為衣。",
        "from": "X68n1319_006:0527c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨即書之。",
        "from": "X68n1319_006:0527c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朕今刊錄。",
        "from": "X68n1319_006:0527c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刪輯雲門言句。",
        "from": "X68n1319_006:0527c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且道與雲門意旨是同是別。",
        "from": "X68n1319_006:0527c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雲門古德豈畏落草。",
        "from": "X68n1319_006:0527c15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朕亦大丈夫。",
        "from": "X68n1319_006:0527c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈問與雲門是同是別者哉。",
        "from": "X68n1319_006:0527c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖然超情絕解。",
        "from": "X68n1319_006:0527c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直指自心。",
        "from": "X68n1319_006:0527c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如雲門者。",
        "from": "X68n1319_006:0527c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實為奇特。",
        "from": "X68n1319_006:0527c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "垂示後世。",
        "from": "X68n1319_006:0527c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雲門與朕實是大慈大悲。",
        "from": "X68n1319_006:0527c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設使燈籠露柱向前致問。",
        "from": "X68n1319_006:0527c18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還慈悲個什麼。",
        "from": "X68n1319_006:0527c18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "X68n1319_006:0527c19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鑒。",
        "from": "X68n1319_006:0527c19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進云。",
        "from": "X68n1319_006:0527c19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "落草了也。",
        "from": "X68n1319_006:0527c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "X68n1319_006:0527c19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咦。",
        "from": "X68n1319_006:0527c19_15",
        "context_with_next_sentence": false
    }
]