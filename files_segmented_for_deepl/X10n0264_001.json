[
    {
        "source_sentence": "大寶積經述",
        "from": "X10n0264_001:0771a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐朝議郎行河南府告成縣主簿徐鍔撰夫日月出矣。",
        "from": "X10n0264_001:0771a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而輝耀十方。",
        "from": "X10n0264_001:0771a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時雨降矣。",
        "from": "X10n0264_001:0771a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而澡灑萬物。",
        "from": "X10n0264_001:0771a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "況我身常樂。",
        "from": "X10n0264_001:0771a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "湛虗空之相。",
        "from": "X10n0264_001:0771a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙覺圓明。",
        "from": "X10n0264_001:0771a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "融心行之本。",
        "from": "X10n0264_001:0771a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯噭唯昧。",
        "from": "X10n0264_001:0771a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不生不滅者哉。",
        "from": "X10n0264_001:0771a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是以闢無學之地。",
        "from": "X10n0264_001:0771a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聿修迦蘭。",
        "from": "X10n0264_001:0771a08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "啟息言之津。",
        "from": "X10n0264_001:0771a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亶作羅柰。",
        "from": "X10n0264_001:0771a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智勝菩薩。",
        "from": "X10n0264_001:0771a09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起方便之緣。",
        "from": "X10n0264_001:0771a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨居天人。",
        "from": "X10n0264_001:0771a09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發成就之力。",
        "from": "X10n0264_001:0771a10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱謂所絕者。",
        "from": "X10n0264_001:0771a10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其第一義乎。",
        "from": "X10n0264_001:0771a10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自恆星夜掩。",
        "from": "X10n0264_001:0771a10_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仙虹晝爍。",
        "from": "X10n0264_001:0771a11_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "青鉢傳其睟容。",
        "from": "X10n0264_001:0771a11_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶棺現其金臂。",
        "from": "X10n0264_001:0771a11_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法山摧仞。",
        "from": "X10n0264_001:0771a11_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拂魔箭於危屏。",
        "from": "X10n0264_001:0771a12_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直水橫流。",
        "from": "X10n0264_001:0771a12_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫慈航於彼岸。",
        "from": "X10n0264_001:0771a12_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烏虖。",
        "from": "X10n0264_001:0771a12_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙藏不可以常祕。",
        "from": "X10n0264_001:0771a12_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戒輪不可以終盭。",
        "from": "X10n0264_001:0771a13_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖雙林下。",
        "from": "X10n0264_001:0771a13_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "砉示於泥洹。",
        "from": "X10n0264_001:0771a13_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逮一千年。",
        "from": "X10n0264_001:0771a14_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遹被於聲教。",
        "from": "X10n0264_001:0771a14_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍持貝葉。",
        "from": "X10n0264_001:0771a14_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亟傳摩竭之城。",
        "from": "X10n0264_001:0771a14_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "象負蓮華。",
        "from": "X10n0264_001:0771a15_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂滿真丹之境。",
        "from": "X10n0264_001:0771a15_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十七品。",
        "from": "X10n0264_001:0771a15_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈悲於火宅。",
        "from": "X10n0264_001:0771a15_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一十二經。",
        "from": "X10n0264_001:0771a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "引喻於沙界矣。",
        "from": "X10n0264_001:0771a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大寶積經者。",
        "from": "X10n0264_001:0771a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後漢迦葉摩騰竺法蘭。",
        "from": "X10n0264_001:0771a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及今朝玄奘法師。",
        "from": "X10n0264_001:0771a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩提流志等。",
        "from": "X10n0264_001:0771a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸自西天竺所致也。",
        "from": "X10n0264_001:0771a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來昔在鷲峯。",
        "from": "X10n0264_001:0771a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利建平等。",
        "from": "X10n0264_001:0771a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金口注海。",
        "from": "X10n0264_001:0771a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酌之而不竭。",
        "from": "X10n0264_001:0771a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶言如綸。",
        "from": "X10n0264_001:0771a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "振之而有緒。",
        "from": "X10n0264_001:0771a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烱茲瑞憲。",
        "from": "X10n0264_001:0771a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "久翳鴻都。",
        "from": "X10n0264_001:0771a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "原壑屢非。",
        "from": "X10n0264_001:0771a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "市朝多變。",
        "from": "X10n0264_001:0771a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歷代侚齊之主。",
        "from": "X10n0264_001:0771a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "競興參譯。",
        "from": "X10n0264_001:0771a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "跋陁授記之言。",
        "from": "X10n0264_001:0771a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "罕能丕就。",
        "from": "X10n0264_001:0771a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洎我唐之有天下也。",
        "from": "X10n0264_001:0771a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功橫鐵圍。",
        "from": "X10n0264_001:0771a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化緜忉利。",
        "from": "X10n0264_001:0771a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苑禦千界。",
        "from": "X10n0264_001:0771a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提封萬剎。",
        "from": "X10n0264_001:0771a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "張四攝之扉。",
        "from": "X10n0264_001:0771a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣納諸有。",
        "from": "X10n0264_001:0771a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "騁六道之驥。",
        "from": "X10n0264_001:0771a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冥濟羣惑。",
        "from": "X10n0264_001:0771a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "太上皇以澤深智海。",
        "from": "X10n0264_001:0771a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掌耀禪珠。",
        "from": "X10n0264_001:0771a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神皇帝以勛格梵空。",
        "from": "X10n0264_001:0771a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "胸懸法印。",
        "from": "X10n0264_001:0771a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "肅敷玄誥。",
        "from": "X10n0264_001:0771a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照灑鴻波。",
        "from": "X10n0264_001:0771a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歷選緇徒。",
        "from": "X10n0264_001:0771b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明敡列[穴/釆]。",
        "from": "X10n0264_001:0771b01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "博考同異。",
        "from": "X10n0264_001:0771b01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聿興刊緝。",
        "from": "X10n0264_001:0771b01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勇振頺綱。",
        "from": "X10n0264_001:0771b01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚴持絕紐。",
        "from": "X10n0264_001:0771b02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爰有沙門大德思忠。",
        "from": "X10n0264_001:0771b02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東天竺國婆羅門大首領臣伊舍羅等。",
        "from": "X10n0264_001:0771b02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譯梵文者。",
        "from": "X10n0264_001:0771b03_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求善住緣。",
        "from": "X10n0264_001:0771b03_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟無生忍。",
        "from": "X10n0264_001:0771b03_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "博聞強識。",
        "from": "X10n0264_001:0771b04_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精而譯之。",
        "from": "X10n0264_001:0771b04_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有天竺沙門波若屈多。",
        "from": "X10n0264_001:0771b04_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙門達摩。",
        "from": "X10n0264_001:0771b04_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證梵義者。",
        "from": "X10n0264_001:0771b05_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開忍辱場。",
        "from": "X10n0264_001:0771b05_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破煩惱眾。",
        "from": "X10n0264_001:0771b05_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弼諧神侶。",
        "from": "X10n0264_001:0771b05_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明而辨之。",
        "from": "X10n0264_001:0771b05_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有沙門大德履方。",
        "from": "X10n0264_001:0771b06_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宗一。",
        "from": "X10n0264_001:0771b06_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普敬慧覺等筆授者。",
        "from": "X10n0264_001:0771b06_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令聞孔膠。",
        "from": "X10n0264_001:0771b07_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "威儀不忒。",
        "from": "X10n0264_001:0771b07_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手握仙札。",
        "from": "X10n0264_001:0771b07_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受而字之。",
        "from": "X10n0264_001:0771b07_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有沙門大德深亮。",
        "from": "X10n0264_001:0771b07_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勝莊。",
        "from": "X10n0264_001:0771b08_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塵外。",
        "from": "X10n0264_001:0771b08_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無著。",
        "from": "X10n0264_001:0771b08_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧迪等證義者。",
        "from": "X10n0264_001:0771b08_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國之大師。",
        "from": "X10n0264_001:0771b08_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛之右臂。",
        "from": "X10n0264_001:0771b09_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "探諸了義。",
        "from": "X10n0264_001:0771b09_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "演而證之。",
        "from": "X10n0264_001:0771b09_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有大德沙門承禮。",
        "from": "X10n0264_001:0771b09_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雲觀。",
        "from": "X10n0264_001:0771b10_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神暕。",
        "from": "X10n0264_001:0771b10_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道本等次文者。",
        "from": "X10n0264_001:0771b10_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庇影多林。",
        "from": "X10n0264_001:0771b10_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "息肩香窟。",
        "from": "X10n0264_001:0771b10_54",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勤修精進。",
        "from": "X10n0264_001:0771b10_55",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纂而次之。",
        "from": "X10n0264_001:0771b11_56",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有潤文官者。",
        "from": "X10n0264_001:0771b11_57",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "銀青光祿大夫邠王傅上柱國固安縣開國伯盧粲。",
        "from": "X10n0264_001:0771b11_58",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "銀青光祿大夫太子詹事崇文館學士兼修國史上柱國東海縣開國公徐堅。",
        "from": "X10n0264_001:0771b12_59",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朝議大夫守中書舍人崇文館學士上柱國舒王縣開國男蘇晉。",
        "from": "X10n0264_001:0771b14_60",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朝議郎給事中內供奉崔璩等。",
        "from": "X10n0264_001:0771b15_61",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "位列鳳墀。",
        "from": "X10n0264_001:0771b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聲流雞圃。",
        "from": "X10n0264_001:0771b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分別二諦。",
        "from": "X10n0264_001:0771b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "潤而色之。",
        "from": "X10n0264_001:0771b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有銀青光祿大夫守侍中兼太子左庶子兼修國史上柱國鉅鹿縣開國公魏知古。",
        "from": "X10n0264_001:0771b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兵部尚書上柱國郭元振。",
        "from": "X10n0264_001:0771b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "銀青光祿大夫檢校中書令上柱國范陽縣開國男張說。",
        "from": "X10n0264_001:0771b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "銀青光祿大夫行中書侍郎同中書門下三品監修國史上柱國興平縣開國侯陸象先等。",
        "from": "X10n0264_001:0771b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朝踐瑣闈。",
        "from": "X10n0264_001:0771b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夕游珠域。",
        "from": "X10n0264_001:0771b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護持四法。",
        "from": "X10n0264_001:0771b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總而閱之。",
        "from": "X10n0264_001:0771b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾乃杖錫之士。",
        "from": "X10n0264_001:0771b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端珪之俊。",
        "from": "X10n0264_001:0771b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "麻列定筵。",
        "from": "X10n0264_001:0771b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "林攢樂土。",
        "from": "X10n0264_001:0771b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蔭祥雲而演譯。",
        "from": "X10n0264_001:0771b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倐換炎涼。",
        "from": "X10n0264_001:0771b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吸甘露而勤求。",
        "from": "X10n0264_001:0771b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "載淹衡晷。",
        "from": "X10n0264_001:0771b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大乘章句。",
        "from": "X10n0264_001:0771b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義不唐捐。",
        "from": "X10n0264_001:0771b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小品精微。",
        "from": "X10n0264_001:0771b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拯無遺溺。",
        "from": "X10n0264_001:0771c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能事畢矣。",
        "from": "X10n0264_001:0771c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛何言哉。",
        "from": "X10n0264_001:0771c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今所新翻經。",
        "from": "X10n0264_001:0771c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡有四十九會七十七品。",
        "from": "X10n0264_001:0771c01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合一十二帙。",
        "from": "X10n0264_001:0771c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以類相從。",
        "from": "X10n0264_001:0771c02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "撰寫咸畢。",
        "from": "X10n0264_001:0771c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以先天二年六月三十日進太上皇。",
        "from": "X10n0264_001:0771c03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八月二十一日進皇帝。",
        "from": "X10n0264_001:0771c03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禁闈曉闢。",
        "from": "X10n0264_001:0771c04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真教上聞。",
        "from": "X10n0264_001:0771c04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仙宇克怡。",
        "from": "X10n0264_001:0771c04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宸襟允穆。",
        "from": "X10n0264_001:0771c04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竦鈞陣於白日。",
        "from": "X10n0264_001:0771c05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "親御靈臺。",
        "from": "X10n0264_001:0771c05_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "落雲雨於彤霄。",
        "from": "X10n0264_001:0771c05_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "荐加殊尉。",
        "from": "X10n0264_001:0771c05_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢愚稽首。",
        "from": "X10n0264_001:0771c06_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為利見仁王。",
        "from": "X10n0264_001:0771c06_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真俗歸心。",
        "from": "X10n0264_001:0771c06_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為潛登覺道。",
        "from": "X10n0264_001:0771c06_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次有清信佛弟子。",
        "from": "X10n0264_001:0771c07_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前少府監丞李式顏等。",
        "from": "X10n0264_001:0771c07_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皇朝金紫光祿大夫兵部尚書贈侍中隴西公逈秀子也。",
        "from": "X10n0264_001:0771c07_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有清信佛弟子。",
        "from": "X10n0264_001:0771c08_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前右拾遺徐鐈等。",
        "from": "X10n0264_001:0771c09_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皇朝銀青光祿大夫太子賓客昭文館學士高平公字也。",
        "from": "X10n0264_001:0771c09_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸屬彼穹降禍。",
        "from": "X10n0264_001:0771c10_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "私門墜構。",
        "from": "X10n0264_001:0771c11_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陟遙岵而崩心。",
        "from": "X10n0264_001:0771c11_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞻冥途而獻福。",
        "from": "X10n0264_001:0771c11_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是胠篋探笥。",
        "from": "X10n0264_001:0771c11_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "檀波羅蜜。",
        "from": "X10n0264_001:0771c12_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣疊簡牋。",
        "from": "X10n0264_001:0771c12_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "首崇書寫。",
        "from": "X10n0264_001:0771c12_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不變槐火。",
        "from": "X10n0264_001:0771c12_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遽盈苔帙。",
        "from": "X10n0264_001:0771c12_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後裝之鏤軸。",
        "from": "X10n0264_001:0771c13_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "綴以瓊籤。",
        "from": "X10n0264_001:0771c13_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅綵簟而霓舒。",
        "from": "X10n0264_001:0771c13_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "播珠函而錦縟。",
        "from": "X10n0264_001:0771c13_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方使猛風吹嶽。",
        "from": "X10n0264_001:0771c14_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長存拓路之文。",
        "from": "X10n0264_001:0771c14_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劫火燒天。",
        "from": "X10n0264_001:0771c14_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不壞多羅之典。",
        "from": "X10n0264_001:0771c15_50",
        "context_with_next_sentence": false
    }
]