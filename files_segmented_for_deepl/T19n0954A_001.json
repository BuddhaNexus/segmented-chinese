[
    {
        "source_sentence": "一字頂輪王念誦儀軌",
        "from": "T19n0954A_001:0307c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿",
        "from": "T19n0954A_001:0307c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "肅國公食邑三千戶賜紫贈司",
        "from": "T19n0954A_001:0307c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空諡大鑒正號大廣智大興善",
        "from": "T19n0954A_001:0307c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寺三藏沙門不空奉　詔譯",
        "from": "T19n0954A_001:0307c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今依忉利天宮會釋迦牟尼如來所說無比力超勝世間出世間真言上上一切佛頂主宰一字頂輪王念誦儀則。",
        "from": "T19n0954A_001:0307c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行者先當入此頂輪王大曼荼羅。",
        "from": "T19n0954A_001:0307c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得阿闍梨灌頂印可。",
        "from": "T19n0954A_001:0307c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方受此法。",
        "from": "T19n0954A_001:0307c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須善明解。",
        "from": "T19n0954A_001:0307c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後於清淨處安本尊像。",
        "from": "T19n0954A_001:0307c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面西稽首禮。",
        "from": "T19n0954A_001:0307c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受三歸捨身說罪受戒發菩提心隨喜勸請發願迴向已。",
        "from": "T19n0954A_001:0307c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應結佛部三昧耶印。",
        "from": "T19n0954A_001:0307c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手內相叉。",
        "from": "T19n0954A_001:0307c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雙並。",
        "from": "T19n0954A_001:0307c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二大拇指。",
        "from": "T19n0954A_001:0307c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成。",
        "from": "T19n0954A_001:0307c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名一切如來心印真言曰。",
        "from": "T19n0954A_001:0307c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵邇那邇(入聲呼)",
        "from": "T19n0954A_001:0307c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結蓮華部三昧耶印。",
        "from": "T19n0954A_001:0307c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前佛部心印。",
        "from": "T19n0954A_001:0307c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈左大拇指入掌。",
        "from": "T19n0954A_001:0307c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右大指准前直竪即成。",
        "from": "T19n0954A_001:0307c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名蓮華部心印真言曰。",
        "from": "T19n0954A_001:0307c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿嚧力",
        "from": "T19n0954A_001:0307c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結金剛部三昧耶印。",
        "from": "T19n0954A_001:0307c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前佛部心印。",
        "from": "T19n0954A_001:0307c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈右大指入掌。",
        "from": "T19n0954A_001:0307c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直竪左大拇指即成。",
        "from": "T19n0954A_001:0307c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名金剛部心印真言曰。",
        "from": "T19n0954A_001:0307c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉(二合)地力(二合)",
        "from": "T19n0954A_001:0307c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結甲冑印。",
        "from": "T19n0954A_001:0307c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手內相叉。",
        "from": "T19n0954A_001:0307c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二中指各屈上節如劍形。",
        "from": "T19n0954A_001:0307c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二頭指各拄中指背。",
        "from": "T19n0954A_001:0307c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印加持額右肩左肩心喉五處一遍真言曰。",
        "from": "T19n0954A_001:0307c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵斫羯囉(二合)靺[口*栗]底鉢囉(二合)賖弭多囉捺囉(二合引)囉捺囉(二合引)婆(去)娑摩(二合)車盧瑟尼(二合)沙[口*路]乞灑(二合)[口*路]乞灑(二合)[牟*含]吽發吒(半音)娑嚩(二合引)訶(引)次結佛眼印。",
        "from": "T19n0954A_001:0308a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手合掌。",
        "from": "T19n0954A_001:0308a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指各拄中指背。",
        "from": "T19n0954A_001:0308a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並屈二大拇指入掌。",
        "from": "T19n0954A_001:0308a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印真言加持五處真言曰。",
        "from": "T19n0954A_001:0308a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱(引)南(引一)唵嚕嚕薩普(二合)嚕入嚩(二合)羅底瑟咤(二合)悉馱盧者寧薩嚩囉他(二合)娑(引)達泥娑嚩(二合引)訶次結大海印。",
        "from": "T19n0954A_001:0308a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手內相叉。",
        "from": "T19n0954A_001:0308a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌。",
        "from": "T19n0954A_001:0308a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擘開二大指。",
        "from": "T19n0954A_001:0308a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印右旋三匝。",
        "from": "T19n0954A_001:0308a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想成大海水真言曰。",
        "from": "T19n0954A_001:0308a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵微摩嚧娜地娑嚩(二合引)訶(引)次於大海中想須彌盧山四寶所成。",
        "from": "T19n0954A_001:0308a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手內相叉。",
        "from": "T19n0954A_001:0308a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "急握作拳。",
        "from": "T19n0954A_001:0308a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合腕並竪。",
        "from": "T19n0954A_001:0308a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成真言曰。",
        "from": "T19n0954A_001:0308a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿者羅吽",
        "from": "T19n0954A_001:0308a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次於須彌盧山上。",
        "from": "T19n0954A_001:0308a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想七寶樓閣。",
        "from": "T19n0954A_001:0308a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結加持寶樓閣印。",
        "from": "T19n0954A_001:0308a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手金剛合掌。",
        "from": "T19n0954A_001:0308a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左右十指各交初分即成真言曰。",
        "from": "T19n0954A_001:0308a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱(引)南(引)唵薩嚩他欠嗢娜蘖(二合)帝薩頗(二合)囉呬(引)[牟*含]誐誐曩劍(平聲呼)娑嚩(二合引)訶(引)次結佛頂輪王印。",
        "from": "T19n0954A_001:0308a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉作拳。",
        "from": "T19n0954A_001:0308a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二中指屈上節如劍形。",
        "from": "T19n0954A_001:0308a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並竪二大指。",
        "from": "T19n0954A_001:0308a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指捻二大指頭上。",
        "from": "T19n0954A_001:0308a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成。",
        "from": "T19n0954A_001:0308a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印五處加護真言曰。",
        "from": "T19n0954A_001:0308a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱(引)南步嚕唵(三合)次結網橛印。",
        "from": "T19n0954A_001:0308a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0308a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指上節。",
        "from": "T19n0954A_001:0308a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "背不相著。",
        "from": "T19n0954A_001:0308a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大拇指各壓上下。",
        "from": "T19n0954A_001:0308b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "揮轉即成結上下界真言曰。",
        "from": "T19n0954A_001:0308b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱(引)南(引)阿鉢囉(二合)底賀多舍娑娜南唵微枳囉拏微特防(二合)娑尼迦比羅貳嚩里尼怛囉(二合)娑耶嚩日囉(二合)吠賖薩帝呶(引)囉特嚩(二合)能(上)瑟吒囉(二合)[口*路]吃沙(二合)[牟*含]發次結牆印。",
        "from": "T19n0954A_001:0308b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0308b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指兩節相逼。",
        "from": "T19n0954A_001:0308b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平竪二大拇指。",
        "from": "T19n0954A_001:0308b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "附二頭指。",
        "from": "T19n0954A_001:0308b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋三匝即成金剛牆界真言曰。",
        "from": "T19n0954A_001:0308b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "噁(引)莫壑(歸命同前)",
        "from": "T19n0954A_001:0308b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結車輅印。",
        "from": "T19n0954A_001:0308b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉仰掌。",
        "from": "T19n0954A_001:0308b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "申二頭指令甲側相拄。",
        "from": "T19n0954A_001:0308b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二大指各柱頭指根下。",
        "from": "T19n0954A_001:0308b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想於他方世界。",
        "from": "T19n0954A_001:0308b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉迎本尊真言曰。",
        "from": "T19n0954A_001:0308b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵覩嚕覩嚕吽",
        "from": "T19n0954A_001:0308b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結迎車輅印。",
        "from": "T19n0954A_001:0308b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前車輅印。",
        "from": "T19n0954A_001:0308b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大指各撥中指頭。",
        "from": "T19n0954A_001:0308b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向身三招真言曰。",
        "from": "T19n0954A_001:0308b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨悉底哩也(四合)地尾(二合)迦南呾他蘖多南唵嚩日[口*朗](二合)儗(妍以反)儞也(二合)羯哩沙(二合)耶娑嚩(二合引)訶(若奉送除羯哩沙二合耶迦尾薩若耶)次結迎請印。",
        "from": "T19n0954A_001:0308b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0308b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈右頭指於中指後。",
        "from": "T19n0954A_001:0308b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向前三招真言曰。",
        "from": "T19n0954A_001:0308b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨婆誐嚩帝阿鉢囉(二合)底賀覩瑟尼(二合)沙耶翳呬翳呬婆誐吻達麼囉惹鉢囉(二合)底掣(鴟奚反)南遏鉗巘談補澀奔(補甘反二合)度奔(上)末隣儞半者滿遮避路吃沙(二合)阿鉢囉(二合)底賀多麼囉鉢囉(二合)羯囉(二合)麼耶娑嚩(二合引)訶(引)次結一切辦事佛頂印。",
        "from": "T19n0954A_001:0308b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手內相叉。",
        "from": "T19n0954A_001:0308c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二中指屈上節如劍形。",
        "from": "T19n0954A_001:0308c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸供養物及浴水洗淨土等並用此真言。",
        "from": "T19n0954A_001:0308c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持辟除去垢結界皆用此真言。",
        "from": "T19n0954A_001:0308c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左旋辟除右旋結界真言曰。",
        "from": "T19n0954A_001:0308c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱南(一)唵吒嚕唵(三合)滿馱娑嚩(二合引)訶(引)次重結前網橛印。",
        "from": "T19n0954A_001:0308c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一用結上方界。",
        "from": "T19n0954A_001:0308c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結阿娑莽倪(倪枳反)尼印。",
        "from": "T19n0954A_001:0308c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "側二手左掩右。",
        "from": "T19n0954A_001:0308c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二大指即成。",
        "from": "T19n0954A_001:0308c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右轉一匝即成密縫真言曰。",
        "from": "T19n0954A_001:0308c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿娑莽倪尼吽發",
        "from": "T19n0954A_001:0308c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結獻閼伽印。",
        "from": "T19n0954A_001:0308c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0308c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指各附中指。",
        "from": "T19n0954A_001:0308c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二大指。",
        "from": "T19n0954A_001:0308c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各附頭指根側真言曰。",
        "from": "T19n0954A_001:0308c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱南唵閼伽囉訶閼伽必哩(二合)野鉢囉(二合)底掣(鴟奚反)娜末鉗娑嚩(二合引)訶(引)次重結根本印。",
        "from": "T19n0954A_001:0308c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結獻師子座印。",
        "from": "T19n0954A_001:0309a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0309a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指於二大指甲側。",
        "from": "T19n0954A_001:0309a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0954A_001:0309a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿者囉尾囉耶娑嚩(二合引)訶(引)次結塗香印。",
        "from": "T19n0954A_001:0309a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0309a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈右頭指倚於左中指下節真言曰。",
        "from": "T19n0954A_001:0309a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多　沒馱南唵怛[口*賴](二合)盧枳也(二合)巘馱蘖帝吽吽發發娑嚩(二合引)訶次結獻花印。",
        "from": "T19n0954A_001:0309a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前塗香印。",
        "from": "T19n0954A_001:0309a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "改左頭指。",
        "from": "T19n0954A_001:0309a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倚左中指下節真言曰。",
        "from": "T19n0954A_001:0309a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱南(引)唵薩嚩盧迦補澁波(二合)步多耶吽吽發發娑嚩(二合引)訶(引)次結燒香印。",
        "from": "T19n0954A_001:0309a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0309a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指各倚於中指中節真言曰。",
        "from": "T19n0954A_001:0309a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱南唵尾囉蘖跢微",
        "from": "T19n0954A_001:0309a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘖多度跛耶吽吽發發娑嚩(二合引)訶(引)次結獻食印。",
        "from": "T19n0954A_001:0309a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0309a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指上節。",
        "from": "T19n0954A_001:0309a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各附於大指側真言曰。",
        "from": "T19n0954A_001:0309a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱南唵薩嚩盧迦麼",
        "from": "T19n0954A_001:0309a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "里必哩(二合)夜(引)吽吽發發娑嚩(二合引)訶(引)次結獻燈明印。",
        "from": "T19n0954A_001:0309a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0309a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指兩節。",
        "from": "T19n0954A_001:0309a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令不相著。",
        "from": "T19n0954A_001:0309a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指各附於頭指上真言曰。",
        "from": "T19n0954A_001:0309a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱南唵薩嚩盧迦珊捺囉(二合)捨那耶吽吽發發娑嚩(二合)訶(引)次結普供養加持印　二手虛心合掌上頂各兩節相交真言曰。",
        "from": "T19n0954A_001:0309a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱南冒地薩怛嚩(二合)南唵薩嚩怛囉(二合)僧俱蘇弭多避枳惹囉始寧曩謨薩覩(二合)諦娑嚩(二合)訶(引)次結遍照佛頂印。",
        "from": "T19n0954A_001:0309a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉。",
        "from": "T19n0954A_001:0309b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合為拳。",
        "from": "T19n0954A_001:0309b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令二中指節微起。",
        "from": "T19n0954A_001:0309b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0954A_001:0309b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱南噁(引)莫[山*含]次結白傘蓋佛頂印。",
        "from": "T19n0954A_001:0309b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大指。",
        "from": "T19n0954A_001:0309b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各捻二無名指甲上側。",
        "from": "T19n0954A_001:0309b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相合二頭指屈如蓋形。",
        "from": "T19n0954A_001:0309b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指微屈相合。",
        "from": "T19n0954A_001:0309b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二小指各竪相合真言曰。",
        "from": "T19n0954A_001:0309b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱南阿鉢囉(二合)底賀多舍娑那南唵麼麼麼吽匿(儞翼反)次結光聚佛頂印。",
        "from": "T19n0954A_001:0309b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前白傘蓋印。",
        "from": "T19n0954A_001:0309b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坼開二頭指即成真言曰。",
        "from": "T19n0954A_001:0309b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱南阿鉢囉(二合)底賀多舍娑娜南唵怛他櫱都瑟尼(二合)娑阿耶嚩盧枳多慕[口*栗]馱帝儒囉始吽入嚩(二合)攞入嚩(二合)攞馱迦馱迦娜囉娜囉微娜囉微娜囉嗔那嗔那頻那頻那吽吽發發娑嚩(二合引)訶(引)次結高佛頂印。",
        "from": "T19n0954A_001:0309b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前白傘蓋印。",
        "from": "T19n0954A_001:0309b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指。",
        "from": "T19n0954A_001:0309b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各拄中指中節背。",
        "from": "T19n0954A_001:0309b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0954A_001:0309b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱南阿鉢囉(二合)底賀多舍娑娜南唵(引)儞[卑*也]儞[卑*也]卑瑜(二合)娜誐都瑟尼(二合)沙吽吽發發娑嚩(二合)訶(引)次結勝佛頂印。",
        "from": "T19n0954A_001:0309b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准高佛頂印。",
        "from": "T19n0954A_001:0309b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "移二頭指。",
        "from": "T19n0954A_001:0309b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向上兩[麩夫+廣]麥許真言曰。",
        "from": "T19n0954A_001:0309b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱南阿鉢囉(二合)底賀多舍娑那南唵入嚩(二合)羅惹諭瑟尼(二合)沙吽發娑嚩(二合引)訶(引)次結摧毀佛頂(尾枳囉拏)印。",
        "from": "T19n0954A_001:0309b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手內相叉作拳。",
        "from": "T19n0954A_001:0309b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二中指屈節。",
        "from": "T19n0954A_001:0309b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右中指拄左中指面。",
        "from": "T19n0954A_001:0309b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令出半節許真言曰。",
        "from": "T19n0954A_001:0309b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱南阿鉢囉(二合)底賀多舍娑娜南唵尾枳囉拏度那度那犢(引)次結摧碎佛頂印。",
        "from": "T19n0954A_001:0309c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前改左中指。",
        "from": "T19n0954A_001:0309c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拄右中指面。",
        "from": "T19n0954A_001:0309c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦出半節許真言曰。",
        "from": "T19n0954A_001:0309c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨三滿多沒馱南阿鉢囉(二合)底賀多舍娑那喃唵阿鉢囉(二合)底賀都瑟尼(二合)沙耶薩嚩尾伽曩(二合)尾特望(二合)娑那迦囉耶怛嚧(二合)吒耶娑嚩(二合)訶(引)次結輪王佛頂心印。",
        "from": "T19n0954A_001:0309c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0309c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指各拄中指上節真言曰。",
        "from": "T19n0954A_001:0309c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱南阿鉢囉(二合)底賀多舍娑娜南唵怛他櫱都瑟尼(二合)沙阿那嚩盧枳多沒馱尼斫羯羅(二合)靺[口*栗](二合)底吽入嚩(二合)羅入嚩(二合)羅馱迦馱迦度那微度那怛囉(二合)娑耶麼囉逾瑳囉耶賀那賀那伴惹伴惹暗惡屩鉢龍(二合)企尼君吒哩尼阿鉢囉(二合)爾多薩怛羅(二合)馱哩尼吽發娑嚩(二合)訶次結心中心印。",
        "from": "T19n0954A_001:0309c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0310a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指各加於二中指上節上真言曰。",
        "from": "T19n0954A_001:0310a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱南阿鉢囉(二合)底賀多舍娑那南唵阿鉢囉(二合)爾多特(他翼反)普通諸佛頂印二手虛心金剛合掌。",
        "from": "T19n0954A_001:0310a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如花在掌中。",
        "from": "T19n0954A_001:0310a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行者若急遽不能遍結諸佛頂印。",
        "from": "T19n0954A_001:0310a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但結此印誦諸佛頂真言。",
        "from": "T19n0954A_001:0310a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結頂印。",
        "from": "T19n0954A_001:0310a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0310a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈右頭指。",
        "from": "T19n0954A_001:0310a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪於右中指後。",
        "from": "T19n0954A_001:0310a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令不相著真言曰。",
        "from": "T19n0954A_001:0310a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱南阿鉢囉(三合)底賀多舍娑那南唵斫羯羅(二合)靺[口*栗](二合)底唵吽次結頭印。",
        "from": "T19n0954A_001:0310a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0310a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開二頭指各直竪於中指後。",
        "from": "T19n0954A_001:0310a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令不相著微屈真言曰(歸命同上)。",
        "from": "T19n0954A_001:0310a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵斫羯囉(二合)靺[口*栗](二合)底吽發娑嚩(二合引)訶(引)次又結根本印。",
        "from": "T19n0954A_001:0310a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結大三昧耶印。",
        "from": "T19n0954A_001:0310a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加護本尊二手內相叉。",
        "from": "T19n0954A_001:0310a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二中指。",
        "from": "T19n0954A_001:0310a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指中指後。",
        "from": "T19n0954A_001:0310a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如鉤相去一[麩夫+廣]麥許。",
        "from": "T19n0954A_001:0310a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大指各附頭指根下。",
        "from": "T19n0954A_001:0310a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋三匝真言曰。",
        "from": "T19n0954A_001:0310a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵商羯哩摩訶三昧延娑嚩(二合)訶(引)次誦一百八名讚讚歎。",
        "from": "T19n0954A_001:0310a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲念誦。",
        "from": "T19n0954A_001:0310a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以五支成本尊。",
        "from": "T19n0954A_001:0310a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或五相成本尊瑜伽。",
        "from": "T19n0954A_001:0310a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於三處(頂舌心也)想一字頂輪。",
        "from": "T19n0954A_001:0310a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成本尊坐八葉蓮華。",
        "from": "T19n0954A_001:0310a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一一葉上想七寶。",
        "from": "T19n0954A_001:0310a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯當前蓮華葉上想佛眼尊。",
        "from": "T19n0954A_001:0310a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應持珠(此依菩提道場所說經)。",
        "from": "T19n0954A_001:0310a28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次持珠合掌捧珠。",
        "from": "T19n0954A_001:0310a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦淨珠真言七遍真言曰。",
        "from": "T19n0954A_001:0310a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿娜步(二合)低尾惹曳(而奚反)悉地悉馱囉梯娑嚩(二合引)訶次結持珠印。",
        "from": "T19n0954A_001:0310b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手各以大指。",
        "from": "T19n0954A_001:0310b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捻無名指甲上。",
        "from": "T19n0954A_001:0310b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直竪二中指二小指。",
        "from": "T19n0954A_001:0310b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指於中指後。",
        "from": "T19n0954A_001:0310b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令不相著(如毘盧遮那經半金剛杵印)。",
        "from": "T19n0954A_001:0310b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0954A_001:0310b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨婆誐嚩底蘇悉第　娑馱耶悉馱囉梯娑嚩(二合引)訶(引)　次應淨其心　　如法而念誦　持珠令當心　　繫心於鼻端　字句分明呼　　不緩亦不急　不頻伸噦嗌　　咳嗽與唾涕　染等心相應　　及心緣苦受　如是等過患　　皆不得成就當念誦時。",
        "from": "T19n0954A_001:0310b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身心不得疲怠。",
        "from": "T19n0954A_001:0310b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若勞倦即應結五供養印。",
        "from": "T19n0954A_001:0310b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦讚歎獻閼伽念。",
        "from": "T19n0954A_001:0310b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦畢持珠頂上。",
        "from": "T19n0954A_001:0310b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結前密縫印左轉一匝。",
        "from": "T19n0954A_001:0310b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成解界。",
        "from": "T19n0954A_001:0310b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結奉送印。",
        "from": "T19n0954A_001:0310b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前根本印。",
        "from": "T19n0954A_001:0310b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左頭指外擲。",
        "from": "T19n0954A_001:0310b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦迎請真言除翳呬翳呬。",
        "from": "T19n0954A_001:0310b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加蘖車蘖車句。",
        "from": "T19n0954A_001:0310b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成奉送。",
        "from": "T19n0954A_001:0310b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次應復結牆及網橛等印。",
        "from": "T19n0954A_001:0310b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加護處上下及所成就物。",
        "from": "T19n0954A_001:0310b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結計里枳里印。",
        "from": "T19n0954A_001:0310b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左大指壓左小指甲上。",
        "from": "T19n0954A_001:0310b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘三指頭拆開。",
        "from": "T19n0954A_001:0310b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直竪如三股杵形。",
        "from": "T19n0954A_001:0310b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋轉三匝成結界真言曰。",
        "from": "T19n0954A_001:0310b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵枳里枳里嚩日囉(二合)吽發",
        "from": "T19n0954A_001:0310b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結軍吒利印。",
        "from": "T19n0954A_001:0310b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二小指於掌中交。",
        "from": "T19n0954A_001:0310b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二無名指小指上。",
        "from": "T19n0954A_001:0310b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大指壓二無名指上。",
        "from": "T19n0954A_001:0310b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪二中指相合。",
        "from": "T19n0954A_001:0310b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈二頭指於中指後令一麥許不相著。",
        "from": "T19n0954A_001:0310b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋三匝即成結界真言曰。",
        "from": "T19n0954A_001:0310c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨囉怛曩(二合)怛囉(二合)夜(引)也曩莫室戰(二合)拏嚩日囉(二合)跛拏曳摩訶藥乞叉(二合)細曩跛多曳曩莫室戰(二合)拏嚩日囉(二合)句路馱耶唵虎嚕虎嚕底瑟吒(二合)底瑟吒(二合)滿馱滿馱呵那呵那阿蜜哩(二合)帝吽發娑嚩(二合)訶　從一字真言　　至於十五字　每計於字數　　十字一洛叉　乃至於三字　　應誦三洛叉　應作先事法　　三十字已上　應誦一萬遍一字頂輪王念誦儀軌一卷",
        "from": "T19n0954A_001:0310c02_0",
        "context_with_next_sentence": false
    }
]