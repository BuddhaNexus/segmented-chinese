[
    {
        "source_sentence": "穢跡金剛說神通大滿陀羅尼法",
        "from": "T21n1228_001:0158a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "術靈要門一卷",
        "from": "T21n1228_001:0158a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北天竺國三藏沙門阿質",
        "from": "T21n1228_001:0158a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "達霰唐言無能勝將譯",
        "from": "T21n1228_001:0158a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T21n1228_001:0158a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在拘尸那國力士生處跋提河邊娑羅雙樹間。",
        "from": "T21n1228_001:0158a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時如來臨入涅槃。",
        "from": "T21n1228_001:0158a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時有無量百千萬眾天龍八部人非人等。",
        "from": "T21n1228_001:0158a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "啼泣向佛四面哽咽悲惱而住。",
        "from": "T21n1228_001:0158a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時復有諸天大眾釋提桓因等。",
        "from": "T21n1228_001:0158a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆來供養。",
        "from": "T21n1228_001:0158a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯有蠡髻梵王。",
        "from": "T21n1228_001:0158a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將諸天女。",
        "from": "T21n1228_001:0158a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依於四面圍繞而坐。",
        "from": "T21n1228_001:0158a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後天女千萬億眾。",
        "from": "T21n1228_001:0158a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共相娛樂。",
        "from": "T21n1228_001:0158a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞如來入般涅槃而不來覲省。",
        "from": "T21n1228_001:0158a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時諸大眾為言。",
        "from": "T21n1228_001:0158a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今日如來臨般涅槃。",
        "from": "T21n1228_001:0158a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是彼梵王何不來耶。",
        "from": "T21n1228_001:0158a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其王必有我慢之心。",
        "from": "T21n1228_001:0158a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而不來至此。",
        "from": "T21n1228_001:0158a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我等徒眾驅使小呪仙。",
        "from": "T21n1228_001:0158a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往彼令取。",
        "from": "T21n1228_001:0158a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是語已策百千眾呪仙。",
        "from": "T21n1228_001:0158a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到於彼處。",
        "from": "T21n1228_001:0158a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃見種種不淨而為城塹。",
        "from": "T21n1228_001:0158a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其仙見已。",
        "from": "T21n1228_001:0158a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各犯呪而死。",
        "from": "T21n1228_001:0158a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時諸大眾怪未曾有。",
        "from": "T21n1228_001:0158a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復策無量金剛。",
        "from": "T21n1228_001:0158a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦令持呪而去。",
        "from": "T21n1228_001:0158a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至七日無人取得。",
        "from": "T21n1228_001:0158a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大眾見是事倍復悲哀。",
        "from": "T21n1228_001:0158a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時大眾同聲而說偈言。",
        "from": "T21n1228_001:0158a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　苦哉大聖尊　　入真何太速",
        "from": "T21n1228_001:0158a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　諸天猶決定　　天人追喚得",
        "from": "T21n1228_001:0158a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　痛哉天中天　　入真如火滅",
        "from": "T21n1228_001:0158a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時諸大眾說此偈已。",
        "from": "T21n1228_001:0158a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倍復哽咽悲啼嘷哭。",
        "from": "T21n1228_001:0158a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時如來愍諸大眾。",
        "from": "T21n1228_001:0158a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以大遍知神力。",
        "from": "T21n1228_001:0158a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨左心化出不壞金剛。",
        "from": "T21n1228_001:0158a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於眾中從座而起。",
        "from": "T21n1228_001:0158b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白大眾言。",
        "from": "T21n1228_001:0158b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我有大神呪能取彼梵王。",
        "from": "T21n1228_001:0158b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是語已即於大眾之中顯大神通。",
        "from": "T21n1228_001:0158b02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變此三千大千世界六返震動。",
        "from": "T21n1228_001:0158b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天宮龍宮諸鬼神宮皆悉崩摧。",
        "from": "T21n1228_001:0158b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即自騰身至梵王所。",
        "from": "T21n1228_001:0158b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以指指之。",
        "from": "T21n1228_001:0158b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其彼醜穢物變為大地。",
        "from": "T21n1228_001:0158b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時金剛至彼報言。",
        "from": "T21n1228_001:0158b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝大愚癡我如來欲入涅槃。",
        "from": "T21n1228_001:0158b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝何不去。",
        "from": "T21n1228_001:0158b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以金剛不壞之力。",
        "from": "T21n1228_001:0158b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微以指之。",
        "from": "T21n1228_001:0158b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵王發心至如來所。",
        "from": "T21n1228_001:0158b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時大眾讚言。",
        "from": "T21n1228_001:0158b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大力士汝能有是神力。",
        "from": "T21n1228_001:0158b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取彼梵王來至於此。",
        "from": "T21n1228_001:0158b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時金剛即報言。",
        "from": "T21n1228_001:0158b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有世間眾生。",
        "from": "T21n1228_001:0158b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被諸天惡魔一切外道所惱亂者但誦我呪十萬遍。",
        "from": "T21n1228_001:0158b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我自現身令一切有情隨意滿足。",
        "from": "T21n1228_001:0158b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永離貧窮常令安樂。",
        "from": "T21n1228_001:0158b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其呪如是。",
        "from": "T21n1228_001:0158b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先發此大願。",
        "from": "T21n1228_001:0158b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無我本師釋迦牟尼佛。",
        "from": "T21n1228_001:0158b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於如來滅後受持此呪。",
        "from": "T21n1228_001:0158b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誓度群生。",
        "from": "T21n1228_001:0158b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令佛法不滅久住於世。",
        "from": "T21n1228_001:0158b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是願已。",
        "from": "T21n1228_001:0158b16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說大圓滿陀羅尼神呪穢跡真言曰。",
        "from": "T21n1228_001:0158b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵[口*佛]咶啒[口*聿]摩訶鉢囉(二合)[口*恨]那[得-彳+口](三音)吻汁吻(四音)微咭微摩那栖(六音)嗚深慕(七音)啒[口*聿][合*牛][合*牛]泮泮泮娑訶時彼金剛說此呪已。",
        "from": "T21n1228_001:0158b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復作是言。",
        "from": "T21n1228_001:0158b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我於如來滅後常誦此呪。",
        "from": "T21n1228_001:0158b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生請願受持此呪者。",
        "from": "T21n1228_001:0158b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我常為給使者令所求如願。",
        "from": "T21n1228_001:0158b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今於如來前說此神呪。",
        "from": "T21n1228_001:0158b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願如來於真中照知我等。",
        "from": "T21n1228_001:0158b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊若有眾生。",
        "from": "T21n1228_001:0158b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多被諸惡鬼神之所惱亂。",
        "from": "T21n1228_001:0158b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此呪者皆不能為害。",
        "from": "T21n1228_001:0158b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永離苦難。",
        "from": "T21n1228_001:0158b26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊若有善男子善女人。",
        "from": "T21n1228_001:0158b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲救療萬病者。",
        "from": "T21n1228_001:0158b27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦上呪四十萬遍。",
        "from": "T21n1228_001:0158b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見有病者治之有驗。",
        "from": "T21n1228_001:0158b27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無問淨與不淨隨意驅使。",
        "from": "T21n1228_001:0158b28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當隨從滿一切願。",
        "from": "T21n1228_001:0158b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲令枯樹生枝葉者。",
        "from": "T21n1228_001:0158b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取白膠香一大兩。",
        "from": "T21n1228_001:0158c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗樹心。",
        "from": "T21n1228_001:0158c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "楊枝呪樹一百遍。",
        "from": "T21n1228_001:0158c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日三時至滿三日即生華果。",
        "from": "T21n1228_001:0158c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲令枯泉出水者。",
        "from": "T21n1228_001:0158c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨灰圍之。",
        "from": "T21n1228_001:0158c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即井華水三斗置泉水中。",
        "from": "T21n1228_001:0158c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於寅時呪一百八遍。",
        "from": "T21n1228_001:0158c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水如車輪涌出。",
        "from": "T21n1228_001:0158c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲令枯山生草木。",
        "from": "T21n1228_001:0158c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取鑌鐵刀一口。",
        "from": "T21n1228_001:0158c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於四方圍山。",
        "from": "T21n1228_001:0158c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三千遍七日滿則生。",
        "from": "T21n1228_001:0158c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲令野獸歸伏者。",
        "from": "T21n1228_001:0158c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取安悉香燒向有獸住處呪一千遍。",
        "from": "T21n1228_001:0158c07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其獸至夜間。",
        "from": "T21n1228_001:0158c07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並集持法人門首。",
        "from": "T21n1228_001:0158c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸降如人間六畜相似。",
        "from": "T21n1228_001:0158c08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意驅使永不相捨。",
        "from": "T21n1228_001:0158c09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若令夜叉自來歸降者。",
        "from": "T21n1228_001:0158c09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取桃枝十翦齊截。",
        "from": "T21n1228_001:0158c10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取水一石煎取五斗。",
        "from": "T21n1228_001:0158c10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "澇桃柳枝出。",
        "from": "T21n1228_001:0158c11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以丁香三大兩乳頭香三大兩白膠香三大兩。",
        "from": "T21n1228_001:0158c11_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後和柳水煎取五斗。",
        "from": "T21n1228_001:0158c12_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即置一破盆中。",
        "from": "T21n1228_001:0158c12_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取一桃枝長三尺。",
        "from": "T21n1228_001:0158c13_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攪水誦呪一百遍。",
        "from": "T21n1228_001:0158c13_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切夜叉羅剎皆來現。",
        "from": "T21n1228_001:0158c14_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共行法人語。",
        "from": "T21n1228_001:0158c14_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請求與人先為侍者。",
        "from": "T21n1228_001:0158c14_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若令諸惡鬼神毒蛇蝎猛獸等毒以滅者。",
        "from": "T21n1228_001:0158c15_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取淨灰圍所居穴孔。",
        "from": "T21n1228_001:0158c16_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並自出來。",
        "from": "T21n1228_001:0158c16_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當微出聲呪之一百遍。",
        "from": "T21n1228_001:0158c17_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其蛇等一切蠱獸。",
        "from": "T21n1228_001:0158c17_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各滅毒心不敢傷人。",
        "from": "T21n1228_001:0158c18_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速得解脫。",
        "from": "T21n1228_001:0158c18_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若令惡狗不傷人者。",
        "from": "T21n1228_001:0158c18_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取食一摶呪七遍與食。",
        "from": "T21n1228_001:0158c19_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永不傷人復不出聲。",
        "from": "T21n1228_001:0158c19_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若令惡人來降伏者。",
        "from": "T21n1228_001:0158c20_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書前人姓名置呪人脚下。",
        "from": "T21n1228_001:0158c20_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪之百遍。",
        "from": "T21n1228_001:0159a01_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心念彼人其人立至。",
        "from": "T21n1228_001:0159a01_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降伏捨怨憎之心。",
        "from": "T21n1228_001:0159a02_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲人相憎者。",
        "from": "T21n1228_001:0159a02_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書彼二人名號。",
        "from": "T21n1228_001:0159a02_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於自足下呪二百一十八遍。",
        "from": "T21n1228_001:0159a03_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人等便相離背不相愛敬。",
        "from": "T21n1228_001:0159a03_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有相憎人令相愛敬者。",
        "from": "T21n1228_001:0159a04_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即書取彼名姓。",
        "from": "T21n1228_001:0159a05_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於自足下呪一百八遍。",
        "from": "T21n1228_001:0159a05_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人便相愛重永不相捨。",
        "from": "T21n1228_001:0159a06_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有未安樂之人令安樂者。",
        "from": "T21n1228_001:0159a06_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取前人名字書足下。",
        "from": "T21n1228_001:0159a07_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三百遍。",
        "from": "T21n1228_001:0159a07_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當為彼人發大誓願。",
        "from": "T21n1228_001:0159a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我於彼時即自送。",
        "from": "T21n1228_001:0159a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辯才無滯隨行者意。",
        "from": "T21n1228_001:0159a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所須之者並悉施與。",
        "from": "T21n1228_001:0159a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若持呪人求種種珍寶摩尼如意珠等者。",
        "from": "T21n1228_001:0159a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但至心誦呪自限多少。",
        "from": "T21n1228_001:0159a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我即自送滿其所願。",
        "from": "T21n1228_001:0159a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲治人病者。",
        "from": "T21n1228_001:0159a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作頓病印。",
        "from": "T21n1228_001:0159a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先以左手頭指中指。",
        "from": "T21n1228_001:0159a12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "押索文。",
        "from": "T21n1228_001:0159a12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印呪之一百遍以印頓病人七下立差。",
        "from": "T21n1228_001:0159a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若病人臨欲死者。",
        "from": "T21n1228_001:0159a13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先於禁五路印然後治之。",
        "from": "T21n1228_001:0159a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即不死印目。",
        "from": "T21n1228_001:0159a14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是先准印以無名指屈向掌中。",
        "from": "T21n1228_001:0159a15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竪小指呪之百遍其患速除。",
        "from": "T21n1228_001:0159a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若治邪病者。",
        "from": "T21n1228_001:0159a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但於病患人頭邊。",
        "from": "T21n1228_001:0159a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒安悉香誦之。",
        "from": "T21n1228_001:0159a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪立除之。",
        "from": "T21n1228_001:0159a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若蟲毒病者。",
        "from": "T21n1228_001:0159a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書患人名字紙上呪之即差。",
        "from": "T21n1228_001:0159a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "治精魅病者亦如法。",
        "from": "T21n1228_001:0159a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若治伏連病者。",
        "from": "T21n1228_001:0159a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書患人姓名及作病鬼姓名。",
        "from": "T21n1228_001:0159a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "埋患人床下呪之。",
        "from": "T21n1228_001:0159a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其鬼遠奉名字自出現身。",
        "from": "T21n1228_001:0159a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便令彼鬼看三世之事。",
        "from": "T21n1228_001:0159a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一具說向人其病速差。",
        "from": "T21n1228_001:0159a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有患時氣病者。",
        "from": "T21n1228_001:0159a22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪師見之即差。",
        "from": "T21n1228_001:0159a23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲令行病鬼王不入界者。",
        "from": "T21n1228_001:0159a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於十齋日誦我此呪一千八遍。",
        "from": "T21n1228_001:0159a24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除萬里病患。",
        "from": "T21n1228_001:0159a25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "穢跡金剛說神通大滿陀羅尼法術靈要門一卷",
        "from": "T21n1228_001:0159a26_0",
        "context_with_next_sentence": false
    }
]