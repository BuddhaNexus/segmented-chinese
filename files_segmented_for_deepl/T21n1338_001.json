[
    {
        "source_sentence": "呪三首經",
        "from": "T21n1338_001:0640a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐三藏地婆訶羅奉　詔譯",
        "from": "T21n1338_001:0640a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大輪金剛陀羅尼一",
        "from": "T21n1338_001:0640a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "訥謨薩怛唎耶　地毘迦喃　怛他揭多喃　唵毘囉　時毘囉時　摩訶斫迦囉　跋闍唎薩多薩多娑囉帝娑囉帝　怛唎曳怛唎曳毘陀末儞　三槃若儞　怛囉末底悉陀阿揭唎怛炎　娑婆訶誦此陀羅尼三七遍即當入一切漫荼羅(壇也)所作皆成(誦呪。",
        "from": "T21n1338_001:0640a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有身印手印。",
        "from": "T21n1338_001:0640a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作印誦呪法即易成。",
        "from": "T21n1338_001:0640a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若未入壇。",
        "from": "T21n1338_001:0640a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得輙作。",
        "from": "T21n1338_001:0640a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今令誦此呪。",
        "from": "T21n1338_001:0640a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即當入壇作印行不成盜法也)。",
        "from": "T21n1338_001:0640a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日光菩薩呪二",
        "from": "T21n1338_001:0640a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南謨勃陀瞿(上聲)那(上聲)迷　南摩達摩　莫訶南謨僧伽多也泥底(丁以切)哩部卑薩(僧沒切)咄(登沒反)擔(平聲)納摩(誦此呪滅一切障亦能辟魔及除天災。",
        "from": "T21n1338_001:0640a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又若誦一遍禮佛一拜。",
        "from": "T21n1338_001:0640a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如此日別三時誦呪禮佛。",
        "from": "T21n1338_001:0640a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未來當得二相端正具足果報)。",
        "from": "T21n1338_001:0640a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩利支天呪三",
        "from": "T21n1338_001:0640b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(上)摩利支曳　薩婆薩多婆頞陀利沙利娑(上聲)婆(去聲)訶誦此呪極護人身。",
        "from": "T21n1338_001:0640b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當日別三時各誦一百八遍。",
        "from": "T21n1338_001:0640b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三首經",
        "from": "T21n1338_001:0640b07_0",
        "context_with_next_sentence": false
    }
]