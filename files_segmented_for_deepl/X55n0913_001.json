[
    {
        "source_sentence": "天台智者大師發願文",
        "from": "X55n0913_001:0688c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫欲念誦。",
        "from": "X55n0913_001:0688c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅罪障。",
        "from": "X55n0913_001:0688c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "助菩提。",
        "from": "X55n0913_001:0688c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應須發願。",
        "from": "X55n0913_001:0688c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先淨三業。",
        "from": "X55n0913_001:0688c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十指合掌。",
        "from": "X55n0913_001:0688c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面向諸佛。",
        "from": "X55n0913_001:0688c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口陳辭句。",
        "from": "X55n0913_001:0688c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是言。",
        "from": "X55n0913_001:0688c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弟子比丘某甲(餘眾各准此稱名)。",
        "from": "X55n0913_001:0688c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稽首和南。",
        "from": "X55n0913_001:0688c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心歸命盡十方三世一切諸佛．尊法．般若．十二部經真淨妙法．諸大菩薩摩訶薩眾．諸賢聖僧．常住三寶。",
        "from": "X55n0913_001:0688c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願神力。",
        "from": "X55n0913_001:0688c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈哀護念。",
        "from": "X55n0913_001:0688c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願當證知。",
        "from": "X55n0913_001:0688c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此念誦。",
        "from": "X55n0913_001:0688c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善根功德。",
        "from": "X55n0913_001:0688c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "專用莊嚴四恩三有．盡十方法界一切眾生。",
        "from": "X55n0913_001:0688c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願共法界諸眾生等。",
        "from": "X55n0913_001:0688c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從無始已來。",
        "from": "X55n0913_001:0688c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至於今日。",
        "from": "X55n0913_001:0688c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昏沈倒惑。",
        "from": "X55n0913_001:0688c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六根．三業造一切鄣道之罪。",
        "from": "X55n0913_001:0688c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若麤若細。",
        "from": "X55n0913_001:0688c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "齊佛所知。",
        "from": "X55n0913_001:0688c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時消滅。",
        "from": "X55n0913_001:0688c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生生世世。",
        "from": "X55n0913_001:0688c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永離地獄．餓鬼．畜生身。",
        "from": "X55n0913_001:0688c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永離女人身。",
        "from": "X55n0913_001:0688c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永離邊地．下賤．八難身。",
        "from": "X55n0913_001:0688c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願共法界諸眾生等。",
        "from": "X55n0913_001:0688c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於此生。",
        "from": "X55n0913_001:0688c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於餘生。",
        "from": "X55n0913_001:0688c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常值善知識。",
        "from": "X55n0913_001:0688c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常聞無上大乘正法。",
        "from": "X55n0913_001:0688c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於此身。",
        "from": "X55n0913_001:0688c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟解常住佛性。",
        "from": "X55n0913_001:0688c21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大涅槃海。",
        "from": "X55n0913_001:0688c22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸法。",
        "from": "X55n0913_001:0688c22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本來寂滅。",
        "from": "X55n0913_001:0688c22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具大菩提。",
        "from": "X55n0913_001:0688c22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了達根塵能所。",
        "from": "X55n0913_001:0688c22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一念起心。",
        "from": "X55n0913_001:0688c23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具一切法。",
        "from": "X55n0913_001:0688c23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百界三千。",
        "from": "X55n0913_001:0688c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空．假．中。",
        "from": "X55n0913_001:0688c23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即法界印。",
        "from": "X55n0913_001:0688c23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可思議。",
        "from": "X55n0913_001:0688c24_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心．佛．眾生。",
        "from": "X55n0913_001:0688c24_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三無差別。",
        "from": "X55n0913_001:0688c24_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體權識實。",
        "from": "X55n0913_001:0688c24_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發大菩提心。",
        "from": "X55n0913_001:0688c24_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上求下化。",
        "from": "X55n0913_001:0689a01_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拔生死苦。",
        "from": "X55n0913_001:0689a01_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與究竟樂。",
        "from": "X55n0913_001:0689a01_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修牢強精進。",
        "from": "X55n0913_001:0689a01_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四種三昧。",
        "from": "X55n0913_001:0689a02_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "只不思議一觀三觀。",
        "from": "X55n0913_001:0689a02_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀不思議一諦三諦。",
        "from": "X55n0913_001:0689a02_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀諦冥一。",
        "from": "X55n0913_001:0689a03_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即四魔三鄣。",
        "from": "X55n0913_001:0689a03_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歷緣對境。",
        "from": "X55n0913_001:0689a03_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以妙十法成圓乘。",
        "from": "X55n0913_001:0689a03_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行解更資。",
        "from": "X55n0913_001:0689a04_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心心明寂。",
        "from": "X55n0913_001:0689a04_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發真中道。",
        "from": "X55n0913_001:0689a04_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開佛知見。",
        "from": "X55n0913_001:0689a04_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心心現前。",
        "from": "X55n0913_001:0689a04_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有慧身。",
        "from": "X55n0913_001:0689a05_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不由佗悟。",
        "from": "X55n0913_001:0689a05_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然流入薩婆若海。",
        "from": "X55n0913_001:0689a05_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住無生忍。",
        "from": "X55n0913_001:0689a05_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具大智慧。",
        "from": "X55n0913_001:0689a06_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成摩訶衍。",
        "from": "X55n0913_001:0689a06_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通達漸頓諸教。",
        "from": "X55n0913_001:0689a06_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破一微塵。",
        "from": "X55n0913_001:0689a06_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出大千經卷。",
        "from": "X55n0913_001:0689a06_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒沙佛法。",
        "from": "X55n0913_001:0689a07_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心中曉。",
        "from": "X55n0913_001:0689a07_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覺佛慈悲。",
        "from": "X55n0913_001:0689a07_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無諸慳恡。",
        "from": "X55n0913_001:0689a07_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說於止觀。",
        "from": "X55n0913_001:0689a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍施一切。",
        "from": "X55n0913_001:0689a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開門傾藏。",
        "from": "X55n0913_001:0689a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨如意珠。",
        "from": "X55n0913_001:0689a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "理即．名字．觀行．相似．分真．究竟。",
        "from": "X55n0913_001:0689a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓伊三點。",
        "from": "X55n0913_001:0689a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不縱不橫。",
        "from": "X55n0913_001:0689a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正法大城。",
        "from": "X55n0913_001:0689a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛寶藏。",
        "from": "X55n0913_001:0689a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切佛法。",
        "from": "X55n0913_001:0689a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自行化佗。",
        "from": "X55n0913_001:0689a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一生有辦。",
        "from": "X55n0913_001:0689a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設未明了。",
        "from": "X55n0913_001:0689a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非因之因。",
        "from": "X55n0913_001:0689a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從因感果。",
        "from": "X55n0913_001:0689a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不果之果。",
        "from": "X55n0913_001:0689a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲勝果報。",
        "from": "X55n0913_001:0689a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願共法界諸眾生等。",
        "from": "X55n0913_001:0689a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨命終時。",
        "from": "X55n0913_001:0689a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七日之前。",
        "from": "X55n0913_001:0689a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自知時節。",
        "from": "X55n0913_001:0689a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心不顛倒。",
        "from": "X55n0913_001:0689a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心不錯亂。",
        "from": "X55n0913_001:0689a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心不共念。",
        "from": "X55n0913_001:0689a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身心無諸苦痛。",
        "from": "X55n0913_001:0689a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "跏趺正念。",
        "from": "X55n0913_001:0689a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身心安樂。",
        "from": "X55n0913_001:0689a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入深三昧。",
        "from": "X55n0913_001:0689a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見十方佛。",
        "from": "X55n0913_001:0689a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願彌陀世尊．三世諸佛．觀音．勢至．諸大菩薩．一切聖眾。",
        "from": "X55n0913_001:0689a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不捨本誓。",
        "from": "X55n0913_001:0689a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉現在前。",
        "from": "X55n0913_001:0689a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同時放大光明。",
        "from": "X55n0913_001:0689a15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照觸身心。",
        "from": "X55n0913_001:0689a16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有罪障。",
        "from": "X55n0913_001:0689a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時消滅。",
        "from": "X55n0913_001:0689a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸根豁然。",
        "from": "X55n0913_001:0689a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深入知見。",
        "from": "X55n0913_001:0689a17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大神力。",
        "from": "X55n0913_001:0689a17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝受身心。",
        "from": "X55n0913_001:0689a17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "授金色手。",
        "from": "X55n0913_001:0689a17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩頂接引。",
        "from": "X55n0913_001:0689a17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡喜踊躍。",
        "from": "X55n0913_001:0689a18_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘佛願力。",
        "from": "X55n0913_001:0689a18_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐金剛臺。",
        "from": "X55n0913_001:0689a18_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨從如來聖眾。",
        "from": "X55n0913_001:0689a18_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一念頃。",
        "from": "X55n0913_001:0689a18_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上品上生西方阿彌陀佛極樂世界。",
        "from": "X55n0913_001:0689a19_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金蓮化生。",
        "from": "X55n0913_001:0689a19_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟入無生寂滅諸忍。",
        "from": "X55n0913_001:0689a20_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "超登上地。",
        "from": "X55n0913_001:0689a20_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲六神通。",
        "from": "X55n0913_001:0689a20_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住首楞嚴。",
        "from": "X55n0913_001:0689a20_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不動法性。",
        "from": "X55n0913_001:0689a21_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一念頃。",
        "from": "X55n0913_001:0689a21_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遊歷十方。",
        "from": "X55n0913_001:0689a21_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉事三世一切諸佛。",
        "from": "X55n0913_001:0689a21_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一佛前。",
        "from": "X55n0913_001:0689a22_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉有我身。",
        "from": "X55n0913_001:0689a22_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修諸供養。",
        "from": "X55n0913_001:0689a22_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞法受記。",
        "from": "X55n0913_001:0689a22_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時普能受持三世如來甚深法海。",
        "from": "X55n0913_001:0689a22_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即自明解。",
        "from": "X55n0913_001:0689a23_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不由佗悟。",
        "from": "X55n0913_001:0689a23_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大理。",
        "from": "X55n0913_001:0689a23_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大願。",
        "from": "X55n0913_001:0689a24_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大莊嚴。",
        "from": "X55n0913_001:0689a24_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大智斷。",
        "from": "X55n0913_001:0689a24_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大遍知。",
        "from": "X55n0913_001:0689a24_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大道。",
        "from": "X55n0913_001:0689a24_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大用。",
        "from": "X55n0913_001:0689a24_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大權實。",
        "from": "X55n0913_001:0689a24_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大利益。",
        "from": "X55n0913_001:0689a24_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大無住。",
        "from": "X55n0913_001:0689b01_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛所有一切波羅蜜行。",
        "from": "X55n0913_001:0689b01_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時修習。",
        "from": "X55n0913_001:0689b01_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與文殊．普賢等諸大士為伴侶。",
        "from": "X55n0913_001:0689b01_54",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足一切諸菩薩無量無邊廣大行願。",
        "from": "X55n0913_001:0689b02_55",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得諸大陀羅尼門．諸三昧門。",
        "from": "X55n0913_001:0689b03_56",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普知諸佛剎嚴淨佛國。",
        "from": "X55n0913_001:0689b03_57",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於剎那頃。",
        "from": "X55n0913_001:0689b04_58",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普於十方一切微塵道中。",
        "from": "X55n0913_001:0689b04_59",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成等正覺。",
        "from": "X55n0913_001:0689b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉大法輪。",
        "from": "X55n0913_001:0689b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "度脫眾生。",
        "from": "X55n0913_001:0689b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無緣慈悲。",
        "from": "X55n0913_001:0689b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普覆法界。",
        "from": "X55n0913_001:0689b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不動真際。",
        "from": "X55n0913_001:0689b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普入三世。",
        "from": "X55n0913_001:0689b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "度脫一切眾生。",
        "from": "X55n0913_001:0689b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或作佛身。",
        "from": "X55n0913_001:0689b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施權實。",
        "from": "X55n0913_001:0689b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或為九界像。",
        "from": "X55n0913_001:0689b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對揚漸頓。",
        "from": "X55n0913_001:0689b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請轉漸頓。",
        "from": "X55n0913_001:0689b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弘通漸頓。",
        "from": "X55n0913_001:0689b07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三輪攝化。",
        "from": "X55n0913_001:0689b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝化無窮。",
        "from": "X55n0913_001:0689b08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同虗空遍界。",
        "from": "X55n0913_001:0689b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而作佛事。",
        "from": "X55n0913_001:0689b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從本垂迹。",
        "from": "X55n0913_001:0689b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝迹還本。",
        "from": "X55n0913_001:0689b08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本迹雖殊。",
        "from": "X55n0913_001:0689b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不思議一。",
        "from": "X55n0913_001:0689b09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自佗行圓。",
        "from": "X55n0913_001:0689b09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常住秘藏。",
        "from": "X55n0913_001:0689b09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法",
        "from": "X55n0913_001:0689b09_19",
        "context_with_next_sentence": false
    }
]