[
    {
        "source_sentence": "註楞伽阿跋多羅寶經卷第一",
        "from": "X17n0321_001:0091a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　迦葉拘留孫　　拘那含是我",
        "from": "X17n0321_001:0091a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一百一問。",
        "from": "X17n0321_001:0091a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以世間見現在釋迦。",
        "from": "X17n0321_001:0091a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與過去三佛有異。",
        "from": "X17n0321_001:0091a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將明化跡有殊。",
        "from": "X17n0321_001:0091a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而德體無二。",
        "from": "X17n0321_001:0091a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此會權歸下四等廣明。",
        "from": "X17n0321_001:0091a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　何故說斷常　　及與我無我",
        "from": "X17n0321_001:0091a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一百二問。",
        "from": "X17n0321_001:0091a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛以何故。",
        "from": "X17n0321_001:0091a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說斷常及我無我。",
        "from": "X17n0321_001:0091a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　何不一切時　　演說真實義",
        "from": "X17n0321_001:0091a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一百三問。",
        "from": "X17n0321_001:0091a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何故不說真實義邪。",
        "from": "X17n0321_001:0091a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　而復為眾生　　分別說心量",
        "from": "X17n0321_001:0091a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一百四問。",
        "from": "X17n0321_001:0091a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復以何故說於心量。",
        "from": "X17n0321_001:0091a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　何因男女林　　訶梨阿摩勒",
        "from": "X17n0321_001:0091a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一百五問。",
        "from": "X17n0321_001:0091a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間果實。",
        "from": "X17n0321_001:0091a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何故不同。",
        "from": "X17n0321_001:0091a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有巨而味甘。",
        "from": "X17n0321_001:0091a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有形如男女。",
        "from": "X17n0321_001:0091a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具如立世毗曇論明。",
        "from": "X17n0321_001:0091a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　雞羅及鐵圍　　金剛等諸山",
        "from": "X17n0321_001:0091a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一百六問。",
        "from": "X17n0321_001:0091a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "按梵李。",
        "from": "X17n0321_001:0091a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雞羅婆山。",
        "from": "X17n0321_001:0091a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及鐵圍金剛山等。",
        "from": "X17n0321_001:0091a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並以何差別此十山之數。",
        "from": "X17n0321_001:0091a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仙聖所居。",
        "from": "X17n0321_001:0091a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今略舉三。",
        "from": "X17n0321_001:0091a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "意該餘七。",
        "from": "X17n0321_001:0091a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無量寶莊嚴",
        "from": "X17n0321_001:0091a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一百七問。",
        "from": "X17n0321_001:0091a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此等諸山。",
        "from": "X17n0321_001:0091a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以何土石所致。",
        "from": "X17n0321_001:0091a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及眾寶而成。",
        "from": "X17n0321_001:0091a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　仙闥婆充滿",
        "from": "X17n0321_001:0091b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一百八問。",
        "from": "X17n0321_001:0091b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闥婆者。",
        "from": "X17n0321_001:0091b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天樂神之稱。",
        "from": "X17n0321_001:0091b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居在地上寶山之中。",
        "from": "X17n0321_001:0091b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若天須樂。",
        "from": "X17n0321_001:0091b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輙身現異相。",
        "from": "X17n0321_001:0091b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便即昇天作樂。",
        "from": "X17n0321_001:0091b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "事訖。",
        "from": "X17n0321_001:0091b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還其本處。",
        "from": "X17n0321_001:0091b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此闥婆。",
        "from": "X17n0321_001:0091b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及仙人等多止寶山。",
        "from": "X17n0321_001:0091b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故云充滿。",
        "from": "X17n0321_001:0091b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上四十八偈半。",
        "from": "X17n0321_001:0091b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並大慧所陳百八義問。",
        "from": "X17n0321_001:0091b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無上世間解　　聞彼所說偈　　大乘諸度門　諸佛心第一自下四十行偈。",
        "from": "X17n0321_001:0091b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛牒上問。",
        "from": "X17n0321_001:0091b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明略答。",
        "from": "X17n0321_001:0091b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "訶片及問有不周。",
        "from": "X17n0321_001:0091b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來教問。",
        "from": "X17n0321_001:0091b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此之一行。",
        "from": "X17n0321_001:0091b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結集生起。",
        "from": "X17n0321_001:0091b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂佛聞前問。",
        "from": "X17n0321_001:0091b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "意請大乘度門。",
        "from": "X17n0321_001:0091b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛心勝義。",
        "from": "X17n0321_001:0091b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故云第一。",
        "from": "X17n0321_001:0091b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下三十九偈。",
        "from": "X17n0321_001:0091b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並佛之所說。",
        "from": "X17n0321_001:0091b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　善哉善哉問",
        "from": "X17n0321_001:0091b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此上所問。",
        "from": "X17n0321_001:0091b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "含諸佛心義。",
        "from": "X17n0321_001:0091b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在益既弘。",
        "from": "X17n0321_001:0091b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故斯重讚。",
        "from": "X17n0321_001:0091b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　大慧善諦聽",
        "from": "X17n0321_001:0091b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將示深義。",
        "from": "X17n0321_001:0091b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故先飭聽。",
        "from": "X17n0321_001:0091b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　我今當次第　　如汝所問說",
        "from": "X17n0321_001:0091b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次明許說。",
        "from": "X17n0321_001:0091b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "X17n0321_001:0091b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀下文非次。",
        "from": "X17n0321_001:0091b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何故兩處言次第邪。",
        "from": "X17n0321_001:0091b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫生為念本。",
        "from": "X17n0321_001:0091b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下既牒生即不生。",
        "from": "X17n0321_001:0091b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此義之次也。",
        "from": "X17n0321_001:0091b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦欲片問非次。",
        "from": "X17n0321_001:0091b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　生及與不生　　涅槃空剎那",
        "from": "X17n0321_001:0091b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初句。",
        "from": "X17n0321_001:0091b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉生即不生。",
        "from": "X17n0321_001:0091b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次句。",
        "from": "X17n0321_001:0091b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牒上之三問。",
        "from": "X17n0321_001:0091b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　趣至無自性",
        "from": "X17n0321_001:0091b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此句。",
        "from": "X17n0321_001:0091b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總答上問。",
        "from": "X17n0321_001:0091b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼該上下。",
        "from": "X17n0321_001:0091b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "趣即是心。",
        "from": "X17n0321_001:0091b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至即為境。",
        "from": "X17n0321_001:0091b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心境既無自性。",
        "from": "X17n0321_001:0091b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘法准而可知。",
        "from": "X17n0321_001:0091b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　佛諸波羅蜜　　佛子與聲聞　　緣覺諸外道　及與無色行　　如是種種事此牒。",
        "from": "X17n0321_001:0091c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛與三乘。",
        "from": "X17n0321_001:0091c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能脩行人。",
        "from": "X17n0321_001:0091c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "波羅蜜。",
        "from": "X17n0321_001:0091c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即所行之法。",
        "from": "X17n0321_001:0091c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸外道。",
        "from": "X17n0321_001:0091c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂人。",
        "from": "X17n0321_001:0091c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無色行。",
        "from": "X17n0321_001:0091c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即法。",
        "from": "X17n0321_001:0091c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此總舉。",
        "from": "X17n0321_001:0091c04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內外人法。",
        "from": "X17n0321_001:0091c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及餘種種。",
        "from": "X17n0321_001:0091c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆無性。",
        "from": "X17n0321_001:0091c05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上之三行。",
        "from": "X17n0321_001:0091c05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總明略答。",
        "from": "X17n0321_001:0091c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　須彌巨海山　　洲渚剎土地",
        "from": "X17n0321_001:0091c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此下八行。",
        "from": "X17n0321_001:0091c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明牒問訶責。",
        "from": "X17n0321_001:0091c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此之二句。",
        "from": "X17n0321_001:0091c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牒上云何名為地。",
        "from": "X17n0321_001:0091c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　星宿及日月　　外道天脩羅",
        "from": "X17n0321_001:0091c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此牒上星宿日月天諸外道脩羅。",
        "from": "X17n0321_001:0091c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上問中無。",
        "from": "X17n0321_001:0091c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因而附出少類天耳。",
        "from": "X17n0321_001:0091c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　解脫自在通　　力禪三摩提",
        "from": "X17n0321_001:0091c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牒上何因神通及自在三昧。",
        "from": "X17n0321_001:0091c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　滅及如意足　　覺支及道品",
        "from": "X17n0321_001:0091c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上句初滅。",
        "from": "X17n0321_001:0091c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義牒四勤及四如意足。",
        "from": "X17n0321_001:0091c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覺支一句。",
        "from": "X17n0321_001:0091c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正牒上何因說覺支及與菩提分。",
        "from": "X17n0321_001:0091c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　諸禪定無量　　諸陰身往來",
        "from": "X17n0321_001:0091c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上句。",
        "from": "X17n0321_001:0091c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義牒四禪四無量。",
        "from": "X17n0321_001:0091c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下句。",
        "from": "X17n0321_001:0091c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牒上眾生生諸趣。",
        "from": "X17n0321_001:0091c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　正受滅盡定　　三昧起心說",
        "from": "X17n0321_001:0091c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牒上云何無色定滅正受。",
        "from": "X17n0321_001:0091c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明須彌巨海乃至滅定三昧。",
        "from": "X17n0321_001:0091c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並是牒問。",
        "from": "X17n0321_001:0091c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯起心說。",
        "from": "X17n0321_001:0091c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為略片。",
        "from": "X17n0321_001:0091c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　心意及與識　　無我法有五",
        "from": "X17n0321_001:0091c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牒上云何名為藏意及識何等二無我。",
        "from": "X17n0321_001:0091c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼義牒五法一門。",
        "from": "X17n0321_001:0091c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　自性想所想　　及與現二見",
        "from": "X17n0321_001:0092a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義牒三性。",
        "from": "X17n0321_001:0092a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼牒自性及與心云何作有見。",
        "from": "X17n0321_001:0092a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　乘及諸種性　　金銀摩尼等",
        "from": "X17n0321_001:0092a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牒上云何三乘及種性誰生諸寶性等三問。",
        "from": "X17n0321_001:0092a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一闡提大種　　荒乱及一佛",
        "from": "X17n0321_001:0092a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牒上云何闡提國土乱何大牟尼等三問。",
        "from": "X17n0321_001:0092a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其大種因而附出。",
        "from": "X17n0321_001:0092a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　智爾燄得向　　眾生有無有",
        "from": "X17n0321_001:0092a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牒上爾燄淨解脫脩行者二問。",
        "from": "X17n0321_001:0092a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其眾生有無有一句。",
        "from": "X17n0321_001:0092a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此即片辭。",
        "from": "X17n0321_001:0092a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明上之所問。",
        "from": "X17n0321_001:0092a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並是眾生有無二見。",
        "from": "X17n0321_001:0092a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　象馬諸禽獸　　云何而捕取",
        "from": "X17n0321_001:0092a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牒上云何象馬鹿一問。",
        "from": "X17n0321_001:0092a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　譬因成悉檀　　及與作所作",
        "from": "X17n0321_001:0092a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牒上悉檀及緣起何所生作所作二問。",
        "from": "X17n0321_001:0092a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　叢林迷惑通　　心量不現有",
        "from": "X17n0321_001:0092a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牒上見癡惑一問。",
        "from": "X17n0321_001:0092a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其心量不現有一句。",
        "from": "X17n0321_001:0092a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是片辭。",
        "from": "X17n0321_001:0092a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明上之所問。",
        "from": "X17n0321_001:0092a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但唯心量。",
        "from": "X17n0321_001:0092a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實無有法而可現也。",
        "from": "X17n0321_001:0092a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此該上下。",
        "from": "X17n0321_001:0092a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　諸地不相去　　百變百無受",
        "from": "X17n0321_001:0092a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牒上何因度諸地及百變易百無受等二問。",
        "from": "X17n0321_001:0092a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　醫方工巧論　　技術諸明處",
        "from": "X17n0321_001:0092a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牒上云何醫方論明處及技術等二問。",
        "from": "X17n0321_001:0092a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並心量耳。",
        "from": "X17n0321_001:0092a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有何現邪。",
        "from": "X17n0321_001:0092a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　諸山須彌地　　巨海日月量　　下中上眾生　身各幾微塵自下九行半。",
        "from": "X17n0321_001:0092a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大慧問有不周。",
        "from": "X17n0321_001:0092b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛教重問者。",
        "from": "X17n0321_001:0092b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略有二義。",
        "from": "X17n0321_001:0092b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一為遣物情外。",
        "from": "X17n0321_001:0092b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二以大慧上既言。",
        "from": "X17n0321_001:0092b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我名大慧通達大乘者。",
        "from": "X17n0321_001:0092b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且如妙高及四洲土地大海日月下中上身。",
        "from": "X17n0321_001:0092b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此等形量。",
        "from": "X17n0321_001:0092b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各用幾塵而成。",
        "from": "X17n0321_001:0092b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知其數不。",
        "from": "X17n0321_001:0092b05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不知者。",
        "from": "X17n0321_001:0092b05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈曰大乘。",
        "from": "X17n0321_001:0092b06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又諸物既積微所。",
        "from": "X17n0321_001:0092b06_13",
        "context_with_next_sentence": false
    }
]