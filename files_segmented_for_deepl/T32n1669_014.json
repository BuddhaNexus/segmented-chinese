[
    {
        "source_sentence": "大宗地玄文本論卷第十四",
        "from": "T32n1669_014:0686c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬鳴菩薩造",
        "from": "T32n1669_014:0686c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真諦三藏譯",
        "from": "T32n1669_014:0686c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫縛地地品類不吉祥道路大決擇分第二十七如是已說摩訶寶輪王廣大圓滿無上地地大決擇分。",
        "from": "T32n1669_014:0686c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說繫縛地地品類不吉祥道路大決擇分。",
        "from": "T32n1669_014:0686c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_014:0686c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_014:0686c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　繫縛道路中　　亦有金剛位",
        "from": "T32n1669_014:0686c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　依位立轉相　　則有四種法",
        "from": "T32n1669_014:0686c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　謂能所障果　　如是四種法",
        "from": "T32n1669_014:0686c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　皆悉有為量　　如法應觀察",
        "from": "T32n1669_014:0686c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_014:0686c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就繫縛地地品類不吉祥道路分中。",
        "from": "T32n1669_014:0686c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有五十一金剛位。",
        "from": "T32n1669_014:0686c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依此諸位建立轉相。",
        "from": "T32n1669_014:0686c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有四種法。",
        "from": "T32n1669_014:0686c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為四。",
        "from": "T32n1669_014:0686c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者能證智法。",
        "from": "T32n1669_014:0686c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者所證理法。",
        "from": "T32n1669_014:0686c22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者障礙事法。",
        "from": "T32n1669_014:0686c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者證得果法。",
        "from": "T32n1669_014:0686c23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為四。",
        "from": "T32n1669_014:0686c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是四法皆有為量。",
        "from": "T32n1669_014:0686c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應審思擇。",
        "from": "T32n1669_014:0686c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈繫縛道路中亦有金剛位依位立轉相則有四種法。",
        "from": "T32n1669_014:0686c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂能所障果如是四種法皆悉有為量如法應觀察故。",
        "from": "T32n1669_014:0686c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是四法各有幾數。",
        "from": "T32n1669_014:0686c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其轉形相當如何耶。",
        "from": "T32n1669_014:0686c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_014:0686c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　各有二種法　　謂本始體相",
        "from": "T32n1669_014:0686c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　生滅及增減　　轉相唯上上",
        "from": "T32n1669_014:0687a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_014:0687a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四種法中各有二種。",
        "from": "T32n1669_014:0687a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何二智。",
        "from": "T32n1669_014:0687a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者本古性德智。",
        "from": "T32n1669_014:0687a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者始今起德智。",
        "from": "T32n1669_014:0687a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為二。",
        "from": "T32n1669_014:0687a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何二理。",
        "from": "T32n1669_014:0687a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者體有實理。",
        "from": "T32n1669_014:0687a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者相有實理。",
        "from": "T32n1669_014:0687a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為二。",
        "from": "T32n1669_014:0687a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何二障。",
        "from": "T32n1669_014:0687a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者本生本生障。",
        "from": "T32n1669_014:0687a05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者本滅本滅障。",
        "from": "T32n1669_014:0687a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為二。",
        "from": "T32n1669_014:0687a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何二果。",
        "from": "T32n1669_014:0687a06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者增長功德果。",
        "from": "T32n1669_014:0687a06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者損減過患果。",
        "from": "T32n1669_014:0687a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為二。",
        "from": "T32n1669_014:0687a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈各有二種法。",
        "from": "T32n1669_014:0687a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂本始體相生滅及增減故修行轉相唯上上故。",
        "from": "T32n1669_014:0687a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈轉相唯上上故。",
        "from": "T32n1669_014:0687a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "障治證果對量差別。",
        "from": "T32n1669_014:0687a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形相如何。",
        "from": "T32n1669_014:0687a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_014:0687a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　本生體增對　　始滅相減對",
        "from": "T32n1669_014:0687a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　從多亦通了　　如法應觀察",
        "from": "T32n1669_014:0687a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_014:0687a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本古性德智。",
        "from": "T32n1669_014:0687a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷除本生本生障。",
        "from": "T32n1669_014:0687a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證得體有實理。",
        "from": "T32n1669_014:0687a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就增長功德果。",
        "from": "T32n1669_014:0687a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈本生體增對故。",
        "from": "T32n1669_014:0687a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始今起德智。",
        "from": "T32n1669_014:0687a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷除本滅本滅障。",
        "from": "T32n1669_014:0687a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證得相有實理。",
        "from": "T32n1669_014:0687a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就損減過患果。",
        "from": "T32n1669_014:0687a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈治滅相減對故。",
        "from": "T32n1669_014:0687a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是四法。",
        "from": "T32n1669_014:0687a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五十一種真金位中皆悉具足。",
        "from": "T32n1669_014:0687a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應審思擇。",
        "from": "T32n1669_014:0687a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯一向轉俱種轉耶。",
        "from": "T32n1669_014:0687a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱種轉故。",
        "from": "T32n1669_014:0687a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈從多亦通了如法應觀察故。",
        "from": "T32n1669_014:0687a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生滅二障業用差別。",
        "from": "T32n1669_014:0687a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形相如何。",
        "from": "T32n1669_014:0687a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_014:0687a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　本主生滅時　　勝生勝滅故",
        "from": "T32n1669_014:0687a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_014:0687a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功德善根出興。",
        "from": "T32n1669_014:0687a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對治轉勝出興。",
        "from": "T32n1669_014:0687a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉勝對滅作逆事故。",
        "from": "T32n1669_014:0687a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈本主生滅時勝生勝滅故。",
        "from": "T32n1669_014:0687a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故大金剛山寶海會眾修多羅中。",
        "from": "T32n1669_014:0687a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說。",
        "from": "T32n1669_014:0687a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次文殊師利。",
        "from": "T32n1669_014:0687a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝前所問。",
        "from": "T32n1669_014:0687a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名為諸法無常一道一種。",
        "from": "T32n1669_014:0687a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一轉門者。",
        "from": "T32n1669_014:0687a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以四無常故。",
        "from": "T32n1669_014:0687a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我作如是唱。",
        "from": "T32n1669_014:0687a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為四。",
        "from": "T32n1669_014:0687a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者智無常。",
        "from": "T32n1669_014:0687a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者理無常。",
        "from": "T32n1669_014:0687a27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者無常無常。",
        "from": "T32n1669_014:0687a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者上果無常。",
        "from": "T32n1669_014:0687a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為四。",
        "from": "T32n1669_014:0687a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊師利。",
        "from": "T32n1669_014:0687a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言智無常者。",
        "from": "T32n1669_014:0687a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷煩惱故。",
        "from": "T32n1669_014:0687a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言理無常者。",
        "from": "T32n1669_014:0687a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智所證故言無常。",
        "from": "T32n1669_014:0687b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無常者。",
        "from": "T32n1669_014:0687b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被斷除故。",
        "from": "T32n1669_014:0687b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言果無常者。",
        "from": "T32n1669_014:0687b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "待因力故。",
        "from": "T32n1669_014:0687b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至廣說故。",
        "from": "T32n1669_014:0687b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫縛地地自然本王摩訶[糸*為]品大決擇分第二十八如是已說繫縛地地品類不吉祥道路大決擇分。",
        "from": "T32n1669_014:0687b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說繫縛地地自然本王摩訶[糸*為]品大決擇分。",
        "from": "T32n1669_014:0687b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_014:0687b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_014:0687b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　自然本王中　　有為無為法",
        "from": "T32n1669_014:0687b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　具足圓滿轉　　此中具上下",
        "from": "T32n1669_014:0687b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_014:0687b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就自然本王摩訶[糸*為]品分中。",
        "from": "T32n1669_014:0687b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則有二轉。",
        "from": "T32n1669_014:0687b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為二。",
        "from": "T32n1669_014:0687b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者有為轉。",
        "from": "T32n1669_014:0687b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者無為轉。",
        "from": "T32n1669_014:0687b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為二。",
        "from": "T32n1669_014:0687b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈自然本王中有為無為法具足圓滿轉故。",
        "from": "T32n1669_014:0687b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有二轉。",
        "from": "T32n1669_014:0687b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為二。",
        "from": "T32n1669_014:0687b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者上轉。",
        "from": "T32n1669_014:0687b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者下轉。",
        "from": "T32n1669_014:0687b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為二。",
        "from": "T32n1669_014:0687b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈此中具上下故。",
        "from": "T32n1669_014:0687b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有為無為各有幾數。",
        "from": "T32n1669_014:0687b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上下轉相當如何耶。",
        "from": "T32n1669_014:0687b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_014:0687b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無為唯有一　　有為有二種",
        "from": "T32n1669_014:0687b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如次實本始　　上下無為主",
        "from": "T32n1669_014:0687b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　出生二有為　　轉勝廣大轉",
        "from": "T32n1669_014:0687b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_014:0687b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無為有一。",
        "from": "T32n1669_014:0687b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有為有二。",
        "from": "T32n1669_014:0687b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一謂有實故。",
        "from": "T32n1669_014:0687b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二謂本始故。",
        "from": "T32n1669_014:0687b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈無為唯有一。",
        "from": "T32n1669_014:0687b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有為有二種。",
        "from": "T32n1669_014:0687b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如次實本始故。",
        "from": "T32n1669_014:0687b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是三法五十一種金剛位中亦上亦下增長功德轉相。",
        "from": "T32n1669_014:0687b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何謂上時中。",
        "from": "T32n1669_014:0687b22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一位位無為法主。",
        "from": "T32n1669_014:0687b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆悉各各出生增長一萬本始清妙覺慧其下時中。",
        "from": "T32n1669_014:0687b23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一位位皆悉各各二億本始清妙覺慧具足圓滿出生增長。",
        "from": "T32n1669_014:0687b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是如是如如上下至小無量。",
        "from": "T32n1669_014:0687b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如說本存眷屬亦爾。",
        "from": "T32n1669_014:0687b26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈上下無為主出生二有為轉勝廣大轉故。",
        "from": "T32n1669_014:0687b27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如其次第。",
        "from": "T32n1669_014:0687b28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增數應知。",
        "from": "T32n1669_014:0687b28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "品地經論修多羅中。",
        "from": "T32n1669_014:0687b28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說。",
        "from": "T32n1669_014:0687b29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間藏地本王海中。",
        "from": "T32n1669_014:0687b29_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無常功德眾多無數。",
        "from": "T32n1669_014:0687b29_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常住功德其數微少。",
        "from": "T32n1669_014:0687c01_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故說言世間藏地乃至廣說故。",
        "from": "T32n1669_014:0687c01_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "◎大宗地玄文本論卷第十四",
        "from": "T32n1669_014:0687c02_24",
        "context_with_next_sentence": false
    }
]