[
    {
        "source_sentence": "一切如來說佛頂輪王一百八名",
        "from": "T19n0960_001:0330a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讚",
        "from": "T19n0960_001:0330a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試鴻臚卿",
        "from": "T19n0960_001:0330a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳法大師臣施護奉　詔譯",
        "from": "T19n0960_001:0330a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時一切如來。",
        "from": "T19n0960_001:0330a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說無量功德大明呪主轉輪王一百八名。",
        "from": "T19n0960_001:0330a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說讚曰。",
        "from": "T19n0960_001:0330a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　歸命最上師　　一切世間主",
        "from": "T19n0960_001:0330a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　恒懷大慈悲　　顯揚大名稱",
        "from": "T19n0960_001:0330a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　十力大丈夫　　福德超三界",
        "from": "T19n0960_001:0330a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　具足一切相　　天人常恭敬",
        "from": "T19n0960_001:0330a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　攝受諸眾生　　與作大福聚",
        "from": "T19n0960_001:0330a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　復名大自在　　一切佛頂王",
        "from": "T19n0960_001:0330a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　轉一切呪輪　　成就正法句",
        "from": "T19n0960_001:0330a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　聖智善通達　　一切悉解脫",
        "from": "T19n0960_001:0330a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　恒為大主宰　　出過一切天",
        "from": "T19n0960_001:0330a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　為大禪定像　　平等視眾生",
        "from": "T19n0960_001:0330a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　具足無邊智　　無實無不實",
        "from": "T19n0960_001:0330a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無一亦無二　　號曰大仙天",
        "from": "T19n0960_001:0330a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　世所甚希有　　本來寂滅相",
        "from": "T19n0960_001:0330a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　清淨而無垢　　善入無盡性",
        "from": "T19n0960_001:0330a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　廓然無怖畏　　不斷亦不破",
        "from": "T19n0960_001:0330a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無想亦不空　　能捨於萬有",
        "from": "T19n0960_001:0330a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　不著如虛空　　妙色即空色",
        "from": "T19n0960_001:0330a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　善相為善瑞　　寂靜常決定",
        "from": "T19n0960_001:0330a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　善意生妙梵　　為彼天中像",
        "from": "T19n0960_001:0330a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　自在廣無邊　　功德無邊量",
        "from": "T19n0960_001:0330a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　心口難思議　　無等無等等",
        "from": "T19n0960_001:0330b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　真像廣無邊　　究竟無能勝",
        "from": "T19n0960_001:0330b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無邊世界中　　一切皆恭敬",
        "from": "T19n0960_001:0330b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一切呪像主　　此像未曾有",
        "from": "T19n0960_001:0330b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　善像難有像　　天眼及三眼",
        "from": "T19n0960_001:0330b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　真實三解脫　　實語實寂靜",
        "from": "T19n0960_001:0330b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　實法住實際　　思惟妙法身",
        "from": "T19n0960_001:0330b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　湛然常堅久　　無垢性決定",
        "from": "T19n0960_001:0330b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　風天與月天　　日光及妙意",
        "from": "T19n0960_001:0330b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　文字三六字　　一呪一字王",
        "from": "T19n0960_001:0330b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一智一切智　　自在大自在",
        "from": "T19n0960_001:0330b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　梵天那羅延　　一佛一法相",
        "from": "T19n0960_001:0330b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一天居一眾　　一定一相應",
        "from": "T19n0960_001:0330b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一切世間中　　高談眾妙法",
        "from": "T19n0960_001:0330b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　以此神通光　　化光一切處",
        "from": "T19n0960_001:0330b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　大光威德力　　為一切天主",
        "from": "T19n0960_001:0330b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一切佛法主　　一切法智主",
        "from": "T19n0960_001:0330b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無盡功德聚　　是名一百八",
        "from": "T19n0960_001:0330b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來說佛頂輪王一百八名讚",
        "from": "T19n0960_001:0330b19_0",
        "context_with_next_sentence": false
    }
]