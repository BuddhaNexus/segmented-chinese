[
    {
        "source_sentence": "佛說檀特羅麻油述經",
        "from": "T21n1391_001:0908a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東晉竺曇無蘭譯",
        "from": "T21n1391_001:0908a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛在摩竭國因沙奪山中。",
        "from": "T21n1391_001:0908a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時佛子羅云隨佛在山中。",
        "from": "T21n1391_001:0908a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅云夜臥為鬼神所嬈驚起。",
        "from": "T21n1391_001:0908a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明日至佛所為佛作禮。",
        "from": "T21n1391_001:0908a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "却在一面樹下坐。",
        "from": "T21n1391_001:0908a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅云以手扶頰。",
        "from": "T21n1391_001:0908a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "低頭不樂默然不語。",
        "from": "T21n1391_001:0908a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛即問羅云。",
        "from": "T21n1391_001:0908a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以低頭如畏怖狀。",
        "from": "T21n1391_001:0908a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅云言我昨日夜臥。",
        "from": "T21n1391_001:0908a10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為鬼神所嬈。",
        "from": "T21n1391_001:0908a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語羅云。",
        "from": "T21n1391_001:0908a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天下或生人嬈人。",
        "from": "T21n1391_001:0908a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或山神嬈人。",
        "from": "T21n1391_001:0908a11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或道溝邊鬼神嬈人。",
        "from": "T21n1391_001:0908a12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或善死鬼神嬈人。",
        "from": "T21n1391_001:0908a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來欲試人經道恐人。",
        "from": "T21n1391_001:0908a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲知其心堅濡。",
        "from": "T21n1391_001:0908a13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語羅云。",
        "from": "T21n1391_001:0908a13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝取佛辟鬼神呪。",
        "from": "T21n1391_001:0908a14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後儻有鬼神來嬈汝者。",
        "from": "T21n1391_001:0908a14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持是鬼神名字。",
        "from": "T21n1391_001:0908a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以慈心說之。",
        "from": "T21n1391_001:0908a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿波竭　證證竭　無多薩　憘遲比遲沾波沾波迦羅准　維陵無　因輪無　指輪無　漢沙無　因登羅　宋林羅　和林羅　波耶越羅　檀特羅佛言是檀特羅經。",
        "from": "T21n1391_001:0908a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛故為諸弟子結恩經。",
        "from": "T21n1391_001:0908a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告諸弟子。",
        "from": "T21n1391_001:0908a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有急者當讀之。",
        "from": "T21n1391_001:0908a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼神儻來嬈人者。",
        "from": "T21n1391_001:0908a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當持慈心哀心。",
        "from": "T21n1391_001:0908a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨還自視五藏思念五藏。",
        "from": "T21n1391_001:0908a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說是經時。",
        "from": "T21n1391_001:0908a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日月尚有墮地。",
        "from": "T21n1391_001:0908a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛語終不有異。",
        "from": "T21n1391_001:0908a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今佛說是檀特羅經。",
        "from": "T21n1391_001:0908a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以說生人欲來嬈人者不得嬈人。",
        "from": "T21n1391_001:0908a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "山神亦不得嬈人。",
        "from": "T21n1391_001:0908a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道溝邊鬼神亦不得嬈人。",
        "from": "T21n1391_001:0908a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "腥死鬼神亦不得嬈人。",
        "from": "T21n1391_001:0908a26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善死鬼神亦不得嬈人。",
        "from": "T21n1391_001:0908a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞是語火為不然。",
        "from": "T21n1391_001:0908a27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食飯得毒毒為不行。",
        "from": "T21n1391_001:0908a27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲殺人刀為不向。",
        "from": "T21n1391_001:0908a28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "溺深水中為不沒難。",
        "from": "T21n1391_001:0908a28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "群有四子人行空閑處。",
        "from": "T21n1391_001:0908a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若行縣邑中大國中。",
        "from": "T21n1391_001:0908a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若對會若大坐中。",
        "from": "T21n1391_001:0908b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耆老中伴侶中。",
        "from": "T21n1391_001:0908b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "步行中坐臥中。",
        "from": "T21n1391_001:0908b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "值有蠱道家者。",
        "from": "T21n1391_001:0908b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向讀是經蠱道為不行。",
        "from": "T21n1391_001:0908b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛即為羅云說。",
        "from": "T21n1391_001:0908b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使羅云為諸比丘比丘尼優婆塞優婆夷及諸白衣。",
        "from": "T21n1391_001:0908b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆令諷誦之。",
        "from": "T21n1391_001:0908b04_2",
        "context_with_next_sentence": false
    }
]