[
    {
        "source_sentence": "維摩詰所說經卷二",
        "from": "X19n0347_002:0552b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方便品",
        "from": "X19n0347_002:0552b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時毗耶離大城中。",
        "from": "X19n0347_002:0552b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有長者名維摩詰。",
        "from": "X19n0347_002:0552b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已曾供養無量諸佛。",
        "from": "X19n0347_002:0552b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深植善本。",
        "from": "X19n0347_002:0552b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得無生忍。",
        "from": "X19n0347_002:0552b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辯才無礙。",
        "from": "X19n0347_002:0552b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遊戲神通。",
        "from": "X19n0347_002:0552b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逮諸總持。",
        "from": "X19n0347_002:0552b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲無所畏。",
        "from": "X19n0347_002:0552b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降魔勞怨。",
        "from": "X19n0347_002:0552b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入深法門。",
        "from": "X19n0347_002:0552b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善於智度。",
        "from": "X19n0347_002:0552b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通達方便。",
        "from": "X19n0347_002:0552b17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大願成就。",
        "from": "X19n0347_002:0552b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明了眾生心之所趣。",
        "from": "X19n0347_002:0552b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又能分別諸根利鈍。",
        "from": "X19n0347_002:0552b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "久於佛道。",
        "from": "X19n0347_002:0552b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心已純淑。",
        "from": "X19n0347_002:0552b18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "決定大乘。",
        "from": "X19n0347_002:0552b19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸有所作。",
        "from": "X19n0347_002:0552b19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能善思量。",
        "from": "X19n0347_002:0552b19_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住佛威儀。",
        "from": "X19n0347_002:0552b19_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心大如海。",
        "from": "X19n0347_002:0552b19_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛咨嗟。",
        "from": "X19n0347_002:0552b20_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弟子釋梵世主所敬。",
        "from": "X19n0347_002:0552b20_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲度人故。",
        "from": "X19n0347_002:0552b20_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以善方便。",
        "from": "X19n0347_002:0552b21_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居毗耶離。",
        "from": "X19n0347_002:0552b21_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "資財無量。",
        "from": "X19n0347_002:0552b21_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝諸貧民。",
        "from": "X19n0347_002:0552b21_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉戒清淨。",
        "from": "X19n0347_002:0552b21_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝諸毀禁。",
        "from": "X19n0347_002:0552b22_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以忍調行。",
        "from": "X19n0347_002:0552b22_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝諸恚怒。",
        "from": "X19n0347_002:0552b22_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大精進。",
        "from": "X19n0347_002:0552b22_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝諸懈怠。",
        "from": "X19n0347_002:0552b23_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心禪寂。",
        "from": "X19n0347_002:0552b23_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝諸亂意。",
        "from": "X19n0347_002:0552b23_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以決定慧。",
        "from": "X19n0347_002:0552b23_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝諸無智。",
        "from": "X19n0347_002:0552b23_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖為白衣。",
        "from": "X19n0347_002:0552b24_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉持沙門清淨律行。",
        "from": "X19n0347_002:0552b24_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖處居家。",
        "from": "X19n0347_002:0552b24_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不著三界。",
        "from": "X19n0347_002:0552c01_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "示有妻子。",
        "from": "X19n0347_002:0552c01_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常修梵行。",
        "from": "X19n0347_002:0552c01_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現有眷屬。",
        "from": "X19n0347_002:0552c01_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常樂遠離。",
        "from": "X19n0347_002:0552c01_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖服寶飾。",
        "from": "X19n0347_002:0552c02_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而以相好嚴身。",
        "from": "X19n0347_002:0552c02_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖復飲食。",
        "from": "X19n0347_002:0552c02_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而以禪悅為味。",
        "from": "X19n0347_002:0552c02_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若至博奕戲處。",
        "from": "X19n0347_002:0552c03_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輙以度人。",
        "from": "X19n0347_002:0552c03_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受諸異道。",
        "from": "X19n0347_002:0552c03_54",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不毀正信。",
        "from": "X19n0347_002:0552c04_55",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖明世典。",
        "from": "X19n0347_002:0552c04_56",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常樂佛法。",
        "from": "X19n0347_002:0552c04_57",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切見敬。",
        "from": "X19n0347_002:0552c04_58",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為供養中最。",
        "from": "X19n0347_002:0552c04_59",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執持正法。",
        "from": "X19n0347_002:0552c05_60",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝諸長幼。",
        "from": "X19n0347_002:0552c05_61",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切治生諧偶。",
        "from": "X19n0347_002:0552c05_62",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖獲俗利。",
        "from": "X19n0347_002:0552c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不以喜悅。",
        "from": "X19n0347_002:0552c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遊諸四衢。",
        "from": "X19n0347_002:0552c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "饒益眾生。",
        "from": "X19n0347_002:0552c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入治正法。",
        "from": "X19n0347_002:0552c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救護一切。",
        "from": "X19n0347_002:0552c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入講論處。",
        "from": "X19n0347_002:0552c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "導以大乘。",
        "from": "X19n0347_002:0552c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入諸學堂。",
        "from": "X19n0347_002:0552c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誘開童蒙。",
        "from": "X19n0347_002:0552c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入諸婬舍。",
        "from": "X19n0347_002:0552c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "示欲之過。",
        "from": "X19n0347_002:0552c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入諸酒肆。",
        "from": "X19n0347_002:0552c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能立其志。",
        "from": "X19n0347_002:0552c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若在長者。",
        "from": "X19n0347_002:0552c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長者中尊。",
        "from": "X19n0347_002:0552c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為說勝法。",
        "from": "X19n0347_002:0552c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若在居士。",
        "from": "X19n0347_002:0552c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居士中尊。",
        "from": "X19n0347_002:0552c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷其貪著。",
        "from": "X19n0347_002:0552c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若在剎利。",
        "from": "X19n0347_002:0552c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剎利中尊。",
        "from": "X19n0347_002:0552c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教以忍辱。",
        "from": "X19n0347_002:0552c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若在婆羅門。",
        "from": "X19n0347_002:0552c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婆羅門中尊。",
        "from": "X19n0347_002:0552c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除其我慢。",
        "from": "X19n0347_002:0552c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若在大臣。",
        "from": "X19n0347_002:0552c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大臣中尊。",
        "from": "X19n0347_002:0552c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教以正法。",
        "from": "X19n0347_002:0552c12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若在王子。",
        "from": "X19n0347_002:0552c12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王子中尊。",
        "from": "X19n0347_002:0552c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "示以忠孝。",
        "from": "X19n0347_002:0553a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若在內官。",
        "from": "X19n0347_002:0553a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內官中尊。",
        "from": "X19n0347_002:0553a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化正宮女。",
        "from": "X19n0347_002:0553a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若在庶民。",
        "from": "X19n0347_002:0553a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庶民中尊。",
        "from": "X19n0347_002:0553a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令興福力。",
        "from": "X19n0347_002:0553a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若在梵天。",
        "from": "X19n0347_002:0553a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵天中尊。",
        "from": "X19n0347_002:0553a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誨以勝慧。",
        "from": "X19n0347_002:0553a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若在帝釋。",
        "from": "X19n0347_002:0553a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝釋中尊。",
        "from": "X19n0347_002:0553a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "示現無常。",
        "from": "X19n0347_002:0553a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若在護世。",
        "from": "X19n0347_002:0553a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護世中尊。",
        "from": "X19n0347_002:0553a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護諸眾生長者維摩詰以如是等無量方便。",
        "from": "X19n0347_002:0553a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "饒益眾生。",
        "from": "X19n0347_002:0553a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其以方便現身有疾(過現疾處血脉聯貫)。",
        "from": "X19n0347_002:0553a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以其疾故。",
        "from": "X19n0347_002:0553a05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國王大臣。",
        "from": "X19n0347_002:0553a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長者居士。",
        "from": "X19n0347_002:0553a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婆羅門等。",
        "from": "X19n0347_002:0553a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸王子。",
        "from": "X19n0347_002:0553a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并餘官屬。",
        "from": "X19n0347_002:0553a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無數千人。",
        "from": "X19n0347_002:0553a07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆往問疾。",
        "from": "X19n0347_002:0553a07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其往者。",
        "from": "X19n0347_002:0553a07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "維摩詰。",
        "from": "X19n0347_002:0553a07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因以身疾。",
        "from": "X19n0347_002:0553a08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣為說法。",
        "from": "X19n0347_002:0553a08_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸仁者。",
        "from": "X19n0347_002:0553a08_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身無常。",
        "from": "X19n0347_002:0553a08_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無強。",
        "from": "X19n0347_002:0553a08_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無力。",
        "from": "X19n0347_002:0553a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無堅。",
        "from": "X19n0347_002:0553a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速朽之法。",
        "from": "X19n0347_002:0553a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可信也。",
        "from": "X19n0347_002:0553a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為苦為惱。",
        "from": "X19n0347_002:0553a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾病所集。",
        "from": "X19n0347_002:0553a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸仁者。",
        "from": "X19n0347_002:0553a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如此身。",
        "from": "X19n0347_002:0553a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明智者所不怙。",
        "from": "X19n0347_002:0553a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身如聚沫。",
        "from": "X19n0347_002:0553a10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可撮摩。",
        "from": "X19n0347_002:0553a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身如泡。",
        "from": "X19n0347_002:0553a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得久立。",
        "from": "X19n0347_002:0553a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身如焰。",
        "from": "X19n0347_002:0553a11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從渴愛生。",
        "from": "X19n0347_002:0553a11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身如芭蕉。",
        "from": "X19n0347_002:0553a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中無有堅。",
        "from": "X19n0347_002:0553a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身如幻。",
        "from": "X19n0347_002:0553a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從顛倒起。",
        "from": "X19n0347_002:0553a12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身如夢。",
        "from": "X19n0347_002:0553a13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為虗妄見。",
        "from": "X19n0347_002:0553a13_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身如影。",
        "from": "X19n0347_002:0553a13_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從業緣現。",
        "from": "X19n0347_002:0553a13_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身如響。",
        "from": "X19n0347_002:0553a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屬諸因緣。",
        "from": "X19n0347_002:0553a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身如浮雲。",
        "from": "X19n0347_002:0553a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須臾變滅。",
        "from": "X19n0347_002:0553a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身如電。",
        "from": "X19n0347_002:0553a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念念不住。",
        "from": "X19n0347_002:0553a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身無主為如地。",
        "from": "X19n0347_002:0553a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身無我為如火。",
        "from": "X19n0347_002:0553a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身無壽為如風。",
        "from": "X19n0347_002:0553a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身無人為如水。",
        "from": "X19n0347_002:0553a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身不實。",
        "from": "X19n0347_002:0553a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四大為家。",
        "from": "X19n0347_002:0553a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身為空。",
        "from": "X19n0347_002:0553a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離我我所。",
        "from": "X19n0347_002:0553a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身無知。",
        "from": "X19n0347_002:0553a17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如草木瓦礫。",
        "from": "X19n0347_002:0553a18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身無作。",
        "from": "X19n0347_002:0553a18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風力所轉。",
        "from": "X19n0347_002:0553a18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身不淨。",
        "from": "X19n0347_002:0553a18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "穢惡充滿。",
        "from": "X19n0347_002:0553a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身為虗偽。",
        "from": "X19n0347_002:0553a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖假以澡浴衣食。",
        "from": "X19n0347_002:0553a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必歸磨滅。",
        "from": "X19n0347_002:0553a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身為灾。",
        "from": "X19n0347_002:0553a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百一病惱。",
        "from": "X19n0347_002:0553a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身如丘井。",
        "from": "X19n0347_002:0553a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為老所逼。",
        "from": "X19n0347_002:0553a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身無定。",
        "from": "X19n0347_002:0553a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為要當死。",
        "from": "X19n0347_002:0553a21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是身如毒蛇。",
        "from": "X19n0347_002:0553a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如怨賊。",
        "from": "X19n0347_002:0553a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如空聚。",
        "from": "X19n0347_002:0553a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陰界諸入。",
        "from": "X19n0347_002:0553a22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所共合成。",
        "from": "X19n0347_002:0553a22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸仁者。",
        "from": "X19n0347_002:0553a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此可患厭。",
        "from": "X19n0347_002:0553a22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當樂佛身。",
        "from": "X19n0347_002:0553a22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何。",
        "from": "X19n0347_002:0553a23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛身者。",
        "from": "X19n0347_002:0553a23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即法身也。",
        "from": "X19n0347_002:0553a23_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從無量功德智慧生。",
        "from": "X19n0347_002:0553a23_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從戒定慧解脫解脫知見生。",
        "from": "X19n0347_002:0553a24_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從慈悲喜捨生。",
        "from": "X19n0347_002:0553a24_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從布施持戒忍辱柔和勤行精進禪定解脫三昧。",
        "from": "X19n0347_002:0553b01_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多聞智慧。",
        "from": "X19n0347_002:0553b02_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸波羅蜜生(到彼岸也)。",
        "from": "X19n0347_002:0553b02_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從方便生。",
        "from": "X19n0347_002:0553b02_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從六通生。",
        "from": "X19n0347_002:0553b02_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從三明生。",
        "from": "X19n0347_002:0553b03_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從三十七道品生。",
        "from": "X19n0347_002:0553b03_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從止觀生。",
        "from": "X19n0347_002:0553b03_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從十力四無所畏十八不共法生。",
        "from": "X19n0347_002:0553b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從斷一切不善法。",
        "from": "X19n0347_002:0553b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "集一切善法生。",
        "from": "X19n0347_002:0553b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從真實生。",
        "from": "X19n0347_002:0553b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從不放逸生。",
        "from": "X19n0347_002:0553b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從如是無量清淨法生如來身。",
        "from": "X19n0347_002:0553b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸仁者。",
        "from": "X19n0347_002:0553b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲得佛身斷一切眾生病者。",
        "from": "X19n0347_002:0553b06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當發阿耨多羅三藐三菩提心。",
        "from": "X19n0347_002:0553b07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是。",
        "from": "X19n0347_002:0553b08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長者維摩詰。",
        "from": "X19n0347_002:0553b08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸問疾者。",
        "from": "X19n0347_002:0553b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如應說法。",
        "from": "X19n0347_002:0553b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令無數千人。",
        "from": "X19n0347_002:0553b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆發阿耨多羅三藐三菩提心。",
        "from": "X19n0347_002:0553b09_14",
        "context_with_next_sentence": false
    }
]