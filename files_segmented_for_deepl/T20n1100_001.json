        "from": "T20n1100_001:0447b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)鉢囉(二合)拏捨嚩哩(二)吽發吒(半聲三)時觀自在菩薩。",
        "from": "T20n1100_001:0448a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此陀羅尼已。",
        "from": "T20n1100_001:0448a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊。",
        "from": "T20n1100_001:0448a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人。",
        "from": "T20n1100_001:0448a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此陀羅尼一遍。",
        "from": "T20n1100_001:0448a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即護自身。",
        "from": "T20n1100_001:0448a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦兩遍即護伴侶。",
        "from": "T20n1100_001:0448a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦三遍能護一家。",
        "from": "T20n1100_001:0448a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦四遍護一聚落。",
        "from": "T20n1100_001:0448a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦五遍護一國界。",
        "from": "T20n1100_001:0448a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若國內疫病流行。",
        "from": "T20n1100_001:0448a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應取白[疊*毛]闊一肘半長二肘。",
        "from": "T20n1100_001:0448a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先令畫人潔淨齋戒。",
        "from": "T20n1100_001:0448a10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以瞿摩夷汁和少青碌。",
        "from": "T20n1100_001:0448a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以香膠和。",
        "from": "T20n1100_001:0448a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿用皮膠。",
        "from": "T20n1100_001:0448a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取鬼宿日。",
        "from": "T20n1100_001:0448a11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫葉衣觀自在菩薩像。",
        "from": "T20n1100_001:0448a11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其像作天女形。",
        "from": "T20n1100_001:0448a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "首戴寶冠冠有無量壽佛。",
        "from": "T20n1100_001:0448a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瓔珞環釧莊嚴其身。",
        "from": "T20n1100_001:0448a13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身有圓光火焰圍遶。",
        "from": "T20n1100_001:0448a13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "像有四臂右第一手當心持吉祥菓。",
        "from": "T20n1100_001:0448a14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二手作施願手。",
        "from": "T20n1100_001:0448a15_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左第一手持鉞斧。",
        "from": "T20n1100_001:0448a15_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二手持羂索。",
        "from": "T20n1100_001:0448a15_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐蓮華上。",
        "from": "T20n1100_001:0448a16_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫像成已懸於竿上。",
        "from": "T20n1100_001:0448a16_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令一人執持。",
        "from": "T20n1100_001:0448a17_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執竿之人無間斷誦葉衣觀自在菩薩陀羅尼。",
        "from": "T20n1100_001:0448a17_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聲鼓鳴磬。",
        "from": "T20n1100_001:0448a18_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所擊之杖用樀枳王真言。",
        "from": "T20n1100_001:0448a18_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持二十一遍方乃擊之。",
        "from": "T20n1100_001:0448a19_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1100_001:0448a19_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵樀枳吽(短聲)弱",
        "from": "T20n1100_001:0448a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又令二人誦讚。",
        "from": "T20n1100_001:0448a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一人誦吉慶讚。",
        "from": "T20n1100_001:0448a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一人誦吉祥讚。",
        "from": "T20n1100_001:0448a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令知法弟子三五人。",
        "from": "T20n1100_001:0448a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一人持香爐燒安悉香。",
        "from": "T20n1100_001:0448a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其香以葉衣觀自在菩薩心真言。",
        "from": "T20n1100_001:0448a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持一百八遍。",
        "from": "T20n1100_001:0448a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後取燒香煙勿令斷絕。",
        "from": "T20n1100_001:0448a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一人持賢瓶。",
        "from": "T20n1100_001:0448a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿盛香水插華菓樹枝。",
        "from": "T20n1100_001:0448a25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令持瓶人在前先行引像。",
        "from": "T20n1100_001:0448a25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二人吹蠡。",
        "from": "T20n1100_001:0448a26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "引入王宮有旋一匝。",
        "from": "T20n1100_001:0448a26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南門而出復從東。",
        "from": "T20n1100_001:0448a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入却遶城內坊市一匝。",
        "from": "T20n1100_001:0448a27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便城南門出。",
        "from": "T20n1100_001:0448a27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "城南門外置一大水瓶。",
        "from": "T20n1100_001:0448a28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中置種種飲食雜菓及麨。",
        "from": "T20n1100_001:0448a28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿闍梨誦妙色身如來真言加持七遍。",
        "from": "T20n1100_001:0448a29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後誦葉衣觀自在菩薩陀羅尼七遍。",
        "from": "T20n1100_001:0448b01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於真言句中。",
        "from": "T20n1100_001:0448b02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱國王名號加持。",
        "from": "T20n1100_001:0448b02_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願國王界無諸災難。",
        "from": "T20n1100_001:0448b02_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後於路側曠野。",
        "from": "T20n1100_001:0448b03_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "棄擲水瓶令破。",
        "from": "T20n1100_001:0448b03_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是告言。",
        "from": "T20n1100_001:0448b04_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "閻魔界中行病鬼等。",
        "from": "T20n1100_001:0448b04_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等受領此飲食。",
        "from": "T20n1100_001:0448b04_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復道而歸。",
        "from": "T20n1100_001:0448b05_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸有情起大慈心。",
        "from": "T20n1100_001:0448b05_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令此國界無諸災難。",
        "from": "T20n1100_001:0448b06_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法欲求長壽無病者。",
        "from": "T20n1100_001:0448b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意大小。",
        "from": "T20n1100_001:0448b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[疊*毛]上畫葉衣觀自在菩薩像。",
        "from": "T20n1100_001:0448b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於施願手下。",
        "from": "T20n1100_001:0448b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫彼男女。",
        "from": "T20n1100_001:0448b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其像置道場中。",
        "from": "T20n1100_001:0448b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每日香華飲食旋繞供養發願。",
        "from": "T20n1100_001:0448b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常得加持滿其所願。",
        "from": "T20n1100_001:0448b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若國王男女難長難養。",
        "from": "T20n1100_001:0448b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或短壽疾病纏眠寢食不安。",
        "from": "T20n1100_001:0448b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆由宿業因緣生惡宿直。",
        "from": "T20n1100_001:0448b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或數被五曜陵逼本宿。",
        "from": "T20n1100_001:0448b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令身不安。",
        "from": "T20n1100_001:0448b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則於所居之處。",
        "from": "T20n1100_001:0448b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用牛黃或紙或素。",
        "from": "T20n1100_001:0448b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上書二十八大藥叉將真言。",
        "from": "T20n1100_001:0448b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帖四壁上。",
        "from": "T20n1100_001:0448b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先於東方壁上。",
        "from": "T20n1100_001:0448b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帖四大藥叉將真言。",
        "from": "T20n1100_001:0448b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從東北角起首。",
        "from": "T20n1100_001:0448b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂第一藥叉將真言曰。",
        "from": "T20n1100_001:0448b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵儞(引)囉伽(二合)吒抧吽弱娑嚩(二合引)訶第二藥叉將真言曰。",
        "from": "T20n1100_001:0448b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵蘇寗怛囉(二合)吒抧吽弱娑嚩(二合)訶第三藥叉將真言曰。",
        "from": "T20n1100_001:0448b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵布囉拏(二合)迦吒抧吽弱娑嚩(二合)訶第四藥叉將真言曰。",
        "from": "T20n1100_001:0448b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵迦比攞吒抧吽弱娑嚩(二合)訶",
        "from": "T20n1100_001:0448b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次於南方壁上。",
        "from": "T20n1100_001:0448b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貼四大藥叉將真言。",
        "from": "T20n1100_001:0448b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一藥叉將真言曰。",
        "from": "T20n1100_001:0448b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵僧伽吒抧吽弱娑嚩(二合引)訶(引)第二藥叉將真言曰。",
        "from": "T20n1100_001:0448b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵塢波僧伽吒抧吽弱娑嚩(二合引)訶(引)第三藥叉將真言曰。",
        "from": "T20n1100_001:0448b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵啇企羅吒抧吽弱娑嚩(二合引)訶(引)第四藥叉將真言曰。",
        "from": "T20n1100_001:0448c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵難(上)娜(上)吒抧吽弱娑嚩(二合引)訶(引)次於西方壁上。",
        "from": "T20n1100_001:0448c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帖四大藥叉將真言。",
        "from": "T20n1100_001:0448c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一藥叉將真言曰。",
        "from": "T20n1100_001:0448c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵訶哩吒抧吽弱娑嚩(二合引)訶",
        "from": "T20n1100_001:0448c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二藥叉將真言曰。",
        "from": "T20n1100_001:0448c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵訶哩計奢吒抧吽弱娑嚩(二合引)訶(引)第三藥叉將真言曰。",
        "from": "T20n1100_001:0448c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵鉢囉(二合)僕吒抧吽弱娑嚩(二合引)訶(引)第四藥叉將真言曰。",
        "from": "T20n1100_001:0448c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵迦比羅吒抧吽弱娑嚩(二合引)訶(引)次於北方壁上。",
        "from": "T20n1100_001:0448c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帖四大藥叉將真言。",
        "from": "T20n1100_001:0448c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一藥叉將真言曰。",
        "from": "T20n1100_001:0448c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵馱邏拏吒抧吽弱娑嚩(二合引)訶(引)第二藥叉將真言曰。",
        "from": "T20n1100_001:0448c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵馱邏難(上)那吒抧吽弱娑嚩(二合引)訶第三藥叉將真言曰。",
        "from": "T20n1100_001:0448c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵塢儞庾(二合)誐跛羅吒抧吽弱娑嚩(二合引)訶第四藥叉將真言曰。",
        "from": "T20n1100_001:0448c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵尾灑弩吒抧吽弱娑嚩(二合引)訶次於東北隅。",
        "from": "T20n1100_001:0448c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帖一大藥叉將真言曰。",
        "from": "T20n1100_001:0449a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵半支迦吒抧吽弱娑嚩(二合)訶(引)次於東南隅。",
        "from": "T20n1100_001:0449a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帖一大藥叉將真言曰。",
        "from": "T20n1100_001:0449a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵半左(引)羅巘拏吒抧吽弱娑嚩(二合引)訶(引)次於西南隅。",
        "from": "T20n1100_001:0449a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帖一大藥叉將真言曰。",
        "from": "T20n1100_001:0449a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵娑(上)跢儗哩吒抧吽弱娑嚩(二合)訶(引)次於西北隅。",
        "from": "T20n1100_001:0449a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帖一大藥叉將真言曰。",
        "from": "T20n1100_001:0449a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵害麼嚩多吒抧吽弱娑嚩(二合引)訶次於下方足不踏處石上。",
        "from": "T20n1100_001:0449a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鐫四大藥叉將真言。",
        "from": "T20n1100_001:0449a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置於四方地下。",
        "from": "T20n1100_001:0449a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東方地下一大藥叉將真言曰。",
        "from": "T20n1100_001:0449a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵步莫吒抧吽弱娑嚩(二合)訶",
        "from": "T20n1100_001:0449a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南方地下一大藥叉將真言曰。",
        "from": "T20n1100_001:0449a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵蘇步莫吒抧吽弱娑嚩(二合)訶",
        "from": "T20n1100_001:0449a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方地下一大藥叉將真言曰。",
        "from": "T20n1100_001:0449a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵迦羅吒抧吽弱娑嚩(二合)訶",
        "from": "T20n1100_001:0449a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方地下一大藥叉將真言曰。",
        "from": "T20n1100_001:0449a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵塢波迦羅吒抧吽弱娑嚩(二合引)訶次於上方四隅舍上。",
        "from": "T20n1100_001:0449a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各帖一大藥叉將真言。",
        "from": "T20n1100_001:0449a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東北隅舍上一大藥叉將真言曰。",
        "from": "T20n1100_001:0449a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵蘇哩也(二合)吒抧吽弱娑嚩(二合引)訶東南隅舍上一大藥叉將真言曰。",
        "from": "T20n1100_001:0449a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿銀儞(二合)吒抧吽弱娑嚩(二合引)訶西南隅舍上一大藥叉將真言曰。",
        "from": "T20n1100_001:0449a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵蘇摩吒抧吽弱娑嚩(二合引)訶(引)西北隅舍上一大藥叉將真言曰。",
        "from": "T20n1100_001:0449a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩庾吒抧吽弱娑嚩(二合引)訶",
        "from": "T20n1100_001:0449a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帖真言已。",
        "from": "T20n1100_001:0449a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於二十八大藥叉將位。",
        "from": "T20n1100_001:0449a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各各以香塗一小壇。",
        "from": "T20n1100_001:0449a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壇上燒香雜華。",
        "from": "T20n1100_001:0449a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲食燈燭閼伽虔誠啟告。",
        "from": "T20n1100_001:0449a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願二十八大藥叉將并諸眷屬。",
        "from": "T20n1100_001:0449b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各住本方護持守護某甲令除災禍不祥疾病夭壽。",
        "from": "T20n1100_001:0449b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲得色力增長聰慧。",
        "from": "T20n1100_001:0449b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "威肅端嚴具足。",
        "from": "T20n1100_001:0449b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "易養易長壽命長遠。",
        "from": "T20n1100_001:0449b04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是加持已。",
        "from": "T20n1100_001:0449b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十八大藥叉將不敢違越諸佛。",
        "from": "T20n1100_001:0449b04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如觀自在菩薩及金剛手菩薩教勅。",
        "from": "T20n1100_001:0449b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晝夜擁護臥安覺安獲大威德。",
        "from": "T20n1100_001:0449b06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有國王作此法者。",
        "from": "T20n1100_001:0449b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其王境內災疾消滅。",
        "from": "T20n1100_001:0449b07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國土安寧人民歡樂。",
        "from": "T20n1100_001:0449b08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法應畫本生宿直。",
        "from": "T20n1100_001:0449b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每月供養。",
        "from": "T20n1100_001:0449b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作如是法者。",
        "from": "T20n1100_001:0449b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡宿直轉成吉祥。",
        "from": "T20n1100_001:0449b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以白檀香刻作葉衣觀自在菩薩像。",
        "from": "T20n1100_001:0449b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并於樺皮上書此真言。",
        "from": "T20n1100_001:0449b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共帶。",
        "from": "T20n1100_001:0449b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作此法取鬼宿直日受灌頂。",
        "from": "T20n1100_001:0449b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其灌頂瓶以繒繫項。",
        "from": "T20n1100_001:0449b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿盛香水。",
        "from": "T20n1100_001:0449b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水中著七寶及五種藥。",
        "from": "T20n1100_001:0449b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂娑訶者囉等。",
        "from": "T20n1100_001:0449b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娑訶泥嚩。",
        "from": "T20n1100_001:0449b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建吒迦哩。",
        "from": "T20n1100_001:0449b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿哩訶底。",
        "from": "T20n1100_001:0449b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儗哩羯囉拏。",
        "from": "T20n1100_001:0449b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及五種子諸香等。",
        "from": "T20n1100_001:0449b15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以葉衣觀自在陀羅尼。",
        "from": "T20n1100_001:0449b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持一百八遍。",
        "from": "T20n1100_001:0449b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以用灌頂洗諸障難。",
        "from": "T20n1100_001:0449b17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灌頂已。",
        "from": "T20n1100_001:0449b17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取一瓦椀盛種種飲食。",
        "from": "T20n1100_001:0449b17_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼男女頭上遶三匝。",
        "from": "T20n1100_001:0449b18_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令一知法遠送擲破。",
        "from": "T20n1100_001:0449b18_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即結線索。",
        "from": "T20n1100_001:0449b19_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以葉衣真言加持繫其頭上。",
        "from": "T20n1100_001:0449b19_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作如是法。",
        "from": "T20n1100_001:0449b20_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身上疾病鬼魅&#;禱。",
        "from": "T20n1100_001:0449b20_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執曜淩逼本命宿所皆悉殄滅。",
        "from": "T20n1100_001:0449b21_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若人疫病。",
        "from": "T20n1100_001:0449b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取舍彌木(此國無取楮木替之)燃火然後酥護摩。",
        "from": "T20n1100_001:0449b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人髮人骨投一百八遍於火中燒。",
        "from": "T20n1100_001:0449b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七日已來每日供養。",
        "from": "T20n1100_001:0449b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "葉衣觀自在菩薩。",
        "from": "T20n1100_001:0449b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護摩之時。",
        "from": "T20n1100_001:0449b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱彼國城名聚落名村坊名。",
        "from": "T20n1100_001:0449b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切災難悉皆除滅。",
        "from": "T20n1100_001:0449b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是象疫馬疫牛疫水牛疫。",
        "from": "T20n1100_001:0449b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各取本類骨本毛。",
        "from": "T20n1100_001:0449b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作護摩七日七夜亦皆災滅。",
        "from": "T20n1100_001:0449b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法取一瓶滿盛香水。",
        "from": "T20n1100_001:0449c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言加持一百八遍以水淋彼畜。",
        "from": "T20n1100_001:0449c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切疫病悉皆消滅。",
        "from": "T20n1100_001:0449c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若人頭痛。",
        "from": "T20n1100_001:0449c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取有香氣華加持一百八遍。",
        "from": "T20n1100_001:0449c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令鼻嗅即得除愈。",
        "from": "T20n1100_001:0449c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若人患鬼。",
        "from": "T20n1100_001:0449c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魅取粳米粉。",
        "from": "T20n1100_001:0449c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "担作彼魅形。",
        "from": "T20n1100_001:0449c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以鑌刀誦段段截之。",
        "from": "T20n1100_001:0449c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七日護摩即得除差。",
        "from": "T20n1100_001:0449c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若人患瘧。",
        "from": "T20n1100_001:0449c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一日二日三日。",
        "from": "T20n1100_001:0449c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至七日或長時。",
        "from": "T20n1100_001:0449c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "患瘧用牛黃書此真言。",
        "from": "T20n1100_001:0449c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戴即得除差。",
        "from": "T20n1100_001:0449c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法或嬰孩鬼魅。",
        "from": "T20n1100_001:0449c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書此真言帶則得除愈。",
        "from": "T20n1100_001:0449c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法劫賊侵奪坊市村邑。",
        "from": "T20n1100_001:0449c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或欲遠遊路畏劫盜取佉陀羅木末。",
        "from": "T20n1100_001:0449c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護摩誦真言一百八遍。",
        "from": "T20n1100_001:0449c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所去之處無諸障難。",
        "from": "T20n1100_001:0449c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若虫食苗稼。",
        "from": "T20n1100_001:0449c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取砂以真言加持一百八遍。",
        "from": "T20n1100_001:0449c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散於田中虫自遠去五穀豐熟。",
        "from": "T20n1100_001:0449c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "葉衣觀自在菩薩經",
        "from": "T20n1100_001:0449c16_0",
        "context_with_next_sentence": false
    }
]