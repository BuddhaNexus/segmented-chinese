    },
    {
        "source_sentence": "汝當受持讀誦書寫解說。",
        "from": "T21n1353_001:0866c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣令流布此陀羅尼句。",
        "from": "T21n1353_001:0866c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何。",
        "from": "T21n1353_001:0866c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難此呪是三世諸佛所說。",
        "from": "T21n1353_001:0866c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普除一切患難魔事繫縛。",
        "from": "T21n1353_001:0866c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擁護某甲令得安隱。",
        "from": "T21n1353_001:0866c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有毒病若風冷病。",
        "from": "T21n1353_001:0866c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若除結病若心腹脊痛。",
        "from": "T21n1353_001:0866c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼耳身痛及以頭痛。",
        "from": "T21n1353_001:0866c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等患。",
        "from": "T21n1353_001:0866c27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最勝燈王如來。",
        "from": "T21n1353_001:0866c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遣陀羅尼句來與世尊。",
        "from": "T21n1353_001:0866c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦欲令此娑婆世界眾生長夜安隱。",
        "from": "T21n1353_001:0866c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲大善利色力名譽。",
        "from": "T21n1353_001:0866c29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說呪如上。",
        "from": "T21n1353_001:0867a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時四天王以佛神力命不中夭。",
        "from": "T21n1353_001:0867a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告阿難。",
        "from": "T21n1353_001:0867a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若入賊中當念此呪。",
        "from": "T21n1353_001:0867a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若入水火欲詣王宮。",
        "from": "T21n1353_001:0867a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦當念此呪。",
        "from": "T21n1353_001:0867a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有種種鬪諍疾病怖畏之處。",
        "from": "T21n1353_001:0867a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦當憶念此呪。",
        "from": "T21n1353_001:0867a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有王賊水火。",
        "from": "T21n1353_001:0867a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若天龍若夜叉。",
        "from": "T21n1353_001:0867a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若浮陀若富單那若毘舍闍。",
        "from": "T21n1353_001:0867a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若餓鬼若乾陀鬼。",
        "from": "T21n1353_001:0867a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若狂人鬼若使人心亂鬼。",
        "from": "T21n1353_001:0867a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若方道鬼若起死屍鬼若吸人精氣鬼。",
        "from": "T21n1353_001:0867a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一日二日三日四日發病鬼。",
        "from": "T21n1353_001:0867a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若日月星宿師子虎豹。",
        "from": "T21n1353_001:0867a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒蛇狼狗若蝙蝠。",
        "from": "T21n1353_001:0867a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人若非人而來惱害。",
        "from": "T21n1353_001:0867a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怖畏之者無有是處。",
        "from": "T21n1353_001:0867a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難若有比丘比丘尼善男子及善女人。",
        "from": "T21n1353_001:0867a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有受持讀誦書寫解說。",
        "from": "T21n1353_001:0867a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣令流布此陀羅尼句。",
        "from": "T21n1353_001:0867a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火不能燒不溺水死。",
        "from": "T21n1353_001:0867a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀不能傷毒不能害不中方道。",
        "from": "T21n1353_001:0867a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若以此呪結縷繫枯樹上。",
        "from": "T21n1353_001:0867a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求哀救護還生華果。",
        "from": "T21n1353_001:0867a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況人耶。",
        "from": "T21n1353_001:0867a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟除宿業已定果報。",
        "from": "T21n1353_001:0867a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難若於清旦日中及以日暮。",
        "from": "T21n1353_001:0867a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三時誦行不令使有廢捨。",
        "from": "T21n1353_001:0867a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重罪為輕。",
        "from": "T21n1353_001:0867a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有死罪得杖即止。",
        "from": "T21n1353_001:0867a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應被杖者呵責便當即止。",
        "from": "T21n1353_001:0867a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有所求即得勝意。",
        "from": "T21n1353_001:0867a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此神呪力故。",
        "from": "T21n1353_001:0867a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛聖人之所護念故。",
        "from": "T21n1353_001:0867a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時四大天王。",
        "from": "T21n1353_001:0867a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即從座起整衣服。",
        "from": "T21n1353_001:0867a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偏袒右肩右膝著地。",
        "from": "T21n1353_001:0867a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌叉手前白佛言世尊。",
        "from": "T21n1353_001:0867a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今亦欲說陀羅尼句用饒益眾生。",
        "from": "T21n1353_001:0867a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令獲善利色力名譽若有比丘比丘尼優婆塞優婆夷。",
        "from": "T21n1353_001:0867a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持讀誦書寫解說。",
        "from": "T21n1353_001:0867a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣令流布此陀羅尼句。",
        "from": "T21n1353_001:0867a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有惡人若非人。",
        "from": "T21n1353_001:0867a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能得其便。",
        "from": "T21n1353_001:0867a26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1353_001:0867a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多[口*姪]他　多檀多檀啑　毘檀啑　多陀耶槃泥　亶互啑　過羯泥　噘檛泥噘檛泥　談摩隷　襜摩隷娑波呵爾時世尊即說偈言。",
        "from": "T21n1353_001:0867a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　此陀羅尼經　　諸佛先已說",
        "from": "T21n1353_001:0867b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　我今亦演暢　　饒益諸眾生",
        "from": "T21n1353_001:0867b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若人信樂心　　聞持如是呪",
        "from": "T21n1353_001:0867b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　能增於辯才　　其所往來處",
        "from": "T21n1353_001:0867b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　不遭諸厄難　　無有人非人",
        "from": "T21n1353_001:0867b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　鳩槃富單那　　不能作惱害",
        "from": "T21n1353_001:0867b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　誦持此呪故　　不為他所殺",
        "from": "T21n1353_001:0867b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　誦持此呪故　　天魔王波旬",
        "from": "T21n1353_001:0867b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　欲界中特尊　　不能作留難",
        "from": "T21n1353_001:0867b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　誦持此經故　　父母及兄弟",
        "from": "T21n1353_001:0867b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　眷屬及親厚　　不逢諸衰禍",
        "from": "T21n1353_001:0867b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　誦持此經故　　於百千億劫",
        "from": "T21n1353_001:0867b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　造作不善業　　十惡及五逆",
        "from": "T21n1353_001:0867b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　七日能令滅　　誦持此呪故",
        "from": "T21n1353_001:0867b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　菩薩於億劫　　積集諸功德",
        "from": "T21n1353_001:0867b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　七日能令勝　　誦持此呪故",
        "from": "T21n1353_001:0867b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　四大天王尊　　毘頭羅提頭",
        "from": "T21n1353_001:0867b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　博叉毘留茶　　大臣眾眷屬",
        "from": "T21n1353_001:0867b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　歡喜而讚歎　　恭敬常守護",
        "from": "T21n1353_001:0867b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　誦持此呪故",
        "from": "T21n1353_001:0867b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說此經已。",
        "from": "T21n1353_001:0867b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利弗及諸比丘。",
        "from": "T21n1353_001:0867b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切世間天人阿修羅等。",
        "from": "T21n1353_001:0867b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛所說皆大歡喜。",
        "from": "T21n1353_001:0867b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂戴奉行作禮而去。",
        "from": "T21n1353_001:0867b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乞夢即知吉凶陀羅尼",
        "from": "T21n1353_001:0867b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無三寶　南無摩尼跋陀　大鬼神將　摩訶怛茶　陀羅尼　尼律師　那氷伽梨　阿梨伽梨[口*戒]　多氷伽梨　阿跋多蛇　阿跋多蛇　摩尼跋陀林　摩訶乾豆波乾陀四天王提頭賴吒天毘樓勒天。",
        "from": "T21n1353_001:0867b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟睒門天毘沙門天。",
        "from": "T21n1353_001:0867c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忉利天鹽摩羅天。",
        "from": "T21n1353_001:0867c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兜率陀天不憍樂天。",
        "from": "T21n1353_001:0867c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他應聲天化自在天。",
        "from": "T21n1353_001:0867c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵眾妙天梵輔祿天。",
        "from": "T21n1353_001:0867c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩訶梵天水行梵天。",
        "from": "T21n1353_001:0867c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水微梵天水無量梵天。",
        "from": "T21n1353_001:0867c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水應林天約淨天。",
        "from": "T21n1353_001:0867c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍淨明天守妙天。",
        "from": "T21n1353_001:0867c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微妙天極妙天。",
        "from": "T21n1353_001:0867c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福愛勝天近際天。",
        "from": "T21n1353_001:0867c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普觀天快見天。",
        "from": "T21n1353_001:0867c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無結受天色究竟天。",
        "from": "T21n1353_001:0867c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨光天普等天。",
        "from": "T21n1353_001:0867c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為三十三。",
        "from": "T21n1353_001:0867c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "訶尼訶尼　訶尼佉　訶尼佉　頭佉尼佉　知佉　知佉呪水與人飲之阿竭帝　波羅竭帝　波羅僧竭帝　婆羅毘羅延帝　波羅波羅　摩牟闍摩牟闍羅帝　三波羅闍羅帝　毘提提希希尼尼　憂句隷牟句頭帝　檀那竭帝　檀那羅帝　檀那僧多羅　伊波呵大要縛鬼(一縛鬼二香爐)婆利摩訶婆利闍梨摩訶闍梨　塸牟訶沙　婆帝摩訶沙婆帝　阿波吒僧金吒　陀羅尼　多陀呵　阿伽闍　尼婆毘薩婆達摩尼婆毘　伊舍摩陀　毘婆舍那　毘摩羅　修鉢離達摩尼佉那婆樓尼　文夜多　摩隷遮隷睺樓睺樓　朔鞞帝　曼多羅曼多羅　娑呵支波晝支波晝　呼奴波晝呼奴波晝　浮樓波晝浮樓波晝　支波晝支波晝　阿若波晝　都呼那鉢晝　舍摩奴波晝　胡修帝鉢晝　蜜祇呼那波晝　伊呼希那鉢晝　尼犁祇帝帝那鉢晝　婆若帝那鉢晝　密若奴　呼帝那鉢晝　欝庶臭帝那波晝　娑呵(上天)轉鉢呪南無摩等耆那　南無摩等阿　羅闍羅　南無摩才伽伽利伽他利　伽那怛衫那摩悉吉利埵飲　摩毘婆羅婆叉咩　桑毘闍桑毘闍底　阿吒尼婆吒尼　婆曼陀阿樓休　篼吒僧伽毘咩　阿悉多多梨央求瑟吒波羶底　他利伽陀利　伽那　地脾那　斫蓄娑　佛陀薩氈　達摩薩氈　僧伽薩氈　辟支佛陀薩氈　阿羅漢薩氈　提婆薩氈　律師薩氈　波羶底　他利伽他利伽那地脾那　斫蓄娑　莎呵呪鏡須童男子若見道人威儀齊整及外國人者。",
        "from": "T21n1353_001:0867c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此事皆吉。",
        "from": "T21n1353_001:0868a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見道人形容醜陋。",
        "from": "T21n1353_001:0868a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及見人啼哭著敗衣裳。",
        "from": "T21n1353_001:0868a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是不吉相。",
        "from": "T21n1353_001:0868a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東方最勝燈王陀羅尼經",
        "from": "T21n1353_001:0868a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(此經丹藏即云東方最勝燈王如來經。",
        "from": "T21n1353_001:0868a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文句大多陪宋本而有餘。",
        "from": "T21n1353_001:0868a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "按開元錄云。",
        "from": "T21n1353_001:0868a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後四譯三存一失。",
        "from": "T21n1353_001:0868a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即東晉失譯。",
        "from": "T21n1353_001:0868a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為陀羅尼章句經。",
        "from": "T21n1353_001:0868a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或無章字者是第三譯而一闕本也。",
        "from": "T21n1353_001:0868a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此崛多譯。",
        "from": "T21n1353_001:0868a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宋丹兩本文義大別名亦小異。",
        "from": "T21n1353_001:0868a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未知孰是。",
        "from": "T21n1353_001:0868a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "意者宋本古質。",
        "from": "T21n1353_001:0868a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "似非崛多之譯。",
        "from": "T21n1353_001:0868a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又於文中數云陀羅尼句。",
        "from": "T21n1353_001:0868a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即恐此經是失本陀羅尼句經。",
        "from": "T21n1353_001:0868a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宋藏失崛多譯而得東晉失本陀羅尼經。",
        "from": "T21n1353_001:0868a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見經中有最勝燈王之言。",
        "from": "T21n1353_001:0868a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂為崛多所譯燈王經耳。",
        "from": "T21n1353_001:0868a22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不然何一人之譯。",
        "from": "T21n1353_001:0868a23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而有名題始末之殊。",
        "from": "T21n1353_001:0868a23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣略質文之異耶。",
        "from": "T21n1353_001:0868a24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今雙存之則四譯還具矣)。",
        "from": "T21n1353_001:0868a24_20",
        "context_with_next_sentence": false
    }
]