[
    {
        "source_sentence": "東方最勝燈王陀羅尼經",
        "from": "T21n1353_001:0866a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大隋北印度三藏闍那崛多譯",
        "from": "T21n1353_001:0866a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T21n1353_001:0866a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在舍衛國祇陀林中孤獨精舍。",
        "from": "T21n1353_001:0866a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大比丘僧千二百五十人俱。",
        "from": "T21n1353_001:0866a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大菩薩眾四萬人俱。",
        "from": "T21n1353_001:0866a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娑婆世界主梵天王釋提桓因。",
        "from": "T21n1353_001:0866a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四大天王提頭賴吒天王毘留勒叉天王。",
        "from": "T21n1353_001:0866a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘留博叉天王毘沙門天王。",
        "from": "T21n1353_001:0866a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將二十八部鬼神大將軍。",
        "from": "T21n1353_001:0866a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等諸天八萬四千。",
        "from": "T21n1353_001:0866a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呵利多將其子及眷屬悉來會坐。",
        "from": "T21n1353_001:0866a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時去此佛世界過十萬億佛土有世界名眾華。",
        "from": "T21n1353_001:0866a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛號最勝燈王如來等正覺。",
        "from": "T21n1353_001:0866a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今現在說法。",
        "from": "T21n1353_001:0866a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遣二菩薩。",
        "from": "T21n1353_001:0866a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名大光二名無量光。",
        "from": "T21n1353_001:0866a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而告之曰。",
        "from": "T21n1353_001:0866a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子汝持此陀羅尼句。",
        "from": "T21n1353_001:0866a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至娑婆世界與釋迦牟尼佛。",
        "from": "T21n1353_001:0866a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此呪多所饒益。",
        "from": "T21n1353_001:0866a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令眾生長夜安隱。",
        "from": "T21n1353_001:0866a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲大善利色力名譽。",
        "from": "T21n1353_001:0866a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1353_001:0866a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[口*殊]隷　摩訶[口*殊]隷　[口*殊]羅尼　阿溪摩溪(用吳音)娑曼汦　摩訶娑曼汦　薩移莎攬寐　娑波呵時二菩薩從佛受持此陀羅尼句。",
        "from": "T21n1353_001:0866a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如壯夫屈申臂頃。",
        "from": "T21n1353_001:0866b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從眾華國沒。",
        "from": "T21n1353_001:0866b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到舍衛國祇陀林中孤獨精舍。",
        "from": "T21n1353_001:0866b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時二菩薩前禮佛足。",
        "from": "T21n1353_001:0866b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "却住一面白佛言世尊。",
        "from": "T21n1353_001:0866b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "去此佛世界過十萬億佛土。",
        "from": "T21n1353_001:0866b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有世界名眾華。",
        "from": "T21n1353_001:0866b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛號最勝燈王如來正等覺。",
        "from": "T21n1353_001:0866b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今現在說法。",
        "from": "T21n1353_001:0866b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遣我等來問訊世尊無病無惱。",
        "from": "T21n1353_001:0866b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸弟子眾不為若魔若鬼。",
        "from": "T21n1353_001:0866b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若天若龍若夜叉。",
        "from": "T21n1353_001:0866b07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若浮陀若富單那若毘舍耶。",
        "from": "T21n1353_001:0866b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若餓鬼若干人鬼若乾陀鬼。",
        "from": "T21n1353_001:0866b08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若狂人鬼若使人心亂鬼。",
        "from": "T21n1353_001:0866b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若方道鬼若起死屍鬼。",
        "from": "T21n1353_001:0866b09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一日二日三日四日發病鬼。",
        "from": "T21n1353_001:0866b10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若日月星宿師子虎豹。",
        "from": "T21n1353_001:0866b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒蛇狼狗蝙蝠人若非人。",
        "from": "T21n1353_001:0866b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而來惱害。",
        "from": "T21n1353_001:0866b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怖畏之者無有是處。",
        "from": "T21n1353_001:0866b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難若有善男子善女人。",
        "from": "T21n1353_001:0866b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持讀誦書寫解說。",
        "from": "T21n1353_001:0866b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣令流布此陀羅尼句。",
        "from": "T21n1353_001:0866b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火不能燒不溺水死。",
        "from": "T21n1353_001:0866b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀不能傷毒不能害。",
        "from": "T21n1353_001:0866b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王亦不殺梵等諸天不嫌恨之。",
        "from": "T21n1353_001:0866b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能自憶念十世生事。",
        "from": "T21n1353_001:0866b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難此呪是七十七億諸佛所說。",
        "from": "T21n1353_001:0866b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有違犯此呪及持呪者奪其精氣。",
        "from": "T21n1353_001:0866b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即為侵毀違逆是諸佛已。",
        "from": "T21n1353_001:0866b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時彌勒菩薩。",
        "from": "T21n1353_001:0866b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即從坐起整衣服。",
        "from": "T21n1353_001:0866b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偏袒右肩右膝著地。",
        "from": "T21n1353_001:0866b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌叉手前白佛言世尊。",
        "from": "T21n1353_001:0866b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今亦欲說陀羅尼句用饒益眾生。",
        "from": "T21n1353_001:0866b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令獲善利色力名譽。",
        "from": "T21n1353_001:0866b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1353_001:0866b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿[口*致]婆[口*致]　哪[口*致]拘哪[口*致]　阿[少/兔]跋帝　婆羅提跋致　兜樓寐　樓漏寐　阿那迦賜　波那迦賜　迦賜　摩訶迦賜　阿[少/兔]多　波羅[少/兔]多　却翅　摩訶却翅　斯陀[這-言+卑]啼利　多檀底啼利　寐利　知利　尸利　哹寐　摩訶哹寐　哹哹寐　哹賒婆帝　娑波呵爾時佛復告阿難。",
        "from": "T21n1353_001:0866b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當受持讀誦書寫解說。",
        "from": "T21n1353_001:0866b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣令流布此陀羅尼句。",
        "from": "T21n1353_001:0866c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何。",
        "from": "T21n1353_001:0866c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難此呪能自憶念十三世生事。",
        "from": "T21n1353_001:0866c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難此呪是八十四億諸佛所說。",
        "from": "T21n1353_001:0866c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有違犯此呪及持呪者奪其精氣。",
        "from": "T21n1353_001:0866c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即為侵毀違逆是諸佛已。",
        "from": "T21n1353_001:0866c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時文殊師利法王子菩薩摩訶薩。",
        "from": "T21n1353_001:0866c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即從坐起齊整衣服。",
        "from": "T21n1353_001:0866c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偏袒右肩右膝著地。",
        "from": "T21n1353_001:0866c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌叉手前白佛言。",
        "from": "T21n1353_001:0866c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我今亦欲說陀羅尼句欲饒益眾生。",
        "from": "T21n1353_001:0866c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令獲善利色力名譽。",
        "from": "T21n1353_001:0866c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1353_001:0866c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿叉企　娑企　那祇　那祇膩　那祇娑隷　婆那尼利　[口*虫*也]呵疑　[口*虫*也]呵羅跋致　禪弟疑　禪提羅跋致　娑波呵爾時佛復告阿難。",
        "from": "T21n1353_001:0866c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當受持讀誦書寫解說。",
        "from": "T21n1353_001:0866c12_1",
