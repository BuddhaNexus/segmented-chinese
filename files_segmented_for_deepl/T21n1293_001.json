[
    {
        "source_sentence": "般若守護十六善神王形體",
        "from": "T21n1293_001:0378a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛智三藏譯",
        "from": "T21n1293_001:0378a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提頭攞宅善神　綠青色。",
        "from": "T21n1293_001:0378a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開口現忿怒相貌。",
        "from": "T21n1293_001:0378a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被甲冑著赤衣。",
        "from": "T21n1293_001:0378a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持大刀。",
        "from": "T21n1293_001:0378a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手捧鉾。",
        "from": "T21n1293_001:0378a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髮紫色也。",
        "from": "T21n1293_001:0378a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘盧勒叉善神　赤紫色。",
        "from": "T21n1293_001:0378a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忿怒相現閉唇之形。",
        "from": "T21n1293_001:0378a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右拔折囉。",
        "from": "T21n1293_001:0378a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左押腰。",
        "from": "T21n1293_001:0378a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被甲冑著白青色衣。",
        "from": "T21n1293_001:0378a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬢髮紺色。",
        "from": "T21n1293_001:0378a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摧伏毒害善神　鬢聳立弭也。",
        "from": "T21n1293_001:0378a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "赤肉色。",
        "from": "T21n1293_001:0378a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持大刀。",
        "from": "T21n1293_001:0378a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手向外當胸。",
        "from": "T21n1293_001:0378a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被甲冑著白色衣服餘如常。",
        "from": "T21n1293_001:0378a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增益善神　赤肉色。",
        "from": "T21n1293_001:0378a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四臂容怒鬼。",
        "from": "T21n1293_001:0378a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右第一手持大刀。",
        "from": "T21n1293_001:0378a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二手把劍輪。",
        "from": "T21n1293_001:0378a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左第一手持楊枝。",
        "from": "T21n1293_001:0378a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二手捧半月。",
        "from": "T21n1293_001:0378a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著袈裟其色綠色。",
        "from": "T21n1293_001:0378a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "裳紅懸頸瓔珞。",
        "from": "T21n1293_001:0378a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡喜善神　綠色。",
        "from": "T21n1293_001:0378a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂上有孔雀。",
        "from": "T21n1293_001:0378a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其色金色。",
        "from": "T21n1293_001:0378a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持一股鉤。",
        "from": "T21n1293_001:0378a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手押腰。",
        "from": "T21n1293_001:0378a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被甲冑著赤衣。",
        "from": "T21n1293_001:0378a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬢髮丹色。",
        "from": "T21n1293_001:0378a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面體忿怒也。",
        "from": "T21n1293_001:0378a29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除一切障難善神　黃色。",
        "from": "T21n1293_001:0378b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作瞋怒相。",
        "from": "T21n1293_001:0378b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六臂被鮮白衣服。",
        "from": "T21n1293_001:0378b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持三戟叉。",
        "from": "T21n1293_001:0378b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次手持經典。",
        "from": "T21n1293_001:0378b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次手捧舍利寶塔。",
        "from": "T21n1293_001:0378b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持紅蓮華。",
        "from": "T21n1293_001:0378b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次手持寶鉤。",
        "from": "T21n1293_001:0378b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次手把寶螺。",
        "from": "T21n1293_001:0378b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戴寶冠著袈裟。",
        "from": "T21n1293_001:0378b04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吹天衣風。",
        "from": "T21n1293_001:0378b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瓔珞環釧如常。",
        "from": "T21n1293_001:0378b04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拔除罪垢善神　裸形被赤綠色袈裟。",
        "from": "T21n1293_001:0378b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髮毛聳立。",
        "from": "T21n1293_001:0378b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "赤黃色相也。",
        "from": "T21n1293_001:0378b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持棒。",
        "from": "T21n1293_001:0378b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其棒有五叉。",
        "from": "T21n1293_001:0378b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手作拳置頂上。",
        "from": "T21n1293_001:0378b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞋眼開口。",
        "from": "T21n1293_001:0378b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利牙出上下。",
        "from": "T21n1293_001:0378b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甚怖畏也。",
        "from": "T21n1293_001:0378b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能忍善神　空色。",
        "from": "T21n1293_001:0378b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持大刀。",
        "from": "T21n1293_001:0378b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持矟。",
        "from": "T21n1293_001:0378b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被頂巾。",
        "from": "T21n1293_001:0378b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其色白下至腂纔見冑端。",
        "from": "T21n1293_001:0378b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬢髮紫也(已上右方立之)。",
        "from": "T21n1293_001:0378b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吠室羅摩拏善神　青黑色。",
        "from": "T21n1293_001:0378b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現瞋王相。",
        "from": "T21n1293_001:0378b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作閉唇相。",
        "from": "T21n1293_001:0378b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持金剛棓。",
        "from": "T21n1293_001:0378b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手捧全身舍利寶塔。",
        "from": "T21n1293_001:0378b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被甲冑著赤衣。",
        "from": "T21n1293_001:0378b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬢髮紫色也。",
        "from": "T21n1293_001:0378b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘盧博叉善神　肉色。",
        "from": "T21n1293_001:0378b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懸黑絲臂。",
        "from": "T21n1293_001:0378b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以筆作書寫之勢。",
        "from": "T21n1293_001:0378b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被甲冑著綠色衣服。",
        "from": "T21n1293_001:0378b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬢髮赤色。",
        "from": "T21n1293_001:0378b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微笑之形也。",
        "from": "T21n1293_001:0378b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離一切怖畏神　形體如帝釋天。",
        "from": "T21n1293_001:0378b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但頂上有髑髏。",
        "from": "T21n1293_001:0378b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髑髏上有三股形。",
        "from": "T21n1293_001:0378b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救護一切善神　青白色。",
        "from": "T21n1293_001:0378b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相貌如毘沙門天。",
        "from": "T21n1293_001:0378b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左右手合掌。",
        "from": "T21n1293_001:0378b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十指端有紅色微敷蓮華。",
        "from": "T21n1293_001:0378b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣服赤黃色。",
        "from": "T21n1293_001:0378b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝伏諸魔善神　髮毛聳立。",
        "from": "T21n1293_001:0378b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黑肉色。",
        "from": "T21n1293_001:0378b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相貌以瓔珞為莊嚴。",
        "from": "T21n1293_001:0378b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持劍。",
        "from": "T21n1293_001:0378b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手下掌向外。",
        "from": "T21n1293_001:0378b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣服黃色。",
        "from": "T21n1293_001:0378b25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能救諸有善神　白綠色顏貌玉相也。",
        "from": "T21n1293_001:0378b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手虛心合掌屈二頭指。",
        "from": "T21n1293_001:0378b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "附中指背第一文。",
        "from": "T21n1293_001:0378b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冑上被白色袈裟。",
        "from": "T21n1293_001:0378b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "裳赤色也。",
        "from": "T21n1293_001:0378b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師子威猛善神　肉色戴師子寶冠。",
        "from": "T21n1293_001:0378c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右第一持斧。",
        "from": "T21n1293_001:0378c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二手持劍。",
        "from": "T21n1293_001:0378c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左第一手持梵夾。",
        "from": "T21n1293_001:0378c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二手持三叉戟。",
        "from": "T21n1293_001:0378c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被袈裟其色紺色也。",
        "from": "T21n1293_001:0378c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著小服虎皮。",
        "from": "T21n1293_001:0378c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "極惡忿怒也。",
        "from": "T21n1293_001:0378c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勇猛心地善神　被甲冑綠色。",
        "from": "T21n1293_001:0378c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如能救諸有。",
        "from": "T21n1293_001:0378c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手外縛當胸。",
        "from": "T21n1293_001:0378c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微笑之相也(已上左方立之)。",
        "from": "T21n1293_001:0378c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本奧云。",
        "from": "T21n1293_001:0378c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又陀羅尼集經第三卷說十六善神名字并總呪。",
        "from": "T21n1293_001:0378c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又如空比丘夢感自塔院律師之(已上)。",
        "from": "T21n1293_001:0378c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "谷響集八云。",
        "from": "T21n1293_001:0378c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世有金剛智所譯般若守護十六善神一卷三紙。",
        "from": "T21n1293_001:0378c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讀之明知非是金剛智筆語。",
        "from": "T21n1293_001:0378c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "綴文甚拙。",
        "from": "T21n1293_001:0378c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想先達依金剛智圖畫錄之。",
        "from": "T21n1293_001:0378c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而假金智名矣。",
        "from": "T21n1293_001:0378c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不自揣之甚。",
        "from": "T21n1293_001:0378c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今謂於翻譯席設譯語筆授證義綴文等職。",
        "from": "T21n1293_001:0378c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翻梵為漢。",
        "from": "T21n1293_001:0378c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必不譯主之一手。",
        "from": "T21n1293_001:0378c15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而有再治有未治。",
        "from": "T21n1293_001:0378c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯以義通為其長。",
        "from": "T21n1293_001:0378c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈以文飾為。",
        "from": "T21n1293_001:0378c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今歲得一本於智積院方丈。",
        "from": "T21n1293_001:0378c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "挍小池坊藏本而印行。",
        "from": "T21n1293_001:0378c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時享和二壬戌年仲夏月。",
        "from": "T21n1293_001:0378c18_0",
