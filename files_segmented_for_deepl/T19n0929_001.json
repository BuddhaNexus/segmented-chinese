[
    {
        "source_sentence": "淨瑠璃淨土摽",
        "from": "T19n0929_001:0066b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥師瑠璃光如來。",
        "from": "T19n0929_001:0066b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通身淺碧色。",
        "from": "T19n0929_001:0066b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶窟中百寶蓮華。",
        "from": "T19n0929_001:0066b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師座上大空三昧。",
        "from": "T19n0929_001:0066b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放無數光明。",
        "from": "T19n0929_001:0066b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一光中。",
        "from": "T19n0929_001:0066b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量分身諸佛。",
        "from": "T19n0929_001:0066c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉大空三昧相。",
        "from": "T19n0929_001:0066c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右臂揚掌調手開葉。",
        "from": "T19n0929_001:0066c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左安臍下勾小指。",
        "from": "T19n0929_001:0066c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號大寶瑠璃藥。",
        "from": "T19n0929_001:0066c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其地紺瑠璃色。",
        "from": "T19n0929_001:0066c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塵數無量寶花樹。",
        "from": "T19n0929_001:0066c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樹華葉形。",
        "from": "T19n0929_001:0066c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或佛形。",
        "from": "T19n0929_001:0066c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放無量光明。",
        "from": "T19n0929_001:0066c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名佛波羅蜜。",
        "from": "T19n0929_001:0066c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或菩薩形。",
        "from": "T19n0929_001:0066c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生無量伎樂。",
        "from": "T19n0929_001:0066c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名三昧總持。",
        "from": "T19n0929_001:0066c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或金堂形。",
        "from": "T19n0929_001:0066c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放無量百寶雲。",
        "from": "T19n0929_001:0066c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名菩提妙嚴。",
        "from": "T19n0929_001:0066c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或寶塔形。",
        "from": "T19n0929_001:0066c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生無量天眾。",
        "from": "T19n0929_001:0066c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名覺智樹漠日雨輪樹(津島云功德智惠)無量百千幢幡行列。",
        "from": "T19n0929_001:0066c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量寶網寶鬘鈴鐸等莊嚴。",
        "from": "T19n0929_001:0066c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛前左右四性出家皆入定(所謂國王大臣波羅門居士)左第一重作金剛藏王菩薩。",
        "from": "T19n0929_001:0066c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩通身淺紅青色。",
        "from": "T19n0929_001:0066c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐寶蓮臺上。",
        "from": "T19n0929_001:0066c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手真中掌中立三股金剛杵。",
        "from": "T19n0929_001:0066c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手屈臂五指[网(ㄨ*ㄨ)+又]上。",
        "from": "T19n0929_001:0066c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指押小指爪上。",
        "from": "T19n0929_001:0066c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為塵數菩薩眾侍者。",
        "from": "T19n0929_001:0066c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂雲香菩薩。",
        "from": "T19n0929_001:0066c13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華玉菩薩。",
        "from": "T19n0929_001:0066c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛合掌菩薩。",
        "from": "T19n0929_001:0066c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火頭天相菩薩。",
        "from": "T19n0929_001:0066c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仙相菩薩等。",
        "from": "T19n0929_001:0066c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右第一重作蓮花幢金剛手菩薩。",
        "from": "T19n0929_001:0066c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通身淺青色。",
        "from": "T19n0929_001:0066c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "座寶蓮花臺上。",
        "from": "T19n0929_001:0066c15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手執真陀摩尼。",
        "from": "T19n0929_001:0066c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白寶蓮華臺上安金剛。",
        "from": "T19n0929_001:0066c16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手作與願印(屈中指)塵數菩薩眾以為侍者。",
        "from": "T19n0929_001:0066c17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂青蓮花菩薩。",
        "from": "T19n0929_001:0066c18_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三股金剛杵菩薩。",
        "from": "T19n0929_001:0066c18_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一股金剛鈴鐸菩薩等也。",
        "from": "T19n0929_001:0066c18_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左第二重作日光菩薩。",
        "from": "T19n0929_001:0066c19_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通身赤紅色。",
        "from": "T19n0929_001:0066c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手掌安日。",
        "from": "T19n0929_001:0066c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手執蔓朱赤花貌。",
        "from": "T19n0929_001:0066c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或開敷。",
        "from": "T19n0929_001:0066c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或半開。",
        "from": "T19n0929_001:0066c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或未開。",
        "from": "T19n0929_001:0066c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或其寶菓為之。",
        "from": "T19n0929_001:0066c21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塵數菩薩以為侍者。",
        "from": "T19n0929_001:0066c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂持香爐持摩訶蔓朱赤花等菩薩也。",
        "from": "T19n0929_001:0066c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右第二重作月光菩薩。",
        "from": "T19n0929_001:0066c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通身白紅色左手掌月輪。",
        "from": "T19n0929_001:0066c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手執紅白蓮花。",
        "from": "T19n0929_001:0066c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塵數菩薩以為侍者。",
        "from": "T19n0929_001:0066c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂持鉤持鉤鈴等菩薩。",
        "from": "T19n0929_001:0066c25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白如來近後。",
        "from": "T19n0929_001:0066c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作十弟子及二十一三十五等羅漢。",
        "from": "T19n0929_001:0066c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各以無數聲聞眾以為眷屬。",
        "from": "T19n0929_001:0066c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上首各取物依本形契眷屬等者各在承法右存法形作左右後仰左右後作無數仙人仙女等捧隨時采花。",
        "from": "T19n0929_001:0066c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛前左右外院。",
        "from": "T19n0929_001:0067a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作無量天眾妓樂形。",
        "from": "T19n0929_001:0067a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂堅達婆女琴天樂奉獻貌。",
        "from": "T19n0929_001:0067a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緊那羅自心金鼓樂貌。",
        "from": "T19n0929_001:0067a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伽樓羅女笛天樂貌。",
        "from": "T19n0929_001:0067a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地天女皆鼓樂貌。",
        "from": "T19n0929_001:0067a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿修羅女奉天鼓樂貌。",
        "from": "T19n0929_001:0067a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩睺羅女奉笙樂貌。",
        "from": "T19n0929_001:0067a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大魔侶天眾奉獻舞天樂貌也。",
        "from": "T19n0929_001:0067a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外院上下八方。",
        "from": "T19n0929_001:0067a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作十二神將眾。",
        "from": "T19n0929_001:0067a06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無數金剛童子眾以為眷屬也。",
        "from": "T19n0929_001:0067a06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂十二神將形。",
        "from": "T19n0929_001:0067a07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一宮毘羅伊舍那方可畏大將。",
        "from": "T19n0929_001:0067a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其色黃摽駕虎。",
        "from": "T19n0929_001:0067a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執戰荼劍。",
        "from": "T19n0929_001:0067a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手拳押左腰。",
        "from": "T19n0929_001:0067a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或云作東北方。",
        "from": "T19n0929_001:0067a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二伐折羅因達方金剛大將。",
        "from": "T19n0929_001:0067a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其色青眼赤駕[少/兔]。",
        "from": "T19n0929_001:0067a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執三股伐折羅。",
        "from": "T19n0929_001:0067a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手屈風仰左。",
        "from": "T19n0929_001:0067a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或云作東方。",
        "from": "T19n0929_001:0067a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三迷企羅阿揭拏方護法大將。",
        "from": "T19n0929_001:0067a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其色帶炎上青煙駕龍。",
        "from": "T19n0929_001:0067a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執鉾上炎旗二手調之。",
        "from": "T19n0929_001:0067a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或云作東南方。",
        "from": "T19n0929_001:0067a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四安儞羅阿揭多方護比大將。",
        "from": "T19n0929_001:0067a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其色帶炎上黃煙駕蛇。",
        "from": "T19n0929_001:0067a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執戰荼鉞。",
        "from": "T19n0929_001:0067a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手拳申風指火珠也。",
        "from": "T19n0929_001:0067a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或云作南方。",
        "from": "T19n0929_001:0067a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五安底羅閻魔羅方正法大將。",
        "from": "T19n0929_001:0067a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其色赤放炎雷眼火髮駕兩翼馬。",
        "from": "T19n0929_001:0067a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左執代月盧。",
        "from": "T19n0929_001:0067a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右執鏡上炎也。",
        "from": "T19n0929_001:0067a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或云作西南方。",
        "from": "T19n0929_001:0067a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六珊底羅涅哩堅底方羅剎大將。",
        "from": "T19n0929_001:0067a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其色黃赤作羅剎像駕羊。",
        "from": "T19n0929_001:0067a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左執鏡右執戰荼劍。",
        "from": "T19n0929_001:0067a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或執代只哩也。",
        "from": "T19n0929_001:0067a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或云作西方。",
        "from": "T19n0929_001:0067a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第七因達羅涅哩底方帝使大將。",
        "from": "T19n0929_001:0067a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其色黃色駕[荻火+隹]。",
        "from": "T19n0929_001:0067a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左執白拂。",
        "from": "T19n0929_001:0067a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右執鈴令瞻也。",
        "from": "T19n0929_001:0067a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或云作西方北方。",
        "from": "T19n0929_001:0067a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八跋伊羅傳瞢荼方狼龍大將。",
        "from": "T19n0929_001:0067a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其色白錯駕金翅鳥。",
        "from": "T19n0929_001:0067a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左執如意珠。",
        "from": "T19n0929_001:0067b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右執金剛鐸。",
        "from": "T19n0929_001:0067b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或云作北方。",
        "from": "T19n0929_001:0067b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第九摩睺羅嚩庚方大將。",
        "from": "T19n0929_001:0067b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或云折風大將。",
        "from": "T19n0929_001:0067b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "駕狗狗形可畏。",
        "from": "T19n0929_001:0067b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂天狗。",
        "from": "T19n0929_001:0067b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左拳申風指輪。",
        "from": "T19n0929_001:0067b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右屈風其色如黑風。",
        "from": "T19n0929_001:0067b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或云作上方。",
        "from": "T19n0929_001:0067b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十真達羅縛庚方折水大將。",
        "from": "T19n0929_001:0067b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "駕猪右執鈎左屈地水其色如黑水。",
        "from": "T19n0929_001:0067b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或云作下方。",
        "from": "T19n0929_001:0067b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十一招杜羅毘沙門方護世大將。",
        "from": "T19n0929_001:0067b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又摩尼大將。",
        "from": "T19n0929_001:0067b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "駕鼠。",
        "from": "T19n0929_001:0067b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右執如意珠。",
        "from": "T19n0929_001:0067b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左執羂索。",
        "from": "T19n0929_001:0067b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其色夏日黑雲色也。",
        "from": "T19n0929_001:0067b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或云作西方。",
        "from": "T19n0929_001:0067b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十二毘羯羅伊舍那方懃忿大將。",
        "from": "T19n0929_001:0067b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "駕牛即水牛也。",
        "from": "T19n0929_001:0067b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手執寶弓箭。",
        "from": "T19n0929_001:0067b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其色黑青。",
        "from": "T19n0929_001:0067b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但稍滅前色。",
        "from": "T19n0929_001:0067b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或云关方。",
        "from": "T19n0929_001:0067b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛前內護作迦陵頻伽白鵠。",
        "from": "T19n0929_001:0067b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此鳥諸上首五之所說法音悉同音惕說。",
        "from": "T19n0929_001:0067b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故號共命鳥也。",
        "from": "T19n0929_001:0067b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨瑠璃土可作消息已也。",
        "from": "T19n0929_001:0067b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端題下云　元睿山本(云云)",
        "from": "T19n0929_001:0067b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　　　　　　二校了",
        "from": "T19n0929_001:0067b18_0",
        "context_with_next_sentence": false
    }
]