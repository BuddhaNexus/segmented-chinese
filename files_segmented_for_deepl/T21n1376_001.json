[
    {
        "source_sentence": "佛說聖莊嚴陀羅尼經卷上",
        "from": "T21n1376_001:0895c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試鴻臚少卿",
        "from": "T21n1376_001:0895c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳法大師臣施護奉　詔譯",
        "from": "T21n1376_001:0895c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T21n1376_001:0895c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時世尊。",
        "from": "T21n1376_001:0895c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在迦毘羅城無憂樹園。",
        "from": "T21n1376_001:0895c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與阿難陀等大比丘眾。",
        "from": "T21n1376_001:0896a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及慈氏菩薩摩訶薩俱會一處。",
        "from": "T21n1376_001:0896a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時復有天眾龍眾夜叉眾。",
        "from": "T21n1376_001:0896a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅剎眾阿修羅眾。",
        "from": "T21n1376_001:0896a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦樓羅眾乾闥婆眾。",
        "from": "T21n1376_001:0896a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緊曩羅眾毘舍遮眾。",
        "from": "T21n1376_001:0896a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畢哩多眾鳩槃茶眾。",
        "from": "T21n1376_001:0896a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "布單曩眾執魅眾。",
        "from": "T21n1376_001:0896a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十八大藥叉軍主眾。",
        "from": "T21n1376_001:0896a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四大藥叉軍主眾。",
        "from": "T21n1376_001:0896a05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切星曜眾。",
        "from": "T21n1376_001:0896a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至雪山苦行諸仙眾。",
        "from": "T21n1376_001:0896a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等眾皆來集會。",
        "from": "T21n1376_001:0896a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有他方一切羅剎等眾。",
        "from": "T21n1376_001:0896a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂護迦儞隷布寫那哩枳吒迦囉拏穌摩娜摩賀賀哩摩怛哩眾。",
        "from": "T21n1376_001:0896a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等眾亦來集會。",
        "from": "T21n1376_001:0896a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有四方四大羅剎。",
        "from": "T21n1376_001:0896a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂東方賀哩羅剎。",
        "from": "T21n1376_001:0896a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南方虞拏羅剎。",
        "from": "T21n1376_001:0896a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方枳設哩羅剎。",
        "from": "T21n1376_001:0896a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方摩尼袒哆羅剎。",
        "from": "T21n1376_001:0896a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此四羅剎性極暴惡。",
        "from": "T21n1376_001:0896a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各有六十眷屬。",
        "from": "T21n1376_001:0896a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是等羅剎亦來在會。",
        "from": "T21n1376_001:0896a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時淨飯王宮羅睺羅童子。",
        "from": "T21n1376_001:0896a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於夜臥時為惡羅剎而來嬈亂。",
        "from": "T21n1376_001:0896a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數數驚怖不得安隱。",
        "from": "T21n1376_001:0896a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時羅睺羅母耶輸陀羅及諸眷屬。",
        "from": "T21n1376_001:0896a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉知世尊與諸聖眾。",
        "from": "T21n1376_001:0896a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在無憂樹園去宮城不遠。",
        "from": "T21n1376_001:0896a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於夜後分令羅睺羅。",
        "from": "T21n1376_001:0896a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與眾童子及諸侍人。",
        "from": "T21n1376_001:0896a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後導從往詣佛所。",
        "from": "T21n1376_001:0896a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時羅睺羅童子至佛會已頂禮佛足。",
        "from": "T21n1376_001:0896a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獨於佛前涕泣而住。",
        "from": "T21n1376_001:0896a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告阿難言。",
        "from": "T21n1376_001:0896a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難汝見羅睺羅童子。",
        "from": "T21n1376_001:0896a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來詣我所涕泣已否。",
        "from": "T21n1376_001:0896a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難言唯然已見。",
        "from": "T21n1376_001:0896a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言阿難。",
        "from": "T21n1376_001:0896a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅睺羅童子於夜臥時。",
        "from": "T21n1376_001:0896a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為惡羅剎及種種鬼神。",
        "from": "T21n1376_001:0896a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而來嬈亂不得安隱。",
        "from": "T21n1376_001:0896a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故來我所而求擁護。",
        "from": "T21n1376_001:0896a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難非獨今日羅睺羅童子。",
        "from": "T21n1376_001:0896a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼此嬈亂不得安隱。",
        "from": "T21n1376_001:0896a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我滅度後末世之時。",
        "from": "T21n1376_001:0896a27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生福力微劣。",
        "from": "T21n1376_001:0896a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於不善業增益熾盛。",
        "from": "T21n1376_001:0896a28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由是諸惡羅剎。",
        "from": "T21n1376_001:0896a28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及鬼神等遊行世間。",
        "from": "T21n1376_001:0896a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒與眾生作不饒益。",
        "from": "T21n1376_001:0896a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種嬈亂使無安隱。",
        "from": "T21n1376_001:0896a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我觀是事深可憐愍。",
        "from": "T21n1376_001:0896b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難我今為擁護羅睺羅童子。",
        "from": "T21n1376_001:0896b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及愍念未來眾生。",
        "from": "T21n1376_001:0896b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說大莊嚴陀羅尼。",
        "from": "T21n1376_001:0896b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當諦聽。",
        "from": "T21n1376_001:0896b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時阿難及會大眾。",
        "from": "T21n1376_001:0896b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸各默然願佛宣說。",
        "from": "T21n1376_001:0896b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊即說大莊嚴陀羅尼曰。",
        "from": "T21n1376_001:0896b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也(二合引)他(引)伽儞(引)誐囉(二合引)伽曩鉢底具彌(引)誐囉(二合引)具摩鉢底瑟波(二合)哩(引)迦伽哩尼伽囉鉢底娑(引)囉鉢底馱曩鉢底酥囉鉢底阿隷半阿隷半鉢底摩鉢底阿鉢底嚩　護鉢底護羅賀囉(二合引)呬隷誐囉(二合)護嚕烏底隷普呬隷唵(引)唧致隷鉢哆(二合)鉢致(引)吒吒吠(引)賀囉(二合引)唵(引)羅吠(引)摩尼娑哩(引)護摩賀(引)呬隷護護三滿多娑娑哩阿三滿多娑哩波隷佉娑哩(引)阿(引)挽哩哆(二合)摩賀(引)挽哩帝(二合引)伽儞(引)彌佉隷(引)賀囉(二合引)娑嚩(二合引)賀(引)阿難此大莊嚴陀羅尼。",
        "from": "T21n1376_001:0896b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃是過去諸佛之所宣說。",
        "from": "T21n1376_001:0896b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今為擁護羅睺羅童子。",
        "from": "T21n1376_001:0896b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦復利益末世眾生。",
        "from": "T21n1376_001:0896b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今復宣說。",
        "from": "T21n1376_001:0896b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難若有眾生。",
        "from": "T21n1376_001:0896b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被諸羅剎及惡鬼神種種嬈亂者。",
        "from": "T21n1376_001:0896b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當以此莊嚴陀羅尼書於紙素。",
        "from": "T21n1376_001:0896b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作彼隨求戴於身臂。",
        "from": "T21n1376_001:0896b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當令諸惡羅剎。",
        "from": "T21n1376_001:0896b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至世間一切食膏血者。",
        "from": "T21n1376_001:0896b21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡鬼神類不敢侵近悉皆奔走。",
        "from": "T21n1376_001:0896b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四方四維十由旬外遠避而去。",
        "from": "T21n1376_001:0896b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當令眾生得安隱樂。",
        "from": "T21n1376_001:0896b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難若有眾生。",
        "from": "T21n1376_001:0896b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒常讀誦此大莊嚴陀羅尼者。",
        "from": "T21n1376_001:0896b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有一切刀杖毒藥不能為害。",
        "from": "T21n1376_001:0896b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至邪明呪術羂索繫縛。",
        "from": "T21n1376_001:0896b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人及非人一切惡事悉皆解脫。",
        "from": "T21n1376_001:0896b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至十由旬內。",
        "from": "T21n1376_001:0896b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生皆蒙擁護。",
        "from": "T21n1376_001:0896b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切災沴咸皆殄滅。",
        "from": "T21n1376_001:0896b27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難若有眾生。",
        "from": "T21n1376_001:0896b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲持念此大莊嚴陀羅尼擁護自身。",
        "from": "T21n1376_001:0896b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及為饒益一切眾生者。",
        "from": "T21n1376_001:0896c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先當擇取清淨勝地。",
        "from": "T21n1376_001:0896c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "預立幖幟。",
        "from": "T21n1376_001:0896c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然自齋戒香水澡浴。",
        "from": "T21n1376_001:0896c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當令身心內外清淨。",
        "from": "T21n1376_001:0896c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以線結界香泥塗地作曼拏羅。",
        "from": "T21n1376_001:0896c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於曼拏羅中備諸名花。",
        "from": "T21n1376_001:0896c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗香燒香然燈種種供養。",
        "from": "T21n1376_001:0896c04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面北端坐正念想佛世尊。",
        "from": "T21n1376_001:0896c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來降曼拏羅已。",
        "from": "T21n1376_001:0896c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後奉獻種種供養。",
        "from": "T21n1376_001:0896c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命讚歎。",
        "from": "T21n1376_001:0896c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發堅固心勇猛不退。",
        "from": "T21n1376_001:0896c06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲饒益擁護諸眾生故。",
        "from": "T21n1376_001:0896c07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及求現前所作成就故。",
        "from": "T21n1376_001:0896c07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先當持誦結界莊嚴陀羅尼。",
        "from": "T21n1376_001:0896c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "警覺召請梵王帝釋及一切天龍夜叉羅剎諸鬼神等皆來集會。",
        "from": "T21n1376_001:0896c09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當令世間一切惱害眾生。",
        "from": "T21n1376_001:0896c10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸惡夜叉羅剎部多等。",
        "from": "T21n1376_001:0896c11_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞此陀羅尼已。",
        "from": "T21n1376_001:0896c11_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "驚怖退散無諸障難得願圓滿。",
        "from": "T21n1376_001:0896c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說結界陀羅尼曰。",
        "from": "T21n1376_001:0896c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也(二合引)他儞隷儞隷尾摩隷(引)怛摩隷(引)護唧護唧悉哩底哩怛哩(二合)娑儞(引)囉普莎賀(引)阿難若誦此結界莊嚴陀羅尼已。",
        "from": "T21n1376_001:0896c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後誦彼聖大莊嚴陀羅尼。",
        "from": "T21n1376_001:0896c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切所作無不成就。",
        "from": "T21n1376_001:0896c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時慈氏菩薩摩訶薩從座而起。",
        "from": "T21n1376_001:0896c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住立佛前白言世尊。",
        "from": "T21n1376_001:0896c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦為欲擁護羅睺羅童子令得安隱。",
        "from": "T21n1376_001:0896c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦復為欲利益未來一切眾生。",
        "from": "T21n1376_001:0896c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有被諸嬈惱。",
        "from": "T21n1376_001:0896c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及一切惡人刀杖毒藥。",
        "from": "T21n1376_001:0896c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸惡禁呪羂索繫縛。",
        "from": "T21n1376_001:0896c21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等事悉令解脫。",
        "from": "T21n1376_001:0896c22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於晝夜中安隱快樂。",
        "from": "T21n1376_001:0896c22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦說莊嚴陀羅尼。",
        "from": "T21n1376_001:0896c23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願世尊。",
        "from": "T21n1376_001:0896c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "許我宣說。",
        "from": "T21n1376_001:0896c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言當隨汝意。",
        "from": "T21n1376_001:0896c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時慈氏菩薩即說莊嚴陀羅尼曰。",
        "from": "T21n1376_001:0896c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也(二合引)他(引)阿嚩那(引)底播那(引)儞阿尾那(引)努祖嚕祖嚕惹哩馱(二合)難尼(引)難尼(引)阿馱難尼(引)三滿哆難尼(引)儞哩瞢(二合)尼(引)儞哩弭(二合引)目儞(引)扇(引)底娑嚩(二合)悉怛也(三合引)野喃摩摩薩哩嚩(二合)薩怛嚩(二合引)喃(引)左洛乞叉(二合引)俱嚕娑嚩(二合引)賀(引)爾時娑婆世界主大梵天王。",
        "from": "T21n1376_001:0896c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前白佛言。",
        "from": "T21n1376_001:0897a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦為欲擁護羅睺羅童子。",
        "from": "T21n1376_001:0897a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及擁護未來一切眾生。",
        "from": "T21n1376_001:0897a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說自莊嚴明。",
        "from": "T21n1376_001:0897a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願如來許我宣說。",
        "from": "T21n1376_001:0897a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言善哉善哉當隨汝意。",
        "from": "T21n1376_001:0897a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時大梵天王即說莊嚴明曰。",
        "from": "T21n1376_001:0897a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三野體難盎虞哩(引)半虞哩(引)阿努哩(引)嫩努哩(引)尸契孕(二合)迦尸佉哩(引)嫩度哩(引)努哩努(二合)囉野底阿左阿左尾目左扇(引)底孕(二合)娑嚩(二合)悉怛野(三合)野喃俱嚕摩摩薩哩嚩(二合)薩怛嚩(二合引)喃(引)左娑嚩(二合引)賀(引)爾時世尊。",
        "from": "T21n1376_001:0897a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知彼天眾龍眾夜叉等眾及諸鬼神。",
        "from": "T21n1376_001:0897a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各各為欲擁護羅睺羅童子及未來眾生。",
        "from": "T21n1376_001:0897a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆悉欲說自明。",
        "from": "T21n1376_001:0897a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言汝等天眾龍眾及諸鬼神等。",
        "from": "T21n1376_001:0897a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲說自明為擁護諸眾生者。",
        "from": "T21n1376_001:0897a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨汝等意各各說之。",
        "from": "T21n1376_001:0897a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時善時分天。",
        "from": "T21n1376_001:0897a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於佛前說自明曰。",
        "from": "T21n1376_001:0897a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三野體難囉瑟致哩(三合引)波那(引)瑟致哩(三合引)嚕地囉迦囉尼(二合引)儼鼻哩(引)努哩努(二合)哩母唧(引)設(引)訖哩(二合)囉(引)濟曳(二合引)扇(引)底孕(二合)娑嚩(二合)悉怛也(三合)野喃迦嚕弭摩摩洛乞叉(二合引)俱嚕娑嚩(二合引)賀(引)佛說聖莊嚴陀羅尼經卷上",
        "from": "T21n1376_001:0897a19_0",
        "context_with_next_sentence": false
    }
]