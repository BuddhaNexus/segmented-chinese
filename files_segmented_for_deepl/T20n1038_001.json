        "from": "T20n1038_001:0021b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結界陀羅尼呪曰。",
        "from": "T20n1038_001:0022a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那謨喝囉(上)怛那(二合)跢羅(二合)夜耶(一)那(上)謨訶梨耶婆路枳羝攝皤(二合)囉(去)耶(二)菩(去)提薩埵(去)耶(三)摩訶薩埵(去)耶(四)摩訶迦(去)嚧尼迦(去)耶(去五)跢姪他(六)至里盤陀(去)弭(上七)弭里槃陀(去)弭(上八)至里弭(上)里槃陀弭(上九)似摩(去)槃陀弭(上十)似摩(去)摩迷迦(上)失質(二合)陀底(上)末羯囉(二合)摩都(十一)薩訶(十二)呪水二十一遍散灑十方即成結界。",
        "from": "T20n1038_001:0022a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此陀羅尼呪。",
        "from": "T20n1038_001:0022a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先須受持預前結界。",
        "from": "T20n1038_001:0022a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如我結界即得成就。",
        "from": "T20n1038_001:0022a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉請陀羅尼曰。",
        "from": "T20n1038_001:0022a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "納謨囉(上)怛那(二合)跢囉(二合)夜耶(一)那(上)謨訶梨耶　婆路枳羝攝皤囉耶(二)菩提薩埵耶(三)摩訶薩埵(去)耶(四)摩訶迦嚕尼迦(去)耶(五)跢姪他(六)至里至里(七)弭(上)里弭(上)里(八)至里梨(九)[王*垔]醯(上)婆伽畔(十)阿梨耶婆路枳羝攝皤囉耶(十一)莎訶(十二)若欲請我。",
        "from": "T20n1038_001:0022a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如我所說心請已後。",
        "from": "T20n1038_001:0022a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從白月八日至十五日。",
        "from": "T20n1038_001:0022a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日日三時香湯洗浴著新淨衣。",
        "from": "T20n1038_001:0022a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日日三時時別各誦一百八遍呪。",
        "from": "T20n1038_001:0022a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至十五日倍勝供養誦無遍數。",
        "from": "T20n1038_001:0022a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即其夜半觀世音菩薩。",
        "from": "T20n1038_001:0022a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自來為現金色之身相好莊嚴。",
        "from": "T20n1038_001:0022a29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種光明放千種光。",
        "from": "T20n1038_001:0022b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時呪師心莫怖畏。",
        "from": "T20n1038_001:0022b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者見已即得勝地陀羅尼三摩地已。",
        "from": "T20n1038_001:0022b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即見東方阿閦鞞佛南方寶相佛。",
        "from": "T20n1038_001:0022b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西方阿彌陀佛北方微妙聲佛。",
        "from": "T20n1038_001:0022b04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見如是等十方無量諸佛如來光明色相。",
        "from": "T20n1038_001:0022b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨此身已生淨佛土。",
        "from": "T20n1038_001:0022b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切處佛之所讚歎。",
        "from": "T20n1038_001:0022b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已說普賢陀羅尼竟。",
        "from": "T20n1038_001:0022b06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時觀世音菩薩說是呪已。",
        "from": "T20n1038_001:0022b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九十二俱胝菩薩。",
        "from": "T20n1038_001:0022b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆得住阿毘跋致地。",
        "from": "T20n1038_001:0022b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及得見一切諸佛得聞正法。",
        "from": "T20n1038_001:0022b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得滅一切三障重罪得大功德。",
        "from": "T20n1038_001:0022b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如閻浮提履地微塵等數。",
        "from": "T20n1038_001:0022b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者自身得種種功德莊嚴。",
        "from": "T20n1038_001:0022b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切病苦及諸惡業並皆消滅。",
        "from": "T20n1038_001:0022b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又得捷疾辯又得自在心隨願悉滿。",
        "from": "T20n1038_001:0022b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得具足一切諸波羅蜜。",
        "from": "T20n1038_001:0022b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意往生十方淨土。",
        "from": "T20n1038_001:0022b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見一切諸佛聞說正法。",
        "from": "T20n1038_001:0022b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得首楞嚴等一切三昧。",
        "from": "T20n1038_001:0022b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又得七寶三摩提放光三摩提。",
        "from": "T20n1038_001:0022b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大海水三昧騰空三昧出沒三昧。",
        "from": "T20n1038_001:0022b16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得如是等恒河沙等三昧。",
        "from": "T20n1038_001:0022b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又得無量大力陀羅尼門。",
        "from": "T20n1038_001:0022b17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此呪功德與八十萬陀羅尼功德無異。",
        "from": "T20n1038_001:0022b18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此呪力令我得成如是法身。",
        "from": "T20n1038_001:0022b19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又能利益一切眾生。",
        "from": "T20n1038_001:0022b20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速得種種聰明辯才清淨法身。",
        "from": "T20n1038_001:0022b20_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此呪力一聞總持永不忘失。",
        "from": "T20n1038_001:0022b21_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此呪力我所放索所著之處。",
        "from": "T20n1038_001:0022b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生重罪消滅。",
        "from": "T20n1038_001:0022b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切惡人及惡魔惡鬼惡神。",
        "from": "T20n1038_001:0022b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然降伏消滅。",
        "from": "T20n1038_001:0022b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生聞此呪名及受持者。",
        "from": "T20n1038_001:0022b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永不墮地獄餓鬼畜生。",
        "from": "T20n1038_001:0022b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言善哉善哉汝此呪力。",
        "from": "T20n1038_001:0022b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有四眾能受持者。",
        "from": "T20n1038_001:0022b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所得功德及其威力如我無異。",
        "from": "T20n1038_001:0022b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時觀世音菩薩說是呪時。",
        "from": "T20n1038_001:0022b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三千大千世界六種震動。",
        "from": "T20n1038_001:0022b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上至阿迦膩吒天。",
        "from": "T20n1038_001:0022b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其中所有一切天龍夜叉。",
        "from": "T20n1038_001:0022b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切鬼神人非人等。",
        "from": "T20n1038_001:0022c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉恐怖不安身毛皆竪。",
        "from": "T20n1038_001:0022c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讚言善哉此呪神力不可思議。",
        "from": "T20n1038_001:0022c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生皆蒙利益。",
        "from": "T20n1038_001:0022c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時觀世音菩薩復白佛言。",
        "from": "T20n1038_001:0022c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願佛證知善哉。",
        "from": "T20n1038_001:0022c03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切四眾於我所說呪。",
        "from": "T20n1038_001:0022c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆用心受持恭敬供養。",
        "from": "T20n1038_001:0022c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時觀世音菩薩白佛言。",
        "from": "T20n1038_001:0022c05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今受持此呪。",
        "from": "T20n1038_001:0022c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切大地六種震動。",
        "from": "T20n1038_001:0022c06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生及諸天龍悉皆忙怕。",
        "from": "T20n1038_001:0022c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此呪神力不可思議。",
        "from": "T20n1038_001:0022c07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以何因緣令我及一切眾生。",
        "from": "T20n1038_001:0022c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有受持者得金剛三昧。",
        "from": "T20n1038_001:0022c08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令汝及一切眾生。",
        "from": "T20n1038_001:0022c09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有受持者心得安隱。",
        "from": "T20n1038_001:0022c09_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所得功德及其神力如我無異。",
        "from": "T20n1038_001:0022c10_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此呪功能我更為說。",
        "from": "T20n1038_001:0022c10_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旦起誦呪二十一遍。",
        "from": "T20n1038_001:0022c11_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中時二十一遍。",
        "from": "T20n1038_001:0022c11_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向暮二十一遍。",
        "from": "T20n1038_001:0022c12_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒持不忘能除五逆重罪。",
        "from": "T20n1038_001:0022c12_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又得成就首楞嚴等一切三昧。",
        "from": "T20n1038_001:0022c13_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦得成就一切陀羅尼。",
        "from": "T20n1038_001:0022c13_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又得成就一切佛法。",
        "from": "T20n1038_001:0022c14_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能恒常用心誦念不忘。",
        "from": "T20n1038_001:0022c14_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常見釋迦牟尼佛普賢菩薩。",
        "from": "T20n1038_001:0022c15_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀世音菩薩。",
        "from": "T20n1038_001:0022c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及見天女請受佛法。",
        "from": "T20n1038_001:0022c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲造像。",
        "from": "T20n1038_001:0022c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當畫釋迦牟尼佛坐華座上。",
        "from": "T20n1038_001:0022c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身黃金色著五彩衣。",
        "from": "T20n1038_001:0022c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左廂畫普賢菩薩坐須彌山。",
        "from": "T20n1038_001:0022c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其山左邊有七頭龍繞山。",
        "from": "T20n1038_001:0022c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於左邊出七箇頭。",
        "from": "T20n1038_001:0022c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向菩薩看。",
        "from": "T20n1038_001:0022c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右邊有五頭龍繞山。",
        "from": "T20n1038_001:0022c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於右邊出五箇頭。",
        "from": "T20n1038_001:0022c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向菩薩看。",
        "from": "T20n1038_001:0022c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其菩薩結跏趺坐。",
        "from": "T20n1038_001:0022c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩手執經夾讀著五色衣。",
        "from": "T20n1038_001:0022c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其佛右手作印文。",
        "from": "T20n1038_001:0022c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手捉袈裟按膝說法。",
        "from": "T20n1038_001:0022c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右廂畫觀世音坐華座。",
        "from": "T20n1038_001:0022c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著白色衣胡跪合掌。",
        "from": "T20n1038_001:0022c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面向佛看聽佛說法。",
        "from": "T20n1038_001:0022c24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左廂三手。",
        "from": "T20n1038_001:0022c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一手執華。",
        "from": "T20n1038_001:0022c24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一手捉澡罐。",
        "from": "T20n1038_001:0023a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一手捉經甲。",
        "from": "T20n1038_001:0023a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右廂三手。",
        "from": "T20n1038_001:0023a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一手施無畏出寶。",
        "from": "T20n1038_001:0023a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一手捉索一手捉珠。",
        "from": "T20n1038_001:0023a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩頂上有佛又向。",
        "from": "T20n1038_001:0023a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下作行者。",
        "from": "T20n1038_001:0023a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "胡跪燒香捉珠。",
        "from": "T20n1038_001:0023a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向菩薩看。",
        "from": "T20n1038_001:0023a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普賢下作毘陀天女。",
        "from": "T20n1038_001:0023a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "胡跪坐手捧華冠。",
        "from": "T20n1038_001:0023a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著白衣坐具上。",
        "from": "T20n1038_001:0023a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向菩薩看。",
        "from": "T20n1038_001:0023a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其像淨畫不得有膠。",
        "from": "T20n1038_001:0023a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫依淨法。",
        "from": "T20n1038_001:0023a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說入檀受持法。",
        "from": "T20n1038_001:0023a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右從白月八日入道場。",
        "from": "T20n1038_001:0023a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用牛糞塗地方作壇。",
        "from": "T20n1038_001:0023a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初四肘乃至八肘五色作。",
        "from": "T20n1038_001:0023a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須十六罐子安水及花果子。",
        "from": "T20n1038_001:0023a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須十六具香爐十六燈盞。",
        "from": "T20n1038_001:0023a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作四肘壇。",
        "from": "T20n1038_001:0023a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安四罐子。",
        "from": "T20n1038_001:0023a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四具香爐四枚燈盞。",
        "from": "T20n1038_001:0023a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲食種種果子酥密石密。",
        "from": "T20n1038_001:0023a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒種種香六時莫絕。",
        "from": "T20n1038_001:0023a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者澡浴著淨衣如法。",
        "from": "T20n1038_001:0023a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯得食粳米飯粥乳酪酥密果子石密乾薑胡椒畢鉢。",
        "from": "T20n1038_001:0023a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘者不得其飲食。",
        "from": "T20n1038_001:0023a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從八日獻佛乃至十五日不收。",
        "from": "T20n1038_001:0023a13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十六日收送擲水中及火燒。",
        "from": "T20n1038_001:0023a14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "却從初入道場八日。",
        "from": "T20n1038_001:0023a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依對佛壇前。",
        "from": "T20n1038_001:0023a15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更作一肘壇牛糞作。",
        "from": "T20n1038_001:0023a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取乾穀木及桑木。",
        "from": "T20n1038_001:0023a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取一片呪一遍放壇中。",
        "from": "T20n1038_001:0023a16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至二十一遍竟。",
        "from": "T20n1038_001:0023a17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後取七種穀子。",
        "from": "T20n1038_001:0023a17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用手取呪一遍著火中。",
        "from": "T20n1038_001:0023a18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至二十一遍。",
        "from": "T20n1038_001:0023a18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白日亦得夜闇亦得。",
        "from": "T20n1038_001:0023a19_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日別一度至十五日燒勿忘竟十五日夜不得睡眠一心誦呪。",
        "from": "T20n1038_001:0023a19_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其夜或時地動或時聞大聲。",
        "from": "T20n1038_001:0023a21_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者莫生驚怪。",
        "from": "T20n1038_001:0023a21_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安心誦呪念二菩薩。",
        "from": "T20n1038_001:0023a22_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時普賢觀世音。",
        "from": "T20n1038_001:0023a22_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即為現身語行者言。",
        "from": "T20n1038_001:0023a22_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝須何願隨索皆能滿足。",
        "from": "T20n1038_001:0023a23_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不得相見稱心。",
        "from": "T20n1038_001:0023a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從十六日除壇。",
        "from": "T20n1038_001:0023a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更作新壇。",
        "from": "T20n1038_001:0023a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准前法行道誦呪。",
        "from": "T20n1038_001:0023a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至以得見滿願為限。",
        "from": "T20n1038_001:0023a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其行者入道場。",
        "from": "T20n1038_001:0023a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯得見一人使令共語。",
        "from": "T20n1038_001:0023a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘人更不得語不得見。",
        "from": "T20n1038_001:0023a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者在道場內亦不得語。",
        "from": "T20n1038_001:0023a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須語待出道場。",
        "from": "T20n1038_001:0023a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀世音普賢陀羅尼法具足。",
        "from": "T20n1038_001:0023a28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清淨觀世音普賢陀羅尼經",
        "from": "T20n1038_001:0023a29_0",
        "context_with_next_sentence": false
    }
]