[
    {
        "source_sentence": "清淨觀世音普賢陀羅尼經",
        "from": "T20n1038_001:0021b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大唐總持寺沙門釋智通譯",
        "from": "T20n1038_001:0021b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T20n1038_001:0021b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時婆伽婆。",
        "from": "T20n1038_001:0021b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在王舍城耆闍崛山。",
        "from": "T20n1038_001:0021b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大比丘眾五百人俱。",
        "from": "T20n1038_001:0021b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩無央數。",
        "from": "T20n1038_001:0021b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時觀世音菩薩。",
        "from": "T20n1038_001:0021b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與九十二俱胝菩薩同座。",
        "from": "T20n1038_001:0021b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時觀世音菩薩。",
        "from": "T20n1038_001:0021b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於晨朝從坐而起。",
        "from": "T20n1038_001:0021b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "整衣服合掌恭敬。",
        "from": "T20n1038_001:0021b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭面禮佛而白佛言。",
        "from": "T20n1038_001:0021b21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我欲說普賢陀羅尼。",
        "from": "T20n1038_001:0021b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為憐愍利益一切眾生故。",
        "from": "T20n1038_001:0021b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我從過去月光佛所受得此呪。",
        "from": "T20n1038_001:0021b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今欲對佛說此陀羅尼。",
        "from": "T20n1038_001:0021b23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊聽我說之。",
        "from": "T20n1038_001:0021b24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T20n1038_001:0021b24_16",
        "context_with_next_sentence": true
    },
    {
