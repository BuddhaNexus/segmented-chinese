[
    {
        "source_sentence": "◎高僧傳序錄卷第十四",
        "from": "T50n2059_014:0418b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梁會稽嘉祥寺沙門釋慧皎撰",
        "from": "T50n2059_014:0418b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "原夫至道沖漠。",
        "from": "T50n2059_014:0418b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "假蹄筌而後彰。",
        "from": "T50n2059_014:0418b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玄致幽凝。",
        "from": "T50n2059_014:0418b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藉師保以成用。",
        "from": "T50n2059_014:0418b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是由聖迹迭興賢能異託。",
        "from": "T50n2059_014:0418b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辯忠烈孝慈。",
        "from": "T50n2059_014:0418b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以定君敬之道。",
        "from": "T50n2059_014:0418b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明詩書禮樂。",
        "from": "T50n2059_014:0418b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以成風俗之訓。",
        "from": "T50n2059_014:0418b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或忘功遺事。",
        "from": "T50n2059_014:0418b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尚彼虛沖。",
        "from": "T50n2059_014:0418b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或體任榮枯。",
        "from": "T50n2059_014:0418b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重茲達命。",
        "from": "T50n2059_014:0418b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而皆教但域中功存近益。",
        "from": "T50n2059_014:0418b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斯蓋漸染之方未奧盡其神性。",
        "from": "T50n2059_014:0418b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至若能仁之為訓也。",
        "from": "T50n2059_014:0418b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "考業果幽微。",
        "from": "T50n2059_014:0418b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則循復三世。",
        "from": "T50n2059_014:0418b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言至理高妙。",
        "from": "T50n2059_014:0418b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則貫絕百靈。",
        "from": "T50n2059_014:0418b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若夫啟十地以辯慧宗。",
        "from": "T50n2059_014:0418b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯二諦以詮智府。",
        "from": "T50n2059_014:0418b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "窮神盡性之旨。",
        "from": "T50n2059_014:0418b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "管一樞極之致。",
        "from": "T50n2059_014:0418b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘教方之猶群流之歸巨壑。",
        "from": "T50n2059_014:0418b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾星之共北辰。",
        "from": "T50n2059_014:0418b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悠哉邈矣。",
        "from": "T50n2059_014:0418b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信難得以言尚至。",
        "from": "T50n2059_014:0418b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迺教滿三千。",
        "from": "T50n2059_014:0418b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形遍六道。",
        "from": "T50n2059_014:0418b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆所以接引幽昏為大利益。",
        "from": "T50n2059_014:0418b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而以淨穢異聞昇墜殊見。",
        "from": "T50n2059_014:0418b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故秋方先音形之本。",
        "from": "T50n2059_014:0418b18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東國後見聞之益。",
        "from": "T50n2059_014:0418b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雲龍表於夜明。",
        "from": "T50n2059_014:0418b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風虎彰乎宵夢。",
        "from": "T50n2059_014:0418b19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鴻風既扇。",
        "from": "T50n2059_014:0418b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大化斯融。",
        "from": "T50n2059_014:0418b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自爾西域名僧往往而至。",
        "from": "T50n2059_014:0418b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或傳度經法。",
        "from": "T50n2059_014:0418b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或教授禪道。",
        "from": "T50n2059_014:0418b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或以異迹化人。",
        "from": "T50n2059_014:0418b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或以神力救物。",
        "from": "T50n2059_014:0418b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自漢之梁。",
        "from": "T50n2059_014:0418b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "紀曆彌遠。",
        "from": "T50n2059_014:0418b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世涉六代。",
        "from": "T50n2059_014:0418b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年將五百。",
        "from": "T50n2059_014:0418b23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此土桑門含章秀起。",
        "from": "T50n2059_014:0418b23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "群英間出迭有其人。",
        "from": "T50n2059_014:0418b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾家記錄敘載各異。",
        "from": "T50n2059_014:0418b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙門法濟偏敘高逸一迹。",
        "from": "T50n2059_014:0418b25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙門法安但列志節一行。",
        "from": "T50n2059_014:0418b25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙門僧寶止命遊方一科。",
        "from": "T50n2059_014:0418b26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙門法進迺通撰傳論。",
        "from": "T50n2059_014:0418b26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而辭事闕略。",
        "from": "T50n2059_014:0418b27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並皆互有繁簡出沒成異。",
        "from": "T50n2059_014:0418b27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "考之行事未見其歸宋。",
        "from": "T50n2059_014:0418b28_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨川康王義慶宣驗記及幽明錄。",
        "from": "T50n2059_014:0418b28_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大原王琰冥祥記。",
        "from": "T50n2059_014:0418b29_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彭城劉俊益部寺記。",
        "from": "T50n2059_014:0418c01_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙門曇宗京師寺記。",
        "from": "T50n2059_014:0418c01_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "太原王延秀感應傳。",
        "from": "T50n2059_014:0418c02_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朱君台徵應傳。",
        "from": "T50n2059_014:0418c02_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陶淵明搜神錄。",
        "from": "T50n2059_014:0418c02_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並傍出諸僧敘其風素。",
        "from": "T50n2059_014:0418c03_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而皆是附見。",
        "from": "T50n2059_014:0418c03_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亟多疎闕齊竟陵文宣王三寶記傳。",
        "from": "T50n2059_014:0418c04_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或稱佛史。",
        "from": "T50n2059_014:0418c04_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或號僧錄。",
        "from": "T50n2059_014:0418c05_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既三寶共敘。",
        "from": "T50n2059_014:0418c05_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辭旨相關。",
        "from": "T50n2059_014:0418c05_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "混濫難求。",
        "from": "T50n2059_014:0418c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更為蕪昧。",
        "from": "T50n2059_014:0418c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瑯瑘王巾所撰僧史意似該綜。",
        "from": "T50n2059_014:0418c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而文體未足。",
        "from": "T50n2059_014:0418c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙門僧祐撰三藏記。",
        "from": "T50n2059_014:0418c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "止有三十餘僧。",
        "from": "T50n2059_014:0418c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所無甚眾。",
        "from": "T50n2059_014:0418c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中書郎郄景興東山僧傳。",
        "from": "T50n2059_014:0418c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "治中張孝秀廬山僧傳。",
        "from": "T50n2059_014:0418c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中書陸明霞沙門傳。",
        "from": "T50n2059_014:0418c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各競舉一方不通今古。",
        "from": "T50n2059_014:0418c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "務存一善不及餘行。",
        "from": "T50n2059_014:0418c10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逮乎即時。",
        "from": "T50n2059_014:0418c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦繼有作者。",
        "from": "T50n2059_014:0418c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然或褒贊之下。",
        "from": "T50n2059_014:0418c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過相揄揚。",
        "from": "T50n2059_014:0418c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或敘事之中。",
        "from": "T50n2059_014:0418c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空列辭費。",
        "from": "T50n2059_014:0418c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求之實理無的可稱。",
        "from": "T50n2059_014:0418c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或復嫌以繁廣刪減其事。",
        "from": "T50n2059_014:0418c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而抗迹之奇。",
        "from": "T50n2059_014:0418c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多所遺削。",
        "from": "T50n2059_014:0418c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂出家之士。",
        "from": "T50n2059_014:0418c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處國賓王。",
        "from": "T50n2059_014:0418c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應勵然自遠高蹈獨絕尋辭榮棄愛本以異俗為賢。",
        "from": "T50n2059_014:0418c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若此而不論竟何所紀。",
        "from": "T50n2059_014:0418c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嘗以暇日遇覽群作。",
        "from": "T50n2059_014:0418c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輒搜撿雜錄數十餘家。",
        "from": "T50n2059_014:0418c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及晉宋齊梁春秋書史。",
        "from": "T50n2059_014:0418c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "秦趙燕涼荒朝偽曆。",
        "from": "T50n2059_014:0418c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地理雜篇。",
        "from": "T50n2059_014:0418c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孤文片記。",
        "from": "T50n2059_014:0418c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并博諮古老。",
        "from": "T50n2059_014:0418c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣訪先達。",
        "from": "T50n2059_014:0418c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "校其有無。",
        "from": "T50n2059_014:0418c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取其同異。",
        "from": "T50n2059_014:0418c20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始于漢明帝永平十年。",
        "from": "T50n2059_014:0418c21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終至梁天監十八年。",
        "from": "T50n2059_014:0418c21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡四百五十三載。",
        "from": "T50n2059_014:0418c22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二百五十七人。",
        "from": "T50n2059_014:0418c22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又傍出附見者二百餘人。",
        "from": "T50n2059_014:0418c22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開其德業大為十例。",
        "from": "T50n2059_014:0418c23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一曰譯經。",
        "from": "T50n2059_014:0418c24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二曰義解。",
        "from": "T50n2059_014:0418c24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三曰神異。",
        "from": "T50n2059_014:0418c24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四曰習禪。",
        "from": "T50n2059_014:0418c24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五曰明律。",
        "from": "T50n2059_014:0418c25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六曰遺身。",
        "from": "T50n2059_014:0418c25_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七曰誦經。",
        "from": "T50n2059_014:0418c25_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八曰興福。",
        "from": "T50n2059_014:0418c25_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九曰經師。",
        "from": "T50n2059_014:0418c26_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十曰唱導。",
        "from": "T50n2059_014:0418c26_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然法流東土。",
        "from": "T50n2059_014:0418c26_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓋由傳譯之勳。",
        "from": "T50n2059_014:0418c26_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或踰越沙險。",
        "from": "T50n2059_014:0419a01_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或泛漾洪波。",
        "from": "T50n2059_014:0419a01_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆忘形殉道。",
        "from": "T50n2059_014:0419a01_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "委命弘法。",
        "from": "T50n2059_014:0419a02_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "震旦開明一焉是賴。",
        "from": "T50n2059_014:0419a02_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "茲德可崇。",
        "from": "T50n2059_014:0419a03_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故列之篇首。",
        "from": "T50n2059_014:0419a03_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至若慧解開神。",
        "from": "T50n2059_014:0419a03_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則道兼萬億。",
        "from": "T50n2059_014:0419a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通感適化。",
        "from": "T50n2059_014:0419a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則彊暴以綏。",
        "from": "T50n2059_014:0419a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "靖念安禪則功德森茂。",
        "from": "T50n2059_014:0419a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弘贊毘尼。",
        "from": "T50n2059_014:0419a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則禁行清潔。",
        "from": "T50n2059_014:0419a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忘形遺體。",
        "from": "T50n2059_014:0419a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則矜吝革心。",
        "from": "T50n2059_014:0419a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歌誦法言。",
        "from": "T50n2059_014:0419a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則幽顯含慶。",
        "from": "T50n2059_014:0419a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樹興福善。",
        "from": "T50n2059_014:0419a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則遺像可傳。",
        "from": "T50n2059_014:0419a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡此八科。",
        "from": "T50n2059_014:0419a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並以軌迹不同。",
        "from": "T50n2059_014:0419a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化洽殊異。",
        "from": "T50n2059_014:0419a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而皆德効四依。",
        "from": "T50n2059_014:0419a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功在三業。",
        "from": "T50n2059_014:0419a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故為群經之所稱美。",
        "from": "T50n2059_014:0419a09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾聖之所褒述。",
        "from": "T50n2059_014:0419a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及夫討覈源流商搉取捨。",
        "from": "T50n2059_014:0419a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆列諸贊論。",
        "from": "T50n2059_014:0419a10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "備之後文。",
        "from": "T50n2059_014:0419a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而論所著辭微異恒體。",
        "from": "T50n2059_014:0419a11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始標大意類猶前序。",
        "from": "T50n2059_014:0419a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未辯時人事同後議。",
        "from": "T50n2059_014:0419a12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若間施前後。",
        "from": "T50n2059_014:0419a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如謂煩雜。",
        "from": "T50n2059_014:0419a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故總布一科之末。",
        "from": "T50n2059_014:0419a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通稱為論。",
        "from": "T50n2059_014:0419a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其轉讀宣唱。",
        "from": "T50n2059_014:0419a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖源出非遠。",
        "from": "T50n2059_014:0419a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然而應機悟俗。",
        "from": "T50n2059_014:0419a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實有偏功。",
        "from": "T50n2059_014:0419a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故齊宋雜記。",
        "from": "T50n2059_014:0419a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸條列秀者。",
        "from": "T50n2059_014:0419a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今之所取。",
        "from": "T50n2059_014:0419a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必其製用超絕。",
        "from": "T50n2059_014:0419a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及有一分通感。",
        "from": "T50n2059_014:0419a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迺編之傳末。",
        "from": "T50n2059_014:0419a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如或異者非所存焉。",
        "from": "T50n2059_014:0419a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡十科所敘。",
        "from": "T50n2059_014:0419a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆散在眾記。",
        "from": "T50n2059_014:0419a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今止刪聚一處。",
        "from": "T50n2059_014:0419a18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故述而無作。",
        "from": "T50n2059_014:0419a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俾夫披覽於一本之內可兼諸要。",
        "from": "T50n2059_014:0419a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其有繁辭虛贊或德不及稱者。",
        "from": "T50n2059_014:0419a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一皆省略。",
        "from": "T50n2059_014:0419a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故述六代賢異。",
        "from": "T50n2059_014:0419a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "止為十三卷。",
        "from": "T50n2059_014:0419a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并序錄合十四軸。",
        "from": "T50n2059_014:0419a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號曰高僧傳。",
        "from": "T50n2059_014:0419a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自前代所撰多曰名僧。",
        "from": "T50n2059_014:0419a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然名者本實之賓也。",
        "from": "T50n2059_014:0419a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若實行潛光則高而不名。",
        "from": "T50n2059_014:0419a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寡德適時。",
        "from": "T50n2059_014:0419a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則名而不高。",
        "from": "T50n2059_014:0419a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名而不高本非所紀。",
        "from": "T50n2059_014:0419a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高而不名則備今錄。",
        "from": "T50n2059_014:0419a25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故省名音代以高字。",
        "from": "T50n2059_014:0419a25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其間草創或有遺逸。",
        "from": "T50n2059_014:0419a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此一十四卷。",
        "from": "T50n2059_014:0419a26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "備贊論者意以為定。",
        "from": "T50n2059_014:0419a27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如未隱括覽者詳焉。",
        "from": "T50n2059_014:0419a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高僧傳第一卷(譯經上十五人)。",
        "from": "T50n2059_014:0419a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漢雒陽白馬寺攝摩騰",
        "from": "T50n2059_014:0419b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漢雒陽白馬寺竺法蘭",
        "from": "T50n2059_014:0419b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漢雒陽安清",
        "from": "T50n2059_014:0419b03_0",
        "context_with_next_sentence": true
