        "from": "T50n2059_014:0419b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "◎弟子孤子曼頴。",
        "from": "T50n2059_014:0422b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頓首和南。",
        "from": "T50n2059_014:0422b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日蒙示所撰高僧傳。",
        "from": "T50n2059_014:0422b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並使其掎摭力尋始竟。",
        "from": "T50n2059_014:0422b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但見偉才。",
        "from": "T50n2059_014:0422b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "紙弊墨渝迄未能罷。",
        "from": "T50n2059_014:0422b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若迺至法既被名德已興。",
        "from": "T50n2059_014:0422b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年幾五百時經六代。",
        "from": "T50n2059_014:0422b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自摩騰法蘭發軫西域。",
        "from": "T50n2059_014:0422b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安侯支讖荷錫東都。",
        "from": "T50n2059_014:0422b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖跡標出沒行實深淺。",
        "from": "T50n2059_014:0422b25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸作舟梁大為利益。",
        "from": "T50n2059_014:0422b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "固宜緇素傳美鉛槧定辭照示後昆揄揚往秀。",
        "from": "T50n2059_014:0422b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而道安羅什間表秦書。",
        "from": "T50n2059_014:0422b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛澄道進雜聞趙刪。",
        "from": "T50n2059_014:0422b28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晉史見捨恨局當時。",
        "from": "T50n2059_014:0422b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宋典所存頗因其會。",
        "from": "T50n2059_014:0422b29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼且攙出君台之記。",
        "from": "T50n2059_014:0422b29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "糅在元亮之說。",
        "from": "T50n2059_014:0422c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "感應或所商搉。",
        "from": "T50n2059_014:0422c01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幽明不無梗概泛顯傍文未足光闡。",
        "from": "T50n2059_014:0422c01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "間有諸傳又非隱括。",
        "from": "T50n2059_014:0422c02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "景興偶採居山之人。",
        "from": "T50n2059_014:0422c03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧寶偏綴遊方之士。",
        "from": "T50n2059_014:0422c03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法濟唯張高逸之例。",
        "from": "T50n2059_014:0422c04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法安止命志節之科。",
        "from": "T50n2059_014:0422c04_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "康泓專紀單開。",
        "from": "T50n2059_014:0422c05_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王秀但稱高座。",
        "from": "T50n2059_014:0422c05_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧瑜卓爾獨載。",
        "from": "T50n2059_014:0422c05_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玄暢超然孤錄。",
        "from": "T50n2059_014:0422c06_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯釋法進所造。",
        "from": "T50n2059_014:0422c06_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王巾有著意。",
        "from": "T50n2059_014:0422c06_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "存該綜可擅一家。",
        "from": "T50n2059_014:0422c07_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然進名博而未廣。",
        "from": "T50n2059_014:0422c07_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "巾體立而不就。",
        "from": "T50n2059_014:0422c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梁來作者亦有病諸。",
        "from": "T50n2059_014:0422c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧祐成蘭既同法濟之責。",
        "from": "T50n2059_014:0422c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孝秀染毫復獲景興之誚。",
        "from": "T50n2059_014:0422c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其唱公纂集最實近之。",
        "from": "T50n2059_014:0422c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求其鄙意更恨煩冗。",
        "from": "T50n2059_014:0422c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法師此製始所謂不刊之鴻筆也。",
        "from": "T50n2059_014:0422c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "綿亘古今包括內外。",
        "from": "T50n2059_014:0422c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屬辭比事不文不質。",
        "from": "T50n2059_014:0422c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂繁難省云約豈加。",
        "from": "T50n2059_014:0422c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以高為名既使弗逮者恥。",
        "from": "T50n2059_014:0422c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開例成廣足使有善者勸。",
        "from": "T50n2059_014:0422c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向之二三諸子前後撰述。",
        "from": "T50n2059_014:0422c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈得挈長量短同年共日而語之哉。",
        "from": "T50n2059_014:0422c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信門徒竟無一言可豫。",
        "from": "T50n2059_014:0422c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "市肆空設千金之賞。",
        "from": "T50n2059_014:0422c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方入簉龍函上登麟閣。",
        "from": "T50n2059_014:0422c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出內瓊笈卷舒王笥弟子雖實不敏少嘗好學。",
        "from": "T50n2059_014:0422c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頃日尫餘觸途多昧。",
        "from": "T50n2059_014:0422c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且獲披來帙斯文在斯。",
        "from": "T50n2059_014:0422c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鑽仰弗暇討論何所。",
        "from": "T50n2059_014:0422c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誠非子通見元則之論。",
        "from": "T50n2059_014:0422c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "良愧處道知休奕之書。",
        "from": "T50n2059_014:0422c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徒深謝安慕竺曠風流殷浩憚支遁才俊耳。",
        "from": "T50n2059_014:0422c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不見旬日窮情已勞。",
        "from": "T50n2059_014:0422c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "扶力此白以伐訴盡。",
        "from": "T50n2059_014:0422c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弟子孤子王曼頴頓首和南。",
        "from": "T50n2059_014:0422c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "君白一日以所撰高僧傳相簡。",
        "from": "T50n2059_014:0422c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "意存箴艾。",
        "from": "T50n2059_014:0422c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而來告累紙。",
        "from": "T50n2059_014:0422c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更加拂拭。",
        "from": "T50n2059_014:0422c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顧惟道藉人弘理由教顯。",
        "from": "T50n2059_014:0422c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而弘道釋教莫尚高僧。",
        "from": "T50n2059_014:0422c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故漸染以來昭明遺法。",
        "from": "T50n2059_014:0422c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殊功異行列代而興。",
        "from": "T50n2059_014:0422c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敦厲後生理宜綜綴。",
        "from": "T50n2059_014:0422c27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貧道少乏懷書抱筴自課之勤長慕鉛墨塗青揚善之美。",
        "from": "T50n2059_014:0422c28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故於聽覽餘閒厝心傳錄。",
        "from": "T50n2059_014:0422c29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每見一分可稱。",
        "from": "T50n2059_014:0423a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輒有懷三省。",
        "from": "T50n2059_014:0423a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但歷尋眾記繁約。",
        "from": "T50n2059_014:0423a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或編不同列參差。",
        "from": "T50n2059_014:0423a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或行事出沒。",
        "from": "T50n2059_014:0423a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已詳別序兼具來告。",
        "from": "T50n2059_014:0423a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以不量寸管。",
        "from": "T50n2059_014:0423a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輒樹十科。",
        "from": "T50n2059_014:0423a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "商搉條流。",
        "from": "T50n2059_014:0423a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "意言略舉而筆路蒼茫。",
        "from": "T50n2059_014:0423a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辭語陋拙。",
        "from": "T50n2059_014:0423a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本以自備疎遺。",
        "from": "T50n2059_014:0423a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈宜濫入高聽檀越既學兼孔釋解貫玄儒。",
        "from": "T50n2059_014:0423a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "抽入綴藻內外淹劭。",
        "from": "T50n2059_014:0423a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "披覽餘暇脫助詳閱。",
        "from": "T50n2059_014:0423a06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故忘鄙俚用簡龍門。",
        "from": "T50n2059_014:0423a07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然事高辭野久懷多愧。",
        "from": "T50n2059_014:0423a07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來告吹噓更增[怡-台+典][怡-台+墨]。",
        "from": "T50n2059_014:0423a08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今以所著贊論十科。",
        "from": "T50n2059_014:0423a08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重以相簡。",
        "from": "T50n2059_014:0423a09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如有紕謬請備斟酌。",
        "from": "T50n2059_014:0423a09_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋君白。",
        "from": "T50n2059_014:0423a09_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此傳是會稽嘉祥寺慧皎法師所撰。",
        "from": "T50n2059_014:0423a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法師學通內外善講經律。",
        "from": "T50n2059_014:0423a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著涅槃疏十卷梵網戒等義疏。",
        "from": "T50n2059_014:0423a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並為世軌。",
        "from": "T50n2059_014:0423a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又著此高僧傳十三卷。",
        "from": "T50n2059_014:0423a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梁末承聖二年太歲癸酉避侯景難來至湓城。",
        "from": "T50n2059_014:0423a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少時講說。",
        "from": "T50n2059_014:0423a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甲戌年二月捨化。",
        "from": "T50n2059_014:0423a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時年五十有八江州僧正慧恭經始葬",
        "from": "T50n2059_014:0423a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廬山禪閣寺墓。",
        "from": "T50n2059_014:0423a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍光寺僧果同避難在山。",
        "from": "T50n2059_014:0423a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遇見時事聊記之云爾。",
        "from": "T50n2059_014:0423a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高僧傳序錄卷第十四",
        "from": "T50n2059_014:0423a19_0",
        "context_with_next_sentence": false
    }
]