    },
    {
        "source_sentence": "祿食小豆。",
        "from": "T21n1307_001:0425c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有厄宜供養此經及帶本星符。",
        "from": "T21n1307_001:0425c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大吉。",
        "from": "T21n1307_001:0425c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　ホ",
        "from": "T21n1307_001:0425c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辰生申生人。",
        "from": "T21n1307_001:0425c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同向此星下生。",
        "from": "T21n1307_001:0425c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祿食麻子。",
        "from": "T21n1307_001:0425c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有厄宜供養此經及帶本星符。",
        "from": "T21n1307_001:0425c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大吉。",
        "from": "T21n1307_001:0425c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　ヘト",
        "from": "T21n1307_001:0425c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "巳生未生人。",
        "from": "T21n1307_001:0425c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同向此星下生。",
        "from": "T21n1307_001:0425c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祿食大豆。",
        "from": "T21n1307_001:0425c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有厄宜供養此經及帶本星符。",
        "from": "T21n1307_001:0425c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大吉。",
        "from": "T21n1307_001:0425c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　チ",
        "from": "T21n1307_001:0425c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "午生人。",
        "from": "T21n1307_001:0425c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向此星下生。",
        "from": "T21n1307_001:0425c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祿食小豆。",
        "from": "T21n1307_001:0425c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有厄宜供養此經及帶本星符。",
        "from": "T21n1307_001:0425c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大吉。",
        "from": "T21n1307_001:0425c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無貪狼星。",
        "from": "T21n1307_001:0426a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是東方最勝世界運意通證如來佛。",
        "from": "T21n1307_001:0426a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無巨門星。",
        "from": "T21n1307_001:0426a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是東方妙寶世界光音自在如來佛。",
        "from": "T21n1307_001:0426a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無祿存星。",
        "from": "T21n1307_001:0426a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是東方圓滿世界金色成就如來佛。",
        "from": "T21n1307_001:0426a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無文曲星。",
        "from": "T21n1307_001:0426a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是東方無憂世界最勝吉祥如來佛。",
        "from": "T21n1307_001:0426a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無廉貞星。",
        "from": "T21n1307_001:0426a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是東方淨住世界廣達智辨如來佛。",
        "from": "T21n1307_001:0426a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無武曲星。",
        "from": "T21n1307_001:0426a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是東方法意世界法海遊戲如來佛。",
        "from": "T21n1307_001:0426a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無破軍星。",
        "from": "T21n1307_001:0426a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是東方琉璃世界藥師琉璃光如來佛。",
        "from": "T21n1307_001:0426a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遇行年災厄禮此經七拜。",
        "from": "T21n1307_001:0426a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛告文殊師利菩薩。",
        "from": "T21n1307_001:0426a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所說此經。",
        "from": "T21n1307_001:0426a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大威神有大威力。",
        "from": "T21n1307_001:0426a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能救一切眾生重罪能滅一切業障。",
        "from": "T21n1307_001:0426a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有比丘僧比丘。",
        "from": "T21n1307_001:0426a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宰官居士善男子善女人。",
        "from": "T21n1307_001:0426a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若貴若賤大小生命。",
        "from": "T21n1307_001:0426a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆屬北斗七星所管。",
        "from": "T21n1307_001:0426a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若聞此經受持供養轉讀。",
        "from": "T21n1307_001:0426a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勸於朋友親族骨肉受持者現世獲福後世得生天上。",
        "from": "T21n1307_001:0426a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人。",
        "from": "T21n1307_001:0426a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或先亡過者墮於地獄。",
        "from": "T21n1307_001:0426a22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或受種種苦楚。",
        "from": "T21n1307_001:0426a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若聞此經信敬供養。",
        "from": "T21n1307_001:0426a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得先亡離於地獄。",
        "from": "T21n1307_001:0426a23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生於極樂世界　若有善男子善女人。",
        "from": "T21n1307_001:0426a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或被鬼魅所侵邪魔所嬈。",
        "from": "T21n1307_001:0426a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡夢怪異魂魄驚恐。",
        "from": "T21n1307_001:0426a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若聞此經受持供養。",
        "from": "T21n1307_001:0426a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得魂魄安寧。",
        "from": "T21n1307_001:0426a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永無恐怖　若善男子善女人。",
        "from": "T21n1307_001:0426a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有仕官身役善發征行。",
        "from": "T21n1307_001:0426a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遇此經信敬供養。",
        "from": "T21n1307_001:0426a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得仕官高遷甚大吉　若善男子善女人。",
        "from": "T21n1307_001:0426a28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或是疾病纏身。",
        "from": "T21n1307_001:0426a29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲求輕差。",
        "from": "T21n1307_001:0426b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當於淨室燒香供養此經。",
        "from": "T21n1307_001:0426b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疾病痊瘥　若善男子善女人。",
        "from": "T21n1307_001:0426b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲得進達及以出行經紀。",
        "from": "T21n1307_001:0426b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求財稱遂興生貨賣。",
        "from": "T21n1307_001:0426b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遇此經信敬供養。",
        "from": "T21n1307_001:0426b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得求財稱遂出入大吉。",
        "from": "T21n1307_001:0426b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人。",
        "from": "T21n1307_001:0426b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或養蠶虛耗六畜不安。",
        "from": "T21n1307_001:0426b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於淨室燒香供養此經。",
        "from": "T21n1307_001:0426b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得田蠶遂意六畜孳榮。",
        "from": "T21n1307_001:0426b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永無損失亦無災障　若有女人懷胎難月。",
        "from": "T21n1307_001:0426b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遇此經信敬供養。",
        "from": "T21n1307_001:0426b07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即一得母子分解厄難消除。",
        "from": "T21n1307_001:0426b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所生兒女。",
        "from": "T21n1307_001:0426b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆得端正長命果報　若善男子善女人。",
        "from": "T21n1307_001:0426b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須知北斗七星管人生命。",
        "from": "T21n1307_001:0426b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一生之中所有災厄。",
        "from": "T21n1307_001:0426b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "官事口舌釜鳴百怪。",
        "from": "T21n1307_001:0426b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遇此經信敬供養。",
        "from": "T21n1307_001:0426b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一無妨害　爾時文殊師利言善男子善女人恭敬信受作禮而散。",
        "from": "T21n1307_001:0426b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說北斗七星延命經",
        "from": "T21n1307_001:0426b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(薄草決十七本命下。",
        "from": "T21n1307_001:0426b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "引用此經。",
        "from": "T21n1307_001:0426b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今年幸得栂尾山法鼓臺傳習之本於洛北福勝密寺。",
        "from": "T21n1307_001:0426b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施國訓鑄版。",
        "from": "T21n1307_001:0426b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "于時享和二歲次壬戌紀號仲夏月沙門快道記豐山小池坊輪下總持院　　一挍加筆訖慈順)。",
        "from": "T21n1307_001:0426b15_4",
        "context_with_next_sentence": false
    }
]