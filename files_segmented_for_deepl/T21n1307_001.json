    },
    {
        "source_sentence": "　ハ",
        "from": "T21n1307_001:0425c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寅生戌生人。",
        "from": "T21n1307_001:0425c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同向此星下生。",
        "from": "T21n1307_001:0425c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祿食粳米。",
        "from": "T21n1307_001:0425c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有厄宜供養此經及帶本星符。",
        "from": "T21n1307_001:0425c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大吉。",
        "from": "T21n1307_001:0425c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　ニ",
        "from": "T21n1307_001:0425c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卯生酉生人。",
        "from": "T21n1307_001:0425c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同向此星下生。",
        "from": "T21n1307_001:0425c17_1",
