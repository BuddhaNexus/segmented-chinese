[
    {
        "source_sentence": "佛說北斗七星延命經",
        "from": "T21n1307_001:0425b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婆羅門僧將到此經唐朝受持",
        "from": "T21n1307_001:0425b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　イ",
        "from": "T21n1307_001:0425c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "子生人。",
        "from": "T21n1307_001:0425c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向此星下生。",
        "from": "T21n1307_001:0425c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祿食黍。",
        "from": "T21n1307_001:0425c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有厄宜供養此經及帶本星符。",
        "from": "T21n1307_001:0425c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大吉。",
        "from": "T21n1307_001:0425c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　ロ",
        "from": "T21n1307_001:0425c08_0",
