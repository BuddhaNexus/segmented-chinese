    },
    {
        "source_sentence": "丑生亥生人。",
        "from": "T21n1307_001:0425c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同向此星下生。",
        "from": "T21n1307_001:0425c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祿食粟。",
        "from": "T21n1307_001:0425c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有厄宜供養此經及帶本星符。",
        "from": "T21n1307_001:0425c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大吉。",
        "from": "T21n1307_001:0425c10_4",
