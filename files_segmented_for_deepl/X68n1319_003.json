[
    {
        "source_sentence": "御製序",
        "from": "X68n1319_003:0526c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寒山詩三百餘首。",
        "from": "X68n1319_003:0526c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拾得詩五十餘首。",
        "from": "X68n1319_003:0526c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐閭邱太守寫自寒巖。",
        "from": "X68n1319_003:0526c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流傳閻浮提界。",
        "from": "X68n1319_003:0526c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讀者或以為俗語。",
        "from": "X68n1319_003:0526c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或以為韻語。",
        "from": "X68n1319_003:0526c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或以為教語。",
        "from": "X68n1319_003:0526c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或以為禪語。",
        "from": "X68n1319_003:0526c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如摩尼珠。",
        "from": "X68n1319_003:0526c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體非一色。",
        "from": "X68n1319_003:0526c24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處處皆圓。",
        "from": "X68n1319_003:0526c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨人目之所見。",
        "from": "X68n1319_003:0527a01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朕以為非俗非韻。",
        "from": "X68n1319_003:0527a01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非教非禪。",
        "from": "X68n1319_003:0527a01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真乃古佛直心直語也。",
        "from": "X68n1319_003:0527a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永明云。",
        "from": "X68n1319_003:0527a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修習空花萬行。",
        "from": "X68n1319_003:0527a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宴坐水月道場。",
        "from": "X68n1319_003:0527a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降伏鏡裏魔軍。",
        "from": "X68n1319_003:0527a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大作夢中佛事。",
        "from": "X68n1319_003:0527a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如二大士者。",
        "from": "X68n1319_003:0527a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其庶幾乎正信調直。",
        "from": "X68n1319_003:0527a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不離和尚因緣。",
        "from": "X68n1319_003:0527a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓滿光華。",
        "from": "X68n1319_003:0527a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周遍大千世界。",
        "from": "X68n1319_003:0527a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不萌枝上。",
        "from": "X68n1319_003:0527a05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金鳳翱翔。",
        "from": "X68n1319_003:0527a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無影樹邊。",
        "from": "X68n1319_003:0527a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玉象圍繞。",
        "from": "X68n1319_003:0527a05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性空行實。",
        "from": "X68n1319_003:0527a06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性實行空。",
        "from": "X68n1319_003:0527a06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妄有真無。",
        "from": "X68n1319_003:0527a06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妄無真有。",
        "from": "X68n1319_003:0527a06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有空無實。",
        "from": "X68n1319_003:0527a06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念念不留。",
        "from": "X68n1319_003:0527a07_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有實無空。",
        "from": "X68n1319_003:0527a07_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如如不動。",
        "from": "X68n1319_003:0527a07_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是以直心直語。",
        "from": "X68n1319_003:0527a07_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是如是。",
        "from": "X68n1319_003:0527a07_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "學者狐疑淨盡。",
        "from": "X68n1319_003:0527a08_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓證真如。",
        "from": "X68n1319_003:0527a08_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能有無一體。",
        "from": "X68n1319_003:0527a08_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性行一貫。",
        "from": "X68n1319_003:0527a08_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃可與讀二大士之詩。",
        "from": "X68n1319_003:0527a09_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "否則隨文生解。",
        "from": "X68n1319_003:0527a09_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總無交涉也。",
        "from": "X68n1319_003:0527a09_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刪而錄之。",
        "from": "X68n1319_003:0527a10_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以貽後世。",
        "from": "X68n1319_003:0527a10_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寒山子云。",
        "from": "X68n1319_003:0527a10_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有子期。",
        "from": "X68n1319_003:0527a10_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辨此音。",
        "from": "X68n1319_003:0527a10_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為序。",
        "from": "X68n1319_003:0527a11_37",
        "context_with_next_sentence": false
    }
]