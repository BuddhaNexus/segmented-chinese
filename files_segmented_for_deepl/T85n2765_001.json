[
    {
        "source_sentence": "涅槃經疏",
        "from": "T85n2765_001:0304a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得理踰明除身口意十惡名觸。",
        "from": "T85n2765_001:0304a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三業淨得正命者。",
        "from": "T85n2765_001:0304a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除十惡□命相續名得正命觸也。",
        "from": "T85n2765_001:0304a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受名攝取者。",
        "from": "T85n2765_001:0304a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生受時取境苦□有無名受。",
        "from": "T85n2765_001:0304a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二地智慧踰明領納二地下功德故名受為攝取也。",
        "from": "T85n2765_001:0304a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受故起煩惱。",
        "from": "T85n2765_001:0304a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十七品能懷三惡道報。",
        "from": "T85n2765_001:0304a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故以受為攝取。",
        "from": "T85n2765_001:0304a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此就所除制名也。",
        "from": "T85n2765_001:0304a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思惟是三地心。",
        "from": "T85n2765_001:0304a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷見諦麁細惑盡。",
        "from": "T85n2765_001:0304a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解心勝前二地名增得。",
        "from": "T85n2765_001:0304a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是三十七品破煩惱者。",
        "from": "T85n2765_001:0304a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是四地心。",
        "from": "T85n2765_001:0304a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能破修道中上品煩惱。",
        "from": "T85n2765_001:0304a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要藉正念主領功德□念為主也。",
        "from": "T85n2765_001:0304a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如世間中已下以喻來解也。",
        "from": "T85n2765_001:0304a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既入定者五地心。",
        "from": "T85n2765_001:0304a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修三十七品能善分別法相引生後慧故。",
        "from": "T85n2765_001:0304a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以主為導也。",
        "from": "T85n2765_001:0304a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是三十七品者。",
        "from": "T85n2765_001:0304a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六地緣空般若。",
        "from": "T85n2765_001:0304a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七地便般若能分別法相。",
        "from": "T85n2765_001:0304a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智解最勝。",
        "from": "T85n2765_001:0304a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以慧為勝也。",
        "from": "T85n2765_001:0304a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若壞煩惱去三種□□功德。",
        "from": "T85n2765_001:0304a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八地九地十地。",
        "from": "T85n2765_001:0304a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此三地勝解乃名為實。",
        "from": "T85n2765_001:0304a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是三十七品雖得四沙門及以解脫者。",
        "from": "T85n2765_001:0304a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須陀洹乃至羅漢。",
        "from": "T85n2765_001:0304a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此四果猶不得名畢竟。",
        "from": "T85n2765_001:0304a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷除三十七品所行之事。",
        "from": "T85n2765_001:0304a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心行處滅名大涅槃。",
        "from": "T85n2765_001:0304a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次已下第四就因果相發生解。",
        "from": "T85n2765_001:0304a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛念心是欲者。",
        "from": "T85n2765_001:0304b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念欲得菩提念欲生二地名欲也。",
        "from": "T85n2765_001:0304b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因下品愛念為善發生上品□為觸。",
        "from": "T85n2765_001:0304b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為因也。",
        "from": "T85n2765_001:0304b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前二心發二地名受為攝取。",
        "from": "T85n2765_001:0304b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因近善友者。",
        "from": "T85n2765_001:0304b04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因前二地□惟常理解理踰明名增也。",
        "from": "T85n2765_001:0304b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因是四法者。",
        "from": "T85n2765_001:0304b05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藉因前根因攝增四法能生長四地無漏名念定五地心智是六地七地則名主勝也。",
        "from": "T85n2765_001:0304b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因是三法者。",
        "from": "T85n2765_001:0304b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因主□智發後三地名二解脫。",
        "from": "T85n2765_001:0304b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "癡能障慧愛能縛心。",
        "from": "T85n2765_001:0304b08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷愛故心得脫。",
        "from": "T85n2765_001:0304b09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除癡故慧得明。",
        "from": "T85n2765_001:0304b09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心慧二解脫是名為實。",
        "from": "T85n2765_001:0304b09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是八法空解相續畢竟得果名大涅槃。",
        "from": "T85n2765_001:0304b10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲者是發心已下。",
        "from": "T85n2765_001:0304b11_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五就出家法中深淺解。",
        "from": "T85n2765_001:0304b11_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初地下品心是初無漏名初發出家也。",
        "from": "T85n2765_001:0304b12_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觸是白四羯磨者。",
        "from": "T85n2765_001:0304b13_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觸是上心品如似出家後白四羯磨名二地心名。",
        "from": "T85n2765_001:0304b13_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝如白四羯磨後發二種戒。",
        "from": "T85n2765_001:0304b14_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增者踰勝如因二種戒修得四禪定心也。",
        "from": "T85n2765_001:0304b15_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "主轉強如似定後發慧斷見諦結盡。",
        "from": "T85n2765_001:0304b16_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修道中六品結盡。",
        "from": "T85n2765_001:0304b17_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證須陀洹斯陀二果。",
        "from": "T85n2765_001:0304b17_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "導者斷三微惑盡。",
        "from": "T85n2765_001:0304b18_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如似阿那含也。",
        "from": "T85n2765_001:0304b18_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勝者二種般若是勝名阿羅漢。",
        "from": "T85n2765_001:0304b18_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實者後三地真心法流有無雙照最利名辟支也。",
        "from": "T85n2765_001:0304b19_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畢竟者有遂因義名無上大果也。",
        "from": "T85n2765_001:0304b20_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲名為識者。",
        "from": "T85n2765_001:0304b21_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九法中欲最是其初。",
        "from": "T85n2765_001:0304b21_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如似一報中識最是初故言欲名識也。",
        "from": "T85n2765_001:0304b22_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觸名六入者根本觸對六塵。",
        "from": "T85n2765_001:0304b23_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如似初地上品心觸對二地亦可觸境生解。",
        "from": "T85n2765_001:0304b23_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名觸六入。",
        "from": "T85n2765_001:0304b24_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝名為受者。",
        "from": "T85n2765_001:0304b25_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受本納領苦樂名受攝能領納二地下功德故言攝名為受也。",
        "from": "T85n2765_001:0304b25_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增名無明者。",
        "from": "T85n2765_001:0304b26_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過去無明生惑中強。",
        "from": "T85n2765_001:0304b27_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如似三地信忍之上品有強於前名。",
        "from": "T85n2765_001:0304b27_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增主名名色者。",
        "from": "T85n2765_001:0304b28_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名色相依使不壞散相續受百年一段果報。",
        "from": "T85n2765_001:0304b28_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如似四地下心無漏相續故言主名名色也。",
        "from": "T85n2765_001:0304b29_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "導名為愛者。",
        "from": "T85n2765_001:0304c01_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛本貪染境界。",
        "from": "T85n2765_001:0304c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如似五地心能引生後慧。",
        "from": "T85n2765_001:0304c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故言導名愛也。",
        "from": "T85n2765_001:0304c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勝為取者。",
        "from": "T85n2765_001:0304c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "染取境界名取。",
        "from": "T85n2765_001:0304c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如似六地智慧般若。",
        "from": "T85n2765_001:0304c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七地方便般若。",
        "from": "T85n2765_001:0304c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此之二慧得理踰明故言勝也。",
        "from": "T85n2765_001:0304c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實名有者。",
        "from": "T85n2765_001:0304c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "含果未吐名有。",
        "from": "T85n2765_001:0304c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辯生老中強名有。",
        "from": "T85n2765_001:0304c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲明八地九地十地此三地心是大無生忍。",
        "from": "T85n2765_001:0304c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功德踰明辯大果義強。",
        "from": "T85n2765_001:0304c07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故言實名為有。",
        "from": "T85n2765_001:0304c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畢竟名老死。",
        "from": "T85n2765_001:0304c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "老死本一報之最終。",
        "from": "T85n2765_001:0304c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如似涅槃果是終極之處。",
        "from": "T85n2765_001:0304c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故言畢竟名老死。",
        "from": "T85n2765_001:0304c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為前十二因得如涅槃為前八法所得也。",
        "from": "T85n2765_001:0304c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉言已下第四文略上九法為三法也。",
        "from": "T85n2765_001:0304c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就中亦有六幡明義。",
        "from": "T85n2765_001:0304c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初一直列名。",
        "from": "T85n2765_001:0304c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二根即是作已下轉名解義。",
        "from": "T85n2765_001:0304c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三未來之世已下直就當現二法解。",
        "from": "T85n2765_001:0304c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四根即是求已下正三法名。",
        "from": "T85n2765_001:0304c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五根即見道已下就覺無覺明義。",
        "from": "T85n2765_001:0304c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六根即正因已下就因果相發生解義。",
        "from": "T85n2765_001:0304c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "根本因增云何取異。",
        "from": "T85n2765_001:0304c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子已下答。",
        "from": "T85n2765_001:0304c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初地至七地皆是初發。",
        "from": "T85n2765_001:0304c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因者後三地同在法流位中不為相間故名相似。",
        "from": "T85n2765_001:0304c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增者滅向者三相似得果中真相似未來果報。",
        "from": "T85n2765_001:0304c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以未現受故屬行人名因。",
        "from": "T85n2765_001:0304c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬行備種智現時名增。",
        "from": "T85n2765_001:0304c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下文可解也。",
        "from": "T85n2765_001:0304c22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉言如是涅槃云何可得。",
        "from": "T85n2765_001:0304c22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此問所以來由。",
        "from": "T85n2765_001:0304c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上言性地心中三十七品猶故不取為梵行。",
        "from": "T85n2765_001:0304c23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要初地以上終至於佛空心相續得作梵行時。",
        "from": "T85n2765_001:0304c24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人聞以生其退心。",
        "from": "T85n2765_001:0304c25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初地已上始勘為梵行。",
        "from": "T85n2765_001:0304c26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地下不階。",
        "from": "T85n2765_001:0304c26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今天眾未有地下之因。",
        "from": "T85n2765_001:0304c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地上之行何由可階。",
        "from": "T85n2765_001:0304c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因行既不成。",
        "from": "T85n2765_001:0304c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大涅槃果云何可得也。",
        "from": "T85n2765_001:0304c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子已下答。",
        "from": "T85n2765_001:0304c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四部眾但修十相為因。",
        "from": "T85n2765_001:0304c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次生初地相續終得大涅槃。",
        "from": "T85n2765_001:0304c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝修十相故便得。",
        "from": "T85n2765_001:0305a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何待多行。",
        "from": "T85n2765_001:0305a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是其智遠。",
        "from": "T85n2765_001:0305a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從相生名相也。",
        "from": "T85n2765_001:0305a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十相義就文易解。",
        "from": "T85n2765_001:0305a02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憍陳如品",
        "from": "T85n2765_001:0305a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此品經師解言。",
        "from": "T85n2765_001:0305a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前之二品勸人修道。",
        "from": "T85n2765_001:0305a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既有修道為畢得果。",
        "from": "T85n2765_001:0305a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故憍陳如一品意在流通故興品。",
        "from": "T85n2765_001:0305a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以先命陳如欲明陳如是眾之上坐。",
        "from": "T85n2765_001:0305a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來滅後欲使流通千載不滅。",
        "from": "T85n2765_001:0305a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一意故命。",
        "from": "T85n2765_001:0305a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二來說經已竟十仙悟道緣在陳如。",
        "from": "T85n2765_001:0305a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二意故命也。",
        "from": "T85n2765_001:0305a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三來初始成道在於鹿苑始說四諦傖為陳如。",
        "from": "T85n2765_001:0305a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今日垂終之教在於雙林亦為陳如。",
        "from": "T85n2765_001:0305a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "表明如來有始有終也。",
        "from": "T85n2765_001:0305a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中流通因無常色獲得常色。",
        "from": "T85n2765_001:0305a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既名流通。",
        "from": "T85n2765_001:0305a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與上品明義。",
        "from": "T85n2765_001:0305a13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以名流通也。",
        "from": "T85n2765_001:0305a13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來自言。",
        "from": "T85n2765_001:0305a14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我上來說色是無常。",
        "from": "T85n2765_001:0305a14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝滅無常色獲得常色。",
        "from": "T85n2765_001:0305a14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝於未來流通我此法。",
        "from": "T85n2765_001:0305a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故此品由名流通也。",
        "from": "T85n2765_001:0305a15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色者常果之體。",
        "from": "T85n2765_001:0305a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷除生死□著色盡。",
        "from": "T85n2765_001:0305a16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體明了顯白義同於色。",
        "from": "T85n2765_001:0305a17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來斷下三受永亡。",
        "from": "T85n2765_001:0305a17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體是至極。",
        "from": "T85n2765_001:0305a18_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "領納真功德名受。",
        "from": "T85n2765_001:0305a18_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真照之智達假名相盡名想。",
        "from": "T85n2765_001:0305a18_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者下地聚積生死名之為行。",
        "from": "T85n2765_001:0305a19_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來斷除眾惡萬善聚積名行。",
        "from": "T85n2765_001:0305a20_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下地識了別青黃名識。",
        "from": "T85n2765_001:0305a20_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真智能達實法名識。",
        "from": "T85n2765_001:0305a21_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下地陰者陰蓋。",
        "from": "T85n2765_001:0305a21_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行人恒在生死名陰果。",
        "from": "T85n2765_001:0305a22_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯陰者能陰覆行人使逍然方外故名陰也。",
        "from": "T85n2765_001:0305a22_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人能解上常無常義者是名沙門。",
        "from": "T85n2765_001:0305a23_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真息惡人名婆羅門真淨行人。",
        "from": "T85n2765_001:0305a24_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我佛法中有沙門人。",
        "from": "T85n2765_001:0305a25_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若離佛法外道有沙門法有沙門人無有是處。",
        "from": "T85n2765_001:0305a25_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道有者都是外道也。",
        "from": "T85n2765_001:0305a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故。",
        "from": "T85n2765_001:0305a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "興此文者。",
        "from": "T85n2765_001:0305a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譏切外道欲使各立神我。",
        "from": "T85n2765_001:0305a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與佛論義辯其是非。",
        "from": "T85n2765_001:0305a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因此言論歸其正化故興此文也。",
        "from": "T85n2765_001:0305a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸外道言。",
        "from": "T85n2765_001:0305a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞿曇昔日說苦無常空無我。",
        "from": "T85n2765_001:0305a29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "集我門徒。",
        "from": "T85n2765_001:0305b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由故可忍。",
        "from": "T85n2765_001:0305b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與我解異。",
        "from": "T85n2765_001:0305b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無智從之。",
        "from": "T85n2765_001:0305b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有智歸我。",
        "from": "T85n2765_001:0305b02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故言可忍。",
        "from": "T85n2765_001:0305b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今日在雙林中說常樂我淨。",
        "from": "T85n2765_001:0305b03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有門徒皆歸化盡。",
        "from": "T85n2765_001:0305b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "情不能忍。",
        "from": "T85n2765_001:0305b04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故求論譏。",
        "from": "T85n2765_001:0305b04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾中有婆羅門。",
        "from": "T85n2765_001:0305b04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來上十六天國與諸外道論義。",
        "from": "T85n2765_001:0305b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆從正化盡。",
        "from": "T85n2765_001:0305b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中十仙更別有一段外道。",
        "from": "T85n2765_001:0305b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今始欲來論義。",
        "from": "T85n2765_001:0305b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此十仙中要根機速疾悟機。",
        "from": "T85n2765_001:0305b07_4",
        "context_with_next_sentence": false
    }
]