[
    {
        "source_sentence": "大宗地玄文本論卷第八",
        "from": "T32n1669_008:0681c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬鳴菩薩造",
        "from": "T32n1669_008:0681c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真諦三藏譯",
        "from": "T32n1669_008:0681c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "◎獨地非亂一定一定道路大決擇分第十五如是已說一切虛空一切微塵數量高王大決擇分。",
        "from": "T32n1669_008:0681c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說獨地非亂一定一定。",
        "from": "T32n1669_008:0681c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道路大決擇分。",
        "from": "T32n1669_008:0681c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_008:0681c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_008:0681c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　非亂道路中　　亦有金剛位",
        "from": "T32n1669_008:0681c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一一位各各　　有本存一故",
        "from": "T32n1669_008:0681c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一百二數成　　依位立轉相",
        "from": "T32n1669_008:0681c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　則有五種重　　謂上一本一",
        "from": "T32n1669_008:0681c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　俱轉及不雜　　圓滿具足位",
        "from": "T32n1669_008:0682a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_008:0682a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就獨地非亂一定一定道路分中。",
        "from": "T32n1669_008:0682a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有五十一種真金剛位。",
        "from": "T32n1669_008:0682a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一位位皆悉各各有本存一。",
        "from": "T32n1669_008:0682a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此義故。",
        "from": "T32n1669_008:0682a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一百二數成立而已。",
        "from": "T32n1669_008:0682a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈非亂道路中亦有金剛位一一位各各有本存一故一百二數成故。",
        "from": "T32n1669_008:0682a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依如是位。",
        "from": "T32n1669_008:0682a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建立轉相則有五種。",
        "from": "T32n1669_008:0682a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為五。",
        "from": "T32n1669_008:0682a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者上上一一轉相門。",
        "from": "T32n1669_008:0682a07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者本本一一轉相門。",
        "from": "T32n1669_008:0682a07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者俱行不離轉相門。",
        "from": "T32n1669_008:0682a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者區區不雜轉相門。",
        "from": "T32n1669_008:0682a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者圓滿具足轉相門。",
        "from": "T32n1669_008:0682a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為五。",
        "from": "T32n1669_008:0682a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈依位立轉相則有五種。",
        "from": "T32n1669_008:0682a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重謂上一本一俱轉及不雜圓滿具足位故。",
        "from": "T32n1669_008:0682a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一轉相形相如何。",
        "from": "T32n1669_008:0682a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_008:0682a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　五十一種位　　一各攝五十",
        "from": "T32n1669_008:0682a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一時一處轉　　然不可合一",
        "from": "T32n1669_008:0682a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_008:0682a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名為上一一門。",
        "from": "T32n1669_008:0682a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂五十一種位。",
        "from": "T32n1669_008:0682a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一位位各攝五十一時處轉。",
        "from": "T32n1669_008:0682a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若爾合集應作一體。",
        "from": "T32n1669_008:0682a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各別一轉不可合一。",
        "from": "T32n1669_008:0682a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈五十一種位一各攝五十一時一處轉然不可合一故。",
        "from": "T32n1669_008:0682a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是已說上上一一轉相門。",
        "from": "T32n1669_008:0682a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說本本一一轉相門。",
        "from": "T32n1669_008:0682a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_008:0682a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_008:0682a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　五十一本一　　一各攝五十",
        "from": "T32n1669_008:0682a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一時一處轉　　然不可合一",
        "from": "T32n1669_008:0682a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_008:0682a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名為本一一門。",
        "from": "T32n1669_008:0682a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂五十一種本。",
        "from": "T32n1669_008:0682a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一種本一各攝五十一時處轉。",
        "from": "T32n1669_008:0682a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若爾合集應作一體。",
        "from": "T32n1669_008:0682a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各別一轉不可合一。",
        "from": "T32n1669_008:0682a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈五十一本一一各攝五十一時一處轉然不可合一故。",
        "from": "T32n1669_008:0682a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是已說本本一一轉相門。",
        "from": "T32n1669_008:0682a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說俱行不離轉相門。",
        "from": "T32n1669_008:0682a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_008:0682a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_008:0682a28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　本一及上一　　互各攝諸位",
        "from": "T32n1669_008:0682a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　俱轉不捨離　　然不可合一",
        "from": "T32n1669_008:0682b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_008:0682b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名為俱行轉門。",
        "from": "T32n1669_008:0682b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂本一五十一法。",
        "from": "T32n1669_008:0682b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一各各攝上一五十一法。",
        "from": "T32n1669_008:0682b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上一五十一法。",
        "from": "T32n1669_008:0682b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能攝彼本。",
        "from": "T32n1669_008:0682b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱行俱轉不相離故。",
        "from": "T32n1669_008:0682b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然各別別一時處轉不可合一。",
        "from": "T32n1669_008:0682b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈本一及上一互各攝諸位俱轉不捨離然不可合一故。",
        "from": "T32n1669_008:0682b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是已說俱行不離轉相門。",
        "from": "T32n1669_008:0682b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說區區不雜轉相門。",
        "from": "T32n1669_008:0682b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_008:0682b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_008:0682b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　本上一切位　　一一皆各各",
        "from": "T32n1669_008:0682b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　安住自家中　　不攝他法故",
        "from": "T32n1669_008:0682b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_008:0682b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名為區不雜門。",
        "from": "T32n1669_008:0682b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂所有一切種種本上諸位。",
        "from": "T32n1669_008:0682b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一各各唯安住自家中。",
        "from": "T32n1669_008:0682b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不移轉亦不出入。",
        "from": "T32n1669_008:0682b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不攝他亦不定常。",
        "from": "T32n1669_008:0682b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常恒具足遍廣大故。",
        "from": "T32n1669_008:0682b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈本上一切位一一皆各各安住自家中不攝他法故。",
        "from": "T32n1669_008:0682b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故如是已說區區不雜轉相門。",
        "from": "T32n1669_008:0682b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說具足圓滿轉相門。",
        "from": "T32n1669_008:0682b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_008:0682b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_008:0682b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　前所說四門　　一切時處等",
        "from": "T32n1669_008:0682b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無礙自在轉　　名具足圓滿",
        "from": "T32n1669_008:0682b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_008:0682b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名為具足轉門。",
        "from": "T32n1669_008:0682b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂如前所說四門。",
        "from": "T32n1669_008:0682b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時轉一處轉。",
        "from": "T32n1669_008:0682b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "異時轉異處轉。",
        "from": "T32n1669_008:0682b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一轉離轉總轉別轉。",
        "from": "T32n1669_008:0682b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無礙自在故。",
        "from": "T32n1669_008:0682b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈前所說四門。",
        "from": "T32n1669_008:0682b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切時處等無礙自在轉名具足圓滿故。",
        "from": "T32n1669_008:0682b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩訶衍大陀羅尼金剛神呪修多羅中。",
        "from": "T32n1669_008:0682b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說。",
        "from": "T32n1669_008:0682b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一一切一切離雜有無一稱一量法藏門海中。",
        "from": "T32n1669_008:0682b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總有二法。",
        "from": "T32n1669_008:0682b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為二。",
        "from": "T32n1669_008:0682b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者是總二者是別。",
        "from": "T32n1669_008:0682b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言別者。",
        "from": "T32n1669_008:0682b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四種俱輪地故。",
        "from": "T32n1669_008:0682b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言總者。",
        "from": "T32n1669_008:0682b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四種俱輪自在轉故。",
        "from": "T32n1669_008:0682b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至廣說故。",
        "from": "T32n1669_008:0682b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獨地獨天一種廣大無二山王大決擇分第十六如是已說獨地非亂一定一定道路大決擇分。",
        "from": "T32n1669_008:0682b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說獨地獨天一種廣大無二山王大決擇分。",
        "from": "T32n1669_008:0682c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_008:0682c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_008:0682c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　此山王體中　　則有二種門",
        "from": "T32n1669_008:0682c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　謂自性本本　　如次應觀察",
        "from": "T32n1669_008:0682c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_008:0682c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就此獨地獨天一種廣大無二山王體中。",
        "from": "T32n1669_008:0682c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則有二門。",
        "from": "T32n1669_008:0682c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為二。",
        "from": "T32n1669_008:0682c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者本一自性。",
        "from": "T32n1669_008:0682c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "位地門。",
        "from": "T32n1669_008:0682c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者本一之本位地門。",
        "from": "T32n1669_008:0682c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為二。",
        "from": "T32n1669_008:0682c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如其次第應審觀察。",
        "from": "T32n1669_008:0682c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈此山王體中則有二種門。",
        "from": "T32n1669_008:0682c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂自性本本如次應觀察故。",
        "from": "T32n1669_008:0682c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自性位地形相如何。",
        "from": "T32n1669_008:0682c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_008:0682c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　彼本一法中　　不待他自然",
        "from": "T32n1669_008:0682c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　有五十一位　　是名自性位",
        "from": "T32n1669_008:0682c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如是五十一　　一一皆各各",
        "from": "T32n1669_008:0682c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　有五百法門　　周遍廣大轉",
        "from": "T32n1669_008:0682c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_008:0682c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名為自性位地。",
        "from": "T32n1669_008:0682c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂如前所說五十一種本一法中。",
        "from": "T32n1669_008:0682c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一各各不待他力。",
        "from": "T32n1669_008:0682c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然自性有五十一真金剛位。",
        "from": "T32n1669_008:0682c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故說言自性位地。",
        "from": "T32n1669_008:0682c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈彼本一法中不待他自然有五十一位是名自性位故。",
        "from": "T32n1669_008:0682c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是各各五十一位。",
        "from": "T32n1669_008:0682c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一各各有五百法門之大海。",
        "from": "T32n1669_008:0682c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦周遍轉亦廣大轉。",
        "from": "T32n1669_008:0682c22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈如是五十一一一皆各各有五百法門周遍廣大轉故。",
        "from": "T32n1669_008:0682c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此位中亦如前說五種大門。",
        "from": "T32n1669_008:0682c24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足具足圓滿圓滿應審思擇。",
        "from": "T32n1669_008:0682c25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是已說本一自性位地門。",
        "from": "T32n1669_008:0682c25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說本一之本位地門。",
        "from": "T32n1669_008:0682c26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_008:0682c27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_008:0682c27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　五十一本法　　亦各有本法",
        "from": "T32n1669_008:0682c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　名空空空一　　此中亦有位",
        "from": "T32n1669_008:0682c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_008:0683a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本一所依空空空一中。",
        "from": "T32n1669_008:0683a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有諸位。",
        "from": "T32n1669_008:0683a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一位位皆悉各各十萬法門。",
        "from": "T32n1669_008:0683a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足圓滿無闕失轉。",
        "from": "T32n1669_008:0683a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此位中亦有前說五種大門。",
        "from": "T32n1669_008:0683a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足具足圓滿圓滿。",
        "from": "T32n1669_008:0683a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應審思擇。",
        "from": "T32n1669_008:0683a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "品論修多羅中。",
        "from": "T32n1669_008:0683a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說禪定摩訶衍體中。",
        "from": "T32n1669_008:0683a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則有三大門。",
        "from": "T32n1669_008:0683a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為三。",
        "from": "T32n1669_008:0683a05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者上地安立廣大海會門。",
        "from": "T32n1669_008:0683a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者宗本有有有一門。",
        "from": "T32n1669_008:0683a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者根本空空空一門。",
        "from": "T32n1669_008:0683a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是三門皆有諸位。",
        "from": "T32n1669_008:0683a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足圓滿同轉異轉等。",
        "from": "T32n1669_008:0683a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至廣說故◎。",
        "from": "T32n1669_008:0683a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大宗地玄文本論卷第八",
        "from": "T32n1669_008:0683a10_0",
        "context_with_next_sentence": false
    }
]