[
    {
        "source_sentence": "歷朝釋氏資鑑卷第十二",
        "from": "X76n1517_012:0253a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "閩扆峯沙門　熈仲　集",
        "from": "X76n1517_012:0253a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大元昌運",
        "from": "X76n1517_012:0253a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國朝肇造區宇。",
        "from": "X76n1517_012:0253a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奄有四方。",
        "from": "X76n1517_012:0253a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "太祖聖武皇帝。",
        "from": "X76n1517_012:0253a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "握乾符而起朔土。",
        "from": "X76n1517_012:0253a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以神武而膺帝圖。",
        "from": "X76n1517_012:0253a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "太宗皇帝。",
        "from": "X76n1517_012:0253a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "列聖相承。",
        "from": "X76n1517_012:0253a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以成大業。",
        "from": "X76n1517_012:0253a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殄滅殘金。",
        "from": "X76n1517_012:0253a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平定汴蔡。",
        "from": "X76n1517_012:0253a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "撫安中原。",
        "from": "X76n1517_012:0253a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世祖聖德神功文武皇帝。",
        "from": "X76n1517_012:0253b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "渡江之後。",
        "from": "X76n1517_012:0253b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓋將深入。",
        "from": "X76n1517_012:0253b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃聞國中重以僉軍之擾。",
        "from": "X76n1517_012:0253b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黎庶驚駭。",
        "from": "X76n1517_012:0253b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馹騎馳歸。",
        "from": "X76n1517_012:0253b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宗盟推載。",
        "from": "X76n1517_012:0253b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以世祖皇帝。",
        "from": "X76n1517_012:0253b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃太祖皇帝嫡孫之中。",
        "from": "X76n1517_012:0253b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先皇母弟之列。",
        "from": "X76n1517_012:0253b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以賢為長。",
        "from": "X76n1517_012:0253b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "止于一人。",
        "from": "X76n1517_012:0253b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖在征伐之間。",
        "from": "X76n1517_012:0253b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有仁愛愽施濟眾。",
        "from": "X76n1517_012:0253b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實可為天下主。",
        "from": "X76n1517_012:0253b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歲庚申四月。",
        "from": "X76n1517_012:0253b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即皇帝位。",
        "from": "X76n1517_012:0253b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五月。",
        "from": "X76n1517_012:0253b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建年號為中統元年。",
        "from": "X76n1517_012:0253b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建元表歲。",
        "from": "X76n1517_012:0253b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "示人君萬世之傳紀。",
        "from": "X76n1517_012:0253b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時書王見天下一家之義。",
        "from": "X76n1517_012:0253b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法春秋之正。",
        "from": "X76n1517_012:0253b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始體大易之乾元。",
        "from": "X76n1517_012:0253b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "炳煥皇猷。",
        "from": "X76n1517_012:0253b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "權輿治道。",
        "from": "X76n1517_012:0253b12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內立都省。",
        "from": "X76n1517_012:0253b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以總宏綱。",
        "from": "X76n1517_012:0253b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外設總司。",
        "from": "X76n1517_012:0253b12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以平庶政。",
        "from": "X76n1517_012:0253b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燕京修營。",
        "from": "X76n1517_012:0253b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宮室加號。",
        "from": "X76n1517_012:0253b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　上都五年。",
        "from": "X76n1517_012:0253b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "改為至元八年。",
        "from": "X76n1517_012:0253b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建國號曰大元。",
        "from": "X76n1517_012:0253b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓋易經乾元之義。",
        "from": "X76n1517_012:0253b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以明資始之功。",
        "from": "X76n1517_012:0253b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天戈南指。",
        "from": "X76n1517_012:0253b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命帥遣將。",
        "from": "X76n1517_012:0253b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戒以不殺。",
        "from": "X76n1517_012:0253b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "江南州郡。",
        "from": "X76n1517_012:0253b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "望風歸附。",
        "from": "X76n1517_012:0253b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十三年歲丙子正月。",
        "from": "X76n1517_012:0253b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "収宋南北混一。",
        "from": "X76n1517_012:0253b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天成地平。",
        "from": "X76n1517_012:0253b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上應天心。",
        "from": "X76n1517_012:0253b17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下慰民望。",
        "from": "X76n1517_012:0253b18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大哉乾元。",
        "from": "X76n1517_012:0253b18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至哉坤元。",
        "from": "X76n1517_012:0253b18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為民父母。",
        "from": "X76n1517_012:0253b18_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　神武一奮。",
        "from": "X76n1517_012:0253b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疆宇泰恢。",
        "from": "X76n1517_012:0253b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天地覆載之間。",
        "from": "X76n1517_012:0253b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉歸一洪化。",
        "from": "X76n1517_012:0253b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日月照臨之所。",
        "from": "X76n1517_012:0253b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆彼□容光。",
        "from": "X76n1517_012:0253b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓋一念之仁。",
        "from": "X76n1517_012:0253b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有以基也。",
        "from": "X76n1517_012:0253b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洪惟娑婆世主。",
        "from": "X76n1517_012:0253b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛心天子。",
        "from": "X76n1517_012:0253b22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自剎帝利種以流芳。",
        "from": "X76n1517_012:0253b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直至今大元而續焰。",
        "from": "X76n1517_012:0253b22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金枝而枝枝挺秀。",
        "from": "X76n1517_012:0253b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玉葉而葉葉相承。",
        "from": "X76n1517_012:0253b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法輪大轉於支那。",
        "from": "X76n1517_012:0253b23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝日長輝於震旦。",
        "from": "X76n1517_012:0253c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光融沙界。",
        "from": "X76n1517_012:0253c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凉蔭蒼生。",
        "from": "X76n1517_012:0253c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內立帝師。",
        "from": "X76n1517_012:0253c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為舟航於法海。",
        "from": "X76n1517_012:0253c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外設僧統。",
        "from": "X76n1517_012:0253c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃撫治於教門。",
        "from": "X76n1517_012:0253c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故我仁宗皇帝。",
        "from": "X76n1517_012:0253c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖旨頒行。",
        "from": "X76n1517_012:0253c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天下各路。",
        "from": "X76n1517_012:0253c04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建立帝師殿。",
        "from": "X76n1517_012:0253c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高文廟五尺。",
        "from": "X76n1517_012:0253c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則加號曰皇天之下。",
        "from": "X76n1517_012:0253c05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一人之上。",
        "from": "X76n1517_012:0253c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開敷宣文。",
        "from": "X76n1517_012:0253c06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輔治大聖。",
        "from": "X76n1517_012:0253c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至德普覺。",
        "from": "X76n1517_012:0253c06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真智祐國。",
        "from": "X76n1517_012:0253c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如意大寶法王。",
        "from": "X76n1517_012:0253c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天佛子大元帝師。",
        "from": "X76n1517_012:0253c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以彰其尊師重法也。",
        "from": "X76n1517_012:0253c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天下寺院裏。",
        "from": "X76n1517_012:0253c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祝粮休納。",
        "from": "X76n1517_012:0253c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "差彼體當歷朝遣使馳驛降香。",
        "from": "X76n1517_012:0253c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍行天下寺院裏。",
        "from": "X76n1517_012:0253c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開讀大藏經文。",
        "from": "X76n1517_012:0253c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "布施齋料。",
        "from": "X76n1517_012:0253c11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有司准給。",
        "from": "X76n1517_012:0253c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "專為告天祝壽。",
        "from": "X76n1517_012:0253c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以表其敬僧弘教也。",
        "from": "X76n1517_012:0253c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖旨節該。",
        "from": "X76n1517_012:0253c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡有重刑至死者。",
        "from": "X76n1517_012:0253c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如府州審問獄成。",
        "from": "X76n1517_012:0253c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便行斷決。",
        "from": "X76n1517_012:0253c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "死者不可復生。",
        "from": "X76n1517_012:0253c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷者不可復續。",
        "from": "X76n1517_012:0253c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案牘繁冗。",
        "from": "X76n1517_012:0253c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須臾決判萬一差誤人命。",
        "from": "X76n1517_012:0253c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悔將何及。",
        "from": "X76n1517_012:0253c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　朕實矜怜。",
        "from": "X76n1517_012:0253c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今後凡有死刑。",
        "from": "X76n1517_012:0253c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "審覆無疑。",
        "from": "X76n1517_012:0253c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呈省聞奏。",
        "from": "X76n1517_012:0253c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "待報斷決。",
        "from": "X76n1517_012:0253c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖堯仁如天。",
        "from": "X76n1517_012:0253c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不過是。",
        "from": "X76n1517_012:0253c18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其崇儒也。",
        "from": "X76n1517_012:0253c18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立國子監。",
        "from": "X76n1517_012:0253c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置大學。",
        "from": "X76n1517_012:0253c19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "免一切差發。",
        "from": "X76n1517_012:0253c19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天下之士心歸焉。",
        "from": "X76n1517_012:0253c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其恤民也。",
        "from": "X76n1517_012:0253c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置按察司。",
        "from": "X76n1517_012:0253c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "選擇官吏。",
        "from": "X76n1517_012:0253c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勸課農桑。",
        "from": "X76n1517_012:0253c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問民疾苦。",
        "from": "X76n1517_012:0253c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天時灾旱。",
        "from": "X76n1517_012:0253c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "腹裏則減免包銀絲綿。",
        "from": "X76n1517_012:0253c21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "江南則減憂稅秋粮。",
        "from": "X76n1517_012:0253c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天下之民心歸焉。",
        "from": "X76n1517_012:0253c22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其恤軍也。",
        "from": "X76n1517_012:0253c22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠征陣亡子弟。",
        "from": "X76n1517_012:0253c22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "承襲以爵。",
        "from": "X76n1517_012:0253c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "家口贍以月粮。",
        "from": "X76n1517_012:0253c23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天下之軍心歸焉。",
        "from": "X76n1517_012:0253c23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以至商稅三十分取一。",
        "from": "X76n1517_012:0253c24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深得古人什一之制。",
        "from": "X76n1517_012:0253c24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孝子節婦之旌賞。",
        "from": "X76n1517_012:0254a01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "游手好閑之懲勸。",
        "from": "X76n1517_012:0254a01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "靡所不周也。",
        "from": "X76n1517_012:0254a01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大元肇興。",
        "from": "X76n1517_012:0254a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "間世英武。",
        "from": "X76n1517_012:0254a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "席掩八方。",
        "from": "X76n1517_012:0254a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "囊括四海。",
        "from": "X76n1517_012:0254a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東至日出之晹谷。",
        "from": "X76n1517_012:0254a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西至日昧之虞淵。",
        "from": "X76n1517_012:0254a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寸地尺天。",
        "from": "X76n1517_012:0254a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆入版圖。",
        "from": "X76n1517_012:0254a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人心歸之。",
        "from": "X76n1517_012:0254a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天命歸之。",
        "from": "X76n1517_012:0254a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九流樂業。",
        "from": "X76n1517_012:0254a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬國謳歌。",
        "from": "X76n1517_012:0254a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自闢以來。",
        "from": "X76n1517_012:0254a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實未之有也。",
        "from": "X76n1517_012:0254a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得一混一三千大千世界。",
        "from": "X76n1517_012:0254a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而統御金輪。",
        "from": "X76n1517_012:0254a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大元至元。",
        "from": "X76n1517_012:0254a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百億萬億年華。",
        "from": "X76n1517_012:0254a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而延洪寶曆。",
        "from": "X76n1517_012:0254a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "容摭事實。",
        "from": "X76n1517_012:0254a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鋟梓流通。",
        "from": "X76n1517_012:0254a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自堯甲辰即位。",
        "from": "X76n1517_012:0254a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至大元元統二年歲甲戌。",
        "from": "X76n1517_012:0254a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡三千六百九十二年。",
        "from": "X76n1517_012:0254a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "計六十二周甲子。",
        "from": "X76n1517_012:0254a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舊都通玄關北。",
        "from": "X76n1517_012:0254a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有永安寺。",
        "from": "X76n1517_012:0254a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廢盡惟塔存焉。",
        "from": "X76n1517_012:0254a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其石刻釋迦舍利塔。",
        "from": "X76n1517_012:0254a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大遼壽昌二年三月十五日。",
        "from": "X76n1517_012:0254a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯密圓通法師道[辰*殳]造。",
        "from": "X76n1517_012:0254a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內有舍利戒珠二十粒。",
        "from": "X76n1517_012:0254a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香泥小塔二千。",
        "from": "X76n1517_012:0254a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及陀羅經五部。",
        "from": "X76n1517_012:0254a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兵火蕪沒。",
        "from": "X76n1517_012:0254a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨夜屢放光明。",
        "from": "X76n1517_012:0254a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉御禿列。",
        "from": "X76n1517_012:0254a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奏其祥瑞。",
        "from": "X76n1517_012:0254a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有旨開舊塔。",
        "from": "X76n1517_012:0254a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "石凾中有鐵塔。",
        "from": "X76n1517_012:0254a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塔內有銅缾。",
        "from": "X76n1517_012:0254a18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香水盈滿。",
        "from": "X76n1517_012:0254a18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色如玉漿。",
        "from": "X76n1517_012:0254a19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利堅圓。",
        "from": "X76n1517_012:0254a19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燦若金粟。",
        "from": "X76n1517_012:0254a19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瓶底獲一錢。",
        "from": "X76n1517_012:0254a19_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上鑄至元通寶。",
        "from": "X76n1517_012:0254a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃知聖人制法。",
        "from": "X76n1517_012:0254a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "預定待時呈顯。",
        "from": "X76n1517_012:0254a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即至元八年三月二十五日也。",
        "from": "X76n1517_012:0254a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝后王臣閱之。",
        "from": "X76n1517_012:0254a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愈加崇重。",
        "from": "X76n1517_012:0254a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即迎舍利。",
        "from": "X76n1517_012:0254a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重立斯塔。",
        "from": "X76n1517_012:0254a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而倍增莊嚴焉(文多不盡錄辯偽錄)。",
        "from": "X76n1517_012:0254a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歷朝釋氏資鑑卷第十二(終)",
        "from": "X76n1517_012:0254a24_0",
        "context_with_next_sentence": false
    }
]