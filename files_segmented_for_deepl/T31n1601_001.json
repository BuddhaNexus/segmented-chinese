[
    {
        "source_sentence": "辯中邊論頌一卷",
        "from": "T31n1601_001:0477c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彌勒菩薩說",
        "from": "T31n1601_001:0477c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏法師玄奘奉　詔譯",
        "from": "T31n1601_001:0477c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辯相品第一",
        "from": "T31n1601_001:0477c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　唯相障真實　　及修諸對治",
        "from": "T31n1601_001:0477c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　即此修分位　　得果無上乘",
        "from": "T31n1601_001:0477c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　虛妄分別有　　於此二都無",
        "from": "T31n1601_001:0477c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　此中唯有空　　於彼亦有此",
        "from": "T31n1601_001:0477c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　故說一切法　　非空非不空",
        "from": "T31n1601_001:0477c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　有無及有故　　是則契中道",
        "from": "T31n1601_001:0477c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　識生變似義　　有情我及了",
        "from": "T31n1601_001:0477c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　此境實非有　　境無故識無",
        "from": "T31n1601_001:0477c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　虛妄分別性　　由此義得成",
        "from": "T31n1601_001:0477c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　非實有全無　　許滅解脫故",
        "from": "T31n1601_001:0477c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　唯所執依他　　及圓成實性",
        "from": "T31n1601_001:0477c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　境故分別故　　及二空故說",
        "from": "T31n1601_001:0477c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　依識有所得　　境無所得生",
        "from": "T31n1601_001:0477c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　依境無所得　　識無所得生",
        "from": "T31n1601_001:0477c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　由識有得性　　亦成無所得",
        "from": "T31n1601_001:0477c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　故知二有得　　無得性平等",
        "from": "T31n1601_001:0477c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　三界心心所　　是虛妄分別",
        "from": "T31n1601_001:0477c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　唯了境名心　　亦別名心所",
        "from": "T31n1601_001:0477c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一則名緣識　　第二名受者",
        "from": "T31n1601_001:0477c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　此中能受用　　分別推心所",
        "from": "T31n1601_001:0477c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　覆障及安立　　將導攝圓滿",
        "from": "T31n1601_001:0477c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　三分別受用　　引起并連縛",
        "from": "T31n1601_001:0477c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　現前苦果故　　唯此惱世間",
        "from": "T31n1601_001:0477c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　三二七雜染　　由虛妄分別",
        "from": "T31n1601_001:0478a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　諸相及異門　　義差別成立",
        "from": "T31n1601_001:0478a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　應知二空性　　略說唯由此",
        "from": "T31n1601_001:0478a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無二有無故　　非有亦非無",
        "from": "T31n1601_001:0478a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　非異亦非一　　是說為空相",
        "from": "T31n1601_001:0478a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　略說空異門　　謂真如實際",
        "from": "T31n1601_001:0478a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無相勝義性　　法界等應知",
        "from": "T31n1601_001:0478a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　由無變無倒　　相滅聖智境",
        "from": "T31n1601_001:0478a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　及諸聖法因　　異門義如次",
        "from": "T31n1601_001:0478a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　此雜染清淨　　由有垢無垢",
        "from": "T31n1601_001:0478a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如水界全空　　淨故許為淨",
        "from": "T31n1601_001:0478a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　能食及所食　　此依身所住",
        "from": "T31n1601_001:0478a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　能見如此理　　所求二淨空",
        "from": "T31n1601_001:0478a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　為常益有情　　為不捨生死",
        "from": "T31n1601_001:0478a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　為善無窮盡　　故觀此為空",
        "from": "T31n1601_001:0478a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　為種性清淨　　為得諸相好",
        "from": "T31n1601_001:0478a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　為淨諸佛法　　故菩薩觀空",
        "from": "T31n1601_001:0478a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　補特伽羅法　　實性俱非有",
        "from": "T31n1601_001:0478a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　此無性有性　　故別立二空",
        "from": "T31n1601_001:0478a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　此若無雜染　　一切應自脫",
        "from": "T31n1601_001:0478a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　此若無清淨　　功用應無果",
        "from": "T31n1601_001:0478a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　非染非不染　　非淨非不淨",
        "from": "T31n1601_001:0478a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　心性本淨故　　由客塵所染",
        "from": "T31n1601_001:0478a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辯障品第二",
        "from": "T31n1601_001:0478a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　具分及一分　　增盛與平等",
        "from": "T31n1601_001:0478a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　於生死取捨　　說障二種性",
        "from": "T31n1601_001:0478a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　九種煩惱相　　謂愛等九結",
        "from": "T31n1601_001:0478a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　初二障厭捨　　餘七障真見",
        "from": "T31n1601_001:0478a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　謂能障身見　　彼事滅道寶",
        "from": "T31n1601_001:0478a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　利養恭敬等　　遠離遍知故",
        "from": "T31n1601_001:0478b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無加行非處　　不如理不生",
        "from": "T31n1601_001:0478b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　不起正思惟　　資糧未圓滿",
        "from": "T31n1601_001:0478b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　闕種性善友　　心極疲厭性",
        "from": "T31n1601_001:0478b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　及闕於正行　　鄙惡者同居",
        "from": "T31n1601_001:0478b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　倒麁重三餘　　般若未成就",
        "from": "T31n1601_001:0478b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　及本性麁重　　懈怠放逸性",
        "from": "T31n1601_001:0478b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　著有著資財　　及心性下劣",
        "from": "T31n1601_001:0478b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　不信無勝解　　如言而取義",
        "from": "T31n1601_001:0478b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　輕法重名利　　於有情無悲",
        "from": "T31n1601_001:0478b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　匱聞及少聞　　不修治妙定",
        "from": "T31n1601_001:0478b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　善菩提攝受　　有慧無亂障",
        "from": "T31n1601_001:0478b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　迴向不怖慳　　自在名善等",
        "from": "T31n1601_001:0478b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如是善等十　　各有前三障",
        "from": "T31n1601_001:0478b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　於覺分度地　　有別障應知",
        "from": "T31n1601_001:0478b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　於事不善巧　　懈怠定減二",
        "from": "T31n1601_001:0478b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　不植羸劣性　　見麁重過失",
        "from": "T31n1601_001:0478b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　障富貴善趣　　不捨諸有情",
        "from": "T31n1601_001:0478b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　於失德減增　　令趣入解脫",
        "from": "T31n1601_001:0478b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　障施等諸善　　無盡亦無間",
        "from": "T31n1601_001:0478b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　所作善決定　　受用法成熟",
        "from": "T31n1601_001:0478b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　遍行與最勝　　勝流及無攝",
        "from": "T31n1601_001:0478b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　相續無差別　　無雜染清淨",
        "from": "T31n1601_001:0478b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　種種法無別　　及不增不減",
        "from": "T31n1601_001:0478b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　并無分別等　　四自在依義",
        "from": "T31n1601_001:0478b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　於斯十法界　　有不染無明",
        "from": "T31n1601_001:0478b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　障十地功德　　故說為十障",
        "from": "T31n1601_001:0478b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　已說諸煩惱　　及諸所知障",
        "from": "T31n1601_001:0478b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　許此二盡故　　一切障解脫",
        "from": "T31n1601_001:0478b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辯真實品第三",
        "from": "T31n1601_001:0478c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　真實唯有十　　謂根本與相",
        "from": "T31n1601_001:0478c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無顛倒因果　　及麁細真實",
        "from": "T31n1601_001:0478c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　極成淨所行　　攝受并差別",
        "from": "T31n1601_001:0478c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　十善巧真實　　皆為除我見",
        "from": "T31n1601_001:0478c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　許於三自性　　唯一常非有",
        "from": "T31n1601_001:0478c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一有而不真　　一有無真實",
        "from": "T31n1601_001:0478c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　於法數取趣　　及所取能取",
        "from": "T31n1601_001:0478c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　有非有性中　　增益損減見",
        "from": "T31n1601_001:0478c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　知此故不轉　　是名真實相",
        "from": "T31n1601_001:0478c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無性與生滅　　垢淨三無常",
        "from": "T31n1601_001:0478c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　所取及事相　　和合苦三種",
        "from": "T31n1601_001:0478c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　空亦有三種　　謂無異自性",
        "from": "T31n1601_001:0478c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無相及異相　　自相三無我",
        "from": "T31n1601_001:0478c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如次四三種　　依根本真實",
        "from": "T31n1601_001:0478c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　苦三相已說　　集亦有三種",
        "from": "T31n1601_001:0478c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　謂習氣等起　　及相未離繫",
        "from": "T31n1601_001:0478c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　自性二不生　　垢寂二三滅",
        "from": "T31n1601_001:0478c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　遍知及永斷　　證得三道諦",
        "from": "T31n1601_001:0478c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　應知世俗諦　　差別有三種",
        "from": "T31n1601_001:0478c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　謂假行顯了　　如次依本三",
        "from": "T31n1601_001:0478c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　勝義諦亦三　　謂義得正行",
        "from": "T31n1601_001:0478c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　依本一無變　　無倒二圓實",
        "from": "T31n1601_001:0478c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　世極成依一　　理極成依三",
        "from": "T31n1601_001:0478c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　淨所行有二　　依一圓成實",
        "from": "T31n1601_001:0478c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　名遍計所執　　相分別依他",
        "from": "T31n1601_001:0478c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　真如及正智　　圓成實所攝",
        "from": "T31n1601_001:0478c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　流轉與安立　　邪行依初二",
        "from": "T31n1601_001:0478c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　實相唯識淨　　正行依後一",
        "from": "T31n1601_001:0478c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　於蘊等我見　　執一因受者",
        "from": "T31n1601_001:0479a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　作者自在轉　　增上義及常",
        "from": "T31n1601_001:0479a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　雜染清淨依　　觀縛解者性",
        "from": "T31n1601_001:0479a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　此所執分別　　法性義在彼",
        "from": "T31n1601_001:0479a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　非一及總略　　分段議名蘊",
        "from": "T31n1601_001:0479a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　能所取彼取　　種子義名界",
        "from": "T31n1601_001:0479a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　能受所了境　　用門義名處",
        "from": "T31n1601_001:0479a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　緣起義於因　　果用無增減",
        "from": "T31n1601_001:0479a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　於非愛愛淨　　俱生及勝主",
        "from": "T31n1601_001:0479a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　得行不自在　　是處非處義",
        "from": "T31n1601_001:0479a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　根於取住續　　用二淨增上",
        "from": "T31n1601_001:0479a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　因果已未用　　是世義應知",
        "from": "T31n1601_001:0479a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　受及受資糧　　彼所因諸行",
        "from": "T31n1601_001:0479a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　二寂滅對治　　是諦義應知",
        "from": "T31n1601_001:0479a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　由功德過失　　及無分別智",
        "from": "T31n1601_001:0479a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　依他自出離　　是乘義應知",
        "from": "T31n1601_001:0479a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　有為無為義　　謂若假若因",
        "from": "T31n1601_001:0479a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若相若寂靜　　若彼所觀義",
        "from": "T31n1601_001:0479a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辯修對治品第四",
        "from": "T31n1601_001:0479a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　以麁重愛因　　我事無迷故",
        "from": "T31n1601_001:0479a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　為入四聖諦　　修念住應知",
        "from": "T31n1601_001:0479a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　已遍知障治　　一切種差別",
        "from": "T31n1601_001:0479a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　為遠離修集　　勤修四正斷",
        "from": "T31n1601_001:0479a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　依住堪能性　　為一切事成",
        "from": "T31n1601_001:0479a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　滅除五過失　　勤修八斷行",
        "from": "T31n1601_001:0479a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　懈怠忘聖言　　及惛沈掉舉",
        "from": "T31n1601_001:0479a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　不作行作行　　是五失應知",
        "from": "T31n1601_001:0479a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　為斷除懈怠　　修欲勤信安",
        "from": "T31n1601_001:0479a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　即所依能依　　及所因能果",
        "from": "T31n1601_001:0479a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　為除餘四失　　修念智思捨",
        "from": "T31n1601_001:0479b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　記言覺沈掉　　伏行滅等流",
        "from": "T31n1601_001:0479b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　已種順解脫　　復修五增上",
        "from": "T31n1601_001:0479b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　謂欲行不忘　　不散亂思擇",
        "from": "T31n1601_001:0479b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　即損障名力　　因果立次第",
        "from": "T31n1601_001:0479b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　順決擇二二　　在五根五力",
        "from": "T31n1601_001:0479b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　覺支略有五　　謂所依自性",
        "from": "T31n1601_001:0479b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　出離并利益　　及三無染支",
        "from": "T31n1601_001:0479b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　由因緣所依　　自性義差別",
        "from": "T31n1601_001:0479b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　故輕安定捨　　說為無染支",
        "from": "T31n1601_001:0479b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　分別及誨示　　令他信有三",
        "from": "T31n1601_001:0479b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　對治障亦三　　故道支成八",
        "from": "T31n1601_001:0479b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　表見戒遠離　　令他深信受",
        "from": "T31n1601_001:0479b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　對治本隨惑　　及自在障故",
        "from": "T31n1601_001:0479b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　有倒順無倒　　無倒有倒隨",
        "from": "T31n1601_001:0479b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無倒無倒隨　　是修治差別",
        "from": "T31n1601_001:0479b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　菩薩所修習　　由所緣作意",
        "from": "T31n1601_001:0479b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　證得殊勝故　　與二乘差別",
        "from": "T31n1601_001:0479b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辯修分位品第五",
        "from": "T31n1601_001:0479b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　所說修對治　　分位有十八",
        "from": "T31n1601_001:0479b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　謂因入行果　　作無作殊勝",
        "from": "T31n1601_001:0479b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　上無上解行　　入出離記說",
        "from": "T31n1601_001:0479b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　灌頂及證得　　勝利成所作",
        "from": "T31n1601_001:0479b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　應知法界中　　略有三分位",
        "from": "T31n1601_001:0479b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　不淨淨不淨　　清淨隨所應",
        "from": "T31n1601_001:0479b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　依前諸位中　　所有差別相",
        "from": "T31n1601_001:0479b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　隨所應建立　　諸補特伽羅",
        "from": "T31n1601_001:0479b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辯得果品第六",
        "from": "T31n1601_001:0479b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　器說為異熟　　力是彼增上",
        "from": "T31n1601_001:0479b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　愛樂增長淨　　如次即五果",
        "from": "T31n1601_001:0479c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　復略說餘果　　後後初數習",
        "from": "T31n1601_001:0479c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　究竟順障滅　　離勝上無上",
        "from": "T31n1601_001:0479c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辯無上乘品第七",
        "from": "T31n1601_001:0479c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　總由三無上　　說為無上乘",
        "from": "T31n1601_001:0479c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　謂正行所緣　　及修證無上",
        "from": "T31n1601_001:0479c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　正行有六種　　謂最勝作意",
        "from": "T31n1601_001:0479c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　隨法離二邊　　差別無差別",
        "from": "T31n1601_001:0479c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　最勝有十二　　謂廣大長時",
        "from": "T31n1601_001:0479c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　依處及無盡　　無間無難性",
        "from": "T31n1601_001:0479c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　自在攝發起　　得等流究竟",
        "from": "T31n1601_001:0479c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　由斯說十度　　名波羅蜜多",
        "from": "T31n1601_001:0479c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　十波羅蜜多　　謂施戒安忍",
        "from": "T31n1601_001:0479c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　精進定般若　　方便願力智",
        "from": "T31n1601_001:0479c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　饒益不害受　　增德能入脫",
        "from": "T31n1601_001:0479c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無盡常起定　　受用成熟他",
        "from": "T31n1601_001:0479c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　菩薩以三慧　　恒思惟大乘",
        "from": "T31n1601_001:0479c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如所施設法　　名作意正行",
        "from": "T31n1601_001:0479c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　此增長善界　　入義及事成",
        "from": "T31n1601_001:0479c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　此助伴應知　　即十種法行",
        "from": "T31n1601_001:0479c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　謂書寫供養　　施供聽披讀",
        "from": "T31n1601_001:0479c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　受持正開演　　諷誦及思修",
        "from": "T31n1601_001:0479c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　行十法行者　　獲福聚無量",
        "from": "T31n1601_001:0479c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　勝故無盡故　　由攝他不息",
        "from": "T31n1601_001:0479c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　隨法行二種　　謂諸無散亂",
        "from": "T31n1601_001:0479c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無顛倒轉變　　諸菩薩應知",
        "from": "T31n1601_001:0479c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　出定於境流　　味沈掉矯示",
        "from": "T31n1601_001:0479c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　我執心下劣　　諸智者應知",
        "from": "T31n1601_001:0479c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　智見於文義　　作意及不動",
        "from": "T31n1601_001:0479c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　二相染淨客　　無怖高無倒",
        "from": "T31n1601_001:0480a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　知但由相應　　串習或翻此",
        "from": "T31n1601_001:0480a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　有義及非有　　是於文無倒",
        "from": "T31n1601_001:0480a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　似二性顯現　　如現實非有",
        "from": "T31n1601_001:0480a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　知離有非有　　是於義無倒",
        "from": "T31n1601_001:0480a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　於作意無倒　　知彼言熏習",
        "from": "T31n1601_001:0480a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　言作意彼依　　現似二因故",
        "from": "T31n1601_001:0480a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　於不動無倒　　謂知義非有",
        "from": "T31n1601_001:0480a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　非無如幻等　　有無不動故",
        "from": "T31n1601_001:0480a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　於自相無倒　　知一切唯名",
        "from": "T31n1601_001:0480a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　離一切分別　　依勝義自相",
        "from": "T31n1601_001:0480a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　以離真法界　　無別有一法",
        "from": "T31n1601_001:0480a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　故通達此者　　於共相無倒",
        "from": "T31n1601_001:0480a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　知顛倒作意　　未滅及已滅",
        "from": "T31n1601_001:0480a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　於法界雜染　　清淨無顛倒",
        "from": "T31n1601_001:0480a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　知法界本性　　清淨如虛空",
        "from": "T31n1601_001:0480a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　故染淨非主　　是於客無倒",
        "from": "T31n1601_001:0480a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　有情法無故　　染淨性俱無",
        "from": "T31n1601_001:0480a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　知此無怖高　　是於二無倒",
        "from": "T31n1601_001:0480a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　異性與一性　　外道及聲聞",
        "from": "T31n1601_001:0480a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　增益損減邊　　有情法各二",
        "from": "T31n1601_001:0480a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　所治及能治　　常住與斷滅",
        "from": "T31n1601_001:0480a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　所取能取邊　　染淨二三種",
        "from": "T31n1601_001:0480a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　分別二邊性　　應知復有七",
        "from": "T31n1601_001:0480a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　謂有非有邊　　所能寂怖畏",
        "from": "T31n1601_001:0480a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　所能取正邪　　有用并無用",
        "from": "T31n1601_001:0480a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　不起及時等　　是分別二邊",
        "from": "T31n1601_001:0480a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　差別無差別　　應知於十地",
        "from": "T31n1601_001:0480a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　十波羅蜜多　　增上等修集",
        "from": "T31n1601_001:0480a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　所緣謂安界　　所能立任持",
        "from": "T31n1601_001:0480b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　印內持通達　　增證運最勝",
        "from": "T31n1601_001:0480b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　修證謂無闕　　不毀動圓滿",
