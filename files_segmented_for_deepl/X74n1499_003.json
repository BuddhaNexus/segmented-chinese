[
    {
        "source_sentence": "水陸道場法輪寶懺卷第三",
        "from": "X74n1499_003:0981b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請佛說梵網經(拜觀同上)",
        "from": "X74n1499_003:0981b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姚秦天竺沙門鳩摩羅什譯。",
        "from": "X74n1499_003:0981b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但有菩薩心地一品分上下。",
        "from": "X74n1499_003:0981b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛於第四禪。",
        "from": "X74n1499_003:0981b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "接眾歸蓮華藏。",
        "from": "X74n1499_003:0981b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盧舍那佛。",
        "from": "X74n1499_003:0981b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於千華座上。",
        "from": "X74n1499_003:0981b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先說三十心十地。",
        "from": "X74n1499_003:0981b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說十重四十八輕。",
        "from": "X74n1499_003:0981b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩戒。",
        "from": "X74n1499_003:0981b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令化身釋迦。",
        "from": "X74n1499_003:0981b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉教眾生。",
        "from": "X74n1499_003:0981b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行菩薩道。",
        "from": "X74n1499_003:0981b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入如來地。",
        "from": "X74n1499_003:0981b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大乘律部。",
        "from": "X74n1499_003:0981b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本與諸經襍見。",
        "from": "X74n1499_003:0981b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不必定如小乘分兩藏也。",
        "from": "X74n1499_003:0981b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然說律義多者入律部。",
        "from": "X74n1499_003:0981b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請菩薩瓔珞本業經(拜觀同上)姚秦涼州沙門竺法念譯。",
        "from": "X74n1499_003:0981b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "集眾品第一。",
        "from": "X74n1499_003:0981c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛重遊道樹下。",
        "from": "X74n1499_003:0981c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放四十二光。",
        "from": "X74n1499_003:0981c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "集十首菩薩。",
        "from": "X74n1499_003:0981c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從十林剎十精進佛所來。",
        "from": "X74n1499_003:0981c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并集十切諸天眾等　賢聖名字品第二。",
        "from": "X74n1499_003:0981c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說十住十行十回向十地等覺妙覺名字。",
        "from": "X74n1499_003:0981c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆華梵雙舉。",
        "from": "X74n1499_003:0981c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又說住前十心。",
        "from": "X74n1499_003:0981c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信。",
        "from": "X74n1499_003:0981c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念。",
        "from": "X74n1499_003:0981c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進。",
        "from": "X74n1499_003:0981c05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定。",
        "from": "X74n1499_003:0981c05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧。",
        "from": "X74n1499_003:0981c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戒。",
        "from": "X74n1499_003:0981c05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迴向。",
        "from": "X74n1499_003:0981c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護法。",
        "from": "X74n1499_003:0981c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨。",
        "from": "X74n1499_003:0981c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願。",
        "from": "X74n1499_003:0981c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又說二十四願偈。",
        "from": "X74n1499_003:0981c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十不可悔戒　賢聖學觀品第三。",
        "from": "X74n1499_003:0981c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂十住名習種性。",
        "from": "X74n1499_003:0981c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十行名性種性。",
        "from": "X74n1499_003:0981c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十向名道種性。",
        "from": "X74n1499_003:0981c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十地名聖種性。",
        "from": "X74n1499_003:0981c08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次等覺性。",
        "from": "X74n1499_003:0981c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次妙覺性。",
        "from": "X74n1499_003:0981c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又名六堅。",
        "from": "X74n1499_003:0981c09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又名六忍。",
        "from": "X74n1499_003:0981c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又名六慧。",
        "from": "X74n1499_003:0981c09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又名六定。",
        "from": "X74n1499_003:0981c09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又名六觀。",
        "from": "X74n1499_003:0981c10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又名六寶瓔珞。",
        "from": "X74n1499_003:0981c10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂銅寶銀寶金寶瑠璃寶摩尼寶水晶寶。",
        "from": "X74n1499_003:0981c10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并說六位所修說行　譯義品第四。",
        "from": "X74n1499_003:0981c11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋前十信及四十二位之義　佛母名品第五。",
        "from": "X74n1499_003:0981c12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答敬首菩薩問二諦義。",
        "from": "X74n1499_003:0981c13_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并說小中大劫差別　因果品第六。",
        "from": "X74n1499_003:0981c13_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明十波羅蜜。",
        "from": "X74n1499_003:0981c14_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各有三緣。",
        "from": "X74n1499_003:0981c14_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又說七財四攝四辯四依等為因。",
        "from": "X74n1499_003:0981c15_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二種法身為果　大眾受學品第七。",
        "from": "X74n1499_003:0981c15_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛復放光集眾。",
        "from": "X74n1499_003:0981c16_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "囑文殊及善財七大菩薩各領大眾受學。",
        "from": "X74n1499_003:0981c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因分別三種受戒。",
        "from": "X74n1499_003:0981c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及受戒儀式。",
        "from": "X74n1499_003:0981c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由受戒故。",
        "from": "X74n1499_003:0981c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次第入於住行向地等妙覺中　集散品第八。",
        "from": "X74n1499_003:0981c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三勸大眾發菩提心。",
        "from": "X74n1499_003:0981c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至再囑。",
        "from": "X74n1499_003:0981c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須受菩薩戒已。",
        "from": "X74n1499_003:0981c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方可為說此大法門　天台判別圓位次。",
        "from": "X74n1499_003:0981c20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請菩薩善戒經(拜觀同上)",
        "from": "X74n1499_003:0981c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劉宋中天竺沙門求那跋摩等譯。",
        "from": "X74n1499_003:0981c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名菩薩地。",
        "from": "X74n1499_003:0981c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "序品第一。",
        "from": "X74n1499_003:0981c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與寶積中決定毗尼經同　善行性品第二　發菩提心品第三　利益內外品第四　真實義品第五　不可思議品第六　調伏品第七　菩提品第八　菩提力性品第九　施品第十　戒品第十一　忍品第十二　精進品第十三　禪品第十四　慧品第十五　輭語品第十六　供養三寶品第十七　三十七功道品第十八　助菩提數法餘品第十九　功德品第二十　如法住菩薩相品二十一　如法住禪品二十二　如法住定心品二十三　如法住生菩薩地品二十四　如法住畢竟地生品又第一　畢竟地攝取品又第二　畢竟地畢竟品三　畢竟行品四　畢竟地三十二相八十種好品五　畢竟地住品六。",
        "from": "X74n1499_003:0981c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明十力四無畏等。",
        "from": "X74n1499_003:0982a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後有優波離問結經名　從第二品以下並與瑜伽師地論中菩薩地同意。",
        "from": "X74n1499_003:0982a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彌勒菩薩宗此經成地論。",
        "from": "X74n1499_003:0982a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而地持一經。",
        "from": "X74n1499_003:0982a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又從地論錄出別行。",
        "from": "X74n1499_003:0982a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故仍與此大同也。",
        "from": "X74n1499_003:0982a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請菩薩善戒經(拜觀同上)",
        "from": "X74n1499_003:0982a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明六重八重及四十八輕。",
        "from": "X74n1499_003:0982a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即上經戒品中別出。",
        "from": "X74n1499_003:0982a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與菩薩戒本及羯磨文。",
        "from": "X74n1499_003:0982a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大同小異　優波離明菩薩受戒法。",
        "from": "X74n1499_003:0982a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請佛說受十善戒經(拜觀同上)開元錄拾遺單本。",
        "from": "X74n1499_003:0982a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十惡業品第一。",
        "from": "X74n1499_003:0982a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛為舍利弗說十惡業名。",
        "from": "X74n1499_003:0982a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并示以授十善戒法。",
        "from": "X74n1499_003:0982a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身三口四意三。",
        "from": "X74n1499_003:0982a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三番歸依而受。",
        "from": "X74n1499_003:0982b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次受八戒齋法　十施報品第二。",
        "from": "X74n1499_003:0982b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歎頌不殺功德。",
        "from": "X74n1499_003:0982b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說殺生十報。",
        "from": "X74n1499_003:0982b02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略歎不盜功德。",
        "from": "X74n1499_003:0982b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說偷盜十報。",
        "from": "X74n1499_003:0982b03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讚歎不婬有五功德。",
        "from": "X74n1499_003:0982b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說婬有十過。",
        "from": "X74n1499_003:0982b03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌讚不妄語等功德。",
        "from": "X74n1499_003:0982b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說四過惡業。",
        "from": "X74n1499_003:0982b04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請佛說十善業道經。",
        "from": "X74n1499_003:0982b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及佛說娑伽羅龍王所說大乘法經(拜觀同上)唐于闐國沙門實叉難陀譯。",
        "from": "X74n1499_003:0982b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二宋北印土沙門施護譯。",
        "from": "X74n1499_003:0982b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即方等部海龍王經中一品。",
        "from": "X74n1499_003:0982b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二同。",
        "from": "X74n1499_003:0982b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請文殊師利問經(拜觀同上)蕭梁扶南國沙門僧伽婆羅譯。",
        "from": "X74n1499_003:0982b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "序品第一。",
        "from": "X74n1499_003:0982b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛住耆闍崛山。",
        "from": "X74n1499_003:0982b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與比丘菩薩眾俱　菩薩戒品第二。",
        "from": "X74n1499_003:0982b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與沙彌十戒同　不可思議品第三。",
        "from": "X74n1499_003:0982b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明如來不入涅槃　無我品第四　涅槃品第五　般若波羅蜜品第六　有餘氣品第七　來去品第八　中道品第九　世間戒品第十。",
        "from": "X74n1499_003:0982b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦即沙彌十戒　出世間戒品第十一。",
        "from": "X74n1499_003:0982b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明菩薩七聚　上出世間戒品第十二。",
        "from": "X74n1499_003:0982b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀第一義也　菩薩受戒品第十三。",
        "from": "X74n1499_003:0982b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發菩提心也　字母品第十四　分別部品第十五。",
        "from": "X74n1499_003:0982b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明佛滅後。",
        "from": "X74n1499_003:0982b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧祇上座二部。",
        "from": "X74n1499_003:0982b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共出二十部。",
        "from": "X74n1499_003:0982b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆是大乘出。",
        "from": "X74n1499_003:0982b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無是亦無非　襍問品第十六。",
        "from": "X74n1499_003:0982b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設為外道種種邪難。",
        "from": "X74n1499_003:0982b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求佛解釋　囑累品第十七。",
        "from": "X74n1499_003:0982b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既囑累已。",
        "from": "X74n1499_003:0982b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復廣明在家過患。",
        "from": "X74n1499_003:0982b22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出家功德。",
        "from": "X74n1499_003:0982b22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次明念佛三昧。",
        "from": "X74n1499_003:0982b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得見餘世界佛。",
        "from": "X74n1499_003:0982b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次明用供養華呪法。",
        "from": "X74n1499_003:0982b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請佛說菩薩內戒經(拜觀同上)劉宋中天竺沙門求那跋摩譯。",
        "from": "X74n1499_003:0982c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛以十五日說戒時。",
        "from": "X74n1499_003:0982c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊請問初發意道俗菩薩。",
        "from": "X74n1499_003:0982c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當作何功德。",
        "from": "X74n1499_003:0982c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛為說十二時戒法　一歸依懺悔誓行六度。",
        "from": "X74n1499_003:0982c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發三願。",
        "from": "X74n1499_003:0982c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及行十法則　二受四十七戒　三受佛法身般若。",
        "from": "X74n1499_003:0982c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各二十因緣　四受二十因緣。",
        "from": "X74n1499_003:0982c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行之自知宿命　五受四禪法　六受般若三昧法　七受菩薩三昧法　八受月三昧。",
        "from": "X74n1499_003:0982c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平心行之　九六根去惡為善。",
        "from": "X74n1499_003:0982c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以六度教化一切　十外如地。",
        "from": "X74n1499_003:0982c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內如水　十一願一切眾生。",
        "from": "X74n1499_003:0982c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各得其樂　十二修六妙門。",
        "from": "X74n1499_003:0982c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不住三乘果。",
        "from": "X74n1499_003:0982c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說十住菩薩。",
        "from": "X74n1499_003:0982c11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各有十功德。",
        "from": "X74n1499_003:0982c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請佛藏經(拜觀同上)",
        "from": "X74n1499_003:0982c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姚秦天竺沙門鳩摩羅什譯。",
        "from": "X74n1499_003:0982c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸法實相品第一。",
        "from": "X74n1499_003:0982c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為舍利弗說十空等十喻。",
        "from": "X74n1499_003:0982c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言如來說一切法無生滅　念佛品第二。",
        "from": "X74n1499_003:0982c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有分別。",
        "from": "X74n1499_003:0982c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無取無捨。",
        "from": "X74n1499_003:0982c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是真念佛　念法品第三　念僧品第四　淨戒品第五。",
        "from": "X74n1499_003:0982c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明破戒比丘。",
        "from": "X74n1499_003:0982c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就十憂惱箭。",
        "from": "X74n1499_003:0982c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必墮惡道　淨法品第六。",
        "from": "X74n1499_003:0982c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明迦葉佛預記釋迦佛法中。",
        "from": "X74n1499_003:0982c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多受供養。",
        "from": "X74n1499_003:0982c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故法當疾滅。",
        "from": "X74n1499_003:0982c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如諸盲為賊所誑。",
        "from": "X74n1499_003:0982c20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "墮於深坑　往古品第七。",
        "from": "X74n1499_003:0982c20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明大莊嚴王滅後。",
        "from": "X74n1499_003:0982c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾分五部。",
        "from": "X74n1499_003:0982c21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一解正法者。",
        "from": "X74n1499_003:0982c21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即今滿慈子。",
        "from": "X74n1499_003:0982c22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四說邪法者。",
        "from": "X74n1499_003:0982c22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "久墮地獄。",
        "from": "X74n1499_003:0982c22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今出為調達等　淨見品第八。",
        "from": "X74n1499_003:0982c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明釋迦曾於過去歷侍多佛。",
        "from": "X74n1499_003:0982c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以有所得故。",
        "from": "X74n1499_003:0982c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不獲受記。",
        "from": "X74n1499_003:0982c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至定光佛時。",
        "from": "X74n1499_003:0982c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟無生忍。",
        "from": "X74n1499_003:0983a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃得受記　了戒品第九。",
        "from": "X74n1499_003:0983a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明三種人不喜聞此經。",
        "from": "X74n1499_003:0983a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一破戒。",
        "from": "X74n1499_003:0983a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二增上慢。",
        "from": "X74n1499_003:0983a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三不淨說法及貪著我者。",
        "from": "X74n1499_003:0983a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次明白毫相光一分。",
        "from": "X74n1499_003:0983a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能供如法修道比丘。",
        "from": "X74n1499_003:0983a03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不必愁慮四事。",
        "from": "X74n1499_003:0983a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但一心辦道　囑累品第十。",
        "from": "X74n1499_003:0983a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具明末世不如法事。",
        "from": "X74n1499_003:0983a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并囑累竟。",
        "from": "X74n1499_003:0983a05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魔王憂惱啼哭。",
        "from": "X74n1499_003:0983a05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以佛預為之防。",
        "from": "X74n1499_003:0983a06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能破正法故。",
        "from": "X74n1499_003:0983a06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請優婆塞戒經(拜觀同上)",
        "from": "X74n1499_003:0983a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北涼中天竺沙門曇無讖譯。",
        "from": "X74n1499_003:0983a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "集會品第一。",
        "from": "X74n1499_003:0983a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善生長者。",
        "from": "X74n1499_003:0983a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "述外道禮六方法。",
        "from": "X74n1499_003:0983a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告以六度。",
        "from": "X74n1499_003:0983a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "示一切法無性。",
        "from": "X74n1499_003:0983a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發菩提心。",
        "from": "X74n1499_003:0983a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持八重六重。",
        "from": "X74n1499_003:0983a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名在家出家菩薩　發菩提心品第二　悲品第三　解脫品第四　三種菩提品第五。",
        "from": "X74n1499_003:0983a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂聲聞緣覺佛菩提也。",
        "from": "X74n1499_003:0983a12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦云從聞思修得　修三十二相業品第六　發願品第七　名義菩薩品第八　義菩薩心堅固品第九　自利利他品第十　自他莊嚴品第十一　二莊嚴品第十二。",
        "from": "X74n1499_003:0983a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂福慧　攝取品第十三　受戒品第十四。",
        "from": "X74n1499_003:0983a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明在家六重二十八戒。",
        "from": "X74n1499_003:0983a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須六月中淨四威儀。",
        "from": "X74n1499_003:0983a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃於二十僧中授之　淨戒品第十五。",
        "from": "X74n1499_003:0983a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明受戒後種種法要。",
        "from": "X74n1499_003:0983a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三法四法等　息惡品第十六　供養三寶品第十七　六波羅蜜品第十八　襍品第十九。",
        "from": "X74n1499_003:0983a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明布施如法不如法清淨不清淨　淨三歸品第二十　八戒齋品第二十一　五戒品第二十二　尸波羅蜜品第二十三　業品第二十四。",
        "from": "X74n1499_003:0983a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即十業十惡　羼提波羅蜜品第二十五　毗離耶波羅蜜品第二十六　禪波羅蜜品第二十七　般若波羅蜜品第二十八。",
        "from": "X74n1499_003:0983a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請佛說法律三昧經(拜觀同上)吳月支國優婆塞支謙譯。",
        "from": "X74n1499_003:0983b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛於摩竭提國。",
        "from": "X74n1499_003:0983b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先說十二自燒。",
        "from": "X74n1499_003:0983b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次為阿難說。",
        "from": "X74n1499_003:0983b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖發大意。",
        "from": "X74n1499_003:0983b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有四墮落。",
        "from": "X74n1499_003:0983b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次因舍利弗自責昔非。",
        "from": "X74n1499_003:0983b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為說大乘法要。",
        "from": "X74n1499_003:0983b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次為勇聲菩薩。",
        "from": "X74n1499_003:0983b07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分別聲聞禪緣覺禪如來禪五通仙人禪之不同。",
        "from": "X74n1499_003:0983b08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請清淨毗尼方廣經。",
        "from": "X74n1499_003:0983b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及佛說文殊師利淨律經。",
        "from": "X74n1499_003:0983b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寂調音所問經(拜觀同上)姚秦天竺沙門鳩摩羅什譯。",
        "from": "X74n1499_003:0983b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二竺法護譯。",
        "from": "X74n1499_003:0983b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三劉宋沙門釋法海譯。",
        "from": "X74n1499_003:0983b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛在耆闍崛山。",
        "from": "X74n1499_003:0983b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寂調伏音天子。",
        "from": "X74n1499_003:0983b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思見文殊。",
        "from": "X74n1499_003:0983b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛放白毫藏光。",
        "from": "X74n1499_003:0983b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從寶相佛國召來。",
        "from": "X74n1499_003:0983b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與十千菩薩俱。",
        "from": "X74n1499_003:0983b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊與天子。",
        "from": "X74n1499_003:0983b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問答第一義諦。",
        "from": "X74n1499_003:0983b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及彼土聲聞所證大法。",
        "from": "X74n1499_003:0983b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有四眾各五百人。",
        "from": "X74n1499_003:0983b16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及五千天子。",
        "from": "X74n1499_003:0983b16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因欲往彼土作聲聞眾。",
        "from": "X74n1499_003:0983b17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而發大心。",
        "from": "X74n1499_003:0983b17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得往生記。",
        "from": "X74n1499_003:0983b17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又問答聲聞菩薩律行不同及種種法門。",
        "from": "X74n1499_003:0983b18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因明住五無間。",
        "from": "X74n1499_003:0983b19_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成無上道。",
        "from": "X74n1499_003:0983b19_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂從初發意中間大心。",
        "from": "X74n1499_003:0983b19_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大慈悲。",
        "from": "X74n1499_003:0983b20_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大捨。",
        "from": "X74n1499_003:0983b20_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分證滿證。",
        "from": "X74n1499_003:0983b20_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆悉無間。",
        "from": "X74n1499_003:0983b20_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直至成佛。",
        "from": "X74n1499_003:0983b20_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又總約大悲。",
        "from": "X74n1499_003:0983b21_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別約六度。",
        "from": "X74n1499_003:0983b21_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明煩惱成菩提義。",
        "from": "X74n1499_003:0983b21_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛讚印。",
        "from": "X74n1499_003:0983b21_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解迦葉疑。",
        "from": "X74n1499_003:0983b22_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊以神力。",
        "from": "X74n1499_003:0983b22_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教示彼土菩薩。",
        "from": "X74n1499_003:0983b22_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此土眾生。",
        "from": "X74n1499_003:0983b22_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而發大願　第二同而分四品。",
        "from": "X74n1499_003:0983b23_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五無間諸文缺。",
        "from": "X74n1499_003:0983b23_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天子名寂順律音　第三同於第二。",
        "from": "X74n1499_003:0983b24_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名如來所說清淨調伏經。",
        "from": "X74n1499_003:0983c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請佛說文殊悔過經(拜觀同上)西晉月支國沙門竺法護譯。",
        "from": "X74n1499_003:0983c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛在靈山。",
        "from": "X74n1499_003:0983c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "新學菩薩有疑。",
        "from": "X74n1499_003:0983c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來齊光照耀菩薩為之請問文殊。",
        "from": "X74n1499_003:0983c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊為說。",
        "from": "X74n1499_003:0983c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懺悔。",
        "from": "X74n1499_003:0983c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勸助。",
        "from": "X74n1499_003:0983c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請法。",
        "from": "X74n1499_003:0983c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "興供。",
        "from": "X74n1499_003:0983c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迴向。",
        "from": "X74n1499_003:0983c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發願。",
        "from": "X74n1499_003:0983c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾得無生忍。",
        "from": "X74n1499_003:0983c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛遙聞而讚歎之。",
        "from": "X74n1499_003:0983c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勸助隨喜也。",
        "from": "X74n1499_003:0983c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請三曼陀颰陀羅菩薩經(拜觀同上)西晉清信士聶道真譯。",
        "from": "X74n1499_003:0983c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "序品第一。",
        "from": "X74n1499_003:0983c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛在摩提國。",
        "from": "X74n1499_003:0983c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊問三曼陀颰陀羅菩薩。",
        "from": "X74n1499_003:0983c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人求菩薩道者。",
        "from": "X74n1499_003:0983c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當作何施行。",
        "from": "X74n1499_003:0983c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先總答之　悔過品第二　願樂品第三。",
        "from": "X74n1499_003:0983c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即隨喜　請勸品第四　法行品第五。",
        "from": "X74n1499_003:0983c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即迴向　譬福品第六。",
        "from": "X74n1499_003:0983c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請菩薩藏經。",
        "from": "X74n1499_003:0983c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及舍利弗悔過經。",
        "from": "X74n1499_003:0983c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大乘三聚懺悔經(拜觀同上)蕭梁扶南國沙門僧伽婆羅譯。",
        "from": "X74n1499_003:0983c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二安世高譯。",
        "from": "X74n1499_003:0983c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三闍那崛多及笈多等譯。",
        "from": "X74n1499_003:0983c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛在祇園。",
        "from": "X74n1499_003:0983c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利弗問懺悔隨喜勸請迴向之法。",
        "from": "X74n1499_003:0983c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛具答之。",
        "from": "X74n1499_003:0983c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初明十世界十佛。",
        "from": "X74n1499_003:0983c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令結壇受持。",
        "from": "X74n1499_003:0983c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後明後去大光明聚如來時。",
        "from": "X74n1499_003:0983c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛為竭伽陀天女受持此經即轉女身。",
        "from": "X74n1499_003:0983c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若女人聞此如來名者即不更受女身　二三同而文略。",
        "from": "X74n1499_003:0983c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請佛說淨業障經(拜觀同上)開元附秦錄。",
        "from": "X74n1499_003:0983c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛住毗舍離菴羅樹園。",
        "from": "X74n1499_003:0983c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無垢光比丘入城乞食。",
        "from": "X74n1499_003:0983c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被婬女所呪。",
        "from": "X74n1499_003:0983c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "犯根本戒。",
        "from": "X74n1499_003:0983c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生大憂悔。",
        "from": "X74n1499_003:0983c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見文殊。",
        "from": "X74n1499_003:0984a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂見佛。",
        "from": "X74n1499_003:0984a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛為說無性之法。",
        "from": "X74n1499_003:0984a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發心得記。",
        "from": "X74n1499_003:0984a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因廣說淨業障法。",
        "from": "X74n1499_003:0984a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說過去無垢光佛滅後。",
        "from": "X74n1499_003:0984a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有勇施比丘犯根本戒。",
        "from": "X74n1499_003:0984a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得聞深法遂發大心。",
        "from": "X74n1499_003:0984a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今已成佛。",
        "from": "X74n1499_003:0984a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請佛說善恭敬經。",
        "from": "X74n1499_003:0984a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及佛說正恭敬經(拜觀同上)隋北天竺沙門闍那崛多譯。",
        "from": "X74n1499_003:0984a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二元魏北天竺沙門佛陀扇多譯。",
        "from": "X74n1499_003:0984a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說教他功德。",
        "from": "X74n1499_003:0984a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及事師軌式　第二唯事師軌式。",
        "from": "X74n1499_003:0984a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請佛說大乘戒經(拜觀同上)宋北印土沙門施護譯。",
        "from": "X74n1499_003:0984a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛在祗園說。",
        "from": "X74n1499_003:0984a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文簡義切。",
        "from": "X74n1499_003:0984a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最宜流通。",
        "from": "X74n1499_003:0984a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請菩薩戒羯磨文(拜觀同上)彌勒菩薩說。",
        "from": "X74n1499_003:0984a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐大慈恩寺沙門釋玄奘譯。",
        "from": "X74n1499_003:0984a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出瑜伽師地論。",
        "from": "X74n1499_003:0984a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受戒羯磨第一　懺悔羯磨第二　得捨差別第三。",
        "from": "X74n1499_003:0984a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請菩薩戒本經。",
        "from": "X74n1499_003:0984a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及菩薩優婆塞五戒威儀經。",
        "from": "X74n1499_003:0984a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩戒本(拜觀同上)",
        "from": "X74n1499_003:0984a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈氏說。",
        "from": "X74n1499_003:0984a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北涼中天竺沙門曇無讖譯。",
        "from": "X74n1499_003:0984a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二求那跋摩譯。",
        "from": "X74n1499_003:0984a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三玄奘譯。",
        "from": "X74n1499_003:0984a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此亦半月半月誦戒本。",
        "from": "X74n1499_003:0984a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與梵網相輔而行　第二附禮佛發願受繩牀等法。",
        "from": "X74n1499_003:0984a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三增入開性罪八條。",
        "from": "X74n1499_003:0984a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請菩薩受齋經(拜觀同上)",
        "from": "X74n1499_003:0984a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西晉清信士聶道真譯。",
        "from": "X74n1499_003:0984a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先明三歸悔過。",
        "from": "X74n1499_003:0984a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次護十念。",
        "from": "X74n1499_003:0984a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次十戒。",
        "from": "X74n1499_003:0984a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次解齋法。",
        "from": "X74n1499_003:0984a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迴向淨土。",
        "from": "X74n1499_003:0984a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請菩薩五法懺悔經(拜觀同上)開元附梁錄。",
        "from": "X74n1499_003:0984a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五悔各作偈語。",
        "from": "X74n1499_003:0984b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無序分。",
        "from": "X74n1499_003:0984b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無流通分。",
        "from": "X74n1499_003:0984b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以上大乘律。",
        "from": "X74n1499_003:0984b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水陸道場法輪寶懺卷第三",
        "from": "X74n1499_003:0984b04_0",
        "context_with_next_sentence": false
    }
]