[
    {
        "source_sentence": "佛說大護明大陀羅尼經",
        "from": "T20n1048_001:0044c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天中印度摩伽陀國那爛陀寺",
        "from": "T20n1048_001:0044c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏傳教大師賜紫沙門臣法天",
        "from": "T20n1048_001:0044c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉　詔譯",
        "from": "T20n1048_001:0044c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊住沒哩際疏聚落。",
        "from": "T20n1048_001:0044c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時世尊告尊者阿難陀言。",
        "from": "T20n1048_001:0044c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難陀我今欲去往詣吠舍離城。",
        "from": "T20n1048_001:0044c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊者阿難陀言。",
        "from": "T20n1048_001:0044c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是世尊我今聽命。",
        "from": "T20n1048_001:0044c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是世尊與阿難陀。",
        "from": "T20n1048_001:0044c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往吠舍離城到菴沒羅園已。",
        "from": "T20n1048_001:0044c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告阿難陀言。",
        "from": "T20n1048_001:0044c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難陀汝可入城擇吉祥地。",
        "from": "T20n1048_001:0044c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安因陀羅枳黎駐足正念。",
        "from": "T20n1048_001:0044c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此大護明大陀羅尼及以伽陀曰。",
        "from": "T20n1048_001:0044c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尾薩囉哆(一)尾薩囉哆(二)尾薩囉哆(三)尾薩囉哆(四)尾薩囉哆(五)過現未來　正遍知者　已彼同意　度於世間　一切諸佛　一切獨覺　諸阿羅漢　及以聲聞　一切戒威德　一切真實言　乃至於大梵　迦弭自在者　帝釋及諸天　一切阿素囉　阿素囉使者　乃至部多等　一切皆同意尾薩囉哆(一)尾薩囉哆(二)尾薩囉哆(三)尾薩囉哆(四)尾薩囉哆(五)過現未來一切佛為度世間故應知。",
        "from": "T20n1048_001:0044c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捫佐(上)哆(一)捫佐(上)哆(二)捫佐(上)哆(三)捫佐(上)哆(四)麼(上引)底瑟姹(二合)覩(五)伊(上)底尾喻(二合)跛舍(引)弭也(二合)哆(六)涅(泥逸切)[薩/女]瑳哆(上七)涅(准上)[薩/女]瑳哆(上八)天中之天天所尊重。",
        "from": "T20n1048_001:0044c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入吠舍離世主梵王。",
        "from": "T20n1048_001:0045a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及天帝釋護世四王。",
        "from": "T20n1048_001:0045a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦復來入無數千天眾。",
        "from": "T20n1048_001:0045a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無殃數阿素囉王。",
        "from": "T20n1048_001:0045a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及阿素囉眾亦復來入。",
        "from": "T20n1048_001:0045a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至無量千部多眾等亦復隨入。",
        "from": "T20n1048_001:0045a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命世尊。",
        "from": "T20n1048_001:0045a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀一切眾生諸災難起。",
        "from": "T20n1048_001:0045a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疾疫流行作成就法。",
        "from": "T20n1048_001:0045a05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速令退散使得安樂。",
        "from": "T20n1048_001:0045a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T20n1048_001:0045a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儞哩誐(二合)蹉哆儞哩誐(二合)蹉哆儞哩誐(二合)蹉哆儞哩誐(二合)蹉哆訖史(二合)鉢覽(二合)波羅(引)野哆野儞喻焰訥瑟吒(二合)唧哆(引)曩設野(二合)哆曳(引)昧怛囉(二合)唧哆(引)曩波覽(引)度迦(引)麼(引)囉訖產(二合引)左(引)努挽哩底(二合)迦(引)麼(引)諦(引)悉底(二合)瑟姹(勅降反二合)哆摩艮鉢囉(二合)沒吐(引)路迦(引)努劍波句(引)惹拏(二合)波野底蘇母蘇母蘇母蘇母嚕蘇母嚕蘇母嚕蘇母蘇嚕蘇嚕蘇嚕蘇嚕母母母母鉢囉(二合)尾捨底嚕蘇母　母嚕母嚕母嚕母嚕母嚕母嚕母嚕母嚕護彌　哩彌哩彌哩彌哩彌哩彌哩　母嚕彌哩　母嚕彌哩　母嚕彌哩　母嚕彌哩　母嚕彌哩　母嚕彌哩　母嚕彌哩　母嚕彌哩　母嚕彌哩　母嚕彌哩　母嚕彌哩　母嚕彌哩　母嚕彌哩　母嚕哩底　母嚕哩底　哩哩哩哩哩哩　彌哩彌哩彌哩彌哩彌哩彌哩　底賀悉彌　彌哩底彌哩哩彌彌迦吒　迦囉迦吒建迦囉迦囉迦佐(引)劍迦囉(引)劍迦囉(引)劍迦囉(引)劍迦囉(引)劍迦囉(引)劍迦囉(引)劍迦嚕(引)迦哩底俱哩勢(引)劍迦哩劍迦哩勢底哩底哩哩哩哩哩(引)頗娑(引)哩普哩普哩普哩普哩普哩普哩普囉曩(引)曩他(引)哩哩[口*體]普哩婆哩布薩哆(二合)阿曩(引)他(引)室左(二合)哩鉢囉(二合)儞哩誐(二合)蹉他波羅(引)野哆哩鉢覽(二合)波羅(引)野哆如來正遍知者為於世間一切眾生。",
        "from": "T20n1048_001:0045a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而來入此除災難處。",
        "from": "T20n1048_001:0045b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大慈大悲大喜大捨。",
        "from": "T20n1048_001:0045b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是真言成就伽陀。",
        "from": "T20n1048_001:0045b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切聖眾乃至部多等眾。",
        "from": "T20n1048_001:0045b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而知此法於諸世間為最為上。",
        "from": "T20n1048_001:0045b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復不惜身命為於一切。",
        "from": "T20n1048_001:0045b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "息除疾疫破壞災難。",
        "from": "T20n1048_001:0045b06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增彼善心令得安樂。",
        "from": "T20n1048_001:0045b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為彼世間說解脫道。",
        "from": "T20n1048_001:0045b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說尊重法。",
        "from": "T20n1048_001:0045b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為彼一切而作安樂。",
        "from": "T20n1048_001:0045b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為度世師為諸世間而起慈心。",
        "from": "T20n1048_001:0045b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安諸有情由如赤子。",
        "from": "T20n1048_001:0045b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒常隨逐一切眾生。",
        "from": "T20n1048_001:0045b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由如燈明而增照燭。",
        "from": "T20n1048_001:0045b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處輪迴者令得正道。",
        "from": "T20n1048_001:0045b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸法之智而不闕乏。",
        "from": "T20n1048_001:0045b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言辭清淨與一切樂。",
        "from": "T20n1048_001:0045b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼彼生時生富貴家。",
        "from": "T20n1048_001:0045b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得大無畏福業成就。",
        "from": "T20n1048_001:0045b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼所生地一切震動一切有情咸皆歡喜。",
        "from": "T20n1048_001:0045b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼菩提場六種震動。",
        "from": "T20n1048_001:0045b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魔及魔民悉皆愁惱。",
        "from": "T20n1048_001:0045b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂彼牟尼當轉法輪說四聖諦以調伏法降彼外道。",
        "from": "T20n1048_001:0045b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生必當作佛。",
        "from": "T20n1048_001:0045b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安樂人天及天帝釋。",
        "from": "T20n1048_001:0045b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至安樂一切部多眾等。",
        "from": "T20n1048_001:0045b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切時使得見佛。",
        "from": "T20n1048_001:0045b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得賢聖意獲福無量。",
        "from": "T20n1048_001:0045b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼所求事咸得成就。",
        "from": "T20n1048_001:0045b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得菩提故安樂四道。",
        "from": "T20n1048_001:0045b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復令眾生晝夜日中。",
        "from": "T20n1048_001:0045b19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及一切處悉皆安樂。",
        "from": "T20n1048_001:0045b19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至蠕動一切有情各令快樂。",
        "from": "T20n1048_001:0045b20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復令眾生不造惡業見諸賢聖。",
        "from": "T20n1048_001:0045b20_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "部多來集所住之地及彼虛空。",
        "from": "T20n1048_001:0045b21_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸眾生常加慈護。",
        "from": "T20n1048_001:0045b22_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於晝於夜於一切時依法而住。",
        "from": "T20n1048_001:0045b22_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是尊者阿難陀。",
        "from": "T20n1048_001:0045b23_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白世尊言如是世尊我今聽命。",
        "from": "T20n1048_001:0045b23_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是安因陀羅枳黎已。",
        "from": "T20n1048_001:0045b24_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸眾生消除災難駐足正念。",
        "from": "T20n1048_001:0045b25_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此大護明大陀羅尼成就伽陀曰。",
        "from": "T20n1048_001:0045b25_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尾薩囉哆(一)尾薩囉哆(二)尾薩囉哆(三)尾薩囉哆(四)尾薩囉哆(五)過現未來　一切諸佛　以正遍知　願垂加護　一切緣覺威德加護　一切阿羅漢威德加護　一切聲聞威德加護　一切戒威德加護　一切真實語威德加護　獨住梵天威德加護　迦弭濕嚩囉威德加護　憍釋迦威德加護　諸天眾威德加護　阿素囉王威德加護　一切阿素囉威力加護　阿素使者威力加護　一切部多威力加護　願佛及聖賢　為一切眾生　咸垂威德力　使災難悉除儞哩誐(二合)蹉哆儞哩誐(二合)蹉哆儞哩誐(二合)蹉哆儞哩誐(二合)蹉哆訖史(二合)鉢囉(二合)波羅(引)野哆野儞喻焰訥瑟吒(二合)唧哆(引)曩設野(二合)哆曳(引)昧(引)怛囉(二合)唧哆(引)曩波覽(引)度迦(引)摩(引)囉訖產(二合引)左(引)努挽哩底(二合)覩迦(引)麼(引)悉諦(二合)底瑟姹(勅降反二合)覩摩艮左正遍知者為世間　除災難故而來入素母素母素母素母嚕素母嚕素母嚕素母嚕素母母嚕母嚕母嚕母嚕母嚕母嚕母嚕母嚕母嚕母嚕母嚕母嚕母嚕母嚕母嚕彌哩尾娑囉哆尾娑囉哆尾娑囉哆尾娑囉哆沒吐路(引)迦(引)努劍波句(引)惹拏(二合)波野底夢左哆夢左哆尾儞野(二合)鉢囉(二合)尾捨底伊底喻(二合)波舍(引)弭野(引)覩儞哩誐(二合)蹉哆儞哩誐(二合)蹉哆彼世界主大梵天王而於天中最尊最上。",
        "from": "T20n1048_001:0045b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及天帝釋護世四王亦復來入。",
        "from": "T20n1048_001:0045c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無數賢聖無數阿素囉王無數阿素囉眾。",
        "from": "T20n1048_001:0045c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至無數部多之眾。",
        "from": "T20n1048_001:0045c26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸覩世尊而陳歸命。",
        "from": "T20n1048_001:0045c27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當為一切眾生消除災難。",
        "from": "T20n1048_001:0045c27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儞哩誐(二合)蹉哆儞哩誐(二合)蹉哆儞哩誐(二合)蹉哆儞哩誐(二合)蹉哆若人非人起於惡心。",
        "from": "T20n1048_001:0045c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行不饒益作諸災難。",
        "from": "T20n1048_001:0046a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當速退散勿復興惡。",
        "from": "T20n1048_001:0046a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不退者當自毀滅。",
        "from": "T20n1048_001:0046a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒起慈心願常守護。",
        "from": "T20n1048_001:0046a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來救世者應當如是知。",
        "from": "T20n1048_001:0046a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "素母嚕　素母嚕　素母嚕　素母嚕　素母嚕　母嚕母嚕母嚕母嚕母嚕　素嚕素嚕素嚕素嚕素嚕素嚕素嚕素嚕　彌哩彌哩彌哩彌哩　母嚕彌哩　母嚕彌哩　母嚕彌哩　母嚕彌哩　母嚕彌哩　母嚕彌哩　母嚕彌哩　母嚕彌哩　母嚕彌哩　母嚕彌哩　母嚕彌哩　母嚕彌哩　母嚕彌哩　嚕素彌彌哩底彌哩悉悉彌劍迦囉(引)迦囉迦吒迦囉迦左(引)劍迦囉劍迦囉劍迦囉劍迦囉劍迦囉劍迦囉劍迦囉劍迦囉劍迦嚕(引)迦哩底俱哩勢(引)劍迦哩迦哩勢(引)底哩哩哩哩哩哩底哩(引)頗娑(引)哩[口*普]哩[口*普]哩[口*普]哩[口*普]哩[口*普]哩[口*普]哩[口*普]哩野儞喻焰阿曩(引)他(引)曩(引)他(引)儞[口*體]母哩母哩儞哩誐(二合)蹉哆儞哩誐(二合)蹉哆一切冤家當速退散。",
        "from": "T20n1048_001:0046a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使一切眾生願皆隨意。",
        "from": "T20n1048_001:0046a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來以大慈大悲大喜大捨。",
        "from": "T20n1048_001:0046a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是真言成就伽陀。",
        "from": "T20n1048_001:0046a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為一切天眾及部多眾。",
        "from": "T20n1048_001:0046a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得最上智慧及法義理。",
        "from": "T20n1048_001:0046a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸世間除諸災沴。",
        "from": "T20n1048_001:0046a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破滅自他貪毒之過。",
        "from": "T20n1048_001:0046a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以彼善心說一切法。",
        "from": "T20n1048_001:0046a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼世間諸眾生等。",
        "from": "T20n1048_001:0046a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見解脫道獲得安樂。",
        "from": "T20n1048_001:0046a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來三界之師。",
        "from": "T20n1048_001:0046a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈護眾生同彼赤子。",
        "from": "T20n1048_001:0046a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復如燈燭照明眾生。",
        "from": "T20n1048_001:0046a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住輪迴者得見正道。",
        "from": "T20n1048_001:0046a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善說法要言詞清淨。",
        "from": "T20n1048_001:0046a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切鬼神而為證明。",
        "from": "T20n1048_001:0046a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又彼彼生得大無畏。",
        "from": "T20n1048_001:0046a25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處於富貴資財廣多饒益一切。",
        "from": "T20n1048_001:0046a25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得義成就得福成就。",
        "from": "T20n1048_001:0046a26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又彼生時一切震動。",
        "from": "T20n1048_001:0046a26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生歡喜獲得安樂。",
        "from": "T20n1048_001:0046a27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼菩提場六種震動。",
        "from": "T20n1048_001:0046a27_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魔及魔民一切憂慼。",
        "from": "T20n1048_001:0046a28_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂彼牟尼轉正法輪。",
        "from": "T20n1048_001:0046a28_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說四聖諦以調伏法。",
        "from": "T20n1048_001:0046a29_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降彼外道一切眾生。",
        "from": "T20n1048_001:0046a29_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安樂諸天及天帝釋。",
        "from": "T20n1048_001:0046b01_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至一切部多之眾。",
        "from": "T20n1048_001:0046b01_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸有情於一切時方。",
        "from": "T20n1048_001:0046b02_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "承佛福故得天意故。",
        "from": "T20n1048_001:0046b02_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若彼求事咸得成就得安樂故得菩提故。",
        "from": "T20n1048_001:0046b03_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願諸眾生皆悉賢善。",
        "from": "T20n1048_001:0046b04_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離眾罪無造罪者。",
        "from": "T20n1048_001:0046b04_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至蠕動一切有情。",
        "from": "T20n1048_001:0046b05_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "災沴悉除無諸病惱。",
        "from": "T20n1048_001:0046b05_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所行之處所住之處。",
        "from": "T20n1048_001:0046b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於晝於夜及日中時一切安樂。",
        "from": "T20n1048_001:0046b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至部多來集。",
        "from": "T20n1048_001:0046b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住於地上及住虛空。",
        "from": "T20n1048_001:0046b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常發慈心依法而住。",
        "from": "T20n1048_001:0046b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此界他方一切諸佛。",
        "from": "T20n1048_001:0046b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至諸天常來衛護得大安樂。",
        "from": "T20n1048_001:0046b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是陀羅尼已。",
        "from": "T20n1048_001:0046b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天眾等歡喜奉行。",
        "from": "T20n1048_001:0046b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說大護明大陀羅尼經",
        "from": "T20n1048_001:0046b11_0",
        "context_with_next_sentence": false
    }
]