[
    {
        "source_sentence": "觀自在菩薩化身蘘麌哩曳童女",
        "from": "T21n1264a_001:0292b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "銷伏毒害陀羅尼經",
        "from": "T21n1264a_001:0292b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T21n1264a_001:0292b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空諡大鑒",
        "from": "T21n1264a_001:0292b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正號大廣智大興善寺三藏沙門",
        "from": "T21n1264a_001:0292b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空奉　詔譯",
        "from": "T21n1264a_001:0292b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T21n1264a_001:0292b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時薄伽梵。",
        "from": "T21n1264a_001:0292b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住舍衛國祇陀園。",
        "from": "T21n1264a_001:0292b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大苾芻眾千二百五十人俱。",
        "from": "T21n1264a_001:0292b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又與無數菩薩摩訶薩。",
        "from": "T21n1264a_001:0292b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及天龍夜叉八部眾等皆來雲集。",
        "from": "T21n1264a_001:0292b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時如來告諸大眾。",
        "from": "T21n1264a_001:0292b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我念往昔。",
        "from": "T21n1264a_001:0292b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住雪山北遊香醉山。",
        "from": "T21n1264a_001:0292b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見一童女。",
        "from": "T21n1264a_001:0292b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百福相好莊嚴其身。",
        "from": "T21n1264a_001:0292b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鹿皮為衣。",
        "from": "T21n1264a_001:0292b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以諸毒蛇而為瓔珞。",
        "from": "T21n1264a_001:0292b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將諸毒虫虎狼師子。",
        "from": "T21n1264a_001:0292b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後圍遶常為伴侶。",
        "from": "T21n1264a_001:0292b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲毒菓漿食毒草菓。",
        "from": "T21n1264a_001:0292b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼女見我作如是言。",
        "from": "T21n1264a_001:0292b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仁者聽我宣說一大真言。",
        "from": "T21n1264a_001:0292b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除世間一切諸毒。",
        "from": "T21n1264a_001:0292b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人聞此呪及念我名者。",
        "from": "T21n1264a_001:0292b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不被一切諸毒所害。",
        "from": "T21n1264a_001:0292b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時童女為我說呪。",
        "from": "T21n1264a_001:0292b28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我得聞已。",
        "from": "T21n1264a_001:0292b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常持此呪饒益有情。",
        "from": "T21n1264a_001:0292b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今當說真言曰。",
        "from": "T21n1264a_001:0292b29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也他唵(引)壹哩蜜帝(引)底(丁壹反)哩蜜帝壹哩帝哩蜜帝(引)努迷努麼(引)哩(引)曳(引)訥泚訥蹉(引)哩曳得羯囉抳(尼貞反)嚩揭囉抳揭濕弭[口*(隸-木+士)](引)揭濕弭囉目訖帝(二合)惡祇惡伽儞惡伽娜惡伽曩伽儞壹哩曳壹哩壹哩曳阿佉夜(引)曳姶(烏合反)播夜曳濕吠帝(引)濕吠多頓嬭阿曩(引)努囉乞砂(二合)伺嚩(二合)訶佛言諸苾芻。",
        "from": "T21n1264a_001:0292c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人一聞此呪。",
        "from": "T21n1264a_001:0292c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "却後七年遠離諸毒。",
        "from": "T21n1264a_001:0292c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人常念者。",
        "from": "T21n1264a_001:0292c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切毒虫及諸毒藥悉不能害。",
        "from": "T21n1264a_001:0292c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有毒蛇來咬此人者。",
        "from": "T21n1264a_001:0292c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其蛇即爛壞。",
        "from": "T21n1264a_001:0292c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其有受持此呪法者。",
        "from": "T21n1264a_001:0292c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以白物先供養師。",
        "from": "T21n1264a_001:0292c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後受之法必成就。",
        "from": "T21n1264a_001:0292c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿於蛇邊念此呪。",
        "from": "T21n1264a_001:0292c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其蛇即死死已令其甦活呪曰。",
        "from": "T21n1264a_001:0292c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也他伊哩彌哩誐囉彌左斫骨嚕斫骨魯斫骨論斫骨論誐囉彌抳句吒句吒曳帝娑普吒娑普吒曳帝那揭黎那伽折迦蘭底哩壹掣囉佛沙尸怛囉卑怛囉悉普吒悉普吒伺嚩(二合)訶若人念一遍。",
        "from": "T21n1264a_001:0292c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經年諸毒不中。",
        "from": "T21n1264a_001:0292c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能常持非言外毒消滅。",
        "from": "T21n1264a_001:0292c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦除內三毒。",
        "from": "T21n1264a_001:0292c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若惡瘡發背癰腫等病。",
        "from": "T21n1264a_001:0292c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唾之念呪即差。",
        "from": "T21n1264a_001:0292c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切惡獸來。",
        "from": "T21n1264a_001:0292c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但念七遍即過不能為害。",
        "from": "T21n1264a_001:0292c22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若為中毒。",
        "from": "T21n1264a_001:0292c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處於中指上頭第一節文。",
        "from": "T21n1264a_001:0292c23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左右手遶瘡。",
        "from": "T21n1264a_001:0292c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逆日轉念呪三七遍即差。",
        "from": "T21n1264a_001:0292c24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常念不得高聲。",
        "from": "T21n1264a_001:0292c25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高聲則有毒虫必死。",
        "from": "T21n1264a_001:0292c25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若惡虫死已。",
        "from": "T21n1264a_001:0292c25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此呪解之即活。",
        "from": "T21n1264a_001:0292c26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念三七遍。",
        "from": "T21n1264a_001:0292c26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解毒呪曰。",
        "from": "T21n1264a_001:0292c26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也他壹哩彌哩議哩那伽斫伽蘭底哩壹掣囉合沙鷄怛吒悉怛吒担吒悉怛吒伺嚩(二合)訶若人患發背。",
        "from": "T21n1264a_001:0292c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逆日轉遶倒書一匝即差。",
        "from": "T21n1264a_001:0293a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡物自出大有神驗。",
        "from": "T21n1264a_001:0293a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說是語已。",
        "from": "T21n1264a_001:0293a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大眾歡喜授時而起。",
        "from": "T21n1264a_001:0293a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘘麌哩曳童女銷伏毒害呪經",
        "from": "T21n1264a_001:0293a05_0",
        "context_with_next_sentence": false
    }
]