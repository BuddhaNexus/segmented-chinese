[
    {
        "source_sentence": "大勝金剛佛頂念誦儀軌",
        "from": "T19n0980_001:0410b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南天竺三藏金剛智奉　詔譯",
        "from": "T19n0980_001:0410b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊為末世一切眾生故。",
        "from": "T19n0980_001:0410b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現五種光明。",
        "from": "T19n0980_001:0410b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從頂上放火光。",
        "from": "T19n0980_001:0410b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照三界所有惡魔鬼神等不信佛教者。",
        "from": "T19n0980_001:0410b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同時悉焚燒。",
        "from": "T19n0980_001:0410b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛手菩薩見是相已。",
        "from": "T19n0980_001:0410b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白世尊言。",
        "from": "T19n0980_001:0410b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來智火被燒所有天魔鬼神於眾生生害者。",
        "from": "T19n0980_001:0410b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉恐怖不得安樂。",
        "from": "T19n0980_001:0410b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲說何法。",
        "from": "T19n0980_001:0410b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願如來演說。",
        "from": "T19n0980_001:0410b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時大毘盧遮那佛告金剛手菩薩言。",
        "from": "T19n0980_001:0410b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有明王名大勝佛頂心呪。",
        "from": "T19n0980_001:0410c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦一遍所求滿足。",
        "from": "T19n0980_001:0410c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方諸佛中最勝明王。",
        "from": "T19n0980_001:0410c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有金剛子。",
        "from": "T19n0980_001:0410c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦滿五萬遍。",
        "from": "T19n0980_001:0410c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸佛大菩薩天龍天女眾。",
        "from": "T19n0980_001:0410c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晝夜共守護令成所願。",
        "from": "T19n0980_001:0410c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時如來說明曰。",
        "from": "T19n0980_001:0410c04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵麼賀縛日羅(二合)瑟抳(二合)灑吽(引)怛路(二合)瀨利(二合)惡(入)吽(引)二手內縛忍願屈如頂。",
        "from": "T19n0980_001:0410c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名根本大印。",
        "from": "T19n0980_001:0410c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有末法轉時。",
        "from": "T19n0980_001:0410c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有諸眾生常結此印持念真言者。",
        "from": "T19n0980_001:0410c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切兵力不能相害。",
        "from": "T19n0980_001:0410c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水火不焚漂。",
        "from": "T19n0980_001:0410c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不便一切惡魔惡鬼。",
        "from": "T19n0980_001:0410c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛胎藏聖眾坐臥咸來增擁護。",
        "from": "T19n0980_001:0410c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次說畫像法。",
        "from": "T19n0980_001:0410c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於黑月房宿取清淨絹素。",
        "from": "T19n0980_001:0410c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫大勝明王。",
        "from": "T19n0980_001:0410c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戴五佛冠。",
        "from": "T19n0980_001:0410c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出現十二臂。",
        "from": "T19n0980_001:0410c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住智拳印。",
        "from": "T19n0980_001:0410c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執五[月*古]青蓮花寶珠羯磨鉤索鎖鈴利劍輪等印。",
        "from": "T19n0980_001:0410c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安住千葉白蓮華。",
        "from": "T19n0980_001:0410c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身色如白光。",
        "from": "T19n0980_001:0410c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍身成火光。",
        "from": "T19n0980_001:0410c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四角畫四明王。",
        "from": "T19n0980_001:0410c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東南角畫不動金剛。",
        "from": "T19n0980_001:0410c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西南角馬頭金剛。",
        "from": "T19n0980_001:0410c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西北角六足金剛。",
        "from": "T19n0980_001:0410c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東北角藥叉金剛。",
        "from": "T19n0980_001:0410c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫此四忿怒王。",
        "from": "T19n0980_001:0410c17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各執器杖。",
        "from": "T19n0980_001:0410c17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現極猛利勢。",
        "from": "T19n0980_001:0410c17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次畫八方天王。",
        "from": "T19n0980_001:0410c18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各卛眷屬恭敬圍繞。",
        "from": "T19n0980_001:0410c18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有真言行者欲供養者。",
        "from": "T19n0980_001:0410c18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於淨室及山林靜靜處。",
        "from": "T19n0980_001:0410c19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒香散時花。",
        "from": "T19n0980_001:0410c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦真言五十萬遍。",
        "from": "T19n0980_001:0410c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我及八佛頂無量天菩薩金剛天等。",
        "from": "T19n0980_001:0410c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現身常隨意奉仕。",
        "from": "T19n0980_001:0410c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生每日誦持此真言。",
        "from": "T19n0980_001:0410c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於現在生之中。",
        "from": "T19n0980_001:0410c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勝一切天人。",
        "from": "T19n0980_001:0410c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當未世中。",
        "from": "T19n0980_001:0410c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身如遍照尊。",
        "from": "T19n0980_001:0410c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持此印真言者。",
        "from": "T19n0980_001:0410c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能成就十億種所願。",
        "from": "T19n0980_001:0410c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復若有行者。",
        "from": "T19n0980_001:0410c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作種種降魔及一切阿尾捨法者。",
        "from": "T19n0980_001:0410c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我作十千金剛童子。",
        "from": "T19n0980_001:0410c25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則隨意持誦者常住。",
        "from": "T19n0980_001:0410c26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若悲心莫他相憎。",
        "from": "T19n0980_001:0410c26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀吽字心上。",
        "from": "T19n0980_001:0410c27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行住坐臥住本尊三摩地。",
        "from": "T19n0980_001:0410c27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就一切願。",
        "from": "T19n0980_001:0410c27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有情見者。",
        "from": "T19n0980_001:0410c28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉歡喜愛念。",
        "from": "T19n0980_001:0410c28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此人如遍照尊。",
        "from": "T19n0980_001:0410c28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量諸佛常來覆護不捨。",
        "from": "T19n0980_001:0410c29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言行者不起貪瞋癡等煩惱。",
        "from": "T19n0980_001:0410c29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深歸命三寶。",
        "from": "T19n0980_001:0411a01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以善根迴向六道者。",
        "from": "T19n0980_001:0411a01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必速證無上菩提。",
        "from": "T19n0980_001:0411a02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者諸穢處用烏瑟摳摩金剛呪印加持五處。",
        "from": "T19n0980_001:0411a02_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右手四指作拳。",
        "from": "T19n0980_001:0411a03_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直竪彈指。",
        "from": "T19n0980_001:0411a04_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密言曰。",
        "from": "T19n0980_001:0411a04_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵俱路(二合)馱曩吽弱",
        "from": "T19n0980_001:0411a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大勝金剛佛頂念誦儀軌",
        "from": "T19n0980_001:0411a06_0",
        "context_with_next_sentence": false
    }
]