[
    {
        "source_sentence": "一字頂輪王瑜伽觀行儀軌一",
        "from": "T19n0955_001:0313b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卷",
        "from": "T19n0955_001:0313b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿",
        "from": "T19n0955_001:0313b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "肅國公食邑三千戶賜紫贈司",
        "from": "T19n0955_001:0313b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空謚大鑒正號大廣智大興善",
        "from": "T19n0955_001:0313b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寺三藏沙門不空奉　詔譯",
        "from": "T19n0955_001:0313b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言行者若求安怛陀娜。",
        "from": "T19n0955_001:0313b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應作是念。",
        "from": "T19n0955_001:0313b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我云何能速取成就。",
        "from": "T19n0955_001:0313b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當習是三摩地。",
        "from": "T19n0955_001:0313c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂一切法無色。",
        "from": "T19n0955_001:0313c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如虛空性自成就。",
        "from": "T19n0955_001:0313c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是勝解。",
        "from": "T19n0955_001:0313c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當如本教淨治地等。",
        "from": "T19n0955_001:0313c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是三摩地。",
        "from": "T19n0955_001:0313c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　普遍三千界　　充滿智成水",
        "from": "T19n0955_001:0313c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　有大寶蓮華　　開敷在水中",
        "from": "T19n0955_001:0313c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　寶莖如須彌　　上方珠網覆",
        "from": "T19n0955_001:0313c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　花上有大寶　　八柱以莊嚴",
        "from": "T19n0955_001:0313c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　想成寶樓閣　　四門當四方",
        "from": "T19n0955_001:0313c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　珠網蓮鐸等　　及妙拂莊嚴",
        "from": "T19n0955_001:0313c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　半滿月珠瓔　　垂寶間錯帶",
        "from": "T19n0955_001:0313c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　是大寶樓閣　　遍滿於有頂",
        "from": "T19n0955_001:0313c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　應隨力思惟　　觀供養雲海",
        "from": "T19n0955_001:0313c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　先所觀樓中　　有悅意白繖",
        "from": "T19n0955_001:0313c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　以眾寶端飾　　寶瓔半滿月",
        "from": "T19n0955_001:0313c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　妙拂等莊嚴　　繖上有大寶",
        "from": "T19n0955_001:0313c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　普遍發光明　　皆散雨眾寶",
        "from": "T19n0955_001:0313c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　真言智觀行　　於彼樓閣中",
        "from": "T19n0955_001:0313c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　金剛師子座　　寶蓮華莊嚴",
        "from": "T19n0955_001:0313c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　珠網蓮繒磬　　白拂等莊嚴",
        "from": "T19n0955_001:0313c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一切欲喜樂　　於中圍繞住",
        "from": "T19n0955_001:0313c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　不遠於身前　　應想第二座",
        "from": "T19n0955_001:0313c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一切皆如上　　唯無師子座",
        "from": "T19n0955_001:0313c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　角門於四處　　嬉戲焚香等",
        "from": "T19n0955_001:0313c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　次作圓具法　　云何作圓具",
        "from": "T19n0955_001:0313c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　先應觀諸法　　無性為自性",
        "from": "T19n0955_001:0313c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　物我同一體　　然後加諸法",
        "from": "T19n0955_001:0313c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　云何加諸法　　生老病憂死",
        "from": "T19n0955_001:0313c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　攪擾於心田　　如是思惟已",
        "from": "T19n0955_001:0313c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　應生大悲愍　　由是生智心",
        "from": "T19n0955_001:0313c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　則是光明心(光明心即菩提心也)　以大悲熏成　　其體如滿月　離能取所取　　菩提心已生　應住身口意　　所明體加持　此名圓具法(即如瑜伽中成身次第也)　族明勝次第知已(族五部明五部真言也)　眾真言威德知已　自住部王法(謂一字頂輪也)　則為當族王　　先應印已成(謂大印也)　次明應思惟　　由身口意體　想身為諸佛　　於心想月輪　無垢猶如佛　　真言者應知　有種種光明　　皆從月形生　遍至無量界　　復入行人心　堅固為五峰　　變為金剛形　想在彼掌中　　復出種種光　其光皆遍滿　　至於無邊界　而作佛遊戲　　還來瑜岐身　如是皆入已　　成普賢大色　應觀大菩薩　　諸相皆成就　一切莊嚴具　　蓮華鬘灌頂　自成摩訶薩　　從瑜岐心生　持真言弓箭　　依住於月輪　或持鬘而住　　或四方安居　隨力觀身前　　諸眾生大樂　最上令成就　　一切印嚴手　惠施於一切　　諸有情如願　并熙怡為舞　　金剛鬘流出　為左於腰側　　住右作舞勢　安立眾生利　　如自明隨形　諸眾生所見　　皆令成調伏　作如是加持　　利益於眾生　及餘諸利益　　攞寫等供養　而已陳供養　　自身自所尊　真言字相應　　隨力而念誦　如聲色相應　　以字為花鬘　智者應思惟　　依瑜伽相應　於聲我今說　　一切最勝成　如來蓮華部　　商佉聲念誦　我讚如雷聲　　分明稱吽字　我說金剛部亦通摩醯首羅。",
        "from": "T19n0955_001:0314a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦者成就南摩尼羯磨。",
        "from": "T19n0955_001:0314b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此部念誦。",
        "from": "T19n0955_001:0314b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如鈴鐸聲。",
        "from": "T19n0955_001:0314b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如箜篌聲笛聲。",
        "from": "T19n0955_001:0314b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如舞動瓔珞聲。",
        "from": "T19n0955_001:0314b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其聲如孔雀鳴。",
        "from": "T19n0955_001:0314b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如一切部法中。",
        "from": "T19n0955_001:0314b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相應一切義。",
        "from": "T19n0955_001:0314b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成如是音聲。",
        "from": "T19n0955_001:0314b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而作念誦。",
        "from": "T19n0955_001:0314b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與真言相應。",
        "from": "T19n0955_001:0314b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言者隨聲。",
        "from": "T19n0955_001:0314b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應思惟其義。",
        "from": "T19n0955_001:0314b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不久當成就。",
        "from": "T19n0955_001:0314b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此通一切部此是聲念誦儀軌。",
        "from": "T19n0955_001:0314b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我說色念誦。",
        "from": "T19n0955_001:0314b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今說一切。",
        "from": "T19n0955_001:0314b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色者說為印。",
        "from": "T19n0955_001:0314b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與此相應轉(謂運為也)善思惟其明。",
        "from": "T19n0955_001:0314b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當安其胸臆。",
        "from": "T19n0955_001:0314b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印焰明觀察。",
        "from": "T19n0955_001:0314b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身中出金剛。",
        "from": "T19n0955_001:0314b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甘露而灌灑。",
        "from": "T19n0955_001:0314b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令本天喜悅。",
        "from": "T19n0955_001:0314b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得隱自身形。",
        "from": "T19n0955_001:0314b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大勤勇我說。",
        "from": "T19n0955_001:0314b21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色念誦由此真言者。",
        "from": "T19n0955_001:0314b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知以印令成就。",
        "from": "T19n0955_001:0314b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不久易餘身。",
        "from": "T19n0955_001:0314b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我說瑜伽。",
        "from": "T19n0955_001:0314b22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦如昔所說。",
        "from": "T19n0955_001:0314b23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其明應思惟。",
        "from": "T19n0955_001:0314b23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安置於自處。",
        "from": "T19n0955_001:0314b23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養以如教。",
        "from": "T19n0955_001:0314b24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "純相應念誦離心喉頂舌鼻[口*(口/咢)]及離念內外。",
        "from": "T19n0955_001:0314b24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯法相應耳。",
        "from": "T19n0955_001:0314b25_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應依於聲。",
        "from": "T19n0955_001:0314b25_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此名為金剛。",
        "from": "T19n0955_001:0314b26_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瑜伽念誦儀。",
        "from": "T19n0955_001:0314b26_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若知真實體。",
        "from": "T19n0955_001:0314b26_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當知成就。",
        "from": "T19n0955_001:0314b26_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而獲得常恒。",
        "from": "T19n0955_001:0314b27_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此瑜伽法。",
        "from": "T19n0955_001:0314b27_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相應而住之。",
        "from": "T19n0955_001:0314b27_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以菩提勝心。",
        "from": "T19n0955_001:0314b28_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為成就不久。",
        "from": "T19n0955_001:0314b28_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆當得如意。",
        "from": "T19n0955_001:0314b28_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其中字念誦。",
        "from": "T19n0955_001:0314b29_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如獲我今說。",
        "from": "T19n0955_001:0314b29_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是積資糧。",
        "from": "T19n0955_001:0314b29_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言應誦持。",
        "from": "T19n0955_001:0314b29_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以文字為色。",
        "from": "T19n0955_001:0314c01_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應分別觀之。",
        "from": "T19n0955_001:0314c01_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作念誦事業。",
        "from": "T19n0955_001:0314c01_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月行列意生。",
        "from": "T19n0955_001:0314c02_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明字與之俱。",
        "from": "T19n0955_001:0314c02_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安本尊胸臆。",
        "from": "T19n0955_001:0314c02_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不久得安怛陀娜。",
        "from": "T19n0955_001:0314c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於月以月合。",
        "from": "T19n0955_001:0314c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切字色。",
        "from": "T19n0955_001:0314c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應於上思惟。",
        "from": "T19n0955_001:0314c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光明輪莊嚴。",
        "from": "T19n0955_001:0314c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行列不間斷。",
        "from": "T19n0955_001:0314c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如以線穿珠。",
        "from": "T19n0955_001:0314c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金色以為光。",
        "from": "T19n0955_001:0314c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晃曜本尊身。",
        "from": "T19n0955_001:0314c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如彼彼月字。",
        "from": "T19n0955_001:0314c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殊勝妙真言。",
        "from": "T19n0955_001:0314c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以成真言者。",
        "from": "T19n0955_001:0314c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則彼彼喜悅。",
        "from": "T19n0955_001:0314c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "力命增益更熾盛。",
        "from": "T19n0955_001:0314c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "意光明相應。",
        "from": "T19n0955_001:0314c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我說為文字。",
        "from": "T19n0955_001:0314c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行列之念誦。",
        "from": "T19n0955_001:0314c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今遍諸儀則成就物光暉。",
        "from": "T19n0955_001:0314c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今說。",
        "from": "T19n0955_001:0314c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦明念誦者。",
        "from": "T19n0955_001:0314c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作彼以光明成就於諸物。",
        "from": "T19n0955_001:0314c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應生念誦相應儀。",
        "from": "T19n0955_001:0314c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切真言皆得成。",
        "from": "T19n0955_001:0314c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一儀與相應。",
        "from": "T19n0955_001:0314c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應作識清淨。",
        "from": "T19n0955_001:0314c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清淨為心識。",
        "from": "T19n0955_001:0314c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智者然當入成就。",
        "from": "T19n0955_001:0314c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以身以語以意。",
        "from": "T19n0955_001:0314c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依如是物而住。",
        "from": "T19n0955_001:0314c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應知四支法。",
        "from": "T19n0955_001:0314c13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瑜伽法成就。",
        "from": "T19n0955_001:0314c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中說為意。",
        "from": "T19n0955_001:0314c13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲通及地等。",
        "from": "T19n0955_001:0314c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以身現諸身(決云謂隨眾生意所樂者)。",
        "from": "T19n0955_001:0314c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以語辯一切。",
        "from": "T19n0955_001:0314c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烟焰等成就。",
        "from": "T19n0955_001:0314c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我說物成就。",
        "from": "T19n0955_001:0314c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中身成就者。",
        "from": "T19n0955_001:0314c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種類有多種。",
        "from": "T19n0955_001:0314c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四印及餘輪壇。",
        "from": "T19n0955_001:0314c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瑜伽者應當盡其所解者而作大曼荼羅。",
        "from": "T19n0955_001:0314c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曼荼羅儀軌成辦中央三昧形。",
        "from": "T19n0955_001:0314c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應安軟坐(西方或以赤麞皮中安氈花而坐)。",
        "from": "T19n0955_001:0314c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而安坐。",
        "from": "T19n0955_001:0314c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切瑜伽三摩地相應晝夜等至(謂入定也)真言者住與慧念相應。",
        "from": "T19n0955_001:0314c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中夜或明相現時。",
        "from": "T19n0955_001:0314c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或三更或明相決定當成就。",
        "from": "T19n0955_001:0314c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或地及神通成就。",
        "from": "T19n0955_001:0314c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來說。",
        "from": "T19n0955_001:0314c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今說意成就。",
        "from": "T19n0955_001:0314c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現身而獲之。",
        "from": "T19n0955_001:0314c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此印加持身。",
        "from": "T19n0955_001:0314c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今為大印。",
        "from": "T19n0955_001:0314c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應作是思惟。",
        "from": "T19n0955_001:0314c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於成就及曼荼羅念誦者。",
        "from": "T19n0955_001:0314c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨心則成就。",
        "from": "T19n0955_001:0315a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我說成就相。",
        "from": "T19n0955_001:0315a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口身或出光明暖烟及增等。",
        "from": "T19n0955_001:0315a02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見昇空去。",
        "from": "T19n0955_001:0315a02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就相應知於身成就。",
        "from": "T19n0955_001:0315a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我廣說已。",
        "from": "T19n0955_001:0315a03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我說語成就法。",
        "from": "T19n0955_001:0315a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如獲次第說。",
        "from": "T19n0955_001:0315a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應為先行法已。",
        "from": "T19n0955_001:0315a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前法應作(謂自建立已來乃至大印等)口心印上住蓮華。",
        "from": "T19n0955_001:0315a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應知於心間於上住商佉(謂在花上也)商佉中出聲。",
        "from": "T19n0955_001:0315a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相續無間斷。",
        "from": "T19n0955_001:0315a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於蓮華中發生金剛舌。",
        "from": "T19n0955_001:0315a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以白色(謂作赤色上有金剛)舌上或想佛或寶或蓮華或羯磨金剛或餘部印契。",
        "from": "T19n0955_001:0315a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或一月及兩月三四及五月乃至八箇月觀想。",
        "from": "T19n0955_001:0315a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此壇結加趺坐。",
        "from": "T19n0955_001:0315a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常以本寂靜相應。",
        "from": "T19n0955_001:0315a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分明觀商佉聲已。",
        "from": "T19n0955_001:0315a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即依蓮華界。",
        "from": "T19n0955_001:0315a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從花蘂出聲量如微塵。",
        "from": "T19n0955_001:0315a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其聲出至咽。",
        "from": "T19n0955_001:0315a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次至於舌。",
        "from": "T19n0955_001:0315a12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便即成其字。",
        "from": "T19n0955_001:0315a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舌字出光焰。",
        "from": "T19n0955_001:0315a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "焰猛而普遍。",
        "from": "T19n0955_001:0315a13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以聲滿虛空。",
        "from": "T19n0955_001:0315a13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者徑定誦或一日及一夜。",
        "from": "T19n0955_001:0315a14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中夜或後夜。",
        "from": "T19n0955_001:0315a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從舌出光明。",
        "from": "T19n0955_001:0315a15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其光有大聲。",
        "from": "T19n0955_001:0315a15_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或從心從脣及齒。",
        "from": "T19n0955_001:0315a15_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出間錯光明。",
        "from": "T19n0955_001:0315a16_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見如是者當知得悉地。",
        "from": "T19n0955_001:0315a16_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此語相應能摧諸異宗令他發淨信。",
        "from": "T19n0955_001:0315a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能為眾生利益。",
        "from": "T19n0955_001:0315a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是瑜伽相應念誦。",
        "from": "T19n0955_001:0315a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有比丘嚩迦者吒。",
        "from": "T19n0955_001:0315a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當得口成就。",
        "from": "T19n0955_001:0315a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於間錯出。",
        "from": "T19n0955_001:0315a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七日而成語成就儀則。",
        "from": "T19n0955_001:0315a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今說物成就。",
        "from": "T19n0955_001:0315a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前瑜伽應成就。",
        "from": "T19n0955_001:0315a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應取餘部物等或用餘部印及真言三簸多(謂以杓應薩嚩拄物便瀉爐中及餘聲未盡還拄物)行相作已。",
        "from": "T19n0955_001:0315a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智者於此曼荼羅。",
        "from": "T19n0955_001:0315a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即入羯磨三昧耶。",
        "from": "T19n0955_001:0315a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自為一切羯磨。",
        "from": "T19n0955_001:0315a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自在其中為色相。",
        "from": "T19n0955_001:0315a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切於本部主。",
        "from": "T19n0955_001:0315a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應與於右手印物為次第。",
        "from": "T19n0955_001:0315a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於金剛縛中安物。",
        "from": "T19n0955_001:0315a26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "類於中積。",
        "from": "T19n0955_001:0315a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縛印當臍下或當心。",
        "from": "T19n0955_001:0315a26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手以縛物獻安自心。",
        "from": "T19n0955_001:0315a27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自明。",
        "from": "T19n0955_001:0315a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智者以此法。",
        "from": "T19n0955_001:0315a27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀身火光聚。",
        "from": "T19n0955_001:0315a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以先所集身口意善資糧用以此身物。",
        "from": "T19n0955_001:0315a28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安於明手中。",
        "from": "T19n0955_001:0315a29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言字火焰聲念誦。",
        "from": "T19n0955_001:0315a29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言者盡其夜應作。",
        "from": "T19n0955_001:0315b01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應破其坐。",
        "from": "T19n0955_001:0315b01_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此儀則法。",
        "from": "T19n0955_001:0315b01_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初夜當生暖。",
        "from": "T19n0955_001:0315b02_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烟當於中夜。",
        "from": "T19n0955_001:0315b02_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光焰於明相。",
        "from": "T19n0955_001:0315b02_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是漸次加。",
        "from": "T19n0955_001:0315b03_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如光焰成物。",
        "from": "T19n0955_001:0315b03_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得飛騰虛空。",
        "from": "T19n0955_001:0315b03_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於三界自在。",
        "from": "T19n0955_001:0315b03_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我說安善那法。",
        "from": "T19n0955_001:0315b04_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我曾已先說。",
        "from": "T19n0955_001:0315b04_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應斷一切取。",
        "from": "T19n0955_001:0315b04_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應依於本明。",
        "from": "T19n0955_001:0315b05_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至自身體。",
        "from": "T19n0955_001:0315b05_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "語心亦如是。",
        "from": "T19n0955_001:0315b05_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以自真言。",
        "from": "T19n0955_001:0315b06_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應隱沒作本明主。",
        "from": "T19n0955_001:0315b06_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應作念誦。",
        "from": "T19n0955_001:0315b06_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大金剛名者。",
        "from": "T19n0955_001:0315b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以金剛大身。",
        "from": "T19n0955_001:0315b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智者應堅住召入縛令喜。",
        "from": "T19n0955_001:0315b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當安於心。",
        "from": "T19n0955_001:0315b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如自心所樂。",
        "from": "T19n0955_001:0315b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我明觀自身。",
        "from": "T19n0955_001:0315b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我是虛空。",
        "from": "T19n0955_001:0315b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此相應成就。",
        "from": "T19n0955_001:0315b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至一切欲界主。",
        "from": "T19n0955_001:0315b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼等不見形。",
        "from": "T19n0955_001:0315b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剎那至梵天。",
        "from": "T19n0955_001:0315b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以盡彼法作鉤召。",
        "from": "T19n0955_001:0315b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應作四印曼荼羅。",
        "from": "T19n0955_001:0315b11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應安意在左手。",
        "from": "T19n0955_001:0315b11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自身安於彼。",
        "from": "T19n0955_001:0315b11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以拳應堅持。",
        "from": "T19n0955_001:0315b12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作拳為虛空。",
        "from": "T19n0955_001:0315b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是安怛陀娜。",
        "from": "T19n0955_001:0315b12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖忉利天宮亦不能見。",
        "from": "T19n0955_001:0315b13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遊於他化自在宮。",
        "from": "T19n0955_001:0315b13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恣意安樂自在乃至意所樂。",
        "from": "T19n0955_001:0315b14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "丸藥口安怛陀娜法。",
        "from": "T19n0955_001:0315b14_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今略說成就法。",
        "from": "T19n0955_001:0315b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是觀自明。",
        "from": "T19n0955_001:0315b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我是一切體(所謂以虛空內有所為一體也)自身同彼印。",
        "from": "T19n0955_001:0315b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住微細金剛三昧。",
        "from": "T19n0955_001:0315b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入於自明心。",
        "from": "T19n0955_001:0315b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於丸藥成就。",
        "from": "T19n0955_001:0315b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀丸藥置於口。",
        "from": "T19n0955_001:0315b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能遊須彌頂。",
        "from": "T19n0955_001:0315b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切世間不能見。",
        "from": "T19n0955_001:0315b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥色成大力(謂體隨意變身也)受忉利天宮慾樂隨意得快樂。",
        "from": "T19n0955_001:0315b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘世間相雜。",
        "from": "T19n0955_001:0315b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今說安怛陀娜。",
        "from": "T19n0955_001:0315b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "丸藥依本教成。",
        "from": "T19n0955_001:0315b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言智應作自明。",
        "from": "T19n0955_001:0315b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智身前求。",
        "from": "T19n0955_001:0315b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就者應獻。",
        "from": "T19n0955_001:0315b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無垢如虛空。",
        "from": "T19n0955_001:0315b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明身物及色。",
        "from": "T19n0955_001:0315b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以明字行列。",
        "from": "T19n0955_001:0315b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光焰相應住。",
        "from": "T19n0955_001:0315b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當入本尊身。",
        "from": "T19n0955_001:0315b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如隱沒住意。",
        "from": "T19n0955_001:0315b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從明口流出。",
        "from": "T19n0955_001:0315b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言光威猛。",
        "from": "T19n0955_001:0315b25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "丸藥善應成。",
        "from": "T19n0955_001:0315b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烟氣騰生已。",
        "from": "T19n0955_001:0315b26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禁止已安口。",
        "from": "T19n0955_001:0315b26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為不現形中自在。",
        "from": "T19n0955_001:0315b27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遊於四洲。",
        "from": "T19n0955_001:0315b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剎那諸世界。",
        "from": "T19n0955_001:0315b27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還來歸本處。",
        "from": "T19n0955_001:0315b28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所去得隨意。",
        "from": "T19n0955_001:0315b28_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種成就。",
        "from": "T19n0955_001:0315b28_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥叉眾常以為眷屬。",
        "from": "T19n0955_001:0315b28_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能遊戲須彌四天王下層四藥叉世界作無量有情利益。",
        "from": "T19n0955_001:0315b29_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "失正道漂曠野賊王水火等逼。",
        "from": "T19n0955_001:0315c01_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起是悲憫心於一切繫縛處。",
        "from": "T19n0955_001:0315c02_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當成就已皆令得解脫。",
        "from": "T19n0955_001:0315c03_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今已略說。",
        "from": "T19n0955_001:0315c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣法如大經。",
        "from": "T19n0955_001:0315c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應觀自心月。",
        "from": "T19n0955_001:0315c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月形具光明。",
        "from": "T19n0955_001:0315c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則於此月中。",
        "from": "T19n0955_001:0315c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一字如金色。",
        "from": "T19n0955_001:0315c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難覩如日輪。",
        "from": "T19n0955_001:0315c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光明普舒遍。",
        "from": "T19n0955_001:0315c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瑜岐想光明。",
        "from": "T19n0955_001:0315c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則其字為輪。",
        "from": "T19n0955_001:0315c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其輪為轉輪。",
        "from": "T19n0955_001:0315c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持妙色形七寶圍遶。",
        "from": "T19n0955_001:0315c07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍身毛孔中。",
        "from": "T19n0955_001:0315c07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流出無量佛。",
        "from": "T19n0955_001:0315c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瑜岐應思惟。",
        "from": "T19n0955_001:0315c08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用金剛界印。",
        "from": "T19n0955_001:0315c08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四處誦真言加持。",
        "from": "T19n0955_001:0315c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勤勇力成就。",
        "from": "T19n0955_001:0315c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念定勤為薪。",
        "from": "T19n0955_001:0315c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "焚燒一切罪。",
        "from": "T19n0955_001:0315c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以真言色火。",
        "from": "T19n0955_001:0315c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當離疑分別。",
        "from": "T19n0955_001:0315c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求大樂棄小樂。",
        "from": "T19n0955_001:0315c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慇懃慧菩提。",
        "from": "T19n0955_001:0315c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住真言儀則。",
        "from": "T19n0955_001:0315c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一字頂輪王瑜伽觀行儀軌一卷",
        "from": "T19n0955_001:0315c12_0",
        "context_with_next_sentence": false
    }
]