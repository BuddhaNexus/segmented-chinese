[
    {
        "source_sentence": "福州鼓山寺古尊宿語要全部目錄",
        "from": "X68n1316_001:0341c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卷一",
        "from": "X68n1316_001:0341c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南泉願(藏古卷十二)投子同(藏古卷三十六)睦州蹤(藏古卷六)趙州諗(藏古卷十三及卷十四)卷二南院顒(藏古卷七)首山念(藏古卷八)葉縣省(藏古卷廿三)神鼎諲(藏古卷廿四)三交嵩(藏古卷十)石門聰(藏古卷八)卷三法華舉(藏古卷廿六)大愚芝(藏古卷廿五)雲峰悅(藏古卷四十)楊岐會(藏古卷十九)道吾真(藏古卷十九)卷四大隋真(藏古卷三十五)子湖蹤(藏古卷十二)國師晏(藏古卷三十七)洞山初(藏古卷三十八)智門祚(藏古卷三十九)右總二十家　如雲臥紀談物初賸語。",
        "from": "X68n1316_001:0341c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並云。",
        "from": "X68n1316_001:0341c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賾藏主所蒐二十二家(止此)。",
        "from": "X68n1316_001:0341c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然則此本少二(總序亦云二十二家)。",
        "from": "X68n1316_001:0342a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "古刊(每半面十二行。",
        "from": "X68n1316_001:0342a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每行廿字或廿一二三四字不齊)第一卷(總九十二丈)第二卷(總九十丈)第三卷(總九十四丈)第四卷(總九十一丈)總三百六十七張。",
        "from": "X68n1316_001:0342a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雲臥紀談上(十一丈)曰。",
        "from": "X68n1316_001:0342a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福州鼓山。",
        "from": "X68n1316_001:0342a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於紹興之初。",
        "from": "X68n1316_001:0342a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刊行古尊宿語錄。",
        "from": "X68n1316_001:0342a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十有二。",
        "from": "X68n1316_001:0342a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洪之翠巖芝禪師者。",
        "from": "X68n1316_001:0342a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其一焉。",
        "from": "X68n1316_001:0342a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "物祖謄語十三(十九丈)。",
        "from": "X68n1316_001:0342a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重刊古尊宿語錄序曰。",
        "from": "X68n1316_001:0342a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有賾藏主者。",
        "from": "X68n1316_001:0342a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旁蒐廣採。",
        "from": "X68n1316_001:0342a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僅得南泉而下二十二家示眾機語。",
        "from": "X68n1316_001:0342a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "厥後又得雲門真淨佛眼三家。",
        "from": "X68n1316_001:0342a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總曰古尊宿語。",
        "from": "X68n1316_001:0342a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非止乎此也。",
        "from": "X68n1316_001:0342a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "據其所搜採而言爾。",
        "from": "X68n1316_001:0342a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賾藏主刊行古尊宿語錄二十二家。",
        "from": "X68n1316_001:0342a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有補於宗門多矣。",
        "from": "X68n1316_001:0342a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惜不略敘其始末。",
        "from": "X68n1316_001:0342a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為闕典。",
        "from": "X68n1316_001:0342a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就中惟大隋趙州有行狀。",
        "from": "X68n1316_001:0342a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　樞使懶窩大居士沈公。",
        "from": "X68n1316_001:0342a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來殿是邦。",
        "from": "X68n1316_001:0342a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "權衡此道。",
        "from": "X68n1316_001:0342a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一見謂住鼓山(德最)曰。",
        "from": "X68n1316_001:0342a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖傳燈廣燈續燈僧寶傳具載。",
        "from": "X68n1316_001:0342a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而衲子未暇檢閱。",
        "from": "X68n1316_001:0342a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卒讀則惘然不知宜。",
        "from": "X68n1316_001:0342a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "撮其大槩。",
        "from": "X68n1316_001:0342a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "標於卷首(德最)。",
        "from": "X68n1316_001:0342a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謹略具其始終出處。",
        "from": "X68n1316_001:0342a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有不載者。",
        "from": "X68n1316_001:0342a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則闕焉。",
        "from": "X68n1316_001:0342a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "學者一覧。",
        "from": "X68n1316_001:0342a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便見是亦一助也。",
        "from": "X68n1316_001:0342a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淳熈戊戌臘月望日。",
        "from": "X68n1316_001:0342a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福州鼓山寺古尊宿語要全部目錄",
        "from": "X68n1316_001:0342a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卷一",
        "from": "X68n1316_001:0342a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南泉願投子同睦州蹤趙州諗",
        "from": "X68n1316_001:0342a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卷二",
        "from": "X68n1316_001:0342b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南院顒首山念葉縣省神鼎諲",
        "from": "X68n1316_001:0342b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三交嵩石門聰",
        "from": "X68n1316_001:0342b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卷三",
        "from": "X68n1316_001:0342b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法華舉大愚芝雲峰悅楊岐會",
        "from": "X68n1316_001:0342b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道吾真",
        "from": "X68n1316_001:0342b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卷四",
        "from": "X68n1316_001:0342b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大隋真子湖蹤國師晏洞山初",
        "from": "X68n1316_001:0342b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智門祚",
        "from": "X68n1316_001:0342b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已上四策。",
        "from": "X68n1316_001:0342b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共二十家。",
        "from": "X68n1316_001:0342b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "係賾藏主刊。",
        "from": "X68n1316_001:0342b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在藏司印行。",
        "from": "X68n1316_001:0342b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天字共一十四家地字共一十六家",
        "from": "X68n1316_001:0342b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日字共一十一家(內揚岐一家重出)月字共一十五家星字共一十五家辰字共一十家已上六策。",
        "from": "X68n1316_001:0342b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總八十家。",
        "from": "X68n1316_001:0342b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "係嘉熈戊戌歲續刊。",
        "from": "X68n1316_001:0342b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在　蒙堂印行。",
        "from": "X68n1316_001:0342b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逐策各有目錄。",
        "from": "X68n1316_001:0342b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具載尊宿名字。",
        "from": "X68n1316_001:0342b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏幸眾悉。",
        "from": "X68n1316_001:0342b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重刻趙州祖師語錄序",
        "from": "X68n1316_001:0342b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞夫破家散宅於十八上。",
        "from": "X68n1316_001:0342b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而善舞太阿。",
        "from": "X68n1316_001:0342b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縱賓奪主於賢聖前。",
        "from": "X68n1316_001:0342b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而逢場作戲。",
        "from": "X68n1316_001:0342b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一物不將來。",
        "from": "X68n1316_001:0342b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便教放下著。",
        "from": "X68n1316_001:0342b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不起一念時。",
        "from": "X68n1316_001:0342b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向道須彌山。",
        "from": "X68n1316_001:0342b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每拈一莖草。",
        "from": "X68n1316_001:0342b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而喚作丈六金身。",
        "from": "X68n1316_001:0342b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口惟一個齒。",
        "from": "X68n1316_001:0342c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而盡知世間滋味。",
        "from": "X68n1316_001:0342c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鎮州蘿蔔。",
        "from": "X68n1316_001:0342c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸方謾云即是師承。",
        "from": "X68n1316_001:0342c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "青州布衫。",
        "from": "X68n1316_001:0342c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "學者休向言中取的。",
        "from": "X68n1316_001:0342c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一個老實頭。",
        "from": "X68n1316_001:0342c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殺活臨機。",
        "from": "X68n1316_001:0342c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頓超他動棒用喝。",
        "from": "X68n1316_001:0342c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三寸綿軟舌。",
        "from": "X68n1316_001:0342c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縱橫自在。",
        "from": "X68n1316_001:0342c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何甞用怪語奇言。",
        "from": "X68n1316_001:0342c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其猶水上按葫蘆。",
        "from": "X68n1316_001:0342c04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "垂手東捺西捺。",
        "from": "X68n1316_001:0342c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "室中懸寶鏡。",
        "from": "X68n1316_001:0342c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "任教凡來聖來。",
        "from": "X68n1316_001:0342c05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈提向上宗乘。",
        "from": "X68n1316_001:0342c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念佛則潄口三日。",
        "from": "X68n1316_001:0342c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善解拖泥帶水。",
        "from": "X68n1316_001:0342c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨而問隨答有無。",
        "from": "X68n1316_001:0342c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南泉真子。",
        "from": "X68n1316_001:0342c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬祖的孫。",
        "from": "X68n1316_001:0342c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其惟此老一人而已矣。",
        "from": "X68n1316_001:0342c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其垂跡也。",
        "from": "X68n1316_001:0342c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滕蚐入口。",
        "from": "X68n1316_001:0342c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而糠食自安。",
        "from": "X68n1316_001:0342c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉現報也。",
        "from": "X68n1316_001:0342c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明珠出海。",
        "from": "X68n1316_001:0342c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而二王供養。",
        "from": "X68n1316_001:0342c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如此則寧非先佛示現利生者哉。",
        "from": "X68n1316_001:0342c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惜其語錄不能盡傳。",
        "from": "X68n1316_001:0342c10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "學者僅獲一帙。",
        "from": "X68n1316_001:0342c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真如甞鼎一臠飲海一滴者矣。",
        "from": "X68n1316_001:0342c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "柰舊刻歲久。",
        "from": "X68n1316_001:0342c11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "字跡糢糊。",
        "from": "X68n1316_001:0342c11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾徒明聲。",
        "from": "X68n1316_001:0342c12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發心重刻。",
        "from": "X68n1316_001:0342c12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詔示後來。",
        "from": "X68n1316_001:0342c12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真報祖師之恩。",
        "from": "X68n1316_001:0342c12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深愜老朽之意。",
        "from": "X68n1316_001:0342c13_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因佳其志。",
        "from": "X68n1316_001:0342c13_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聊綴數言。",
        "from": "X68n1316_001:0342c13_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其會機大用。",
        "from": "X68n1316_001:0342c13_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非予劣智能解。",
        "from": "X68n1316_001:0342c14_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造淵洞微。",
        "from": "X68n1316_001:0342c14_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自有通方作者。",
        "from": "X68n1316_001:0342c14_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳曹洞正宗第二十七代雲門顯聖寺住持散木圓澄撰南院和尚亦云寶應。",
        "from": "X68n1316_001:0342c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名慧顒。",
        "from": "X68n1316_001:0342c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鄉貫姓氏受業不載。",
        "from": "X68n1316_001:0342c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得法於興化獎和尚。",
        "from": "X68n1316_001:0342c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨濟第三世。",
        "from": "X68n1316_001:0342c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後唐莊宗明宗時人。",
        "from": "X68n1316_001:0342c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "首山和尚名省念。",
        "from": "X68n1316_001:0342c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萊州人。",
        "from": "X68n1316_001:0342c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姓狄氏。",
        "from": "X68n1316_001:0342c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南禪院受業。",
        "from": "X68n1316_001:0342c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得法於風穴昭和尚。",
        "from": "X68n1316_001:0342c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽六十八。",
        "from": "X68n1316_001:0342c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨濟第五世。",
        "from": "X68n1316_001:0342c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　本朝太宗淳化中示寂。",
        "from": "X68n1316_001:0342c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "葉縣和尚名歸省。",
        "from": "X68n1316_001:0342c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冀州人。",
        "from": "X68n1316_001:0342c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姓賈氏。",
        "from": "X68n1316_001:0342c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "易州保壽院受業。",
        "from": "X68n1316_001:0342c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得法於首山念和尚。",
        "from": "X68n1316_001:0342c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨濟第六世。",
        "from": "X68n1316_001:0342c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　本朝太宗真宗時人。",
        "from": "X68n1316_001:0342c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神鼎和尚名洪諲。",
        "from": "X68n1316_001:0343a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "襄州人。",
        "from": "X68n1316_001:0343a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姓氏受業不載。",
        "from": "X68n1316_001:0343a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得法於首山念和尚。",
        "from": "X68n1316_001:0343a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年八十餘。",
        "from": "X68n1316_001:0343a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨濟第六世。",
        "from": "X68n1316_001:0343a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與葉縣同時。",
        "from": "X68n1316_001:0343a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "承天和尚名智嵩。",
        "from": "X68n1316_001:0343a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦云三交嵩。",
        "from": "X68n1316_001:0343a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦云唐明嵩。",
        "from": "X68n1316_001:0343a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦云銕佛嵩。",
        "from": "X68n1316_001:0343a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三交即承天也。",
        "from": "X68n1316_001:0343a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鄉貫姓氏受業不載。",
        "from": "X68n1316_001:0343a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得法於首山念和尚。",
        "from": "X68n1316_001:0343a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨濟第六世。",
        "from": "X68n1316_001:0343a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與葉縣同時。",
        "from": "X68n1316_001:0343a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "石門和尚亦住谷隱。",
        "from": "X68n1316_001:0343a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號慈照。",
        "from": "X68n1316_001:0343a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名蘊聰。",
        "from": "X68n1316_001:0343a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南海人。",
        "from": "X68n1316_001:0343a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姓張氏。",
        "from": "X68n1316_001:0343a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受業不載。",
        "from": "X68n1316_001:0343a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得法於首山念和尚。",
        "from": "X68n1316_001:0343a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽六十八。",
        "from": "X68n1316_001:0343a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨濟第六世。",
        "from": "X68n1316_001:0343a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　本朝仁宗天聖中示寂。",
        "from": "X68n1316_001:0343a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法華和尚名齊舉(一本名全舉)。",
        "from": "X68n1316_001:0343a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鄉貫姓氏受業不載。",
        "from": "X68n1316_001:0343a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得法於汾陽昭和尚。",
        "from": "X68n1316_001:0343a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年七十餘。",
        "from": "X68n1316_001:0343a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨濟第七世。",
        "from": "X68n1316_001:0343a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　本朝真宗仁宗時人。",
        "from": "X68n1316_001:0343a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大愚和尚名守芝。",
        "from": "X68n1316_001:0343a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "太原人。",
        "from": "X68n1316_001:0343a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姓王氏。",
        "from": "X68n1316_001:0343a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "潞州承天寺受業。",
        "from": "X68n1316_001:0343a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得法於汾陽昭和尚。",
        "from": "X68n1316_001:0343a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨濟第七世。",
        "from": "X68n1316_001:0343a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　仁宗嘉祐中示寂。",
        "from": "X68n1316_001:0343a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雲峰和尚名文悅。",
        "from": "X68n1316_001:0343a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南昌人。",
        "from": "X68n1316_001:0343a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姓徐氏。",
        "from": "X68n1316_001:0343a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍興寺受業。",
        "from": "X68n1316_001:0343a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得法於大愚芝和尚。",
        "from": "X68n1316_001:0343a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽六十六。",
        "from": "X68n1316_001:0343a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臘五十九。",
        "from": "X68n1316_001:0343a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨濟第八世。",
        "from": "X68n1316_001:0343a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嘉祐中示寂。",
        "from": "X68n1316_001:0343a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雲峰悅禪師語錄序無為　楊傑　撰",
        "from": "X68n1316_001:0343a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祖師門下。",
        "from": "X68n1316_001:0343a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正令全提。",
        "from": "X68n1316_001:0343a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐斷十方。",
        "from": "X68n1316_001:0343a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壁立千仞。",
        "from": "X68n1316_001:0343a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩竭掩室。",
        "from": "X68n1316_001:0343a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已涉塵勞。",
        "from": "X68n1316_001:0343a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨名杜辭。",
        "from": "X68n1316_001:0343a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自彰瑕謫。",
        "from": "X68n1316_001:0343a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別傳一句。",
        "from": "X68n1316_001:0343a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盜賊破家。",
        "from": "X68n1316_001:0343a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不失本宗。",
        "from": "X68n1316_001:0343a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "狐狸戀窟。",
        "from": "X68n1316_001:0343a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西山谷口。",
        "from": "X68n1316_001:0343a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南嶽峰前。",
        "from": "X68n1316_001:0343a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大沙門。",
        "from": "X68n1316_001:0343a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曾行是令。",
        "from": "X68n1316_001:0343a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "叢林學者。",
        "from": "X68n1316_001:0343a22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輻湊景從。",
        "from": "X68n1316_001:0343a22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "報緣將終。",
        "from": "X68n1316_001:0343a22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辭眾坐逝。",
        "from": "X68n1316_001:0343a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利充溢。",
        "from": "X68n1316_001:0343a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "激射寶龕。",
        "from": "X68n1316_001:0343a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "末後度人。",
        "from": "X68n1316_001:0343a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可勝數。",
        "from": "X68n1316_001:0343a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後三十年。",
        "from": "X68n1316_001:0343a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黃梅齊曉山主。",
        "from": "X68n1316_001:0343a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求無為子序其錄云。",
        "from": "X68n1316_001:0343a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪師諱文悅。",
        "from": "X68n1316_001:0343b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鍾陵人也。",
        "from": "X68n1316_001:0343b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "楊岐和尚名文會。",
        "from": "X68n1316_001:0343b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "袁州人。",
        "from": "X68n1316_001:0343b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姓冷氏。",
        "from": "X68n1316_001:0343b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "筠州九峰院受業。",
        "from": "X68n1316_001:0343b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得法於石霜圓和尚。",
        "from": "X68n1316_001:0343b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨濟第八世。",
        "from": "X68n1316_001:0343b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　仁宗慶曆中。",
        "from": "X68n1316_001:0343b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住雲盖。",
        "from": "X68n1316_001:0343b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道吾和尚名悟真。",
        "from": "X68n1316_001:0343b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鄉貫姓氏受業不載。",
        "from": "X68n1316_001:0343b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得法於石霜圓和尚。",
        "from": "X68n1316_001:0343b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨濟第八世。",
        "from": "X68n1316_001:0343b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　仁宗時人。",
        "from": "X68n1316_001:0343b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大隋和尚已有行狀　馬祖第四世。",
        "from": "X68n1316_001:0343b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衢州子湖山定業禪院第一代神力禪師語錄序第三代住持傳法賜紫　慧覺　譔昔神力禪師。",
        "from": "X68n1316_001:0343b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諱利蹤。",
        "from": "X68n1316_001:0343b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有奇行。",
        "from": "X68n1316_001:0343b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藹然叢林。",
        "from": "X68n1316_001:0343b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自唐迨今。",
        "from": "X68n1316_001:0343b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僅于三百載。",
        "from": "X68n1316_001:0343b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所居法席。",
        "from": "X68n1316_001:0343b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廢來久矣。",
        "from": "X68n1316_001:0343b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所示徒語言。",
        "from": "X68n1316_001:0343b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦無聞於世。",
        "from": "X68n1316_001:0343b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其所傳者。",
        "from": "X68n1316_001:0343b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟犬話耳。",
        "from": "X68n1316_001:0343b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "余自元豐三年來。",
        "from": "X68n1316_001:0343b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "主是席。",
        "from": "X68n1316_001:0343b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂探討其語。",
        "from": "X68n1316_001:0343b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未之得也。",
        "from": "X68n1316_001:0343b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一旦往大雲寘師院。",
        "from": "X68n1316_001:0343b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃獲一小策。",
        "from": "X68n1316_001:0343b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開覧翫味。",
        "from": "X68n1316_001:0343b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見其言直其理深。",
        "from": "X68n1316_001:0343b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若醍醐之一滴。",
        "from": "X68n1316_001:0343b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能散其六斛之驢乳。",
        "from": "X68n1316_001:0343b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真南泉之的嗣也。",
        "from": "X68n1316_001:0343b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文字訛舛固多。",
        "from": "X68n1316_001:0343b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "余遂逐一看詳。",
        "from": "X68n1316_001:0343b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "改正鏤板。",
        "from": "X68n1316_001:0343b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳于世。",
        "from": "X68n1316_001:0343b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不獨顯當時之盛事。",
        "from": "X68n1316_001:0343b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貴有益於來者。",
        "from": "X68n1316_001:0343b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時元豐六年癸亥歲五月十有二日序。",
        "from": "X68n1316_001:0343b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讚神力禪師住持傳法賜紫(慧覺)譔欽哉神力。",
        "from": "X68n1316_001:0343b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真善知識。",
        "from": "X68n1316_001:0343b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不住有無。",
        "from": "X68n1316_001:0343b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寧拘順逆。",
        "from": "X68n1316_001:0343b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入門看犬。",
        "from": "X68n1316_001:0343b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思量痛惜。",
        "from": "X68n1316_001:0343b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上山見路。",
        "from": "X68n1316_001:0343b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不妨徑直。",
        "from": "X68n1316_001:0343b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三截取人。",
        "from": "X68n1316_001:0343b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一言最的。",
        "from": "X68n1316_001:0343b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左右轉時。",
        "from": "X68n1316_001:0343b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "湍流峻激。",
        "from": "X68n1316_001:0343b22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東西往來。",
        "from": "X68n1316_001:0343b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庭趨愕立。",
        "from": "X68n1316_001:0343b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清風千古振叢林。",
        "from": "X68n1316_001:0343b22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬里無雲轟霹靂。",
        "from": "X68n1316_001:0343b23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洞山和尚名守初。",
        "from": "X68n1316_001:0343b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳳翔人。",
        "from": "X68n1316_001:0343b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姓傅氏。",
        "from": "X68n1316_001:0343b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "渭州崆峒山受業。",
        "from": "X68n1316_001:0343b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得法於雲門偃和尚。",
        "from": "X68n1316_001:0343c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽八十一。",
        "from": "X68n1316_001:0343c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臘六十五。",
        "from": "X68n1316_001:0343c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　本朝太宗淳化中示寂。",
        "from": "X68n1316_001:0343c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智門和尚名光祚。",
        "from": "X68n1316_001:0343c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鄉貫姓氏受業不載。",
        "from": "X68n1316_001:0343c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得法於香林遠和尚。",
        "from": "X68n1316_001:0343c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雲門第三世。",
        "from": "X68n1316_001:0343c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　真宗時人。",
        "from": "X68n1316_001:0343c04_4",
        "context_with_next_sentence": false
    }
]