[
    {
        "source_sentence": "大唐青龍寺三朝供奉大德行狀",
        "from": "T50n2057_001:0294c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先師諱惠果和尚。",
        "from": "T50n2057_001:0294c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俗姓馬氏。",
        "from": "T50n2057_001:0294c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "京兆府萬年縣歸明鄉人也。",
        "from": "T50n2057_001:0294c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幼年九歲。",
        "from": "T50n2057_001:0294c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便隨聖佛院。",
        "from": "T50n2057_001:0294c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故三朝國師內道場持念賜紫沙門諱曇貞和尚。",
        "from": "T50n2057_001:0294c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立志習經。",
        "from": "T50n2057_001:0294c21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至年十七。",
        "from": "T50n2057_001:0294c22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為緣和尚。",
        "from": "T50n2057_001:0294c22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常在內道場。",
        "from": "T50n2057_001:0294c22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持念不出。",
        "from": "T50n2057_001:0294c22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃於興善寺三藏和上。",
        "from": "T50n2057_001:0294c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求授大佛頂隨求等真言。",
        "from": "T50n2057_001:0294c23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "秊十九。",
        "from": "T50n2057_001:0294c24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏邊教授灌頂散華。",
        "from": "T50n2057_001:0294c24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得轉法輪菩薩。",
        "from": "T50n2057_001:0294c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和上云。",
        "from": "T50n2057_001:0294c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我於南天竺國。",
        "from": "T50n2057_001:0294c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散花得此尊。",
        "from": "T50n2057_001:0294c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如今無異。",
        "from": "T50n2057_001:0294c26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "異於吾後。",
        "from": "T50n2057_001:0294c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弘傳總持大教。",
        "from": "T50n2057_001:0294c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如我無異。",
        "from": "T50n2057_001:0294c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又大曆八年。",
        "from": "T50n2057_001:0294c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三月上旬。",
        "from": "T50n2057_001:0294c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勅於慈恩寺。",
        "from": "T50n2057_001:0294c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置方等道場。",
        "from": "T50n2057_001:0294c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖佛院和上。",
        "from": "T50n2057_001:0294c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉慈恩寺　勅。",
        "from": "T50n2057_001:0294c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置方等道場微僧有二童子。",
        "from": "T50n2057_001:0294c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年滿二十。",
        "from": "T50n2057_001:0294c29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堪授具戒。",
        "from": "T50n2057_001:0294c29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未敢專擅與出家剃頭。",
        "from": "T50n2057_001:0295a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏乞聖慈。",
        "from": "T50n2057_001:0295a01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "許臣歸寺。",
        "from": "T50n2057_001:0295a01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與二童子。",
        "from": "T50n2057_001:0295a02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并授戒衣鉢。",
        "from": "T50n2057_001:0295a02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　勅賜刀一口。",
        "from": "T50n2057_001:0295a02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於青龍大佛殿前。",
        "from": "T50n2057_001:0295a03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "授　勅與和上二童子剃頭。",
        "from": "T50n2057_001:0295a03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賜袈裟衣鉢各兩副。",
        "from": "T50n2057_001:0295a04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "度官告住寺。",
        "from": "T50n2057_001:0295a04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天使和上送至慈恩寺訖。",
        "from": "T50n2057_001:0295a04_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "却入內謝　勅。",
        "from": "T50n2057_001:0295a05_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臣一界微僧。",
        "from": "T50n2057_001:0295a05_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謝聖慈賜與二童子出家授戒。",
        "from": "T50n2057_001:0295a06_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣鉢剃刀。",
        "from": "T50n2057_001:0295a06_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "官告及天使送至慈恩寺道場。",
        "from": "T50n2057_001:0295a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "授戒處訖。",
        "from": "T50n2057_001:0295a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微僧不勝頂賀。",
        "from": "T50n2057_001:0295a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "授戒訖。",
        "from": "T50n2057_001:0295a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "秊二十二。",
        "from": "T50n2057_001:0295a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又於無畏三藏和上弟子玄超和上邊。",
        "from": "T50n2057_001:0295a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求授大悲胎藏毘盧遮那大瑜伽大教。",
        "from": "T50n2057_001:0295a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及蘇悉地大瑜伽法。",
        "from": "T50n2057_001:0295a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸尊瑜伽等法。",
        "from": "T50n2057_001:0295a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一親垂旨授。",
        "from": "T50n2057_001:0295a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又於大興善寺三藏和上邊。",
        "from": "T50n2057_001:0295a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求授金剛頂大瑜伽大教王經法。",
        "from": "T50n2057_001:0295a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸尊瑜伽密印。",
        "from": "T50n2057_001:0295a13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "親承指示。",
        "from": "T50n2057_001:0295a13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先師在內所得。",
        "from": "T50n2057_001:0295a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恩賜等盡將奉上三藏和上。",
        "from": "T50n2057_001:0295a14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "充授法之恩。",
        "from": "T50n2057_001:0295a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每於閣下。",
        "from": "T50n2057_001:0295a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "節食邀期。",
        "from": "T50n2057_001:0295a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時念凡經數遍。",
        "from": "T50n2057_001:0295a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年二十五。",
        "from": "T50n2057_001:0295a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "特奉恩旨詔命入內。",
        "from": "T50n2057_001:0295a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於長生殿。",
        "from": "T50n2057_001:0295a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當時有勅喚。",
        "from": "T50n2057_001:0295a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對問。",
        "from": "T50n2057_001:0295a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師有何功効。",
        "from": "T50n2057_001:0295a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夾天云。",
        "from": "T50n2057_001:0295a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微僧未有功効。",
        "from": "T50n2057_001:0295a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉　勅便誠當時。",
        "from": "T50n2057_001:0295a18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喚童子八人。",
        "from": "T50n2057_001:0295a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "考召加持　恩命所問。",
        "from": "T50n2057_001:0295a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡皆成就轉瓶合竹。",
        "from": "T50n2057_001:0295a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并得成就。",
        "from": "T50n2057_001:0295a19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　帝乃大喜。",
        "from": "T50n2057_001:0295a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至大曆十年。",
        "from": "T50n2057_001:0295a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於當寺。",
        "from": "T50n2057_001:0295a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別　勅賜東塔院一所。",
        "from": "T50n2057_001:0295a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置毘盧遮那灌頂道場。",
        "from": "T50n2057_001:0295a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七僧持念。",
        "from": "T50n2057_001:0295a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至大曆十一年。",
        "from": "T50n2057_001:0295a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持代宗皇帝。",
        "from": "T50n2057_001:0295a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應時便差。",
        "from": "T50n2057_001:0295a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　勅賜紫衣一對。",
        "from": "T50n2057_001:0295a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口奏不敢受。",
        "from": "T50n2057_001:0295a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應時却進。",
        "from": "T50n2057_001:0295a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勅云。",
        "from": "T50n2057_001:0295a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朕已賜師紫衣。",
        "from": "T50n2057_001:0295a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何得不受。",
        "from": "T50n2057_001:0295a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奏云。",
        "from": "T50n2057_001:0295a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一界微僧。",
        "from": "T50n2057_001:0295a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賜紫不勝頂荷。",
        "from": "T50n2057_001:0295a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即合便受。",
        "from": "T50n2057_001:0295a25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏緣和上先皇賜紫。",
        "from": "T50n2057_001:0295a25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弟子不合與師齊。",
        "from": "T50n2057_001:0295a26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝云。",
        "from": "T50n2057_001:0295a26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師大孝。",
        "from": "T50n2057_001:0295a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是朕之錯。",
        "from": "T50n2057_001:0295a26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　勅云。",
        "from": "T50n2057_001:0295a27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他宗異姓上。",
        "from": "T50n2057_001:0295a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "存遵奉孝敬之心。",
        "from": "T50n2057_001:0295a27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此以後。",
        "from": "T50n2057_001:0295a27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堪為國師。",
        "from": "T50n2057_001:0295a28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂賜褐衣一對。",
        "from": "T50n2057_001:0295a28_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉　勅加持。",
        "from": "T50n2057_001:0295a28_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "花陽公主。",
        "from": "T50n2057_001:0295a29_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三日得差。",
        "from": "T50n2057_001:0295a29_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後於申未之問。",
        "from": "T50n2057_001:0295a29_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公主忽然不語。",
        "from": "T50n2057_001:0295a29_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　皇帝與宮人等。",
        "from": "T50n2057_001:0295b01_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一向前。",
        "from": "T50n2057_001:0295b01_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和上奏云。",
        "from": "T50n2057_001:0295b01_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時熱。",
        "from": "T50n2057_001:0295b02_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "望階下與宮人等。",
        "from": "T50n2057_001:0295b02_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "暫向微僧加持即令得差。",
        "from": "T50n2057_001:0295b02_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其公主便差。",
        "from": "T50n2057_001:0295b03_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言詞分明。",
        "from": "T50n2057_001:0295b03_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皇帝大悅。",
        "from": "T50n2057_001:0295b03_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賜絹一百匹。",
        "from": "T50n2057_001:0295b03_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣一對。",
        "from": "T50n2057_001:0295b04_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謝　勅。",
        "from": "T50n2057_001:0295b04_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臣一界微僧。",
        "from": "T50n2057_001:0295b04_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謝聖慈賜絹及衣。",
        "from": "T50n2057_001:0295b04_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不勝頂賀(三日口奏移公主)　大曆秊中。",
        "from": "T50n2057_001:0295b05_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有恩賜錢物。",
        "from": "T50n2057_001:0295b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一千餘貫。",
        "from": "T50n2057_001:0295b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡修塔下功德。",
        "from": "T50n2057_001:0295b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大曆十三年。",
        "from": "T50n2057_001:0295b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奏請前後兩度巡南臺。",
        "from": "T50n2057_001:0295b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依師所奏。",
        "from": "T50n2057_001:0295b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和上於觀音臺。",
        "from": "T50n2057_001:0295b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持念。",
        "from": "T50n2057_001:0295b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜久之間。",
        "from": "T50n2057_001:0295b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大聖觀音。",
        "from": "T50n2057_001:0295b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於大月輪中。",
        "from": "T50n2057_001:0295b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現大身相。",
        "from": "T50n2057_001:0295b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光明由如白日。",
        "from": "T50n2057_001:0295b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祥雲皎潔。",
        "from": "T50n2057_001:0295b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同時數百千人。",
        "from": "T50n2057_001:0295b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遙共瞻禮。",
        "from": "T50n2057_001:0295b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大曆十三年。",
        "from": "T50n2057_001:0295b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　勅長生殿內道場三朝傳法灌頂歿故三藏和上。",
        "from": "T50n2057_001:0295b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　勅語惠果。",
        "from": "T50n2057_001:0295b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏和上歿後。",
        "from": "T50n2057_001:0295b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將為佛法無去時。",
        "from": "T50n2057_001:0295b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞師學得大法。",
        "from": "T50n2057_001:0295b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總在緣國事□和上所邊。",
        "from": "T50n2057_001:0295b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "授得念誦法門。",
        "from": "T50n2057_001:0295b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多有廢忘。",
        "from": "T50n2057_001:0295b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他日降舉問道。",
        "from": "T50n2057_001:0295b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是師位。",
        "from": "T50n2057_001:0295b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大曆十三年十月十五日。",
        "from": "T50n2057_001:0295b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高李憲誠宣。",
        "from": "T50n2057_001:0295b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建中年初。",
        "from": "T50n2057_001:0295b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有訶陵國僧辨弘。",
        "from": "T50n2057_001:0295b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從本國。",
        "from": "T50n2057_001:0295b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將銅鈸一具。",
        "from": "T50n2057_001:0295b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉上聖佛院。",
        "from": "T50n2057_001:0295b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "螺兩具。",
        "from": "T50n2057_001:0295b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "銅□瓶四。",
        "from": "T50n2057_001:0295b17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□奉上和上。",
        "from": "T50n2057_001:0295b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "充供養。",
        "from": "T50n2057_001:0295b18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求授胎藏毘盧遮那大法。",
        "from": "T50n2057_001:0295b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建中二年。",
        "from": "T50n2057_001:0295b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "新羅國僧惠日。",
        "from": "T50n2057_001:0295b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將本國信物。",
        "from": "T50n2057_001:0295b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉上和上。",
        "from": "T50n2057_001:0295b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求授胎藏金剛界蘇悉地等。",
        "from": "T50n2057_001:0295b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并諸尊瑜伽三十本。",
        "from": "T50n2057_001:0295b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已來授訖。",
        "from": "T50n2057_001:0295b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精通後時。",
        "from": "T50n2057_001:0295b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "却歸本國。",
        "from": "T50n2057_001:0295b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣弘大教。",
        "from": "T50n2057_001:0295b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精誠絕粒持念。",
        "from": "T50n2057_001:0295b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉地現前。",
        "from": "T50n2057_001:0295b22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂白日冲天竺國王宮中瞻禮。",
        "from": "T50n2057_001:0295b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求乞其法。",
        "from": "T50n2057_001:0295b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空中□言西大唐國。",
        "from": "T50n2057_001:0295b23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有祕密法法有青龍寺。",
        "from": "T50n2057_001:0295b24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同年新羅國僧悟真。",
        "from": "T50n2057_001:0295b24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "授胎藏毘盧遮那及諸尊持念教法等。",
        "from": "T50n2057_001:0295b25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至貞元五秊。",
        "from": "T50n2057_001:0295b26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往於中天竺國。",
        "from": "T50n2057_001:0295b26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大毘盧遮那經梵夾餘經。",
        "from": "T50n2057_001:0295b26_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吐藩國身歿。",
        "from": "T50n2057_001:0295b27_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當院弟子僧義明。",
        "from": "T50n2057_001:0295b27_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義滿。",
        "from": "T50n2057_001:0295b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義澄。",
        "from": "T50n2057_001:0295b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同時於和上。",
        "from": "T50n2057_001:0295b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求授毘盧遮那胎藏蘇悉地等經三十本。",
        "from": "T50n2057_001:0295b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貞元五年。",
        "from": "T50n2057_001:0295b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉　勅於當寺大佛殿□。",
        "from": "T50n2057_001:0295b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令七僧祈雨。",
        "from": "T50n2057_001:0295c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第七日夜雨足各賜絹一束。",
        "from": "T50n2057_001:0295c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "茶十串表謝。",
        "from": "T50n2057_001:0295c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉　勅於右衛。",
        "from": "T50n2057_001:0295c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍迎真身入內。",
        "from": "T50n2057_001:0295c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貞元六年四月□曰。",
        "from": "T50n2057_001:0295c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉　勅令僧惠果入內。",
        "from": "T50n2057_001:0295c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於長生殿。",
        "from": "T50n2057_001:0295c04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為國持念。",
        "from": "T50n2057_001:0295c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在內七十餘日。",
        "from": "T50n2057_001:0295c04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放歸。",
        "from": "T50n2057_001:0295c05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每人賜絹三十匹。",
        "from": "T50n2057_001:0295c05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "茶二十串。",
        "from": "T50n2057_001:0295c05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後乃分番上下。",
        "from": "T50n2057_001:0295c05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□賜四時衣物。",
        "from": "T50n2057_001:0295c06_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三節料。",
        "from": "T50n2057_001:0295c06_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當年杜相公黃裳韋相公親詣受灌頂。",
        "from": "T50n2057_001:0295c06_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "學持念。",
        "from": "T50n2057_001:0295c07_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "再奏請巡臺。",
        "from": "T50n2057_001:0295c07_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉　勅宜依。",
        "from": "T50n2057_001:0295c08_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "充觀音寺大德。",
        "from": "T50n2057_001:0295c08_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貞元九秊。",
        "from": "T50n2057_001:0295c08_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後至十三秊。",
        "from": "T50n2057_001:0295c08_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義恒。",
        "from": "T50n2057_001:0295c09_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義一。",
        "from": "T50n2057_001:0295c09_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義政。",
        "from": "T50n2057_001:0295c09_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義□。",
        "from": "T50n2057_001:0295c09_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義操。",
        "from": "T50n2057_001:0295c09_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義雲。",
        "from": "T50n2057_001:0295c09_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智興。",
        "from": "T50n2057_001:0295c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義慜。",
        "from": "T50n2057_001:0295c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行堅。",
        "from": "T50n2057_001:0295c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓通。",
        "from": "T50n2057_001:0295c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義倫。",
        "from": "T50n2057_001:0295c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義播。",
        "from": "T50n2057_001:0295c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義潤。",
        "from": "T50n2057_001:0295c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俗弟子吳殷。",
        "from": "T50n2057_001:0295c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開丕等。",
        "from": "T50n2057_001:0295c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "約五十人學法。",
        "from": "T50n2057_001:0295c11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貞元十四年。",
        "from": "T50n2057_001:0295c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五月大旱。",
        "from": "T50n2057_001:0295c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五月上旬。",
        "from": "T50n2057_001:0295c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉　勅祈雨。",
        "from": "T50n2057_001:0295c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七日。",
        "from": "T50n2057_001:0295c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在內道場。",
        "from": "T50n2057_001:0295c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "專精持念。",
        "from": "T50n2057_001:0295c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈雨日足。",
        "from": "T50n2057_001:0295c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　恩賜絹一束。",
        "from": "T50n2057_001:0295c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "茶十串。",
        "from": "T50n2057_001:0295c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謝。",
        "from": "T50n2057_001:0295c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臣僧等。",
        "from": "T50n2057_001:0295c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "素無功行。",
        "from": "T50n2057_001:0295c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天降甘雨。",
        "from": "T50n2057_001:0295c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皇帝感化。",
        "from": "T50n2057_001:0295c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧等謝絹及茶。",
        "from": "T50n2057_001:0295c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不勝頂賀。",
        "from": "T50n2057_001:0295c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貞元十五年。",
        "from": "T50n2057_001:0295c15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八月下旬。",
        "from": "T50n2057_001:0295c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持皇太子。",
        "from": "T50n2057_001:0295c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三日得差。",
        "from": "T50n2057_001:0295c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各賜絹二十匹吳綾五十匹。",
        "from": "T50n2057_001:0295c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "茶二十串。",
        "from": "T50n2057_001:0295c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臣等一界微僧。",
        "from": "T50n2057_001:0295c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謝聖慈賜綾絹及茶。",
        "from": "T50n2057_001:0295c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不勝頂賀。",
        "from": "T50n2057_001:0295c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十六年冬十一月十六日。",
        "from": "T50n2057_001:0295c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神威軍焦護軍。",
        "from": "T50n2057_001:0295c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請和上軍宅供養。",
        "from": "T50n2057_001:0295c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并和上寫真。",
        "from": "T50n2057_001:0295c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "裝飾送院。",
        "from": "T50n2057_001:0295c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十八年。",
        "from": "T50n2057_001:0295c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和上得疾漸重。",
        "from": "T50n2057_001:0295c20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進狀請退。",
        "from": "T50n2057_001:0295c21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恩命放歸。",
        "from": "T50n2057_001:0295c21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且令寺將息。",
        "from": "T50n2057_001:0295c21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　朕意欲存終始。",
        "from": "T50n2057_001:0295c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "贊即不得。",
        "from": "T50n2057_001:0295c22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其年八月中旬。",
        "from": "T50n2057_001:0295c22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨衣鉢。",
        "from": "T50n2057_001:0295c22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "付屬義明等七人授用。",
        "from": "T50n2057_001:0295c23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貞元十九年。",
        "from": "T50n2057_001:0295c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日本國僧空海。",
        "from": "T50n2057_001:0295c23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉　勅將摩衲及國信物五百餘貫文。",
        "from": "T50n2057_001:0295c24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉上和上。",
        "from": "T50n2057_001:0295c25_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡將修飾道場供養。",
        "from": "T50n2057_001:0295c25_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求授大悲胎藏金剛界。",
        "from": "T50n2057_001:0295c25_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并諸尊瑜伽教法。",
        "from": "T50n2057_001:0295c26_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經五十本。",
        "from": "T50n2057_001:0295c26_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "登時見境界梵阿字日月輪。",
        "from": "T50n2057_001:0295c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現入口中。",
        "from": "T50n2057_001:0295c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貞元二十一年。",
        "from": "T50n2057_001:0295c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八月五日。",
        "from": "T50n2057_001:0295c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "改為永貞元秊。",
        "from": "T50n2057_001:0295c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二月十五日。",
        "from": "T50n2057_001:0295c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北首掩終。",
        "from": "T50n2057_001:0295c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至元和元年正月十七日。",
        "from": "T50n2057_001:0295c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弟子道俗約千餘人。",
        "from": "T50n2057_001:0296a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "送葬。",
        "from": "T50n2057_001:0296a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至孟村龍原大師塔側。",
        "from": "T50n2057_001:0296a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "厥後至寶曆二年八月二十一日。",
        "from": "T50n2057_001:0296a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義一。",
        "from": "T50n2057_001:0296a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深達。",
        "from": "T50n2057_001:0296a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義丹。",
        "from": "T50n2057_001:0296a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滻川之側。",
        "from": "T50n2057_001:0296a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "表藺村。",
        "from": "T50n2057_001:0296a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建塔移葬。",
        "from": "T50n2057_001:0296a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開成四年。",
        "from": "T50n2057_001:0296a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正月十三日。",
        "from": "T50n2057_001:0296a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日本國僧圓行。",
        "from": "T50n2057_001:0296a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將法衣信物。",
        "from": "T50n2057_001:0296a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(寫本批云)",
        "from": "T50n2057_001:0296a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "承安元年(大歲辛卯)八月二十五日戌剋書畢中川本也觀應元年九月晦日。",
        "from": "T50n2057_001:0296a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於東寺西院僧坊。",
        "from": "T50n2057_001:0296a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書寫之訖。",
        "from": "T50n2057_001:0296a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為一具抄中以足之也。",
        "from": "T50n2057_001:0296a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大法師跋他羅摩抳",
        "from": "T50n2057_001:0296a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一校了文字多誤",
        "from": "T50n2057_001:0296a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明曆四年(戊戌)姑洗中旬加修復畢不可及外見也　　　　　　　　　　　(改杲快)",
        "from": "T50n2057_001:0296a13_0",
        "context_with_next_sentence": false
    }
]