[
    {
        "source_sentence": "阿毘達磨俱舍論實義疏卷第五",
        "from": "T29n1561_005:0327a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊者安惠造",
        "from": "T29n1561_005:0327a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分別根品第二之一(論卷第三)",
        "from": "T29n1561_005:0327a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌曰。",
        "from": "T29n1561_005:0327a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　傳說五於四　　四根於二種",
        "from": "T29n1561_005:0327a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　五八染淨中　　各別為增上",
        "from": "T29n1561_005:0327b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T29n1561_005:0327b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼等五根至聞聲別故。",
        "from": "T29n1561_005:0327b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌云。",
        "from": "T29n1561_005:0327b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　譬如明眼人　　能避現險難",
        "from": "T29n1561_005:0327b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　世有聰明者　　能離當苦惠",
        "from": "T29n1561_005:0327b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　多聞能知法　　多聞能離罪",
        "from": "T29n1561_005:0327b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　多聞捨無義　　多聞得涅槃",
        "from": "T29n1561_005:0327b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌曰。",
        "from": "T29n1561_005:0327b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　身由食住　　命託食存　　食已令心　適悅安泰若男入胎於母生愛於父生恚。",
        "from": "T29n1561_005:0327b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若女入胎於父生愛於母生恚。",
        "from": "T29n1561_005:0327b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由如故知能續後有。",
        "from": "T29n1561_005:0327b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆是染心。",
        "from": "T29n1561_005:0327b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是以意根於續後有。",
        "from": "T29n1561_005:0327b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經云。",
        "from": "T29n1561_005:0327b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父母精血入胎者。",
        "from": "T29n1561_005:0327b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識所攝持方能成就羯剌藍也。",
        "from": "T29n1561_005:0327b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋曰。",
        "from": "T29n1561_005:0327b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此顯身語隨心轉義。",
        "from": "T29n1561_005:0327b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如契經言。",
        "from": "T29n1561_005:0327b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心能導世間。",
        "from": "T29n1561_005:0327b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心能遍攝受。",
        "from": "T29n1561_005:0327b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是心一法。",
        "from": "T29n1561_005:0327b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆自在隨行。",
        "from": "T29n1561_005:0327b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌曰。",
        "from": "T29n1561_005:0327b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　了自境增上　　惣立於六根",
        "from": "T29n1561_005:0327b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　從身立二根　　女男情增上",
        "from": "T29n1561_005:0327b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　於同住雜住　　清淨增上",
        "from": "T29n1561_005:0327b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　應知命五受　　信等立為根",
        "from": "T29n1561_005:0327b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　未當知已知　　具知根亦爾",
        "from": "T29n1561_005:0327b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　於得後後道　　涅槃等增上",
        "from": "T29n1561_005:0327b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論頌云。",
        "from": "T29n1561_005:0327b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　身不悅名苦　　即此悅名樂",
        "from": "T29n1561_005:0327b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　及三定心悅　　餘處此名喜",
        "from": "T29n1561_005:0327b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　心不悅名憂　　中捨二無別",
        "from": "T29n1561_005:0327b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　見修無學道　　依九立三根",
        "from": "T29n1561_005:0327b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌曰。",
        "from": "T29n1561_005:0327b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　唯無漏後三　　有色命憂苦",
        "from": "T29n1561_005:0327c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　常知唯有漏　　通二餘九根",
        "from": "T29n1561_005:0327c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言最後三根定是無漏。",
        "from": "T29n1561_005:0327c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除此三根之外不通無漏。",
        "from": "T29n1561_005:0327c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "契經言。",
        "from": "T29n1561_005:0327c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊成正覺已作是思惟。",
        "from": "T29n1561_005:0327c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我所證法甚深微妙。",
        "from": "T29n1561_005:0327c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難見難解。",
        "from": "T29n1561_005:0327c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可思議。",
        "from": "T29n1561_005:0327c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非尋思境。",
        "from": "T29n1561_005:0327c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是智者所知。",
        "from": "T29n1561_005:0327c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非凡愚所及。",
        "from": "T29n1561_005:0327c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斯諸眾生久習邪教。",
        "from": "T29n1561_005:0327c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛著諸有。",
        "from": "T29n1561_005:0327c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸見所惑亂故不能解了。",
        "from": "T29n1561_005:0327c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以不了故不能信受。",
        "from": "T29n1561_005:0327c08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及生嬈惱。",
        "from": "T29n1561_005:0327c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我所言說無所利益。",
        "from": "T29n1561_005:0327c08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐沒劬勞。",
        "from": "T29n1561_005:0327c09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不如寂法受解脫樂。",
        "from": "T29n1561_005:0327c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是念已默然而住。",
        "from": "T29n1561_005:0327c09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時娑婆世界主大梵天王知佛世尊作如是念。",
        "from": "T29n1561_005:0327c10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即自思惟。",
        "from": "T29n1561_005:0327c11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此世界中諸眾生等多所失壞。",
        "from": "T29n1561_005:0327c11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今佛世尊證無上法。",
        "from": "T29n1561_005:0327c12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深廣微妙慮諸眾生不能信受。",
        "from": "T29n1561_005:0327c12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂處閑寂受現法樂。",
        "from": "T29n1561_005:0327c13_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我應詣彼勸請世尊轉妙法輪。",
        "from": "T29n1561_005:0327c14_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是念已。",
        "from": "T29n1561_005:0327c14_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如壯士屈申臂頃。",
        "from": "T29n1561_005:0327c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從梵宮沒。",
        "from": "T29n1561_005:0327c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至於佛所。",
        "from": "T29n1561_005:0327c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂禮佛足。",
        "from": "T29n1561_005:0327c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遶三匝已。",
        "from": "T29n1561_005:0327c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "却住一面。",
        "from": "T29n1561_005:0327c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌恭敬。",
        "from": "T29n1561_005:0327c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而白佛言。",
        "from": "T29n1561_005:0327c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "T29n1561_005:0327c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此世界中諸眾生等久習異教。",
        "from": "T29n1561_005:0327c17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見惑縈纏。",
        "from": "T29n1561_005:0327c17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流轉生死。",
        "from": "T29n1561_005:0327c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無所歸依。",
        "from": "T29n1561_005:0327c18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無所救護。",
        "from": "T29n1561_005:0327c18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊曠劫為諸眾生修行[(妳女)小+水]引。",
        "from": "T29n1561_005:0327c18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今者得成無上正覺。",
        "from": "T29n1561_005:0327c19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證無上法。",
        "from": "T29n1561_005:0327c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出現於世。",
        "from": "T29n1561_005:0327c20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如優曇花。",
        "from": "T29n1561_005:0327c20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為有情隨機演說。",
        "from": "T29n1561_005:0327c20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處於閑寂。",
        "from": "T29n1561_005:0327c21_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "靜默而住。",
        "from": "T29n1561_005:0327c21_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊。",
        "from": "T29n1561_005:0327c21_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降大悲心。",
        "from": "T29n1561_005:0327c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "哀愍世間。",
        "from": "T29n1561_005:0327c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸有情。",
        "from": "T29n1561_005:0327c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說正法要。",
        "from": "T29n1561_005:0327c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有諸眾生。",
        "from": "T29n1561_005:0327c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸結微薄。",
        "from": "T29n1561_005:0327c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "機宜成熟。",
        "from": "T29n1561_005:0327c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利根易化。",
        "from": "T29n1561_005:0327c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不聞法故。",
        "from": "T29n1561_005:0327c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然損減。",
        "from": "T29n1561_005:0327c24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "失大法利。",
        "from": "T29n1561_005:0327c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊。",
        "from": "T29n1561_005:0327c24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸眾生。",
        "from": "T29n1561_005:0327c24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開甘露門。",
        "from": "T29n1561_005:0327c25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說微妙法。",
        "from": "T29n1561_005:0327c25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸有情利益安樂。",
        "from": "T29n1561_005:0327c25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時大梵天王。",
        "from": "T29n1561_005:0327c26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勸請世尊。",
        "from": "T29n1561_005:0327c26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而說偈言。",
        "from": "T29n1561_005:0327c26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　世尊出現為優曇　　曠劫難逢今一遇　普為有情獲法利　　唯願廣開甘露門爾時世尊聞梵天王勸請偈已。",
        "from": "T29n1561_005:0327c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起大悲心。",
        "from": "T29n1561_005:0327c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以道眼觀諸有情。",
        "from": "T29n1561_005:0327c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有鈍根執著諸有。",
        "from": "T29n1561_005:0328a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有中根少厭生死。",
        "from": "T29n1561_005:0328a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有利根而不放逸。",
        "from": "T29n1561_005:0328a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是世尊觀諸世間一切有情。",
        "from": "T29n1561_005:0328a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處在世間或生或長。",
        "from": "T29n1561_005:0328a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "根有利鈍。",
        "from": "T29n1561_005:0328a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有易化得道。",
        "from": "T29n1561_005:0328a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如實知已。",
        "from": "T29n1561_005:0328a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受梵王請。",
        "from": "T29n1561_005:0328a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "許轉法輪。",
        "from": "T29n1561_005:0328a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而說偈言。",
        "from": "T29n1561_005:0328a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　大梵天王善諦聽　　我所得法甚微妙　今為汝等廣開演　　令於長夜獲安樂頌曰。",
        "from": "T29n1561_005:0328a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　命唯是異熟　　憂及後八非",
        "from": "T29n1561_005:0328a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　色意餘四受　　一一皆通二",
        "from": "T29n1561_005:0328a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論唯一命根定是異熟。",
        "from": "T29n1561_005:0328a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋曰。",
        "from": "T29n1561_005:0328a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無一命根非是異熟。",
        "from": "T29n1561_005:0328a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌曰。",
        "from": "T29n1561_005:0328a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　北洲定千年　　西東半日減",
        "from": "T29n1561_005:0328a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　此洲壽不定　　後十初叵量",
        "from": "T29n1561_005:0328a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "等。",
        "from": "T29n1561_005:0328a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間有情所餘福業。",
        "from": "T29n1561_005:0328a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸發願言。",
        "from": "T29n1561_005:0328a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願我長壽。",
        "from": "T29n1561_005:0328a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不願言願我住壽至於百年或住九十八十年等。",
        "from": "T29n1561_005:0328a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有尊人及親友等恒祝之言。",
        "from": "T29n1561_005:0328a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願汝長壽。",
        "from": "T29n1561_005:0328a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不定言爾所時等以此洲人所作諸業。",
        "from": "T29n1561_005:0328a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與貪心俱作是願。",
        "from": "T29n1561_005:0328a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "契經中說。",
        "from": "T29n1561_005:0328a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苾芻當知。",
        "from": "T29n1561_005:0328a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘婆尸佛時。",
        "from": "T29n1561_005:0328a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人壽八萬歲。",
        "from": "T29n1561_005:0328a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至迦葉波佛時。",
        "from": "T29n1561_005:0328a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人壽二萬歲。",
        "from": "T29n1561_005:0328a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今出世。",
        "from": "T29n1561_005:0328a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人壽百歲。",
        "from": "T29n1561_005:0328a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少出多減等。",
        "from": "T29n1561_005:0328a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽若無定限者。",
        "from": "T29n1561_005:0328a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊何故作如是說。",
        "from": "T29n1561_005:0328a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劫初諸根大種殊勝。",
        "from": "T29n1561_005:0328a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽即延長。",
        "from": "T29n1561_005:0328a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今時漸劣故壽難至于十歲。",
        "from": "T29n1561_005:0328a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福盡即死。",
        "from": "T29n1561_005:0328a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不因命也。",
        "from": "T29n1561_005:0328a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修故塔寺能延其壽。",
        "from": "T29n1561_005:0328a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌曰。",
        "from": "T29n1561_005:0328a25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　梵行妙成立　　聖道已善修",
        "from": "T29n1561_005:0328a26_0",
        "context_with_next_sentence": false
    }
]