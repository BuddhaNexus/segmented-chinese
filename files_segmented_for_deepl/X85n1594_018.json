[
    {
        "source_sentence": "佛祖綱目卷第十八(來字號)",
        "from": "X85n1594_018:0580c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甲子(漢宣帝五鳳元年起)戊辰(改元甘露)壬申(改元黃龍)癸酉(元帝初元元年)戊寅(改元永光)癸未(改元建昭)戊子(改元景寧)己丑(成帝建始元年)癸巳(改元河平)丁酉(改元陽朔)辛丑(改元鴻嘉)乙巳(改元永始)己酉(改元元延)癸丑(改元綏和)乙卯(哀帝建平元年)己未(改元元壽)辛酉(平帝元始元年)癸亥(漢平帝元始三年止)壬寅。",
        "from": "X85n1594_018:0580c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鴻嘉二年。",
        "from": "X85n1594_018:0580c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光祿大夫劉向。",
        "from": "X85n1594_018:0580c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "撰神仙傳云。",
        "from": "X85n1594_018:0580c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黃帝以下。",
        "from": "X85n1594_018:0580c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迄今。",
        "from": "X85n1594_018:0580c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得仙道者。",
        "from": "X85n1594_018:0580c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百四十九人。",
        "from": "X85n1594_018:0580c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其七十四人。",
        "from": "X85n1594_018:0580c18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已見佛經矣。",
        "from": "X85n1594_018:0580c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又言。",
        "from": "X85n1594_018:0580c19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "博觀典籍。",
        "from": "X85n1594_018:0580c19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往往見有佛經。",
        "from": "X85n1594_018:0580c19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "己未。",
        "from": "X85n1594_018:0580c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元壽元年。",
        "from": "X85n1594_018:0580c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "景憲使大月氏國。",
        "from": "X85n1594_018:0580c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還得浮圖經。",
        "from": "X85n1594_018:0580c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時莫有識者。",
        "from": "X85n1594_018:0580c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(戊申)十八祖伽耶舍多傳法鳩摩羅多伽耶舍多。",
        "from": "X85n1594_018:0580c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩提國人也。",
        "from": "X85n1594_018:0580c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姓鬱頭藍。",
        "from": "X85n1594_018:0580c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父天蓋。",
        "from": "X85n1594_018:0580c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母方聖。",
        "from": "X85n1594_018:0580c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甞夢大神持鑑。",
        "from": "X85n1594_018:0580c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因而有娠。",
        "from": "X85n1594_018:0580c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡七日而誕。",
        "from": "X85n1594_018:0580c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "肌體瑩如琉璃。",
        "from": "X85n1594_018:0580c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未甞洗沐。",
        "from": "X85n1594_018:0581a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然香潔。",
        "from": "X85n1594_018:0581a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幼好閑靜。",
        "from": "X85n1594_018:0581a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "語非常。",
        "from": "X85n1594_018:0581a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "童持鑑出遊。",
        "from": "X85n1594_018:0581a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遇僧伽難提尊者。",
        "from": "X85n1594_018:0581a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "付法後。",
        "from": "X85n1594_018:0581a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "領徒至大月氏國。",
        "from": "X85n1594_018:0581a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見一婆羅門舍有異氣。",
        "from": "X85n1594_018:0581a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍多將入彼舍。",
        "from": "X85n1594_018:0581a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍主鳩摩羅多問曰。",
        "from": "X85n1594_018:0581a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是何徒眾。",
        "from": "X85n1594_018:0581a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍多曰。",
        "from": "X85n1594_018:0581a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是佛弟子。",
        "from": "X85n1594_018:0581a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼聞佛號。",
        "from": "X85n1594_018:0581a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心神竦然。",
        "from": "X85n1594_018:0581a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即時閉戶。",
        "from": "X85n1594_018:0581a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍多良久。",
        "from": "X85n1594_018:0581a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "扣其門。",
        "from": "X85n1594_018:0581a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅多曰。",
        "from": "X85n1594_018:0581a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此舍無人。",
        "from": "X85n1594_018:0581a05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍多曰。",
        "from": "X85n1594_018:0581a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答無者誰。",
        "from": "X85n1594_018:0581a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅多聞語。",
        "from": "X85n1594_018:0581a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知是異人。",
        "from": "X85n1594_018:0581a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遽開門延接。",
        "from": "X85n1594_018:0581a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍多指其舍簷之下犬宿之處。",
        "from": "X85n1594_018:0581a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂羅多曰。",
        "from": "X85n1594_018:0581a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝父埋金於此。",
        "from": "X85n1594_018:0581a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今為犬以守之。",
        "from": "X85n1594_018:0581a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂掘地。",
        "from": "X85n1594_018:0581a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果得金。",
        "from": "X85n1594_018:0581a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復謂羅多曰。",
        "from": "X85n1594_018:0581a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔世尊記曰。",
        "from": "X85n1594_018:0581a08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾滅後一千年。",
        "from": "X85n1594_018:0581a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大士。",
        "from": "X85n1594_018:0581a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出現於月氏國。",
        "from": "X85n1594_018:0581a09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "紹隆玄化。",
        "from": "X85n1594_018:0581a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今汝值吾。",
        "from": "X85n1594_018:0581a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應斯嘉運。",
        "from": "X85n1594_018:0581a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是羅多發宿命智。",
        "from": "X85n1594_018:0581a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "投誠出家。",
        "from": "X85n1594_018:0581a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍多度之。",
        "from": "X85n1594_018:0581a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "授具已。",
        "from": "X85n1594_018:0581a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "付法眼。",
        "from": "X85n1594_018:0581a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "X85n1594_018:0581a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有種有心地。",
        "from": "X85n1594_018:0581a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因緣能發明。",
        "from": "X85n1594_018:0581a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於緣不相礙。",
        "from": "X85n1594_018:0581a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當生生不生。",
        "from": "X85n1594_018:0581a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伽耶舍多既付法已。",
        "from": "X85n1594_018:0581a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "踴身虗空。",
        "from": "X85n1594_018:0581a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現十八變化。",
        "from": "X85n1594_018:0581a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火光三昧。",
        "from": "X85n1594_018:0581a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自焚其身。",
        "from": "X85n1594_018:0581a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾以舍利建塔。",
        "from": "X85n1594_018:0581a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當此土漢成帝二十年戊申歲也。",
        "from": "X85n1594_018:0581a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛祖綱目卷第十八",
        "from": "X85n1594_018:0581a16_0",
        "context_with_next_sentence": false
    }
]