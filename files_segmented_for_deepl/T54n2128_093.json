[
    {
        "source_sentence": "一切經音義卷第九十三",
        "from": "T54n2128_093:0891a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翻經沙門慧琳撰",
        "from": "T54n2128_093:0891a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "續高僧傳卷第十一　從十一盡十六",
        "from": "T54n2128_093:0891a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[木*巳]梓(上音起木名也山海經東始之山有木狀如楊而赤理其汁如血無實名曰[木*巳]下茲死反南方木名也相梓漆堪作琴瑟說文並從木形聲字也)。",
        "from": "T54n2128_093:0891a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勍鋒(上競迎反左傳云勍敵之人也說文強也埋蒼健也廣雅武皃從力京聲也)。",
        "from": "T54n2128_093:0891a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竚聆(上除呂反字鏡竚立待也或作佇宁音訓同下力丁反韻詮云聆聽也或從立作聆說文從耳令聲)。",
        "from": "T54n2128_093:0891a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亟動(上矝力反說文從二從古人從口從又韻英云亟急也天地之間人於手口最急會意字也)。",
        "from": "T54n2128_093:0891a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自揣(初委反除也量也說文從手耑聲也)。",
        "from": "T54n2128_093:0891a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "齊鑣(表苗反今之馬銜上排沫也傳文從馬作騼非也)。",
        "from": "T54n2128_093:0891a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敻古(血縣反考聲云遠也營求也說文從人從皿旻聲也[ㄇ@儿]音營旻音兄域反)。",
        "from": "T54n2128_093:0891a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王頍(窺絹反說文頭小銳舉頭也從支頁窺音鈌規反)。",
        "from": "T54n2128_093:0891b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "砥礪(上音止下音例尚書孔注云砥細於礪皆磨石也山海經云出崦嵫山多此石)。",
        "from": "T54n2128_093:0891b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有瀅(縈定反水澄湛之皃也)。",
        "from": "T54n2128_093:0891b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[戀心+足]躄(上力傳反下卑亦反不具足也前已訓也)。",
        "from": "T54n2128_093:0891b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘄州(上音其)。",
        "from": "T54n2128_093:0891b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "層巘(上賊棱反下言謇反山巖也)。",
        "from": "T54n2128_093:0891b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "崤函(上戶交反下音咸也)。",
        "from": "T54n2128_093:0891b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寺墌(征亦反基也)。",
        "from": "T54n2128_093:0891b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "郿(音眉縣名也)。",
        "from": "T54n2128_093:0891b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卓犖(力卓反考聲云高皃也言其人英傑俊彥也)。",
        "from": "T54n2128_093:0891b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雄憨(呼濫反玉篇引毛詩傳憨愚也從心敢聲)。",
        "from": "T54n2128_093:0891b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常莅(梨至反考聲臨也從草位聲)。",
        "from": "T54n2128_093:0891b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "江漘(順春反韻英云水涯際也郭注爾雅云厓上平坦而下水深者為漘不發聲也從水脣聲也)。",
        "from": "T54n2128_093:0891b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "止泊(膊莫反王注楚辭云泊止也水中可居止曰泊從水白聲)。",
        "from": "T54n2128_093:0891b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鉦鼓(上之盈反毛詩傳曰鉦以靜之鼓以動之說文鐃也從金正聲)。",
        "from": "T54n2128_093:0891b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "麈尾(上音主毛扇也象麈鹿之尾以寶飾其柄名麈尾扇講論者以為談柄也)。",
        "from": "T54n2128_093:0891b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慰賻(符付反與附同音鄭注周禮云謂贈[(哭、)/巳]家死衣助不足也歸死者曰[賜勿+月]歸生者曰賻[賜勿+月]音豊諷反說文從貝尃聲)。",
        "from": "T54n2128_093:0891b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瘞于(於罽反爾雅云祭天曰燔柴祭地曰[厂@坴][卄/埋]郭璞曰幽藏也[病丙+坴]埋也)。",
        "from": "T54n2128_093:0891b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "續高僧傳卷第十二",
        "from": "T54n2128_093:0891b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恭撝(毀為反王注周易云左右指撝也形聲字也)。",
        "from": "T54n2128_093:0891b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恪慎(康各反韻英云敬也或作[各/心]愙並同)。",
        "from": "T54n2128_093:0891b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "確法師(腔角反腔音可江反確或作塙碻韻英云牢固皃也確定不移也)。",
        "from": "T54n2128_093:0891b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷覈(上端亂反斷決獄也不衡革反漢書云審其罪也說文考實事也從西西呀賈反敫聲敫音擊也)。",
        "from": "T54n2128_093:0891b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "暅公([罩卓+山]鄧反韻英云暅乾也形聲字亦兼會意字也)。",
        "from": "T54n2128_093:0891c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "商搉(音角顧野王云搉量也)。",
        "from": "T54n2128_093:0891c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "濤浦(上音桃下音普並從水)。",
        "from": "T54n2128_093:0891c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照鑠(下傷灼反郭璞曰鑠光明也形聲字也)。",
        "from": "T54n2128_093:0891c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孝凱(開改反韻英云振旅樂名也)。",
        "from": "T54n2128_093:0891c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "撤膳(上恥列反韻英云以手抽去也從手形聲字也)。",
        "from": "T54n2128_093:0891c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧昞(兵皿反皿音明丙反或昺昺明也人名也)。",
        "from": "T54n2128_093:0891c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幽賾(下柴責反幽深之稱也從[〡*臣][〡*臣]音夷)。",
        "from": "T54n2128_093:0891c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摳衣(口侯反禮記兩手摳衣考聲搴衣也從手)。",
        "from": "T54n2128_093:0891c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傅縡(音載人名也下縡事也)。",
        "from": "T54n2128_093:0891c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "霈然(音配考聲云霶霈多雨皃也)。",
        "from": "T54n2128_093:0891c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庂席(上音側書曰明明揚庂陋廣求賢人也)。",
        "from": "T54n2128_093:0891c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "踟蹰(上長知反或躊長[堊王+田]反下長誅反或作躊長於反二字各二體並通用考聲云徘徊也小疑未審不進也並從足形聲字也)。",
        "from": "T54n2128_093:0891c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "度棧(查限反險逕板道也從木戔聲戔音同上)。",
        "from": "T54n2128_093:0891c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捻掇(下端活反手拈取也拈音念兼反從手)。",
        "from": "T54n2128_093:0891c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冲邃(上逐融反下雖醉反幽深不測皃見小爾雅)。",
        "from": "T54n2128_093:0891c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恂恂(音巡恂恂不倦誘也)。",
        "from": "T54n2128_093:0891c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陟[阿可+古](上徵力反下音古後周時寺名也)。",
        "from": "T54n2128_093:0891c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彞倫(上音夷或從[雪雨]廾作彞彝常也古文作[(┐@一)/一/粉]也)。",
        "from": "T54n2128_093:0891c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "扃閉(上癸營反)。",
        "from": "T54n2128_093:0891c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淹漬(上掩炎反左傳久浸也下慈寺反說文漬漚也亦浸也)。",
        "from": "T54n2128_093:0891c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "打[利禾+(夕/小)](上徒丁反或作楟說文云打撞也撞音濁江也)。",
        "from": "T54n2128_093:0891c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繕構(上音善杜注左傳云繕治也說文補也下溝候反玉篇架屋也說文蓋也形聲字者也)。",
        "from": "T54n2128_093:0891c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廝下(先齎反考聲云曀也聲破也賤士也故廝下之徒也)。",
        "from": "T54n2128_093:0892a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "情槩(該礙反考聲云節也從木會意字也)。",
        "from": "T54n2128_093:0892a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[火*(晏女+(大/((犀尸)牛+十)))]裂(上色貌反火灹也有聲曰爆)。",
        "from": "T54n2128_093:0892a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徒跣(先剪反親於地不躡履也說文從足先聲)。",
        "from": "T54n2128_093:0892a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "躓[跳兆+疑](上知利反玉篇躓頓也下我蓋反考聲不進之也)。",
        "from": "T54n2128_093:0892a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "褒美(上保毛反讚美也)。",
        "from": "T54n2128_093:0892a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "焜煌(上魂穩反下音皇方言光盛皃焜燿也說文煌煇也形聲字)。",
        "from": "T54n2128_093:0892a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚫錫(初阠反阠音莘儭反傳文從貝非也韻英云親施也從口親聲也)。",
        "from": "T54n2128_093:0892a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "機捷(潛葉反考聲云慧也疾也戰勝也健也說文從手疌聲疌音同上)。",
        "from": "T54n2128_093:0892a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歔欷(上音虛下音希考聲云悲傷也泣之餘聲也並從欠形聲字也)。",
        "from": "T54n2128_093:0892a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "連洏(下耳之反韻略云連洏者泣淚流皃易曰泣血漣洏是也形聲字)。",
        "from": "T54n2128_093:0892a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擢知(音濁揀擇知也)。",
        "from": "T54n2128_093:0892a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高屧(暹葉反考聲云如屐而厚無足也之[尸@(ㄊ/木)][尸@(ㄊ/木)]音同以木為底也)。",
        "from": "T54n2128_093:0892a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "荷擔(荅南反韻詮云以木荷物也從手上音賀也)。",
        "from": "T54n2128_093:0892a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瀛洲(上音盈以征反)。",
        "from": "T54n2128_093:0892a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "荐餧(上錢廌反考聲云仍歲不熟曰荐說文從草存聲下奴猥反孔注論語云餧餓也轉注字從食委聲)。",
        "from": "T54n2128_093:0892a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斃者(毘袂反韻英云斃死也會意字也)。",
        "from": "T54n2128_093:0892a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "岷嶓(上密彬反彬音筆旻反蜀地山名也江水出焉下音波嶓眾山名也漢水上原也在漢中也)。",
        "from": "T54n2128_093:0892a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喎偏(上快垂反口偏也下音篇形聲字也)。",
        "from": "T54n2128_093:0892a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隩隅(上音奧下音愚並從阜也)。",
        "from": "T54n2128_093:0892a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自縊(下伊計反杜注左傳云自經死也說文從糸益聲)。",
        "from": "T54n2128_093:0892a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "商搉(下音角考聲專利也略也說文從手寉聲)。",
        "from": "T54n2128_093:0892a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歎訝(下牙稼反考聲)。",
        "from": "T54n2128_093:0892a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "秀峙(下馳里反考聲云山獨立也或作峙跱用並同者也)。",
        "from": "T54n2128_093:0892a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鏘鏘(鵲陽反嚴整皃)。",
        "from": "T54n2128_093:0892b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迎睇(下啼戾反前第四卷中已釋訖)。",
        "from": "T54n2128_093:0892b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廊廡(下音舞前第一卷已釋訖)。",
        "from": "T54n2128_093:0892b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "締構(上音蹄王注楚辭云締構也架構堂也)。",
        "from": "T54n2128_093:0892b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恢恢(苦回反大也)。",
        "from": "T54n2128_093:0892b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "胷襟(上音凶說文膺也或作匈亦通也)。",
        "from": "T54n2128_093:0892b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智琚(音居僧名也)。",
        "from": "T54n2128_093:0892b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父褘(偣希反琚師父名也從衣)。",
        "from": "T54n2128_093:0892b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "罕醍(上老刀反正體字也從宀省從牛造字本意)。",
        "from": "T54n2128_093:0892b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "葷辛(上暉雲反前高僧傳第十二卷已具釋訖也)。",
        "from": "T54n2128_093:0892b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舊垗(下音奧說文云古文奧字也文字典說云土窀也又趙姚二音說文窯也燒瓦竈也傳作烑非也)。",
        "from": "T54n2128_093:0892b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鍬鍤(上峭霄反玉篇從喿作鐰方言云趙魏之間謂鍤郭注爾雅及言作鍫訓亦鍤爾雅又作庣音同下楚甲反鍤亦鍫也或作[疀田+缶]古字也臿音同上也)。",
        "from": "T54n2128_093:0892b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安叡(悅慧反說文叡智也從目從[(├/(同(一/口)+、))*又][(├/(同(一/口)+、))*又]音殘從谷省也)。",
        "from": "T54n2128_093:0892b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殲(上接鹽反毛詩傳殲盡也何注公羊殲死也滅絕也從歺歺音矜)。",
        "from": "T54n2128_093:0892b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幽洌(下連哲反周易井洌寒泉食王注云洌潔也寒也清也形聲字)。",
        "from": "T54n2128_093:0892b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "續高僧傳卷第十三",
        "from": "T54n2128_093:0892b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智璀(崔磊反人名也)。",
        "from": "T54n2128_093:0892b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[貝*親]遺(上測近反施物也下惟恚反考聲云加也與也相惠也)。",
        "from": "T54n2128_093:0892b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蕭瑀(于迂反與禹同音唐朝南省長官也)。",
        "from": "T54n2128_093:0892b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "溫彞(以脂反聲云常也人名者也)。",
        "from": "T54n2128_093:0892b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諠譁(上音喧下音花多人也)。",
        "from": "T54n2128_093:0892b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[濂兼+(朔月)]流(上音索逆流而上也見韻詮說文從水[蝷虫]聲也)。",
        "from": "T54n2128_093:0892b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猜隱(釆哉反意不能明也似犬多疑故從犬青聲也)。",
        "from": "T54n2128_093:0892b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼迾(上頗鹽反正體兼字會意字手執二禾曰兼下音列考聲清也潔也寒之也)。",
        "from": "T54n2128_093:0892c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞥見(片滅反字集略云蹔見也纔見不久也說文從目敝聲)。",
        "from": "T54n2128_093:0892c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愔鬱(上壹淫反和悅皃也靜也下經律前文已釋訖)。",
        "from": "T54n2128_093:0892c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "溟渤(上音冥下音勃東海之別名也)。",
        "from": "T54n2128_093:0892c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[垂┴+木]舶(下音白海中大船也纍枋木為之板薄不禁大波浪以椰子皮索連之不用鐵丁恐相磨火出千人共駕長百文大船也)。",
        "from": "T54n2128_093:0892c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緬謝(上綿遍反緬想也)。",
        "from": "T54n2128_093:0892c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "糅以(上尼救反鄭注儀禮云雜也古今正字從米柔聲或作[米*(刃/一)]音同上又音而救反)。",
        "from": "T54n2128_093:0892c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[絢日+目]彩(上血絹反鄭注儀禮云彩文盛曰絢馬注論語云絢文皃說文從糸[句口+目]聲[句口+目]音縣糸音覓)。",
        "from": "T54n2128_093:0892c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "織綜(下宗送反列女傳織者可以喻政推而去引而來曰綜說文從糸宗聲也)。",
        "from": "T54n2128_093:0892c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "負囊(下諾郎反毛詩傳曰有底曰囊無底曰橐橐音託也)。",
        "from": "T54n2128_093:0892c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[踦可+(舛夕)]轢(上誇寡反前第四卷已釋訖下零的反蒼頡篇云轢[車*及]也呂氏春秋陵轢諸俟是也說文云轢謂車所軗也從車樂聲[車*及]音尼展反)。",
        "from": "T54n2128_093:0892c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頿髯(上子斯反左傳云靈王生而有頿說文云口上須也從須此聲傳文從髟作髭亦同下染占反髯即頰須也從須冉聲莊生云美須[((彰章)/冉)*頁][((彰章)/冉)*頁]即頰毛也傳文作髯亦通時用字也冉音同上也)。",
        "from": "T54n2128_093:0892c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坑坎(上客庚反或從阜作阬顧野王云坑壑也亦池漸也說文坑墟也從土亢聲下堪敢反考聲云小坑)。",
        "from": "T54n2128_093:0892c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祖禰(上租古反爾雅祖始也顧野王云祖上也廣雅本也遠也說文祖始廟也從示且聲下泥底反古文作禰今俗從草[肄聿+(企止+米)]作檷周禮父廟也文字典說從示爾聲轉注字)。",
        "from": "T54n2128_093:0892c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "睆爾(上還綰反論語云睆爾小笑皃也毛詩美皃也說文從目皃聲皃音治官反)。",
        "from": "T54n2128_093:0892c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灑塵(上沙雅反水灑地也)。",
        "from": "T54n2128_093:0892c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刺服(上此四反正體[利禾+夾]字也)。",
        "from": "T54n2128_093:0892c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鉎刃(上音生於柔鐵上火臨生鐵也令其剛利)。",
        "from": "T54n2128_093:0892c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鵬舉(上音朋大鳥名也形聲字下舉字下從手)。",
        "from": "T54n2128_093:0892c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "林藪(下桑走反鄭注周禮云澤無水曰藪說文大澤也從草藪聲也)。",
        "from": "T54n2128_093:0892c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衒才(上玄絹反說文行賣也從行玄聲亦作[彳*言*亍])。",
        "from": "T54n2128_093:0893a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "桎梏(上音質下音谷亦名杻械在手曰桔在足曰桎說文二字並從木形聲字也)。",
        "from": "T54n2128_093:0893a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬻德(上融祝反周禮鬻賣也說文從鬲粥聲也鬲音歷也)。",
        "from": "T54n2128_093:0893a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嘲謔(上張交反或從口作啁蒼頡篇云嘲調也戲也下香約反郭注爾雅云以言相調戲也形聲字)。",
        "from": "T54n2128_093:0893a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亹斖(微匪反上正下俗前文已具釋訓訖)。",
        "from": "T54n2128_093:0893a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汪濊(上艧黃反艧音尫郭反杜注左傳云汪污池也楚辭注云大水深廣也下隈外反韓詩云水浸流也說云濊疑流也從水歲聲正體歲字也)。",
        "from": "T54n2128_093:0893a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玄詧(音察人名也)。",
        "from": "T54n2128_093:0893a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "駐蹕(上誅屢反蒼頡篇云駐止也住也下音畢鄭注周禮云君車行清道即今之警蹕也或從走作[起巳+畢]或從言作[言*畢]義訓並同說文從足也)。",
        "from": "T54n2128_093:0893a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "久頒(入蠻反或作班左傳頒布也列也說文從分分字從八從刀作分)。",
        "from": "T54n2128_093:0893a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擺撥(上百賣反說文揮手也或從畢作[打丁+畢]並從手下半末反說文以手撥物也從手發聲也)。",
        "from": "T54n2128_093:0893a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "管轄(閑戞反戞音間八反車軸頭堅鐵也)。",
        "from": "T54n2128_093:0893a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縶維(上知立反繫也禁也下音惟方角也)。",
        "from": "T54n2128_093:0893a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倜儻(上天的反下湯朗反司馬相如封禪支云倜儻窮變驍健也廣雅卓異也並從人形聲字也)。",
        "from": "T54n2128_093:0893a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[大/(舛夕)]父([踦可+(舛夕)]瓜反[病丙+土]子云與曰角走渴死於北地說文從大[舛夕]聲[舛夕]音苦封反論作夸俗字也)。",
        "from": "T54n2128_093:0893a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲[耳*希](下音希說文[耳*希]望也或作希)。",
        "from": "T54n2128_093:0893a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瀅中(上縈定反前第十卷已說)。",
        "from": "T54n2128_093:0893a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捫淚(上音門聲類云捫猶摸也從手門聲)。",
        "from": "T54n2128_093:0893a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傀偉(上公回反方言云傀盛也司馬注莊子云美也下韋鬼反廣雅奇也二字並從人)。",
        "from": "T54n2128_093:0893a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樊綽(下昌若反人名也)。",
        "from": "T54n2128_093:0893a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐殯(下賓刃反鄭注禮記死者在棺未葬停西堦之上曰殯說文從歺賓聲傳文從土作[土*賓]非也正體從丐)。",
        "from": "T54n2128_093:0893a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淋落(上音林下音洛說文以水沃聲)。",
        "from": "T54n2128_093:0893a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "續高僧傳卷第十四",
        "from": "T54n2128_093:0893a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧頵(下於倫反僧名也)。",
        "from": "T54n2128_093:0893b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧瑜(下廋珠反亦僧名也)。",
        "from": "T54n2128_093:0893b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父珉(下密彬反知琳父名也)。",
        "from": "T54n2128_093:0893b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昂形(上我岡反案昂形即昂藏丈夫之稱也說文云從曰卭聲字書從卩作昂卭音同上)。",
        "from": "T54n2128_093:0893b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皭法師(上音爵史記徐廣注云白浮貌說文青白色從爵從白傳文從肉作[月*爵]非也爵字正體也)。",
        "from": "T54n2128_093:0893b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "邳國(上被悲反左傳奚仲遷于邳即今下邳是也說文從邑丕聲丕音披)。",
        "from": "T54n2128_093:0893b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鋒鏑(上音烽下音丁歷反前高僧傳第一卷中已具釋訖)。",
        "from": "T54n2128_093:0893b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未爇(下擩拙反壖音耎專反杜注左傳云[蓺/火]燒也然也說文從火蓺聲或從火作[火*苪]音訓並同也)。",
        "from": "T54n2128_093:0893b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "浹旬(咸甲反韻詮云浹潤也十日名旬從甲至癸也)。",
        "from": "T54n2128_093:0893b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凜懷(上林錦反考聲云凜敬懼也心晤也埤蒼云懍悷也古今正字從冰稟聲下懷乖反論語懷安也韻英思也說文從心褱聲音同上)。",
        "from": "T54n2128_093:0893b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蹊逕(上系鷄反糸音奚計反杜注左傳云蹊即逕也鄭注禮記蹊禽獸道也古今正字從足奚聲下經脛反鄭注周禮云逕道也所以通車徒於國都也鄭注禮記云逕直渡也說文作徑正也)。",
        "from": "T54n2128_093:0893b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迴旟(下與徐反韻英云隼旟也畫鷹隼於上取其迅疾也郭注爾雅云剝鳥皮置之竿頭言其勇也韻詮云旟行如鷹隼之疾也說文從[施也]與聲)。",
        "from": "T54n2128_093:0893b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歧嶷(上巨移韻英云歧路也下凝棘反或從口作[口*疑]言其語勢高大如岐山也)。",
        "from": "T54n2128_093:0893b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縉紳(上音晉下音申案縉紳者大帶也作紅白色古者衣冠之貴飾也書以記事也)。",
        "from": "T54n2128_093:0893b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "履軾(上履字說文從尸從彳從久舟以麻作之下音式韻英云車前橫木也敬也廣雅車[車*秋]謂之軾以綵結馬尾為軾也)。",
        "from": "T54n2128_093:0893b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[宋木+禹]內(上音宇宇宙之內也)。",
        "from": "T54n2128_093:0893b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[溘去+太]爾(堪鴿反楚辭云寧[溘去+太]死以流亾考聲云大水忽至或作溘亦通)。",
        "from": "T54n2128_093:0893b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高邈(下庬剝反毛詩傳邈大皃王注楚辭云遠也方言廣也曠遠之皃也)。",
        "from": "T54n2128_093:0893b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "城塹(上城字正體中從丁世本云[骨*系]作城淮南子玄[骨*系]作九仞之城七尺曰仞說文從士成聲下妾焰反字書塹城隍也說文從土斬聲)。",
        "from": "T54n2128_093:0893b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆[卄/造](下初瘦反杜注左傳云[卄/造]副也韻詮云草根也說文從草造之聲也)。",
        "from": "T54n2128_093:0893c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "氓庶(上音盲毛詩傳氓民也鄭注周禮云[(懞一)卄+((並(前刖))一)]也無知皃也)。",
        "from": "T54n2128_093:0893c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擐甲(還慣反杜注左傳云擐穿貫也說文從手睘聲貫音官也)。",
        "from": "T54n2128_093:0893c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闔境(含蛤反爾雅云[門@直]謂之雇閉也蛤音甘荅反)。",
        "from": "T54n2128_093:0893c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萎悴(上委為反毛詩傳云草木死壞也下情遂反郭注方言悴傷也說文憂也從心)。",
        "from": "T54n2128_093:0893c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "談謔(下香約反傳文從虎作諕書誤也前第五卷已訓釋)。",
        "from": "T54n2128_093:0893c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "砥礪(上之耳反下力制反二字皆磨石也)。",
        "from": "T54n2128_093:0893c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "支派(下拍賣反玉篇云水分流說文從反書永字也)。",
        "from": "T54n2128_093:0893c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斲石(上音卓孔注尚書斲斫也或作[石*豕]爾雅云削也說文亦斫也從斤[斲斤]聲也[斲斤]音豆也)。",
        "from": "T54n2128_093:0893c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為龕(下坎甘反韻詮云龍穴也土室也韻英云龍貌也說文著佛像處從龍從含省聲也)。",
        "from": "T54n2128_093:0893c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "邛僰(上共恭反漢書蜀郡有卭縣說文從邑工聲下朋北反聲類云僰者羗之別種說文棘捷為蠻夷從人棘聲也棘音矜力從並二朿不是束)。",
        "from": "T54n2128_093:0893c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酇國(上藏何反說文南陽有酇縣沛郡亦有酇縣贊正體贊字)。",
        "from": "T54n2128_093:0893c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天潢(晃光反杜注左傳潢污池也停水處也亦積水也一云潢潦也)。",
        "from": "T54n2128_093:0893c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝系(說文云帝者王天下之號也從古文上字朿聲也古文作帝下奚計反世本有帝糸篇子孫相承繼續不斷絕也說文云系繫也從糸[必心]聲[必心]音曳也)。",
        "from": "T54n2128_093:0893c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八埏(下演栴反漢書音義延地極邊際也古今正字從土延聲傳文從手非也)。",
        "from": "T54n2128_093:0893c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智[契大+石](良酌反僧名也韻英利也)。",
        "from": "T54n2128_093:0893c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吳濞(下披萸反吳王劉濞名也)。",
        "from": "T54n2128_093:0893c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滬瀆(上胡古反山海經云黃帝臨玄滬之水靈龜負圖而出即此水也文字典說從水扈聲扈音同上下音獨)。",
        "from": "T54n2128_093:0893c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "揮飊(上音暉下比宵反韻英云風名也郭注爾雅云[日/出/八/((犀尸)牛+十)]風從上向下曰飇猋音同上從三犬不從火)。",
        "from": "T54n2128_093:0893c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闞迴(上堪濫反蒼頡篇云闞視也或從目作瞰訛義同說文從門敢聲下熒炅反爾雅迴遠也說文從辵冋聲也)。",
        "from": "T54n2128_093:0893c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道[思*劦](下嫌頰反爾雅[思*(刀/(刀*刀))]和也說文思也形聲字)。",
        "from": "T54n2128_093:0893c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "哮吼(上孝交反埤蒼云哮赫怒也說文從口孝聲下訶苟反楚辭熊羆嘷吼考聲牛虎吼聲也)。",
        "from": "T54n2128_093:0893c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遷[(夕*(ㄗ@、))/貝](上音千賈注國語云易也從也杜注左傳云移也說文登也從辵[遷(這言)]聲[遷(這言)]音千下矛候反玉篇云[夗/貝]交易說文易財也從貝夘聲也)。",
        "from": "T54n2128_093:0894a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汲冢(上音急地名也汲郡有安斄王古冢晉時有人盜發此冡得竹簡數束皆古之墳典也時俗号為汲冡記說文冢字從包從豖豖音丑錄反豖亦聲也)。",
        "from": "T54n2128_093:0894a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而陿(下咸甲反顧野王云陿謂不廣大也說文從阜匧聲也匧音謙葉反傳文從手作[打丁+(匚@夾)]非也)。",
        "from": "T54n2128_093:0894a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帷扆(上葦悲反鄭注周禮云在傍曰帷廣雅帷帳也說文從巾隹聲下衣豈反郭注爾雅云[窗/心]東西曰扆說文從戶衣聲者也)。",
        "from": "T54n2128_093:0894a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賜縑(下頰嫌反廣雅緝謂縑說文縑絲繒也從糸兼聲也)。",
        "from": "T54n2128_093:0894a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "笠澤(上音立下音宅字書云笠澤吳地澤名也此澤無樹木常張蓋笠而行因以名焉從竹從立)。",
        "from": "T54n2128_093:0894a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冕瑬(上音免鄭注周禮云六服同名冕此首飾也尊又云冕者延上之覆也世本云黃帝作冕說文云從曰日音[幌光+月]免聲下音流毛詩傳曰[沆几+(加/王)]章尊者之首飾垂珠以蔽忽也說文從玉流聲形聲字也)。",
        "from": "T54n2128_093:0894a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賻贈(上音附韻詮云以財帛助[(哭、)/(匚@一)]家不足曰賻說文從貝尃聲尃音[敷方+寸]或作[賜勿+月]豊夢反韻英云以車馬送死曰[賜勿+月]說文從貝[曰/月]聲[曰/月]音[悍干+月]下賊鄧反以財相遺也玉篇云贈饋也韻英以物相送也說文從貝曾聲)。",
        "from": "T54n2128_093:0894a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遞給(啼禮反上聲字前第一已釋也)。",
        "from": "T54n2128_093:0894a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧驡(臧朗反或作駔僧名也王逸注楚辭云駿馬也訓釋字義駔同一云千里馬也會意字駿音俊也)。",
        "from": "T54n2128_093:0894a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "琬琰(上於遠反玉篇云琬玉圭也下閻染反說文云玉圭長九寸執以為信以征不義也)。",
        "from": "T54n2128_093:0894a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧懁(恚玄反韻英急之也)。",
        "from": "T54n2128_093:0894a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "暠師(上音高老反人名也)。",
        "from": "T54n2128_093:0894a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獷厲(上虢猛反下力滯反獷厲惡性也)。",
        "from": "T54n2128_093:0894a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "續高僧傳卷第十五",
        "from": "T54n2128_093:0894a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧眺(下[袖由+兆]詔反僧名也傳文從月作朓誤)。",
        "from": "T54n2128_093:0894a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "踰閫(上庾珠反毛詩傳云踰越也廣雅云渡也說文從足俞聲下坤本反也鄭注禮記云閫門限也說文從木作梱門橛也)。",
        "from": "T54n2128_093:0894a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嗚咽(上屋孤反考聲云悲歎之聲字書亦從欠作歍下煙結反字書考聲亦從壹作噎聲類云不平聲也亦氣逆也)。",
        "from": "T54n2128_093:0894a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有娠(下失真反字書正從人作[仁二+身]廣雅云[仁二+身]即孕重任娠也娠[仁二+身]音義同說文從人從身聲)。",
        "from": "T54n2128_093:0894b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竹笴矟(次笴葛旱反尚書大傳云笴若射儀之笴括鄭玄注云箭笴也古今正字從竹可聲下雙捉反廣雅云矟矛也埤蒼云矟長文八尺矛也文字典說云今戟矟也從矛肖聲傳文從水作槊俗字者也)。",
        "from": "T54n2128_093:0894b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "芮城(上儒銳反今河東有芮城縣也)。",
        "from": "T54n2128_093:0894b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "紊亂(上聞憤反孔注尚書云紊乱說文云紊亦亂也從糸文聲亂正亂字也)。",
        "from": "T54n2128_093:0894b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[巳/廾]度(上移志反孔注尚書云[巳/廾]已也已猶退也說文從廾已聲廾音拱字)。",
        "from": "T54n2128_093:0894b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸[怡台+巠](下形冷反王逸注楚辭云[怡台+巠]恨也文字典說亦恨也說文從心巠聲巠音經)。",
        "from": "T54n2128_093:0894b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "補綻(上逋姥反顧野王云補猶塞其[門@報]治其蔽也說文從衣甫聲下棖限反裳[袖由+定]裂紉針請補綴鄭注禮記云椗解也文字典說云椗裂也從衣定聲)。",
        "from": "T54n2128_093:0894b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觸故(上衝欲反傳文作[象(豕一)+(革廿)]古文觸字廣雅云觸揬也說文云悅也從角蜀聲也)。",
        "from": "T54n2128_093:0894b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "澧鄠(上覆風反澧水名下胡故反縣名也已見前釋)。",
        "from": "T54n2128_093:0894b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偃仆(上謁蹇反孔注論語云偃僵仆也廣雅云偃猶仰也說文從人匽聲下朋北反字統云仆頓覆也考聲云前倒覆也文字典說從人卜聲者也)。",
        "from": "T54n2128_093:0894b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西院(下袁怨反許叔重曰院以候風雨也亦周垣也說文從阜從完聲完音桓傳文作[阿可+兒]誤也)。",
        "from": "T54n2128_093:0894b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南裔(下延曀反孔注尚書云裔末也杜注左傳云遠也廣雅云裔表也說文裔衣裙也從衣從冏聲冏音女滑反)。",
        "from": "T54n2128_093:0894b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "驩州(上音歡郡國志云驩州今交州安南管也)。",
        "from": "T54n2128_093:0894b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "揣文(上初委反郭璞注方言云揣度成也左傳云揣高卑之度說文云揣量也從手耑聲耑知桓反也)。",
        "from": "T54n2128_093:0894b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[米*萬]食(上蘭恒反淮南云糲粮之食也蒼頡篇云[米*萬][夕/鹿]米也亦脫粟米也說文從米從萬聲傳文從厂作糲俗字也)。",
        "from": "T54n2128_093:0894b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "琰皭(上鹽苒反下牆略反琰皭二字高僧名也已見前釋訖)。",
        "from": "T54n2128_093:0894b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坎壇(前第六卷已具釋訖)。",
        "from": "T54n2128_093:0894b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聯鑣(上列纏反鄭玄注周禮云古為連皆作聯聲類云聯綿不絕也說文云聯連也從耳從[聯耳]連不絕也[聯耳]音思下表苗反前第十卷已釋訖)。",
        "from": "T54n2128_093:0894b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陗急(上音悄謂山陵陗陖也廣雅云陗急也許叔重注淮南子云陗陖也亦嚴刻也說文亦陖也從阜肖聲傳文作峭俗字也)。",
        "from": "T54n2128_093:0894b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "續高僧傳卷第十六",
        "from": "T54n2128_093:0894c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "林藪(下蘇走反前第十三卷已釋訖)。",
        "from": "T54n2128_093:0894c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蕭淵(下壹玄反人名也)。",
        "from": "T54n2128_093:0894c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[這言+(雨/手)]擊(上狄形反前第十卷已釋訖下正擊字說文云擊支也從手[擊手]聲[擊手]音喫)。",
        "from": "T54n2128_093:0894c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧廩(下林錦反字書正作[稟禾]周禮[稟禾]人掌九穀之數鮮也鄭玄注禮云魯謂之米[稟禾]也說文云[稟禾]穀所振從禾作廩傳文從米作[稟禾+米]俗字)。",
        "from": "T54n2128_093:0894c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "荒荐(下錢羨反前第十一卷已釋訖)。",
        "from": "T54n2128_093:0894c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蹋[蹂矛+ㄊ]磗(上談合反次怙叶反傳文作[跳兆+冓]誤也下惰和反字書正從石作碢傳文從金作[堶土+金]俗字者也)。",
        "from": "T54n2128_093:0894c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧稠(下宙流反僧名也)。",
        "from": "T54n2128_093:0894c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "挻埴(上扇然反下承食反老子挻埴以為器河上公注云挻和也埴土也)。",
        "from": "T54n2128_093:0894c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "河涘(下鋤史反說文亦水涯也從水矣聲)。",
        "from": "T54n2128_093:0894c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳檄(下刑鷁反史記云張儀相秦為文檄楚相是也顧野王云槃書所以罪責當代者而曉喻譬慰百姓之書也漢書可傳檄千里定也說文從木敫聲)。",
        "from": "T54n2128_093:0894c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "澄汰(下達帶反廣雅云汰洗也郭注爾雅云以水去土許叔重注淮南子云達也說文從水太聲也)。",
        "from": "T54n2128_093:0894c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鉛墨(上悅宣反前第三卷已具釋訖)。",
        "from": "T54n2128_093:0894c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廖公(上了消反人名也)。",
        "from": "T54n2128_093:0894c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "託事(上傳文化杔木名誤也字書從言作託方言云託寄也又依也)。",
        "from": "T54n2128_093:0894c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一盋(下半末反下同已見前釋)。",
        "from": "T54n2128_093:0894c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廮陶(上纓郢反漢書云趙州有廮陶縣也)。",
        "from": "T54n2128_093:0894c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "挾帚(上嫌莢反何休注公羊傳云挾猶懷也說文云挾持也從手夾聲下周柳反世本云少康作箕帚說文云帚除糞也從又持巾掃冂內會意字者也)。",
        "from": "T54n2128_093:0894c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咆響(上鮑交反楚辭云熊羆咆也廣雅云咆鳴也說文云咆嘷也從口色聲也)。",
        "from": "T54n2128_093:0894c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "家默(上古寂字傳文從人作家誤說文正作寂無人聲也)。",
        "from": "T54n2128_093:0894c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摺山[离〦+山](上藍蛤反廣雅摺折也考聲云敗也說文云亦敗也從手習聲下恥知反字書正作[山/离]杜注左傳云[山/离]山神獸形也說文[山/离]亦猛獸也從禽頭從禸[艸屮]聲傳文從虫作螭非本義禸音而九反虫音毀也)。",
        "from": "T54n2128_093:0895a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虓虎(上孝交反毛詩傳云虎之自怒虓然謂虎怒聲也說文虓虎鳴也從虎九聲)。",
        "from": "T54n2128_093:0895a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "穨山(上兌迴反字書正作穨字也前第四卷已具釋訖也)。",
        "from": "T54n2128_093:0895a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鴟吻(上齒之反字書正殦字下聞粉反案鴟吻今殿堂結脊兩頭之飾也)。",
        "from": "T54n2128_093:0895a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重瞳(上厨龍反下童尚書大傳云舜目四瞳埤蒼云瞳目珠子也文字典說云從目從童聲)。",
        "from": "T54n2128_093:0895a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傴僂(上紆禹反下錄主反顧野王云傴僂身曲恭益加也廣雅背曲也說文云傴僂尫也二字並從人區婁皆聲區音駈婁音樓也)。",
        "from": "T54n2128_093:0895a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[滕月+舟]公(上鄧能反[滕月+舟]國名也漢書夏侯嬰之邑号也)。",
        "from": "T54n2128_093:0895a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酈食其(上音歷下音箕漢書云人姓名也)。",
        "from": "T54n2128_093:0895a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧瑋(下韋鬼反僧名也)。",
        "from": "T54n2128_093:0895a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輪奐(下歡玩反禮記云美哉輪焉美哉奐焉鄭注云奐明也前高僧傳第十卷已釋訖)。",
        "from": "T54n2128_093:0895a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旱澇(下牢到反考聲云澇水[洐亍+(唚口)]苗也禮記月令作潦云水潦敗其城郭也說文從水尞聲尞音力召反)。",
        "from": "T54n2128_093:0895a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "囂撓(上虗嬌反鄭注禮記云囂猶讙也說文云囂氣出頭從[器犬]頁聲[器犬]音戢頁音纈下拏咬反廣雅云撓亂也聲類云撓攪也云擾也從手堯聲也)。",
        "from": "T54n2128_093:0895a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "驪山(上利知反山海經云山名也已見前釋)。",
        "from": "T54n2128_093:0895a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譎對(上涓悅反鄭注論語云譎詐也鄭箋毛詩云不直言也說文云譎權詐也從言矞聲矞巨聿反下正對字已如前釋)。",
        "from": "T54n2128_093:0895a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鐫勒(上蕝宣反前第一卷已釋訖)。",
        "from": "T54n2128_093:0895a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繖蓋(上桑亶反下該愛反山名也東觀漢書云天雨天上騎持繖蓋從百餘騎上騎即上騎都尉也諸葛亮集云不得持繖蓋是也古今正字云繖即蓋也從糸散聲傳作傘俗字也)。",
        "from": "T54n2128_093:0895a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "揭坩(上[虔文+ㄆ]蘗反史記云揭高舉也說文從手曷聲下坎甘反方言云坩瓦器也古今正字從土甘聲)。",
        "from": "T54n2128_093:0895a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切經音義卷第九十三",
        "from": "T54n2128_093:0895a23_0",
        "context_with_next_sentence": false
    }
]