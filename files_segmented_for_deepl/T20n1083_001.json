[
    {
        "source_sentence": "觀世音菩薩如意摩尼陀羅尼經",
        "from": "T20n1083_001:0200b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐天竺三藏寶思惟譯",
        "from": "T20n1083_001:0200b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時觀世音菩薩。",
        "from": "T20n1083_001:0200b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊。",
        "from": "T20n1083_001:0200b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我有明呪法大壇。",
        "from": "T20n1083_001:0200b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名蓮花峯金剛加持祕密無礙觀世音蓮華如意摩尼轉輪心陀羅尼。",
        "from": "T20n1083_001:0200b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀世音心最勝成就。",
        "from": "T20n1083_001:0200b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊為能與一切眾生願成就故。",
        "from": "T20n1083_001:0200b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於世尊前我今當說。",
        "from": "T20n1083_001:0200b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊加護於我。",
        "from": "T20n1083_001:0200b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為一切呪仙雨大寶雨。",
        "from": "T20n1083_001:0200b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如大劫樹如如意摩尼。",
        "from": "T20n1083_001:0200b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當令一切眾生所願滿足。",
        "from": "T20n1083_001:0200b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言我已加持汝。",
        "from": "T20n1083_001:0200b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今當說一切悕求皆令滿足無能障礙觀世音心祕密與願。",
        "from": "T20n1083_001:0200b12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時觀世音菩薩摩訶薩。",
        "from": "T20n1083_001:0200b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往詣佛所頂禮佛足。",
        "from": "T20n1083_001:0200b14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右繞三匝於世尊前熙怡微笑。",
        "from": "T20n1083_001:0200b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大悲心說此呪王。",
        "from": "T20n1083_001:0200b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T20n1083_001:0200b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那謨喝囉怛曩(二合)怛囉(二合)[口*夜]吔(一)那摩阿哩吔(二)婆嚕吉帝說(長引聲)婆囉吔(三)菩提薩埵吔(四)摩訶薩埵吔(五)摩訶迦嚧膩迦吔(六)怛姪他(七)烏唵(二合八)斫迦囉(二合九)靺哩底(二合十)震哆末尼(十一)摩訶鉢特迷(二合十二)嚕嚕(十三)底瑟姹(二合十四)啜囉(二合十五)阿羯哩灑(二合)耶(十六)虎[合*牛](二合十七)泮吒(二合半聲十八)莎(引)訶(十九)此名根本呪次說心呪呪曰。",
        "from": "T20n1083_001:0200b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烏唵(二合一)鉢特摩(二合二)震哆末尼(平三)啜囉(二合四)虎[合*牛](二合五)次說隨心呪呪曰。",
        "from": "T20n1083_001:0200b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烏唵(二合一)嚩囉陀(二)鉢特迷(二合三)虎[合*牛](二合四)說此呪已。",
        "from": "T20n1083_001:0200b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大地應時六種震動。",
        "from": "T20n1083_001:0200b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切天宮亦皆搖動。",
        "from": "T20n1083_001:0200b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切龍宮亦皆戰掉。",
        "from": "T20n1083_001:0200c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切夜叉乾闥婆阿修羅迦樓羅緊那羅摩睺落伽。",
        "from": "T20n1083_001:0200c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種皆大戰掉。",
        "from": "T20n1083_001:0200c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切惡魔及頻那野迦。",
        "from": "T20n1083_001:0200c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作障礙者悉皆驚怖。",
        "from": "T20n1083_001:0200c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切魔宮無有光明。",
        "from": "T20n1083_001:0200c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切鄙惡眾生惡龍惡鬼女夜叉女羅剎。",
        "from": "T20n1083_001:0200c04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸極惡者伏面倒地。",
        "from": "T20n1083_001:0200c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸惡毒蟲藏竄孔穴。",
        "from": "T20n1083_001:0200c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切地獄無不散壞。",
        "from": "T20n1083_001:0200c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地獄眾生悉得解脫皆生天上。",
        "from": "T20n1083_001:0200c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於世尊前雨天供養花香衣服諸莊嚴具。",
        "from": "T20n1083_001:0200c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於虛空中奏諸天樂。",
        "from": "T20n1083_001:0200c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊以加陵頻伽美妙音聲。",
        "from": "T20n1083_001:0200c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讚觀世音菩薩言。",
        "from": "T20n1083_001:0200c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉汝為利益一切眾生故。",
        "from": "T20n1083_001:0200c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當說此呪功能。",
        "from": "T20n1083_001:0200c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀世音菩薩白佛言。",
        "from": "T20n1083_001:0200c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊若有善男子善女人比丘比丘尼。",
        "from": "T20n1083_001:0200c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲現前求諸利益願即獲者。",
        "from": "T20n1083_001:0200c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此如意呪印應當勤修。",
        "from": "T20n1083_001:0200c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不須作法不求宿日。",
        "from": "T20n1083_001:0200c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不須持齋不須洗浴不須別衣。",
        "from": "T20n1083_001:0200c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持之時不須辛苦。",
        "from": "T20n1083_001:0200c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但讀皆即成就。",
        "from": "T20n1083_001:0200c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦者辦諸事業。",
        "from": "T20n1083_001:0200c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能作百千種種事業。",
        "from": "T20n1083_001:0200c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有餘呪與此等者。",
        "from": "T20n1083_001:0200c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初能銷罪。",
        "from": "T20n1083_001:0200c17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往昔以來所積集者。",
        "from": "T20n1083_001:0200c17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所作事業皆令成就。",
        "from": "T20n1083_001:0200c18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今當說但讀及誦。",
        "from": "T20n1083_001:0200c18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即能成就無上之事。",
        "from": "T20n1083_001:0200c19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纔讀即令墮阿鼻者。",
        "from": "T20n1083_001:0200c19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆得清淨。",
        "from": "T20n1083_001:0200c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五無間者銷滅無餘病者皆除。",
        "from": "T20n1083_001:0200c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有患者但讀此呪即皆除差。",
        "from": "T20n1083_001:0200c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸毒呪術厭蠱皆不能害。",
        "from": "T20n1083_001:0200c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸瘡不著其身。",
        "from": "T20n1083_001:0200c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切鬼神頻那夜迦。",
        "from": "T20n1083_001:0200c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸毒刀杖風雨惡災。",
        "from": "T20n1083_001:0200c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魔軍及賊他軍王難怨家伺求。",
        "from": "T20n1083_001:0200c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毒蠱猛獸皆不能為害。",
        "from": "T20n1083_001:0200c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終不橫死不作惡夢。",
        "from": "T20n1083_001:0200c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戰伐鬪諍無不皆勝。",
        "from": "T20n1083_001:0200c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此等諸事纔讀即成。",
        "from": "T20n1083_001:0200c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脣內誦之滿一百遍。",
        "from": "T20n1083_001:0200c26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀世音菩薩即於其日現彼人前。",
        "from": "T20n1083_001:0200c26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與其所願皆令成就。",
        "from": "T20n1083_001:0200c27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又見一切諸佛如來。",
        "from": "T20n1083_001:0200c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及見西方無量壽佛。",
        "from": "T20n1083_001:0200c28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "極樂世界及菩薩會。",
        "from": "T20n1083_001:0200c29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "補特勒伽山中觀世音菩薩宮殿。",
        "from": "T20n1083_001:0200c29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其身清淨貴人供養眾人樂見。",
        "from": "T20n1083_001:0201a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "罪障蓋纏無不清淨。",
        "from": "T20n1083_001:0201a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所生之處得宿命智。",
        "from": "T20n1083_001:0201a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮華化生一切妙具。",
        "from": "T20n1083_001:0201a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆自裝嚴直至菩提。",
        "from": "T20n1083_001:0201a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永離惡趣究竟成佛。",
        "from": "T20n1083_001:0201a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "罪惡消滅善道清淨。",
        "from": "T20n1083_001:0201a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纔讀誦者皆即成就。",
        "from": "T20n1083_001:0201a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時觀世音菩薩。",
        "from": "T20n1083_001:0201a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說最上祕密功能。",
        "from": "T20n1083_001:0201a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若為利益哀愍一切眾生。",
        "from": "T20n1083_001:0201a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝取令伏遮止惡人。",
        "from": "T20n1083_001:0201a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令慈增長念誦即成。",
        "from": "T20n1083_001:0201a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能與眾生作大利益。",
        "from": "T20n1083_001:0201a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸智者得大安樂。",
        "from": "T20n1083_001:0201a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貨食增長富貴資具。",
        "from": "T20n1083_001:0201a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無不豐足色力滋盛。",
        "from": "T20n1083_001:0201a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此呪祕密不得妄說。",
        "from": "T20n1083_001:0201a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲真實成就無上如意摩尼大印念誦即成。",
        "from": "T20n1083_001:0201a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若已食若未食若淨若不淨。",
        "from": "T20n1083_001:0201a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每常誦念悉無過患。",
        "from": "T20n1083_001:0201a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦念之時當念憶觀世音菩薩永作依怙。",
        "from": "T20n1083_001:0201a13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若貴若賤若男若女沙門外道。",
        "from": "T20n1083_001:0201a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱其名字注心繫念。",
        "from": "T20n1083_001:0201a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而誦此呪應以後夜若平明時。",
        "from": "T20n1083_001:0201a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "男最貴者一千八遍。",
        "from": "T20n1083_001:0201a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "女最貴者一千十遍。",
        "from": "T20n1083_001:0201a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二貴男誦八百遍。",
        "from": "T20n1083_001:0201a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二貴女誦七百遍。",
        "from": "T20n1083_001:0201a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三貴女誦六百遍。",
        "from": "T20n1083_001:0201a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若婆羅門誦五百遍。",
        "from": "T20n1083_001:0201a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剎利四百遍。",
        "from": "T20n1083_001:0201a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "首陀三百。",
        "from": "T20n1083_001:0201a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘舍二百。",
        "from": "T20n1083_001:0201a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比丘一百八遍。",
        "from": "T20n1083_001:0201a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "女人一百三十遍。",
        "from": "T20n1083_001:0201a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "丈夫一百五十遍。",
        "from": "T20n1083_001:0201a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "童男六十。",
        "from": "T20n1083_001:0201a20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "童女九十。",
        "from": "T20n1083_001:0201a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此念誦法無不歸依。",
        "from": "T20n1083_001:0201a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝百由旬迅疾如風。",
        "from": "T20n1083_001:0201a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦八百遍觀音真身現前令見。",
        "from": "T20n1083_001:0201a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切所願皆能與之。",
        "from": "T20n1083_001:0201a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生明呪成就。",
        "from": "T20n1083_001:0201a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦十千遍。",
        "from": "T20n1083_001:0201a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖者執金剛真身現前令呪者見。",
        "from": "T20n1083_001:0201a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如所愛子鞠養抱持。",
        "from": "T20n1083_001:0201a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切明呪皆令成就。",
        "from": "T20n1083_001:0201a25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡有所欲願與之令滿。",
        "from": "T20n1083_001:0201a25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦十三千遍得見一切諸佛如來。",
        "from": "T20n1083_001:0201a26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦滿七日諸呪仙王。",
        "from": "T20n1083_001:0201a27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆以真身現呪者前。",
        "from": "T20n1083_001:0201a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各以所成明呪授與。",
        "from": "T20n1083_001:0201a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隱蔽其形隨逐擁護。",
        "from": "T20n1083_001:0201a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切安樂無不現前。",
        "from": "T20n1083_001:0201a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七日之內每日之中。",
        "from": "T20n1083_001:0201a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於後夜時誦三千遍。",
        "from": "T20n1083_001:0201a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝釋天王及諸天女。",
        "from": "T20n1083_001:0201b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下來歸依與明呪願。",
        "from": "T20n1083_001:0201b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等事但誦即成念誦法了。",
        "from": "T20n1083_001:0201b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時復說見者伏法無上成就。",
        "from": "T20n1083_001:0201b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若纔用者一切隨順。",
        "from": "T20n1083_001:0201b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牛黃　白檀香　欝金香　龍腦香　麝香　肉豆蔲　白豆蔲　丁香　紅蓮花　青蓮花　金赤土已上物等分。",
        "from": "T20n1083_001:0201b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用白石蜜和之。",
        "from": "T20n1083_001:0201b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是轉輪香。",
        "from": "T20n1083_001:0201b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦呪一千八遍而和合。",
        "from": "T20n1083_001:0201b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒以薰衣塗額塗眼瞼上塗身。",
        "from": "T20n1083_001:0201b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所去之處如日威光眾所樂見。",
        "from": "T20n1083_001:0201b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若在手者悉皆成就。",
        "from": "T20n1083_001:0201b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生若貴若賤。",
        "from": "T20n1083_001:0201b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自身及財亦皆歸伏。",
        "from": "T20n1083_001:0201b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時觀世音菩薩。",
        "from": "T20n1083_001:0201b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為利益一切眾生故。",
        "from": "T20n1083_001:0201b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說眼藥之法成就最上。",
        "from": "T20n1083_001:0201b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有用者即得成就決定無疑　摩那叱羅　雄黃　迦俱婆婆樹子汁　紅蓮花　青蓮花　海沫(一名海浮石)　牛黃　欝金根(一名黃薑)　小柏根　胡椒　畢撥　乾薑。",
        "from": "T20n1083_001:0201b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以前件藥。",
        "from": "T20n1083_001:0201b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並擣研為極細末。",
        "from": "T20n1083_001:0201b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以龍腦香麝香和之。",
        "from": "T20n1083_001:0201b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦心呪一千八遍。",
        "from": "T20n1083_001:0201b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨心呪一千八遍。",
        "from": "T20n1083_001:0201b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦根本大呪一千八遍。",
        "from": "T20n1083_001:0201b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以手取藥觸觀世音菩薩足。",
        "from": "T20n1083_001:0201b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即塗眼中已所有眼病。",
        "from": "T20n1083_001:0201b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至有目青盲胎努肉悉得除差。",
        "from": "T20n1083_001:0201b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二遍塗。",
        "from": "T20n1083_001:0201b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切壯熱頭痛半頭痛口病悉得除差。",
        "from": "T20n1083_001:0201b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三遍塗。",
        "from": "T20n1083_001:0201b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切猛惡鬼魅。",
        "from": "T20n1083_001:0201b25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及以瘨癎悉得除差。",
        "from": "T20n1083_001:0201b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四遍塗一切惡頻那夜迦悉得消滅。",
        "from": "T20n1083_001:0201b26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五遍塗一切怨家鬪諍悉皆得勝。",
        "from": "T20n1083_001:0201b27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六遍塗一切罪障諸毒。",
        "from": "T20n1083_001:0201b28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應墮地獄受無間罪悉得消滅。",
        "from": "T20n1083_001:0201b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切惡夢惡想不吉祥相。",
        "from": "T20n1083_001:0201b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百千集會悉皆消滅。",
        "from": "T20n1083_001:0201c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終不更起皆內眼藥。",
        "from": "T20n1083_001:0201c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第七日塗。",
        "from": "T20n1083_001:0201c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纔見一切男女貴賤道俗。",
        "from": "T20n1083_001:0201c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內外眷屬皆悉隨伏。",
        "from": "T20n1083_001:0201c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二七日塗。",
        "from": "T20n1083_001:0201c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得大自在。",
        "from": "T20n1083_001:0201c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三七日塗。",
        "from": "T20n1083_001:0201c04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當粟散位一切隨伏。",
        "from": "T20n1083_001:0201c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四七日塗一切夜叉夜叉女。",
        "from": "T20n1083_001:0201c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及眷屬悉皆隨伏。",
        "from": "T20n1083_001:0201c05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五七日塗。",
        "from": "T20n1083_001:0201c05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅剎女并眷屬悉皆隨伏。",
        "from": "T20n1083_001:0201c06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六七日塗幻術成者。",
        "from": "T20n1083_001:0201c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能現種種身皆受驅使。",
        "from": "T20n1083_001:0201c07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至菩提隨逐順伏。",
        "from": "T20n1083_001:0201c07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七七日塗摩訶迦羅。",
        "from": "T20n1083_001:0201c08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸女鬼神眷屬悉皆隨伏。",
        "from": "T20n1083_001:0201c08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八七日塗。",
        "from": "T20n1083_001:0201c09_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見隱形者。",
        "from": "T20n1083_001:0201c09_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九七日塗。",
        "from": "T20n1083_001:0201c09_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得見一切藏。",
        "from": "T20n1083_001:0201c09_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十七日塗。",
        "from": "T20n1083_001:0201c10_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切窟宅門皆得入。",
        "from": "T20n1083_001:0201c10_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十一七日塗。",
        "from": "T20n1083_001:0201c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入諸仙藥諸大園苑皆得成就。",
        "from": "T20n1083_001:0201c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切藥物草木諸神悉即現形。",
        "from": "T20n1083_001:0201c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二七日塗。",
        "from": "T20n1083_001:0201c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切山闈門皆得入十三七日塗。",
        "from": "T20n1083_001:0201c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至一切諸門無不得入。",
        "from": "T20n1083_001:0201c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸人無不得見。",
        "from": "T20n1083_001:0201c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十四七日塗。",
        "from": "T20n1083_001:0201c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲界諸天宮殿無不開者悉皆得見。",
        "from": "T20n1083_001:0201c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十五七日塗。",
        "from": "T20n1083_001:0201c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜見如晝。",
        "from": "T20n1083_001:0201c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十六七日塗。",
        "from": "T20n1083_001:0201c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠見地下所有諸事。",
        "from": "T20n1083_001:0201c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十七七日塗世界之中諸有地獄悉皆得見。",
        "from": "T20n1083_001:0201c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其受罪者皆得解脫。",
        "from": "T20n1083_001:0201c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十八七日塗之。",
        "from": "T20n1083_001:0201c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身有威德猶如日光。",
        "from": "T20n1083_001:0201c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黑暗之中所有窟宅悉皆得見。",
        "from": "T20n1083_001:0201c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十九七日塗之。",
        "from": "T20n1083_001:0201c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖者執金剛現來為證。",
        "from": "T20n1083_001:0201c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十七日塗之。",
        "from": "T20n1083_001:0201c21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀世音菩薩親來現前為證而住。",
        "from": "T20n1083_001:0201c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現其自身與一切願。",
        "from": "T20n1083_001:0201c22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心所悕求悉令滿足。",
        "from": "T20n1083_001:0201c22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十一七日塗之。",
        "from": "T20n1083_001:0201c23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得騰空無有障礙。",
        "from": "T20n1083_001:0201c23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及見一切諸佛剎土具見佛身。",
        "from": "T20n1083_001:0201c24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切菩薩所住之處悉皆得見。",
        "from": "T20n1083_001:0201c24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿足一年祕密塗藥五眼清淨。",
        "from": "T20n1083_001:0201c25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "等共眾生如前祕說。",
        "from": "T20n1083_001:0201c26_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆具得勿生疑惑。",
        "from": "T20n1083_001:0201c26_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是成就但讀及誦。",
        "from": "T20n1083_001:0201c27_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆當得一切如來轉輪。",
        "from": "T20n1083_001:0201c27_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是摩尼觀世音菩薩蓮花清淨名眼藥成就了復次說最生降伏成就然火之法壽命增長。",
        "from": "T20n1083_001:0201c28_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅一切罪離諸蓋纏。",
        "from": "T20n1083_001:0202a01_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切怨家無不降伏。",
        "from": "T20n1083_001:0202a01_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切障礙皆自消滅。",
        "from": "T20n1083_001:0202a02_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生自然歸伏。",
        "from": "T20n1083_001:0202a02_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白檀香　稻穀花(燒稻穀取花)白芥子(本無白字)酪蜜酥相和。",
        "from": "T20n1083_001:0202a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪一千八遍燒之。",
        "from": "T20n1083_001:0202a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以香木然火。",
        "from": "T20n1083_001:0202a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纔作此已一切罪障無間皆自消滅。",
        "from": "T20n1083_001:0202a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽命一千歲。",
        "from": "T20n1083_001:0202a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一七日燒得壽命二千歲。",
        "from": "T20n1083_001:0202a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身得清淨永離蓋纏。",
        "from": "T20n1083_001:0202a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二七日作法一切男女無不隨伏。",
        "from": "T20n1083_001:0202a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三七日作法三十三天諸有眷屬。",
        "from": "T20n1083_001:0202a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并日天等皆來隨伏。",
        "from": "T20n1083_001:0202a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切所願皆令滿足。",
        "from": "T20n1083_001:0202a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四天王等無不下來。",
        "from": "T20n1083_001:0202a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為常護養持呪者故。",
        "from": "T20n1083_001:0202a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖者執金剛與願成就。",
        "from": "T20n1083_001:0202a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀世音菩薩為與願者。",
        "from": "T20n1083_001:0202a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有悕求悉令滿足。",
        "from": "T20n1083_001:0202a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切喜見無不自在。",
        "from": "T20n1083_001:0202a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求雨法以酥和芥子。",
        "from": "T20n1083_001:0202a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三夜燒之諸龍下雨。",
        "from": "T20n1083_001:0202a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若雨過多即以其灰擲散空中雨便自止。",
        "from": "T20n1083_001:0202a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以此灰繞口。",
        "from": "T20n1083_001:0202a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擲著火中滿一百八遍。",
        "from": "T20n1083_001:0202a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切惡風雪雨悉皆不起。",
        "from": "T20n1083_001:0202a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若常念誦一切諸處無有障礙。",
        "from": "T20n1083_001:0202a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當得氣力勇猛精進。",
        "from": "T20n1083_001:0202a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨身已後得生西方極樂國土。",
        "from": "T20n1083_001:0202a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生生之處得宿命智。",
        "from": "T20n1083_001:0202a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是世尊若比丘比丘尼優婆塞優婆夷諸持呪者。",
        "from": "T20n1083_001:0202a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大如意摩尼轉輪觀世音心印讀誦成就。",
        "from": "T20n1083_001:0202a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當受持讀誦聽聞。",
        "from": "T20n1083_001:0202a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當得如是大福德聚得大善根。",
        "from": "T20n1083_001:0202a22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切男女無不隨順。",
        "from": "T20n1083_001:0202a23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪者恒常應當護口所作之事。",
        "from": "T20n1083_001:0202a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終不退轉得大供養。",
        "from": "T20n1083_001:0202a24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切財寶無不皆得。",
        "from": "T20n1083_001:0202a24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生願皆滿足。",
        "from": "T20n1083_001:0202a25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告觀世音菩薩言。",
        "from": "T20n1083_001:0202a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝為利益一切眾生。",
        "from": "T20n1083_001:0202a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於閻浮提流轉此法。",
        "from": "T20n1083_001:0202a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無上心呪但讀即成。",
        "from": "T20n1083_001:0202a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應如是作。",
        "from": "T20n1083_001:0202a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀世音菩薩應當護持一切眾生。",
        "from": "T20n1083_001:0202a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數數看視數令成就。",
        "from": "T20n1083_001:0202a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生令願滿足。",
        "from": "T20n1083_001:0202a29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀世音菩薩白佛言世尊我為利益護念一切眾生故。",
        "from": "T20n1083_001:0202b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今受持為令一切願成就故。",
        "from": "T20n1083_001:0202b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讀誦明呪諸願成滿。",
        "from": "T20n1083_001:0202b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故世尊我今誠諦。",
        "from": "T20n1083_001:0202b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊慈悲加護。",
        "from": "T20n1083_001:0202b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀世音菩薩如意摩尼陀羅尼經",
        "from": "T20n1083_001:0202b05_0",
        "context_with_next_sentence": false
    }
]