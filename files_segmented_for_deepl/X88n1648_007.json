[
    {
        "source_sentence": "冥報記輯書卷第七",
        "from": "X88n1648_007:0320b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐居士李信者并州文水縣之太平里人也。",
        "from": "X88n1648_007:0320b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身為隆政府衛士。",
        "from": "X88n1648_007:0320b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至顯慶年冬。",
        "from": "X88n1648_007:0320b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨例往。",
        "from": "X88n1648_007:0320b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朔州赴番。",
        "from": "X88n1648_007:0320b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘赤艸馬一疋并將艸駒。",
        "from": "X88n1648_007:0320b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時歲晚凝陰風雪嚴厚。",
        "from": "X88n1648_007:0320b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行十數里馬遂不進。",
        "from": "X88n1648_007:0320b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信以番期逼促撾之數十下。",
        "from": "X88n1648_007:0320b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬遂作人語。",
        "from": "X88n1648_007:0320b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂信曰。",
        "from": "X88n1648_007:0320b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我是汝母。",
        "from": "X88n1648_007:0320b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為生平避汝父將石餘米乞女故獲此報。",
        "from": "X88n1648_007:0320b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此駒即是汝妹也。",
        "from": "X88n1648_007:0320b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以力償債向了。",
        "from": "X88n1648_007:0320b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝復何苦敦逼如是。",
        "from": "X88n1648_007:0320b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信聞之驚愕流洟。",
        "from": "X88n1648_007:0320b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能自勝。",
        "from": "X88n1648_007:0320b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃拜謝之。",
        "from": "X88n1648_007:0320b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "躬駄鞍轡謂曰。",
        "from": "X88n1648_007:0320b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若是信孃當自行歸家。",
        "from": "X88n1648_007:0320b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬遂前行。",
        "from": "X88n1648_007:0320b22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信負鞍轡隨之至家。",
        "from": "X88n1648_007:0320b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信兄弟等見之悲哀相對。",
        "from": "X88n1648_007:0320b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別為厰櫪養飼有同事母。",
        "from": "X88n1648_007:0320b23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈僧營齋合門莫不精進。",
        "from": "X88n1648_007:0320b24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鄉閭道俗咸歎異之。",
        "from": "X88n1648_007:0320b24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時工部侍郎溫無隱。",
        "from": "X88n1648_007:0320c01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歧州司法張金停俱為丁艱。",
        "from": "X88n1648_007:0320c01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在家聞而奇之。",
        "from": "X88n1648_007:0320c02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故就信家顧訪見。",
        "from": "X88n1648_007:0320c02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬猶在問其由。",
        "from": "X88n1648_007:0320c02_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "委並如所傳。",
        "from": "X88n1648_007:0320c03_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯慶中。",
        "from": "X88n1648_007:0320c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平州有人。",
        "from": "X88n1648_007:0320c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姓孫名壽。",
        "from": "X88n1648_007:0320c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於海濵游獵。",
        "from": "X88n1648_007:0320c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見野火焰熾草木蕩盡。",
        "from": "X88n1648_007:0320c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯有一叢茂草獨不焚燎。",
        "from": "X88n1648_007:0320c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疑此草中有獸。",
        "from": "X88n1648_007:0320c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂以火燒之。",
        "from": "X88n1648_007:0320c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竟不能著。",
        "from": "X88n1648_007:0320c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽甚怪之。",
        "from": "X88n1648_007:0320c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂入草間尋覔。",
        "from": "X88n1648_007:0320c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃見一函金剛般若經。",
        "from": "X88n1648_007:0320c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其傍又見一死僧。",
        "from": "X88n1648_007:0320c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顏色不變。",
        "from": "X88n1648_007:0320c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火不延燎。",
        "from": "X88n1648_007:0320c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓋由此也。",
        "from": "X88n1648_007:0320c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信知經像非凡所測。",
        "from": "X88n1648_007:0320c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孫壽親自說之。",
        "from": "X88n1648_007:0320c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐并州城西有山寺。",
        "from": "X88n1648_007:0320c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寺名童子。",
        "from": "X88n1648_007:0320c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大像。",
        "from": "X88n1648_007:0320c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐高一百七十餘尺。",
        "from": "X88n1648_007:0320c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皇帝崇敬釋教。",
        "from": "X88n1648_007:0320c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯慶末年巡幸并州。",
        "from": "X88n1648_007:0320c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共皇后親到此寺。",
        "from": "X88n1648_007:0320c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及幸北谷開化等。",
        "from": "X88n1648_007:0320c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大像高二百尺。",
        "from": "X88n1648_007:0320c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮敬瞻覩嗟歎希奇。",
        "from": "X88n1648_007:0320c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大捨珍寶財物衣服。",
        "from": "X88n1648_007:0320c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并諸妃嬪內宮之人。",
        "from": "X88n1648_007:0320c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並各損捨。",
        "from": "X88n1648_007:0320c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并勑州官長吏竇軌等。",
        "from": "X88n1648_007:0320c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令速莊嚴備飾聖容。",
        "from": "X88n1648_007:0320c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并託龕前地務令寬廣。",
        "from": "X88n1648_007:0320c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還京之日至龍朔二年秋七月。",
        "from": "X88n1648_007:0320c15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內官出袈裟兩領遣中使馳送二寺大像。",
        "from": "X88n1648_007:0320c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其童子寺像披袈裟日。",
        "from": "X88n1648_007:0320c16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從旦至暮放五色光。",
        "from": "X88n1648_007:0320c17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流照崖巖洞燭山川。",
        "from": "X88n1648_007:0320c17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又入南龕小佛赫奕堂殿。",
        "from": "X88n1648_007:0320c18_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道俗瞻覩數千萬眾。",
        "from": "X88n1648_007:0320c18_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "城中貴賤覩此而遷善者。",
        "from": "X88n1648_007:0320c19_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十室而七八焉。",
        "from": "X88n1648_007:0320c19_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾人共知不言可悉。",
        "from": "X88n1648_007:0320c19_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐龍朔元年雒州景福寺比丘尼。",
        "from": "X88n1648_007:0320c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行房中。",
        "from": "X88n1648_007:0320c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有侍童任五娘。",
        "from": "X88n1648_007:0320c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "死後修行為五娘立靈。",
        "from": "X88n1648_007:0320c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經月餘日。",
        "from": "X88n1648_007:0320c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其姊及弟於夜中忽聞靈座上呻吟。",
        "from": "X88n1648_007:0320c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其弟初甚恐懼。",
        "from": "X88n1648_007:0320c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後乃問之。",
        "from": "X88n1648_007:0320c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "X88n1648_007:0320c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我生時於寺上食肉。",
        "from": "X88n1648_007:0320c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐此大苦痛。",
        "from": "X88n1648_007:0320c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我體上有瘡。",
        "from": "X88n1648_007:0320c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恐污牀席。",
        "from": "X88n1648_007:0321a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝可多將灰置牀上也。",
        "from": "X88n1648_007:0321a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弟依其言置灰。",
        "from": "X88n1648_007:0321a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後看牀上大有膿血。",
        "from": "X88n1648_007:0321a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又語弟曰。",
        "from": "X88n1648_007:0321a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姊患不能縫衣。",
        "from": "X88n1648_007:0321a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝大繿縷。",
        "from": "X88n1648_007:0321a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宜將布來。",
        "from": "X88n1648_007:0321a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為汝作衫及韈。",
        "from": "X88n1648_007:0321a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弟置布於靈牀上。",
        "from": "X88n1648_007:0321a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經宿即成。",
        "from": "X88n1648_007:0321a03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又語其妹曰。",
        "from": "X88n1648_007:0321a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兒小時患漆。",
        "from": "X88n1648_007:0321a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂救一螃蟹取汁塗瘡得差。",
        "from": "X88n1648_007:0321a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今入刀林地獄。",
        "from": "X88n1648_007:0321a05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "肉中現有折刀七枚。",
        "from": "X88n1648_007:0321a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願姊慈念為作功德救助。",
        "from": "X88n1648_007:0321a05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知姊煎迫交不濟辨。",
        "from": "X88n1648_007:0321a06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但隨身衣服無益死者。",
        "from": "X88n1648_007:0321a06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今並未壞。",
        "from": "X88n1648_007:0321a07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請以用之。",
        "from": "X88n1648_007:0321a07_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姊未報間。",
        "from": "X88n1648_007:0321a07_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃曰。",
        "from": "X88n1648_007:0321a07_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兒自取去。",
        "from": "X88n1648_007:0321a07_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "良久又曰。",
        "from": "X88n1648_007:0321a08_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣服已來。",
        "from": "X88n1648_007:0321a08_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見在牀上。",
        "from": "X88n1648_007:0321a08_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其姊試往觀之。",
        "from": "X88n1648_007:0321a08_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃所斂之服也。",
        "from": "X88n1648_007:0321a09_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂送淨土寺寶獻師處。",
        "from": "X88n1648_007:0321a09_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憑寫金剛般若經。",
        "from": "X88n1648_007:0321a09_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每寫一卷了。",
        "from": "X88n1648_007:0321a10_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即報云。",
        "from": "X88n1648_007:0321a10_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已出一刀。",
        "from": "X88n1648_007:0321a10_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡寫七卷了。",
        "from": "X88n1648_007:0321a10_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃云。",
        "from": "X88n1648_007:0321a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七刀並得出訖。",
        "from": "X88n1648_007:0321a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今蒙福助即往託生。",
        "from": "X88n1648_007:0321a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與姊及弟哭別而去。",
        "from": "X88n1648_007:0321a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吳興沈玄法說。",
        "from": "X88n1648_007:0321a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨土寺僧智整所說亦同。",
        "from": "X88n1648_007:0321a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐姚明解者本是普光寺沙門也。",
        "from": "X88n1648_007:0321a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性聰敏有文藻。",
        "from": "X88n1648_007:0321a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "工書翰善丹青。",
        "from": "X88n1648_007:0321a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至於鼓琴亦當時獨絕。",
        "from": "X88n1648_007:0321a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每欣俗網不樂道門。",
        "from": "X88n1648_007:0321a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至龍朔元年舉應詔人。",
        "from": "X88n1648_007:0321a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "躬赴雒陽。",
        "from": "X88n1648_007:0321a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及升第歸俗。",
        "from": "X88n1648_007:0321a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頗有餘言。",
        "from": "X88n1648_007:0321a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未幾而卒。",
        "from": "X88n1648_007:0321a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後託夢於相知淨土寺僧智整曰。",
        "from": "X88n1648_007:0321a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明解宿無福業不遵內教。",
        "from": "X88n1648_007:0321a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今大受罪非常飢乏。",
        "from": "X88n1648_007:0321a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儻有故人之情。",
        "from": "X88n1648_007:0321a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頗能惠一飡不。",
        "from": "X88n1648_007:0321a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智整夢中許諾。",
        "from": "X88n1648_007:0321a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及其寤後乃為設食。",
        "from": "X88n1648_007:0321a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至夜纔眠。",
        "from": "X88n1648_007:0321a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即見明解來愧謝之。",
        "from": "X88n1648_007:0321a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至二年秋中又託夢於畫工曰。",
        "from": "X88n1648_007:0321a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我以不信佛法今大受苦痛。",
        "from": "X88n1648_007:0321a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "努力為我寫二三卷經。",
        "from": "X88n1648_007:0321a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執手殷勤賦詩言別。",
        "from": "X88n1648_007:0321a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教畫工讀十八徧令記。",
        "from": "X88n1648_007:0321a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寤乃憶之。",
        "from": "X88n1648_007:0321a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其詩曰。",
        "from": "X88n1648_007:0321a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "握手不能別。",
        "from": "X88n1648_007:0321a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "撫膺還自傷。",
        "from": "X88n1648_007:0321a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "痛矣時陰短。",
        "from": "X88n1648_007:0321a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悲哉泉路長。",
        "from": "X88n1648_007:0321a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "松林驚野吹。",
        "from": "X88n1648_007:0321a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "荒[土*遂]落寒霜。",
        "from": "X88n1648_007:0321a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言離何以贈。",
        "from": "X88n1648_007:0321a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "留心內典章。",
        "from": "X88n1648_007:0321a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐癰州萬年縣閻村。",
        "from": "X88n1648_007:0321b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即灞渭之間也。",
        "from": "X88n1648_007:0321b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有婦女謝氏。",
        "from": "X88n1648_007:0321b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "適同縣。",
        "from": "X88n1648_007:0321b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元氏有女。",
        "from": "X88n1648_007:0321b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "適迴龍村人來阿照。",
        "from": "X88n1648_007:0321b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謝氏永徽末亡。",
        "from": "X88n1648_007:0321b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍朔元年八月。",
        "from": "X88n1648_007:0321b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "託夢於來氏女曰。",
        "from": "X88n1648_007:0321b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為生時酤酒。",
        "from": "X88n1648_007:0321b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小作升方取價太多。",
        "from": "X88n1648_007:0321b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "量酒復少。",
        "from": "X88n1648_007:0321b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今坐此罪。",
        "from": "X88n1648_007:0321b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於北山下人家為牛。",
        "from": "X88n1648_007:0321b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "近被賣與法界寺夏侯師家。",
        "from": "X88n1648_007:0321b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今將我向城南耕稻田。",
        "from": "X88n1648_007:0321b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非常辛苦。",
        "from": "X88n1648_007:0321b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及寤其女洟泣為阿照言之。",
        "from": "X88n1648_007:0321b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至二年正月。",
        "from": "X88n1648_007:0321b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有法界寺尼至阿照村。",
        "from": "X88n1648_007:0321b07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "女問尼。",
        "from": "X88n1648_007:0321b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尼報云。",
        "from": "X88n1648_007:0321b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有夏侯師。",
        "from": "X88n1648_007:0321b07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是實女。",
        "from": "X88n1648_007:0321b08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即就寺訪之。",
        "from": "X88n1648_007:0321b08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云近於北山下買得一牛。",
        "from": "X88n1648_007:0321b08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見在城南耕地。",
        "from": "X88n1648_007:0321b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其女洟泣求請寺尼。",
        "from": "X88n1648_007:0321b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃遣人送其女就之。",
        "from": "X88n1648_007:0321b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此牛平常唯一人禁制。",
        "from": "X88n1648_007:0321b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遇餘人必陸梁觝觸。",
        "from": "X88n1648_007:0321b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見其女至乃舐其遍體。",
        "from": "X88n1648_007:0321b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又流淚焉。",
        "from": "X88n1648_007:0321b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "女即憑夏侯師贖之。",
        "from": "X88n1648_007:0321b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃隨其女去。",
        "from": "X88n1648_007:0321b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今現在阿照家養飼。",
        "from": "X88n1648_007:0321b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "女常呼為阿孃。",
        "from": "X88n1648_007:0321b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "承奉不闕。",
        "from": "X88n1648_007:0321b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "京師王侯妃媵多令召視。",
        "from": "X88n1648_007:0321b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "競施錢帛。",
        "from": "X88n1648_007:0321b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐曹州濟陰縣西二十里村中有精舍。",
        "from": "X88n1648_007:0321b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至龍朔二年冬十月。",
        "from": "X88n1648_007:0321b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "野火暴起非常熾盛。",
        "from": "X88n1648_007:0321b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及至精舍踰越而過焉。",
        "from": "X88n1648_007:0321b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比僧房草舍焚燎總盡。",
        "from": "X88n1648_007:0321b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯金剛般若經一卷儼然如舊。",
        "from": "X88n1648_007:0321b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曹州參軍事席文禮說之。",
        "from": "X88n1648_007:0321b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐幽州漁陽縣無終戍城內有百許家。",
        "from": "X88n1648_007:0321b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍朔二年夏四月戍城火災。",
        "from": "X88n1648_007:0321b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "門樓及人家屋宇並為煨燼。",
        "from": "X88n1648_007:0321b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯二精舍及浮圖并佛龕上紙簾蘧蒢等。",
        "from": "X88n1648_007:0321b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但有佛像獨不延燎。",
        "from": "X88n1648_007:0321b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火既不燒巋然獨存。",
        "from": "X88n1648_007:0321b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時人見者莫不嗟異。",
        "from": "X88n1648_007:0321b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為佛力支持。",
        "from": "X88n1648_007:0321b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中山郎餘令既任彼官。",
        "from": "X88n1648_007:0321b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又家兄餘慶交友人。",
        "from": "X88n1648_007:0321b22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "郎將齊郡因如使營州。",
        "from": "X88n1648_007:0321b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並親見其事。",
        "from": "X88n1648_007:0321b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具為餘令說之。",
        "from": "X88n1648_007:0321b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐兖州曲阜人倪買得妻皇甫氏。",
        "from": "X88n1648_007:0321b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為有疾病祈禱泰山。",
        "from": "X88n1648_007:0321b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稍得瘳愈。",
        "from": "X88n1648_007:0321c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因被冥道使。",
        "from": "X88n1648_007:0321c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為伺命每被使。",
        "from": "X88n1648_007:0321c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即死經一二日。",
        "from": "X88n1648_007:0321c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "事了以後還復如故。",
        "from": "X88n1648_007:0321c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後取人亦眾矣。",
        "from": "X88n1648_007:0321c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自云。",
        "from": "X88n1648_007:0321c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曾被遣取鄉人龐領軍小女。",
        "from": "X88n1648_007:0321c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為其庭前有齋壇讀誦。",
        "from": "X88n1648_007:0321c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "久不得入。",
        "from": "X88n1648_007:0321c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少間屬讀誦稍閑。",
        "from": "X88n1648_007:0321c04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又因執燭者詣病女處。",
        "from": "X88n1648_007:0321c04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃隨而入。",
        "from": "X88n1648_007:0321c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方取得去。",
        "from": "X88n1648_007:0321c05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問其取由。",
        "from": "X88n1648_007:0321c05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃府君四郎所命。",
        "from": "X88n1648_007:0321c05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "府君不知也。",
        "from": "X88n1648_007:0321c05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論說地獄具有條貫。",
        "from": "X88n1648_007:0321c06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又云。",
        "from": "X88n1648_007:0321c06_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地下訴說生人非止一二。",
        "from": "X88n1648_007:0321c06_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但人微有福報。",
        "from": "X88n1648_007:0321c07_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "追不可得。",
        "from": "X88n1648_007:0321c07_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如其有罪攝之則易。",
        "from": "X88n1648_007:0321c07_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皇甫見被使役。",
        "from": "X88n1648_007:0321c08_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至今猶存。",
        "from": "X88n1648_007:0321c08_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今男子作生伺命者。",
        "from": "X88n1648_007:0321c08_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兖州見有三四人。",
        "from": "X88n1648_007:0321c08_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但不知其姓名耳。",
        "from": "X88n1648_007:0321c09_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐前大理司直河內司馬喬卿天性純謹有志行。",
        "from": "X88n1648_007:0321c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到永徽中為楊州戶曹。",
        "from": "X88n1648_007:0321c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "丁母憂居喪毀瘠。",
        "from": "X88n1648_007:0321c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刺心上血寫金剛般若經一卷。",
        "from": "X88n1648_007:0321c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未幾於廬上生芝草二莖。",
        "from": "X88n1648_007:0321c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經九日長尺有八寸。",
        "from": "X88n1648_007:0321c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "綠莖朱蓋日瀝汁一升。",
        "from": "X88n1648_007:0321c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傍人食之味甘如蜜。",
        "from": "X88n1648_007:0321c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "去而復生。",
        "from": "X88n1648_007:0321c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如此數四。",
        "from": "X88n1648_007:0321c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喬卿同僚數人。",
        "from": "X88n1648_007:0321c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並向餘令陳說。",
        "from": "X88n1648_007:0321c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天下士人多共知之。",
        "from": "X88n1648_007:0321c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冥報記輯書卷第七(終)",
        "from": "X88n1648_007:0321c17_0",
        "context_with_next_sentence": false
    }
]