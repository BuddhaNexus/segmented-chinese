[
    {
        "source_sentence": "阿彌陀經異本",
        "from": "X01n0006_001:0079b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心不亂下。",
        "from": "X01n0006_001:0079b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有專持名號以稱名故諸罪消滅即是多善根福德因緣二十一字。",
        "from": "X01n0006_001:0079b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是襄陽石割經本文。",
        "from": "X01n0006_001:0079b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元照戒度皆以為佚文。",
        "from": "X01n0006_001:0079b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "袾宏則曰。",
        "from": "X01n0006_001:0079b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前人解經之語。",
        "from": "X01n0006_001:0079b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "混入本文者。",
        "from": "X01n0006_001:0079b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "按梵文反覆丁寧。",
        "from": "X01n0006_001:0079b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譯者時或從省略。",
        "from": "X01n0006_001:0079b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則譯者省之乎。",
        "from": "X01n0006_001:0079b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "抑亦譯者不省。",
        "from": "X01n0006_001:0079b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後偶佚之乎。",
        "from": "X01n0006_001:0079b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆不可知也。",
        "from": "X01n0006_001:0079b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍舒淨土文曰。",
        "from": "X01n0006_001:0079b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "襄陽石經。",
        "from": "X01n0006_001:0079b11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隋陳仁稜所書。",
        "from": "X01n0006_001:0079b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "字畵清婉。",
        "from": "X01n0006_001:0079b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人多慕玩。",
        "from": "X01n0006_001:0079b12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "治承年間。",
        "from": "X01n0006_001:0079b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平重盛遣使于宋。",
        "from": "X01n0006_001:0079b13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求藏經及石經。",
        "from": "X01n0006_001:0079b13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建久九年秋。",
        "from": "X01n0006_001:0079b13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宋之使者。",
        "from": "X01n0006_001:0079b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "載石經來。",
        "from": "X01n0006_001:0079b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "船著筑前宗像。",
        "from": "X01n0006_001:0079b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拓本傳世。",
        "from": "X01n0006_001:0079b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文中名曰極樂曰作為。",
        "from": "X01n0006_001:0079b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其國眾生國作土。",
        "from": "X01n0006_001:0079b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆自然生作自然皆生。",
        "from": "X01n0006_001:0079b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然他本亦有同之者。",
        "from": "X01n0006_001:0079b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯此二十一字。",
        "from": "X01n0006_001:0079b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "襄陽石經獨有之耳。",
        "from": "X01n0006_001:0079b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大正元年十月初一日　　赤松連城　識",
        "from": "X01n0006_001:0079b18_0",
        "context_with_next_sentence": false
    }
]