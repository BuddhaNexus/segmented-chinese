[
    {
        "source_sentence": "大唐東京大敬愛寺一切經論目",
        "from": "T55n2148_001:0180c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "序",
        "from": "T55n2148_001:0180c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋靜泰撰",
        "from": "T55n2148_001:0180c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫文字即解脫真場。",
        "from": "T55n2148_001:0180c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摽不二之宗。",
        "from": "T55n2148_001:0180c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名句稱波若玄扄。",
        "from": "T55n2148_001:0180c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開唯一之路。",
        "from": "T55n2148_001:0180c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是以法籍馳簡。",
        "from": "T55n2148_001:0180c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詔百億而同文。",
        "from": "T55n2148_001:0180c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至教驅轍。",
        "from": "T55n2148_001:0180c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "環大千而共軌。",
        "from": "T55n2148_001:0180c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玉箱花題。",
        "from": "T55n2148_001:0180c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "炳發天城。",
        "from": "T55n2148_001:0180c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金牒寶詞。",
        "from": "T55n2148_001:0180c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昭彰龍宇。",
        "from": "T55n2148_001:0180c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自悲雲西起。",
        "from": "T55n2148_001:0180c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈液東飛。",
        "from": "T55n2148_001:0180c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "肇漸昭於周星。",
        "from": "T55n2148_001:0180c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終大明於漢日。",
        "from": "T55n2148_001:0180c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘭騰欝乎首唱。",
        "from": "T55n2148_001:0180c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竺什欣然嗣武。",
        "from": "T55n2148_001:0180c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是三乘警轡八藏張門。",
        "from": "T55n2148_001:0180c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懸二耀之高暉。",
        "from": "T55n2148_001:0180c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亘兩儀之長久。",
        "from": "T55n2148_001:0180c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩屬當塗。",
        "from": "T55n2148_001:0180c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍駭典午。",
        "from": "T55n2148_001:0180c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳧驚函谷。",
        "from": "T55n2148_001:0180c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風霾道鏡如隱。",
        "from": "T55n2148_001:0180c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金淩水鬪。",
        "from": "T55n2148_001:0180c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性珠俄逸。",
        "from": "T55n2148_001:0180c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂有論參經語。",
        "from": "T55n2148_001:0180c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疏涉論名。",
        "from": "T55n2148_001:0180c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘紊大小。",
        "from": "T55n2148_001:0180c16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教齊凡聖。",
        "from": "T55n2148_001:0180c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有黃巾之醜。",
        "from": "T55n2148_001:0180c16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "混莊釋為同源。",
        "from": "T55n2148_001:0180c16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "素褐之首。",
        "from": "T55n2148_001:0180c17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "格儒佛為派緒。",
        "from": "T55n2148_001:0180c17_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "叨天侮聖。",
        "from": "T55n2148_001:0180c17_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "肆虐胸襟。",
        "from": "T55n2148_001:0180c18_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顏子摽光淨之談。",
        "from": "T55n2148_001:0180c18_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尼父牓儒童之稱。",
        "from": "T55n2148_001:0180c18_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進退惟谷。",
        "from": "T55n2148_001:0180c19_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "首尾陷機。",
        "from": "T55n2148_001:0180c19_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竊負神器。",
        "from": "T55n2148_001:0180c19_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偷安智識。",
        "from": "T55n2148_001:0180c19_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "波旬妄說。",
        "from": "T55n2148_001:0180c20_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "邪辯亂真。",
        "from": "T55n2148_001:0180c20_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疑偽之興寔由於此。",
        "from": "T55n2148_001:0180c20_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庸庸之輩。",
        "from": "T55n2148_001:0180c21_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "握魚目而[禾*禺]驪珠。",
        "from": "T55n2148_001:0180c21_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皎皎之流。",
        "from": "T55n2148_001:0180c21_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "揚濁涇而分清渭。",
        "from": "T55n2148_001:0180c21_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開士所以扼腕高僧以之長息者也。",
        "from": "T55n2148_001:0180c22_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晉道安創裁目錄。",
        "from": "T55n2148_001:0180c23_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "齊法上亦為條例。",
        "from": "T55n2148_001:0180c23_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非無小異。",
        "from": "T55n2148_001:0180c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "張置大同。",
        "from": "T55n2148_001:0180c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫不以單譯居第一。",
        "from": "T55n2148_001:0180c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重翻處第二。",
        "from": "T55n2148_001:0180c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵集配第三。",
        "from": "T55n2148_001:0180c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別生安第四。",
        "from": "T55n2148_001:0180c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疑偽充第五。",
        "from": "T55n2148_001:0180c25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "位雖列五繕寫唯三。",
        "from": "T55n2148_001:0180c26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "良以別生與本部不殊。",
        "from": "T55n2148_001:0180c26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疑偽固非留限。",
        "from": "T55n2148_001:0180c27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "芟夷蕪穢。",
        "from": "T55n2148_001:0180c27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洗拂塵瑕。",
        "from": "T55n2148_001:0180c27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坦矣法疇。",
        "from": "T55n2148_001:0180c27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "差無稂莠。",
        "from": "T55n2148_001:0180c28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我皇馭曆。",
        "from": "T55n2148_001:0180c28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道應天飛。",
        "from": "T55n2148_001:0180c28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廓五梵而論都。",
        "from": "T55n2148_001:0180c28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奄四州而作后。",
        "from": "T55n2148_001:0180c29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "披十善以陶化。",
        "from": "T55n2148_001:0180c29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憑五衍而貞風。",
        "from": "T55n2148_001:0180c29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣樹仁祠。",
        "from": "T55n2148_001:0181a01_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大隆教義。",
        "from": "T55n2148_001:0181a01_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯慶年際西明寺成御造藏經。",
        "from": "T55n2148_001:0181a01_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更令隱鍊區格盡爾無所間然。",
        "from": "T55n2148_001:0181a02_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "律師道宣又為錄序。",
        "from": "T55n2148_001:0181a03_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殷因夏禮無革前修。",
        "from": "T55n2148_001:0181a03_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於三例外附申雜藏。",
        "from": "T55n2148_001:0181a04_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即法苑法集高僧僧史之流是也。",
        "from": "T55n2148_001:0181a04_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頗以毘贊有功。",
        "from": "T55n2148_001:0181a05_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故載之云爾。",
        "from": "T55n2148_001:0181a05_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皇太子列耀紫微。",
        "from": "T55n2148_001:0181a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "承扉閶闔。",
        "from": "T55n2148_001:0181a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "銑華雲蔚。",
        "from": "T55n2148_001:0181a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瑤躬岳峙。",
        "from": "T55n2148_001:0181a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內精七淨。",
        "from": "T55n2148_001:0181a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外暢九言。",
        "from": "T55n2148_001:0181a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "挹梓圖恭。",
        "from": "T55n2148_001:0181a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佩瑜端孝。",
        "from": "T55n2148_001:0181a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爰崇淨域薦祉二皇。",
        "from": "T55n2148_001:0181a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元良三寶。",
        "from": "T55n2148_001:0181a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永貞四眾。",
        "from": "T55n2148_001:0181a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍朔三年正月二十二日。",
        "from": "T55n2148_001:0181a09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勅令於敬愛道場寫一切經典。",
        "from": "T55n2148_001:0181a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又奉麟德元年正月二十六日。",
        "from": "T55n2148_001:0181a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勅取履味沙門十人。",
        "from": "T55n2148_001:0181a11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惠概明玉神察道英曇邃等。",
        "from": "T55n2148_001:0181a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並選翹楚尤閑文義。",
        "from": "T55n2148_001:0181a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "參覆量挍首末三年。",
        "from": "T55n2148_001:0181a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又置官寮是塗供給。",
        "from": "T55n2148_001:0181a13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勅使洛州長史銀青光祿大夫南康郡開國公韓威。",
        "from": "T55n2148_001:0181a13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "判官洛州司功參軍李亮。",
        "from": "T55n2148_001:0181a14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臺使鄭州司士參軍盧行訥。",
        "from": "T55n2148_001:0181a15_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "判官王屋縣尉鄭祖均等。",
        "from": "T55n2148_001:0181a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精加撿覆。",
        "from": "T55n2148_001:0181a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寫舊經論。",
        "from": "T55n2148_001:0181a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七百四十一部。",
        "from": "T55n2148_001:0181a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二千七百三十一卷。",
        "from": "T55n2148_001:0181a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又寫大唐三藏法師新譯經論。",
        "from": "T55n2148_001:0181a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七十五部。",
        "from": "T55n2148_001:0181a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一千三百三十五卷。",
        "from": "T55n2148_001:0181a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合新舊八百一十六部。",
        "from": "T55n2148_001:0181a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四千六十六卷入藏。",
        "from": "T55n2148_001:0181a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其有古來有目而無本者。",
        "from": "T55n2148_001:0181a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合三百八十二部。",
        "from": "T55n2148_001:0181a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七百二十五卷。",
        "from": "T55n2148_001:0181a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨訪隨寫。",
        "from": "T55n2148_001:0181a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真所謂偉哉法寶。",
        "from": "T55n2148_001:0181a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內外無瑕。",
        "from": "T55n2148_001:0181a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洞矣法流晈鏡深淺。",
        "from": "T55n2148_001:0181a22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "純一無雜具足清白而已哉。",
        "from": "T55n2148_001:0181a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並毫疎寒兔。",
        "from": "T55n2148_001:0181a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "紙落秋蒲。",
        "from": "T55n2148_001:0181a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "題華碧字。",
        "from": "T55n2148_001:0181a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "軸貞香木。",
        "from": "T55n2148_001:0181a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所冀天衣銷石不墜斯文。",
        "from": "T55n2148_001:0181a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地墨窮界微言尚在。",
        "from": "T55n2148_001:0181a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "紹隆之福囑累之功。",
        "from": "T55n2148_001:0181a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏惟皇帝繼天理物。",
        "from": "T55n2148_001:0181a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘虬制寓。",
        "from": "T55n2148_001:0181a26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "河砂衍壽。",
        "from": "T55n2148_001:0181a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "海滴覃基。",
        "from": "T55n2148_001:0181a27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中宮葛蕈固本。",
        "from": "T55n2148_001:0181a27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摎木垂芳。",
        "from": "T55n2148_001:0181a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曦魄清襟。",
        "from": "T55n2148_001:0181a27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虹電流感。",
        "from": "T55n2148_001:0181a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儲君儀風。",
        "from": "T55n2148_001:0181a28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "象室晅日。",
        "from": "T55n2148_001:0181a28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "春居道契吟銅睟溫。",
        "from": "T55n2148_001:0181a28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玉裕宸柯。",
        "from": "T55n2148_001:0181a29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝萼藩國。",
        "from": "T55n2148_001:0181a29_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "維城內宷外寮肅朝。",
        "from": "T55n2148_001:0181a29_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清野霄形。",
        "from": "T55n2148_001:0181b01_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "荷法橋之濟。",
        "from": "T55n2148_001:0181b01_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永隔邪津。",
        "from": "T55n2148_001:0181b01_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "履識承惠炬之輝。",
        "from": "T55n2148_001:0181b02_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長遵正術。",
        "from": "T55n2148_001:0181b02_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "靜泰不惟鄙昧。",
        "from": "T55n2148_001:0181b02_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輒撰斯文。",
        "from": "T55n2148_001:0181b03_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敢事加損。",
        "from": "T55n2148_001:0181b03_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還循舊轍。",
        "from": "T55n2148_001:0181b03_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三章久布。",
        "from": "T55n2148_001:0181b03_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫一承風。",
        "from": "T55n2148_001:0181b04_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明誠古人。",
        "from": "T55n2148_001:0181b04_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請祛雜藏。",
        "from": "T55n2148_001:0181b04_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恐文溺質用除濫觴。",
        "from": "T55n2148_001:0181b04_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合成五卷。",
        "from": "T55n2148_001:0181b05_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次之於左。",
        "from": "T55n2148_001:0181b05_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾經目錄隋沙門法經等撰",
        "from": "T55n2148_001:0181b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "都合二千二百一十九部六千九百九十四卷。",
        "from": "T55n2148_001:0181b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "單本(源來一本更無別翻)　合四百四十一部二千七百八十三卷右第一卷。",
        "from": "T55n2148_001:0181b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重翻(本是一經或有二重翻者乃至六重翻者)　合三百二十四部一千一百一十六卷賢聖集傳(賢聖所撰翻譯有源)　合五十部一百八十六卷(二千七百八十九紙)右第二卷。",
        "from": "T55n2148_001:0181b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已前二卷三分　合八百一十九部四千八十六卷。",
        "from": "T55n2148_001:0181b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入藏見錄六百八十九部二千五百三十二卷。",
        "from": "T55n2148_001:0181b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仁壽二年。",
        "from": "T55n2148_001:0181b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勘定三十一部一百五十八卷。",
        "from": "T55n2148_001:0181b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貞觀九年。",
        "from": "T55n2148_001:0181b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉行二十部二十一卷。",
        "from": "T55n2148_001:0181b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貞觀九年。",
        "from": "T55n2148_001:0181b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於闕本內訪得入藏。",
        "from": "T55n2148_001:0181b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翻得六十部。",
        "from": "T55n2148_001:0181b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六百七十卷。",
        "from": "T55n2148_001:0181b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貞觀已來玄奘見所翻。",
        "from": "T55n2148_001:0181b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯慶四年西明寺奉勅寫經。",
        "from": "T55n2148_001:0181b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具錄入目施。",
        "from": "T55n2148_001:0181b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一十五部六百六十四卷。",
        "from": "T55n2148_001:0181b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯慶已來玄奘法師後所譯得龍朔三年敬愛寺奉勅寫經具錄入藏。",
        "from": "T55n2148_001:0181b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別生(於大部內抄出別行)　合八百一十部一千二百八十八卷右第三卷。",
        "from": "T55n2148_001:0181b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疑偽(名雖似正義涉人造)　合二百八部四百九十六卷右第四卷。",
        "from": "T55n2148_001:0181b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已前二卷二分　合一千一十八部一千七百八十三卷不須鈔寫。",
        "from": "T55n2148_001:0181c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闕本(舊錄有目而無經本)　合三百八十二部七百二十五卷　請訪右第五卷。",
        "from": "T55n2148_001:0181c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾經目錄卷第一",
        "from": "T55n2148_001:0181c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隋開皇十四年勅翻經沙門法經等撰",
        "from": "T55n2148_001:0181c11_0",
        "context_with_next_sentence": true
    },
