[
    {
        "source_sentence": "佛說寶藏神大明曼拏羅儀軌經",
        "from": "T21n1283_002:0346a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卷下",
        "from": "T21n1283_002:0346a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試鴻臚少卿",
        "from": "T21n1283_002:0346a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳教大師臣法天奉　詔譯",
        "from": "T21n1283_002:0346a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次下諸部真言。",
        "from": "T21n1283_002:0346a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請召發遣供獻加持並皆通用。",
        "from": "T21n1283_002:0346a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難哆鉢納摩(二合引)野娑嚩(二合引)賀(引)此是蓮花寶藏真言。",
        "from": "T21n1283_002:0346a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難哆商佉(引)野娑嚩(二合引)賀(引)此是硨磲寶藏真言。",
        "from": "T21n1283_002:0346b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "囉惹曩(引)婆(引)野馱曩(引)地鉢哆曳娑嚩(二合引)賀(引)此是囉惹哆曩婆夜叉王真言。",
        "from": "T21n1283_002:0346b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩尼曩(引)婆(引)野(引)馱曩(引)地鉢哆曳娑嚩(二合引)賀(引)此是囉怛曩(二合)曩(引)婆夜叉王真言。",
        "from": "T21n1283_002:0346b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酥曩(引)婆(引)野馱曩(引)地鉢哆曳(引)娑嚩(二合引)賀此是酥曩(引)婆夜叉王真言。",
        "from": "T21n1283_002:0346b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "暗婆曩(引)婆(引)野馱曩(引)地鉢哆曳(引)娑嚩(二合引)賀此是暗婆曩(引)婆夜叉王真言。",
        "from": "T21n1283_002:0346b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儞囉曩(引)婆(引)野馱曩(引)地鉢哆曳(引)娑嚩(二合引)賀此是羅曩婆夜叉王真言。",
        "from": "T21n1283_002:0346b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吠室囉(二合)摩拏也(二合)野馱曩(引)地鉢哆曳(引)娑嚩(二合引)賀此是毘沙門夜叉王真言。",
        "from": "T21n1283_002:0346b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩賀(引)曩(引)婆(引)野馱曩(引)地鉢哆曳(引)娑嚩(二合)賀此是摩賀(引)曩(引)婆夜叉王真言。",
        "from": "T21n1283_002:0346b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賀囉惹敢(二合)婆迦吽頗吒娑嚩(二合)賀此是賀囉惹敢(二合)婆夜叉王真言。",
        "from": "T21n1283_002:0346b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惹敢(二合)婆迦野羼捺囉(二合)野娑嚩(二合)賀此是惹敢(二合)婆迦夜叉王真言。",
        "from": "T21n1283_002:0346b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護努護努喃曩(引)酥細拏也(二合)野娑嚩(二合)賀此是酥細拏夜叉王真言。",
        "from": "T21n1283_002:0346b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賀囉賀囉贊捺囉(二合)細(引)曩(引)野娑嚩(二合引)賀此是贊捺囉(二合)細(引)曩夜叉王真言。",
        "from": "T21n1283_002:0346c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惹婆曩(引)野娑嚩(二合)賀",
        "from": "T21n1283_002:0346c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是護摩真言。",
        "from": "T21n1283_002:0346c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作護摩。",
        "from": "T21n1283_002:0346c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用阿波摩陵誐濕柴。",
        "from": "T21n1283_002:0346c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以酥酪蜜搵此柴頭。",
        "from": "T21n1283_002:0346c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持戒一晝夜。",
        "from": "T21n1283_002:0346c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作護摩八千。",
        "from": "T21n1283_002:0346c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得大聰明。",
        "from": "T21n1283_002:0346c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令誦人日記一千頌永不忘失。",
        "from": "T21n1283_002:0346c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若以阿里迦木根作寶藏神相。",
        "from": "T21n1283_002:0346c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可長六指已來。",
        "from": "T21n1283_002:0346c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭戴天冠種種莊嚴。",
        "from": "T21n1283_002:0346c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以香花供養。",
        "from": "T21n1283_002:0346c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然用阿波摩陵誐濕柴。",
        "from": "T21n1283_002:0346c10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於酥酪蜜中搵過。",
        "from": "T21n1283_002:0346c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作護摩一洛叉。",
        "from": "T21n1283_002:0346c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日日得銀錢一百。",
        "from": "T21n1283_002:0346c11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說寶藏神心真言。",
        "from": "T21n1283_002:0346c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨(引)囉怛曩(二合)怛囉(二合)夜(引)野曩謨(引)摩(引)尼婆捺囉(二合引)野摩賀(引)藥乞叉(二合)細(引)曩(引)鉢哆曳(引)酥(引)嚩(引)酥(引)尾訖里(二合引)尼野彌里致娑嚩(二合引)賀此寶藏神心真言。",
        "from": "T21n1283_002:0346c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能得最上成就。",
        "from": "T21n1283_002:0346c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若入水至膝作水護摩誦此真言能獲財利又若以儞淫(二合)摩木。",
        "from": "T21n1283_002:0346c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可長四指令平正用。",
        "from": "T21n1283_002:0346c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持戒畫人畫寶賢相莊嚴具足。",
        "from": "T21n1283_002:0346c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以飲食等供獻已。",
        "from": "T21n1283_002:0346c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日三時用焰。",
        "from": "T21n1283_002:0346c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多煙少之火。",
        "from": "T21n1283_002:0346c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使紫礦作護摩八千。",
        "from": "T21n1283_002:0346c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能降伏大財主。",
        "from": "T21n1283_002:0346c21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說別夜叉王心真言文句用圓。",
        "from": "T21n1283_002:0346c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能令得最上成就。",
        "from": "T21n1283_002:0346c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "囉怛曩(二合)婆捺囉(二合)哆帝三摩野摩努瑟摩(二合)囉摩尼婆捺囉(二合)野婆誐嚩覩(引)夜(引)唧哆娑覩(二合)馱摩馱摩摩(引)唧囉摩賀(引)藥乞叉(二合)細(引)曩(引)必隣(二合)哆覩嚕覩嚕摩(引)尼婆捺囉(二合)覩嚕覩嚕摩(引)尼婆捺囉(二合)俱致俱致摩(引)尼婆捺囉(二合)覩致覩致摩(引)尼婆捺囉(二合)帝致帝致摩尼婆捺囉(二合)娑嚩(二合引)賀復有壇法不揀吉星曜日。",
        "from": "T21n1283_002:0346c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不假持戒之者。",
        "from": "T21n1283_002:0347a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但用阿里迦木或儞淫(二合)摩木及別樹木作寶賢相。",
        "from": "T21n1283_002:0347a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以香花供養。",
        "from": "T21n1283_002:0347a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就日中用白芥子菜油。",
        "from": "T21n1283_002:0347a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗在寶賢像身。",
        "from": "T21n1283_002:0347a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦前真言百千遍。",
        "from": "T21n1283_002:0347a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日得八錢。",
        "from": "T21n1283_002:0347a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得已須令使盡。",
        "from": "T21n1283_002:0347a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得留一錢日日如是。",
        "from": "T21n1283_002:0347a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶藏神復說真言儀軌。",
        "from": "T21n1283_002:0347a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令持誦者先自乞食持齋。",
        "from": "T21n1283_002:0347a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦根本真言三洛叉。",
        "from": "T21n1283_002:0347a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後於如來前設大供獻。",
        "from": "T21n1283_002:0347a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言八千能得多財。",
        "from": "T21n1283_002:0347a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次明水護摩每日以手掬水。",
        "from": "T21n1283_002:0347a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而却傾下水中。",
        "from": "T21n1283_002:0347a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是至百掬或千掬。",
        "from": "T21n1283_002:0347a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對賢聖前而伸供獻。",
        "from": "T21n1283_002:0347a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是至六箇月能得大財。",
        "from": "T21n1283_002:0347a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說入大曼拏羅諸部灌頂真言。",
        "from": "T21n1283_002:0347a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惹敢(二合)婆曩吒謨吒迦(引)野娑嚩(二合引)賀此是謨吒迦夜叉女真言。",
        "from": "T21n1283_002:0347a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "囉訖哆(二合)即以(二合)賀嚩(二合)娑嚩(二合引)賀此是囉訖哆[口*爾]嚩夜叉女真言。",
        "from": "T21n1283_002:0347a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩護(引)那囉(引)野娑嚩(二合引)賀此是摩護那羅夜叉女真言。",
        "from": "T21n1283_002:0347a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尾摩羅尾摩羅捺里舍(二合)儞阿(引)夜(引)呬娑嚩(二合)賀尾摩羅(引)尾摩羅尾舍(引)羅曩野儞(引)冒馱(引)野娑嚩(二合引)賀此是尾摩羅尾舍羅夜叉女真言。",
        "from": "T21n1283_002:0347a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "贊捺囉(二合引)輸摩帝贊捺里(二合引)贊捺囉(二合)摩帝娑嚩(二合引)賀此是贊捺囉輸摩帝夜叉女真言。",
        "from": "T21n1283_002:0347a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿嚩賀儞酥路左曩唵(引)贊捺囉(二合)鉢納彌(二合引)娑嚩(二合引)賀鉢納摩(二合)娑囉(引)酥訥嚕(二合)摩努(引)囉彌(引)娑嚩(二合引)賀酥部嚕(二合)嚩(引)護鉢囉(二合)怛也(二合)野娑嚩(二合引)賀嚩護嚩呬儞(二合)末隷伽吒具(引)致娑嚩(二合引)賀此是摩賀彌伽夜叉女等真言。",
        "from": "T21n1283_002:0347a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酥枳(引)舍摩(引)隷枳(引)舍波里尼娑嚩(二合引)賀此是酥枳舍夜叉女真言。",
        "from": "T21n1283_002:0347b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚩曩摩(引)隷摩(引)隷儞娑嚩(二合引)賀此是摩曩摩羅夜叉女真言。",
        "from": "T21n1283_002:0347b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烏里底(二合)瑟吒(二合)布室左(二合)隷(引)娑嚩(二合引)賀此是布左隷夜叉女真言。",
        "from": "T21n1283_002:0347b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "訥嚕(二合)摩摩(引)隷迦(引)迦(引)隷賀隷訥嚕(二合)彌(引)枳隷枳隷娑嚩(二合引)賀迦摩摩(引)隷儞尾羅(引)悉儞娑嚩(二合引)賀迦摩摩隷薩里嚩(二合引)禰(引)嚩薩里嚩(二合)誐囉(二合)摩努(引)惹尾(引)婆誐嚩帝娑嚩(二合)賀摩努(引)惹嚩(引)迦(引)摩嚕閉(引)婆(引)尾儞娑嚩(二合)賀此是迦摩嚕閉等夜叉女真言。",
        "from": "T21n1283_002:0347b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酥嚕(二合)哆嚩(引)吉也(二合)緊迦里娑嚩(二合)賀此是酥嚕哆嚩(引)吉也(二合)夜叉女真言。",
        "from": "T21n1283_002:0347b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酥捺里舍(二合)儞(引)迦(引)彌儞娑嚩(二合)賀此是酥那里舍曩夜叉女真言。",
        "from": "T21n1283_002:0347b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩護(引)誐里(二合引)婆焰迦里娑嚩(二合)賀此是摩護仡囉夜叉女真言。",
        "from": "T21n1283_002:0347b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印捺囉(二合引)野禰(引)嚩(引)地鉢哆野娑嚩(二合)賀此是帝釋心真言。",
        "from": "T21n1283_002:0347b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿誐曩(二合引)曳(引)賀尾也(二合)地鉢多曳(引)娑嚩(二合)賀此是火天真言。",
        "from": "T21n1283_002:0347b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜摩(引)野必里(二合引)哆(引)地鉢哆曳(引)娑嚩(二合引)賀此是夜摩天心真言。",
        "from": "T21n1283_002:0347c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃(引)里怛也(二合)曳(引)洛乞叉(二合)娑(引)地鉢哆曳(引)娑嚩(二合)賀此是乃乙里帝心真言。",
        "from": "T21n1283_002:0347c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚩嚕拏也(二合)野惹羅(引)地鉢哆曳(引)娑嚩(二合引)賀此是水天心真言。",
        "from": "T21n1283_002:0347c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚩(引)野尾(引)按哆里乞叉(二合引)地鉢哆曳(引)娑嚩(二合引)賀此是風天心真言。",
        "from": "T21n1283_002:0347c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱尾(引)囉(引)野馱曩鉢哆曳(引)娑嚩(二合引)賀此是俱尾羅心真言。",
        "from": "T21n1283_002:0347c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伊舍曩(引)野馱曩(引)地鉢哆曳(引)娑嚩(二合引)賀此是伊舍曩天心真言。",
        "from": "T21n1283_002:0347c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惹里馱(二合)沒囉(二合)憾摩(二合)尼所(二合)娑嚩(二合引)賀此是大梵天心真言。",
        "from": "T21n1283_002:0347c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿冒(引)尾瑟拏(二合)吠(引)娑嚩(二合引)賀此是那羅延天心真言。",
        "from": "T21n1283_002:0347c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿儞嚩里哆(二合)曩(引)野娑嚩(二合引)賀此是金剛心真言。",
        "from": "T21n1283_002:0347c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賀囉(二合)哆(引)娑曩(引)野娑嚩(二合引)賀此是槊心真言。",
        "from": "T21n1283_002:0347c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩賀(引)寫(引)摩(引)野娑嚩(二合引)賀此是杖心真言。",
        "from": "T21n1283_002:0347c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佉陵誐(二合)尾摩羅馱(引)里尼娑嚩(二合引)賀此是劍心真言。",
        "from": "T21n1283_002:0347c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚩嚕拏嚩摩(引)野娑嚩(二合引)賀此是羂索心真言。",
        "from": "T21n1283_002:0348a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鉢囉(二合)普敢(二合)惹曩野娑嚩(二合引)賀此是幢心真言。",
        "from": "T21n1283_002:0348a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩賀(引)燥(引)摩也(二合)囉娑摩曳娑嚩(二合引)賀此是輪心真言。",
        "from": "T21n1283_002:0348a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左(引)隷(引)閉孕(二合)誐隷(引)娑嚩(二合引)賀此是三股叉心真言。",
        "from": "T21n1283_002:0348a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并結界同用。",
        "from": "T21n1283_002:0348a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娑囉捺(引)野娑嚩(二合引)賀",
        "from": "T21n1283_002:0348a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是弓箭真言。",
        "from": "T21n1283_002:0348a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)惹里馱(二合)惹羅嚩(口)囉(二合)半惹囉(引)野娑嚩(二合)賀此是金剛網真言。",
        "from": "T21n1283_002:0348a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是各各本尊真言。",
        "from": "T21n1283_002:0348a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一持誦加持賢瓶。",
        "from": "T21n1283_002:0348a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有入曼拏羅弟子。",
        "from": "T21n1283_002:0348a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用此賢瓶而與灌頂。",
        "from": "T21n1283_002:0348a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令得富貴吉祥。",
        "from": "T21n1283_002:0348a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若入大曼拏羅。",
        "from": "T21n1283_002:0348a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "志心依法虔誠灌頂。",
        "from": "T21n1283_002:0348a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復能得見寶藏神。",
        "from": "T21n1283_002:0348a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三時教勅通達大印。",
        "from": "T21n1283_002:0348a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復作護摩以二十種物作廣大供獻。",
        "from": "T21n1283_002:0348a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦聖天及大夜叉王本尊真言。",
        "from": "T21n1283_002:0348a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日三時隨力供養。",
        "from": "T21n1283_002:0348a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得三昧真言最上成就為自擁護。",
        "from": "T21n1283_002:0348a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言我今復說所作精熟最上印法及真言句。",
        "from": "T21n1283_002:0348a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時大夜叉王等。",
        "from": "T21n1283_002:0348a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心生渴仰請佛宣說。",
        "from": "T21n1283_002:0348a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊遍觀大會。",
        "from": "T21n1283_002:0348a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作師子吼告言。",
        "from": "T21n1283_002:0348a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等諦聽諦聽。",
        "from": "T21n1283_002:0348a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此寶藏神真言印法具大慈悲。",
        "from": "T21n1283_002:0348a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難調伏者而能調伏。",
        "from": "T21n1283_002:0348a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與一切眾生成就一切勝利之事。",
        "from": "T21n1283_002:0348a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜叉王等右膝著地。",
        "from": "T21n1283_002:0348a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌向佛默然而聽。",
        "from": "T21n1283_002:0348a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊。",
        "from": "T21n1283_002:0348a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如大師子自在無畏說真言印。",
        "from": "T21n1283_002:0348a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手平掌相並。",
        "from": "T21n1283_002:0348a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指相交。",
        "from": "T21n1283_002:0348a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二無名指如針。",
        "from": "T21n1283_002:0348a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈第三節。",
        "from": "T21n1283_002:0348a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二頭指附無名指。",
        "from": "T21n1283_002:0348b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指順依頭指。",
        "from": "T21n1283_002:0348b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二小指如針。",
        "from": "T21n1283_002:0348b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是寶藏神根本印。",
        "from": "T21n1283_002:0348b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此印結中間。",
        "from": "T21n1283_002:0348b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得寶藏神現身。",
        "from": "T21n1283_002:0348b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀察誦人得施願成就。",
        "from": "T21n1283_002:0348b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言。",
        "from": "T21n1283_002:0348b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨(引)囉怛曩(二合)怛囉(二合)夜野(引)曩謨摩(引)尼婆捺囉(二合引)野摩賀(引)藥乞叉(二合)細(引)曩(引)鉢哆曳(引)唵(引)惹敢(二合)婆羅惹隣捺囉(二合引)野娑嚩(二合引)賀此真言及印。",
        "from": "T21n1283_002:0348b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於自身上五處用之。",
        "from": "T21n1283_002:0348b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作此法時彼持誦人。",
        "from": "T21n1283_002:0348b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如自為寶藏神。",
        "from": "T21n1283_002:0348b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次作左隣捺囉夜叉王印。",
        "from": "T21n1283_002:0348b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手合拳小指如針。",
        "from": "T21n1283_002:0348b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言。",
        "from": "T21n1283_002:0348b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)左隣(引)捺囉(二合引)野娑嚩(二合引)賀此真言印。",
        "from": "T21n1283_002:0348b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令夜叉王而作擁護。",
        "from": "T21n1283_002:0348b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次作目契夜叉王印。",
        "from": "T21n1283_002:0348b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以手合拳小指入掌。",
        "from": "T21n1283_002:0348b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無名指如針誦此真言。",
        "from": "T21n1283_002:0348b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)目契孕(二合)捺囉(二合)野娑嚩(二合引)賀此真言印。",
        "from": "T21n1283_002:0348b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能擁護誦人。",
        "from": "T21n1283_002:0348b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次作一切夜叉女印。",
        "from": "T21n1283_002:0348b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以小指入掌。",
        "from": "T21n1283_002:0348b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無名指屈第三節。",
        "from": "T21n1283_002:0348b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言。",
        "from": "T21n1283_002:0348b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)惡儞",
        "from": "T21n1283_002:0348b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此真言印亦能擁護。",
        "from": "T21n1283_002:0348b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "召請亦得用。",
        "from": "T21n1283_002:0348b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次作曼拏羅牆印。",
        "from": "T21n1283_002:0348b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手合拳。",
        "from": "T21n1283_002:0348b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭指各各如針。",
        "from": "T21n1283_002:0348b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言。",
        "from": "T21n1283_002:0348b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)左隣捺囉(二合)滿馱禰苦[口*癹]吒次作曼拏羅上面網印。",
        "from": "T21n1283_002:0348b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前印相却令頭指相合如針。",
        "from": "T21n1283_002:0348b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言。",
        "from": "T21n1283_002:0348b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)目契孕(二合)捺囉(二合引)野[口*癹]吒次作曼拏羅橛印。",
        "from": "T21n1283_002:0348b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手作拳。",
        "from": "T21n1283_002:0348b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "垂手向地。",
        "from": "T21n1283_002:0348b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言。",
        "from": "T21n1283_002:0348b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)賀里(二合)入",
        "from": "T21n1283_002:0348c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次召請七眷屬印。",
        "from": "T21n1283_002:0348c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前根本印屈大指。",
        "from": "T21n1283_002:0348c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言。",
        "from": "T21n1283_002:0348c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曀呬曀呬祖嚕祖嚕惹羅娑野曩必里(二合)夜(引)野娑嚩(二合引)賀如是儀則。",
        "from": "T21n1283_002:0348c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令七眷屬皆來集會。",
        "from": "T21n1283_002:0348c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次作獻閼伽水印。",
        "from": "T21n1283_002:0348c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手作掬。",
        "from": "T21n1283_002:0348c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指及頭指附著中指。",
        "from": "T21n1283_002:0348c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言。",
        "from": "T21n1283_002:0348c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)藥羼曩那(引)曳捺囉(二合)馱曩必里(二合)野鉢舍也(二合)鉢舍也(二合引)娑嚩(二合引)賀次作獻座印。",
        "from": "T21n1283_002:0348c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌舉手三雙指屈第三節。",
        "from": "T21n1283_002:0348c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中指磔開大指各各相離。",
        "from": "T21n1283_002:0348c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是寶藏神師子座印。",
        "from": "T21n1283_002:0348c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此印結中間。",
        "from": "T21n1283_002:0348c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜叉王并眷屬同坐師子座。",
        "from": "T21n1283_002:0348c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切願誦此真言。",
        "from": "T21n1283_002:0348c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)惹敢(二合)婆羅惹隣捺囉(二合)伊那摩(引)娑曩摩里伽(二合)娑嚩(二合引)賀次結洗浴印。",
        "from": "T21n1283_002:0348c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請召寶藏神并眷屬洗浴。",
        "from": "T21n1283_002:0348c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手作掬。",
        "from": "T21n1283_002:0348c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小指相交。",
        "from": "T21n1283_002:0348c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二無名指安入掌中。",
        "from": "T21n1283_002:0348c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二中指如針。",
        "from": "T21n1283_002:0348c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭指附中指第三節。",
        "from": "T21n1283_002:0348c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指亦附頭指。",
        "from": "T21n1283_002:0348c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此真言。",
        "from": "T21n1283_002:0348c21_0",
        "context_with_next_sentence": true
    },
    {
