[
    {
        "source_sentence": "貞元新定釋教目錄卷第十九",
        "from": "T55n2157_019:0903c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西京西明寺三藏沙門圓照撰",
        "from": "T55n2157_019:0903c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總集群經錄上之十九",
        "from": "T55n2157_019:0903c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敘列古今諸家目錄之二",
        "from": "T55n2157_019:0903c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大唐開元釋教錄二十卷　庚午歲西崇福寺沙門智昇撰右此中所撰總分上下兩錄具件如左。",
        "from": "T55n2157_019:0903c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總集群經錄上(從漢至唐所出經教區別人代具顯此中目錄始終續於後列)。",
        "from": "T55n2157_019:0903c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後漢傳譯緇素一十二人。",
        "from": "T55n2157_019:0903c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經并新舊集失譯諸經。",
        "from": "T55n2157_019:0903c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總二百九十二部合三百九十五卷(九十七部一百三十一卷見在一百五十五部二百六十卷闕本)。",
        "from": "T55n2157_019:0903c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曹魏傳譯沙門五人。",
        "from": "T55n2157_019:0904a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經戒羯磨。",
        "from": "T55n2157_019:0904a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一十二部合一十八卷(四部五卷見在八部一十三卷闕本)。",
        "from": "T55n2157_019:0904a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第一卷。",
        "from": "T55n2157_019:0904a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吳代傳譯緇素五人。",
        "from": "T55n2157_019:0904a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經等并及失譯。",
        "from": "T55n2157_019:0904a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一百八十九部合四百一十七卷(六十一部九十二卷見在一百二十八部三百二十五卷闕本)。",
        "from": "T55n2157_019:0904a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西晉傳譯緇素一十二人。",
        "from": "T55n2157_019:0904a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經戒等并新舊集失譯諸經。",
        "from": "T55n2157_019:0904a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總三百三十三部合五百九十一卷(一百五十六部二百三十一卷見在一百七十七部三百六十九卷本闕)。",
        "from": "T55n2157_019:0904a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第二卷。",
        "from": "T55n2157_019:0904a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東晉傳譯緇素一十六人。",
        "from": "T55n2157_019:0904a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論并新舊集失譯諸經。",
        "from": "T55n2157_019:0904a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一百六十八部合四百六十八卷(八十五部三百三十六卷見在八十三部三百三十二卷本闕)。",
        "from": "T55n2157_019:0904a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "符秦傳譯沙門六人。",
        "from": "T55n2157_019:0904a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論等。",
        "from": "T55n2157_019:0904a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一十五部合一百九十七卷(七部六十五卷見在八部一百三十二卷本闕)。",
        "from": "T55n2157_019:0904a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第三卷。",
        "from": "T55n2157_019:0904a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姚秦傳譯沙門五人。",
        "from": "T55n2157_019:0904a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論等。",
        "from": "T55n2157_019:0904a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總九十四部合六百二十四卷(六十六部五百二十八卷見在二十八部九十六卷闕本)。",
        "from": "T55n2157_019:0904a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乞伏秦傳譯沙門一人所出經并三秦代新舊失譯經律論等。",
        "from": "T55n2157_019:0904a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總五十六部合一百一十卷(三十二部七十九卷見在二十四部三十一卷闕本)。",
        "from": "T55n2157_019:0904a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前涼傳譯外國優婆塞一人。",
        "from": "T55n2157_019:0904a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經。",
        "from": "T55n2157_019:0904a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總四部合六卷(一部一卷見在三部五卷本闕)。",
        "from": "T55n2157_019:0904a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北涼傳譯緇素九人。",
        "from": "T55n2157_019:0904a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論等并新舊集失譯諸經。",
        "from": "T55n2157_019:0904a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總八十二部合三百一十一卷(二十五部二百九卷見在五十七部一百二卷本闕)。",
        "from": "T55n2157_019:0904a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第四卷。",
        "from": "T55n2157_019:0904a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宋代傳譯緇素二十二人。",
        "from": "T55n2157_019:0904a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論等并新集失譯諸經。",
        "from": "T55n2157_019:0904a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總四百六十五部合七百一十七卷(九十三部二百四十三卷見在三百七十二部四百七十四卷本闕)。",
        "from": "T55n2157_019:0904b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第五卷。",
        "from": "T55n2157_019:0904b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蕭齊傳譯沙門七人。",
        "from": "T55n2157_019:0904b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律。",
        "from": "T55n2157_019:0904b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一十二部合三十三卷(七部二十三卷見在五部五卷本闕)。",
        "from": "T55n2157_019:0904b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梁代傳譯緇素八人。",
        "from": "T55n2157_019:0904b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經論及諸傳記。",
        "from": "T55n2157_019:0904b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并新集失譯經律集等。",
        "from": "T55n2157_019:0904b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總四十六部。",
        "from": "T55n2157_019:0904b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合二百一十七卷(四十部一百九十一卷見在六部十卷本闕)。",
        "from": "T55n2157_019:0904b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元魏傳譯緇素一十二人。",
        "from": "T55n2157_019:0904b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經論。",
        "from": "T55n2157_019:0904b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總八十三部合二百七十四卷(七十三部二百五十五卷見在一十部一十九卷本闕)。",
        "from": "T55n2157_019:0904b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高齊傳譯緇素二人。",
        "from": "T55n2157_019:0904b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經論。",
        "from": "T55n2157_019:0904b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總八部合五十二卷(並在無闕)。",
        "from": "T55n2157_019:0904b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第六卷。",
        "from": "T55n2157_019:0904b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周朝傳譯沙門四人。",
        "from": "T55n2157_019:0904b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經論等。",
        "from": "T55n2157_019:0904b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一十四部合二十九卷(六部一十一卷見在八部一十八卷本闕)。",
        "from": "T55n2157_019:0904b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陳代傳譯緇素三人。",
        "from": "T55n2157_019:0904b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論及集傳等。",
        "from": "T55n2157_019:0904b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總四十部合一百三十三卷(二十六部八十九卷見在一十四部四十四卷本闕)。",
        "from": "T55n2157_019:0904b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隋朝傳譯緇素九人。",
        "from": "T55n2157_019:0904b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經論傳錄等。",
        "from": "T55n2157_019:0904b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總九十九部合三百一卷(六十二部二百八十七卷見在二部一十四卷本闕)。",
        "from": "T55n2157_019:0904b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第七卷。",
        "from": "T55n2157_019:0904b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皇朝傳譯緇素已有三十七人。",
        "from": "T55n2157_019:0904b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論及傳錄等。",
        "from": "T55n2157_019:0904b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總三百一部合二千一百七十卷(二百八十一部二千一百四十三卷見在二十部二十七卷訪本未獲)。",
        "from": "T55n2157_019:0904b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "都計一十九代傳譯道俗等。",
        "from": "T55n2157_019:0904b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一百七十六人。",
        "from": "T55n2157_019:0904b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出大小乘經律論及賢聖集傳。",
        "from": "T55n2157_019:0904b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二千二百七十八部合七千四十六卷(一千一百三十部五千四十八卷見在一千一百四十八部一千九百九十八卷本闕)。",
        "from": "T55n2157_019:0904b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第八第九卷。",
        "from": "T55n2157_019:0904c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合從古錄至開元釋教新錄及續補舊闕。",
        "from": "T55n2157_019:0904c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總有四十一家具如前列。",
        "from": "T55n2157_019:0904c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第十卷。",
        "from": "T55n2157_019:0904c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別分乘藏錄下(上錄所辦總顯出經而大小未分三藏混雜此錄之內具件科條闕本等經續附於後)。",
        "from": "T55n2157_019:0904c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從別錄中曲分為七。",
        "from": "T55n2157_019:0904c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有譯有本錄第一(一千一百二十四部五千四十八卷)。",
        "from": "T55n2157_019:0904c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復就此錄更開為三。",
        "from": "T55n2157_019:0904c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩三藏錄第一(六百八十六部二千七百四十五卷)菩薩契經藏(五百七十三部二千一百七十三卷)大乘經單重合譯(總四百三十二部一千八百八十卷)般若經新舊譯(二十一部七百三十六卷)寶積經新舊譯(八十二部一百六十九卷)大集經新舊譯(三十四部一百四十二卷)花嚴經新舊譯(二十六部一百八十七卷)涅槃經新舊譯(六部五十八卷)五大部外諸重譯經(二百七十三部五百八十八卷)大乘經單譯(總一百三十一部二百九十三卷)菩薩調伏藏(二十六部五十四卷)菩薩對法藏(九十七部五百一十八卷)大乘釋經論(二十一部一百五十五卷)大乘集義論(七十六部三百六十三卷)右為第十一十二卷。",
        "from": "T55n2157_019:0904c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聲聞三藏錄第二(三百三十部一千七百六十二卷)聲聞契經藏(二百四十部六百一十八卷)小乘經單重合譯(總一百五十三部三百九十四卷)根本四阿含經(五部二百三卷)長阿含中別譯經(二十二卷)中阿含中別譯經(六十卷)增壹阿含中別譯經(五十一卷)雜阿含中別譯經(五十卷)別譯雜阿含經(二十卷)四含外諸重譯經(一百四十八部一百九十一卷)小乘經單譯(總八十七部二百二十四卷)聲聞調伏藏(五十四部四百四十六卷)正調伏藏(四十一部三百五十八卷)調伏藏眷屬(一十三部八十八卷)聲聞對法藏(三十六部六百九十八卷)有部根本身足論(八部一百三十一卷)有部及餘支派論(二十八部五百六十七卷)聖賢傳記錄第三(一百八部五百四十一卷)梵本翻譯集傳(六十八部一百七十三卷)此方撰述集傳(四十部三百六十八卷)右為第十三卷。",
        "from": "T55n2157_019:0904c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有譯無本錄第二(一千一百四十八部一千九百八十卷)大乘經闕本(四百八部八百一卷)大乘經重譯闕本(總二百部四百八十四卷)般若部中闕本(一十部二十七卷)寶積部中闕本(三十部五十四卷)大集部中闕本(一十三部九十七卷)花嚴部中闕本(一十二部二十七卷)涅槃部中闕本(四部二十六卷)諸重譯經闕本(一百三十一部二百五十三卷)大乘經單譯闕本(總二百八部三百一十七卷)大乘律闕本(二十二部二十五卷)大乘論闕本(二十部四十八卷)大乘釋經論闕本(四部一十一卷)大乘集義論闕本(一十六部三十七卷)右為第十四卷。",
        "from": "T55n2157_019:0905a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小乘經闕本(六百五部八百一十五卷)小乘重譯經闕本(總一百二十五部二百六十五卷)根本四阿含闕本(二部一百九卷)長阿含部分闕本(一十四部二十八卷)中阿含部分闕本(一十四部一十四卷)增一阿含部分闕本(七部八卷)雜阿含部分闕本(四十五部四十五卷)諸重譯經闕本(四十三部六十二卷)小乘經單譯闕本(總四百八十部五百五十卷)小乘律闕本(三十七部四十二卷)賢聖集傳闕本(四十七部一百八十四卷)右為第十五卷。",
        "from": "T55n2157_019:0905b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "支派別行錄第三(六百八十二部八百一十二卷)大乘別生經(二百九部三百二十八卷)般若部中別生(一十部一百一十九卷)寶積部中別生(一十三部一十五卷)大集部中別生(五十一部五十八卷)花嚴部中別生(一十部一十一卷)諸大乘經別生(二百二十五部一百二十五卷)大乘律別生(七部七卷)大乘論別生(七部一十一卷)小乘別生經(二百八十三部二百八十三卷)長阿含部分別生(一十四部一十四卷)中阿含部分別生(二十四部二十四卷)增一阿含部分別生(二十六部二十六卷)雜阿含部分別生(一百三十六部一百三十六卷)諸小乘經別生(八十三部八十三卷)小乘律別生(小乘論無別生四十二部四十九卷)賢聖集傳別生(一百三十四部一百三十四卷)右為第十六卷。",
        "from": "T55n2157_019:0905b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刪略繁重錄第四(一百四十七部四百八卷)新撿出別生經(六十七部一百八十五卷)新撿出名異文同經(二十部五十二卷)新撿出重上錄經(八部三十卷)補闕拾遺錄第五(三百六部一千一百一十一卷)大乘經舊譯(五十九部六十九卷)大乘律舊譯(二部二卷)大乘論舊譯(三部四卷)小乘經舊譯(五十九部六十八卷)小乘律舊譯(小乘舊論無遺五部五卷)賢聖集傳舊譯(三十六部五十七卷)右已上一百六十部二百五卷。",
        "from": "T55n2157_019:0905c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並是舊譯今見有本。",
        "from": "T55n2157_019:0906a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大周入藏兼拾遺編入。",
        "from": "T55n2157_019:0906a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "新譯大乘經(六十一部三百三十六卷)新譯大乘律(一部一卷)新譯大乘論(一十二部一十九卷)新譯小乘經(八部八卷)新譯小乘律(小乘論無新譯一十一部一百五十九卷)新譯賢聖集傳(三部五卷)右已上九十六部五百二十八卷並是大周刊定錄後。",
        "from": "T55n2157_019:0906a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "新譯所以前錄未載。",
        "from": "T55n2157_019:0906a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今補闕編。",
        "from": "T55n2157_019:0906a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又小乘律戒羯磨(六部一十卷然並撰述有據時代盛行補闕編入)。",
        "from": "T55n2157_019:0906a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此方所撰集傳(四十部三百六十八卷然皆補助正教故然補闕編入目錄)。",
        "from": "T55n2157_019:0906a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第十七卷。",
        "from": "T55n2157_019:0906a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疑惑再詳錄第六(一十四部一十九卷)偽妄亂真錄第七(三百九十四部或九十四一千五十七卷或五十五卷未詳)開元釋教錄新編偽經(三十九部或三十七五十六卷或五十四未詳)苻秦釋道安錄中偽經(二十五部二十八卷)梁釋僧祐錄中偽經(一十四部一十六卷)簫齊釋道備偽撰經(見長房錄五部五卷)簫齊僧法尼誦出經(僧祐等錄並編入偽二十一部三十五卷)元魏孫敬夢授經(見內典錄一部一卷)梁沙門妙光偽造經(見僧祐錄一部一卷)隋開皇眾經錄中偽經(八十六部一百四十一卷)隋仁壽眾經錄中偽經(一十一部四十一卷)大唐內典錄中偽經(二十二部八十七卷)大周刊定錄中偽經(八十部或二十未詳一百一卷)隋沙門信行三階集偽錄(周錄雖載收之不盡三十五部　四十四卷)諸雜抄經增減聖說(五十四部　五百一卷)有為第十八卷。",
        "from": "T55n2157_019:0906a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大乘入藏錄上(大乘經律論總六百三十八部二千七百四十五卷　二百五十八帙此直述注名及摽紙數餘如廣錄)大乘經(五百六十三部　二千一百七十三卷　二百三帙)大乘律(二十六部　五十四卷　五帙)大乘論(九十七部　五百一十八卷　五十帙)右為第十九卷。",
        "from": "T55n2157_019:0906b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小乘入藏錄下(小乘經律論總三百三十一部　一千七百六十二卷　一百六十五帙集傳錄附此直述經名及摽紙數餘如廣錄)小乘經(二百四十部　六百一十八卷　四十八帙)小乘律(五十四部　四百四十六卷　四十五帙)小乘論(三十六部　六百九十八卷　七十二帙)賢聖集(一百八部　五百四十一卷　五十七帙)都計大小乘經律論及賢聖傳見入藏者。",
        "from": "T55n2157_019:0906b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一千七十六部　合五千四十八卷　四百八十帙(此入藏中大乘經部數與前廣錄部數不同者以前大寶積經諸部合成故在本數為四十九部上錄此中合為一部故欠四十八不同)。",
        "from": "T55n2157_019:0906b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第二十卷。",
        "from": "T55n2157_019:0906b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(庚辰歲西明寺翻經臨壇賜紫沙門圓照等奉勅撰)。",
        "from": "T55n2157_019:0906b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貞元新定釋教目錄三十卷",
        "from": "T55n2157_019:0906b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右此中所撰准舊。",
        "from": "T55n2157_019:0906b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦總分上下兩錄。",
        "from": "T55n2157_019:0906b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具件如左。",
        "from": "T55n2157_019:0906b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總集群經錄上。",
        "from": "T55n2157_019:0906b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中分二。",
        "from": "T55n2157_019:0906b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一特奉制旨。",
        "from": "T55n2157_019:0906b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二總集群經。",
        "from": "T55n2157_019:0906c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後漢傳譯緇素一十二人。",
        "from": "T55n2157_019:0906c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律并新舊集失譯諸經。",
        "from": "T55n2157_019:0906c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總二百九十二部合三百九十五卷(九十七部一百三十一卷見在一百九十五部二百六十卷本闕)。",
        "from": "T55n2157_019:0906c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曹魏傳譯沙門五人。",
        "from": "T55n2157_019:0906c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經戒羯磨。",
        "from": "T55n2157_019:0906c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一十二部合一十八卷(四部五卷見在八部一十三卷本闕)。",
        "from": "T55n2157_019:0906c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吳代傳譯緇素五人。",
        "from": "T55n2157_019:0906c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經等并及失譯。",
        "from": "T55n2157_019:0906c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一百八十九部合四百一十七卷(六十一部九十二卷見在一百二十八部三百二十五卷本闕)。",
        "from": "T55n2157_019:0906c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西晉傳譯緇素一十二人。",
        "from": "T55n2157_019:0906c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經戒等并新舊集失譯諸經。",
        "from": "T55n2157_019:0906c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總三百三十三部合五百九十一卷(一百五十六部三百三十一卷見在一百七十七部二百六十九卷本闕)。",
        "from": "T55n2157_019:0906c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第一第二第三第四卷。",
        "from": "T55n2157_019:0906c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東晉傳譯緇素一十六人。",
        "from": "T55n2157_019:0906c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論并新舊集失譯經一百六十八部。",
        "from": "T55n2157_019:0906c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合四百六十八卷(八十五部三百三十六卷見在八十三部三百三十二卷本闕)。",
        "from": "T55n2157_019:0906c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苻秦傳譯沙門六人。",
        "from": "T55n2157_019:0906c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論等。",
        "from": "T55n2157_019:0906c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一十五部。",
        "from": "T55n2157_019:0906c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合一百九十七卷(七部六十五卷見在八部一百三十二卷闕本)。",
        "from": "T55n2157_019:0906c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第五卷。",
        "from": "T55n2157_019:0906c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姚秦傳譯沙門五人。",
        "from": "T55n2157_019:0906c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論等。",
        "from": "T55n2157_019:0906c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總九十四部。",
        "from": "T55n2157_019:0906c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合六百二十四卷(六十六部五百二十八卷見在二十八部九十六卷本闕)。",
        "from": "T55n2157_019:0906c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乞伏秦傳譯沙門一人。",
        "from": "T55n2157_019:0906c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經并三秦代新舊失譯經律論等。",
        "from": "T55n2157_019:0906c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總五十六部。",
        "from": "T55n2157_019:0906c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合一百一十卷(三十二部七十九卷見在二十四部三十一卷本闕)。",
        "from": "T55n2157_019:0906c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前涼傳譯外國優婆塞一人。",
        "from": "T55n2157_019:0906c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經總四部合六卷(一部一卷見在三部五卷本闕)。",
        "from": "T55n2157_019:0906c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北涼傳譯緇素九人。",
        "from": "T55n2157_019:0906c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論等并新舊集失譯諸經。",
        "from": "T55n2157_019:0906c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總八十二部。",
        "from": "T55n2157_019:0907a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合三百一十一卷(一十五部二百九卷見在五十七部一百二卷本闕)。",
        "from": "T55n2157_019:0907a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第六卷。",
        "from": "T55n2157_019:0907a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宋代傳譯緇素二十二人。",
        "from": "T55n2157_019:0907a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論等并集失譯諸經。",
        "from": "T55n2157_019:0907a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總四百六十五部。",
        "from": "T55n2157_019:0907a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合七百一十七卷(九十三部二百四十三卷見在三百七十三部四百七十四卷本闕)。",
        "from": "T55n2157_019:0907a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "簫齊傳譯沙門七人。",
        "from": "T55n2157_019:0907a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律。",
        "from": "T55n2157_019:0907a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一十二部。",
        "from": "T55n2157_019:0907a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合三十三卷(七部二十八卷見在五卷本闕)。",
        "from": "T55n2157_019:0907a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第七第八卷。",
        "from": "T55n2157_019:0907a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梁代傳譯緇素九人。",
        "from": "T55n2157_019:0907a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經論。",
        "from": "T55n2157_019:0907a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸傳記并新集失譯經律集等。",
        "from": "T55n2157_019:0907a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總四十七部。",
        "from": "T55n2157_019:0907a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合二百一十七卷(四十一部二百七卷見在六部十卷本闕)。",
        "from": "T55n2157_019:0907a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元魏傳譯緇素一十二人。",
        "from": "T55n2157_019:0907a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經論總八十三部。",
        "from": "T55n2157_019:0907a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合二百七十四卷(七十三部二百五十五卷見在一十部一十九卷本闕)。",
        "from": "T55n2157_019:0907a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高齊傳譯緇素二人。",
        "from": "T55n2157_019:0907a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經論。",
        "from": "T55n2157_019:0907a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總八部合五十二卷(並在無闕)。",
        "from": "T55n2157_019:0907a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第九卷。",
        "from": "T55n2157_019:0907a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周朝傳譯沙門四人。",
        "from": "T55n2157_019:0907a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經論等。",
        "from": "T55n2157_019:0907a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一十四部。",
        "from": "T55n2157_019:0907a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合二十九卷(六部一十一卷見在八部一十八卷本闕)。",
        "from": "T55n2157_019:0907a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陳代傳譯緇素三人。",
        "from": "T55n2157_019:0907a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論及集傳等。",
        "from": "T55n2157_019:0907a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總四十部。",
        "from": "T55n2157_019:0907a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合一百三十三卷(二十六部八十九卷見在一十四部四十四卷本闕)。",
        "from": "T55n2157_019:0907a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隋朝傳譯緇素十人。",
        "from": "T55n2157_019:0907a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經論傳集錄等。",
        "from": "T55n2157_019:0907a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總九十九部。",
        "from": "T55n2157_019:0907a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合三百四十五卷(九十七部三百三十一卷見在二部一十四卷本闕)。",
        "from": "T55n2157_019:0907a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第十卷。",
        "from": "T55n2157_019:0907a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皇朝傳譯緇素已有四十六人。",
        "from": "T55n2157_019:0907a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出經律論及傳錄等。",
        "from": "T55n2157_019:0907a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總四百三十四部。",
        "from": "T55n2157_019:0907a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合二千四百六十三卷(四百十部二千三百八十六卷見在二十七部七十七卷訪本未獲足)。",
        "from": "T55n2157_019:0907a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "都計一十九代傳譯道俗。",
        "from": "T55n2157_019:0907b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一百八十七人。",
        "from": "T55n2157_019:0907b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所出大小乘經律論及賢聖集傳。",
        "from": "T55n2157_019:0907b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總二千四百四十七部。",
        "from": "T55n2157_019:0907b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合七千三百七十九卷(一千二百五十七部五千三百七十八卷見在一千一百五十五部二千四十八卷本闕內五十遺編律文七部五十卷闕三十卷故此重摽五十卷)。",
        "from": "T55n2157_019:0907b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第十一十二十三十四十五十六十七卷。",
        "from": "T55n2157_019:0907b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合從古錄至貞元新定釋教目錄及。",
        "from": "T55n2157_019:0907b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "續補舊闕。",
        "from": "T55n2157_019:0907b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總有四十二家。",
        "from": "T55n2157_019:0907b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具如前列。",
        "from": "T55n2157_019:0907b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第十八十九卷。",
        "from": "T55n2157_019:0907b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別分乘藏錄下(上餘所辯總顯出經而大小未分三藏混雜此錄之內具件科條闕本等經續附於後)。",
        "from": "T55n2157_019:0907b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中分二。",
        "from": "T55n2157_019:0907b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初別分乘藏。",
        "from": "T55n2157_019:0907b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二賢傳記。",
        "from": "T55n2157_019:0907b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就別錄曲分第七。",
        "from": "T55n2157_019:0907b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有譯有本錄第一(一千二百五十六部五千三百七十八卷)。",
        "from": "T55n2157_019:0907b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復就此錄更開為二。",
        "from": "T55n2157_019:0907b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一三藏差殊。",
        "from": "T55n2157_019:0907b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二聖賢傳記。",
        "from": "T55n2157_019:0907b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏復二。",
        "from": "T55n2157_019:0907b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一菩薩三藏。",
        "from": "T55n2157_019:0907b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二聲聞三藏。",
        "from": "T55n2157_019:0907b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩三藏錄第一(八百七部二千九百七十八卷)菩薩契經藏(六百八十一部二千四百三卷)大乘經單重合譯(五百五十四部二千一百二卷)般若經新舊譯(三十一部七百五十七卷)寶積經新舊譯(八十三部一百七十二卷)大集經新舊譯(三十七部一百五十二卷)華嚴經新舊譯(四十部二百四十七卷)涅槃經舊譯(六部五十八卷)五大部外諸重譯經(三百一十八部六百四十五卷)大乘經單譯(總一百七十九部三百九十七卷)菩薩調伏藏(二十七部五十五卷)菩薩對法藏(九十九部五百二十卷)大乘釋經論(二十一部一百五十五卷)大乘集義論(七十八部三百六十五卷)右為第二十卷二十一卷二十二卷。",
        "from": "T55n2157_019:0907b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聲聞三藏錄第二(三百三十八部一千八百一十二卷)聲聞契經藏(二百四十部六百一十八卷)小乘經單重合譯(總一百五十三部三百九十四卷)根本四阿含經(五部二百三卷)長阿含中別譯經(二十二卷)中阿含中別譯經(六十卷)增壹阿含中別譯經(五十一卷)雜阿含中別譯經(五十卷)別譯雜阿含經(二十卷)四阿含外諸重譯經(一百四十八部一百九十一卷)小乘經單譯(總八十七部二百二十四卷)聲聞調伏藏(六十一部四百九十三卷)正調伏藏(四十八部四百五卷內欠三卷未獲本)調伏藏眷屬(一十三部八十八卷)聲聞對法藏(三十六部六百九十八卷)有部根本身足論(八部一百三十一卷)有部及餘支派論(二十八部五百六十七卷)一聖賢傳記錄第三(一百一十二部五百九十一卷)就此錄中復分為二。",
        "from": "T55n2157_019:0907c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一梵本翻譯集傳(六十八部一百七十三卷)二此方撰述集傳(四十四部四百一十八卷)右為第二十三卷。",
        "from": "T55n2157_019:0908a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有譯無本錄第二(一千一百四十部一千九百八十卷)大乘經闕本(四百八部八百一卷)大乘重譯闕本(總二百部四百八十四卷)般若部中闕本(一十部二十七卷)寶積部中闕本(十三部五十四卷)大集部中闕本(一十三部九十七卷)華嚴部中闕本(一十二部二十七卷)涅槃部中闕本(四部二十六卷)諸重譯經闕本(一百三十一部二百五十三卷)大乘經單譯闕本(總二百八部三百一十七卷)大乘律闕本(二十二部二十五卷)大乘論闕本(二十部四十八卷)大乘釋經論闕本(四部一十一卷)大乘集義論闕本(一十六部三十七卷)右為第二十四卷。",
        "from": "T55n2157_019:0908a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小乘經闕本(六百五部八百一十五卷)小乘經重譯闕本(總二百二十五部二百六十五卷)根本四阿含闕本(二部一百九卷)長阿含部分闕本(一十四部二十八卷)中阿含部分闕本(一十四部一十四卷)增一阿含部分闕本(七部八卷)雜阿含部分闕本(四十五部四十五卷)諸重譯經闕本(四十三部六十二卷)小乘經單譯闕本(總四百八十部五百五十卷)小乘律闕本(四十四部九十二卷)小乘論闕本(九部六十五卷)賢聖集傳闕本(四十七部一百八十四卷)右為第二十五卷。",
        "from": "T55n2157_019:0908a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "支派別行錄第三(六百八十二部八百一十二卷)大乘別生經(二百九部三百二十八卷)般若部中別生(一十部一百一十九卷)寶積部中別生(一十二部一十五卷)大集部中別生(五十一部五十八卷)華嚴部中別生(一十部一十一卷)諸大乘經別生(一百二十五部一百二十五卷)大乘律別生(七部七卷)大乘論別生(七部一十一卷)小乘別生經(二百八十三部二百八十三卷)長阿含部分別生(一十四部一十四卷)中阿含部分別生(二十四部二十四卷)增一阿含部分別生(二十六部二十六卷)雜阿含部分別生(一百三十部一百三十六卷)諸小乘經別生(八十三部八十三卷)小乘律別生(小乘論無別生四十二部四十九卷)賢聖集傳別生(一百三十四部一百三十四卷)右為第二十六卷。",
        "from": "T55n2157_019:0908b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刪略繁重錄第四(一百四十七部四百八卷)新撿出別生經(六十七部一百八十五卷)新撿出名異文同經(三十部五十二卷)新撿出重上錄經(八部三十卷)新撿出合入大部經(五十二部一百四十一卷)補闕拾遺錄第五(三百六部一千一百一十一卷)大乘經舊譯(五十九部六十九卷)大乘律舊譯(二部二卷)大乘論舊譯(三部四卷)小乘經舊譯(五十九部六十八卷)小乘律舊譯(小乘舊論無遺五部五卷)賢聖集論舊譯(三十六部五十七卷)右已上一百六十四部二百五卷並是舊譯今見有本。",
        "from": "T55n2157_019:0908c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大周入藏中無。",
        "from": "T55n2157_019:0908c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今拾遺編入。",
        "from": "T55n2157_019:0908c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "新譯大乘經(六十一部三百三十六卷)新譯大乘律(一部一卷)新譯大乘論(一十二部一十九卷)新譯小乘經(八部八卷)新譯小乘律(小乘論無新譯一十一部一百五十九卷)新譯賢聖集傳(三部五卷)右已上九十六部(五百二十八卷)並是大周刊定錄後新譯所以前錄未載。",
        "from": "T55n2157_019:0908c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今補闕編入。",
        "from": "T55n2157_019:0908c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又小乘律戒羯磨(六部一十卷然並撰述有據時代盛行補闕編入)。",
        "from": "T55n2157_019:0909a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此方所撰集傳(四十部三百六十八卷然皆補助正教並補闕編入見錄)。",
        "from": "T55n2157_019:0909a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此方撰集經(一部一十六卷或十八卷)。",
        "from": "T55n2157_019:0909a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右撿舊錄。",
        "from": "T55n2157_019:0909a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在偽妄亂真錄中。",
        "from": "T55n2157_019:0909a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今准勅編入貞元新定釋教目錄(此一朝傳出梁朝今准入錄年代唐朝年月日編在梁末)。",
        "from": "T55n2157_019:0909a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此方法琳別傳三卷。",
        "from": "T55n2157_019:0909a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右開元錄。",
        "from": "T55n2157_019:0909a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中禁斷。",
        "from": "T55n2157_019:0909a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今再詳准勅編入貞元釋教錄。",
        "from": "T55n2157_019:0909a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此方續開元釋教錄一部三卷。",
        "from": "T55n2157_019:0909a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "准勅編入貞元釋教錄。",
        "from": "T55n2157_019:0909a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此方撰述目錄一部三十卷(准例編入貞元新定釋教目錄)。",
        "from": "T55n2157_019:0909a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第二十七卷。",
        "from": "T55n2157_019:0909a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疑惑再詳錄第六(一十四部一十九卷)偽妄亂真錄第七(三百九十四部一千四十一卷)開元釋教錄新編偽經(三十九部五十六卷)符秦釋道安錄中偽經(三十五部二十八卷)梁釋僧祐錄中偽經(一十四部一十六卷)蕭齊釋道備偽撰經(見長房錄)五卷元魏孫敬德夢授經(見內典錄一部一卷)梁沙門妙光偽造經(見僧祐錄一部一卷)隋開皇眾經中偽經(八十六部一百四十一卷)隋仁壽眾經錄中偽經(一十一部四十一卷)大唐內典錄中偽經(二十二部八十七卷)大周刊定錄中偽經(八十部一百一卷)隋沙門信行三階集錄(周錄雖載收之不盡三十五部　四十四卷)諸雜抄經增減聖說(五十四部　五百一卷)右為第二十八卷。",
        "from": "T55n2157_019:0909a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大乘入藏錄上(大乘經律論總七百六十三部　二千九百五十八卷　二百六十七帙此直列經名及標紙數餘如廣錄)大乘經(六百八十六部　二千七百四十五卷　二百二十帙)大乘律(二十七部　五十五卷　五帙)大乘論(九十九部　五百二十卷　五十帙)右為第二十九卷。",
        "from": "T55n2157_019:0909b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小乘藏入藏錄下(小乘經律論總四百四十七部　二千三百八十九卷　一百七十帙傳錄附此未此直述經名及標紙數餘如廣錄)小乘經(二百四十部　六百一十八卷　四十八帙)小乘律(六十一部　四百九十三卷　五十帙)小乘論(三十六部　六百九十八卷　七十帙)賢聖集(一百一十二部　五百九十一卷　六十四帙)都大小乘經律論及賢聖傳見入藏者。",
        "from": "T55n2157_019:0909b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總一千二百五十六部。",
        "from": "T55n2157_019:0909b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合五千三百七十八卷。",
        "from": "T55n2157_019:0909b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五百九帙。",
        "from": "T55n2157_019:0909b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此入藏中大乘經部數與前廣錄數不同者。",
        "from": "T55n2157_019:0909b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前以大寶積經諸部合成。",
        "from": "T55n2157_019:0909b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故存本數為四十九部上錄。",
        "from": "T55n2157_019:0909b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中合為一部。",
        "from": "T55n2157_019:0909b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故欠四十八。",
        "from": "T55n2157_019:0909b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不同。",
        "from": "T55n2157_019:0909b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右為第三十卷。",
        "from": "T55n2157_019:0909b21_0",
        "context_with_next_sentence": true
    },
