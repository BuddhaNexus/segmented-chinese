[
    {
        "source_sentence": "佛說無能勝幡王如來莊嚴陀羅",
        "from": "T19n0943_001:0098b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尼經",
        "from": "T19n0943_001:0098b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天北印度烏填曩國帝釋宮寺",
        "from": "T19n0943_001:0098b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏賜紫沙門臣施護奉　詔譯",
        "from": "T19n0943_001:0098b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T19n0943_001:0098b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時世尊。",
        "from": "T19n0943_001:0098b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在忉利天帝釋宮中善法堂會。",
        "from": "T19n0943_001:0098b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而於是時忽有阿素囉王。",
        "from": "T19n0943_001:0098b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與諸眷屬部領兵眾。",
        "from": "T19n0943_001:0098b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各各身被甲冑手執器仗。",
        "from": "T19n0943_001:0098b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後圍遶來相戰鬪。",
        "from": "T19n0943_001:0098b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "討伐忉利天宮。",
        "from": "T19n0943_001:0098b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時帝釋天主與諸天眾。",
        "from": "T19n0943_001:0098b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身嚴寶鎧執持器仗與之共戰。",
        "from": "T19n0943_001:0098b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時阿素囉王兵眾得勝。",
        "from": "T19n0943_001:0098b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忉利天眾退敗怖散馳走。",
        "from": "T19n0943_001:0098b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時帝釋天主。",
        "from": "T19n0943_001:0098b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隱天主容儀現常人相。",
        "from": "T19n0943_001:0098b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速疾奔馳往覲世尊。",
        "from": "T19n0943_001:0098b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到佛所已頂禮佛足。",
        "from": "T19n0943_001:0098b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而白佛言世尊。",
        "from": "T19n0943_001:0098b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今為阿素囉王將諸兵眾來相戰伐。",
        "from": "T19n0943_001:0098b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忉利天眾退散怖散馳走。",
        "from": "T19n0943_001:0098c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以何方便救我此難。",
        "from": "T19n0943_001:0098c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時。",
        "from": "T19n0943_001:0098c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告帝釋天主言。",
        "from": "T19n0943_001:0098c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當諦聽吾為汝說。",
        "from": "T19n0943_001:0098c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有陀羅尼名無能勝幡莊嚴。",
        "from": "T19n0943_001:0098c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我於往昔為菩薩時。",
        "from": "T19n0943_001:0098c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有佛號無能勝幡王如來。",
        "from": "T19n0943_001:0098c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彼佛所得聞此陀羅尼。",
        "from": "T19n0943_001:0098c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從是已來我亦為他。",
        "from": "T19n0943_001:0098c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣說是法得大徵祥。",
        "from": "T19n0943_001:0098c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而未曾有彈指之頃怖畏之事。",
        "from": "T19n0943_001:0098c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時佛復告帝釋天主言。",
        "from": "T19n0943_001:0098c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今為汝說是無能勝幡莊嚴陀羅尼曰。",
        "from": "T19n0943_001:0098c08_6",
        "context_with_next_sentence": true
    },
