        "from": "T19n0943_001:0098c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此無能勝幡莊嚴陀羅尼。",
        "from": "T19n0943_001:0099b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常於內外加護於我。",
        "from": "T19n0943_001:0099b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝應持是陀羅尼。",
        "from": "T19n0943_001:0099b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以雜色綵作囊盛之繫於旌旗之上。",
        "from": "T19n0943_001:0099b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令汝所往之處。",
        "from": "T19n0943_001:0099b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或與他敵相鬪相罵相殺。",
        "from": "T19n0943_001:0099b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或與他冤言詞相競一切之處無不獲勝。",
        "from": "T19n0943_001:0099b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝應書寫此陀羅尼。",
        "from": "T19n0943_001:0099b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以囊盛之繫於項上護持於汝。",
        "from": "T19n0943_001:0099b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦為未來之世。",
        "from": "T19n0943_001:0099b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間仁王一切之處作其衛護。",
        "from": "T19n0943_001:0099b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼無能勝幡王如來。",
        "from": "T19n0943_001:0099b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現天女相在於面前。",
        "from": "T19n0943_001:0099b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施與無畏護持獲勝。",
        "from": "T19n0943_001:0099b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼冤敵軍眾散敗。",
        "from": "T19n0943_001:0099b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若是仁王復能書寫流布受持。",
        "from": "T19n0943_001:0099b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讀誦此陀羅尼者。",
        "from": "T19n0943_001:0099b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常獲清淨吉祥之事。",
        "from": "T19n0943_001:0099b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸外冤敵無能得勝。",
        "from": "T19n0943_001:0099b19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說是經已。",
        "from": "T19n0943_001:0099b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝釋天主及諸天眾。",
        "from": "T19n0943_001:0099b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞佛所說皆大歡喜。",
        "from": "T19n0943_001:0099b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信受奉行作禮而退。",
        "from": "T19n0943_001:0099b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說無能勝幡莊嚴陀羅尼經",
        "from": "T19n0943_001:0099b22_0",
        "context_with_next_sentence": false
    }
]