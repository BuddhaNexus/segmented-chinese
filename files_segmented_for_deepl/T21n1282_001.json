[
    {
        "source_sentence": "寶藏天女陀羅尼法",
        "from": "T21n1282_001:0342c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時三十三天主摩醯首羅天王。",
        "from": "T21n1282_001:0342c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共諸天女眾讚寶藏天王名吒羅佉。",
        "from": "T21n1282_001:0342c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大威德色力無比能於世間行殊勝事。",
        "from": "T21n1282_001:0342c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迴天動地傾山覆海。",
        "from": "T21n1282_001:0342c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兵戈勝負志意而成。",
        "from": "T21n1282_001:0342c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "財帛金寶積如山岳。",
        "from": "T21n1282_001:0342c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間受用。",
        "from": "T21n1282_001:0342c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1282_001:0342c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵吒羅佉吒羅佉毘多羅儞莎嚩訶",
        "from": "T21n1282_001:0342c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲受持此法。",
        "from": "T21n1282_001:0342c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先畫一天女形。",
        "from": "T21n1282_001:0342c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "泥檀法五月五日前七日。",
        "from": "T21n1282_001:0342c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喫希粥長齋。",
        "from": "T21n1282_001:0342c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "意欲去處須淨潔也。",
        "from": "T21n1282_001:0342c27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡行此法天女不喚自來。",
        "from": "T21n1282_001:0342c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然須思存用表至誠。",
        "from": "T21n1282_001:0342c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此法必須五月五日夜受持即果成就。",
        "from": "T21n1282_001:0342c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五月五日夜。",
        "from": "T21n1282_001:0343a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "注想思存換換誦呪一千八遍。",
        "from": "T21n1282_001:0343a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得急。",
        "from": "T21n1282_001:0343a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至三更即心昏似睡。",
        "from": "T21n1282_001:0343a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天女使諸女來令相試弄。",
        "from": "T21n1282_001:0343a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必莫共語。",
        "from": "T21n1282_001:0343a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至第二遍。",
        "from": "T21n1282_001:0343a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其天女吒羅佉自來相見。",
        "from": "T21n1282_001:0343a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其作法人不得赴。",
        "from": "T21n1282_001:0343a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便[跍*月]跪合掌頂禮兩拜。",
        "from": "T21n1282_001:0343a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即相問之有何所須。",
        "from": "T21n1282_001:0343a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其作法人隨口答之。",
        "from": "T21n1282_001:0343a05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乞好藥長生不死通靈。",
        "from": "T21n1282_001:0343a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間萬事之情。",
        "from": "T21n1282_001:0343a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終身奉事。",
        "from": "T21n1282_001:0343a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天女意生欲為夫妻。",
        "from": "T21n1282_001:0343a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天女聞已甚大歡喜。",
        "from": "T21n1282_001:0343a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當夜便與交諸願如意。",
        "from": "T21n1282_001:0343a08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲作姊妹。",
        "from": "T21n1282_001:0343a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於事必須分明勿生狐疑。",
        "from": "T21n1282_001:0343a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其壇法二肘半量用牛糞泥地。",
        "from": "T21n1282_001:0343a09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更以香泥塗地。",
        "from": "T21n1282_001:0343a10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五色彩畫又五色綵各二尺一寸。",
        "from": "T21n1282_001:0343a10_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在壇內安天女座。",
        "from": "T21n1282_001:0343a11_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "酥密乳等菓子各一盤。",
        "from": "T21n1282_001:0343a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五色花餅各五枚。",
        "from": "T21n1282_001:0343a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已雜栗麥燒花隨意供養。",
        "from": "T21n1282_001:0343a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其畫像法天女身長二尺五寸。",
        "from": "T21n1282_001:0343a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭作花冠所點花極妙端正。",
        "from": "T21n1282_001:0343a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身著紫袍金帶烏靴。",
        "from": "T21n1282_001:0343a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手把蓮花。",
        "from": "T21n1282_001:0343a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手把如意寶珠。",
        "from": "T21n1282_001:0343a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其天女端正光明。",
        "from": "T21n1282_001:0343a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間畫師無能畫者。",
        "from": "T21n1282_001:0343a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必須好手不得論價。",
        "from": "T21n1282_001:0343a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應時通靈心神自在。",
        "from": "T21n1282_001:0343a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "財帛盈溢官位高遷。",
        "from": "T21n1282_001:0343a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中一切禍福了達。",
        "from": "T21n1282_001:0343a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貴賤男女皆悉歡喜。",
        "from": "T21n1282_001:0343a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡有所動一切鬼神皆來迎待如敬天官。",
        "from": "T21n1282_001:0343a19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悚懼瞻仰便得無礙。",
        "from": "T21n1282_001:0343a20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至世間好妙藝隨意而成。",
        "from": "T21n1282_001:0343a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有惡來相害者。",
        "from": "T21n1282_001:0343a21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便呼賊各皆得自縛自打。",
        "from": "T21n1282_001:0343a21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言放即放世間走失死亡如驗。",
        "from": "T21n1282_001:0343a22_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目前萬病舉意持之皆差。",
        "from": "T21n1282_001:0343a22_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又八真言各須清淨沐浴著新淨衣。",
        "from": "T21n1282_001:0343a23_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於佛前形像前各誦三七遍。",
        "from": "T21n1282_001:0343a24_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後作法。",
        "from": "T21n1282_001:0343a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一真言",
        "from": "T21n1282_001:0343a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵興婆羅帝吒急急娑嚩(二合)賀",
        "from": "T21n1282_001:0343a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二真言",
        "from": "T21n1282_001:0343a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵拔羅帝吒唎摩那羅娑嚩賀",
        "from": "T21n1282_001:0343a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三真言",
        "from": "T21n1282_001:0343b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵勒叉那帝吒娑吒娑嚩(二合)賀",
        "from": "T21n1282_001:0343b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四真言",
        "from": "T21n1282_001:0343b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵婆婆隷若帝吒娑嚩(二合)賀",
        "from": "T21n1282_001:0343b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五真言",
        "from": "T21n1282_001:0343b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵勃惹羅帝吒娑嚩(二合)賀",
        "from": "T21n1282_001:0343b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六真言",
        "from": "T21n1282_001:0343b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵藥叉藥叉底那婆娑嚩(二合)賀",
        "from": "T21n1282_001:0343b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第七真言",
        "from": "T21n1282_001:0343b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵遮帝毘折曳吽泮吒娑嚩(二合)賀第八真言唵折勒那耶利婆帝吒娑嚩(二合)賀以中指無名指屈。",
        "from": "T21n1282_001:0343b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大母指押二指甲。",
        "from": "T21n1282_001:0343b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舒頭指及小指安心上。",
        "from": "T21n1282_001:0343b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手把數珠念真言。",
        "from": "T21n1282_001:0343b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此名玉環印。",
        "from": "T21n1282_001:0343b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶藏天女法竟",
        "from": "T21n1282_001:0343b16_0",
        "context_with_next_sentence": false
    }
]