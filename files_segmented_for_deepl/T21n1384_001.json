        "from": "T21n1384_001:0904c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得聞世尊說此陀羅尼已。",
        "from": "T21n1384_001:0905a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白言世尊願勿有慮。",
        "from": "T21n1384_001:0905a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當憶持宣流於世。",
        "from": "T21n1384_001:0905a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使諸眾生獲大善利。",
        "from": "T21n1384_001:0905a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是語已即用頭頂。",
        "from": "T21n1384_001:0905b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮世尊足歡喜而退。",
        "from": "T21n1384_001:0905b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說鉢蘭那賒嚩哩大陀羅尼經",
        "from": "T21n1384_001:0905b03_0",
        "context_with_next_sentence": false
    }
]