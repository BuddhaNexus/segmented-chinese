[
    {
        "source_sentence": "佛說鉢蘭那賖嚩哩大陀羅",
        "from": "T21n1384_001:0904c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尼經",
        "from": "T21n1384_001:0904c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試光祿卿",
        "from": "T21n1384_001:0904c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明教大師臣法賢奉　詔譯",
        "from": "T21n1384_001:0904c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告阿難言。",
        "from": "T21n1384_001:0904c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我觀世間無量眾生。",
        "from": "T21n1384_001:0904c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多造罪業墮於惡趣。",
        "from": "T21n1384_001:0904c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經無量時方得出離。",
        "from": "T21n1384_001:0904c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縱生為人身肢不具。",
        "from": "T21n1384_001:0904c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設得完具薄福少慧。",
        "from": "T21n1384_001:0904c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常為惡魔及惡鬼神。",
        "from": "T21n1384_001:0904c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於晝夜中伺得其便種種惱害不得安隱。",
        "from": "T21n1384_001:0904c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我愍斯等與說鉢蘭那賒嚩哩大陀羅尼。",
        "from": "T21n1384_001:0904c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辟除惡魔及惡鬼神。",
        "from": "T21n1384_001:0904c13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生得聞此陀羅尼。",
        "from": "T21n1384_001:0904c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發至誠心受持讀誦供養恭敬。",
        "from": "T21n1384_001:0904c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼惡魔及惡鬼神悉皆遠離。",
        "from": "T21n1384_001:0904c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "災害消除無諸疾疫。",
        "from": "T21n1384_001:0904c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所住之處安隱快樂。",
        "from": "T21n1384_001:0904c16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊即說陀羅尼曰。",
        "from": "T21n1384_001:0904c16_15",
        "context_with_next_sentence": true
    },
