[
    {
        "source_sentence": " 彌陀舌相",
        "from": "X37n0674_008:0771b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西溪俍亭挺　著",
        "from": "X37n0674_008:0771b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同學　吳山濤",
        "from": "X37n0674_008:0771b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　戴斑立　仝閱",
        "from": "X37n0674_008:0771b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "X37n0674_008:0771b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來說法四眾臨筵。",
        "from": "X37n0674_008:0771b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為甚麼彌陀會中比丘尼優婆夷以及人非人等一齊不到。",
        "from": "X37n0674_008:0771b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "X37n0674_008:0771b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成佛久矣。",
        "from": "X37n0674_008:0771b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌。",
        "from": "X37n0674_008:0771b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清淨從來無女人。",
        "from": "X37n0674_008:0771b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畜生那敢並追尋。",
        "from": "X37n0674_008:0771b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獨憐共命頻伽鳥。",
        "from": "X37n0674_008:0771b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "古佛光中再現身。",
        "from": "X37n0674_008:0771b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "X37n0674_008:0771b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迢迢十萬乃是西方。",
        "from": "X37n0674_008:0771b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為甚又說箇惟心淨土。",
        "from": "X37n0674_008:0771b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "X37n0674_008:0771b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不信長安在日邊。",
        "from": "X37n0674_008:0771b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌。",
        "from": "X37n0674_008:0771b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白鷺池邊半畝居。",
        "from": "X37n0674_008:0771b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "綠楊兩岸間芙蕖。",
        "from": "X37n0674_008:0771b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怪他無限癡男女。",
        "from": "X37n0674_008:0771b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奔走迢迢十萬餘。",
        "from": "X37n0674_008:0771b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "X37n0674_008:0771b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彌陀佛今現在是果然麼。",
        "from": "X37n0674_008:0771b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "X37n0674_008:0771b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "錯過不少。",
        "from": "X37n0674_008:0771b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌。",
        "from": "X37n0674_008:0771b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "古佛過去久矣。",
        "from": "X37n0674_008:0771b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "快把淨瓶扶起。",
        "from": "X37n0674_008:0771b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分明渡水逢渠。",
        "from": "X37n0674_008:0771b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "原來是姓。",
        "from": "X37n0674_008:0771b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "張的老李。",
        "from": "X37n0674_008:0771b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "X37n0674_008:0771b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "極樂國土清淨有餘。",
        "from": "X37n0674_008:0771b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說什麼硨磲碼瑪。",
        "from": "X37n0674_008:0771b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "X37n0674_008:0771b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "暴富貧兒自古多。",
        "from": "X37n0674_008:0771b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌。",
        "from": "X37n0674_008:0771b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋迦貧迦葉富。",
        "from": "X37n0674_008:0771b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玉泉皓前世不修。",
        "from": "X37n0674_008:0771b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "打倒了葡萄棚。",
        "from": "X37n0674_008:0771b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "留得一條布褲。",
        "from": "X37n0674_008:0771b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿逸多阿逸多。",
        "from": "X37n0674_008:0771b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "太無厭生。",
        "from": "X37n0674_008:0771b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "討個大光錢認主顧。",
        "from": "X37n0674_008:0771b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "X37n0674_008:0771b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東方不動地瑠璃光菩薩。",
        "from": "X37n0674_008:0771b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦與西方無勝世界安養一般。",
        "from": "X37n0674_008:0771b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為甚麼落日懸鼓耑以生西為念。",
        "from": "X37n0674_008:0771c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "X37n0674_008:0771c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "切忌指東話西。",
        "from": "X37n0674_008:0771c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌。",
        "from": "X37n0674_008:0771c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一為遷客去長沙。",
        "from": "X37n0674_008:0771c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西望長安不見家。",
        "from": "X37n0674_008:0771c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黃鶴樓中吹玉笛。",
        "from": "X37n0674_008:0771c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "江城五月落梅花。",
        "from": "X37n0674_008:0771c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "X37n0674_008:0771c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙華供養遍往十方。",
        "from": "X37n0674_008:0771c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仍以須臾還至本國。",
        "from": "X37n0674_008:0771c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫是神足通麼。",
        "from": "X37n0674_008:0771c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "X37n0674_008:0771c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不許夜行投明須到。",
        "from": "X37n0674_008:0771c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌。",
        "from": "X37n0674_008:0771c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "驢兒三脚弄蹄行。",
        "from": "X37n0674_008:0771c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "木馬臨風吼幾群。",
        "from": "X37n0674_008:0771c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無足鐵牛連夜走。",
        "from": "X37n0674_008:0771c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "看煞區區沒眼人。",
        "from": "X37n0674_008:0771c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "X37n0674_008:0771c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孔雀鸚鵡演暢法音。",
        "from": "X37n0674_008:0771c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喚他作畜生得麼。",
        "from": "X37n0674_008:0771c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "X37n0674_008:0771c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖行畜生行不得畜生報。",
        "from": "X37n0674_008:0771c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌。",
        "from": "X37n0674_008:0771c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燕子飛燕子飛。",
        "from": "X37n0674_008:0771c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何處黃鶯帶雨啼。",
        "from": "X37n0674_008:0771c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更杜鵑流血。",
        "from": "X37n0674_008:0771c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雙雙瓦雀踏破柔荑。",
        "from": "X37n0674_008:0771c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "X37n0674_008:0771c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶樹出音使人念佛。",
        "from": "X37n0674_008:0771c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無情說法在在有之。",
        "from": "X37n0674_008:0771c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是果然否。",
        "from": "X37n0674_008:0771c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "X37n0674_008:0771c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燈籠拍手笑呵呵。",
        "from": "X37n0674_008:0771c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌。",
        "from": "X37n0674_008:0771c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燈籠拍手。",
        "from": "X37n0674_008:0771c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "露柱開口。",
        "from": "X37n0674_008:0771c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八角磨盤空裏走。",
        "from": "X37n0674_008:0771c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喫不盡沽來酒。",
        "from": "X37n0674_008:0771c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說甚麼趙州無常州有。",
        "from": "X37n0674_008:0771c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "切忌切忌播揚家醜。",
        "from": "X37n0674_008:0771c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "X37n0674_008:0771c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人民國土無量僧祇成佛以來今止十劫。",
        "from": "X37n0674_008:0771c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然則十劫以前當不得名為極樂也。",
        "from": "X37n0674_008:0771c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "X37n0674_008:0771c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "春風是處馬蹄香。",
        "from": "X37n0674_008:0771c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌。",
        "from": "X37n0674_008:0771c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量阿僧祇劫。",
        "from": "X37n0674_008:0771c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怪道不生不滅。",
        "from": "X37n0674_008:0771c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東風搖曳柳絲長。",
        "from": "X37n0674_008:0771c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三箇烏龜證箇鱉。",
        "from": "X37n0674_008:0771c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果然是秤錘嚼鐵。",
        "from": "X37n0674_008:0771c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "X37n0674_008:0771c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "補處稱尊。",
        "from": "X37n0674_008:0771c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獨許一生授記。",
        "from": "X37n0674_008:0771c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為甚麼有餘方便令他凡聖同居。",
        "from": "X37n0674_008:0771c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "X37n0674_008:0771c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未容揀擇。",
        "from": "X37n0674_008:0771c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌。",
        "from": "X37n0674_008:0771c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一箇兩箇千萬箇。",
        "from": "X37n0674_008:0771c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "海壇馬子似驢大。",
        "from": "X37n0674_008:0771c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烏梅結網打蜻蜓。",
        "from": "X37n0674_008:0771c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師姑原是女人做。",
        "from": "X37n0674_008:0771c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "X37n0674_008:0772a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不到西方必愁退轉。",
        "from": "X37n0674_008:0772a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋迦彌勒何為睹吏陀天。",
        "from": "X37n0674_008:0772a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "X37n0674_008:0772a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "珊瑚枝枝撐著月。",
        "from": "X37n0674_008:0772a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌。",
        "from": "X37n0674_008:0772a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "溪西鷄齊啼。",
        "from": "X37n0674_008:0772a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屋北鹿獨宿。",
        "from": "X37n0674_008:0772a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不是上天堂。",
        "from": "X37n0674_008:0772a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便是落地獄。",
        "from": "X37n0674_008:0772a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雪山六載何曾出家。",
        "from": "X37n0674_008:0772a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少室九年也未離俗。",
        "from": "X37n0674_008:0772a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "X37n0674_008:0772a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心不亂即得往生。",
        "from": "X37n0674_008:0772a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "古人道生則必定生。",
        "from": "X37n0674_008:0772a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "去則實不去。",
        "from": "X37n0674_008:0772a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是否。",
        "from": "X37n0674_008:0772a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "X37n0674_008:0772a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到頭雲定不離山。",
        "from": "X37n0674_008:0772a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌。",
        "from": "X37n0674_008:0772a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天地只爾戶庭已。",
        "from": "X37n0674_008:0772a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悠世與我以相遺。",
        "from": "X37n0674_008:0772a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復駕言兮焉求。",
        "from": "X37n0674_008:0772a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "老僧百年後。",
        "from": "X37n0674_008:0772a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "山前檀越家變一頭水牯牛。",
        "from": "X37n0674_008:0772a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "X37n0674_008:0772a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東方舌相遍覆大千。",
        "from": "X37n0674_008:0772a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且問南西北方上下再來作何安置。",
        "from": "X37n0674_008:0772a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "X37n0674_008:0772a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如影投影。",
        "from": "X37n0674_008:0772a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌。",
        "from": "X37n0674_008:0772a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不是心。",
        "from": "X37n0674_008:0772a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不是佛。",
        "from": "X37n0674_008:0772a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不是物。",
        "from": "X37n0674_008:0772a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "古往今來窮不徹。",
        "from": "X37n0674_008:0772a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "韶陽老得一橛。",
        "from": "X37n0674_008:0772a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擬思量沒交涉。",
        "from": "X37n0674_008:0772a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昨夜門前流水聲。",
        "from": "X37n0674_008:0772a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "癡人喚作廣長舌。",
        "from": "X37n0674_008:0772a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "X37n0674_008:0772a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸佛同知護念。",
        "from": "X37n0674_008:0772a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為甚麼觀音勢至獨號當機。",
        "from": "X37n0674_008:0772a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "X37n0674_008:0772a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冤有頭債有主。",
        "from": "X37n0674_008:0772a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌。",
        "from": "X37n0674_008:0772a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "搯數珠修行下坡不走。",
        "from": "X37n0674_008:0772a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晝夜六時披枷帶肘。",
        "from": "X37n0674_008:0772a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一笑歸來何所有。",
        "from": "X37n0674_008:0772a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三三成一九。",
        "from": "X37n0674_008:0772a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩個黃鸝鳴翠柳。",
        "from": "X37n0674_008:0772a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "X37n0674_008:0772a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "子憶母母憶子。",
        "from": "X37n0674_008:0772a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "啐啄同時。",
        "from": "X37n0674_008:0772a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為甚麼又道作家不啐啄。",
        "from": "X37n0674_008:0772a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "X37n0674_008:0772a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真誠不是小兒嬉。",
        "from": "X37n0674_008:0772a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌。",
        "from": "X37n0674_008:0772a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "子啐母啄。",
        "from": "X37n0674_008:0772a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母覺子鷇。",
        "from": "X37n0674_008:0772a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀音彌勒太煞婆心。",
        "from": "X37n0674_008:0772a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牛頭夜义寒威卓朔。",
        "from": "X37n0674_008:0772a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咄咄老鼠哥哥入牛角。",
        "from": "X37n0674_008:0772a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問。",
        "from": "X37n0674_008:0772a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娑婆教主能忍能行。",
        "from": "X37n0674_008:0772a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與彼阿彌陀如來。",
        "from": "X37n0674_008:0772a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畢竟誰君誰臣誰賓誰主。",
        "from": "X37n0674_008:0772a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "X37n0674_008:0772a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向時奉奉時向。",
        "from": "X37n0674_008:0772a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌。",
        "from": "X37n0674_008:0772a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娑婆極樂教中王。",
        "from": "X37n0674_008:0772a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此土他方任主張。",
        "from": "X37n0674_008:0772a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但得主賓齊一致。",
        "from": "X37n0674_008:0772a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫教行旅往來忙。",
        "from": "X37n0674_008:0772b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彌陀舌相(終)",
        "from": "X37n0674_008:0772b03_0",
        "context_with_next_sentence": false
    }
]