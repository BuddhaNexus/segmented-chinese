[
    {
        "source_sentence": "大般涅槃經集解卷第四十九",
        "from": "T37n1763_049:0526c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　釋大涅槃小涅槃義。",
        "from": "T37n1763_049:0526c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉有山大山等喻　釋聲聞習氣義　釋有因緣名及無因緣名義　釋五神通義　辨二種神通。",
        "from": "T37n1763_049:0526c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一與外道共。",
        "from": "T37n1763_049:0526c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一與二乘共　辨涅槃非音聲之音德王品之第五爾時光明遍照高貴德王(至)云何涅槃云何大涅槃。",
        "from": "T37n1763_049:0526c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_049:0526c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_049:0526c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以重問者。",
        "from": "T37n1763_049:0526c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖說斷漏。",
        "from": "T37n1763_049:0526c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能得涅槃常樂我淨。",
        "from": "T37n1763_049:0526c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不說我樂自在之用。",
        "from": "T37n1763_049:0526c17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "涅槃之義未障故也。",
        "from": "T37n1763_049:0526c17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰。",
        "from": "T37n1763_049:0526c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次下答德王第二問也。",
        "from": "T37n1763_049:0526c18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上聞不聞中。",
        "from": "T37n1763_049:0526c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛答六難竟。",
        "from": "T37n1763_049:0526c19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更設二問。",
        "from": "T37n1763_049:0526c19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛答一問竟。",
        "from": "T37n1763_049:0526c19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今更舉第二問。",
        "from": "T37n1763_049:0526c20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以請釋也。",
        "from": "T37n1763_049:0526c20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛讚光明遍照(至)是名涅槃非大涅槃。",
        "from": "T37n1763_049:0526c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_049:0526c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_049:0526c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "涅槃雖有多義。",
        "from": "T37n1763_049:0526c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "會是無苦之稱。",
        "from": "T37n1763_049:0526c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得息苦者。",
        "from": "T37n1763_049:0526c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆得名焉。",
        "from": "T37n1763_049:0526c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰。",
        "from": "T37n1763_049:0526c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今借十譬。",
        "from": "T37n1763_049:0526c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以辨大小之相異。",
        "from": "T37n1763_049:0526c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正以無苦樂。",
        "from": "T37n1763_049:0526c24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名涅槃也。",
        "from": "T37n1763_049:0526c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若得少食。",
        "from": "T37n1763_049:0526c24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以除飢苦。",
        "from": "T37n1763_049:0526c25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名涅槃。",
        "from": "T37n1763_049:0526c25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但不得名大涅槃。",
        "from": "T37n1763_049:0526c25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是十種譬。",
        "from": "T37n1763_049:0526c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其義一例。",
        "from": "T37n1763_049:0526c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小海亦名海。",
        "from": "T37n1763_049:0526c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大海亦名海。",
        "from": "T37n1763_049:0526c26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若除少苦。",
        "from": "T37n1763_049:0526c27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名小涅槃。",
        "from": "T37n1763_049:0526c27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若除苦都盡。",
        "from": "T37n1763_049:0526c27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名大涅槃。",
        "from": "T37n1763_049:0526c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其文中自廣彰云。",
        "from": "T37n1763_049:0526c28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二乘人八萬劫。",
        "from": "T37n1763_049:0526c28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至一萬劫。",
        "from": "T37n1763_049:0526c28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名小涅槃者。",
        "from": "T37n1763_049:0526c29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上乘三處。",
        "from": "T37n1763_049:0526c29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明皆言到佛果。",
        "from": "T37n1763_049:0526c29_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此中說名小涅槃。",
        "from": "T37n1763_049:0527a01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其義云何。",
        "from": "T37n1763_049:0527a01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲於我前。",
        "from": "T37n1763_049:0527a01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不言行八萬劫竟名小。",
        "from": "T37n1763_049:0527a02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未行八萬劫之前。",
        "from": "T37n1763_049:0527a02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨其所得住處。",
        "from": "T37n1763_049:0527a03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此義為小。",
        "from": "T37n1763_049:0527a03_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及其行八萬劫竟得佛。",
        "from": "T37n1763_049:0527a03_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由名大涅槃。",
        "from": "T37n1763_049:0527a04_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而此旨於一部之中。",
        "from": "T37n1763_049:0527a04_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖復說異。",
        "from": "T37n1763_049:0527a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦莫測其然。",
        "from": "T37n1763_049:0527a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要隨義而安也。",
        "from": "T37n1763_049:0527a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子若凡夫人及以聲聞(至)乃得名為大涅槃也。",
        "from": "T37n1763_049:0527a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_049:0527a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_049:0527a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡夫因世俗。",
        "from": "T37n1763_049:0527a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聲聞因聖道者。",
        "from": "T37n1763_049:0527a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更釋非之義也。",
        "from": "T37n1763_049:0527a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還生煩惱者。",
        "from": "T37n1763_049:0527a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋凡夫也。",
        "from": "T37n1763_049:0527a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有習氣者。",
        "from": "T37n1763_049:0527a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋聲聞也。",
        "from": "T37n1763_049:0527a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子譬如有處能受眾流(至)之所得故故名為大。",
        "from": "T37n1763_049:0527a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_049:0527a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_049:0527a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前說人實。",
        "from": "T37n1763_049:0527a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後說法實。",
        "from": "T37n1763_049:0527a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合海譬也。",
        "from": "T37n1763_049:0527a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "河城等皆合也。",
        "from": "T37n1763_049:0527a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十譬之中。",
        "from": "T37n1763_049:0527a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯不合地及天中天二譬也。",
        "from": "T37n1763_049:0527a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何復名為大涅槃(至)以是義故名大涅槃。",
        "from": "T37n1763_049:0527a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_049:0527a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_049:0527a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八自在者。",
        "from": "T37n1763_049:0527a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各有文也。",
        "from": "T37n1763_049:0527a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者不小能小。",
        "from": "T37n1763_049:0527a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不多能多也。",
        "from": "T37n1763_049:0527a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者以小為大也。",
        "from": "T37n1763_049:0527a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者以重為輕也。",
        "from": "T37n1763_049:0527a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者以三事體。",
        "from": "T37n1763_049:0527a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心色不移。",
        "from": "T37n1763_049:0527a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無處不至。",
        "from": "T37n1763_049:0527a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所作事一。",
        "from": "T37n1763_049:0527a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而見者不同也。",
        "from": "T37n1763_049:0527a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者一根知六塵也。",
        "from": "T37n1763_049:0527a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六者見法如空也。",
        "from": "T37n1763_049:0527a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七者得一切法也。",
        "from": "T37n1763_049:0527a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八者不可見。",
        "from": "T37n1763_049:0527a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令一切見。",
        "from": "T37n1763_049:0527a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次善男子譬如寶藏(至)具足成就初分功德。",
        "from": "T37n1763_049:0527a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_049:0527a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_049:0527a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無為名大。",
        "from": "T37n1763_049:0527a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋常住故。",
        "from": "T37n1763_049:0527a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故稱大也。",
        "from": "T37n1763_049:0527a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次善男子云何菩薩(至)昔所不得而今乃得。",
        "from": "T37n1763_049:0527a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_049:0527a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_049:0527a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸漏既盡。",
        "from": "T37n1763_049:0527a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則具神通。",
        "from": "T37n1763_049:0527a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆聖行功德也。",
        "from": "T37n1763_049:0527a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今見者。",
        "from": "T37n1763_049:0527a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂眼通。",
        "from": "T37n1763_049:0527a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更就所得中分別也。",
        "from": "T37n1763_049:0527a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今聞者。",
        "from": "T37n1763_049:0527a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耳通也。",
        "from": "T37n1763_049:0527a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今到者。",
        "from": "T37n1763_049:0527a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如意通也。",
        "from": "T37n1763_049:0527a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今知者。",
        "from": "T37n1763_049:0527a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他心宿命也。",
        "from": "T37n1763_049:0527a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰。",
        "from": "T37n1763_049:0527a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二功德。",
        "from": "T37n1763_049:0527a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以五通為體。",
        "from": "T37n1763_049:0527a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此五是一方之所須。",
        "from": "T37n1763_049:0527a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩化道之要用。",
        "from": "T37n1763_049:0527a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一修天眼通。",
        "from": "T37n1763_049:0527a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須見眾生身業。",
        "from": "T37n1763_049:0527b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二修天耳通。",
        "from": "T37n1763_049:0527b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞其口業。",
        "from": "T37n1763_049:0527b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三修他心通。",
        "from": "T37n1763_049:0527b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知其心業。",
        "from": "T37n1763_049:0527b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖知三業。",
        "from": "T37n1763_049:0527b02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不達其過去。",
        "from": "T37n1763_049:0527b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有善惡之厚薄可化。",
        "from": "T37n1763_049:0527b03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦於化道有闕。",
        "from": "T37n1763_049:0527b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故次修宿命通。",
        "from": "T37n1763_049:0527b04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而下愚之流。",
        "from": "T37n1763_049:0527b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難以玄言。",
        "from": "T37n1763_049:0527b04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必須運身到彼。",
        "from": "T37n1763_049:0527b04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後受化。",
        "from": "T37n1763_049:0527b05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故第五次修身通。",
        "from": "T37n1763_049:0527b05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今就文所辨。",
        "from": "T37n1763_049:0527b05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先明身通。",
        "from": "T37n1763_049:0527b06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔明身通。",
        "from": "T37n1763_049:0527b06_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直據色身轉變而已。",
        "from": "T37n1763_049:0527b06_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今談色心自在。",
        "from": "T37n1763_049:0527b07_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修短所通。",
        "from": "T37n1763_049:0527b07_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "適物所宜。",
        "from": "T37n1763_049:0527b07_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而經亦有言如意通。",
        "from": "T37n1763_049:0527b08_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名身通。",
        "from": "T37n1763_049:0527b08_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若立身通。",
        "from": "T37n1763_049:0527b08_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則沒如意名言。",
        "from": "T37n1763_049:0527b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若立如意。",
        "from": "T37n1763_049:0527b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便廢身通。",
        "from": "T37n1763_049:0527b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若如昔教明五通。",
        "from": "T37n1763_049:0527b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是禪定家果。",
        "from": "T37n1763_049:0527b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要因定後。",
        "from": "T37n1763_049:0527b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方能發通。",
        "from": "T37n1763_049:0527b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就今涅槃經明五通。",
        "from": "T37n1763_049:0527b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡是智慧之異用。",
        "from": "T37n1763_049:0527b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故菩薩摩訶薩。",
        "from": "T37n1763_049:0527b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依六行教學。",
        "from": "T37n1763_049:0527b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不取有無相。",
        "from": "T37n1763_049:0527b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但使物感稱根而應。",
        "from": "T37n1763_049:0527b12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如一家所解。",
        "from": "T37n1763_049:0527b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此五通必是菩薩家化。",
        "from": "T37n1763_049:0527b13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方非外道凡夫聲聞之伎。",
        "from": "T37n1763_049:0527b14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以知耶。",
        "from": "T37n1763_049:0527b14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若實是凡夫。",
        "from": "T37n1763_049:0527b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何得有此法。",
        "from": "T37n1763_049:0527b15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而在外道中唱言有者。",
        "from": "T37n1763_049:0527b15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆是應迹。",
        "from": "T37n1763_049:0527b16_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外道雖言得禪定。",
        "from": "T37n1763_049:0527b16_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而無有道思慧。",
        "from": "T37n1763_049:0527b16_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以無此神德也。",
        "from": "T37n1763_049:0527b17_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聲聞人觀解既不成。",
        "from": "T37n1763_049:0527b17_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復不能即在定。",
        "from": "T37n1763_049:0527b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而變通亦由已安。",
        "from": "T37n1763_049:0527b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法不得所。",
        "from": "T37n1763_049:0527b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復未能忘懷。",
        "from": "T37n1763_049:0527b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故發家唱云。",
        "from": "T37n1763_049:0527b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩所得通。",
        "from": "T37n1763_049:0527b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不與外道聲聞二乘等共之。",
        "from": "T37n1763_049:0527b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通有二種一者內二者外(至)昔所不得而今得之。",
        "from": "T37n1763_049:0527b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_049:0527b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_049:0527b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不與外道。",
        "from": "T37n1763_049:0527b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共五通也。",
        "from": "T37n1763_049:0527b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心作一者。",
        "from": "T37n1763_049:0527b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋不與二乘及外道共。",
        "from": "T37n1763_049:0527b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但舉二乘外道可知也。",
        "from": "T37n1763_049:0527b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰。",
        "from": "T37n1763_049:0527b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖言二乘人。",
        "from": "T37n1763_049:0527b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心作一。",
        "from": "T37n1763_049:0527b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得眾多。",
        "from": "T37n1763_049:0527b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此正表二乘不得道之意。",
        "from": "T37n1763_049:0527b25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聲聞入定發通。",
        "from": "T37n1763_049:0527b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要須定前作意。",
        "from": "T37n1763_049:0527b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作意若一。",
        "from": "T37n1763_049:0527b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在定唯能一變。",
        "from": "T37n1763_049:0527b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作意作五事。",
        "from": "T37n1763_049:0527b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便能現五變。",
        "from": "T37n1763_049:0527b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖在定。",
        "from": "T37n1763_049:0527b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "併發五耳。",
        "from": "T37n1763_049:0527b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由得言一心作一。",
        "from": "T37n1763_049:0527b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以然。",
        "from": "T37n1763_049:0527b28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由其前方便有五故。",
        "from": "T37n1763_049:0527b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若極利根者。",
        "from": "T37n1763_049:0527b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在定亦容可得二變。",
        "from": "T37n1763_049:0527b29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此皆據教迹作。",
        "from": "T37n1763_049:0527b29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸菩薩則不然。",
        "from": "T37n1763_049:0527c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發輒修德。",
        "from": "T37n1763_049:0527c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以曠被為懷。",
        "from": "T37n1763_049:0527c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及至物感。",
        "from": "T37n1763_049:0527c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則稱本心。",
        "from": "T37n1763_049:0527c02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨力而現。",
        "from": "T37n1763_049:0527c02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不假興意。",
        "from": "T37n1763_049:0527c02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無六行解者。",
        "from": "T37n1763_049:0527c03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則不能一心現五通身也。",
        "from": "T37n1763_049:0527c03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復云何昔所不得(至)昔所不得而今得之。",
        "from": "T37n1763_049:0527c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_049:0527c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_049:0527c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如意說名自在。",
        "from": "T37n1763_049:0527c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於自在中。",
        "from": "T37n1763_049:0527c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分作變化。",
        "from": "T37n1763_049:0527c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初對凡夫。",
        "from": "T37n1763_049:0527c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故言不爾。",
        "from": "T37n1763_049:0527c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次善男子菩薩摩訶薩(至)而諸眾生各各見異。",
        "from": "T37n1763_049:0527c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_049:0527c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_049:0527c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得異耳根者。",
        "from": "T37n1763_049:0527c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有三事異。",
        "from": "T37n1763_049:0527c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者下地聞土地。",
        "from": "T37n1763_049:0527c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二遠及恒沙。",
        "from": "T37n1763_049:0527c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三耳識不取相。",
        "from": "T37n1763_049:0527c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是不聞而聞。",
        "from": "T37n1763_049:0527c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初無音聲相者。",
        "from": "T37n1763_049:0527c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩耳識。",
        "from": "T37n1763_049:0527c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真俗兼見。",
        "from": "T37n1763_049:0527c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故不取相。",
        "from": "T37n1763_049:0527c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次善男子云何菩薩(至)昔所不聞而今得聞。",
        "from": "T37n1763_049:0527c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_049:0527c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_049:0527c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此下明天耳通。",
        "from": "T37n1763_049:0527c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而文中敘相。",
        "from": "T37n1763_049:0527c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆是初學之方法。",
        "from": "T37n1763_049:0527c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔小乘明天耳通。",
        "from": "T37n1763_049:0527c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耳識取聲。",
        "from": "T37n1763_049:0527c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後生智。",
        "from": "T37n1763_049:0527c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二心更相資導。",
        "from": "T37n1763_049:0527c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故合二心為通。",
        "from": "T37n1763_049:0527c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天眼通亦爾。",
        "from": "T37n1763_049:0527c17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今用此經中。",
        "from": "T37n1763_049:0527c17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "學得天耳天眼。",
        "from": "T37n1763_049:0527c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至得通。",
        "from": "T37n1763_049:0527c18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六識之用。",
        "from": "T37n1763_049:0527c18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是智慧。",
        "from": "T37n1763_049:0527c18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故天眼通。",
        "from": "T37n1763_049:0527c18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即中作二諦觀。",
        "from": "T37n1763_049:0527c19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經既理教真正。",
        "from": "T37n1763_049:0527c19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明有為無為二理炳然。",
        "from": "T37n1763_049:0527c19_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心行大法。",
        "from": "T37n1763_049:0527c20_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生解亦大。",
        "from": "T37n1763_049:0527c20_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若始行之流。",
        "from": "T37n1763_049:0527c20_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則未及此也。",
        "from": "T37n1763_049:0527c21_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時光明遍照高貴德王(至)云何而言無定無果。",
        "from": "T37n1763_049:0527c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_049:0527c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_049:0527c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是定果相者。",
        "from": "T37n1763_049:0527c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作因定善。",
        "from": "T37n1763_049:0527c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是果相者。",
        "from": "T37n1763_049:0527c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從善因生。",
        "from": "T37n1763_049:0527c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是果也。",
        "from": "T37n1763_049:0527c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時如來讚言善哉善哉(至)凡所演說無定果相。",
        "from": "T37n1763_049:0527c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_049:0527c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_049:0527c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答意謂。",
        "from": "T37n1763_049:0527c26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若刀是定因。",
        "from": "T37n1763_049:0527c26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面是定果者。",
        "from": "T37n1763_049:0527c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何顛倒耶。",
        "from": "T37n1763_049:0527c27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子夫涅槃者實非聲果(至)吾當為汝更分別說。",
        "from": "T37n1763_049:0527c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_049:0527c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_049:0527c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上說一切因果不定。",
        "from": "T37n1763_049:0527c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此說涅槃常法非果。",
        "from": "T37n1763_049:0527c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子若有善男子(至)故得阿耨多羅三藐三菩提。",
        "from": "T37n1763_049:0528a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_049:0528a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_049:0528a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一字不作字相。",
        "from": "T37n1763_049:0528a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一句不作句相。",
        "from": "T37n1763_049:0528a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從經理門。",
        "from": "T37n1763_049:0528a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可說為聞因之成果。",
        "from": "T37n1763_049:0528a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不如是者。",
        "from": "T37n1763_049:0528a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則非聞也。",
        "from": "T37n1763_049:0528a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子如汝所言聞惡聲故(至)昔所不聞而今得聞。",
        "from": "T37n1763_049:0528a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_049:0528a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_049:0528a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "涅槃非果。",
        "from": "T37n1763_049:0528a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可如上說。",
        "from": "T37n1763_049:0528a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三惡是果。",
        "from": "T37n1763_049:0528a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聲是定因。",
        "from": "T37n1763_049:0528a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須更說之。",
        "from": "T37n1763_049:0528a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子云何昔所不見(至)昔所不見而今得見。",
        "from": "T37n1763_049:0528a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_049:0528a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_049:0528a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若依欲界四大者。",
        "from": "T37n1763_049:0528a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依欲界定。",
        "from": "T37n1763_049:0528a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修得天眼通。",
        "from": "T37n1763_049:0528a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉是骨相者。",
        "from": "T37n1763_049:0528a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天眼識能見不淨。",
        "from": "T37n1763_049:0528a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸法定相。",
        "from": "T37n1763_049:0528a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見因緣相者。",
        "from": "T37n1763_049:0528a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空有兩見也。",
        "from": "T37n1763_049:0528a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰。",
        "from": "T37n1763_049:0528a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此下明天眼通。",
        "from": "T37n1763_049:0528a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此明天眼通。",
        "from": "T37n1763_049:0528a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不但見色。",
        "from": "T37n1763_049:0528a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦見眾生善惡心。",
        "from": "T37n1763_049:0528a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至知第一義諦故。",
        "from": "T37n1763_049:0528a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以作知名說也。",
        "from": "T37n1763_049:0528a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見眾生所有色相(至)昔所不知而今得知。",
        "from": "T37n1763_049:0528a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_049:0528a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_049:0528a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知大小根者。",
        "from": "T37n1763_049:0528a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘色故知根也。",
        "from": "T37n1763_049:0528a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一觸衣知心者。",
        "from": "T37n1763_049:0528a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因身知根同。",
        "from": "T37n1763_049:0528a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘色知心。",
        "from": "T37n1763_049:0528a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義屬天眼。",
        "from": "T37n1763_049:0528a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故下知見兩名也。",
        "from": "T37n1763_049:0528a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次善男子云何昔所不知(至)昔所不知而今得知。",
        "from": "T37n1763_049:0528a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_049:0528a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_049:0528a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次明宿命通也。",
        "from": "T37n1763_049:0528a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次善男子云何昔所不知(至)具足成就第二功德。",
        "from": "T37n1763_049:0528a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_049:0528a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_049:0528a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須陀洹十六心者。",
        "from": "T37n1763_049:0528a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三諦由滅見也。",
        "from": "T37n1763_049:0528a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔見滅未徹。",
        "from": "T37n1763_049:0528a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三諦不明。",
        "from": "T37n1763_049:0528a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既已見滅。",
        "from": "T37n1763_049:0528a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍四諦起念現前。",
        "from": "T37n1763_049:0528a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其心甚遠。",
        "from": "T37n1763_049:0528a25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是果十六心也。",
        "from": "T37n1763_049:0528a25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰。",
        "from": "T37n1763_049:0528a25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下去次明他心通。",
        "from": "T37n1763_049:0528a26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知須陀洹初心。",
        "from": "T37n1763_049:0528a26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次第至十六心者。",
        "from": "T37n1763_049:0528a26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聲聞人欲知初心捉法不能得。",
        "from": "T37n1763_049:0528a27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若利根者。",
        "from": "T37n1763_049:0528a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要至第七心方知。",
        "from": "T37n1763_049:0528a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若鈍根者。",
        "from": "T37n1763_049:0528a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至第十六心方知。",
        "from": "T37n1763_049:0528a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩不爾。",
        "from": "T37n1763_049:0528a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十六心一一次第差別盡知也。",
        "from": "T37n1763_049:0528a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大般涅槃經集解卷第四十九",
        "from": "T37n1763_049:0528b02_0",
        "context_with_next_sentence": false
    }
]