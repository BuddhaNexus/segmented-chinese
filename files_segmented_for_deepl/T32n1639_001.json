[
    {
        "source_sentence": "提婆菩薩破楞伽經中外道小乘",
        "from": "T32n1639_001:0155a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四宗論",
        "from": "T32n1639_001:0155a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提波菩薩造",
        "from": "T32n1639_001:0155a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後魏北印度三藏菩提流支譯",
        "from": "T32n1639_001:0155a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1639_001:0155a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外道所立四宗法非佛法者何者是。",
        "from": "T32n1639_001:0155a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1639_001:0155a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂一異俱不俱。",
        "from": "T32n1639_001:0155a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1639_001:0155a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何言一異俱不俱。",
        "from": "T32n1639_001:0155a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1639_001:0155a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有諸外道言。",
        "from": "T32n1639_001:0155a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切法一。",
        "from": "T32n1639_001:0155a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有諸外道言。",
        "from": "T32n1639_001:0155a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切法異。",
        "from": "T32n1639_001:0155a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有諸外道言。",
        "from": "T32n1639_001:0155a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切法俱。",
        "from": "T32n1639_001:0155a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有諸外道言。",
        "from": "T32n1639_001:0155a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切法不俱。",
        "from": "T32n1639_001:0155a22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸外道於虛妄法中各各執著。",
        "from": "T32n1639_001:0155a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為實有物故。",
        "from": "T32n1639_001:0155a23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1639_001:0155a23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等外道說一切法一。",
        "from": "T32n1639_001:0155a23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1639_001:0155a24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言一切法一者。",
        "from": "T32n1639_001:0155a24_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外道僧佉論師說。",
        "from": "T32n1639_001:0155a24_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言一切法異者。",
        "from": "T32n1639_001:0155a25_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外道毘世師論師說。",
        "from": "T32n1639_001:0155a25_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言一切法俱者。",
        "from": "T32n1639_001:0155a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外道尼犍子論師說言一切法不俱者。",
        "from": "T32n1639_001:0155a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外道若提子論師說。",
        "from": "T32n1639_001:0155a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1639_001:0155a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何僧佉人說一切法一。",
        "from": "T32n1639_001:0155a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1639_001:0155a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧佉外道言。",
        "from": "T32n1639_001:0155a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我覺二法是一。",
        "from": "T32n1639_001:0155a28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故。",
        "from": "T32n1639_001:0155a29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二相差別不可得故。",
        "from": "T32n1639_001:0155a29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1639_001:0155a29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何二相差別不可得。",
        "from": "T32n1639_001:0155b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1639_001:0155b01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如牛馬異法。",
        "from": "T32n1639_001:0155b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二相差別可見可取。",
        "from": "T32n1639_001:0155b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言此是牛此是馬。",
        "from": "T32n1639_001:0155b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而我離覺我不可得。",
        "from": "T32n1639_001:0155b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離我覺不可得。",
        "from": "T32n1639_001:0155b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如我經中說。",
        "from": "T32n1639_001:0155b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我覺體相如火與熱。",
        "from": "T32n1639_001:0155b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二法差別不可得。",
        "from": "T32n1639_001:0155b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1639_001:0155b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何差別不可得。",
        "from": "T32n1639_001:0155b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1639_001:0155b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼法不可說異故。",
        "from": "T32n1639_001:0155b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如白疊不可說言此是白此是疊。",
        "from": "T32n1639_001:0155b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二法差別如白疊。",
        "from": "T32n1639_001:0155b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切法因果亦如是。",
        "from": "T32n1639_001:0155b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1639_001:0155b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何毘世師外道說一切法異。",
        "from": "T32n1639_001:0155b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1639_001:0155b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所言異者。",
        "from": "T32n1639_001:0155b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我與覺異。",
        "from": "T32n1639_001:0155b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故。",
        "from": "T32n1639_001:0155b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以說異法。",
        "from": "T32n1639_001:0155b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1639_001:0155b09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名說異法。",
        "from": "T32n1639_001:0155b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1639_001:0155b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如說此是白此是疊。",
        "from": "T32n1639_001:0155b10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是天德。",
        "from": "T32n1639_001:0155b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是天德疊。",
        "from": "T32n1639_001:0155b10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我與覺異亦如是。",
        "from": "T32n1639_001:0155b11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是我此是智故。",
        "from": "T32n1639_001:0155b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1639_001:0155b12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有何差別彼法不可說一。",
        "from": "T32n1639_001:0155b12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1639_001:0155b12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如白疊此是白此是疊。",
        "from": "T32n1639_001:0155b12_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是一切因果各異。",
        "from": "T32n1639_001:0155b13_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可說一故。",
        "from": "T32n1639_001:0155b13_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1639_001:0155b14_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何尼乾子說一切法俱。",
        "from": "T32n1639_001:0155b14_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1639_001:0155b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言一切法俱者。",
        "from": "T32n1639_001:0155b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如我與覺不可說一不可說異。",
        "from": "T32n1639_001:0155b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有異義可說一可說異故。",
        "from": "T32n1639_001:0155b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1639_001:0155b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何不一不異亦一亦異。",
        "from": "T32n1639_001:0155b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1639_001:0155b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如我與命用相有異方便異故言。",
        "from": "T32n1639_001:0155b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如貪瞋癡等得言有異。",
        "from": "T32n1639_001:0155b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如燈明得說言一得說言異。",
        "from": "T32n1639_001:0155b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以有此有彼無此無彼。",
        "from": "T32n1639_001:0155b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得言一。",
        "from": "T32n1639_001:0155b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燈異處明異處。",
        "from": "T32n1639_001:0155b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故得言異。",
        "from": "T32n1639_001:0155b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如燈明因果白疊一切法亦如是。",
        "from": "T32n1639_001:0155b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦得說一亦得說異。",
        "from": "T32n1639_001:0155b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故言俱也。",
        "from": "T32n1639_001:0155b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1639_001:0155b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何若提子外道說一切法不俱。",
        "from": "T32n1639_001:0155b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1639_001:0155b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不俱者。",
        "from": "T32n1639_001:0155b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂一切法不可說一不可說異。",
        "from": "T32n1639_001:0155b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二邊見過故。",
        "from": "T32n1639_001:0155b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以說一異俱論師等皆有過失故。",
        "from": "T32n1639_001:0155b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智者不立如是三法。",
        "from": "T32n1639_001:0155b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1639_001:0155b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何過失。",
        "from": "T32n1639_001:0155b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1639_001:0155b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若離白別無疊者。",
        "from": "T32n1639_001:0155b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白滅疊亦應滅。",
        "from": "T32n1639_001:0155b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若異白更有疊者。",
        "from": "T32n1639_001:0155b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應有疊非白有白非疊。",
        "from": "T32n1639_001:0155b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故一異俱等法我俱不立。",
        "from": "T32n1639_001:0155b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖然一異俱等一切法不可得言無。",
        "from": "T32n1639_001:0155b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1639_001:0155b29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此諸外道虛妄分別。",
        "from": "T32n1639_001:0155b29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是邪見相非是智相。",
        "from": "T32n1639_001:0155c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆是不善。",
        "from": "T32n1639_001:0155c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此義云何。",
        "from": "T32n1639_001:0155c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又一等法虛妄分別。",
        "from": "T32n1639_001:0155c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以不得言即彼法。",
        "from": "T32n1639_001:0155c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼法一不得言瓶。",
        "from": "T32n1639_001:0155c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瓶一以瓶即是瓶故亦不得異法。",
        "from": "T32n1639_001:0155c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "異法一以不得言瓶共。",
        "from": "T32n1639_001:0155c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疊一以瓶相異疊相異以異法離異法。",
        "from": "T32n1639_001:0155c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "異法不得一不得異。",
        "from": "T32n1639_001:0155c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以異法不成異法。",
        "from": "T32n1639_001:0155c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以異法不得言異法。",
        "from": "T32n1639_001:0155c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若二法說一說異。",
        "from": "T32n1639_001:0155c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼二法應說一應說異。",
        "from": "T32n1639_001:0155c07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不說一不說異者。",
        "from": "T32n1639_001:0155c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是虛妄分別。",
        "from": "T32n1639_001:0155c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若彼二法是一者。",
        "from": "T32n1639_001:0155c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得言彼法是異。",
        "from": "T32n1639_001:0155c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無二者云何言一。",
        "from": "T32n1639_001:0155c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以彼法相待成故依世諦虛妄分別。",
        "from": "T32n1639_001:0155c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一義諦中無彼外道虛妄分別戲論過故。",
        "from": "T32n1639_001:0155c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是總答四種外道邪見之相。",
        "from": "T32n1639_001:0155c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自此已下別答四義。",
        "from": "T32n1639_001:0155c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是一一觀察迦毘羅憂樓佉等外道虛妄分別義不成就。",
        "from": "T32n1639_001:0155c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此義云何。",
        "from": "T32n1639_001:0155c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言一切法一者。",
        "from": "T32n1639_001:0155c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此義不然。",
        "from": "T32n1639_001:0155c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以滅應滅。",
        "from": "T32n1639_001:0155c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不滅不應滅。",
        "from": "T32n1639_001:0155c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱滅俱不滅此義云何。",
        "from": "T32n1639_001:0155c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝向說我與覺相差別不可得如白疊。",
        "from": "T32n1639_001:0155c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我破此義。",
        "from": "T32n1639_001:0155c17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故。",
        "from": "T32n1639_001:0155c17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此義不與諸經論相應故。",
        "from": "T32n1639_001:0155c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝說諸法差別不可得者。",
        "from": "T32n1639_001:0155c18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此義不然。",
        "from": "T32n1639_001:0155c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如手爪彼法二相差別不可得故。",
        "from": "T32n1639_001:0155c19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此明何義。",
        "from": "T32n1639_001:0155c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如爪指掌名之為手。",
        "from": "T32n1639_001:0155c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若異此法手不可得。",
        "from": "T32n1639_001:0155c21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是白疊一不可得。",
        "from": "T32n1639_001:0155c21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故。",
        "from": "T32n1639_001:0155c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無異法故。",
        "from": "T32n1639_001:0155c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我覺一不可得。",
        "from": "T32n1639_001:0155c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是白疊一不可得。",
        "from": "T32n1639_001:0155c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如手與指掌。",
        "from": "T32n1639_001:0155c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若此滅者彼亦應滅。",
        "from": "T32n1639_001:0155c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此義云何。",
        "from": "T32n1639_001:0155c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若白滅者疊應滅故。",
        "from": "T32n1639_001:0155c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如截手即截指掌。",
        "from": "T32n1639_001:0155c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝意若謂白滅疊不滅者。",
        "from": "T32n1639_001:0155c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此義不然。",
        "from": "T32n1639_001:0155c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若疊不滅白亦不應滅。",
        "from": "T32n1639_001:0155c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如截於手指掌應在如截指掌手亦應在故。",
        "from": "T32n1639_001:0155c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝意若謂青黃赤等唯滅白色不滅疊者。",
        "from": "T32n1639_001:0155c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何言一。",
        "from": "T32n1639_001:0155c28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不爾者青黃赤等色不應滅。",
        "from": "T32n1639_001:0155c28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不爾疊不滅者青黃白等色亦不應滅。",
        "from": "T32n1639_001:0155c29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1639_001:0156a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我青黃赤等覆白色而不滅白。",
        "from": "T32n1639_001:0156a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此義云何。",
        "from": "T32n1639_001:0156a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1639_001:0156a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疊亦如是覆疊而不滅疊。",
        "from": "T32n1639_001:0156a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又此義不然。",
        "from": "T32n1639_001:0156a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洗疊已還見白色故。",
        "from": "T32n1639_001:0156a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疊亦如是。",
        "from": "T32n1639_001:0156a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覆疊不滅疊。",
        "from": "T32n1639_001:0156a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故白即是疊。",
        "from": "T32n1639_001:0156a05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疊即是白。",
        "from": "T32n1639_001:0156a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若疊滅者青黃赤白等色云何見。",
        "from": "T32n1639_001:0156a05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若汝意謂白滅覆非滅疊應滅覆疊不應滅白。",
        "from": "T32n1639_001:0156a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若爾有法滅覆有法不滅不覆。",
        "from": "T32n1639_001:0156a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何言一。",
        "from": "T32n1639_001:0156a08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故一義不成。",
        "from": "T32n1639_001:0156a08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已答外道僧佉論師一切法一竟。",
        "from": "T32n1639_001:0156a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1639_001:0156a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦那陀外道論師言一切法異者。",
        "from": "T32n1639_001:0156a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我與覺異。",
        "from": "T32n1639_001:0156a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以說異法故。",
        "from": "T32n1639_001:0156a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是我此是覺。",
        "from": "T32n1639_001:0156a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如白疊此是白此是疊故。",
        "from": "T32n1639_001:0156a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1639_001:0156a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此義不然。",
        "from": "T32n1639_001:0156a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以無譬喻故。",
        "from": "T32n1639_001:0156a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如人說言。",
        "from": "T32n1639_001:0156a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是手此是指掌。",
        "from": "T32n1639_001:0156a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼人雖說此語不能說異法。",
        "from": "T32n1639_001:0156a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故不得言我覺異。",
        "from": "T32n1639_001:0156a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如白疊以見世間有二種差別故。",
        "from": "T32n1639_001:0156a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者相二者處。",
        "from": "T32n1639_001:0156a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相差別者。",
        "from": "T32n1639_001:0156a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色香味觸不異相有異相故。",
        "from": "T32n1639_001:0156a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處差別者。",
        "from": "T32n1639_001:0156a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如穀豆等有白疊不異相有差別。",
        "from": "T32n1639_001:0156a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如彼色香味觸。",
        "from": "T32n1639_001:0156a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不爾者有四種過。",
        "from": "T32n1639_001:0156a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此義云何。",
        "from": "T32n1639_001:0156a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白滅疊亦滅。",
        "from": "T32n1639_001:0156a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如彼色香味觸。",
        "from": "T32n1639_001:0156a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如火和合燒瓶成赤色已又為青色。",
        "from": "T32n1639_001:0156a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香味亦爾。",
        "from": "T32n1639_001:0156a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不爾者色香味觸亦不應滅。",
        "from": "T32n1639_001:0156a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如彼白疊異不可得。",
        "from": "T32n1639_001:0156a21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若白滅者疊亦應滅。",
        "from": "T32n1639_001:0156a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疊不滅者白亦不應滅。",
        "from": "T32n1639_001:0156a22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1639_001:0156a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此義不然。",
        "from": "T32n1639_001:0156a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依彼法有此法。",
        "from": "T32n1639_001:0156a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如畫壁依壁有畫。",
        "from": "T32n1639_001:0156a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壁滅畫亦滅。",
        "from": "T32n1639_001:0156a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫滅壁不滅。",
        "from": "T32n1639_001:0156a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我白滅疊不滅義亦如是。",
        "from": "T32n1639_001:0156a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1639_001:0156a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝此譬喻事不相似。",
        "from": "T32n1639_001:0156a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壁是先有畫是後作。",
        "from": "T32n1639_001:0156a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而彼白疊起無前後。",
        "from": "T32n1639_001:0156a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可得言此白先有疊是後作。",
        "from": "T32n1639_001:0156a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已答外道衛世師論師一切法異義竟。",
        "from": "T32n1639_001:0156a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1639_001:0156a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尼揵子外道論師言一切法俱。",
        "from": "T32n1639_001:0156a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦毘羅等論師皆有過失。",
        "from": "T32n1639_001:0156a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以說一異故。",
        "from": "T32n1639_001:0156b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故我說俱而不俱。",
        "from": "T32n1639_001:0156b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如燈明。",
        "from": "T32n1639_001:0156b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有此有彼。",
        "from": "T32n1639_001:0156b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有彼有此。",
        "from": "T32n1639_001:0156b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無此無彼。",
        "from": "T32n1639_001:0156b02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無彼無此。",
        "from": "T32n1639_001:0156b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如有燈有明有明有燈。",
        "from": "T32n1639_001:0156b03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無燈無明無明無燈。",
        "from": "T32n1639_001:0156b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "異者能照所照。",
        "from": "T32n1639_001:0156b04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以燈異處明異處。",
        "from": "T32n1639_001:0156b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故說異。",
        "from": "T32n1639_001:0156b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如我覺白疊等。",
        "from": "T32n1639_001:0156b05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦得說一亦得說異。",
        "from": "T32n1639_001:0156b05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如白於疊中別處不可得言此是白此是疊。",
        "from": "T32n1639_001:0156b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如世間此是牛此是馬等。",
        "from": "T32n1639_001:0156b07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白疊不爾。",
        "from": "T32n1639_001:0156b07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故我不說異亦不說一。",
        "from": "T32n1639_001:0156b08_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一者白滅疊應滅。",
        "from": "T32n1639_001:0156b08_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又若一者亦不應說赤疊黑疊等。",
        "from": "T32n1639_001:0156b09_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故我言得說一得說異。",
        "from": "T32n1639_001:0156b09_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此義云何。",
        "from": "T32n1639_001:0156b10_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1639_001:0156b10_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此義不然。",
        "from": "T32n1639_001:0156b10_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如向說僧佉毘世師等過失。",
        "from": "T32n1639_001:0156b11_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與此無異。",
        "from": "T32n1639_001:0156b11_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以何等義僧佉一如向說。",
        "from": "T32n1639_001:0156b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以何等義毘世師異如向說。",
        "from": "T32n1639_001:0156b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何向說。",
        "from": "T32n1639_001:0156b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如向說言。",
        "from": "T32n1639_001:0156b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燈明一者燈即是明。",
        "from": "T32n1639_001:0156b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明即是燈。",
        "from": "T32n1639_001:0156b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此唯有別數而無別義。",
        "from": "T32n1639_001:0156b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若爾燈亦應明。",
        "from": "T32n1639_001:0156b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明亦應燈。",
        "from": "T32n1639_001:0156b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若此二法。",
        "from": "T32n1639_001:0156b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者云何異處。",
        "from": "T32n1639_001:0156b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如手與指掌無差別。",
        "from": "T32n1639_001:0156b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脚手有差別。",
        "from": "T32n1639_001:0156b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手指掌無差別。",
        "from": "T32n1639_001:0156b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一者云何言異。",
        "from": "T32n1639_001:0156b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故不得言一言異。",
        "from": "T32n1639_001:0156b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此一異義不成。",
        "from": "T32n1639_001:0156b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已答外道尼犍子論師一切法俱竟。",
        "from": "T32n1639_001:0156b18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1639_001:0156b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若提子論師言。",
        "from": "T32n1639_001:0156b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧佉等論師說一切法一異俱皆有過失。",
        "from": "T32n1639_001:0156b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我若提子不說一切法一異俱。",
        "from": "T32n1639_001:0156b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如我論中不許此義。",
        "from": "T32n1639_001:0156b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯許不俱。",
        "from": "T32n1639_001:0156b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故我無僧佉等過失。",
        "from": "T32n1639_001:0156b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖然不得說言無不俱。",
        "from": "T32n1639_001:0156b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此義云何。",
        "from": "T32n1639_001:0156b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1639_001:0156b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此義不然。",
        "from": "T32n1639_001:0156b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以無譬喻故。",
        "from": "T32n1639_001:0156b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以無譬喻者我說。",
        "from": "T32n1639_001:0156b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世諦有如是法。",
        "from": "T32n1639_001:0156b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一義諦中無如是相。",
        "from": "T32n1639_001:0156b25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故此成我所說義。",
        "from": "T32n1639_001:0156b26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此明何義。",
        "from": "T32n1639_001:0156b26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以無彼法即無此法無彼法體亦無此法體。",
        "from": "T32n1639_001:0156b26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此法不成彼法。",
        "from": "T32n1639_001:0156b27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼法不成此法。",
        "from": "T32n1639_001:0156b28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此法畢竟非彼法。",
        "from": "T32n1639_001:0156b28_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼法亦畢竟非此法。",
        "from": "T32n1639_001:0156b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以白非疊以疊非白。",
        "from": "T32n1639_001:0156b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以滅不應滅。",
        "from": "T32n1639_001:0156b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以一者即白是疊疊即是白。",
        "from": "T32n1639_001:0156c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不爾者滅是滅。",
        "from": "T32n1639_001:0156c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不滅者不滅。",
        "from": "T32n1639_001:0156c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若爾云何虛妄分別。",
        "from": "T32n1639_001:0156c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼法是一異俱不俱。",
        "from": "T32n1639_001:0156c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若爾疊亦應非疊非不疊。",
        "from": "T32n1639_001:0156c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白亦應非白非不白。",
        "from": "T32n1639_001:0156c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以疊即是疊白即是白。",
        "from": "T32n1639_001:0156c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故疊非疊白非白。",
        "from": "T32n1639_001:0156c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故非白不得白。",
        "from": "T32n1639_001:0156c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是一異俱不俱皆是虛妄分別。",
        "from": "T32n1639_001:0156c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯有言說無有實義。",
        "from": "T32n1639_001:0156c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我覺因果等義亦如是故。",
        "from": "T32n1639_001:0156c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已答外道若提子論師一切法不俱竟。",
        "from": "T32n1639_001:0156c07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提婆菩薩破楞伽經中外道小乘四宗論",
        "from": "T32n1639_001:0156c09_0",
        "context_with_next_sentence": false
    }
]