[
    {
        "source_sentence": "高峰龍泉院因師集賢語錄卷之十四",
        "from": "X65n1277_014:0050c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "抄題雜化門",
        "from": "X65n1277_014:0050c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化鍾樓疏",
        "from": "X65n1277_014:0050c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千鈞重器已成槖籥之功。",
        "from": "X65n1277_014:0050c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百尺高樓欲遂斧斤之用。",
        "from": "X65n1277_014:0050c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誰能特達。",
        "from": "X65n1277_014:0050c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就此因緣。",
        "from": "X65n1277_014:0050c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶月輕敲驚起海門沙上鴈。",
        "from": "X65n1277_014:0050c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和煙試擊喚回塵世夢中人。",
        "from": "X65n1277_014:0050c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍泉院前虹橋疏",
        "from": "X65n1277_014:0050c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘輿濟眾惠止一時。",
        "from": "X65n1277_014:0050c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷木利人功流萬世。",
        "from": "X65n1277_014:0050c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟龍泉之勝槩。",
        "from": "X65n1277_014:0050c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有虹首之高飛。",
        "from": "X65n1277_014:0050c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頃緣洪水之橫流。",
        "from": "X65n1277_014:0050c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日見行人之病涉。",
        "from": "X65n1277_014:0050c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有阻留題之客。",
        "from": "X65n1277_014:0050c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頗妨期會之人。",
        "from": "X65n1277_014:0050c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今欲經之營之。",
        "from": "X65n1277_014:0050c18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必賴遠者近者。",
        "from": "X65n1277_014:0050c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懷區區之尺牘。",
        "from": "X65n1277_014:0050c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "扣磊磊之丈夫。",
        "from": "X65n1277_014:0050c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願向五達衢出一隻手。",
        "from": "X65n1277_014:0050c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將見問津老子不煩般若之舟。",
        "from": "X65n1277_014:0050c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "策杖禪人咸到菩提之岸。",
        "from": "X65n1277_014:0050c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化粧藏疏",
        "from": "X65n1277_014:0050c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一輪永轉。",
        "from": "X65n1277_014:0050c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬法常明。",
        "from": "X65n1277_014:0050c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲教世眼無塵。",
        "from": "X65n1277_014:0050c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須藉當人潤色。",
        "from": "X65n1277_014:0050c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍宮本來富貴。",
        "from": "X65n1277_014:0050c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛家分外莊嚴。",
        "from": "X65n1277_014:0050c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾手易維持。",
        "from": "X65n1277_014:0050c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雙拳難點畫。",
        "from": "X65n1277_014:0050c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "搖天撼地聲聲鏘玉鳴金。",
        "from": "X65n1277_014:0051a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耀古輝今家家藏珍隱寶。",
        "from": "X65n1277_014:0051a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸仁者儻能成這段好事。",
        "from": "X65n1277_014:0051a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大梵王必不辜莫大深恩。",
        "from": "X65n1277_014:0051a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出頭為有力因緣。",
        "from": "X65n1277_014:0051a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉脚得無窮受用。",
        "from": "X65n1277_014:0051a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化彩畫水陸功德疏",
        "from": "X65n1277_014:0051a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師祖流傳雖有冥陽公案。",
        "from": "X65n1277_014:0051a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道場鋪設柰無彩畫行頭。",
        "from": "X65n1277_014:0051a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小比丘縱會秉呪加持。",
        "from": "X65n1277_014:0051a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸聖像未便隨聲顯現。",
        "from": "X65n1277_014:0051a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擬倩畫工出手。",
        "from": "X65n1277_014:0051a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須是方兄點頭。",
        "from": "X65n1277_014:0051a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敢辭開口告人難。",
        "from": "X65n1277_014:0051a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信道發心為事易。",
        "from": "X65n1277_014:0051a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四十二軸大家無惜相成。",
        "from": "X65n1277_014:0051a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千百億身諸相管教具足。",
        "from": "X65n1277_014:0051a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化粧廢寺佛疏",
        "from": "X65n1277_014:0051a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千百億化身剎剎塵塵俱為佛事。",
        "from": "X65n1277_014:0051a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方諸國土在在處處總是道場。",
        "from": "X65n1277_014:0051a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南州打到北州頭。",
        "from": "X65n1277_014:0051a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東土移來西土着。",
        "from": "X65n1277_014:0051a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有生皆可度。",
        "from": "X65n1277_014:0051a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何處不稱尊。",
        "from": "X65n1277_014:0051a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與其蘆茅穿膝於彭源廢剎之中。",
        "from": "X65n1277_014:0051a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孰若花木嚴身於城山大隱之地。",
        "from": "X65n1277_014:0051a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說彌勒當來下生之事。",
        "from": "X65n1277_014:0051a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯諸人過去現在之因。",
        "from": "X65n1277_014:0051a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不勞更揀燒香。",
        "from": "X65n1277_014:0051a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要捨必須連脚。",
        "from": "X65n1277_014:0051a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請持此疏扣朱門綠戶之賢。",
        "from": "X65n1277_014:0051a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲覔本光驗紫磨黃金之相。",
        "from": "X65n1277_014:0051a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此福德勝前福德。",
        "from": "X65n1277_014:0051a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非莊嚴是名莊嚴。",
        "from": "X65n1277_014:0051a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化粧佛畫壁疏",
        "from": "X65n1277_014:0051a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清淨法身雖云不染。",
        "from": "X65n1277_014:0051a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "粧嚴佛相又且何妨。",
        "from": "X65n1277_014:0051a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不惟壁洛上添个風光。",
        "from": "X65n1277_014:0051a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更使石殿裏重新活計。",
        "from": "X65n1277_014:0051a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若得大家提掇。",
        "from": "X65n1277_014:0051a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便教兩處圓成。",
        "from": "X65n1277_014:0051a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "快請承當。",
        "from": "X65n1277_014:0051a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真个奇特。",
        "from": "X65n1277_014:0051a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化塑七身功德疏",
        "from": "X65n1277_014:0051a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛法充滿於虗空。",
        "from": "X65n1277_014:0051a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本無一相。",
        "from": "X65n1277_014:0051a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵宇莊嚴於功德。",
        "from": "X65n1277_014:0051a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須假七尊。",
        "from": "X65n1277_014:0051a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儻逢多寶如來。",
        "from": "X65n1277_014:0051b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便見真濟菩薩。",
        "from": "X65n1277_014:0051b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化粧羅漢聖像疏",
        "from": "X65n1277_014:0051b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本來具足見成一十八尊。",
        "from": "X65n1277_014:0051b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更假莊嚴始現三十二相。",
        "from": "X65n1277_014:0051b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須是著些精彩。",
        "from": "X65n1277_014:0051b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共成這个因緣。",
        "from": "X65n1277_014:0051b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若發菩提心。",
        "from": "X65n1277_014:0051b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便證羅漢果。",
        "from": "X65n1277_014:0051b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化佛前燈疏",
        "from": "X65n1277_014:0051b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "古佛堂前惠燈不滅。",
        "from": "X65n1277_014:0051b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普光殿內智燭長明。",
        "from": "X65n1277_014:0051b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照破羣昏實由一點。",
        "from": "X65n1277_014:0051b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各請人人返照。",
        "from": "X65n1277_014:0051b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便知祖祖相傳。",
        "from": "X65n1277_014:0051b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "个中若識賣油翁。",
        "from": "X65n1277_014:0051b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直下許見燃燈佛。",
        "from": "X65n1277_014:0051b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化長明燈疏",
        "from": "X65n1277_014:0051b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "只一星兒演出大光明藏。",
        "from": "X65n1277_014:0051b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向五陰處爍破諸煩惱門。",
        "from": "X65n1277_014:0051b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雨風如晦而這个分明。",
        "from": "X65n1277_014:0051b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日月尚虧而此光常在。",
        "from": "X65n1277_014:0051b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相傳無盡。",
        "from": "X65n1277_014:0051b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "點起便知。",
        "from": "X65n1277_014:0051b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須遇特達人。",
        "from": "X65n1277_014:0051b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能成久遠事。",
        "from": "X65n1277_014:0051b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玉樓金殿長明公今已加封。",
        "from": "X65n1277_014:0051b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "銀管寶書燃燈佛昔曾授記。",
        "from": "X65n1277_014:0051b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化設地獄齋疏",
        "from": "X65n1277_014:0051b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛即是心。",
        "from": "X65n1277_014:0051b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心即是佛。",
        "from": "X65n1277_014:0051b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一毛頭上識根源。",
        "from": "X65n1277_014:0051b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人死為鬼。",
        "from": "X65n1277_014:0051b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼死為人。",
        "from": "X65n1277_014:0051b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三惡道中隨影附。",
        "from": "X65n1277_014:0051b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若隔這个便墮群流。",
        "from": "X65n1277_014:0051b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "業風動地不停。",
        "from": "X65n1277_014:0051b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飢火燒心難忍。",
        "from": "X65n1277_014:0051b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "饆羅[飢几+追]子請君快不將來。",
        "from": "X65n1277_014:0051b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鉄酸饀兒與他從頭咬破。",
        "from": "X65n1277_014:0051b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名真法供養。",
        "from": "X65n1277_014:0051b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大家隨喜結緣。",
        "from": "X65n1277_014:0051b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化錢置常住疏",
        "from": "X65n1277_014:0051b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "古佛家風高坐双峰之頂。",
        "from": "X65n1277_014:0051b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道人活計全無一日之粮。",
        "from": "X65n1277_014:0051b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用資數畝之田。",
        "from": "X65n1277_014:0051b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永作千年之寶。",
        "from": "X65n1277_014:0051b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須是飢來喫飯。",
        "from": "X65n1277_014:0051b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誰解心空坐禪。",
        "from": "X65n1277_014:0051b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "檀那儻賜於筆端。",
        "from": "X65n1277_014:0051b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我佛必增於福利。",
        "from": "X65n1277_014:0051b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化齋供疏",
        "from": "X65n1277_014:0051c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施財施食增益福田。",
        "from": "X65n1277_014:0051c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供佛供僧[栽木+土]培善果。",
        "from": "X65n1277_014:0051c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用嚴藥石。",
        "from": "X65n1277_014:0051c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普献人天。",
        "from": "X65n1277_014:0051c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結此無上緣。",
        "from": "X65n1277_014:0051c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須還見在佛。",
        "from": "X65n1277_014:0051c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起建華嚴閣疏",
        "from": "X65n1277_014:0051c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖興堂成己壯此方之風彩。",
        "from": "X65n1277_014:0051c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華嚴閣就重增梵宇之光輝。",
        "from": "X65n1277_014:0051c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不惟再整於高門。",
        "from": "X65n1277_014:0051c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仍使一新於大教。",
        "from": "X65n1277_014:0051c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三事若令具足。",
        "from": "X65n1277_014:0051c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方須藉扶持。",
        "from": "X65n1277_014:0051c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立地便成。",
        "from": "X65n1277_014:0051c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具眼底看。",
        "from": "X65n1277_014:0051c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鐘樓化瓦修蓋疏",
        "from": "X65n1277_014:0051c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樓高百尺不妨聳壑昂霄。",
        "from": "X65n1277_014:0051c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鐘震一音能使聞聲悟道。",
        "from": "X65n1277_014:0051c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歷時既久覺棟宇之飄零。",
        "from": "X65n1277_014:0051c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "整舊如新藉慈悲而喜施。",
        "from": "X65n1277_014:0051c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化米開路疏",
        "from": "X65n1277_014:0051c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道人脚下盡大地作个路頭。",
        "from": "X65n1277_014:0051c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祖師庵前使万里渾無寸草。",
        "from": "X65n1277_014:0051c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若得十方無礙。",
        "from": "X65n1277_014:0051c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何妨八字打開。",
        "from": "X65n1277_014:0051c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未饒寂子之插鍬。",
        "from": "X65n1277_014:0051c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶悟丹霞之剗草。",
        "from": "X65n1277_014:0051c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果得食輪先轉。",
        "from": "X65n1277_014:0051c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何愁心地不通。",
        "from": "X65n1277_014:0051c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須知獨樹不成林。",
        "from": "X65n1277_014:0051c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各請大家齊出手。",
        "from": "X65n1277_014:0051c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化甃路疏",
        "from": "X65n1277_014:0051c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心地坦平永無障礙。",
        "from": "X65n1277_014:0051c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福基鞏固寧患崎嶇。",
        "from": "X65n1277_014:0051c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結十方眾信之緣。",
        "from": "X65n1277_014:0051c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開万劫菩提之路。",
        "from": "X65n1277_014:0051c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實為方便。",
        "from": "X65n1277_014:0051c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各請圓成。",
        "from": "X65n1277_014:0051c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化人披剃疏",
        "from": "X65n1277_014:0051c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "靈鷲如來親剃目犍連之髮。",
        "from": "X65n1277_014:0051c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黃梅祖佛密傳盧行者之衣。",
        "from": "X65n1277_014:0051c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向時既有這般人。",
        "from": "X65n1277_014:0051c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今日豈無如是事。",
        "from": "X65n1277_014:0051c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殿前剗草須逢具眼宗師。",
        "from": "X65n1277_014:0051c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地上布金更藉作家長者。",
        "from": "X65n1277_014:0051c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化度僧疏",
        "from": "X65n1277_014:0051c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩大慈不妨化現。",
        "from": "X65n1277_014:0051c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比丘得度須藉圓成。",
        "from": "X65n1277_014:0051c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "假法身而了佛身。",
        "from": "X65n1277_014:0051c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憑佛寶而為僧寶。",
        "from": "X65n1277_014:0052a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若得手中具眼。",
        "from": "X65n1277_014:0052a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便教頂上放光。",
        "from": "X65n1277_014:0052a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "全資十善檀那。",
        "from": "X65n1277_014:0052a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了取兩重公案。",
        "from": "X65n1277_014:0052a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化度牒疏",
        "from": "X65n1277_014:0052a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辭親割愛出三界家入如來家。",
        "from": "X65n1277_014:0052a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "學佛解空從二乘地至菩薩地。",
        "from": "X65n1277_014:0052a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爭奈頭顱未肯。",
        "from": "X65n1277_014:0052a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難教袈裟自肩。",
        "from": "X65n1277_014:0052a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幸茲時節因緣。",
        "from": "X65n1277_014:0052a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了辦己躬大事。",
        "from": "X65n1277_014:0052a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盧行者本來無物。",
        "from": "X65n1277_014:0052a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孔方兄遭遇有緣。",
        "from": "X65n1277_014:0052a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樓閣開門彌勒不須彌指。",
        "from": "X65n1277_014:0052a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "珠璣滿室居士休把沉江。",
        "from": "X65n1277_014:0052a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若是布施將來。",
        "from": "X65n1277_014:0052a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應以此身得度。",
        "from": "X65n1277_014:0052a08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "士人化度牒疏",
        "from": "X65n1277_014:0052a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說盡之乎者也四十年枉下工夫。",
        "from": "X65n1277_014:0052a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "究竟寂滅苦空億万劫願離障礙。",
        "from": "X65n1277_014:0052a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "回頭猛省。",
        "from": "X65n1277_014:0052a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "落髮寧辭。",
        "from": "X65n1277_014:0052a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要共丹霞同入大法門。",
        "from": "X65n1277_014:0052a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不放寶月獨出一頭地。",
        "from": "X65n1277_014:0052a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "積藏菩薩大家好結歡喜緣。",
        "from": "X65n1277_014:0052a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脫俗秀才只今便現圓滿相。",
        "from": "X65n1277_014:0052a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吏人化度牒疏",
        "from": "X65n1277_014:0052a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法門四闢六通任教攘臂。",
        "from": "X65n1277_014:0052a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受網千纏万結豈易脫身。",
        "from": "X65n1277_014:0052a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "許多日往月來。",
        "from": "X65n1277_014:0052a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頓覺昨非今是。",
        "from": "X65n1277_014:0052a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大木尊者亟辭母。",
        "from": "X65n1277_014:0052a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香囊和尚便拋妻。",
        "from": "X65n1277_014:0052a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "等閑鐘鼓度朝昏。",
        "from": "X65n1277_014:0052a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何必堦墀還卯酉。",
        "from": "X65n1277_014:0052a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文書悉擯從頭點檢佛書。",
        "from": "X65n1277_014:0052a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公印歸還一向掌持心印。",
        "from": "X65n1277_014:0052a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道姑化度牒疏",
        "from": "X65n1277_014:0052a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幻生失路覺諸祖之迷蒙。",
        "from": "X65n1277_014:0052a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本性尋源返一真於寂滅。",
        "from": "X65n1277_014:0052a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既是已沾泥之柳絮。",
        "from": "X65n1277_014:0052a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當為不著水之蓮花。",
        "from": "X65n1277_014:0052a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喜遇聖恩可成佛相。",
        "from": "X65n1277_014:0052a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若大居士筆端半點玉振金聲。",
        "from": "X65n1277_014:0052a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使小尼童頭上一刀雷飛電掃。",
        "from": "X65n1277_014:0052a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彫盤烽燁。",
        "from": "X65n1277_014:0052a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恩海波長。",
        "from": "X65n1277_014:0052a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化三衣疏",
        "from": "X65n1277_014:0052a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逝陀林下世尊拈示商人。",
        "from": "X65n1277_014:0052b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少室山前達磨親傳弟子。",
        "from": "X65n1277_014:0052b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖道寸縣不染。",
        "from": "X65n1277_014:0052b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何妨一線相通。",
        "from": "X65n1277_014:0052b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲得三衣。",
        "from": "X65n1277_014:0052b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共出隻手。",
        "from": "X65n1277_014:0052b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化百衲衣疏",
        "from": "X65n1277_014:0052b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清淨法身雖道寸[糸*系]不掛。",
        "from": "X65n1277_014:0052b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莊嚴妙相何妨五彩合成。",
        "from": "X65n1277_014:0052b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非徒聳世俗之觀瞻。",
        "from": "X65n1277_014:0052b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正欲為法門之壯觀。",
        "from": "X65n1277_014:0052b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若要發一心喜捨。",
        "from": "X65n1277_014:0052b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便能教百衲周圓。",
        "from": "X65n1277_014:0052b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "試著出來。",
        "from": "X65n1277_014:0052b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "官取奇特。",
        "from": "X65n1277_014:0052b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化鐘疏",
        "from": "X65n1277_014:0052b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煙寺一鳴驚回蝶夢。",
        "from": "X65n1277_014:0052b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豐山三擊震動龍天。",
        "from": "X65n1277_014:0052b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鑄成出格規模。",
        "from": "X65n1277_014:0052b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "全賴作家手段。",
        "from": "X65n1277_014:0052b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化磬疏",
        "from": "X65n1277_014:0052b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烈火烹成好个作家手段。",
        "from": "X65n1277_014:0052b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頑銅打就還它本分鉗鎚。",
        "from": "X65n1277_014:0052b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲教立地圓成。",
        "from": "X65n1277_014:0052b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更在當人煅煉。",
        "from": "X65n1277_014:0052b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖難下手。",
        "from": "X65n1277_014:0052b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賴遇知音。",
        "from": "X65n1277_014:0052b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化鐃鈸疏",
        "from": "X65n1277_014:0052b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大道不器。",
        "from": "X65n1277_014:0052b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明道者假器而求。",
        "from": "X65n1277_014:0052b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至理無聲。",
        "from": "X65n1277_014:0052b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟理者緣聲而入。",
        "from": "X65n1277_014:0052b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要這个方明那个。",
        "from": "X65n1277_014:0052b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自有為始證無為。",
        "from": "X65n1277_014:0052b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便須烈火烹成。",
        "from": "X65n1277_014:0052b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何惜頑銅打就。",
        "from": "X65n1277_014:0052b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一響一答直教眼下聞知。",
        "from": "X65n1277_014:0052b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "再舉再揚正好聲前薦取。",
        "from": "X65n1277_014:0052b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲憑出手。",
        "from": "X65n1277_014:0052b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "全賴知音。",
        "from": "X65n1277_014:0052b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化鍋疏",
        "from": "X65n1277_014:0052b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "純銅打就還他本分鉗鎚。",
        "from": "X65n1277_014:0052b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烈火烹成須是當行爐鞴。",
        "from": "X65n1277_014:0052b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誰是作家手段。",
        "from": "X65n1277_014:0052b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鑄成出格規模。",
        "from": "X65n1277_014:0052b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫問純鐵頑銅精金美玉。",
        "from": "X65n1277_014:0052b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總教紅爐上成一片雪。",
        "from": "X65n1277_014:0052b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須使火燄裏轉大法輪。",
        "from": "X65n1277_014:0052b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煆煉出來。",
        "from": "X65n1277_014:0052b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迅燥者看。",
        "from": "X65n1277_014:0052b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣化疏",
        "from": "X65n1277_014:0052b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "這般時年鐵板障巳。",
        "from": "X65n1277_014:0052b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "只退縫有些伎倆。",
        "from": "X65n1277_014:0052b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大布袋未曾開頭。",
        "from": "X65n1277_014:0052b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "檀那筆頭須試出萬丈毫光。",
        "from": "X65n1277_014:0052c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苾蒭時下便做得一分生活。",
        "from": "X65n1277_014:0052c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大家心肯意肯。",
        "from": "X65n1277_014:0052c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "休說頭高頭低。",
        "from": "X65n1277_014:0052c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡喜即大因緣。",
        "from": "X65n1277_014:0052c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "布施不昧因果。",
        "from": "X65n1277_014:0052c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "題彌陀會疏",
        "from": "X65n1277_014:0052c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若據底事不涉修成。",
        "from": "X65n1277_014:0052c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曲為今時豈無方便。",
        "from": "X65n1277_014:0052c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須信道心心淨土。",
        "from": "X65n1277_014:0052c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了得底步步青蓮。",
        "from": "X65n1277_014:0052c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲登九品受生。",
        "from": "X65n1277_014:0052c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須假一音演說。",
        "from": "X65n1277_014:0052c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是男是女大家齊辦肯心。",
        "from": "X65n1277_014:0052c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同號同名立地管取成佛。",
        "from": "X65n1277_014:0052c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "題華嚴會疏",
        "from": "X65n1277_014:0052c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百二十卷真詮雖道本來具足。",
        "from": "X65n1277_014:0052c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五十三員知識不妨直下同參。",
        "from": "X65n1277_014:0052c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常嗟大教之難逢。",
        "from": "X65n1277_014:0052c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "肯使今生之蹉過。",
        "from": "X65n1277_014:0052c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各出隻手夾成般若勝因。",
        "from": "X65n1277_014:0052c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "齊辦肯心同結華嚴善會。",
        "from": "X65n1277_014:0052c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "題金剛會疏",
        "from": "X65n1277_014:0052c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向來佛法尚付國王大臣。",
        "from": "X65n1277_014:0052c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今振祖風須是居士長者。",
        "from": "X65n1277_014:0052c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同具金剛眼。",
        "from": "X65n1277_014:0052c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共結般若緣。",
        "from": "X65n1277_014:0052c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非惟超脫無始輪迴。",
        "from": "X65n1277_014:0052c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "抑且成就莫大功德。",
        "from": "X65n1277_014:0052c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真人奇特。",
        "from": "X65n1277_014:0052c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各請承當。",
        "from": "X65n1277_014:0052c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "題盂蘭盆會疏",
        "from": "X65n1277_014:0052c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金風剪剪適地官赦罪之秋。",
        "from": "X65n1277_014:0052c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "珪月團團乃尊者救親之日。",
        "from": "X65n1277_014:0052c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幸遇此般時節。",
        "from": "X65n1277_014:0052c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何妨結个因緣。",
        "from": "X65n1277_014:0052c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爰鳩花縣吉人共建蘭盆勝會。",
        "from": "X65n1277_014:0052c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不特新千萬年之公案。",
        "from": "X65n1277_014:0052c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又將追五百劫之幽魂。",
        "from": "X65n1277_014:0052c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各請承當。",
        "from": "X65n1277_014:0052c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈容分別。",
        "from": "X65n1277_014:0052c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高峯龍泉院因師集賢語錄卷之十四",
        "from": "X65n1277_014:0052c23_0",
        "context_with_next_sentence": false
    }
]