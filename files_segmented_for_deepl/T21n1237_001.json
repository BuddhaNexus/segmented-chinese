[
    {
        "source_sentence": "阿吒婆拘鬼神大將上佛陀羅尼",
        "from": "T21n1237_001:0178a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神呪經",
        "from": "T21n1237_001:0178a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "失譯人名今附梁錄",
        "from": "T21n1237_001:0178a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T21n1237_001:0178a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在王舍城迦蘭陀竹林中。",
        "from": "T21n1237_001:0178a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時王舍城內有一比丘。",
        "from": "T21n1237_001:0178a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為賊所劫為蛇所螫。",
        "from": "T21n1237_001:0178a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為鬼所嬈受大苦惱。",
        "from": "T21n1237_001:0178a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時鬼神大將阿吒婆拘。",
        "from": "T21n1237_001:0178a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見是比丘受如是苦心生憐愍。",
        "from": "T21n1237_001:0178a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即往佛所至佛所已。",
        "from": "T21n1237_001:0178a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭面禮足在一面立。",
        "from": "T21n1237_001:0178a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊。",
        "from": "T21n1237_001:0178a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以降伏一切極惡諸鬼神等。",
        "from": "T21n1237_001:0178a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今憐愍一切眾生故。",
        "from": "T21n1237_001:0178a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為降伏一切諸惡鬼神及一切惡人惡毒等故。",
        "from": "T21n1237_001:0178a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上佛世尊極嚴惡呪。",
        "from": "T21n1237_001:0178a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以用降伏諸鬼神等。",
        "from": "T21n1237_001:0178a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有讀誦是呪之者。",
        "from": "T21n1237_001:0178a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人威德乃至力能降伏梵天。",
        "from": "T21n1237_001:0178a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況餘惡。",
        "from": "T21n1237_001:0178a19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛告阿吒婆拘鬼神大將。",
        "from": "T21n1237_001:0178a19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我不須此極嚴惡呪。",
        "from": "T21n1237_001:0178a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儻能傷害諸眾生等。",
        "from": "T21n1237_001:0178a20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時阿吒婆拘重白佛言。",
        "from": "T21n1237_001:0178a21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊後惡世之中。",
        "from": "T21n1237_001:0178a21_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡鬼增盛惡人眾多。",
        "from": "T21n1237_001:0178a22_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡毒蟲獸侵害眾生。",
        "from": "T21n1237_001:0178a22_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或值諸難所謂王賊水火刀兵。",
        "from": "T21n1237_001:0178a23_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恐畏怨憎惡鬼等難。",
        "from": "T21n1237_001:0178a23_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若佛弟子出家在家。",
        "from": "T21n1237_001:0178a24_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若住寂靜乞食道人。",
        "from": "T21n1237_001:0178a24_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塚間樹下四部等眾。",
        "from": "T21n1237_001:0178a25_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若行曠野山林道中。",
        "from": "T21n1237_001:0178a25_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若在城邑村里巷陌。",
        "from": "T21n1237_001:0178a26_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當為救護不令遇惡。",
        "from": "T21n1237_001:0178a26_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊慈矜願垂納受。",
        "from": "T21n1237_001:0178a27_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善逝世尊願垂顧錄。",
        "from": "T21n1237_001:0178a27_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊聞是語已默然受之。",
        "from": "T21n1237_001:0178a28_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時阿吒婆拘。",
        "from": "T21n1237_001:0178a28_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見佛默然心懷喜悅。",
        "from": "T21n1237_001:0178a29_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於佛前而說呪曰。",
        "from": "T21n1237_001:0178a29_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豆留咩　豆留咩　陀咩　陀咩　豆留咩　豆留咩　豆留咩　豆留咩　豆留彌隷　尼利　尼利　尼利　那羅　那羅　那羅　尼利　尼利　尼利　尼利　那羅[少/兔]富尼利豆留茶濘豆留茶濘　摩訶豆留茶濘　豆留茶濘　究吒濘　究吒濘　摩訶究吒濘　多吒濘　多吒濘　摩訶多吒濘　多吒濘　吒吒　吒吒　吒吒　吒吒　摩訶吒吒　吒吒　阿毘　阿毘　摩訶阿毘　阿毘阿毘利　阿毘利　摩訶阿毘利　阿毘利　阿婆阿毘　阿婆阿毘　阿婆阿毘　阿婆阿毘　律師　律師　摩訶律師　律師　梨濘梨濘　摩訶　梨濘梨濘　首婁　首婁　摩訶首婁　首婁　仇婁　仇婁　摩訶仇婁　仇婁　留仇牟留仇牟　留仇牟留仇牟　仇摩仇摩　仇摩仇摩　唏利唏利　唏利唏利　伊持伊持　伊持伊持　比持比持　比持比持　呵羅呵羅　呵羅呵羅　唏泥唏泥　唏泥唏泥　休泥休泥　休泥　醯泥　醯泥　呵那呵那　呵那呵那　牟尼牟尼　牟尼牟尼　摩訶牟尼牟尼　娑羅娑羅　娑羅娑羅　尸利睱(究那反)路迦遮利蛇　時那時那　時那時那　無沙婆那暮蛇　修迦都多牟尼　迦羅摩　迦羅摩　迦羅摩　闍竭提多蛇　奢摩陀摩　奢摩陀摩　奢摩陀摩　閻摩陀摩　閻摩陀摩　奢摩目多彌提　那婆羅闍那咩　富留沙多摩牟尼　那毘闍那彌　修伽都多牟尼　那毘闍那咩　莎呵世尊此陀羅尼句。",
        "from": "T21n1237_001:0178b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為一切眾生作護作救。",
        "from": "T21n1237_001:0178c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護持是人悉皆令得安隱寂靜。",
        "from": "T21n1237_001:0178c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令離衰惱滅諸惡毒。",
        "from": "T21n1237_001:0178c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離諸苦惱王難賊難怨憎之難。",
        "from": "T21n1237_001:0178c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若天龍鬼神羅剎夜叉鳩槃茶復多那阿跋漆羅呿屈陀。",
        "from": "T21n1237_001:0178c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等所觸惱者。",
        "from": "T21n1237_001:0178c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所侵損者悉得除滅。",
        "from": "T21n1237_001:0178c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復世間一切諸毒。",
        "from": "T21n1237_001:0178c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "芳草若木根華果。",
        "from": "T21n1237_001:0178c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣裳飲食世間之物。",
        "from": "T21n1237_001:0178c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及蟲鳥禽獸諸能為毒惡傷人者。",
        "from": "T21n1237_001:0178c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉令消壞不能為惡。",
        "from": "T21n1237_001:0178c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復虛空日月星辰。",
        "from": "T21n1237_001:0178c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旋嵐風輪鬼神起風。",
        "from": "T21n1237_001:0178c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲來傷人諸鬼神等。",
        "from": "T21n1237_001:0178c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲來求食吸人精氣食人肉血者。",
        "from": "T21n1237_001:0178c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令人疫病熱病。",
        "from": "T21n1237_001:0178c11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一日二日三日四日乃至七日。",
        "from": "T21n1237_001:0178c12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或令冷病風病濕病寒冷等病。",
        "from": "T21n1237_001:0178c12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若身內若身外一切眾病。",
        "from": "T21n1237_001:0178c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若七日若十六日悉令消滅不能為害。",
        "from": "T21n1237_001:0178c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是等諸鬼神若以手若以口若以脚。",
        "from": "T21n1237_001:0178c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若以舌若以心欲惱人者。",
        "from": "T21n1237_001:0178c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及以惡人欲為人作惱害者。",
        "from": "T21n1237_001:0178c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先當誦此呪力能噤持。",
        "from": "T21n1237_001:0178c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼惡人惡鬼噤碎失念不令為惡。",
        "from": "T21n1237_001:0178c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我今當更說神呪以守護之。",
        "from": "T21n1237_001:0178c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿車阿車(瞋彌反)　牟尼牟尼　摩訶牟尼牟尼[竺二+奧](烏力反)　尼休休　摩訶那迦休休　鬪伽那知阿呼阿伽那知阿多那知　阿吒阿吒　那吒那吒　留豆　留豆　留豆　休休豆留　唏泥唏泥唏泥唏泥　郁仇摩仇摩　仇摩　仇摩　唏梨唏梨唏梨　唏梨　尼利　尼利　摩訶尼利莎訶此陀羅尼為受持讀誦者作護。",
        "from": "T21n1237_001:0178c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有鬼食人精氣。",
        "from": "T21n1237_001:0178c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若損人資產耗人財物。",
        "from": "T21n1237_001:0178c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是一切眾怖怨等悉為結界。",
        "from": "T21n1237_001:0178c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今為某國某甲合家無量作大擁護。",
        "from": "T21n1237_001:0178c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今當重更說防諸惡呪即說呪曰。",
        "from": "T21n1237_001:0179a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "留牟　留牟　留牟　留牟　留摩留摩　留摩留摩　唏梨唏梨　唏梨唏梨　唏梨唏梨　仇那仇那　仇那　仇那　仇那　仇[少/兔]仇[少/兔]　仇[少/兔]　仇[少/兔]　仇留仇留　仇留　仇留　休婁　休婁　休婁　休婁　唏梨暮休　暮休　暮休暮休　暮唏梨暮唏梨　暮唏梨暮唏梨　休牟　休牟　休牟休摩休咩提　摩咩思摩阿提迦羅咩兜　莎訶爾時大將重白佛言。",
        "from": "T21n1237_001:0179a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊此呪極有神力如上所說。",
        "from": "T21n1237_001:0179a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫令持此呪者。",
        "from": "T21n1237_001:0179a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有王畏賊畏火畏水畏。",
        "from": "T21n1237_001:0179a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風毒刀兵等畏。",
        "from": "T21n1237_001:0179a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日月星辰鬼神等畏。",
        "from": "T21n1237_001:0179a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有餘惡知識心生忿姤意生惡害。",
        "from": "T21n1237_001:0179a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲相侵惱者。",
        "from": "T21n1237_001:0179a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當先誦此呪為其結界。",
        "from": "T21n1237_001:0179a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當令彼惡鬼惡人仇怨之人心生侵惡者。",
        "from": "T21n1237_001:0179a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令其愚癡迷悶噤碎。",
        "from": "T21n1237_001:0179a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自遇眾惡不越此界。",
        "from": "T21n1237_001:0179a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能侵犯誦此呪者。",
        "from": "T21n1237_001:0179a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊若有善男子善女人誦此呪者。",
        "from": "T21n1237_001:0179a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切天龍阿修羅諸惡鬼神人非人等。",
        "from": "T21n1237_001:0179a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆悉隨侍擁護不令遇惡。",
        "from": "T21n1237_001:0179a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我是鬼神大將。",
        "from": "T21n1237_001:0179a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "力能降伏一切諸惡鬼。",
        "from": "T21n1237_001:0179a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有誦此呪者。",
        "from": "T21n1237_001:0179a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當將諸鬼神晝夜不離。",
        "from": "T21n1237_001:0179a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擁護其人令不見惡。",
        "from": "T21n1237_001:0179a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不令惡鬼惡人得其便也。",
        "from": "T21n1237_001:0179a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若侵損惱害誦此呪者。",
        "from": "T21n1237_001:0179a22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當以千輻輪轢碎其頭。",
        "from": "T21n1237_001:0179a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸鬼神為作衰害。",
        "from": "T21n1237_001:0179a23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊此呪極有神力極有威德。",
        "from": "T21n1237_001:0179a23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願流布施眾安樂。",
        "from": "T21n1237_001:0179a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊誦此呪者。",
        "from": "T21n1237_001:0179a25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人德力唯佛知之。",
        "from": "T21n1237_001:0179a25_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊此大神呪應付賢德有智善人。",
        "from": "T21n1237_001:0179a25_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不能誦者。",
        "from": "T21n1237_001:0179a26_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應以好紙書寫盛以綵囊。",
        "from": "T21n1237_001:0179a27_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著種種香常持隨身。",
        "from": "T21n1237_001:0179a27_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有憂怖恐難。",
        "from": "T21n1237_001:0179a28_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常當憶念此呪無不消滅。",
        "from": "T21n1237_001:0179a28_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊若有事難憂怖惡鬼神惡夢。",
        "from": "T21n1237_001:0179a29_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲令消滅者。",
        "from": "T21n1237_001:0179a29_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先當結界使諸惡不起。",
        "from": "T21n1237_001:0179b01_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼惡人惡鬼惡賊自受其殃。",
        "from": "T21n1237_001:0179b01_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身體燋枯心意狂亂。",
        "from": "T21n1237_001:0179b02_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲結界之時。",
        "from": "T21n1237_001:0179b02_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應淨洗浴著淨衣服好淨塗地。",
        "from": "T21n1237_001:0179b03_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安七器漿飲。",
        "from": "T21n1237_001:0179b03_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二器著少血飲。",
        "from": "T21n1237_001:0179b04_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二器著種種漿飲然八燈。",
        "from": "T21n1237_001:0179b04_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒熏陸香運心供養。",
        "from": "T21n1237_001:0179b05_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我將諸鬼神至其邊施其所願。",
        "from": "T21n1237_001:0179b05_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人應誦此呪結赤縷然後持行。",
        "from": "T21n1237_001:0179b06_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即能消除一切諸難。",
        "from": "T21n1237_001:0179b07_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛告阿難。",
        "from": "T21n1237_001:0179b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此呪極有大神力。",
        "from": "T21n1237_001:0179b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能消除一切諸難并諸時惡。",
        "from": "T21n1237_001:0179b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擁護眾生多所利益。",
        "from": "T21n1237_001:0179b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝好受持廣令流布。",
        "from": "T21n1237_001:0179b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有城邑村落誦此呪者莫不蒙利。",
        "from": "T21n1237_001:0179b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有國王大臣誦此呪者。",
        "from": "T21n1237_001:0179b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人境土無有惡賊怖難。",
        "from": "T21n1237_001:0179b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "災橫疾疫水旱風霜。",
        "from": "T21n1237_001:0179b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若遇惡賊應誦此呪。",
        "from": "T21n1237_001:0179b12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若繫著高幢頭。",
        "from": "T21n1237_001:0179b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賊見此幢賊尋退散降伏。",
        "from": "T21n1237_001:0179b13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難此呪極有神力。",
        "from": "T21n1237_001:0179b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "極有大德。",
        "from": "T21n1237_001:0179b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應令四眾善誦持之。",
        "from": "T21n1237_001:0179b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時眾會聞佛所說歡喜奉行。",
        "from": "T21n1237_001:0179b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(此中丹本有注云。",
        "from": "T21n1237_001:0179b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牛屎作場外灰緣竪二刀十二隻箭。",
        "from": "T21n1237_001:0179b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請婆拘法餘如前。",
        "from": "T21n1237_001:0179b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "部誦三遍結縷十二結)。",
        "from": "T21n1237_001:0179b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿吒婆拘鬼神大將上佛陀羅尼神呪經",
        "from": "T21n1237_001:0179b18_0",
        "context_with_next_sentence": false
    }
]