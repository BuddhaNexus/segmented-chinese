[
    {
        "source_sentence": "九品往生阿彌陀三摩地集陀",
        "from": "T19n0933_001:0079b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅尼經",
        "from": "T19n0933_001:0079b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "特進試鴻臚卿大興善寺三藏",
        "from": "T19n0933_001:0079b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙門大廣智不空奉　詔譯",
        "from": "T19n0933_001:0079b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時毘盧遮那如來。",
        "from": "T19n0933_001:0079b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於大三摩地門大精舍。",
        "from": "T19n0933_001:0079b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大比丘眾八萬九千人俱。",
        "from": "T19n0933_001:0079b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆是大阿羅漢。",
        "from": "T19n0933_001:0079b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慧善具足所作皆辦。",
        "from": "T19n0933_001:0079b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其名曰神力智辨觀世音菩薩得大勢菩薩。",
        "from": "T19n0933_001:0079b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神通自在王菩薩淨光無垢陀羅尼菩薩。",
        "from": "T19n0933_001:0079b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大力普聞菩薩大莊嚴力菩薩。",
        "from": "T19n0933_001:0079c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量光菩薩慧善慧普光王菩薩。",
        "from": "T19n0933_001:0079c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等大菩薩聲聞大眾。",
        "from": "T19n0933_001:0079c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往詣佛所白言世尊。",
        "from": "T19n0933_001:0079c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量壽國在九品淨識三摩地。",
        "from": "T19n0933_001:0079c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是即諸佛境界如來所居。",
        "from": "T19n0933_001:0079c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三世諸佛從是成正覺。",
        "from": "T19n0933_001:0079c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足三明增長福慧。",
        "from": "T19n0933_001:0079c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其九品境界。",
        "from": "T19n0933_001:0079c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上品上生真色地。",
        "from": "T19n0933_001:0079c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上品中生無垢地。",
        "from": "T19n0933_001:0079c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上品下生離垢地。",
        "from": "T19n0933_001:0079c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中品上生善覺地。",
        "from": "T19n0933_001:0079c07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中品中生明力地中品下生無漏地。",
        "from": "T19n0933_001:0079c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下品上生真覺地。",
        "from": "T19n0933_001:0079c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下品中生賢覺地。",
        "from": "T19n0933_001:0079c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下品下生樂門地。",
        "from": "T19n0933_001:0079c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名曰九品淨識真如境。",
        "from": "T19n0933_001:0079c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是內坐十二大曼陀羅大圓鏡智寶像。",
        "from": "T19n0933_001:0079c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其名曰一切三達無量光佛。",
        "from": "T19n0933_001:0079c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍覺三明無邊光佛。",
        "from": "T19n0933_001:0079c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智道三明無礙光佛。",
        "from": "T19n0933_001:0079c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六真理智三明無對光佛。",
        "from": "T19n0933_001:0079c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色善三明光炎王光佛。",
        "from": "T19n0933_001:0079c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一覺三明清淨光佛。",
        "from": "T19n0933_001:0079c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普門三明歡喜光佛。",
        "from": "T19n0933_001:0079c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入慧三明智慧光佛。",
        "from": "T19n0933_001:0079c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光色三明不斷光佛。",
        "from": "T19n0933_001:0079c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明達三明難思光佛。",
        "from": "T19n0933_001:0079c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五德三明無稱光佛。",
        "from": "T19n0933_001:0079c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智力三明超日月光佛。",
        "from": "T19n0933_001:0079c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是諸佛如來是真色具足。",
        "from": "T19n0933_001:0079c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切三世如來悲相所依。",
        "from": "T19n0933_001:0079c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有眾生欲往生如是九品淨土。",
        "from": "T19n0933_001:0079c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉視十二圓妙。",
        "from": "T19n0933_001:0079c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日夜三時稱如是九品淨土名。",
        "from": "T19n0933_001:0079c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讚十二光佛號。",
        "from": "T19n0933_001:0079c21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即永出三界火宅。",
        "from": "T19n0933_001:0079c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定生真如。",
        "from": "T19n0933_001:0079c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離有漏永入無漏。",
        "from": "T19n0933_001:0079c22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人欲入如是三摩地境。",
        "from": "T19n0933_001:0079c22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足佛慧淨心潔身。",
        "from": "T19n0933_001:0079c23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀念大三摩耶實相真言。",
        "from": "T19n0933_001:0079c23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T19n0933_001:0079c24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿密[口*栗]哆帝濟伽藍吽",
        "from": "T19n0933_001:0079c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子如是真言梵呪。",
        "from": "T19n0933_001:0079c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此坐一切十方三世諸佛如來究竟理智。",
        "from": "T19n0933_001:0079c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二空願根本。",
        "from": "T19n0933_001:0079c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若我弟子欲利樂三界饒益人天。",
        "from": "T19n0933_001:0079c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書寫此經受持讀誦者增長福樂。",
        "from": "T19n0933_001:0079c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增益智慧辨才增長壽命色力。",
        "from": "T19n0933_001:0079c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "消除業障消滅萬病。",
        "from": "T19n0933_001:0080a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增長悲愛無不具足色善。",
        "from": "T19n0933_001:0080a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況至心書寫讀誦者。",
        "from": "T19n0933_001:0080a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "度三世無生變三界火宅。",
        "from": "T19n0933_001:0080a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所誦得往生極樂界會九品淨土。",
        "from": "T19n0933_001:0080a03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時大眾聞佛所說。",
        "from": "T19n0933_001:0080a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆大歡喜信受奉行。",
        "from": "T19n0933_001:0080a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九品往生阿彌陀經",
        "from": "T19n0933_001:0080a05_0",
        "context_with_next_sentence": false
    }
]