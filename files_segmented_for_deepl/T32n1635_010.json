[
    {
        "source_sentence": "大乘寶要義論卷第十",
        "from": "T32n1635_010:0073a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譯經三藏朝散大夫試鴻臚少卿",
        "from": "T32n1635_010:0073a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光梵大師賜紫沙門臣惟淨等",
        "from": "T32n1635_010:0073a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉　詔譯",
        "from": "T32n1635_010:0073a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如華嚴經云。",
        "from": "T32n1635_010:0073a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法界行願分位宣說菩薩無盡佛種世界世尊普遍智境界音聲如來。",
        "from": "T32n1635_010:0073a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從彼剎中來至世尊釋迦牟尼如來之前。",
        "from": "T32n1635_010:0073a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出現菩薩威德身相。",
        "from": "T32n1635_010:0073a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂一切身分。",
        "from": "T32n1635_010:0073a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切相好。",
        "from": "T32n1635_010:0073a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切毛孔。",
        "from": "T32n1635_010:0073a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切莊嚴。",
        "from": "T32n1635_010:0073a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切衣服及緣具等。",
        "from": "T32n1635_010:0073a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切菩薩眷屬具足所有世尊大毘盧遮那如來。",
        "from": "T32n1635_010:0073a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前際中住過去一切如來。",
        "from": "T32n1635_010:0073a12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後際中住若已受記及未受記未來一切如來。",
        "from": "T32n1635_010:0073a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現在十方一切佛剎普遍安住一切菩薩。",
        "from": "T32n1635_010:0073a14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔所修習布施波羅蜜多相應先行海。",
        "from": "T32n1635_010:0073a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切受者及所施物皆悉影現。",
        "from": "T32n1635_010:0073a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切身相一切毛孔諸相好等。",
        "from": "T32n1635_010:0073a17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦悉對現而為表示。",
        "from": "T32n1635_010:0073a17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復身諸分位及語言道。",
        "from": "T32n1635_010:0073a18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切衣飾眾緣具等。",
        "from": "T32n1635_010:0073a18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而亦對現一一表示。",
        "from": "T32n1635_010:0073a19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又昔修習持戒波羅蜜多相應先行海。",
        "from": "T32n1635_010:0073a19_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又諸忍辱波羅蜜多示現割截身諸分位相應先行海。",
        "from": "T32n1635_010:0073a20_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又諸菩薩加行精進相應先行海。",
        "from": "T32n1635_010:0073a21_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又諸如來廣大靜慮辦事相應先行海。",
        "from": "T32n1635_010:0073a22_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又諸如來轉大法輪法事成辦捨諸所有自在身相諸門影現一一表示相應先行海。",
        "from": "T32n1635_010:0073a23_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又諸如來喜見諸菩薩道一切世間最上愛樂相應先行海。",
        "from": "T32n1635_010:0073a25_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又諸菩薩諸勝願海增上法門廣大莊嚴相應先行海。",
        "from": "T32n1635_010:0073a26_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又諸菩薩力波羅蜜多諸行成辦清淨相應先行海。",
        "from": "T32n1635_010:0073a28_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又諸菩薩廣大法界一切神變雲廣大所作諸菩薩智境界相應先行海乘。",
        "from": "T32n1635_010:0073a29_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等十波羅蜜多影現表示。",
        "from": "T32n1635_010:0073b01_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣大法界一切廣大諸神變雲來詣佛所。",
        "from": "T32n1635_010:0073b02_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是菩薩并其眷屬頂禮佛已。",
        "from": "T32n1635_010:0073b03_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住於上方。",
        "from": "T32n1635_010:0073b04_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化現一切金剛帝寶心莊嚴藏大樓閣中金剛帝青蓮華師子座上加趺而坐。",
        "from": "T32n1635_010:0073b04_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現諸寶焰摩尼王網彌覆其上。",
        "from": "T32n1635_010:0073b06_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復以三世一切如來音聲寶王纓珞垂掛莊嚴。",
        "from": "T32n1635_010:0073b06_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂戴摩尼寶冠。",
        "from": "T32n1635_010:0073b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現菩薩身加持而住。",
        "from": "T32n1635_010:0073b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而此菩薩并其眷屬。",
        "from": "T32n1635_010:0073b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆從普賢菩薩行願出生。",
        "from": "T32n1635_010:0073b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切如來足根門中表現清淨。",
        "from": "T32n1635_010:0073b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智眼所觀聞一切如來法輪經中理法音聲勝海。",
        "from": "T32n1635_010:0073b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普攝一切菩薩得諸自在。",
        "from": "T32n1635_010:0073b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及得最上波羅蜜多。",
        "from": "T32n1635_010:0073b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來皆悉雲集。",
        "from": "T32n1635_010:0073b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於剎那剎那中廣現神變。",
        "from": "T32n1635_010:0073b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出現諸有情身廣大境界。",
        "from": "T32n1635_010:0073b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來眾會道場身中光明顯照一切世界。",
        "from": "T32n1635_010:0073b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一世界中普攝影現。",
        "from": "T32n1635_010:0073b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸境界相如微塵聚。",
        "from": "T32n1635_010:0073b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有一切有情善根成熟。",
        "from": "T32n1635_010:0073b16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應化度者而悉應時能來應現。",
        "from": "T32n1635_010:0073b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來轉法輪雲。",
        "from": "T32n1635_010:0073b17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切毛孔吼音境界。",
        "from": "T32n1635_010:0073b18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切世界普遍。",
        "from": "T32n1635_010:0073b18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切善根出生。",
        "from": "T32n1635_010:0073b19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生已即時諸有情身皆悉平等。",
        "from": "T32n1635_010:0073b19_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切方分雲海一心剎那廣現神變。",
        "from": "T32n1635_010:0073b20_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經又云。",
        "from": "T32n1635_010:0073b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩大威德力者。",
        "from": "T32n1635_010:0073b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時善財童子諦觀普賢菩薩身相。",
        "from": "T32n1635_010:0073b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃見菩薩一一身諸分位一一身諸分位分明。",
        "from": "T32n1635_010:0073b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一界體一一界體細分分明。",
        "from": "T32n1635_010:0073b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一身分各別分位。",
        "from": "T32n1635_010:0073b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一身分各別分位分明。",
        "from": "T32n1635_010:0073b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一依聚。",
        "from": "T32n1635_010:0073b25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一依聚分明。",
        "from": "T32n1635_010:0073b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一毛孔。",
        "from": "T32n1635_010:0073b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一毛孔分明。",
        "from": "T32n1635_010:0073b26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現此三千大千世界所有風火地界。",
        "from": "T32n1635_010:0073b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并大海洲渚江河寶山。",
        "from": "T32n1635_010:0073b27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及須彌山鐵圍山等國土城邑聚落方處樹林舍宅。",
        "from": "T32n1635_010:0073b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并諸人民地獄餓鬼畜生之趣。",
        "from": "T32n1635_010:0073b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿修羅界龍界迦樓羅界。",
        "from": "T32n1635_010:0073b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人界天界及其梵界。",
        "from": "T32n1635_010:0073c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲界色界無色界。",
        "from": "T32n1635_010:0073c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸境界。",
        "from": "T32n1635_010:0073c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉以神力加持而住。",
        "from": "T32n1635_010:0073c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至形色顯色雲電星宿。",
        "from": "T32n1635_010:0073c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晝夜圓半月時年分。",
        "from": "T32n1635_010:0073c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住劫壞劫。",
        "from": "T32n1635_010:0073c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現如是等此世界相已。",
        "from": "T32n1635_010:0073c04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有東方一切世界亦如是觀。",
        "from": "T32n1635_010:0073c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南西北方四維上下一切世界。",
        "from": "T32n1635_010:0073c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如應對現所觀亦然。",
        "from": "T32n1635_010:0073c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又諸佛降生眾會道場。",
        "from": "T32n1635_010:0073c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及有情等所有此娑婆世界。",
        "from": "T32n1635_010:0073c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前際中住過去。",
        "from": "T32n1635_010:0073c07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切世界一切互相。",
        "from": "T32n1635_010:0073c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆見普賢菩薩一一身中大人之相。",
        "from": "T32n1635_010:0073c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并諸佛降生一切菩薩眾會道場。",
        "from": "T32n1635_010:0073c09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及有情舍宅晝夜劫數等。",
        "from": "T32n1635_010:0073c10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是後際中住未來一切世界佛剎等普遍一切亦然。",
        "from": "T32n1635_010:0073c10_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆見如此娑婆世界前際後際中住一切世界一切互相見諸影相。",
        "from": "T32n1635_010:0073c12_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是十方前際後際中住一切世界。",
        "from": "T32n1635_010:0073c13_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切皆見普賢菩薩一一身中大人之相一一毛孔中一一分明互相無雜。",
        "from": "T32n1635_010:0073c14_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆見普賢菩薩在世尊大毘盧遮那如來之前坐於大寶蓮華藏師子座上現游戲神通。",
        "from": "T32n1635_010:0073c16_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時此東方蓮華吉祥世界賢吉祥如來。",
        "from": "T32n1635_010:0073c18_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見此所現游戲神通。",
        "from": "T32n1635_010:0073c18_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東方如是普遍十方一切世界亦然。",
        "from": "T32n1635_010:0073c19_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆見一切如來足根門中各有普賢菩薩坐於大寶蓮華藏師子座上現是游戲神通。",
        "from": "T32n1635_010:0073c20_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如其十方一切世界悉見一切如來足根門中有普賢菩薩坐於大寶蓮華藏師子座上現是游戲神通。",
        "from": "T32n1635_010:0073c22_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時如是普遍十方一切佛剎如微塵數。",
        "from": "T32n1635_010:0073c24_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一微塵數皆是廣大法界佛會道場。",
        "from": "T32n1635_010:0073c25_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉見一切如來足根門中有普賢菩薩坐於大寶蓮華藏師子座上現是游戲神通。",
        "from": "T32n1635_010:0073c26_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時一一身相悉得三世一切所緣影相表示。",
        "from": "T32n1635_010:0073c28_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有一切剎土一切有情諸佛降生諸菩薩眾會道場影現表示。",
        "from": "T32n1635_010:0073c29_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時普聞一切有情語言。",
        "from": "T32n1635_010:0074a01_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切佛音聲一切如來轉大法輪一切宣說教授神通變化一切菩薩集會諸佛游戲境相音聲此等總略如其所說。",
        "from": "T32n1635_010:0074a02_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知皆是菩薩摩訶薩大威德力。",
        "from": "T32n1635_010:0074a04_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中應問。",
        "from": "T32n1635_010:0074a05_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等是如來大威德力。",
        "from": "T32n1635_010:0074a05_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如菩薩十地經云。",
        "from": "T32n1635_010:0074a06_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解脫月菩薩問金剛藏菩薩言。",
        "from": "T32n1635_010:0074a06_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛子。",
        "from": "T32n1635_010:0074a07_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸菩薩行境界如是。",
        "from": "T32n1635_010:0074a07_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持神力尚復無量。",
        "from": "T32n1635_010:0074a07_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何能入如來行境界邪。",
        "from": "T32n1635_010:0074a08_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛藏菩薩言。",
        "from": "T32n1635_010:0074a08_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛子。",
        "from": "T32n1635_010:0074a09_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如有人於四大洲世界之中取小石塊。",
        "from": "T32n1635_010:0074a09_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若二若三如豆之量。",
        "from": "T32n1635_010:0074a10_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是言。",
        "from": "T32n1635_010:0074a10_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大地世界頗有過於此邪。",
        "from": "T32n1635_010:0074a11_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或無量邪。",
        "from": "T32n1635_010:0074a11_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是語者。",
        "from": "T32n1635_010:0074a11_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如汝今時以諸如來應供正等正覺無量智法與菩薩法而相等比。",
        "from": "T32n1635_010:0074a12_54",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛子。",
        "from": "T32n1635_010:0074a13_55",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四大洲世界如所取者豆量石塊。",
        "from": "T32n1635_010:0074a13_56",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而極少分。",
        "from": "T32n1635_010:0074a14_57",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘無有量。",
        "from": "T32n1635_010:0074a14_58",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來境界亦復如是。",
        "from": "T32n1635_010:0074a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且法雲地菩薩所有功德。",
        "from": "T32n1635_010:0074a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經無量劫猶尚不能說一少分。",
        "from": "T32n1635_010:0074a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況如來無量智境。",
        "from": "T32n1635_010:0074a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛子。",
        "from": "T32n1635_010:0074a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今實言告汝。",
        "from": "T32n1635_010:0074a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於如來前為我作證。",
        "from": "T32n1635_010:0074a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "假使十方無邊世界如微塵等一切佛剎證菩薩地者。",
        "from": "T32n1635_010:0074a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其數充滿。",
        "from": "T32n1635_010:0074a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如稻麻竹[竺二+韋]甘蔗叢林。",
        "from": "T32n1635_010:0074a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼等菩薩經無邊劫。",
        "from": "T32n1635_010:0074a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宣說開示如來所有一智境界。",
        "from": "T32n1635_010:0074a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百分不及一。",
        "from": "T32n1635_010:0074a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至俱胝那由他百千及譬喻分亦不及一。",
        "from": "T32n1635_010:0074a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入如來功德智不思議境界經云。",
        "from": "T32n1635_010:0074a22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言。",
        "from": "T32n1635_010:0074a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙吉祥。",
        "from": "T32n1635_010:0074a23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此會中或有有情。",
        "from": "T32n1635_010:0074a23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂佛如來初始出家。",
        "from": "T32n1635_010:0074a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或謂久已出家。",
        "from": "T32n1635_010:0074a24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或謂修諸苦行。",
        "from": "T32n1635_010:0074a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或坐菩提場成等正覺。",
        "from": "T32n1635_010:0074a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或破諸魔軍。",
        "from": "T32n1635_010:0074a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或梵王帝釋護世勸請轉大法輪。",
        "from": "T32n1635_010:0074a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或聞宣說聲聞乘法。",
        "from": "T32n1635_010:0074a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或說緣覺乘法。",
        "from": "T32n1635_010:0074a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或說大乘之法。",
        "from": "T32n1635_010:0074a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或覩佛身高一尋量。",
        "from": "T32n1635_010:0074a28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或見佛身高一俱盧舍。",
        "from": "T32n1635_010:0074a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或見佛身高顯過諸由旬百千數量。",
        "from": "T32n1635_010:0074a29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或見佛身如真金色。",
        "from": "T32n1635_010:0074a29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至或見如摩尼寶色。",
        "from": "T32n1635_010:0074b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或見如來寂靜身相。",
        "from": "T32n1635_010:0074b01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或見如來入涅槃相。",
        "from": "T32n1635_010:0074b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或見如來一聚身相。",
        "from": "T32n1635_010:0074b02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或見分布如來舍利。",
        "from": "T32n1635_010:0074b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或謂十年成等正覺。",
        "from": "T32n1635_010:0074b03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或謂十年入大涅槃。",
        "from": "T32n1635_010:0074b04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或謂有於世尊釋迦牟尼如來教中得圓滿者。",
        "from": "T32n1635_010:0074b04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或隱沒者。",
        "from": "T32n1635_010:0074b05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或謂若十二十三十四十。",
        "from": "T32n1635_010:0074b06_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至百千俱胝那由他劫入大涅槃。",
        "from": "T32n1635_010:0074b06_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或謂世尊釋迦牟尼如來。",
        "from": "T32n1635_010:0074b07_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於不可說不可說劫成等正覺。",
        "from": "T32n1635_010:0074b08_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙吉祥。",
        "from": "T32n1635_010:0074b08_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於如是等差別之相。",
        "from": "T32n1635_010:0074b08_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來悉非分別非離分別。",
        "from": "T32n1635_010:0074b09_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而佛如來以無發悟無分別法。",
        "from": "T32n1635_010:0074b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常隨有情心作諸行相。",
        "from": "T32n1635_010:0074b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩十住經云。",
        "from": "T32n1635_010:0074b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言。",
        "from": "T32n1635_010:0074b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙吉祥。",
        "from": "T32n1635_010:0074b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有多種說者。",
        "from": "T32n1635_010:0074b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如有大池縱廣正等五十萬由旬。",
        "from": "T32n1635_010:0074b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "池岸平坦池水清甘。",
        "from": "T32n1635_010:0074b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有蓮華遍覆池內。",
        "from": "T32n1635_010:0074b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若或有人。",
        "from": "T32n1635_010:0074b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以鐵所成千輻輪車而以駿馬。",
        "from": "T32n1635_010:0074b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勝迦樓羅迅疾勢者而駕馭之。",
        "from": "T32n1635_010:0074b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其車行得不著池水。",
        "from": "T32n1635_010:0074b15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬跡亦復不踐蓮華。",
        "from": "T32n1635_010:0074b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙吉祥。",
        "from": "T32n1635_010:0074b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來亦復如是。",
        "from": "T32n1635_010:0074b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘於大車游彼大池中。",
        "from": "T32n1635_010:0074b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有毒蛇鼓水騰踊。",
        "from": "T32n1635_010:0074b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若剎那間。",
        "from": "T32n1635_010:0074b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而彼毒蛇隨車後轉。",
        "from": "T32n1635_010:0074b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其車即時七返右旋。",
        "from": "T32n1635_010:0074b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復毒蛇隨彼大車一返右旋。",
        "from": "T32n1635_010:0074b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其車即時右旋無數。",
        "from": "T32n1635_010:0074b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如阿難等諸大苾芻說法亦然。",
        "from": "T32n1635_010:0074b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有時阿難說十種法表示其義。",
        "from": "T32n1635_010:0074b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復阿難說一種法即時舍利子。",
        "from": "T32n1635_010:0074b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說千種法。",
        "from": "T32n1635_010:0074b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "表示其義。",
        "from": "T32n1635_010:0074b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復舍利子說一種法。",
        "from": "T32n1635_010:0074b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即時大目乾連過八萬世界。",
        "from": "T32n1635_010:0074b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復大目乾連過一世界。",
        "from": "T32n1635_010:0074b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即時如來於十方法界。",
        "from": "T32n1635_010:0074b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最上自在越虛空際。",
        "from": "T32n1635_010:0074b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普遍一切世界海。",
        "from": "T32n1635_010:0074b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一世界。",
        "from": "T32n1635_010:0074b26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一洲渚。",
        "from": "T32n1635_010:0074b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一凡夫道中。",
        "from": "T32n1635_010:0074b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "示現從兜率天宮沒下降人間入胎住胎出胎。",
        "from": "T32n1635_010:0074b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵王帝釋承接沐浴莊嚴。",
        "from": "T32n1635_010:0074b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周行七步觀察十方作師子吼。",
        "from": "T32n1635_010:0074b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "習學一切工巧伎藝明論事業現處太子之位。",
        "from": "T32n1635_010:0074b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於王宮中嬉戲娛樂游賞園林。",
        "from": "T32n1635_010:0074c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出家苦行。",
        "from": "T32n1635_010:0074c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食乳糜粥已。",
        "from": "T32n1635_010:0074c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詣菩提場坐成正覺。",
        "from": "T32n1635_010:0074c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摧伏魔軍觀樹經行。",
        "from": "T32n1635_010:0074c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大梵天王請轉法輪。",
        "from": "T32n1635_010:0074c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入忉利天宮等。",
        "from": "T32n1635_010:0074c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國土分量劫名次第。",
        "from": "T32n1635_010:0074c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "威德身相壽量多少。",
        "from": "T32n1635_010:0074c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾會莊嚴佛剎清淨。",
        "from": "T32n1635_010:0074c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教法莊嚴發菩提心。",
        "from": "T32n1635_010:0074c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修諸行願成波羅蜜多。",
        "from": "T32n1635_010:0074c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓滿諸地神通智忍。",
        "from": "T32n1635_010:0074c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具總持三摩地諸解脫門。",
        "from": "T32n1635_010:0074c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來無量。",
        "from": "T32n1635_010:0074c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸供養事無量。",
        "from": "T32n1635_010:0074c08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來及菩薩法境界無量。",
        "from": "T32n1635_010:0074c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法雲廣大所行分量。",
        "from": "T32n1635_010:0074c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成熟有情方便分位差別之量。",
        "from": "T32n1635_010:0074c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起大游戲神通變化。",
        "from": "T32n1635_010:0074c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "示現一乘大般涅槃。",
        "from": "T32n1635_010:0074c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分布舍利分別教法。",
        "from": "T32n1635_010:0074c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住時久近及法滅時。",
        "from": "T32n1635_010:0074c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆從如是大法池中之所出現。",
        "from": "T32n1635_010:0074c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復於諸外道所修行處。",
        "from": "T32n1635_010:0074c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及一切有情所應作事。",
        "from": "T32n1635_010:0074c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來以無發悟無分別心。",
        "from": "T32n1635_010:0074c13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同時悉現於後邊際究竟分位。",
        "from": "T32n1635_010:0074c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勤勇示現。",
        "from": "T32n1635_010:0074c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡如是等剎那俱時十方一切一一自身毛孔之中。",
        "from": "T32n1635_010:0074c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有遍諸方分三世一切如來及諸菩薩眾會之海。",
        "from": "T32n1635_010:0074c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣大一切佛剎功德莊嚴。",
        "from": "T32n1635_010:0074c17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸有情聚舍宅宮室廣大莊嚴。",
        "from": "T32n1635_010:0074c18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸有情聚眼耳鼻舌身意處廣大施設。",
        "from": "T32n1635_010:0074c19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸菩薩行皆悉積集廣大莊嚴。",
        "from": "T32n1635_010:0074c19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來境界廣大莊嚴。",
        "from": "T32n1635_010:0074c20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等相如來俱時以無發悟無分別心普遍示現。",
        "from": "T32n1635_010:0074c21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於後邊際究竟勤勇剎那俱時。",
        "from": "T32n1635_010:0074c22_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普攝一切十方世界。",
        "from": "T32n1635_010:0074c23_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至凡夫道中有盡無盡無復遺餘。",
        "from": "T32n1635_010:0074c23_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸有情界一切有情一一有情各各身相形色顯色音聲語言譬喻說法。",
        "from": "T32n1635_010:0074c24_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡如是等分量差別。",
        "from": "T32n1635_010:0074c26_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來俱時以無發悟無分別心普遍示現。",
        "from": "T32n1635_010:0074c26_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨諸有情別別心意。",
        "from": "T32n1635_010:0074c27_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於後邊際究竟勤勇。",
        "from": "T32n1635_010:0074c27_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨應表示隨所施作。",
        "from": "T32n1635_010:0074c28_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙吉祥。",
        "from": "T32n1635_010:0074c28_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如白月十五中夜之時。",
        "from": "T32n1635_010:0074c29_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍閻浮提一切男子女人童男童女。",
        "from": "T32n1635_010:0074c29_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各各現前觀月輪相。",
        "from": "T32n1635_010:0075a01_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而彼月輪悉無分別。",
        "from": "T32n1635_010:0075a01_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來亦復如是。",
        "from": "T32n1635_010:0075a02_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以無發悟無分別心作諸行相。",
        "from": "T32n1635_010:0075a02_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由成就不共佛法故。",
        "from": "T32n1635_010:0075a03_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨有情心意隨應化度。",
        "from": "T32n1635_010:0075a03_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切有情各各現前普觀如來。",
        "from": "T32n1635_010:0075a04_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而如來心亦無分別。",
        "from": "T32n1635_010:0075a05_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故當知。",
        "from": "T32n1635_010:0075a05_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有分別若無分別。",
        "from": "T32n1635_010:0075a05_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛如來悉無發悟。",
        "from": "T32n1635_010:0075a06_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而成就不共佛法故。",
        "from": "T32n1635_010:0075a06_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨應施作一切行相。",
        "from": "T32n1635_010:0075a07_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中應知。",
        "from": "T32n1635_010:0075a07_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸廣大甚。",
        "from": "T32n1635_010:0075a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深經。",
        "from": "T32n1635_010:0075a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中而獨顯。",
        "from": "T32n1635_010:0075a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說諸佛菩薩大威德力。",
        "from": "T32n1635_010:0075a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人。",
        "from": "T32n1635_010:0075a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能深生信解書寫讀誦。",
        "from": "T32n1635_010:0075a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊重供養者。",
        "from": "T32n1635_010:0075a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人獲福廣大無量。",
        "from": "T32n1635_010:0075a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經又云。",
        "from": "T32n1635_010:0075a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言。",
        "from": "T32n1635_010:0075a10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙吉祥。",
        "from": "T32n1635_010:0075a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如須彌山王勝出眾山。",
        "from": "T32n1635_010:0075a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯照殊妙。",
        "from": "T32n1635_010:0075a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦高亦廣而復最大。",
        "from": "T32n1635_010:0075a11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙吉祥。",
        "from": "T32n1635_010:0075a12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若菩薩信解此正法者亦復如是。",
        "from": "T32n1635_010:0075a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勝出一切菩薩摩訶薩。",
        "from": "T32n1635_010:0075a13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以十方一切世界。",
        "from": "T32n1635_010:0075a13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如微塵等諸菩薩眾。",
        "from": "T32n1635_010:0075a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於此正法生信解者。",
        "from": "T32n1635_010:0075a14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有五波羅蜜多出生善根。",
        "from": "T32n1635_010:0075a15_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於阿僧祇劫中而能積集。",
        "from": "T32n1635_010:0075a15_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯照殊妙亦高亦廣而復最大。",
        "from": "T32n1635_010:0075a16_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙吉祥。",
        "from": "T32n1635_010:0075a17_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人。",
        "from": "T32n1635_010:0075a17_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有十方一切世界一切有情。",
        "from": "T32n1635_010:0075a17_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而悉勸令發菩提心。",
        "from": "T32n1635_010:0075a18_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有善男子善女人。",
        "from": "T32n1635_010:0075a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信樂無餘依涅槃者。",
        "from": "T32n1635_010:0075a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此所獲福。",
        "from": "T32n1635_010:0075a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比倍於前不可數量。",
        "from": "T32n1635_010:0075a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙吉祥。",
        "from": "T32n1635_010:0075a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正使善男子善女人普令一切有情信樂無餘依涅槃。",
        "from": "T32n1635_010:0075a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復善男子善女人。",
        "from": "T32n1635_010:0075a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此正法能生信樂。",
        "from": "T32n1635_010:0075a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若自書寫若令他書寫。",
        "from": "T32n1635_010:0075a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若自讀誦若令他讀誦。",
        "from": "T32n1635_010:0075a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至但能信樂受持。",
        "from": "T32n1635_010:0075a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以香華燈塗等諸供具而供養之。",
        "from": "T32n1635_010:0075a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此所獲福。",
        "from": "T32n1635_010:0075a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比倍於前不可稱數。",
        "from": "T32n1635_010:0075a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大乘寶要義論卷第十",
        "from": "T32n1635_010:0075a26_0",
        "context_with_next_sentence": false
    }
]