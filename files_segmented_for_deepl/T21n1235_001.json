[
    {
        "source_sentence": "無能勝大明心陀羅尼經",
        "from": "T21n1235_001:0176a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試鴻臚少卿",
        "from": "T21n1235_001:0176a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳教大師臣法天奉　詔譯",
        "from": "T21n1235_001:0176a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　歸命無上覺　　及法聖賢眾",
        "from": "T21n1235_001:0176a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　大明心真言　　無能勝自說",
        "from": "T21n1235_001:0176a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來應正等覺亦說此明。",
        "from": "T21n1235_001:0176a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利益一切眾生。",
        "from": "T21n1235_001:0176a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就一切吉祥事業。",
        "from": "T21n1235_001:0176a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "消除重罪而無中夭。",
        "from": "T21n1235_001:0176a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽命延長遠離眾病。",
        "from": "T21n1235_001:0176a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人童男童女。",
        "from": "T21n1235_001:0176b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此經中恭敬尊重。",
        "from": "T21n1235_001:0176b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有人及非人懷惡心者。",
        "from": "T21n1235_001:0176b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅剎娑烏娑多(二合)羅迦餓鬼。",
        "from": "T21n1235_001:0176b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塞建陀阿鉢娑麼(二合)囉玉失者(二合)迦麼怛囉(二合)部多鬼魅。",
        "from": "T21n1235_001:0176b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸惡之眾所作邪法。",
        "from": "T21n1235_001:0176b05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪詛有情生種種界。",
        "from": "T21n1235_001:0176b05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受飢餓病及一切惡病。",
        "from": "T21n1235_001:0176b06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瘡癤癰腫疥癩疣贅。",
        "from": "T21n1235_001:0176b06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等苦而不能害。",
        "from": "T21n1235_001:0176b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有宿殃罪業。",
        "from": "T21n1235_001:0176b07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更相殺害鬪訟喧諍。",
        "from": "T21n1235_001:0176b07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及王法禁繫水火賊難。",
        "from": "T21n1235_001:0176b08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是諸難逼惱之時。",
        "from": "T21n1235_001:0176b08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憶念讀誦決定消散。",
        "from": "T21n1235_001:0176b09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離其災害吉祥安樂。",
        "from": "T21n1235_001:0176b09_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人。",
        "from": "T21n1235_001:0176b10_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或以樺皮繒帛紙上書寫此明。",
        "from": "T21n1235_001:0176b10_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於餘處書寫供養。",
        "from": "T21n1235_001:0176b11_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或戴頂上或戴項上。",
        "from": "T21n1235_001:0176b11_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或臂上手上或帶衣裏。",
        "from": "T21n1235_001:0176b12_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或結為鬘戴頭髻中。",
        "from": "T21n1235_001:0176b12_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是受持永無災難恒得安樂。",
        "from": "T21n1235_001:0176b13_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若常清旦潔淨身心志誠讀誦。",
        "from": "T21n1235_001:0176b13_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若捨命已於七生中得宿命智。",
        "from": "T21n1235_001:0176b14_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身相端嚴恒得快樂。",
        "from": "T21n1235_001:0176b15_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戒行具足。",
        "from": "T21n1235_001:0176b15_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口中常出憂鉢羅華香。",
        "from": "T21n1235_001:0176b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所言誠諦眾人愛重。",
        "from": "T21n1235_001:0176b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復得生於三十三天。",
        "from": "T21n1235_001:0176b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此真言依法想念必得成就。",
        "from": "T21n1235_001:0176b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說陀羅尼曰。",
        "from": "T21n1235_001:0176b18_2",
        "context_with_next_sentence": true
    },
