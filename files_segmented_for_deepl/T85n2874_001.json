[
    {
        "source_sentence": "佛說小法滅盡經",
        "from": "T85n2874_001:1358c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞如是。",
        "from": "T85n2874_001:1358c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在拘夷那竭國。",
        "from": "T85n2874_001:1358c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來三月當般泥洹。",
        "from": "T85n2874_001:1358c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與諸比丘及諸尊菩薩無央數來詣佛所。",
        "from": "T85n2874_001:1358c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稽首于地眷屬圍繞渴仰聞法。",
        "from": "T85n2874_001:1359a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊寂然默無所說。",
        "from": "T85n2874_001:1359a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光明不亮。",
        "from": "T85n2874_001:1359a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢者阿難長跪叉手白佛言。",
        "from": "T85n2874_001:1359a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後說法啟光獨顯。",
        "from": "T85n2874_001:1359a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今大眾會明更不現。",
        "from": "T85n2874_001:1359a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何故如此。",
        "from": "T85n2874_001:1359a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此必有異。",
        "from": "T85n2874_001:1359a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願聞其意。",
        "from": "T85n2874_001:1359a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛默不應。",
        "from": "T85n2874_001:1359a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是至三。",
        "from": "T85n2874_001:1359a05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告賢者阿難。",
        "from": "T85n2874_001:1359a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾般泥洹後。",
        "from": "T85n2874_001:1359a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五逆濁世魔道興盛。",
        "from": "T85n2874_001:1359a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魔作比丘壞亂吾道。",
        "from": "T85n2874_001:1359a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著俗衣裳樂好袈裟五色之服。",
        "from": "T85n2874_001:1359a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲酒炙肉。",
        "from": "T85n2874_001:1359a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殺生貪味。",
        "from": "T85n2874_001:1359a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有慈心。",
        "from": "T85n2874_001:1359a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更相憎嫉。",
        "from": "T85n2874_001:1359a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時有菩薩辟支羅漢比丘。",
        "from": "T85n2874_001:1359a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精進修德。",
        "from": "T85n2874_001:1359a09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切敬待人所宗何。",
        "from": "T85n2874_001:1359a10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教化平等。",
        "from": "T85n2874_001:1359a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憐貧念老救濟窮厄。",
        "from": "T85n2874_001:1359a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒以經像令人奉事。",
        "from": "T85n2874_001:1359a11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作諸功德。",
        "from": "T85n2874_001:1359a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "志性恩善。",
        "from": "T85n2874_001:1359a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不傷害人。",
        "from": "T85n2874_001:1359a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "損身濟物。",
        "from": "T85n2874_001:1359a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不自惜已。",
        "from": "T85n2874_001:1359a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設有是人。",
        "from": "T85n2874_001:1359a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾魔比丘咸共憎嫉。",
        "from": "T85n2874_001:1359a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誹謗揚惡。",
        "from": "T85n2874_001:1359a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儐出驅遣不令得住。",
        "from": "T85n2874_001:1359a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自共於後不修道德。",
        "from": "T85n2874_001:1359a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寺廟空荒無人修理。",
        "from": "T85n2874_001:1359a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉就毀壞。",
        "from": "T85n2874_001:1359a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但貪錢物積聚不散。",
        "from": "T85n2874_001:1359a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不作功德。",
        "from": "T85n2874_001:1359a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "販賣奴婢耕田墾殖。",
        "from": "T85n2874_001:1359a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "焚燒山林。",
        "from": "T85n2874_001:1359a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傷害眾生無有慈愍。",
        "from": "T85n2874_001:1359a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奴為比丘。",
        "from": "T85n2874_001:1359a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婢為比丘尼。",
        "from": "T85n2874_001:1359a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有道德。",
        "from": "T85n2874_001:1359a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婬妷濁亂男女不別。",
        "from": "T85n2874_001:1359a18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令道薄賤。",
        "from": "T85n2874_001:1359a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆由此輩。",
        "from": "T85n2874_001:1359a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或避縣官依猗吾道求作沙門。",
        "from": "T85n2874_001:1359a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不修戒律。",
        "from": "T85n2874_001:1359a20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月半月盡卦名講戒。",
        "from": "T85n2874_001:1359a20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "厭惓懈怠不欲聽聞。",
        "from": "T85n2874_001:1359a21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "抄略前後不肯盡說。",
        "from": "T85n2874_001:1359a21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經不講讀。",
        "from": "T85n2874_001:1359a22_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設有讀者不識字句。",
        "from": "T85n2874_001:1359a22_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為強言是不諮明者。",
        "from": "T85n2874_001:1359a22_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貢高求名噓天雅步以為勞。",
        "from": "T85n2874_001:1359a23_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冀望人供養眾魔比丘。",
        "from": "T85n2874_001:1359a23_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽終之後精神當墮無澤地獄。",
        "from": "T85n2874_001:1359a24_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五逆罪中無不更歷。",
        "from": "T85n2874_001:1359a25_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒邊沙數罪畢。",
        "from": "T85n2874_001:1359a25_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃出生在邊國無三寶處。",
        "from": "T85n2874_001:1359a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法欲滅時。",
        "from": "T85n2874_001:1359a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人女精懃恒作功德。",
        "from": "T85n2874_001:1359a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "男子懈慢不用法語。",
        "from": "T85n2874_001:1359a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼見沙門如視糞土。",
        "from": "T85n2874_001:1359a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有信心劫欲彌沒。",
        "from": "T85n2874_001:1359a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天泣淚水旱不調。",
        "from": "T85n2874_001:1359a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五穀不熟疫氣流行。",
        "from": "T85n2874_001:1359a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "死亡者眾。",
        "from": "T85n2874_001:1359b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人民懃苦。",
        "from": "T85n2874_001:1359b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縣官[革*占]剋不順法理。",
        "from": "T85n2874_001:1359b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆思樂亂。",
        "from": "T85n2874_001:1359b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡人轉多如海中沙。",
        "from": "T85n2874_001:1359b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劫欲盡故日月短促善者甚少。",
        "from": "T85n2874_001:1359b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一若二。",
        "from": "T85n2874_001:1359b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人命促短。",
        "from": "T85n2874_001:1359b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四十頭白。",
        "from": "T85n2874_001:1359b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "男子婬妷精盡夭命。",
        "from": "T85n2874_001:1359b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年壽六十。",
        "from": "T85n2874_001:1359b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "女人壽命七八九十或至百歲。",
        "from": "T85n2874_001:1359b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大水忽起年至無期。",
        "from": "T85n2874_001:1359b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世人不信。",
        "from": "T85n2874_001:1359b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不問豪賤沒溺浮漂。",
        "from": "T85n2874_001:1359b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魚鱉食噉菩薩辟支阿羅漢比丘。",
        "from": "T85n2874_001:1359b06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾魔驅遂不豫眾會。",
        "from": "T85n2874_001:1359b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三乘入山福德之處。",
        "from": "T85n2874_001:1359b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淡泊自守以為欣快。",
        "from": "T85n2874_001:1359b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽命延長諸天衛護。",
        "from": "T85n2874_001:1359b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月光出世得相遭值。",
        "from": "T85n2874_001:1359b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共興吾道五□□□□楞□□□□□未□□化去□□□□□□□□□□□□□□文。",
        "from": "T85n2874_001:1359b10_1",
        "context_with_next_sentence": false
    }
]