[
    {
        "source_sentence": " 先覺集序",
        "from": "X87n1619_001:0178c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "學佛然後知儒。",
        "from": "X87n1619_001:0178c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此碩儒之論也。",
        "from": "X87n1619_001:0178c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "古人與麼道。",
        "from": "X87n1619_001:0178c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即便與麼讚。",
        "from": "X87n1619_001:0178c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是誠先覺之公心。",
        "from": "X87n1619_001:0178c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今人不知佛之所以為佛。",
        "from": "X87n1619_001:0178c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儒之所以為儒。",
        "from": "X87n1619_001:0178c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不過分門別戶。",
        "from": "X87n1619_001:0178c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逐影循聲焉爾。",
        "from": "X87n1619_001:0178c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊云。",
        "from": "X87n1619_001:0178c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天上天下。",
        "from": "X87n1619_001:0178c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟我獨尊。",
        "from": "X87n1619_001:0178c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且道如何是我。",
        "from": "X87n1619_001:0178c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫子曰。",
        "from": "X87n1619_001:0178c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾道一以貫之。",
        "from": "X87n1619_001:0178c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且道如何是一。",
        "from": "X87n1619_001:0178c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "打頭一著。",
        "from": "X87n1619_001:0178c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能明了。",
        "from": "X87n1619_001:0178c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祇從形迹中論。",
        "from": "X87n1619_001:0178c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "枝葉上觀。",
        "from": "X87n1619_001:0178c13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈非門外歟。",
        "from": "X87n1619_001:0178c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "哲人達士。",
        "from": "X87n1619_001:0178c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "學有本據。",
        "from": "X87n1619_001:0178c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道有實悟。",
        "from": "X87n1619_001:0178c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世出世間。",
        "from": "X87n1619_001:0178c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初無二致。",
        "from": "X87n1619_001:0178c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自覺覺他。",
        "from": "X87n1619_001:0178c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "罔有差殊。",
        "from": "X87n1619_001:0178c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或現比丘比丘尼優婆塞優婆夷。",
        "from": "X87n1619_001:0178c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或現宰官長者婆羅門居士。",
        "from": "X87n1619_001:0178c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨其緣會。",
        "from": "X87n1619_001:0178c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所至即導利乎羣品。",
        "from": "X87n1619_001:0178c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "克垂範於後昆。",
        "from": "X87n1619_001:0178c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫是之謂先覺。",
        "from": "X87n1619_001:0178c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一機一境。",
        "from": "X87n1619_001:0178c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一話一言。",
        "from": "X87n1619_001:0178c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可以開闢人天正眼。",
        "from": "X87n1619_001:0178c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯揚儒佛心宗。",
        "from": "X87n1619_001:0178c18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先覺集之所由著也。",
        "from": "X87n1619_001:0178c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明潛陶公。",
        "from": "X87n1619_001:0178c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輯先覺集。",
        "from": "X87n1619_001:0178c19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而祇存居士者何。",
        "from": "X87n1619_001:0178c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓋緣建立維摩之院供其位。",
        "from": "X87n1619_001:0178c19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即考其入道之由。",
        "from": "X87n1619_001:0178c20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以與雲居和尚。",
        "from": "X87n1619_001:0178c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "暨同道諸君。",
        "from": "X87n1619_001:0178c20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "參詳訂證。",
        "from": "X87n1619_001:0178c21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而成此言。",
        "from": "X87n1619_001:0178c21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖未咸備。",
        "from": "X87n1619_001:0178c21_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已露一斑。",
        "from": "X87n1619_001:0178c21_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "厥功懋哉。",
        "from": "X87n1619_001:0178c21_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其中大意。",
        "from": "X87n1619_001:0178c22_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既宣之於中山碑記矣。",
        "from": "X87n1619_001:0178c22_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲以是集鑴登梨棗。",
        "from": "X87n1619_001:0178c22_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問敘於余。",
        "from": "X87n1619_001:0178c23_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更為申明其說。",
        "from": "X87n1619_001:0178c23_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旹",
        "from": "X87n1619_001:0178c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "康熈十八年歲在己未夏六月傳臨濟正宗三十二世道安靜和尚撰 先覺集序從上以來。",
        "from": "X87n1619_001:0179a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛祖聖賢。",
        "from": "X87n1619_001:0179a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸位菩薩。",
        "from": "X87n1619_001:0179a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說種種法。",
        "from": "X87n1619_001:0179a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "演種種教。",
        "from": "X87n1619_001:0179a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆是鏡中煙塵。",
        "from": "X87n1619_001:0179a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夢裏幻事。",
        "from": "X87n1619_001:0179a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本無實法繫綴于人。",
        "from": "X87n1619_001:0179a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以大覺世尊道。",
        "from": "X87n1619_001:0179a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾四十九年。",
        "from": "X87n1619_001:0179a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未曾說著一字。",
        "from": "X87n1619_001:0179a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是真實語。",
        "from": "X87n1619_001:0179a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又古人道。",
        "from": "X87n1619_001:0179a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大千沙界海中漚。",
        "from": "X87n1619_001:0179a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切聖賢如電拂。",
        "from": "X87n1619_001:0179a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "者裏何曾有佛有法。",
        "from": "X87n1619_001:0179a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有僧有俗。",
        "from": "X87n1619_001:0179a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有覺未覺。",
        "from": "X87n1619_001:0179a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不見道。",
        "from": "X87n1619_001:0179a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未離兜率。",
        "from": "X87n1619_001:0179a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已降王宮。",
        "from": "X87n1619_001:0179a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未出母胎。",
        "from": "X87n1619_001:0179a12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "度人已畢。",
        "from": "X87n1619_001:0179a12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斯乃先覺誌疏。",
        "from": "X87n1619_001:0179a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陶明潛。",
        "from": "X87n1619_001:0179a12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解得此義。",
        "from": "X87n1619_001:0179a13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纂先覺集。",
        "from": "X87n1619_001:0179a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲行於世。",
        "from": "X87n1619_001:0179a13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乞序于予。",
        "from": "X87n1619_001:0179a13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "予以從上古錐。",
        "from": "X87n1619_001:0179a13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掀翻一上。",
        "from": "X87n1619_001:0179a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "會得者作如是觀。",
        "from": "X87n1619_001:0179a14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不會者切忌向先覺集中絆倒。",
        "from": "X87n1619_001:0179a14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旹",
        "from": "X87n1619_001:0179a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "康熈丙寅佛成道日盤山青溝拙菴智朴撰 先覺集序夫繼天立極之道。",
        "from": "X87n1619_001:0179a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不異乎禪。",
        "from": "X87n1619_001:0179a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "垂世立教之書。",
        "from": "X87n1619_001:0179a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "統歸乎理。",
        "from": "X87n1619_001:0179a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道非言而不著。",
        "from": "X87n1619_001:0179a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "理非事而不明。",
        "from": "X87n1619_001:0179a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以儒有墳典經史。",
        "from": "X87n1619_001:0179a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸子百家。",
        "from": "X87n1619_001:0179a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以至稗官小說。",
        "from": "X87n1619_001:0179a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各各有宗趣焉。",
        "from": "X87n1619_001:0179a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟吾釋門。",
        "from": "X87n1619_001:0179a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因西天東土之文。",
        "from": "X87n1619_001:0179b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北教南宗之旨。",
        "from": "X87n1619_001:0179b01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旁流正出。",
        "from": "X87n1619_001:0179b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散聖異人。",
        "from": "X87n1619_001:0179b01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分支別派。",
        "from": "X87n1619_001:0179b02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "授受淵源。",
        "from": "X87n1619_001:0179b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幸存簡牘。",
        "from": "X87n1619_001:0179b02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "考實之徵。",
        "from": "X87n1619_001:0179b02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不致紊亂傳燈。",
        "from": "X87n1619_001:0179b02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誵訛繼席。",
        "from": "X87n1619_001:0179b03_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "益信一切語言三昧。",
        "from": "X87n1619_001:0179b03_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內外典集。",
        "from": "X87n1619_001:0179b03_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆為載道之器。",
        "from": "X87n1619_001:0179b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳遠之用也。",
        "from": "X87n1619_001:0179b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有惠後昆。",
        "from": "X87n1619_001:0179b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功不細矣。",
        "from": "X87n1619_001:0179b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "茲明潛陶居士。",
        "from": "X87n1619_001:0179b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從學多年。",
        "from": "X87n1619_001:0179b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執勞甚久。",
        "from": "X87n1619_001:0179b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦參實究。",
        "from": "X87n1619_001:0179b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歷徧辛酸。",
        "from": "X87n1619_001:0179b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "涉水登山。",
        "from": "X87n1619_001:0179b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉知泠暖。",
        "from": "X87n1619_001:0179b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "于壬子冬。",
        "from": "X87n1619_001:0179b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忽然有省于海會室中。",
        "from": "X87n1619_001:0179b06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未幾辭往中山寺。",
        "from": "X87n1619_001:0179b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薙草耕雲。",
        "from": "X87n1619_001:0179b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "跏趺[門@卞]影。",
        "from": "X87n1619_001:0179b07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不聞塵事。",
        "from": "X87n1619_001:0179b07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復檢殘編。",
        "from": "X87n1619_001:0179b08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂纂先覺集二卷。",
        "from": "X87n1619_001:0179b08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "編敘優婆塞夷從上諸公。",
        "from": "X87n1619_001:0179b08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乞余弁言為[烈列+助]。",
        "from": "X87n1619_001:0179b09_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不覺喟然而嘆。",
        "from": "X87n1619_001:0179b09_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居士身說法。",
        "from": "X87n1619_001:0179b09_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仍以居士身供養。",
        "from": "X87n1619_001:0179b09_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未審鼻祖西來。",
        "from": "X87n1619_001:0179b10_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養有幾。",
        "from": "X87n1619_001:0179b10_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始知彰顯正教。",
        "from": "X87n1619_001:0179b10_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "全在乎人。",
        "from": "X87n1619_001:0179b11_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非其人而不能弘其道。",
        "from": "X87n1619_001:0179b11_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非其道而不能知其人。",
        "from": "X87n1619_001:0179b11_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而名實全備于天下者。",
        "from": "X87n1619_001:0179b12_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "匪至人而誰能之。",
        "from": "X87n1619_001:0179b12_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但緝書紀事之任。",
        "from": "X87n1619_001:0179b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不洞究先覺佛理之精。",
        "from": "X87n1619_001:0179b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "芟補切難。",
        "from": "X87n1619_001:0179b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要免闕遺。",
        "from": "X87n1619_001:0179b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恐二乘見地。",
        "from": "X87n1619_001:0179b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翻以庭前瑞草之誚。",
        "from": "X87n1619_001:0179b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡吾同儕。",
        "from": "X87n1619_001:0179b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必攝無礙觀。",
        "from": "X87n1619_001:0179b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀之便知。",
        "from": "X87n1619_001:0179b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十世古今。",
        "from": "X87n1619_001:0179b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不離當念。",
        "from": "X87n1619_001:0179b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塵影起滅。",
        "from": "X87n1619_001:0179b15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何足記哉。",
        "from": "X87n1619_001:0179b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然舉目視而有欲皆充。",
        "from": "X87n1619_001:0179b16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信手拈而有疾皆愈。",
        "from": "X87n1619_001:0179b16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同瞻緇素。",
        "from": "X87n1619_001:0179b17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣播名藍。",
        "from": "X87n1619_001:0179b17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先哲不孤。",
        "from": "X87n1619_001:0179b17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晚參可托。",
        "from": "X87n1619_001:0179b17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真勝事也。",
        "from": "X87n1619_001:0179b18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故敢引紙運墨。",
        "from": "X87n1619_001:0179b18_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聊為碧眼鉄漢一勗云爾。",
        "from": "X87n1619_001:0179b18_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旹",
        "from": "X87n1619_001:0179b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "康熈丁卯春王吉日圓教解三洪撰",
        "from": "X87n1619_001:0179b21_0",
        "context_with_next_sentence": false
    }
]