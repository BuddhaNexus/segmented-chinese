    },
    {
        "source_sentence": "燈。",
        "from": "T19n1005B_001:0635a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食。",
        "from": "T19n1005B_001:0635a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "閼伽。",
        "from": "T19n1005B_001:0635a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供物及食等。",
        "from": "T19n1005B_001:0635a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持念珠。",
        "from": "T19n1005B_001:0635a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦先誦此真言。",
        "from": "T19n1005B_001:0635a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結趺坐。",
        "from": "T19n1005B_001:0635a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "驚覺一切如來。",
        "from": "T19n1005B_001:0635a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請一切如來。",
        "from": "T19n1005B_001:0635a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求願。",
        "from": "T19n1005B_001:0635b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求菩薩願。",
        "from": "T19n1005B_001:0635b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請一切天龍。",
        "from": "T19n1005B_001:0635b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請四天王等。",
        "from": "T19n1005B_001:0635b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持弟子。",
        "from": "T19n1005B_001:0635b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令弟子入壇。",
        "from": "T19n1005B_001:0635b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獻一切佛一切菩薩諸天等食。",
        "from": "T19n1005B_001:0635b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "訶送諸聖眾。",
        "from": "T19n1005B_001:0635b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獻物護身。",
        "from": "T19n1005B_001:0635b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為弟子灌頂。",
        "from": "T19n1005B_001:0635b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法供養護摩。",
        "from": "T19n1005B_001:0635c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今悉皆禮。",
        "from": "T19n1005B_001:0635c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普遍光明如來心。",
        "from": "T19n1005B_001:0635c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來心印。",
        "from": "T19n1005B_001:0635c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來普遍大寶三昧耶。",
        "from": "T19n1005B_001:0635c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來莊嚴大寶光。",
        "from": "T19n1005B_001:0635c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛師子座。",
        "from": "T19n1005B_001:0635c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大寶出生灌頂。",
        "from": "T19n1005B_001:0635c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摧魔熾然法輪神通加持。",
        "from": "T19n1005B_001:0635c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無能勝。",
        "from": "T19n1005B_001:0635c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來轉法輪。",
        "from": "T19n1005B_001:0636a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛手。",
        "from": "T19n1005B_001:0636a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶金剛菩薩。",
        "from": "T19n1005B_001:0636a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四大天主。",
        "from": "T19n1005B_001:0636a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉祥天女。",
        "from": "T19n1005B_001:0636a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鈎棄尼天女。",
        "from": "T19n1005B_001:0636a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使者天女。",
        "from": "T19n1005B_001:0636a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曼荼羅中一切聖眾。",
        "from": "T19n1005B_001:0636a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "花齒天女。",
        "from": "T19n1005B_001:0636a23_0",
        "context_with_next_sentence": true
    },
