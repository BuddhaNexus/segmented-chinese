[
    {
        "source_sentence": "正行集",
        "from": "X63n1254_001:0735b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西京寶應寺沙門釋　清覺　述",
        "from": "X63n1254_001:0735b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡君子者。",
        "from": "X63n1254_001:0735b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不在乎貴。",
        "from": "X63n1254_001:0735b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不在乎賤。",
        "from": "X63n1254_001:0735b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不在乎貧。",
        "from": "X63n1254_001:0735b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不在乎富。",
        "from": "X63n1254_001:0735b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯在乎行也。",
        "from": "X63n1254_001:0735b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "君子之體。",
        "from": "X63n1254_001:0735b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "德行以成之。",
        "from": "X63n1254_001:0735b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孝敬以加之。",
        "from": "X63n1254_001:0735b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故其廣固貞淨。",
        "from": "X63n1254_001:0735b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世莫得焉。",
        "from": "X63n1254_001:0735b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其廣也如海。",
        "from": "X63n1254_001:0735b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其固也如山。",
        "from": "X63n1254_001:0735b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其貞也如松。",
        "from": "X63n1254_001:0735b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其淨也如水。",
        "from": "X63n1254_001:0735b12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如海之廣者。",
        "from": "X63n1254_001:0735b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深不可量。",
        "from": "X63n1254_001:0735b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闊不可測。",
        "from": "X63n1254_001:0735b12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "減之不降。",
        "from": "X63n1254_001:0735b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "添之不盈。",
        "from": "X63n1254_001:0735b13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "渾之不能使其濁。",
        "from": "X63n1254_001:0735b13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "穢之不能改其清。",
        "from": "X63n1254_001:0735b13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦乃隱於珠懷其寶。",
        "from": "X63n1254_001:0735b14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "浩渺廓落。",
        "from": "X63n1254_001:0735b14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無不納焉。",
        "from": "X63n1254_001:0735b14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如山之固者。",
        "from": "X63n1254_001:0735b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高而不危。",
        "from": "X63n1254_001:0735b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "久而不朽。",
        "from": "X63n1254_001:0735b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "移之不動。",
        "from": "X63n1254_001:0735b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "搖之不傾。",
        "from": "X63n1254_001:0735b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幽靜不能使其絕。",
        "from": "X63n1254_001:0735b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塵埃不能合其有。",
        "from": "X63n1254_001:0735b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦乃隱於賢藏於仙。",
        "from": "X63n1254_001:0735b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "崎嶇曠邈。",
        "from": "X63n1254_001:0735b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無不產焉。",
        "from": "X63n1254_001:0735b17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如松之貞者。",
        "from": "X63n1254_001:0735b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "榮枯莫知。",
        "from": "X63n1254_001:0735b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蒼翠長在。",
        "from": "X63n1254_001:0735b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "霜雪不能令其凋。",
        "from": "X63n1254_001:0735b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冬夏不能變其色。",
        "from": "X63n1254_001:0735b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如水之淨者。",
        "from": "X63n1254_001:0735b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "濟渭各色。",
        "from": "X63n1254_001:0735b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平澹自然。",
        "from": "X63n1254_001:0735b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鑑物不能遁其形。",
        "from": "X63n1254_001:0735b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "潤澤不能踰其利。",
        "from": "X63n1254_001:0735b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡君子者。",
        "from": "X63n1254_001:0735b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寬宏大量。",
        "from": "X63n1254_001:0735b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高識遠見。",
        "from": "X63n1254_001:0735b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "理其性。",
        "from": "X63n1254_001:0735b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "靜其神。",
        "from": "X63n1254_001:0735b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修其德。",
        "from": "X63n1254_001:0735b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘊其行。",
        "from": "X63n1254_001:0735b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以教以緒。",
        "from": "X63n1254_001:0735b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無縱無恣。",
        "from": "X63n1254_001:0735b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不侮不驕。",
        "from": "X63n1254_001:0735b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "絕毀絕譽。",
        "from": "X63n1254_001:0735b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不彰他人之過。",
        "from": "X63n1254_001:0735b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不行自己之非。",
        "from": "X63n1254_001:0735b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和其光而同其塵。",
        "from": "X63n1254_001:0735b22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薦其賢而任其德。",
        "from": "X63n1254_001:0735b23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮無不恭。",
        "from": "X63n1254_001:0735b23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "事無不從。",
        "from": "X63n1254_001:0735b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孝無不順。",
        "from": "X63n1254_001:0735b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義無不忠。",
        "from": "X63n1254_001:0735b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "躰察智慧。",
        "from": "X63n1254_001:0735b24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "審別賢愚。",
        "from": "X63n1254_001:0735b24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不以多求而亂心。",
        "from": "X63n1254_001:0735b24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不以貪暴而害己。",
        "from": "X63n1254_001:0735c01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不以塗炭而樂禍。",
        "from": "X63n1254_001:0735c01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不以忘舊而逐新。",
        "from": "X63n1254_001:0735c01_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故任道而含弘。",
        "from": "X63n1254_001:0735c02_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常堅心而守志。",
        "from": "X63n1254_001:0735c02_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此四者。",
        "from": "X63n1254_001:0735c02_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大人君子之要道也。",
        "from": "X63n1254_001:0735c02_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又須行同日月。",
        "from": "X63n1254_001:0735c03_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "量比山川。",
        "from": "X63n1254_001:0735c03_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "順四時。",
        "from": "X63n1254_001:0735c03_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合萬物。",
        "from": "X63n1254_001:0735c03_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行同日月者。",
        "from": "X63n1254_001:0735c04_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使之無偏照。",
        "from": "X63n1254_001:0735c04_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "量比山川者。",
        "from": "X63n1254_001:0735c04_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使之無遷改。",
        "from": "X63n1254_001:0735c04_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "順四時者。",
        "from": "X63n1254_001:0735c05_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使之有期信。",
        "from": "X63n1254_001:0735c05_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合萬物者。",
        "from": "X63n1254_001:0735c05_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使之知動靜。",
        "from": "X63n1254_001:0735c05_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故曰。",
        "from": "X63n1254_001:0735c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "君子有德。",
        "from": "X63n1254_001:0735c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四方美之。",
        "from": "X63n1254_001:0735c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "郡守有德。",
        "from": "X63n1254_001:0735c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百姓欽之。",
        "from": "X63n1254_001:0735c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鄉閭有德。",
        "from": "X63n1254_001:0735c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾人仰之。",
        "from": "X63n1254_001:0735c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "家尊有德。",
        "from": "X63n1254_001:0735c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "門風顯之皇天無親。",
        "from": "X63n1254_001:0735c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟德是輔。",
        "from": "X63n1254_001:0735c08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如蓬生於麻中。",
        "from": "X63n1254_001:0735c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不扶而自直。",
        "from": "X63n1254_001:0735c08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如蘭雜於叢芳。",
        "from": "X63n1254_001:0735c08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不待薰而自馨。",
        "from": "X63n1254_001:0735c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不以利己而損人。",
        "from": "X63n1254_001:0735c09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不以非他而自是。",
        "from": "X63n1254_001:0735c09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不以卑人而自尊。",
        "from": "X63n1254_001:0735c10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不以輕人而自重。",
        "from": "X63n1254_001:0735c10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不以非道而交友。",
        "from": "X63n1254_001:0735c10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不以結怨而讎人。",
        "from": "X63n1254_001:0735c11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不窺無義之財。",
        "from": "X63n1254_001:0735c11_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不受無功之賞。",
        "from": "X63n1254_001:0735c11_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施恩而不望報。",
        "from": "X63n1254_001:0735c12_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "報恩而恨不深。",
        "from": "X63n1254_001:0735c12_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故書云。",
        "from": "X63n1254_001:0735c12_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨之以財。",
        "from": "X63n1254_001:0735c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則見其廉。",
        "from": "X63n1254_001:0735c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "近之以色。",
        "from": "X63n1254_001:0735c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則見其貞。",
        "from": "X63n1254_001:0735c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告之以危。",
        "from": "X63n1254_001:0735c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則見其勇。",
        "from": "X63n1254_001:0735c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "醉之以酒。",
        "from": "X63n1254_001:0735c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則見其態。",
        "from": "X63n1254_001:0735c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莊子曰。",
        "from": "X63n1254_001:0735c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠使之而觀其忠。",
        "from": "X63n1254_001:0735c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "近使之而觀其敬。",
        "from": "X63n1254_001:0735c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煩使之而觀其能。",
        "from": "X63n1254_001:0735c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卒問之而觀其智。",
        "from": "X63n1254_001:0735c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "急期之而觀其信。",
        "from": "X63n1254_001:0735c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寵貴之人。",
        "from": "X63n1254_001:0735c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀其不驕奢。",
        "from": "X63n1254_001:0735c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "榮顯之人。",
        "from": "X63n1254_001:0735c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀其不矜誇。",
        "from": "X63n1254_001:0735c17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少者觀其恭敬。",
        "from": "X63n1254_001:0735c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "好學者觀其廉潔務行。",
        "from": "X63n1254_001:0735c17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "老者觀其思慎而不逾。",
        "from": "X63n1254_001:0735c18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父子之間。",
        "from": "X63n1254_001:0735c18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀其慈孝。",
        "from": "X63n1254_001:0735c18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兄弟之間。",
        "from": "X63n1254_001:0735c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀其知友。",
        "from": "X63n1254_001:0735c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鄉閭之間。",
        "from": "X63n1254_001:0735c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀其信義。",
        "from": "X63n1254_001:0735c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "君臣之間。",
        "from": "X63n1254_001:0735c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀其忠惠。",
        "from": "X63n1254_001:0735c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故慎初護末。",
        "from": "X63n1254_001:0735c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "砥名礪行。",
        "from": "X63n1254_001:0735c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "邪正不變其心。",
        "from": "X63n1254_001:0735c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "反覆不改其志。",
        "from": "X63n1254_001:0735c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨下以寬。",
        "from": "X63n1254_001:0735c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居上不危。",
        "from": "X63n1254_001:0735c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如此則可為廓之材。",
        "from": "X63n1254_001:0735c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永免愆瑕矣。",
        "from": "X63n1254_001:0735c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四十八等。",
        "from": "X63n1254_001:0735c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　聖人則天法地　　賢人杳冥難測",
        "from": "X63n1254_001:0735c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　道人心無滯碍　　覺人知其本性",
        "from": "X63n1254_001:0736a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　捨人知無便與　　戒人察其罪福",
        "from": "X63n1254_001:0736a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　忍人口無過失　　進人修無懈怠",
        "from": "X63n1254_001:0736a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　定人心無散亂　　智人遠於機變",
        "from": "X63n1254_001:0736a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　義人推功讓德　　審人視聽不非",
        "from": "X63n1254_001:0736a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　政人公道不私　　省人非財不納",
        "from": "X63n1254_001:0736a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　安人心無異動　　隱人遁世育德",
        "from": "X63n1254_001:0736a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　恭人禮度不虧　　敬人尊上愛下",
        "from": "X63n1254_001:0736a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　信人言無反覆　　謹人不越禮度",
        "from": "X63n1254_001:0736a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　寬人臨下不危　　深人遠見未萠",
        "from": "X63n1254_001:0736a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　善人內行不嗔　　行人好述善事",
        "from": "X63n1254_001:0736a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　謀人深知遠見　　忠人事君盡節",
        "from": "X63n1254_001:0736a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　讓人薦賢任能　　儉人節用不費",
        "from": "X63n1254_001:0736a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　孝人奉侍不闕　　明人不處暗事",
        "from": "X63n1254_001:0736a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　辯人不納閑詞　　學人親近知識",
        "from": "X63n1254_001:0736a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　慈人心無殺害　　富人濟貧惠物",
        "from": "X63n1254_001:0736a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　貴人敬佛重僧　　達人不貪五慾",
        "from": "X63n1254_001:0736a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　清人不染俗塵　　靜人覺意知空",
        "from": "X63n1254_001:0736a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　大人心包天地　　好人舉善薦賢",
        "from": "X63n1254_001:0736a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　愚人不知慚耻　　癡人不鑑名賢",
        "from": "X63n1254_001:0736a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　頑人不懼公法　　惡人常好殺生",
        "from": "X63n1254_001:0736a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　濁人不分明白　　俗人貪財戀色",
        "from": "X63n1254_001:0736a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　逆人不孝父母　　小人不識尊卑",
        "from": "X63n1254_001:0736a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡君子立身為人。",
        "from": "X63n1254_001:0736a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無逾此四十八等。",
        "from": "X63n1254_001:0736a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但以大道合其體。",
        "from": "X63n1254_001:0736a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則是聖人賢人之倫。",
        "from": "X63n1254_001:0736b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若孝不行於家。",
        "from": "X63n1254_001:0736b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈不行於己。",
        "from": "X63n1254_001:0736b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恩不行於親。",
        "from": "X63n1254_001:0736b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惠不及於人。",
        "from": "X63n1254_001:0736b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義不施於友。",
        "from": "X63n1254_001:0736b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忠不施於國。",
        "from": "X63n1254_001:0736b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勇不救於危。",
        "from": "X63n1254_001:0736b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謀不信於善。",
        "from": "X63n1254_001:0736b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寬不臨於下。",
        "from": "X63n1254_001:0736b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明不審於事。",
        "from": "X63n1254_001:0736b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見非親而親者。",
        "from": "X63n1254_001:0736b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謟也。",
        "from": "X63n1254_001:0736b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見親貧而疎者。",
        "from": "X63n1254_001:0736b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逆也。",
        "from": "X63n1254_001:0736b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "背善向惡。",
        "from": "X63n1254_001:0736b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謀陷忠良。",
        "from": "X63n1254_001:0736b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "親附小人。",
        "from": "X63n1254_001:0736b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不近君子者。",
        "from": "X63n1254_001:0736b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此乃上天不容之物。",
        "from": "X63n1254_001:0736b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何足言之。",
        "from": "X63n1254_001:0736b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢者欽於德。",
        "from": "X63n1254_001:0736b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愚者重於財。",
        "from": "X63n1254_001:0736b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "綺羅華飾者。",
        "from": "X63n1254_001:0736b06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須知有布素之衣。",
        "from": "X63n1254_001:0736b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "珍飱玉食者。",
        "from": "X63n1254_001:0736b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須知有糟糠之饌。",
        "from": "X63n1254_001:0736b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雕墻峻宇者。",
        "from": "X63n1254_001:0736b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須知有茅茨之室。",
        "from": "X63n1254_001:0736b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "積粟千鍾者。",
        "from": "X63n1254_001:0736b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須知有斗筲之儲。",
        "from": "X63n1254_001:0736b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "榮官大爵者。",
        "from": "X63n1254_001:0736b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須知有冤屈之害。",
        "from": "X63n1254_001:0736b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安居樂處者。",
        "from": "X63n1254_001:0736b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須知有驅役之勞。",
        "from": "X63n1254_001:0736b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "躍馬揮鞭者。",
        "from": "X63n1254_001:0736b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須知有負擔之苦。",
        "from": "X63n1254_001:0736b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人有危困急難。",
        "from": "X63n1254_001:0736b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當須救之。",
        "from": "X63n1254_001:0736b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可坐看成敗。",
        "from": "X63n1254_001:0736b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見危不救者。",
        "from": "X63n1254_001:0736b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非人也。",
        "from": "X63n1254_001:0736b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不自知足者。",
        "from": "X63n1254_001:0736b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非人也。",
        "from": "X63n1254_001:0736b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見貧不濟者。",
        "from": "X63n1254_001:0736b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非人也。",
        "from": "X63n1254_001:0736b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施恩望報者。",
        "from": "X63n1254_001:0736b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非人也。",
        "from": "X63n1254_001:0736b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不改己過者。",
        "from": "X63n1254_001:0736b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非人也。",
        "from": "X63n1254_001:0736b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輕上慢下者。",
        "from": "X63n1254_001:0736b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非人也。",
        "from": "X63n1254_001:0736b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為貴欺賤者。",
        "from": "X63n1254_001:0736b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非人也。",
        "from": "X63n1254_001:0736b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取佞害人者。",
        "from": "X63n1254_001:0736b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非人也。",
        "from": "X63n1254_001:0736b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在公納私者。",
        "from": "X63n1254_001:0736b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非人也。",
        "from": "X63n1254_001:0736b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不孝父母者。",
        "from": "X63n1254_001:0736b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非人也。",
        "from": "X63n1254_001:0736b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不友兄弟者。",
        "from": "X63n1254_001:0736b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非人也。",
        "from": "X63n1254_001:0736b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不和六親者。",
        "from": "X63n1254_001:0736b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非人也。",
        "from": "X63n1254_001:0736b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不樂好事者。",
        "from": "X63n1254_001:0736b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非人也。",
        "from": "X63n1254_001:0736b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫人須通三才。",
        "from": "X63n1254_001:0736b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懷六藝。",
        "from": "X63n1254_001:0736b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "志四方。",
        "from": "X63n1254_001:0736b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具五美。",
        "from": "X63n1254_001:0736b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷四惡。",
        "from": "X63n1254_001:0736b17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除三惑。",
        "from": "X63n1254_001:0736b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思九畏。",
        "from": "X63n1254_001:0736b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知四懼。",
        "from": "X63n1254_001:0736b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫睡眠。",
        "from": "X63n1254_001:0736b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "演胎息。",
        "from": "X63n1254_001:0736b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慎言語。",
        "from": "X63n1254_001:0736b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "節飲食。",
        "from": "X63n1254_001:0736b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此乃謹身節用之本也。",
        "from": "X63n1254_001:0736b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又須立彼四門。",
        "from": "X63n1254_001:0736b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成其具美。",
        "from": "X63n1254_001:0736b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "事君立忠門。",
        "from": "X63n1254_001:0736b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "事父母立孝門。",
        "from": "X63n1254_001:0736b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "事朋友立義門。",
        "from": "X63n1254_001:0736b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "事師長立謹門。",
        "from": "X63n1254_001:0736b20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忠門。",
        "from": "X63n1254_001:0736b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂見危致命也。",
        "from": "X63n1254_001:0736b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孝門。",
        "from": "X63n1254_001:0736b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂色養恭敬也。",
        "from": "X63n1254_001:0736b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義門。",
        "from": "X63n1254_001:0736b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂心無遷改也。",
        "from": "X63n1254_001:0736b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謹門。",
        "from": "X63n1254_001:0736b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無所懈怠也。",
        "from": "X63n1254_001:0736b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三教之說。",
        "from": "X63n1254_001:0736b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其義一同。",
        "from": "X63n1254_001:0736b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儒教。",
        "from": "X63n1254_001:0736b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則仁義禮智信。",
        "from": "X63n1254_001:0736b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸於忠孝君父焉。",
        "from": "X63n1254_001:0736b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋教。",
        "from": "X63n1254_001:0736c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則慈悲救苦。",
        "from": "X63n1254_001:0736c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸於化誘羣迷焉。",
        "from": "X63n1254_001:0736c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道教。",
        "from": "X63n1254_001:0736c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則寂默恬澹。",
        "from": "X63n1254_001:0736c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸於無貪無愛焉。",
        "from": "X63n1254_001:0736c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有故三教之言。",
        "from": "X63n1254_001:0736c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可守而尊之。",
        "from": "X63n1254_001:0736c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尋而究之。",
        "from": "X63n1254_001:0736c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既洞其微。",
        "from": "X63n1254_001:0736c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "達其原。",
        "from": "X63n1254_001:0736c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然得聖人賢人之道。",
        "from": "X63n1254_001:0736c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善人君子之行也。",
        "from": "X63n1254_001:0736c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如此則佐國。",
        "from": "X63n1254_001:0736c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何憂乎陰陽不順。",
        "from": "X63n1254_001:0736c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風雨不時。",
        "from": "X63n1254_001:0736c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百姓不安。",
        "from": "X63n1254_001:0736c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人民不泰。",
        "from": "X63n1254_001:0736c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "治家。",
        "from": "X63n1254_001:0736c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則何憂乎兄弟不睦。",
        "from": "X63n1254_001:0736c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六親不和。",
        "from": "X63n1254_001:0736c06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮樂不行。",
        "from": "X63n1254_001:0736c06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上下不正。",
        "from": "X63n1254_001:0736c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "余為此集。",
        "from": "X63n1254_001:0736c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不敢深其意飾其詞。",
        "from": "X63n1254_001:0736c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所貴匡導盲俗。",
        "from": "X63n1254_001:0736c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "垂于後世。",
        "from": "X63n1254_001:0736c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言之不足。",
        "from": "X63n1254_001:0736c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故為讚以申之。",
        "from": "X63n1254_001:0736c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讚曰。",
        "from": "X63n1254_001:0736c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　美哉君子　　惟善則履　　存忠存孝　　不識其嗜　行不逾經　　言不逾史　　靖默端莊　　高導深旨　向善背惡　　披心求理　　貧則自樂　　富亦好義　嗟乎斯人　　實不容易正行集(終)白雲宗宗主(臣)僧(明仁)承奉都功德使司劄付。",
        "from": "X63n1254_001:0736c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皇慶元年十月二十八日。",
        "from": "X63n1254_001:0736c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拜住怯薛第一日。",
        "from": "X63n1254_001:0736c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嘉禧殿內。",
        "from": "X63n1254_001:0736c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有時分對。",
        "from": "X63n1254_001:0736c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速古兒赤那懷怯里馬赤孛羅察兒同知觀音奴等。",
        "from": "X63n1254_001:0736c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有來本司官大司徒都功德使輦真吃剌思張都功德使副使阿里牙答思特奉聖旨。",
        "from": "X63n1254_001:0736c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙剌巴譯來的藥師儀軌藥師供養法。",
        "from": "X63n1254_001:0737a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更白雲和尚初學記。",
        "from": "X63n1254_001:0737a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恁行與省部文書。",
        "from": "X63n1254_001:0737a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "交江浙省。",
        "from": "X63n1254_001:0737a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白雲宗開板印了。",
        "from": "X63n1254_001:0737a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呵都交大藏經裏入去者麼。",
        "from": "X63n1254_001:0737a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道聖旨了也。",
        "from": "X63n1254_001:0737a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欽此已蒙　　　中書省移咨行者。",
        "from": "X63n1254_001:0737a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欽依行下各路去訖。",
        "from": "X63n1254_001:0737a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(臣)僧(明仁)肅恭寵命。",
        "from": "X63n1254_001:0737a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就大慈隱寺。",
        "from": "X63n1254_001:0737a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命工鋟梓印造。",
        "from": "X63n1254_001:0737a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欽依入藏流通。",
        "from": "X63n1254_001:0737a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所集洪因。",
        "from": "X63n1254_001:0737a10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端為祝延皇帝萬萬歲皇太后睿筭齊年。",
        "from": "X63n1254_001:0737a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皇后同壽萬年。",
        "from": "X63n1254_001:0737a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "太子諸王千秋。",
        "from": "X63n1254_001:0737a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文武官僚常居祿位。",
        "from": "X63n1254_001:0737a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更冀國泰民安。",
        "from": "X63n1254_001:0737a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雨暘時若。",
        "from": "X63n1254_001:0737a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛日增輝。",
        "from": "X63n1254_001:0737a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法輪常轉者。",
        "from": "X63n1254_001:0737a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皇慶二年四月　日(臣)僧(明仁)　謹題",
        "from": "X63n1254_001:0737a17_0",
        "context_with_next_sentence": false
    }
]