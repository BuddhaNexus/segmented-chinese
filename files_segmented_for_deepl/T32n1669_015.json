[
    {
        "source_sentence": "大宗地玄文本論卷第十五",
        "from": "T32n1669_015:0687c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬鳴菩薩造",
        "from": "T32n1669_015:0687c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真諦三藏譯",
        "from": "T32n1669_015:0687c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "◎自然本王廣大轉地無障無礙俱行道路大決擇分第二十九如是已說繫縛地地自然本王摩訶[糸*為]品大決擇分。",
        "from": "T32n1669_015:0687c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說自然本王廣大轉地無障無礙俱行道路大決擇分。",
        "from": "T32n1669_015:0687c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_015:0687c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_015:0687c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　本王道路中　　依位漸漸轉",
        "from": "T32n1669_015:0687c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一主生二伴　　至中無量故",
        "from": "T32n1669_015:0687c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　數變皆悉通　　如法應觀察",
        "from": "T32n1669_015:0687c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_015:0687c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就自然本王廣大轉地無障無礙俱行道路分中。",
        "from": "T32n1669_015:0687c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依五十一位。",
        "from": "T32n1669_015:0687c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如其次第亦上亦下漸漸轉行。",
        "from": "T32n1669_015:0687c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無為法主生二有為。",
        "from": "T32n1669_015:0687c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若上若下增中無量數。",
        "from": "T32n1669_015:0687c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至中無量變故。",
        "from": "T32n1669_015:0687c21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈本王道路中依位漸漸轉一主生二伴至中無量故數變皆悉通如法應觀察故。",
        "from": "T32n1669_015:0687c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "部宗花品修多羅中。",
        "from": "T32n1669_015:0687c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說。",
        "from": "T32n1669_015:0687c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行藏海中有一常德其數無量。",
        "from": "T32n1669_015:0687c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行藏海中有二無常功德品類其數無量。",
        "from": "T32n1669_015:0687c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦昇亦抃其數無量。",
        "from": "T32n1669_015:0687c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至廣說故。",
        "from": "T32n1669_015:0687c26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最極廣大俱行山王無盡海海大決擇分第三十如是已說自然本王廣大轉地無障無礙俱行道路決大擇分。",
        "from": "T32n1669_015:0687c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說最極廣大俱行山王無盡海海大決擇分。",
        "from": "T32n1669_015:0688a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_015:0688a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_015:0688a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無盡海海中　　依位漸漸轉",
        "from": "T32n1669_015:0688a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一主生二伴　　至大無量故",
        "from": "T32n1669_015:0688a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　數及變例前　　應廣通達焉",
        "from": "T32n1669_015:0688a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_015:0688a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就無盡海海藏中。",
        "from": "T32n1669_015:0688a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依五十一位。",
        "from": "T32n1669_015:0688a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如其次第亦上亦下無為法主出生增長二有為法。",
        "from": "T32n1669_015:0688a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增大無量數。",
        "from": "T32n1669_015:0688a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至大無量變故。",
        "from": "T32n1669_015:0688a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈無盡海海中依位漸漸轉一主生二伴至大無量故數及變例前應廣通達焉故。",
        "from": "T32n1669_015:0688a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼修多羅中。",
        "from": "T32n1669_015:0688a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說。",
        "from": "T32n1669_015:0688a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行常山王海中。",
        "from": "T32n1669_015:0688a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有三種大無量。",
        "from": "T32n1669_015:0688a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為三。",
        "from": "T32n1669_015:0688a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者常大無量。",
        "from": "T32n1669_015:0688a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者無常大無量。",
        "from": "T32n1669_015:0688a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者轉變大無量。",
        "from": "T32n1669_015:0688a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至廣說故。",
        "from": "T32n1669_015:0688a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "◎大宗地玄文本論卷第十五",
        "from": "T32n1669_015:0688a13_4",
        "context_with_next_sentence": false
    }
]