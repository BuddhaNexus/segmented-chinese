[
    {
        "source_sentence": "佛祖綱目卷第二十一(秋字號)",
        "from": "X85n1594_021:0583b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甲子(漢安帝延光三年起)丙寅(順帝永建元年)壬申(改陽嘉)丙子(改永和)壬午(改漢安)甲申(改建庸)乙酉(冲帝永嘉元年)丙戌(質帝本初元年)丁亥(桓帝建和元年)庚寅(改和平)辛卯(改元嘉)癸巳(改永興)乙未(改永壽)戊戌(改延熹)丁未(改永康)戊申(靈帝建寧元年)壬子(改嘉平)戊午(改光和)癸亥(漢靈帝光和六年止)戊子。",
        "from": "X85n1594_021:0583b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安息國沙門安清。",
        "from": "X85n1594_021:0583b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "字世高。",
        "from": "X85n1594_021:0583b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至京譯經。",
        "from": "X85n1594_021:0583b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庚戌。",
        "from": "X85n1594_021:0583b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譯成九十五部。",
        "from": "X85n1594_021:0583b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡一百一十五卷。",
        "from": "X85n1594_021:0583b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辛亥。",
        "from": "X85n1594_021:0583b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清至廬山。",
        "from": "X85n1594_021:0583b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有[邱丘+共]亭廟神甚靈。",
        "from": "X85n1594_021:0583b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "會三十餘船。",
        "from": "X85n1594_021:0583b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉牲請福。",
        "from": "X85n1594_021:0583b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神輒降語曰。",
        "from": "X85n1594_021:0583b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舟有沙門。",
        "from": "X85n1594_021:0583b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可與請來。",
        "from": "X85n1594_021:0583b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "客請清至。",
        "from": "X85n1594_021:0583b23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神復降語曰。",
        "from": "X85n1594_021:0583b23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾昔與子。",
        "from": "X85n1594_021:0583b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱出家學道。",
        "from": "X85n1594_021:0583b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾好施而多嗔。",
        "from": "X85n1594_021:0583b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今為廟神。",
        "from": "X85n1594_021:0583c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽盡旦夕。",
        "from": "X85n1594_021:0583c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復恐墮地獄。",
        "from": "X85n1594_021:0583c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾有縑千段。",
        "from": "X85n1594_021:0583c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并雜寶物。",
        "from": "X85n1594_021:0583c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可為立法營塔。",
        "from": "X85n1594_021:0583c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使生善處。",
        "from": "X85n1594_021:0583c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清使出形。",
        "from": "X85n1594_021:0583c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神現大蟒。",
        "from": "X85n1594_021:0583c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悲淚如雨。",
        "from": "X85n1594_021:0583c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清取絹物。",
        "from": "X85n1594_021:0583c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辭別而去。",
        "from": "X85n1594_021:0583c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清至豫章。",
        "from": "X85n1594_021:0583c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為建大安寺。",
        "from": "X85n1594_021:0583c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(因安得名)乃江淮建塔之始也。",
        "from": "X85n1594_021:0583c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俄而神逝。",
        "from": "X85n1594_021:0583c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "報云。",
        "from": "X85n1594_021:0583c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得生善處。",
        "from": "X85n1594_021:0583c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後人於山西澤中。",
        "from": "X85n1594_021:0583c05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見死蟒頭尾數里。",
        "from": "X85n1594_021:0583c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今潯陽縣。",
        "from": "X85n1594_021:0583c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大蛇村。",
        "from": "X85n1594_021:0583c06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "存焉。",
        "from": "X85n1594_021:0583c06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(乙巳)二十二祖摩拏羅傳法鶴勒那摩拏羅。",
        "from": "X85n1594_021:0583c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姓剎帝利。",
        "from": "X85n1594_021:0583c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父常自在王。",
        "from": "X85n1594_021:0583c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年三十。",
        "from": "X85n1594_021:0583c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遇婆修盤頭。",
        "from": "X85n1594_021:0583c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂出家傳法。",
        "from": "X85n1594_021:0583c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至西印度。",
        "from": "X85n1594_021:0583c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼國王名得度。",
        "from": "X85n1594_021:0583c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞿曇種族。",
        "from": "X85n1594_021:0583c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "事佛精進。",
        "from": "X85n1594_021:0583c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日於行道處。",
        "from": "X85n1594_021:0583c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忽現一塔。",
        "from": "X85n1594_021:0583c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高一尺四寸。",
        "from": "X85n1594_021:0583c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其色青玄。",
        "from": "X85n1594_021:0583c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾莫能舉。",
        "from": "X85n1594_021:0583c11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王即大會梵行禪觀呪術三眾。",
        "from": "X85n1594_021:0583c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問塔所繇。",
        "from": "X85n1594_021:0583c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾莫能知。",
        "from": "X85n1594_021:0583c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩拏羅曰。",
        "from": "X85n1594_021:0583c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此塔阿育王所造。",
        "from": "X85n1594_021:0583c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛舍利塔也。",
        "from": "X85n1594_021:0583c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四面示相。",
        "from": "X85n1594_021:0583c13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前則尸毗王割股飼鷹救鴿。",
        "from": "X85n1594_021:0583c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後則慈力王剜身燃燈。",
        "from": "X85n1594_021:0583c13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左則薩埵太子投崖飼虎。",
        "from": "X85n1594_021:0583c14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右則月光王捐捨寶首。",
        "from": "X85n1594_021:0583c14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四面皆是釋迦世尊宿世修道之跡。",
        "from": "X85n1594_021:0583c15_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今王有緣。",
        "from": "X85n1594_021:0583c15_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故出現耳。",
        "from": "X85n1594_021:0583c16_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言已。",
        "from": "X85n1594_021:0583c16_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉之。",
        "from": "X85n1594_021:0583c16_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王大悅。",
        "from": "X85n1594_021:0583c16_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂謂摩拏羅曰。",
        "from": "X85n1594_021:0583c16_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敢問法要。",
        "from": "X85n1594_021:0583c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "X85n1594_021:0583c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛法能具七事去三物。",
        "from": "X85n1594_021:0583c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃可學。",
        "from": "X85n1594_021:0583c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王問三物七事為何。",
        "from": "X85n1594_021:0583c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "X85n1594_021:0583c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所去三物。",
        "from": "X85n1594_021:0583c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貪嗔癡。",
        "from": "X85n1594_021:0583c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所具七事。",
        "from": "X85n1594_021:0583c18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大慈歡喜無我勇猛饒益降魔無證。",
        "from": "X85n1594_021:0583c18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人所以明了不明了以此耳。",
        "from": "X85n1594_021:0583c19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王聞已曰。",
        "from": "X85n1594_021:0583c20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至聖難逢。",
        "from": "X85n1594_021:0583c20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世樂非久。",
        "from": "X85n1594_021:0583c20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即傳位太子出家。",
        "from": "X85n1594_021:0583c20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七日而證四果。",
        "from": "X85n1594_021:0583c21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拏羅深加慰誨曰。",
        "from": "X85n1594_021:0583c21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝居此國。",
        "from": "X85n1594_021:0583c21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善自度人。",
        "from": "X85n1594_021:0583c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今異域有大法器。",
        "from": "X85n1594_021:0583c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾當往化。",
        "from": "X85n1594_021:0583c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得度曰。",
        "from": "X85n1594_021:0583c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師應跡十方。",
        "from": "X85n1594_021:0583c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "動念當至。",
        "from": "X85n1594_021:0583c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寧勞往耶。",
        "from": "X85n1594_021:0583c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拏羅於是焚香。",
        "from": "X85n1594_021:0583c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遙語月支國鶴勒那比丘曰。",
        "from": "X85n1594_021:0583c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝教導鶴眾。",
        "from": "X85n1594_021:0583c24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道果將證。",
        "from": "X85n1594_021:0583c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宜自知之。",
        "from": "X85n1594_021:0583c24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時勒那為彼國王寶印。",
        "from": "X85n1594_021:0584a01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說修多羅偈。",
        "from": "X85n1594_021:0584a01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忽覩異香成穗。",
        "from": "X85n1594_021:0584a01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王曰。",
        "from": "X85n1594_021:0584a02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是何祥也。",
        "from": "X85n1594_021:0584a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰西印土摩拏羅尊者將至。",
        "from": "X85n1594_021:0584a02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此信香也。",
        "from": "X85n1594_021:0584a02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王與勒那。",
        "from": "X85n1594_021:0584a03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱西嚮作禮。",
        "from": "X85n1594_021:0584a03_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拏羅亦以手東嚮。",
        "from": "X85n1594_021:0584a03_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三點於地答之。",
        "from": "X85n1594_021:0584a03_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即辭得度。",
        "from": "X85n1594_021:0584a04_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與其具通眾。",
        "from": "X85n1594_021:0584a04_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凌虗而往。",
        "from": "X85n1594_021:0584a04_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勒那與寶印。",
        "from": "X85n1594_021:0584a04_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迎拏羅。",
        "from": "X85n1594_021:0584a05_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至王宮供養。",
        "from": "X85n1594_021:0584a05_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "異日勒那問曰。",
        "from": "X85n1594_021:0584a05_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我止林間。",
        "from": "X85n1594_021:0584a05_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已經九白。",
        "from": "X85n1594_021:0584a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(印土以一年為一白)有弟子龍子者。",
        "from": "X85n1594_021:0584a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幼而聰慧。",
        "from": "X85n1594_021:0584a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我於三世推窮。",
        "from": "X85n1594_021:0584a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫知其本。",
        "from": "X85n1594_021:0584a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拏羅曰。",
        "from": "X85n1594_021:0584a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此子於第五劫中。",
        "from": "X85n1594_021:0584a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生玅喜國。",
        "from": "X85n1594_021:0584a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婆羅門家。",
        "from": "X85n1594_021:0584a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曾以旃檀。",
        "from": "X85n1594_021:0584a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施於佛宇。",
        "from": "X85n1594_021:0584a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作槌撞鐘。",
        "from": "X85n1594_021:0584a08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受報聰敏。",
        "from": "X85n1594_021:0584a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為眾欽仰。",
        "from": "X85n1594_021:0584a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又問。",
        "from": "X85n1594_021:0584a09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我有何緣。",
        "from": "X85n1594_021:0584a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而感鶴眾。",
        "from": "X85n1594_021:0584a09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拏羅曰。",
        "from": "X85n1594_021:0584a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝第四劫中。",
        "from": "X85n1594_021:0584a09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甞為比丘。",
        "from": "X85n1594_021:0584a10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當赴會龍宮。",
        "from": "X85n1594_021:0584a10_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝諸弟子。",
        "from": "X85n1594_021:0584a10_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸欲隨從。",
        "from": "X85n1594_021:0584a10_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝觀五百眾中。",
        "from": "X85n1594_021:0584a11_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有一人堪任玅供。",
        "from": "X85n1594_021:0584a11_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時諸弟子曰。",
        "from": "X85n1594_021:0584a11_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師甞說法。",
        "from": "X85n1594_021:0584a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於食等者。",
        "from": "X85n1594_021:0584a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於法亦等。",
        "from": "X85n1594_021:0584a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今既不然。",
        "from": "X85n1594_021:0584a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何聖之有。",
        "from": "X85n1594_021:0584a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝即令赴會。",
        "from": "X85n1594_021:0584a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐是濫食。",
        "from": "X85n1594_021:0584a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "報為羽族。",
        "from": "X85n1594_021:0584a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師弟夙緣。",
        "from": "X85n1594_021:0584a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故今相隨。",
        "from": "X85n1594_021:0584a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勒那曰。",
        "from": "X85n1594_021:0584a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以何方便。",
        "from": "X85n1594_021:0584a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼解脫。",
        "from": "X85n1594_021:0584a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拏羅曰。",
        "from": "X85n1594_021:0584a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾有無上法寶。",
        "from": "X85n1594_021:0584a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當聽受。",
        "from": "X85n1594_021:0584a15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化未來際。",
        "from": "X85n1594_021:0584a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說偈曰。",
        "from": "X85n1594_021:0584a15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心隨萬境轉。",
        "from": "X85n1594_021:0584a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉處實能幽。",
        "from": "X85n1594_021:0584a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨流認得性。",
        "from": "X85n1594_021:0584a16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無喜亦無憂。",
        "from": "X85n1594_021:0584a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鶴眾聞偈。",
        "from": "X85n1594_021:0584a16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飛鳴而去。",
        "from": "X85n1594_021:0584a16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拏羅即踴身空中。",
        "from": "X85n1594_021:0584a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呈十八變。",
        "from": "X85n1594_021:0584a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "返座指地。",
        "from": "X85n1594_021:0584a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發一神泉。",
        "from": "X85n1594_021:0584a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說偈曰。",
        "from": "X85n1594_021:0584a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心地清淨泉。",
        "from": "X85n1594_021:0584a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能潤於一切。",
        "from": "X85n1594_021:0584a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從地而湧出。",
        "from": "X85n1594_021:0584a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徧滿十方際。",
        "from": "X85n1594_021:0584a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈已。",
        "from": "X85n1594_021:0584a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "泊然寂滅。",
        "from": "X85n1594_021:0584a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勒那與寶印王。",
        "from": "X85n1594_021:0584a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建塔。",
        "from": "X85n1594_021:0584a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以奉全身。",
        "from": "X85n1594_021:0584a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當此土漢桓帝十九年乙巳也。",
        "from": "X85n1594_021:0584a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛祖綱目卷第二十一",
        "from": "X85n1594_021:0584a22_0",
        "context_with_next_sentence": false
    }
]