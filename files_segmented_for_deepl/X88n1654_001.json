[
    {
        "source_sentence": "大唐青龍寺三朝供奉大德行狀",
        "from": "X88n1654_001:0390b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先師諱惠果和尚。",
        "from": "X88n1654_001:0390b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俗姓馬氏。",
        "from": "X88n1654_001:0390b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "京兆府萬年縣歸明鄉人也。",
        "from": "X88n1654_001:0390b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幼年九歲。",
        "from": "X88n1654_001:0390b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便隨聖佛院。",
        "from": "X88n1654_001:0390b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故三朝國師內道場持念賜紫沙門諱曇真和尚。",
        "from": "X88n1654_001:0390b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立志習經。",
        "from": "X88n1654_001:0390b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至年十七。",
        "from": "X88n1654_001:0390b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為緣和尚。",
        "from": "X88n1654_001:0390b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常在內道場。",
        "from": "X88n1654_001:0390b12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持念不出。",
        "from": "X88n1654_001:0390b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃於興善寺三藏和上。",
        "from": "X88n1654_001:0390b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求授大佛頂隨求等真言。",
        "from": "X88n1654_001:0390b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "秊十九。",
        "from": "X88n1654_001:0390b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏邊教授灌頂散華。",
        "from": "X88n1654_001:0390b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得轉法輪菩薩。",
        "from": "X88n1654_001:0390b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和上云。",
        "from": "X88n1654_001:0390b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我於南天竺國。",
        "from": "X88n1654_001:0390b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散花得此尊。",
        "from": "X88n1654_001:0390b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如今無異。",
        "from": "X88n1654_001:0390b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "異於吾後。",
        "from": "X88n1654_001:0390b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弘傳總持大教。",
        "from": "X88n1654_001:0390b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如我無異。",
        "from": "X88n1654_001:0390b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又大曆八年。",
        "from": "X88n1654_001:0390b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三月上旬。",
        "from": "X88n1654_001:0390b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　勅於慈恩寺。",
        "from": "X88n1654_001:0390b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置方等道場。",
        "from": "X88n1654_001:0390b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖佛院和上。",
        "from": "X88n1654_001:0390b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉慈恩寺　勑。",
        "from": "X88n1654_001:0390b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置方等道場。",
        "from": "X88n1654_001:0390b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微僧有二童子。",
        "from": "X88n1654_001:0390b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年滿二十。",
        "from": "X88n1654_001:0390b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堪授具戒。",
        "from": "X88n1654_001:0390b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未敢專擅與出家剃頭。",
        "from": "X88n1654_001:0390b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏乞聖慈。",
        "from": "X88n1654_001:0390b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "許臣歸寺。",
        "from": "X88n1654_001:0390b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與二童子。",
        "from": "X88n1654_001:0390b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并授戒衣鉢。",
        "from": "X88n1654_001:0390b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勑賜刀一口。",
        "from": "X88n1654_001:0390b19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於青龍大佛殿前。",
        "from": "X88n1654_001:0390b20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "授　勑與和上二童子剃頭。",
        "from": "X88n1654_001:0390b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賜袈裟衣鉢各兩副。",
        "from": "X88n1654_001:0390b20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "度官告住寺。",
        "from": "X88n1654_001:0390b21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天使和上送至慈恩寺訖。",
        "from": "X88n1654_001:0390b21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "却入內謝　勅。",
        "from": "X88n1654_001:0390b22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臣一界微僧。",
        "from": "X88n1654_001:0390b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謝聖慈賜與二童子出家授戒衣鉢剃刀。",
        "from": "X88n1654_001:0390b22_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "官告及天使送至慈恩寺道場。",
        "from": "X88n1654_001:0390b23_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "授戒處訖。",
        "from": "X88n1654_001:0390b24_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微僧不勝頂賀。",
        "from": "X88n1654_001:0390b24_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "授戒訖。",
        "from": "X88n1654_001:0390b24_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "秊廿二。",
        "from": "X88n1654_001:0390b24_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又於無畏三藏和上弟子玄超和上邊。",
        "from": "X88n1654_001:0390b24_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求授大悲胎藏毗盧遮那大瑜伽大教。",
        "from": "X88n1654_001:0390c01_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及蘇悉地大瑜伽法。",
        "from": "X88n1654_001:0390c02_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸尊瑜伽等法。",
        "from": "X88n1654_001:0390c02_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一親垂旨授。",
        "from": "X88n1654_001:0390c03_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又於大興善寺三藏和上邊。",
        "from": "X88n1654_001:0390c03_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求授金剛頂大瑜伽大教王經法。",
        "from": "X88n1654_001:0390c03_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸尊瑜伽密印。",
        "from": "X88n1654_001:0390c04_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "親承指示。",
        "from": "X88n1654_001:0390c04_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先師在內所得。",
        "from": "X88n1654_001:0390c05_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恩賜等書將奉上三藏和上。",
        "from": "X88n1654_001:0390c05_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "充授法之恩。",
        "from": "X88n1654_001:0390c05_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每於閣下。",
        "from": "X88n1654_001:0390c06_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "節食邀期。",
        "from": "X88n1654_001:0390c06_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時念凡經數遍。",
        "from": "X88n1654_001:0390c06_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年廿五。",
        "from": "X88n1654_001:0390c06_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "特奉恩旨詔命入內。",
        "from": "X88n1654_001:0390c07_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於長生殿。",
        "from": "X88n1654_001:0390c07_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當時有勑喚。",
        "from": "X88n1654_001:0390c07_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對問。",
        "from": "X88n1654_001:0390c07_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師有何功効。",
        "from": "X88n1654_001:0390c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夾天云。",
        "from": "X88n1654_001:0390c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微僧未有功効。",
        "from": "X88n1654_001:0390c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉　勑便誡當時。",
        "from": "X88n1654_001:0390c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喚童子八人。",
        "from": "X88n1654_001:0390c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "考召加持　恩命所問。",
        "from": "X88n1654_001:0390c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡皆成就。",
        "from": "X88n1654_001:0390c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉瓶合竹。",
        "from": "X88n1654_001:0390c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并得成就。",
        "from": "X88n1654_001:0390c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　帝乃大喜。",
        "from": "X88n1654_001:0390c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至大曆十年。",
        "from": "X88n1654_001:0390c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於當寺。",
        "from": "X88n1654_001:0390c10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別　勑賜東塔院一所。",
        "from": "X88n1654_001:0390c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置毗盧遮那灌頂道場。",
        "from": "X88n1654_001:0390c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七僧持念。",
        "from": "X88n1654_001:0390c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至大曆十一年。",
        "from": "X88n1654_001:0390c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持代宗皇帝。",
        "from": "X88n1654_001:0390c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應時便差。",
        "from": "X88n1654_001:0390c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　勅賜紫衣一對。",
        "from": "X88n1654_001:0390c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口奏不敢受。",
        "from": "X88n1654_001:0390c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應時却進。",
        "from": "X88n1654_001:0390c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　勅云。",
        "from": "X88n1654_001:0390c13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朕已賜師紫衣。",
        "from": "X88n1654_001:0390c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何得不受。",
        "from": "X88n1654_001:0390c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奏云。",
        "from": "X88n1654_001:0390c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一界微僧。",
        "from": "X88n1654_001:0390c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賜紫不勝頂荷。",
        "from": "X88n1654_001:0390c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即合便受。",
        "from": "X88n1654_001:0390c14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏緣和上先皇賜紫。",
        "from": "X88n1654_001:0390c15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弟子不合與師齊。",
        "from": "X88n1654_001:0390c15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝云。",
        "from": "X88n1654_001:0390c15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師大孝。",
        "from": "X88n1654_001:0390c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是朕之錯。",
        "from": "X88n1654_001:0390c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　勅云。",
        "from": "X88n1654_001:0390c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他宗異姓上。",
        "from": "X88n1654_001:0390c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "存遵奉孝敬之心。",
        "from": "X88n1654_001:0390c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此以後。",
        "from": "X88n1654_001:0390c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堪為國師。",
        "from": "X88n1654_001:0390c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂賜褐衣一對。",
        "from": "X88n1654_001:0390c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉　勅加持。",
        "from": "X88n1654_001:0390c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "花陽公主。",
        "from": "X88n1654_001:0390c18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三日得差。",
        "from": "X88n1654_001:0390c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後於申未之問。",
        "from": "X88n1654_001:0390c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公主忽然不語。",
        "from": "X88n1654_001:0390c18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　皇帝與宮人等。",
        "from": "X88n1654_001:0390c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一向前。",
        "from": "X88n1654_001:0390c19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和上奏云。",
        "from": "X88n1654_001:0390c19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時熱。",
        "from": "X88n1654_001:0390c19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "望階下與宮人等。",
        "from": "X88n1654_001:0390c19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蹔向微僧加持即令得差。",
        "from": "X88n1654_001:0390c20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其公主便差。",
        "from": "X88n1654_001:0390c20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言詞分明。",
        "from": "X88n1654_001:0390c20_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皇帝大悅。",
        "from": "X88n1654_001:0390c21_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賜絹一百疋。",
        "from": "X88n1654_001:0390c21_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣一對。",
        "from": "X88n1654_001:0390c21_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謝　勅。",
        "from": "X88n1654_001:0390c21_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臣一界微僧。",
        "from": "X88n1654_001:0390c21_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謝聖慈賜絹及衣。",
        "from": "X88n1654_001:0390c22_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不勝頂賀。",
        "from": "X88n1654_001:0390c22_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(三日口奏移公主)大曆秊中。",
        "from": "X88n1654_001:0390c22_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有恩賜錢物。",
        "from": "X88n1654_001:0390c23_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一千餘貫。",
        "from": "X88n1654_001:0390c23_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡修塔下功德。",
        "from": "X88n1654_001:0390c23_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大曆十三年。",
        "from": "X88n1654_001:0390c23_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奏請前後兩度巡南臺。",
        "from": "X88n1654_001:0390c24_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依師所奏。",
        "from": "X88n1654_001:0390c24_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和上於觀音臺。",
        "from": "X88n1654_001:0390c24_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持念。",
        "from": "X88n1654_001:0391a01_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜久之間。",
        "from": "X88n1654_001:0391a01_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大聖觀音。",
        "from": "X88n1654_001:0391a01_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於大月輪中。",
        "from": "X88n1654_001:0391a01_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現大身相。",
        "from": "X88n1654_001:0391a01_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光明由如白日。",
        "from": "X88n1654_001:0391a02_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祥雲皎潔。",
        "from": "X88n1654_001:0391a02_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同時數百千人。",
        "from": "X88n1654_001:0391a02_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遙共瞻禮。",
        "from": "X88n1654_001:0391a02_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大歷十三年。",
        "from": "X88n1654_001:0391a03_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　勅長生殿內道場三朝傳法灌頂歿故三藏和上。",
        "from": "X88n1654_001:0391a03_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　勅語惠果。",
        "from": "X88n1654_001:0391a04_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏和上歿後。",
        "from": "X88n1654_001:0391a04_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將為佛法無去時。",
        "from": "X88n1654_001:0391a04_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞師學得大法。",
        "from": "X88n1654_001:0391a05_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總在緣國事□和上所邊。",
        "from": "X88n1654_001:0391a05_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "授得念誦法門。",
        "from": "X88n1654_001:0391a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多有癈忘。",
        "from": "X88n1654_001:0391a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佗日降舉問道。",
        "from": "X88n1654_001:0391a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是師位。",
        "from": "X88n1654_001:0391a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大曆十三年十月十五日。",
        "from": "X88n1654_001:0391a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高李憲誠宣。",
        "from": "X88n1654_001:0391a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建中年初。",
        "from": "X88n1654_001:0391a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有□□國僧辨弘。",
        "from": "X88n1654_001:0391a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從本國。",
        "from": "X88n1654_001:0391a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將銅鈸一具。",
        "from": "X88n1654_001:0391a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉上聖佛院。",
        "from": "X88n1654_001:0391a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "螺兩具。",
        "from": "X88n1654_001:0391a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "銅□瓶四。",
        "from": "X88n1654_001:0391a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□奉上和上。",
        "from": "X88n1654_001:0391a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "充供養。",
        "from": "X88n1654_001:0391a09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求授胎藏毗盧遮那大法。",
        "from": "X88n1654_001:0391a09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建中二年。",
        "from": "X88n1654_001:0391a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "新羅國僧惠日。",
        "from": "X88n1654_001:0391a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將本國信物。",
        "from": "X88n1654_001:0391a10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉上和上。",
        "from": "X88n1654_001:0391a10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求授胎藏金剛界蘇悉地等。",
        "from": "X88n1654_001:0391a11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并諸尊瑜伽三十本。",
        "from": "X88n1654_001:0391a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已來授訖。",
        "from": "X88n1654_001:0391a12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精通後時。",
        "from": "X88n1654_001:0391a12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "却歸本國。",
        "from": "X88n1654_001:0391a12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣弘大教。",
        "from": "X88n1654_001:0391a12_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精誠絕粒持念。",
        "from": "X88n1654_001:0391a12_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉地現前。",
        "from": "X88n1654_001:0391a13_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂白日冲天。",
        "from": "X88n1654_001:0391a13_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其國王宮中瞻禮。",
        "from": "X88n1654_001:0391a13_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求乞其法。",
        "from": "X88n1654_001:0391a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空中□言西大唐國。",
        "from": "X88n1654_001:0391a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有秘密法法有青龍寺。",
        "from": "X88n1654_001:0391a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同年新羅國僧悟真。",
        "from": "X88n1654_001:0391a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "授胎藏毗盧遮那及諸尊持念教法等。",
        "from": "X88n1654_001:0391a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至貞元五秊。",
        "from": "X88n1654_001:0391a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往於中天竺國。",
        "from": "X88n1654_001:0391a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□毗盧遮那經梵夾餘經。",
        "from": "X88n1654_001:0391a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吐藩國身歿。",
        "from": "X88n1654_001:0391a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當院弟子僧義明。",
        "from": "X88n1654_001:0391a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義滿。",
        "from": "X88n1654_001:0391a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義澄。",
        "from": "X88n1654_001:0391a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同時於和上。",
        "from": "X88n1654_001:0391a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求授毗盧遮那胎藏蘇悉地等經三十本。",
        "from": "X88n1654_001:0391a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貞元五年。",
        "from": "X88n1654_001:0391a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉　勅於當寺大佛殿□。",
        "from": "X88n1654_001:0391a19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令七僧祈雨。",
        "from": "X88n1654_001:0391a19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第七日夜雨足。",
        "from": "X88n1654_001:0391a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各賜絹一束。",
        "from": "X88n1654_001:0391a20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "茶十串表謝。",
        "from": "X88n1654_001:0391a20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉　勅於右衛。",
        "from": "X88n1654_001:0391a20_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍迎真身入內。",
        "from": "X88n1654_001:0391a21_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貞元六年四月□曰。",
        "from": "X88n1654_001:0391a21_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉　勅令僧惠果入內。",
        "from": "X88n1654_001:0391a21_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於長生殿。",
        "from": "X88n1654_001:0391a22_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為國持念。",
        "from": "X88n1654_001:0391a22_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在內七十餘日。",
        "from": "X88n1654_001:0391a22_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放歸。",
        "from": "X88n1654_001:0391a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每人賜絹卅疋。",
        "from": "X88n1654_001:0391a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "茶廿串。",
        "from": "X88n1654_001:0391a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後乃分番上下。",
        "from": "X88n1654_001:0391a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□賜四時衣物。",
        "from": "X88n1654_001:0391a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三節料。",
        "from": "X88n1654_001:0391a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當年杜相公黃裳韋相公親言受灌頂。",
        "from": "X88n1654_001:0391a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "學持念。",
        "from": "X88n1654_001:0391b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "再奏請巡臺。",
        "from": "X88n1654_001:0391b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉　勅宜依。",
        "from": "X88n1654_001:0391b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "充觀音寺大德。",
        "from": "X88n1654_001:0391b01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貞元九秊。",
        "from": "X88n1654_001:0391b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後至十三秊。",
        "from": "X88n1654_001:0391b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義恒。",
        "from": "X88n1654_001:0391b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義一。",
        "from": "X88n1654_001:0391b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義政。",
        "from": "X88n1654_001:0391b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義□。",
        "from": "X88n1654_001:0391b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義操。",
        "from": "X88n1654_001:0391b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義雲。",
        "from": "X88n1654_001:0391b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智興。",
        "from": "X88n1654_001:0391b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義慜。",
        "from": "X88n1654_001:0391b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行堅。",
        "from": "X88n1654_001:0391b03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓通。",
        "from": "X88n1654_001:0391b03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義倫。",
        "from": "X88n1654_001:0391b03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義播。",
        "from": "X88n1654_001:0391b03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義潤。",
        "from": "X88n1654_001:0391b03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俗弟子吳殷。",
        "from": "X88n1654_001:0391b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開丕等。",
        "from": "X88n1654_001:0391b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "約五十人學法。",
        "from": "X88n1654_001:0391b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貞元十四年。",
        "from": "X88n1654_001:0391b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五月大旱。",
        "from": "X88n1654_001:0391b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五月上旬。",
        "from": "X88n1654_001:0391b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉　勅祈雨。",
        "from": "X88n1654_001:0391b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七日。",
        "from": "X88n1654_001:0391b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在內道場。",
        "from": "X88n1654_001:0391b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "專精持念。",
        "from": "X88n1654_001:0391b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祈雨日足。",
        "from": "X88n1654_001:0391b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　恩賜絹一束。",
        "from": "X88n1654_001:0391b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "茶十串。",
        "from": "X88n1654_001:0391b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謝。",
        "from": "X88n1654_001:0391b06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臣僧等。",
        "from": "X88n1654_001:0391b06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "索無功行。",
        "from": "X88n1654_001:0391b06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天降甘雨。",
        "from": "X88n1654_001:0391b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皇帝感化。",
        "from": "X88n1654_001:0391b07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧等謝絹及茶。",
        "from": "X88n1654_001:0391b07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不勝頂賀。",
        "from": "X88n1654_001:0391b07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貞元十五年。",
        "from": "X88n1654_001:0391b07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八月下旬。",
        "from": "X88n1654_001:0391b08_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持皇太子。",
        "from": "X88n1654_001:0391b08_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三日得差。",
        "from": "X88n1654_001:0391b08_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各賜絹廿疋。",
        "from": "X88n1654_001:0391b08_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吳綾五十疋。",
        "from": "X88n1654_001:0391b08_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "茶廿串。",
        "from": "X88n1654_001:0391b09_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臣等一界微僧。",
        "from": "X88n1654_001:0391b09_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謝聖慈賜綾絹及茶。",
        "from": "X88n1654_001:0391b09_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不勝頂賀。",
        "from": "X88n1654_001:0391b10_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十六年冬十一月十六日。",
        "from": "X88n1654_001:0391b10_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神威軍焦護軍。",
        "from": "X88n1654_001:0391b10_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請和上軍宅供養。",
        "from": "X88n1654_001:0391b11_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并和上寫真。",
        "from": "X88n1654_001:0391b11_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "裝飾送院。",
        "from": "X88n1654_001:0391b11_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十八年。",
        "from": "X88n1654_001:0391b11_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和上得族漸重。",
        "from": "X88n1654_001:0391b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "進狀請退。",
        "from": "X88n1654_001:0391b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恩命放歸。",
        "from": "X88n1654_001:0391b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且令寺將息。",
        "from": "X88n1654_001:0391b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　朕意欲存終始。",
        "from": "X88n1654_001:0391b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "贊即不得。",
        "from": "X88n1654_001:0391b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其年八月中旬。",
        "from": "X88n1654_001:0391b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨衣鉢。",
        "from": "X88n1654_001:0391b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "付屬義明等七人授用。",
        "from": "X88n1654_001:0391b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貞元十九年。",
        "from": "X88n1654_001:0391b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日本國僧空海。",
        "from": "X88n1654_001:0391b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉　勅將摩衲及國信物五百餘貫文。",
        "from": "X88n1654_001:0391b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉上和上。",
        "from": "X88n1654_001:0391b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡將修飾道場供養。",
        "from": "X88n1654_001:0391b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求授大悲胎藏金剛界。",
        "from": "X88n1654_001:0391b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并諸尊瑜伽教法。",
        "from": "X88n1654_001:0391b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經五十本。",
        "from": "X88n1654_001:0391b17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "登時見境界梵阿字日月輪。",
        "from": "X88n1654_001:0391b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現入口中。",
        "from": "X88n1654_001:0391b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貞元廿一年。",
        "from": "X88n1654_001:0391b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八月五日。",
        "from": "X88n1654_001:0391b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "改為永貞元秊。",
        "from": "X88n1654_001:0391b18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二月十五日。",
        "from": "X88n1654_001:0391b18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北首掩終。",
        "from": "X88n1654_001:0391b19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至元和元年正月十七日。",
        "from": "X88n1654_001:0391b19_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弟子道俗約千餘人。",
        "from": "X88n1654_001:0391b19_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "送葬。",
        "from": "X88n1654_001:0391b20_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至孟村龍原大師塔側。",
        "from": "X88n1654_001:0391b20_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "厥後至寶曆二年八月二十一日。",
        "from": "X88n1654_001:0391b20_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義一。",
        "from": "X88n1654_001:0391b21_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深達。",
        "from": "X88n1654_001:0391b21_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "義丹。",
        "from": "X88n1654_001:0391b21_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滻川之側。",
        "from": "X88n1654_001:0391b21_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "表藺村。",
        "from": "X88n1654_001:0391b21_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建塔移葬。",
        "from": "X88n1654_001:0391b22_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開成四年。",
        "from": "X88n1654_001:0391b22_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正月十三日。",
        "from": "X88n1654_001:0391b22_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日本國僧圓行。",
        "from": "X88n1654_001:0391b22_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將法衣信物。",
        "from": "X88n1654_001:0391b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(寫本批云)",
        "from": "X88n1654_001:0391c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "承安元年(大歲辛卯)八月二十五日戌尅書畢(中川本也)觀應元年九月晦日。",
        "from": "X88n1654_001:0391c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於東寺西院僧坊。",
        "from": "X88n1654_001:0391c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "書寫之訖。",
        "from": "X88n1654_001:0391c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為一具抄中□□之也。",
        "from": "X88n1654_001:0391c05_0",
        "context_with_next_sentence": false
    }
]