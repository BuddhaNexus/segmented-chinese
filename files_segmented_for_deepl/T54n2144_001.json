[
    {
        "source_sentence": "大秦景教流行中國碑頌(并序)",
        "from": "T54n2144_001:1289a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大秦寺僧景淨述",
        "from": "T54n2144_001:1289a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "粵若。",
        "from": "T54n2144_001:1289a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常然真寂。",
        "from": "T54n2144_001:1289a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先先而無元。",
        "from": "T54n2144_001:1289a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "窅然靈虛。",
        "from": "T54n2144_001:1289a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後後而妙有。",
        "from": "T54n2144_001:1289a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總玄摳而造化。",
        "from": "T54n2144_001:1289a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙眾聖以元尊者。",
        "from": "T54n2144_001:1289a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其唯　我三一妙身無元真主阿羅訶歟判。",
        "from": "T54n2144_001:1289a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十字以定四方。",
        "from": "T54n2144_001:1289a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鼓元風而生二氣。",
        "from": "T54n2144_001:1289a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "暗空易而天地開。",
        "from": "T54n2144_001:1289a09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日月運而晝夜作。",
        "from": "T54n2144_001:1289a10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "匠成萬物然立初人。",
        "from": "T54n2144_001:1289a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別賜良和令鎮化海。",
        "from": "T54n2144_001:1289a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "渾元之性虛而不盈。",
        "from": "T54n2144_001:1289a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "素蕩之心本無希嗜。",
        "from": "T54n2144_001:1289a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洎乎娑殫施妄。",
        "from": "T54n2144_001:1289a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鈿飾純精。",
        "from": "T54n2144_001:1289a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "間平大於此是之中。",
        "from": "T54n2144_001:1289a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隙冥同於彼非之內。",
        "from": "T54n2144_001:1289a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是以三百六十五種。",
        "from": "T54n2144_001:1289a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "肩隨結轍。",
        "from": "T54n2144_001:1289a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "競織法羅。",
        "from": "T54n2144_001:1289a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或指物以託宗。",
        "from": "T54n2144_001:1289a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或空有以淪二。",
        "from": "T54n2144_001:1289a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或禱祀以邀福。",
        "from": "T54n2144_001:1289a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或伐善以矯人。",
        "from": "T54n2144_001:1289a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智慮營營。",
        "from": "T54n2144_001:1289a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恩情役役。",
        "from": "T54n2144_001:1289a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "茫然無得。",
        "from": "T54n2144_001:1289a16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煎迫轉燒。",
        "from": "T54n2144_001:1289a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "積昧亡途久迷休復。",
        "from": "T54n2144_001:1289a17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是　我三一分身景尊彌施訶。",
        "from": "T54n2144_001:1289a17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戢隱真威。",
        "from": "T54n2144_001:1289a18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同人出代。",
        "from": "T54n2144_001:1289a18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神天宣慶。",
        "from": "T54n2144_001:1289a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "室女誕聖。",
        "from": "T54n2144_001:1289a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於大秦景宿告祥。",
        "from": "T54n2144_001:1289a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "波斯覩耀以來貢。",
        "from": "T54n2144_001:1289a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓二十四聖有說之舊法。",
        "from": "T54n2144_001:1289a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "理家國於大猷。",
        "from": "T54n2144_001:1289a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設　三一淨風無言之新教。",
        "from": "T54n2144_001:1289a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陶良用於正信。",
        "from": "T54n2144_001:1289a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "制八境之度。",
        "from": "T54n2144_001:1289a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鍊塵成真。",
        "from": "T54n2144_001:1289a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "啟三常之門。",
        "from": "T54n2144_001:1289a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開生滅死。",
        "from": "T54n2144_001:1289a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懸景日以破暗府。",
        "from": "T54n2144_001:1289a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "魔妄於是乎悉摧。",
        "from": "T54n2144_001:1289a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "棹慈航以登明宮。",
        "from": "T54n2144_001:1289a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "含靈於是乎既濟。",
        "from": "T54n2144_001:1289a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能事斯畢。",
        "from": "T54n2144_001:1289a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亭午昇真。",
        "from": "T54n2144_001:1289a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經留二十七部。",
        "from": "T54n2144_001:1289a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "張元化以發靈關。",
        "from": "T54n2144_001:1289a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法浴水風。",
        "from": "T54n2144_001:1289a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滌浮華而潔虛白。",
        "from": "T54n2144_001:1289a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印持十字。",
        "from": "T54n2144_001:1289a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "融四照以合無拘。",
        "from": "T54n2144_001:1289a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擊木震仁惠之音。",
        "from": "T54n2144_001:1289a27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東禮趣生榮之路。",
        "from": "T54n2144_001:1289a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "存鬚所以有外行。",
        "from": "T54n2144_001:1289a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "削頂所以無內情。",
        "from": "T54n2144_001:1289a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不畜臧獲。",
        "from": "T54n2144_001:1289a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "均貴賤於人。",
        "from": "T54n2144_001:1289a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不聚貨財示罄遺於我。",
        "from": "T54n2144_001:1289a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "齋以伏識而成。",
        "from": "T54n2144_001:1289b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戒以靜慎為固。",
        "from": "T54n2144_001:1289b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七時禮讚。",
        "from": "T54n2144_001:1289b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大庇存亡。",
        "from": "T54n2144_001:1289b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七日一薦。",
        "from": "T54n2144_001:1289b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洗心反素。",
        "from": "T54n2144_001:1289b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真常之道。",
        "from": "T54n2144_001:1289b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙而難名。",
        "from": "T54n2144_001:1289b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功用昭彰。",
        "from": "T54n2144_001:1289b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "強稱景教。",
        "from": "T54n2144_001:1289b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟道非聖不弘。",
        "from": "T54n2144_001:1289b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖非道不大。",
        "from": "T54n2144_001:1289b04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道聖符契。",
        "from": "T54n2144_001:1289b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天下文明　太宗文皇帝。",
        "from": "T54n2144_001:1289b04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光華啟運。",
        "from": "T54n2144_001:1289b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明聖臨人。",
        "from": "T54n2144_001:1289b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大秦國有上德。",
        "from": "T54n2144_001:1289b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰阿羅本。",
        "from": "T54n2144_001:1289b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "占青雲而載真經。",
        "from": "T54n2144_001:1289b06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "望風律以馳艱險。",
        "from": "T54n2144_001:1289b06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貞觀九祀至於長安　帝使宰臣房公玄齡總仗西郊賓迎入內。",
        "from": "T54n2144_001:1289b07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翻經書殿。",
        "from": "T54n2144_001:1289b08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問道禁闈。",
        "from": "T54n2144_001:1289b08_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深知正真。",
        "from": "T54n2144_001:1289b09_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "特令傳授。",
        "from": "T54n2144_001:1289b09_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貞觀十有二年秋七月。",
        "from": "T54n2144_001:1289b09_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詔曰道無常名。",
        "from": "T54n2144_001:1289b10_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖無常體。",
        "from": "T54n2144_001:1289b10_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨方設教。",
        "from": "T54n2144_001:1289b10_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密濟群生。",
        "from": "T54n2144_001:1289b11_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大秦國大德阿羅本。",
        "from": "T54n2144_001:1289b11_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠將經像來獻上京。",
        "from": "T54n2144_001:1289b11_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詳其教旨。",
        "from": "T54n2144_001:1289b12_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玄妙無為。",
        "from": "T54n2144_001:1289b12_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀其元宗。",
        "from": "T54n2144_001:1289b12_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生成立要。",
        "from": "T54n2144_001:1289b12_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詞無繁說。",
        "from": "T54n2144_001:1289b13_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "理有忘筌。",
        "from": "T54n2144_001:1289b13_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "濟物利人。",
        "from": "T54n2144_001:1289b13_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宜行天下。",
        "from": "T54n2144_001:1289b13_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所司即於京義寧坊造大秦寺。",
        "from": "T54n2144_001:1289b14_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一所度僧二十一人。",
        "from": "T54n2144_001:1289b14_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宗周德喪。",
        "from": "T54n2144_001:1289b15_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "青駕西昇。",
        "from": "T54n2144_001:1289b15_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "巨唐道光。",
        "from": "T54n2144_001:1289b15_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "景風東扇。",
        "from": "T54n2144_001:1289b16_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旋令有司將　帝寫真轉摸寺壁。",
        "from": "T54n2144_001:1289b16_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天姿汎彩。",
        "from": "T54n2144_001:1289b17_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "英朗景門。",
        "from": "T54n2144_001:1289b17_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖迹騰祥。",
        "from": "T54n2144_001:1289b17_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永輝法界。",
        "from": "T54n2144_001:1289b17_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案西域圖記及漢魏史策。",
        "from": "T54n2144_001:1289b18_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大秦國南統珊瑚之海。",
        "from": "T54n2144_001:1289b18_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北極眾寶之山。",
        "from": "T54n2144_001:1289b19_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西望仙境花林。",
        "from": "T54n2144_001:1289b19_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東接長風弱水。",
        "from": "T54n2144_001:1289b19_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其土出火浣布．返魂香．明月珠．夜光璧。",
        "from": "T54n2144_001:1289b20_54",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俗無寇盜。",
        "from": "T54n2144_001:1289b21_55",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人有樂康。",
        "from": "T54n2144_001:1289b21_56",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法非景不行。",
        "from": "T54n2144_001:1289b21_57",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "主非德不立。",
        "from": "T54n2144_001:1289b21_58",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "土宇廣濶。",
        "from": "T54n2144_001:1289b22_59",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文物昌明。",
        "from": "T54n2144_001:1289b22_60",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　高宗大帝。",
        "from": "T54n2144_001:1289b22_61",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "克恭纘祖。",
        "from": "T54n2144_001:1289b22_62",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "潤色真宗。",
        "from": "T54n2144_001:1289b23_63",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而於諸州各置景寺。",
        "from": "T54n2144_001:1289b23_64",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仍崇阿羅本為鎮國大法主。",
        "from": "T54n2144_001:1289b23_65",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法流十道。",
        "from": "T54n2144_001:1289b24_66",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國富元休。",
        "from": "T54n2144_001:1289b24_67",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寺滿百城。",
        "from": "T54n2144_001:1289b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "家殷景福。",
        "from": "T54n2144_001:1289b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖曆年。",
        "from": "T54n2144_001:1289b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋子用壯。",
        "from": "T54n2144_001:1289b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "騰口於東周。",
        "from": "T54n2144_001:1289b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先天末。",
        "from": "T54n2144_001:1289b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下士大笑。",
        "from": "T54n2144_001:1289b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "訕謗於西鎬。",
        "from": "T54n2144_001:1289b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有若僧首羅含．大德及烈．並金方貴緒．物外高僧。",
        "from": "T54n2144_001:1289b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共振玄網。",
        "from": "T54n2144_001:1289b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱維絕紐　玄宗至道皇帝。",
        "from": "T54n2144_001:1289b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令寧國等五王親臨福宇建立壇場。",
        "from": "T54n2144_001:1289b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法棟暫橈而更崇。",
        "from": "T54n2144_001:1289b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道石時傾而復正。",
        "from": "T54n2144_001:1289c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天寶初。",
        "from": "T54n2144_001:1289c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令大將軍高力士送　五聖寫真寺內安置。",
        "from": "T54n2144_001:1289c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賜絹百匹。",
        "from": "T54n2144_001:1289c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉慶睿圖。",
        "from": "T54n2144_001:1289c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍髯雖遠。",
        "from": "T54n2144_001:1289c03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弓劍可攀。",
        "from": "T54n2144_001:1289c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日角舒光。",
        "from": "T54n2144_001:1289c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天顏咫尺。",
        "from": "T54n2144_001:1289c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三載大秦國有僧佶和。",
        "from": "T54n2144_001:1289c04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞻星向化。",
        "from": "T54n2144_001:1289c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "望日朝尊。",
        "from": "T54n2144_001:1289c04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詔僧羅含僧普論等一七人。",
        "from": "T54n2144_001:1289c05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大德佶和。",
        "from": "T54n2144_001:1289c05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於興慶宮修功德。",
        "from": "T54n2144_001:1289c06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是天題寺牓。",
        "from": "T54n2144_001:1289c06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "額戴龍書。",
        "from": "T54n2144_001:1289c06_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶裝璀翠。",
        "from": "T54n2144_001:1289c07_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灼爍丹霞。",
        "from": "T54n2144_001:1289c07_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "睿扎宏空。",
        "from": "T54n2144_001:1289c07_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "騰淩激曰。",
        "from": "T54n2144_001:1289c07_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寵賚比南山峻極。",
        "from": "T54n2144_001:1289c08_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沛澤與東海齊深。",
        "from": "T54n2144_001:1289c08_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道無不可。",
        "from": "T54n2144_001:1289c08_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所可可名。",
        "from": "T54n2144_001:1289c09_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖無不作。",
        "from": "T54n2144_001:1289c09_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所作可述　肅宗文明皇帝。",
        "from": "T54n2144_001:1289c09_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於靈武等五郡。",
        "from": "T54n2144_001:1289c10_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重立景寺。",
        "from": "T54n2144_001:1289c10_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元善資而福祚開。",
        "from": "T54n2144_001:1289c10_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大慶臨而皇業建　代宗文武皇帝。",
        "from": "T54n2144_001:1289c11_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恢張聖運。",
        "from": "T54n2144_001:1289c12_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從事無為。",
        "from": "T54n2144_001:1289c12_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每於降誕之辰。",
        "from": "T54n2144_001:1289c12_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "錫天香以告成功。",
        "from": "T54n2144_001:1289c12_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頒御饌以光景眾。",
        "from": "T54n2144_001:1289c13_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且乾以美利故能廣生。",
        "from": "T54n2144_001:1289c13_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖以體元故能亭毒　我建中聖神文武皇帝。",
        "from": "T54n2144_001:1289c14_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "披八政以黜陟幽明。",
        "from": "T54n2144_001:1289c15_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闡九疇以惟新景命。",
        "from": "T54n2144_001:1289c15_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化通玄理。",
        "from": "T54n2144_001:1289c16_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祝無愧心。",
        "from": "T54n2144_001:1289c16_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至於方大而虛。",
        "from": "T54n2144_001:1289c16_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "專靜而恕。",
        "from": "T54n2144_001:1289c17_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣慈救眾苦。",
        "from": "T54n2144_001:1289c17_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善貸被群生者。",
        "from": "T54n2144_001:1289c17_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我修行之大猷。",
        "from": "T54n2144_001:1289c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汲引之階漸也。",
        "from": "T54n2144_001:1289c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若使風雨時。",
        "from": "T54n2144_001:1289c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天下靜。",
        "from": "T54n2144_001:1289c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人能理。",
        "from": "T54n2144_001:1289c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "物能清。",
        "from": "T54n2144_001:1289c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "存能昌。",
        "from": "T54n2144_001:1289c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歿能樂。",
        "from": "T54n2144_001:1289c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念生響應。",
        "from": "T54n2144_001:1289c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "情發目誠者。",
        "from": "T54n2144_001:1289c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我景力能事之功用也。",
        "from": "T54n2144_001:1289c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大施主金紫光祿大夫。",
        "from": "T54n2144_001:1289c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同朔方節度副使。",
        "from": "T54n2144_001:1289c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "試殿中監。",
        "from": "T54n2144_001:1289c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賜紫袈裟僧伊斯。",
        "from": "T54n2144_001:1289c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和而好惠。",
        "from": "T54n2144_001:1289c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞道勤行。",
        "from": "T54n2144_001:1289c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠自王舍之城。",
        "from": "T54n2144_001:1289c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聿來中夏。",
        "from": "T54n2144_001:1289c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "術高三代。",
        "from": "T54n2144_001:1289c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藝博十全。",
        "from": "T54n2144_001:1289c23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始効節於丹庭。",
        "from": "T54n2144_001:1289c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃策名於王帳。",
        "from": "T54n2144_001:1289c24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中書令汾陽郡王郭公子儀。",
        "from": "T54n2144_001:1289c24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初總戎於朔方也　肅宗俾之從邁。",
        "from": "T54n2144_001:1289c25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖見親於臥內。",
        "from": "T54n2144_001:1289c26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不自異於行間。",
        "from": "T54n2144_001:1289c26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為公爪牙。",
        "from": "T54n2144_001:1289c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作軍耳目。",
        "from": "T54n2144_001:1289c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能散祿賜。",
        "from": "T54n2144_001:1289c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不積於家。",
        "from": "T54n2144_001:1289c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獻臨恩之頗黎。",
        "from": "T54n2144_001:1289c27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "布辭憩之金罽。",
        "from": "T54n2144_001:1289c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或仍其舊寺。",
        "from": "T54n2144_001:1289c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或重廣法堂。",
        "from": "T54n2144_001:1289c28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "崇飾廊宇。",
        "from": "T54n2144_001:1289c29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如翬斯飛。",
        "from": "T54n2144_001:1289c29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更効景門。",
        "from": "T54n2144_001:1289c29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依仁施利。",
        "from": "T54n2144_001:1289c29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每歲集四寺僧徒。",
        "from": "T54n2144_001:1290a01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虔事精供。",
        "from": "T54n2144_001:1290a01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "備諸五旬。",
        "from": "T54n2144_001:1290a01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餧者來而飯之。",
        "from": "T54n2144_001:1290a02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寒者來而衣之。",
        "from": "T54n2144_001:1290a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "病者療而起之。",
        "from": "T54n2144_001:1290a02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "死者葬而安之。",
        "from": "T54n2144_001:1290a03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清節達娑。",
        "from": "T54n2144_001:1290a03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未聞斯美。",
        "from": "T54n2144_001:1290a03_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白衣景士。",
        "from": "T54n2144_001:1290a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今見其人。",
        "from": "T54n2144_001:1290a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願刻洪碑。",
        "from": "T54n2144_001:1290a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以揚休烈。",
        "from": "T54n2144_001:1290a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詞曰　真主無元。",
        "from": "T54n2144_001:1290a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "湛寂常然。",
        "from": "T54n2144_001:1290a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "權輿匠化。",
        "from": "T54n2144_001:1290a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起地立天。",
        "from": "T54n2144_001:1290a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分身出代。",
        "from": "T54n2144_001:1290a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救度無邊。",
        "from": "T54n2144_001:1290a06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日昇暗滅。",
        "from": "T54n2144_001:1290a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸證真玄　赫赫文皇。",
        "from": "T54n2144_001:1290a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道冠前王。",
        "from": "T54n2144_001:1290a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘時撥亂。",
        "from": "T54n2144_001:1290a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乾廓坤張。",
        "from": "T54n2144_001:1290a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明明景教。",
        "from": "T54n2144_001:1290a08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言歸我唐。",
        "from": "T54n2144_001:1290a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翻經建寺。",
        "from": "T54n2144_001:1290a08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "存歿舟航。",
        "from": "T54n2144_001:1290a08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百福偕作。",
        "from": "T54n2144_001:1290a09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬邦之康　高宗纂祖。",
        "from": "T54n2144_001:1290a09_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更築精宇。",
        "from": "T54n2144_001:1290a09_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和宮敞朗。",
        "from": "T54n2144_001:1290a10_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍滿中土。",
        "from": "T54n2144_001:1290a10_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真道宣明。",
        "from": "T54n2144_001:1290a10_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "式封法主。",
        "from": "T54n2144_001:1290a10_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人有樂康。",
        "from": "T54n2144_001:1290a11_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "物無災苦　玄宗啟聖。",
        "from": "T54n2144_001:1290a11_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "克修真正。",
        "from": "T54n2144_001:1290a11_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "御牓揚輝。",
        "from": "T54n2144_001:1290a12_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天書蔚映。",
        "from": "T54n2144_001:1290a12_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皇圖璀璨。",
        "from": "T54n2144_001:1290a12_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "率土高敬。",
        "from": "T54n2144_001:1290a12_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "庶績咸熈。",
        "from": "T54n2144_001:1290a13_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人賴其慶　肅宗來復。",
        "from": "T54n2144_001:1290a13_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天威引駕。",
        "from": "T54n2144_001:1290a13_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖日舒晶。",
        "from": "T54n2144_001:1290a14_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祥風掃夜。",
        "from": "T54n2144_001:1290a14_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祚歸皇室。",
        "from": "T54n2144_001:1290a14_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祅氛永謝。",
        "from": "T54n2144_001:1290a14_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "止沸定塵。",
        "from": "T54n2144_001:1290a15_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造我區夏　代宗孝義。",
        "from": "T54n2144_001:1290a15_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "德合天地。",
        "from": "T54n2144_001:1290a15_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開貸生成。",
        "from": "T54n2144_001:1290a16_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "物資美利。",
        "from": "T54n2144_001:1290a16_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香以報功。",
        "from": "T54n2144_001:1290a16_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仁以作施。",
        "from": "T54n2144_001:1290a16_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "暘谷來威。",
        "from": "T54n2144_001:1290a17_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月窟畢萃　建中統極。",
        "from": "T54n2144_001:1290a17_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聿修明德。",
        "from": "T54n2144_001:1290a17_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "武肅四溟。",
        "from": "T54n2144_001:1290a18_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文清萬域。",
        "from": "T54n2144_001:1290a18_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燭臨人隱。",
        "from": "T54n2144_001:1290a18_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鏡觀物色。",
        "from": "T54n2144_001:1290a18_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六合昭蘇。",
        "from": "T54n2144_001:1290a19_54",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百蠻取則。",
        "from": "T54n2144_001:1290a19_55",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道惟廣兮。",
        "from": "T54n2144_001:1290a19_56",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應惟密強。",
        "from": "T54n2144_001:1290a19_57",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名言兮演三一　主能作兮臣能述。",
        "from": "T54n2144_001:1290a20_58",
        "context_with_next_sentence": true
    },
