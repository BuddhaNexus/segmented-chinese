[
    {
        "source_sentence": "阿毘達磨識身足論卷第八",
        "from": "T26n1539_008:0568b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提婆設摩阿羅漢造",
        "from": "T26n1539_008:0568b04_0",
