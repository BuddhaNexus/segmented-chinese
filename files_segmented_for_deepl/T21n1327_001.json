[
    {
        "source_sentence": "佛說呪齒經",
        "from": "T21n1327_001:0491b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東晉竺曇無蘭譯",
        "from": "T21n1327_001:0491b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無佛南無法南無比丘僧南無舍利弗大目乾連比丘。",
        "from": "T21n1327_001:0491b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無覺意。",
        "from": "T21n1327_001:0491b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名聞邊北方健陀摩呵衍山。",
        "from": "T21n1327_001:0491b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼有虫王名差吼無。",
        "from": "T21n1327_001:0491b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在某牙齒中止。",
        "from": "T21n1327_001:0491b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今當遣使者無敢食某牙。",
        "from": "T21n1327_001:0491b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及牙根中牙根中牙邊虫。",
        "from": "T21n1327_001:0491b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不即下器中頭破作七分如鳩羅勤蟮。",
        "from": "T21n1327_001:0491b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵天勸是呪南無佛。",
        "from": "T21n1327_001:0491b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令我所呪皆從如願。",
        "from": "T21n1327_001:0491b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說呪齒經",
        "from": "T21n1327_001:0491b13_0",
        "context_with_next_sentence": false
    }
]