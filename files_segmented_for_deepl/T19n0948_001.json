[
    {
        "source_sentence": "金輪王佛頂要略念誦法",
        "from": "T19n0948_001:0189a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T19n0948_001:0189a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空謚大鑒",
        "from": "T19n0948_001:0189a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正號大廣智大興善寺三藏沙門",
        "from": "T19n0948_001:0189a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空奉　詔譯",
        "from": "T19n0948_001:0189a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸供養儀軌。",
        "from": "T19n0948_001:0189a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如餘經中廣說。",
        "from": "T19n0948_001:0189a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今但晨暮修行。",
        "from": "T19n0948_001:0189a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或四時三時常習不間故。",
        "from": "T19n0948_001:0189a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略去重疊簡其要略。",
        "from": "T19n0948_001:0189a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者欲作諸佛頂念誦。",
        "from": "T19n0948_001:0189a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先結三部心印及誦真言。",
        "from": "T19n0948_001:0189a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持三業。",
        "from": "T19n0948_001:0189a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛部心印。",
        "from": "T19n0948_001:0189a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手右押左內相叉。",
        "from": "T19n0948_001:0189a12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二輪並竪真言曰。",
        "from": "T19n0948_001:0189a13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵爾(慈以反下同)曩爾翼(去)迦(半音)次結蓮華部心印。",
        "from": "T19n0948_001:0189a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前印以左輪屈入掌真言曰。",
        "from": "T19n0948_001:0189a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿(去引)嚕(轉舌引)力迦(半音)次結金剛部心印。",
        "from": "T19n0948_001:0189a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前佛部心印。",
        "from": "T19n0948_001:0189a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈右輪入掌真言曰。",
        "from": "T19n0948_001:0189a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉(二合引)地力(二合)迦(半音)次結如來拳印。",
        "from": "T19n0948_001:0189a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手蓋光高勝四指。",
        "from": "T19n0948_001:0189a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "握拳竪左輪。",
        "from": "T19n0948_001:0189a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手作金剛拳。",
        "from": "T19n0948_001:0189a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "握左輪甲真言曰。",
        "from": "T19n0948_001:0189a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵僕(引)欠入嚩(二合)囉吽(引胸喉中聲如牛吼)隨誦真言。",
        "from": "T19n0948_001:0189a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印加持自身五處。",
        "from": "T19n0948_001:0189a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身器清淨與法相應。",
        "from": "T19n0948_001:0189a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便以印加持道場中地。",
        "from": "T19n0948_001:0189a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言七遍。",
        "from": "T19n0948_001:0189a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其處變成金剛界宮。",
        "from": "T19n0948_001:0189a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然而有眾寶。",
        "from": "T19n0948_001:0189a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚴飾如佛淨土。",
        "from": "T19n0948_001:0189a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結被金剛甲冑護身印。",
        "from": "T19n0948_001:0189a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前佛部心印。",
        "from": "T19n0948_001:0189a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二光直竪頭相拄。",
        "from": "T19n0948_001:0189a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈上節如劍形。",
        "from": "T19n0948_001:0189a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二蓋各附二光背。",
        "from": "T19n0948_001:0189a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿相著。",
        "from": "T19n0948_001:0189b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印加持額上右肩左肩心喉五處。",
        "from": "T19n0948_001:0189b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各誦一遍真言曰。",
        "from": "T19n0948_001:0189b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(一)斫羯囉(二合)襪(轉舌)[口*栗]底(二合)鉢囉(二合)捨弭多(二)囉捺囉(三合引)囉捺囉(二合引三)跋娑麼(二合)攃(倉辣反)路瑟抳(二合)灑(四)囉乞灑(二合)囉乞灑(二合)[牟*含](引五)吽(引)發吒(上普鉢反下吒半音)娑嚩(二合引)賀(引六)次結迎請聖眾印。",
        "from": "T19n0948_001:0189b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手仰相叉。",
        "from": "T19n0948_001:0189b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二蓋頭相拄如車輅。",
        "from": "T19n0948_001:0189b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨誦真言。",
        "from": "T19n0948_001:0189b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二輪各撥二光。",
        "from": "T19n0948_001:0189b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向身招之或三或七。",
        "from": "T19n0948_001:0189b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想諸聖眾乘此車輅來降道場。",
        "from": "T19n0948_001:0189b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迎請真言曰。",
        "from": "T19n0948_001:0189b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜麼娑怛[口*(隸木+士)](三合轉舌引)野(一)地尾(二合)迦南(二)薩嚩怛他(去引)誐跢(引)南(三)唵嚩日囉(二合)儗(霓以反)孃(四)羯囉灑(二合)野娑嚩(二合引)賀(引五)次結辦事佛頂印。",
        "from": "T19n0948_001:0189b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辟除結界。",
        "from": "T19n0948_001:0189b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前佛部心印。",
        "from": "T19n0948_001:0189b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直竪二光頭相柱。",
        "from": "T19n0948_001:0189b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈上節如鉤。",
        "from": "T19n0948_001:0189b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨誦真言以印頂上左轉三匝。",
        "from": "T19n0948_001:0189b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辟除一切不祥諸作障者。",
        "from": "T19n0948_001:0189b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便右旋三匝。",
        "from": "T19n0948_001:0189b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成結界辦事真言曰。",
        "from": "T19n0948_001:0189b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜莫三(去)滿多沒馱(引)南(一)唵吒嚕唵(三合)滿馱娑嚩(二合引)賀(引)次結大三昧耶印即前被甲印二。",
        "from": "T19n0948_001:0189b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓋稍開如杵形。",
        "from": "T19n0948_001:0189b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二輪各附二蓋下第一文真言曰。",
        "from": "T19n0948_001:0189b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵餉(商障反)迦隷(轉舌一)摩賀(引)三(去)麼(鼻音)閻娑嚩(二合引)賀隨誦真言。",
        "from": "T19n0948_001:0189b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印准前右旋三匝。",
        "from": "T19n0948_001:0189b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨行人意遠近。",
        "from": "T19n0948_001:0189b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結為大界。",
        "from": "T19n0948_001:0189b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結部母佛眼尊印。",
        "from": "T19n0948_001:0189b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手合掌以二蓋各捻二光背。",
        "from": "T19n0948_001:0189b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令如眼形。",
        "from": "T19n0948_001:0189b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雙屈二輪入掌真言曰。",
        "from": "T19n0948_001:0189b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜莫三(去)滿多沒馱(引)南(一)唵嚕嚕(二字皆轉舌)娑普(二合)嚕(二)入嚩(二合)羅底瑟咤(二合三)悉馱路(引)左寧(四)薩嚩(引)囉他(二合五)娑(去引)馱[寧*頁](寧頂反)娑嚩(二合引)賀(引)次結本尊頂輪王印。",
        "from": "T19n0948_001:0189c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前辦事佛頂印。",
        "from": "T19n0948_001:0189c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二輪並竪。",
        "from": "T19n0948_001:0189c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二蓋平屈兩節頭相拄。",
        "from": "T19n0948_001:0189c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於二輪甲上。",
        "from": "T19n0948_001:0189c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結印當心誦真言七遍或一百八遍。",
        "from": "T19n0948_001:0189c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又加持身五處各一遍。",
        "from": "T19n0948_001:0189c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持力故。",
        "from": "T19n0948_001:0189c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身與本尊合為一體。",
        "from": "T19n0948_001:0189c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一字輪王陀羅尼曰。",
        "from": "T19n0948_001:0189c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜莫三(去)滿多母馱南勃嚕唵(三合嚕字彈舌為一音又引聲從胸喉中出其音如擊大鼓古譯云步林者訛略不正也)次結獻閼伽香水印。",
        "from": "T19n0948_001:0189c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前大三昧耶印。",
        "from": "T19n0948_001:0189c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二蓋附著二光背。",
        "from": "T19n0948_001:0189c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二輪又安二蓋側下第一文。",
        "from": "T19n0948_001:0189c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如商佉羸盃形。",
        "from": "T19n0948_001:0189c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心想印中滿盛香水。",
        "from": "T19n0948_001:0189c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉印額齊。",
        "from": "T19n0948_001:0189c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉獻香水。",
        "from": "T19n0948_001:0189c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "澡浴本尊及諸聖眾真言曰。",
        "from": "T19n0948_001:0189c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜莫三滿多沒馱(引)南(一)唵遏(轉舌)伽(去)囉賀(二合)遏伽(去二)苾哩(二合)野(三)鉢囉(二合)底(丁以反引)砌那沫(轉舌)儉娑嚩(二合引)賀(引)次結普通供養印。",
        "from": "T19n0948_001:0189c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手虛心合掌。",
        "from": "T19n0948_001:0190a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左右五頂各交上節。",
        "from": "T19n0948_001:0190a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想從印流出無量香花飲食宮殿衣服。",
        "from": "T19n0948_001:0190a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養及諸聖眾真言曰。",
        "from": "T19n0948_001:0190a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜莫薩嚩沒馱(一)冒(引)地娑怛嚩(二合引)南(二)唵薩嚩怛囉(二合三)僧(去)矩蘇弭跢(引四)毘吉孃(二合)囉(引)始[寧*頁](五)曩謨娑覩(二合)帝娑嚩(二合引)賀(引六)次結普通佛頂印。",
        "from": "T19n0948_001:0190a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前普供養印稍深交至中節。",
        "from": "T19n0948_001:0190a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如花在掌中。",
        "from": "T19n0948_001:0190a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修行者若為事緣怱速。",
        "from": "T19n0948_001:0190a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能遍結十佛頂印。",
        "from": "T19n0948_001:0190a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但結此印誦諸佛頂真言。",
        "from": "T19n0948_001:0190a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普通。",
        "from": "T19n0948_001:0190a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於修行人亦無過失真言曰。",
        "from": "T19n0948_001:0190a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜莫三(去)滿多母馱(引)南(一)麼(鼻聲)鉢囉(二合)底賀多舍(引)娑(上)曩(引)南(二)唵斫訖囉(二合)襪(轉舌)底唵吽(引三)次誦讚嘆。",
        "from": "T19n0948_001:0190a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿寗(寧定切引)史抳(尼整反引)薩嚩惹(白攞反)孽(言揭反)地帶(引)史抳(一)拽(延結切)捨娑尾(二合)步(引轉舌)囉步(二合)嚩乃(鼻音)迦滿(引)馱吠(敷閉反二)三(去)麼(鼻)娑多(二合)尾儞也(二合引)地跛作訖囉(二合)麼(鼻引)里寧(三)曩謨娑覩(二合)帝怛囉(二合引)怛哩作訖囉(二合)襪(轉舌)底(丁以反)寧(四)每念誦先。",
        "from": "T19n0948_001:0190a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以五支。",
        "from": "T19n0948_001:0190a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或以五相八相等觀智。",
        "from": "T19n0948_001:0190a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成本尊瑜伽(此法並在頂輪瑜伽極深密一一皆須從灌頂阿闍梨親受)或於自身。",
        "from": "T19n0948_001:0190a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想布一字安三處(頂舌心)以字威力能成佛頂輪王尊。",
        "from": "T19n0948_001:0190a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又想曼茶攞中有香水大海。",
        "from": "T19n0948_001:0190a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於海中心有妙高山。",
        "from": "T19n0948_001:0190a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七重金山周匝圍遶。",
        "from": "T19n0948_001:0190a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於妙高頂上有八葉白色蓮華。",
        "from": "T19n0948_001:0190a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一一葉上。",
        "from": "T19n0948_001:0190a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右旋布列輪王七寶。",
        "from": "T19n0948_001:0190a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂金輪象馬珠女兵及主藏。",
        "from": "T19n0948_001:0190a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當前第八葉。",
        "from": "T19n0948_001:0190b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "想安佛眼尊。",
        "from": "T19n0948_001:0190b01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十佛頂眷屬前後圍遶。",
        "from": "T19n0948_001:0190b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先住此觀已。",
        "from": "T19n0948_001:0190b02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後持珠相應念誦。",
        "from": "T19n0948_001:0190b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或百或千或至於萬。",
        "from": "T19n0948_001:0190b02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常功限畢。",
        "from": "T19n0948_001:0190b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捧珠頂戴誦部母尊。",
        "from": "T19n0948_001:0190b03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有功業願尊守護。",
        "from": "T19n0948_001:0190b04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又結根本印加身五處。",
        "from": "T19n0948_001:0190b04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又結普供養印如前供養。",
        "from": "T19n0948_001:0190b05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以悅憙歌讚本尊。",
        "from": "T19n0948_001:0190b05_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量悲願攝護我等。",
        "from": "T19n0948_001:0190b06_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令得解脫悉地相應。",
        "from": "T19n0948_001:0190b06_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣發弘願利樂有情誓成正覺。",
        "from": "T19n0948_001:0190b07_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次捧閼伽如前奉獻。",
        "from": "T19n0948_001:0190b07_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又結辦事佛頂印。",
        "from": "T19n0948_001:0190b08_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左旋解界。",
        "from": "T19n0948_001:0190b08_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又結前迎請印。",
        "from": "T19n0948_001:0190b08_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向外撥二中指。",
        "from": "T19n0948_001:0190b09_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦前真言去羯羅灑野句。",
        "from": "T19n0948_001:0190b09_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應云尾薩惹野。",
        "from": "T19n0948_001:0190b10_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成奉送。",
        "from": "T19n0948_001:0190b10_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又結被甲印誦前真言加身五處。",
        "from": "T19n0948_001:0190b10_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又結三部心印。",
        "from": "T19n0948_001:0190b11_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虔誠作禮發願。",
        "from": "T19n0948_001:0190b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "任出道場或旋繞經行。",
        "from": "T19n0948_001:0190b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉讀大乘經典。",
        "from": "T19n0948_001:0190b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作諸善業以助成就。",
        "from": "T19n0948_001:0190b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金輪王佛頂要略念誦法",
        "from": "T19n0948_001:0190b14_0",
        "context_with_next_sentence": false
    }
]