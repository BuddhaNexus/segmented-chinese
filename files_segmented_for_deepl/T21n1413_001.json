[
    {
        "source_sentence": "佛說尊勝大明王經",
        "from": "T21n1413_001:0927a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫試鴻臚少卿",
        "from": "T21n1413_001:0927a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳法大師臣施護奉　詔譯",
        "from": "T21n1413_001:0927a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告阿難言。",
        "from": "T21n1413_001:0927a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難有尊勝大明王。",
        "from": "T21n1413_001:0927a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃是過去一切諸佛之所宣說。",
        "from": "T21n1413_001:0927a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難彼七俱胝如來亦隨喜宣說。",
        "from": "T21n1413_001:0927a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今復為未來世中一切眾生宣揚顯說。",
        "from": "T21n1413_001:0927a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當諦聽。",
        "from": "T21n1413_001:0927a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難若有善男子善女人。",
        "from": "T21n1413_001:0927a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞此尊勝大明欲受持讀誦者。",
        "from": "T21n1413_001:0927a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先須清淨身心。",
        "from": "T21n1413_001:0927a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命一切諸佛一切尊法一切聖眾。",
        "from": "T21n1413_001:0927a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復歸命大悲觀自在菩薩摩訶薩。",
        "from": "T21n1413_001:0927a15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦復歸命擁護娑婆世界主。",
        "from": "T21n1413_001:0927a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至一切持明天等。",
        "from": "T21n1413_001:0927a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命如是諸佛菩薩及聖眾已。",
        "from": "T21n1413_001:0927a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後可誦。",
        "from": "T21n1413_001:0927a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說尊勝大明王曰。",
        "from": "T21n1413_001:0927a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也(二合)他(引)馱囉馱囉馱囉馱囉馱囉馱囉馱囉　地里地里地里地里地里地里地里　度嚕度嚕度嚕度嚕度嚕度嚕度嚕　酥嚕酥嚕酥嚕酥嚕酥嚕酥嚕酥嚕　摩囉摩囉摩囉摩囉摩囉摩囉摩囉摩囉　彌里彌里彌里彌里彌里彌里彌里彌里　母嚕母嚕母嚕母嚕母嚕母嚕母嚕母嚕　擁護於我及一切眾生　呬隷呬隷呬隷呬隷呬隷呬隷呬隷呬隷呬隷　護嚕護嚕護嚕護嚕護嚕護嚕護嚕護嚕護嚕　吉隷吉隷吉隷吉隷吉隷吉隷吉隷吉隷吉隷　彌隷彌隷彌隷彌隷彌隷彌隷彌隷彌隷彌隷　唧隷唧隷唧隷唧隷唧隷唧隷唧隷唧隷唧隷唧隷　祖嚕祖嚕祖嚕祖嚕祖嚕祖嚕祖嚕祖嚕祖嚕祖嚕　薩哩嚩(二合)部多鉢囉(二合)底史(引)鄧迦嚕彌　莎賀一切冤家發惡心者極毒害者。",
        "from": "T21n1413_001:0927a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲於我及他眾生。",
        "from": "T21n1413_001:0927b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作諸惡法使生諸病或欲障閉令眼不見令耳不聞。",
        "from": "T21n1413_001:0927b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鼻不辯香口不能語。",
        "from": "T21n1413_001:0927b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至一切身分咸欲繫縛者。",
        "from": "T21n1413_001:0927b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而彼冤家或自作或令他作。",
        "from": "T21n1413_001:0927b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至彼類隨喜之者悉皆破壞。",
        "from": "T21n1413_001:0927b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覩嚕覩嚕覩嚕覩嚕莎賀",
        "from": "T21n1413_001:0927b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切魑魅魍魎吠多拏旋風鬼。",
        "from": "T21n1413_001:0927b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸如是等一切鬼神。",
        "from": "T21n1413_001:0927b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲起惡心行不饒益者。",
        "from": "T21n1413_001:0927b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "退散四方遠離而去。",
        "from": "T21n1413_001:0927b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左囉左囉莎賀摩囉摩囉莎賀彌里彌里莎賀度嚕度嚕莎賀一切人非人等發起惡心。",
        "from": "T21n1413_001:0927b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禁制於我及他眾生。",
        "from": "T21n1413_001:0927b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使其狂亂或如癡迷。",
        "from": "T21n1413_001:0927b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或已作或欲作皆悉解除。",
        "from": "T21n1413_001:0927b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儞隷儞羅計世。",
        "from": "T21n1413_001:0927b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莎賀閉帝閉多計世莎賀路(引)呬帝(引)路呬多計世莎賀阿嚩捺(引)帝(引)阿嚩捺(引)多計世莎賀濕吠(二合)帝(引)濕吠(二合)多嚩娑怛囉(三合)馱囉尼(引)曳(引)莎賀王怖賊怖水火等怖。",
        "from": "T21n1413_001:0927b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拏吉儞怖毘舍遮怖。",
        "from": "T21n1413_001:0927b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供畔拏怖羯吒布單曩怖。",
        "from": "T21n1413_001:0927b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "影鬼怖諸如是等惡鬼神怖或在道路或在曠野。",
        "from": "T21n1413_001:0927b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晝夜行時一切安隱。",
        "from": "T21n1413_001:0927b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復冤家見面除解聞語除解。",
        "from": "T21n1413_001:0927b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如親眷屬相見歡喜。",
        "from": "T21n1413_001:0927b25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也(二合)他(引)護嚕護嚕尼吉尼吉里尼吉里尼莎賀一切邪明截斷破壞。",
        "from": "T21n1413_001:0927b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或自焚燒猶如灰燼。",
        "from": "T21n1413_001:0927b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或自傷害或自迷。",
        "from": "T21n1413_001:0927b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亂或釘橛崩倒或隱沒不現。",
        "from": "T21n1413_001:0927c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或依空作法自然墮落。",
        "from": "T21n1413_001:0927c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有文字障礙不行。",
        "from": "T21n1413_001:0927c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "焰魔王界振動驚怖。",
        "from": "T21n1413_001:0927c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也(二合)他(引)娑吒娑吒[口*癹]吒[口*癹]吒[口*癹]吒[口*癹]吒[口*癹]吒部嚕部嚕母嚕母嚕莎賀若魔若魔民以惡心相向。",
        "from": "T21n1413_001:0927c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或自在天或夜叉夜叉女等。",
        "from": "T21n1413_001:0927c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至一切食血者。",
        "from": "T21n1413_001:0927c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食肉者食脂膏者。",
        "from": "T21n1413_001:0927c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食精氣者食涎沫者。",
        "from": "T21n1413_001:0927c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸如是等所執所縛悉皆解脫。",
        "from": "T21n1413_001:0927c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至男女受胎處胎。",
        "from": "T21n1413_001:0927c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初生之時皆得安隱。",
        "from": "T21n1413_001:0927c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難此尊勝大明王是世間母。",
        "from": "T21n1413_001:0927c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切眾生咸同其子。",
        "from": "T21n1413_001:0927c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難若善男子善女人。",
        "from": "T21n1413_001:0927c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得聞如是尊勝大明。",
        "from": "T21n1413_001:0927c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能受持讀誦者。",
        "from": "T21n1413_001:0927c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人無始已來所有罪業皆得除滅。",
        "from": "T21n1413_001:0927c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切吉祥而能增長。",
        "from": "T21n1413_001:0927c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見者歡喜所作成就。",
        "from": "T21n1413_001:0927c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又復善男子善女人。",
        "from": "T21n1413_001:0927c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此明王書於紙素。",
        "from": "T21n1413_001:0927c15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或頂或臂而常戴持。",
        "from": "T21n1413_001:0927c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切不祥悉皆遠離。",
        "from": "T21n1413_001:0927c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於睡夢中亦無惡境。",
        "from": "T21n1413_001:0927c17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火不能燒水不能漂。",
        "from": "T21n1413_001:0927c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸惡皆不侵害。",
        "from": "T21n1413_001:0927c18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又或掬水誦此明王。",
        "from": "T21n1413_001:0927c18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持已吸在鼻中。",
        "from": "T21n1413_001:0927c19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有在身宿業亦得除滅。",
        "from": "T21n1413_001:0927c19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復增善巧恒得安樂。",
        "from": "T21n1413_001:0927c20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切邪明呪詛不饒益事。",
        "from": "T21n1413_001:0927c20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此明王加持淨水或線或白芥子。",
        "from": "T21n1413_001:0927c21_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解除自退。",
        "from": "T21n1413_001:0927c22_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難我先告汝。",
        "from": "T21n1413_001:0927c22_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大明王乃是過去七俱胝如來同共宣說。",
        "from": "T21n1413_001:0927c22_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今復為未來眾生無福無德。",
        "from": "T21n1413_001:0927c23_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有諸惡鬼種種惱亂不得安隱。",
        "from": "T21n1413_001:0927c24_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故宣說汝善受持當使未來流傳不斷。",
        "from": "T21n1413_001:0927c25_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與諸眾生而作恃怙。",
        "from": "T21n1413_001:0927c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊說是尊勝大明王已。",
        "from": "T21n1413_001:0927c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊者阿難歡喜信受禮佛而退。",
        "from": "T21n1413_001:0927c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說尊勝大明王經",
        "from": "T21n1413_001:0927c29_0",
        "context_with_next_sentence": false
    }
]