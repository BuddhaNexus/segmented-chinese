[
    {
        "source_sentence": "佛祖綱目卷第二十(往字號)",
        "from": "X85n1594_020:0582a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甲子(漢明帝永平七年起)丙子(章帝建初元年)甲申(改元和)丁亥(改章和)己丑(和帝永元元年)乙巳(改元興)丙午(殤帝延平元年)丁未(安帝永初元年)甲寅(改元初)庚申(改永寧)辛酉(改建光)壬戌(改延光)癸亥(漢安帝延光二年止)漢明帝。",
        "from": "X85n1594_020:0582a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永平丁卯。",
        "from": "X85n1594_020:0582a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛教始入震旦。",
        "from": "X85n1594_020:0582a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(丁卯)天竺沙門攝摩騰竺法蘭至洛陽東土蔡愔等。",
        "from": "X85n1594_020:0582a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至天竺隣境月支國。",
        "from": "X85n1594_020:0582a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遇梵僧。",
        "from": "X85n1594_020:0582a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝摩騰竺法蘭二僧。",
        "from": "X85n1594_020:0582a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲奉經像。",
        "from": "X85n1594_020:0582a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來此震旦。",
        "from": "X85n1594_020:0582a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂同東還。",
        "from": "X85n1594_020:0582a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永平十年。",
        "from": "X85n1594_020:0582a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至洛陽。",
        "from": "X85n1594_020:0582a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩騰入闕獻經像。",
        "from": "X85n1594_020:0582a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝大悅。",
        "from": "X85n1594_020:0582a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舘於鴻臚寺。",
        "from": "X85n1594_020:0582a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘭亦間行而後至○永平十四年己巳。",
        "from": "X85n1594_020:0582a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明帝詔。",
        "from": "X85n1594_020:0582a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於洛陽城西雍門外。",
        "from": "X85n1594_020:0582a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別立一寺。",
        "from": "X85n1594_020:0582a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使居之。",
        "from": "X85n1594_020:0582a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以白馬馱經而來。",
        "from": "X85n1594_020:0582a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂名白馬寺。",
        "from": "X85n1594_020:0582a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝幸其寺。",
        "from": "X85n1594_020:0582a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問摩騰曰。",
        "from": "X85n1594_020:0582a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛出世後。",
        "from": "X85n1594_020:0582a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以化不及此。",
        "from": "X85n1594_020:0582a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "騰曰。",
        "from": "X85n1594_020:0582a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天竺迦毗羅衛國者。",
        "from": "X85n1594_020:0582a22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三千大千世界。",
        "from": "X85n1594_020:0582a22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百億日月之中。",
        "from": "X85n1594_020:0582a22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三世諸佛。",
        "from": "X85n1594_020:0582a23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆於此出。",
        "from": "X85n1594_020:0582a23_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天神龍鬼。",
        "from": "X85n1594_020:0582a23_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有願力者。",
        "from": "X85n1594_020:0582a23_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡生彼中。",
        "from": "X85n1594_020:0582a23_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受化悟道。",
        "from": "X85n1594_020:0582a24_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘處佛雖不往。",
        "from": "X85n1594_020:0582a24_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然光相及處。",
        "from": "X85n1594_020:0582a24_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千年五百。",
        "from": "X85n1594_020:0582a24_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆有聖人。",
        "from": "X85n1594_020:0582b01_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傳佛聲教。",
        "from": "X85n1594_020:0582b01_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而往化之。",
        "from": "X85n1594_020:0582b01_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝大悅。",
        "from": "X85n1594_020:0582b01_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "騰蘭進曰。",
        "from": "X85n1594_020:0582b01_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寺東何館。",
        "from": "X85n1594_020:0582b02_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝曰。",
        "from": "X85n1594_020:0582b02_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔有阜無因而起。",
        "from": "X85n1594_020:0582b02_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夷之復然。",
        "from": "X85n1594_020:0582b02_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜有異光。",
        "from": "X85n1594_020:0582b02_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "民呼為聖塚。",
        "from": "X85n1594_020:0582b03_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因祀之。",
        "from": "X85n1594_020:0582b03_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疑洛陽神也。",
        "from": "X85n1594_020:0582b03_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "騰曰。",
        "from": "X85n1594_020:0582b03_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "按天竺金藏詮所誌。",
        "from": "X85n1594_020:0582b03_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿育王藏如來舍利於天下。",
        "from": "X85n1594_020:0582b04_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡八萬四千所。",
        "from": "X85n1594_020:0582b04_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今震旦境中。",
        "from": "X85n1594_020:0582b05_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十有九處。",
        "from": "X85n1594_020:0582b05_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此其一也。",
        "from": "X85n1594_020:0582b05_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝大驚。",
        "from": "X85n1594_020:0582b05_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即駕幸拜之。",
        "from": "X85n1594_020:0582b05_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忽有圓光。",
        "from": "X85n1594_020:0582b06_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現塚上。",
        "from": "X85n1594_020:0582b06_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三身現光中。",
        "from": "X85n1594_020:0582b06_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "侍衛呼萬歲。",
        "from": "X85n1594_020:0582b06_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝喜曰。",
        "from": "X85n1594_020:0582b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不遇二大士。",
        "from": "X85n1594_020:0582b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安知上聖遺祐哉。",
        "from": "X85n1594_020:0582b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詔塔其上。",
        "from": "X85n1594_020:0582b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受制度於騰蘭。",
        "from": "X85n1594_020:0582b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塔成九層。",
        "from": "X85n1594_020:0582b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高二百尺。",
        "from": "X85n1594_020:0582b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明年光又現。",
        "from": "X85n1594_020:0582b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有金色手。",
        "from": "X85n1594_020:0582b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出塔頂。",
        "from": "X85n1594_020:0582b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尺許。",
        "from": "X85n1594_020:0582b09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如琉璃。",
        "from": "X85n1594_020:0582b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中見天香郁然。",
        "from": "X85n1594_020:0582b09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝駕幸拜瞻。",
        "from": "X85n1594_020:0582b09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光隨步武旋繞。",
        "from": "X85n1594_020:0582b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自午及申而滅○帝於佛法。",
        "from": "X85n1594_020:0582b10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深相敬信。",
        "from": "X85n1594_020:0582b10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永平十四年。",
        "from": "X85n1594_020:0582b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五嶽道士褚善信費叔才等忌之。",
        "from": "X85n1594_020:0582b11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斥言。",
        "from": "X85n1594_020:0582b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛法虗偽。",
        "from": "X85n1594_020:0582b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "騰蘭白帝曰。",
        "from": "X85n1594_020:0582b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾佛出世間法。",
        "from": "X85n1594_020:0582b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水火不能壞。",
        "from": "X85n1594_020:0582b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請與道士驗之。",
        "from": "X85n1594_020:0582b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝勑善信等。",
        "from": "X85n1594_020:0582b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡出所有奇經秘訣。",
        "from": "X85n1594_020:0582b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與沙門所持梵筴。",
        "from": "X85n1594_020:0582b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於正月十五日。",
        "from": "X85n1594_020:0582b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "築壇火驗。",
        "from": "X85n1594_020:0582b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火作。",
        "from": "X85n1594_020:0582b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而道士諸經俱燼。",
        "from": "X85n1594_020:0582b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟佛經像儼然。",
        "from": "X85n1594_020:0582b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善信等自愧而死。",
        "from": "X85n1594_020:0582b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道士一眾。",
        "from": "X85n1594_020:0582b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稽首欽服。",
        "from": "X85n1594_020:0582b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法蘭於大眾中。",
        "from": "X85n1594_020:0582b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即唱偈言。",
        "from": "X85n1594_020:0582b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "狐非師子類。",
        "from": "X85n1594_020:0582b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燈非日月明。",
        "from": "X85n1594_020:0582b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "池非江海納。",
        "from": "X85n1594_020:0582b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "丘無山岳榮。",
        "from": "X85n1594_020:0582b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法雲垂世界。",
        "from": "X85n1594_020:0582b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善種得開萌。",
        "from": "X85n1594_020:0582b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯通希有法。",
        "from": "X85n1594_020:0582b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "處處化群生。",
        "from": "X85n1594_020:0582b18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自是帝益驚異。",
        "from": "X85n1594_020:0582b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "騰蘭習漢言。",
        "from": "X85n1594_020:0582b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "久之譯四十二章經。",
        "from": "X85n1594_020:0582b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十地斷結。",
        "from": "X85n1594_020:0582b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛本生。",
        "from": "X85n1594_020:0582b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法海藏。",
        "from": "X85n1594_020:0582b20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛本行等五經。",
        "from": "X85n1594_020:0582b20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永平十六年。",
        "from": "X85n1594_020:0582b20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝摩騰入寂。",
        "from": "X85n1594_020:0582b20_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "竺法蘭自譯經五部十三卷。",
        "from": "X85n1594_020:0582b21_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(甲戌)二十祖闍夜多傳法婆修盤頭闍夜多。",
        "from": "X85n1594_020:0582b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北天竺國人。",
        "from": "X85n1594_020:0582b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智慧淵冲。",
        "from": "X85n1594_020:0582b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化導無量。",
        "from": "X85n1594_020:0582b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後至羅閱城。",
        "from": "X85n1594_020:0582b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敷揚頓教。",
        "from": "X85n1594_020:0582b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼有學眾。",
        "from": "X85n1594_020:0582b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟尚辨論。",
        "from": "X85n1594_020:0582b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為之首者。",
        "from": "X85n1594_020:0582b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名婆修盤頭。",
        "from": "X85n1594_020:0582b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(此云徧行)常一食不臥。",
        "from": "X85n1594_020:0582c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六時禮佛。",
        "from": "X85n1594_020:0582c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清淨無欲。",
        "from": "X85n1594_020:0582c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為眾所歸。",
        "from": "X85n1594_020:0582c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜多將欲度之。",
        "from": "X85n1594_020:0582c02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先問彼眾曰。",
        "from": "X85n1594_020:0582c02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此徧行頭陀。",
        "from": "X85n1594_020:0582c02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能修梵行。",
        "from": "X85n1594_020:0582c02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可得佛道乎。",
        "from": "X85n1594_020:0582c03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾曰。",
        "from": "X85n1594_020:0582c03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我師精進。",
        "from": "X85n1594_020:0582c03_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何故不可。",
        "from": "X85n1594_020:0582c03_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜多曰。",
        "from": "X85n1594_020:0582c03_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝師與道遠矣。",
        "from": "X85n1594_020:0582c03_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設苦行歷於塵劫。",
        "from": "X85n1594_020:0582c04_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆虗妄之本也。",
        "from": "X85n1594_020:0582c04_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾曰。",
        "from": "X85n1594_020:0582c04_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊者蘊何德行。",
        "from": "X85n1594_020:0582c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而譏我師。",
        "from": "X85n1594_020:0582c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜多曰。",
        "from": "X85n1594_020:0582c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我不求道。",
        "from": "X85n1594_020:0582c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不顛倒。",
        "from": "X85n1594_020:0582c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我不禮佛。",
        "from": "X85n1594_020:0582c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不輕慢。",
        "from": "X85n1594_020:0582c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我不長坐。",
        "from": "X85n1594_020:0582c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不懈怠。",
        "from": "X85n1594_020:0582c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我不一食。",
        "from": "X85n1594_020:0582c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不雜食。",
        "from": "X85n1594_020:0582c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我不知足。",
        "from": "X85n1594_020:0582c07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不貪慾。",
        "from": "X85n1594_020:0582c07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心無所希。",
        "from": "X85n1594_020:0582c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名之曰道。",
        "from": "X85n1594_020:0582c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時徧行聞已。",
        "from": "X85n1594_020:0582c08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發無漏智。",
        "from": "X85n1594_020:0582c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡喜讚歎。",
        "from": "X85n1594_020:0582c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜多又語彼眾曰。",
        "from": "X85n1594_020:0582c08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "會吾語否。",
        "from": "X85n1594_020:0582c09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾所以然者。",
        "from": "X85n1594_020:0582c09_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為其求道心切。",
        "from": "X85n1594_020:0582c09_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫絃急即斷。",
        "from": "X85n1594_020:0582c09_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故吾不贊。",
        "from": "X85n1594_020:0582c10_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令其住安樂地。",
        "from": "X85n1594_020:0582c10_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入諸佛智。",
        "from": "X85n1594_020:0582c10_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復告徧行曰。",
        "from": "X85n1594_020:0582c10_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾適對眾挫抑。",
        "from": "X85n1594_020:0582c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仁者得無惱於衷乎。",
        "from": "X85n1594_020:0582c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徧行曰。",
        "from": "X85n1594_020:0582c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我憶念七劫前。",
        "from": "X85n1594_020:0582c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生常安樂國。",
        "from": "X85n1594_020:0582c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師於智者月淨。",
        "from": "X85n1594_020:0582c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "記我非久當證斯陀含果。",
        "from": "X85n1594_020:0582c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時有大光明菩薩出世。",
        "from": "X85n1594_020:0582c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我以老故。",
        "from": "X85n1594_020:0582c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "策杖禮謁。",
        "from": "X85n1594_020:0582c13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師叱我曰。",
        "from": "X85n1594_020:0582c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重子輕父。",
        "from": "X85n1594_020:0582c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一何鄙哉。",
        "from": "X85n1594_020:0582c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時我自謂無過。",
        "from": "X85n1594_020:0582c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請師示之。",
        "from": "X85n1594_020:0582c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師曰。",
        "from": "X85n1594_020:0582c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝禮大光明菩薩。",
        "from": "X85n1594_020:0582c15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以杖倚壁畫佛面。",
        "from": "X85n1594_020:0582c15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此過慢。",
        "from": "X85n1594_020:0582c15_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂失二果。",
        "from": "X85n1594_020:0582c16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我責躬悔過以來。",
        "from": "X85n1594_020:0582c16_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞諸惡言。",
        "from": "X85n1594_020:0582c16_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如風如響。",
        "from": "X85n1594_020:0582c16_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "況今獲飲無上甘露。",
        "from": "X85n1594_020:0582c17_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而反生熱惱耶。",
        "from": "X85n1594_020:0582c17_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願大慈。",
        "from": "X85n1594_020:0582c17_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以妙道垂誨。",
        "from": "X85n1594_020:0582c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜多曰。",
        "from": "X85n1594_020:0582c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝久植德本。",
        "from": "X85n1594_020:0582c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當繼吾宗。",
        "from": "X85n1594_020:0582c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聽吾偈曰。",
        "from": "X85n1594_020:0582c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言下合無生。",
        "from": "X85n1594_020:0582c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同於法界性。",
        "from": "X85n1594_020:0582c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能如是解。",
        "from": "X85n1594_020:0582c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通達事理竟。",
        "from": "X85n1594_020:0582c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜多既付法已。",
        "from": "X85n1594_020:0582c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其座上。",
        "from": "X85n1594_020:0582c20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以首倒植。",
        "from": "X85n1594_020:0582c20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "象娑羅樹枝。",
        "from": "X85n1594_020:0582c20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奄然而化。",
        "from": "X85n1594_020:0582c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾欲闍維。",
        "from": "X85n1594_020:0582c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百千人舉之不能。",
        "from": "X85n1594_020:0582c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至諸羅漢以神力舉之。",
        "from": "X85n1594_020:0582c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不能動。",
        "from": "X85n1594_020:0582c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾乃炷香祝之。",
        "from": "X85n1594_020:0582c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂頹然委地。",
        "from": "X85n1594_020:0582c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闍維。",
        "from": "X85n1594_020:0582c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "收舍利建塔。",
        "from": "X85n1594_020:0582c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當此土後漢明帝十七年甲戌也○初月支國王。",
        "from": "X85n1594_020:0582c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞夜多德風。",
        "from": "X85n1594_020:0582c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "躬詣問法。",
        "from": "X85n1594_020:0582c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修敬已。",
        "from": "X85n1594_020:0582c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請開演。",
        "from": "X85n1594_020:0582c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜多曰。",
        "from": "X85n1594_020:0583a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大王來時好道。",
        "from": "X85n1594_020:0583a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今去亦如來時。",
        "from": "X85n1594_020:0583a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王歎服。",
        "from": "X85n1594_020:0583a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(丁巳)二十一祖婆修盤頭傳法摩拏羅婆修盤頭。",
        "from": "X85n1594_020:0583a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅閱城人。",
        "from": "X85n1594_020:0583a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姓毗舍佉。",
        "from": "X85n1594_020:0583a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父光蓋。",
        "from": "X85n1594_020:0583a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母嚴一。",
        "from": "X85n1594_020:0583a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "家富而無子。",
        "from": "X85n1594_020:0583a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父母禱於佛塔。",
        "from": "X85n1594_020:0583a04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而求嗣焉。",
        "from": "X85n1594_020:0583a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一夕母夢。",
        "from": "X85n1594_020:0583a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吞明暗二珠。",
        "from": "X85n1594_020:0583a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覺而有孕。",
        "from": "X85n1594_020:0583a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經七日。",
        "from": "X85n1594_020:0583a05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有一羅漢名賢眾。",
        "from": "X85n1594_020:0583a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至其家。",
        "from": "X85n1594_020:0583a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光蓋設禮。",
        "from": "X85n1594_020:0583a05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢眾端坐受之。",
        "from": "X85n1594_020:0583a06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚴一出拜。",
        "from": "X85n1594_020:0583a06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢眾避席云。",
        "from": "X85n1594_020:0583a06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "回禮法身大士。",
        "from": "X85n1594_020:0583a06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光蓋罔測其繇。",
        "from": "X85n1594_020:0583a07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂取一寶珠跪獻。",
        "from": "X85n1594_020:0583a07_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "試其真偽。",
        "from": "X85n1594_020:0583a07_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢眾即受之。",
        "from": "X85n1594_020:0583a08_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殊無遜謝。",
        "from": "X85n1594_020:0583a08_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光蓋不能忍。",
        "from": "X85n1594_020:0583a08_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "X85n1594_020:0583a08_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我是丈夫。",
        "from": "X85n1594_020:0583a08_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "致禮不顧。",
        "from": "X85n1594_020:0583a09_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我妻何德。",
        "from": "X85n1594_020:0583a09_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊者避之。",
        "from": "X85n1594_020:0583a09_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢眾曰。",
        "from": "X85n1594_020:0583a09_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我受禮納殊。",
        "from": "X85n1594_020:0583a09_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貴福汝耳。",
        "from": "X85n1594_020:0583a10_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝婦懷聖子。",
        "from": "X85n1594_020:0583a10_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生當為世燈慧日。",
        "from": "X85n1594_020:0583a10_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故吾避之。",
        "from": "X85n1594_020:0583a10_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非重女人也。",
        "from": "X85n1594_020:0583a11_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢眾又曰。",
        "from": "X85n1594_020:0583a11_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝婦當一乳而生二子。",
        "from": "X85n1594_020:0583a11_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名婆修盤頭。",
        "from": "X85n1594_020:0583a11_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則吾所尊者也。",
        "from": "X85n1594_020:0583a12_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二名芻尼。",
        "from": "X85n1594_020:0583a12_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(此云野鵲子)昔如來雪山修道。",
        "from": "X85n1594_020:0583a12_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "芻尼巢於頂上。",
        "from": "X85n1594_020:0583a13_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛既成道。",
        "from": "X85n1594_020:0583a13_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "芻尼受報。",
        "from": "X85n1594_020:0583a13_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為那提國王。",
        "from": "X85n1594_020:0583a13_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛記云。",
        "from": "X85n1594_020:0583a14_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝至第二五百年。",
        "from": "X85n1594_020:0583a14_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生羅閱城毗舍佉家。",
        "from": "X85n1594_020:0583a14_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與聖同胞。",
        "from": "X85n1594_020:0583a15_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今無爽矣。",
        "from": "X85n1594_020:0583a15_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後一月果誕二子。",
        "from": "X85n1594_020:0583a15_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊者婆修盤頭。",
        "from": "X85n1594_020:0583a15_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "年十五。",
        "from": "X85n1594_020:0583a16_54",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮光度羅漢出家。",
        "from": "X85n1594_020:0583a16_55",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "感毗婆訶菩薩與之授戒。",
        "from": "X85n1594_020:0583a16_56",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行化至那提國。",
        "from": "X85n1594_020:0583a17_57",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼王名常自在。",
        "from": "X85n1594_020:0583a17_58",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有二子。",
        "from": "X85n1594_020:0583a17_59",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長名摩訶羅。",
        "from": "X85n1594_020:0583a17_60",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次名摩拏羅。",
        "from": "X85n1594_020:0583a18_61",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王問盤頭曰。",
        "from": "X85n1594_020:0583a18_62",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅閱城土風。",
        "from": "X85n1594_020:0583a18_63",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與此何異。",
        "from": "X85n1594_020:0583a18_64",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "X85n1594_020:0583a19_65",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼曾三佛出世。",
        "from": "X85n1594_020:0583a19_66",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今王國有二師化導。",
        "from": "X85n1594_020:0583a19_67",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王曰。",
        "from": "X85n1594_020:0583a19_68",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二師者誰。",
        "from": "X85n1594_020:0583a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "X85n1594_020:0583a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛記。",
        "from": "X85n1594_020:0583a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二五百年。",
        "from": "X85n1594_020:0583a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有二神力大士。",
        "from": "X85n1594_020:0583a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出家繼聖。",
        "from": "X85n1594_020:0583a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一即王之次子摩拏羅。",
        "from": "X85n1594_020:0583a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾雖德薄。",
        "from": "X85n1594_020:0583a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦當其一。",
        "from": "X85n1594_020:0583a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初那提國有惡象為害。",
        "from": "X85n1594_020:0583a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拏羅生而象息。",
        "from": "X85n1594_020:0583a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至是三十年矣。",
        "from": "X85n1594_020:0583a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人尚不知其所以息。",
        "from": "X85n1594_020:0583a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王方同盤頭語。",
        "from": "X85n1594_020:0583a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忽使者報。",
        "from": "X85n1594_020:0583a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有象巨萬逼城。",
        "from": "X85n1594_020:0583a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王憂之。",
        "from": "X85n1594_020:0583a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盤頭曰。",
        "from": "X85n1594_020:0583a24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拏羅出患解矣。",
        "from": "X85n1594_020:0583a24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王試命拏羅出。",
        "from": "X85n1594_020:0583a24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拏羅遂出城南。",
        "from": "X85n1594_020:0583b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向象撫腹大喝。",
        "from": "X85n1594_020:0583b01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "城為震動。",
        "from": "X85n1594_020:0583b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "群象顛仆。",
        "from": "X85n1594_020:0583b01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頃皆馳散。",
        "from": "X85n1594_020:0583b02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至是人始知。",
        "from": "X85n1594_020:0583b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十年之安。",
        "from": "X85n1594_020:0583b02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以拏羅也。",
        "from": "X85n1594_020:0583b02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王大敬信。",
        "from": "X85n1594_020:0583b03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命拏羅。",
        "from": "X85n1594_020:0583b03_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依盤頭出家。",
        "from": "X85n1594_020:0583b03_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盤頭即與授具。",
        "from": "X85n1594_020:0583b03_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "付法偈曰。",
        "from": "X85n1594_020:0583b03_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "泡幻同無礙。",
        "from": "X85n1594_020:0583b04_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如何不了悟。",
        "from": "X85n1594_020:0583b04_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "達法在其中。",
        "from": "X85n1594_020:0583b04_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非今亦非古。",
        "from": "X85n1594_020:0583b04_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "付法已。",
        "from": "X85n1594_020:0583b05_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "踴身高半繇旬。",
        "from": "X85n1594_020:0583b05_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屹然而住。",
        "from": "X85n1594_020:0583b05_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四眾告曰。",
        "from": "X85n1594_020:0583b05_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願尊者無為神通。",
        "from": "X85n1594_020:0583b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我輩欲收舍利。",
        "from": "X85n1594_020:0583b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂復座入寂。",
        "from": "X85n1594_020:0583b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闍維舍利建塔。",
        "from": "X85n1594_020:0583b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當此土後漢安帝十二年丁巳也。",
        "from": "X85n1594_020:0583b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛祖綱目卷第二十",
        "from": "X85n1594_020:0583b09_0",
        "context_with_next_sentence": false
    }
]