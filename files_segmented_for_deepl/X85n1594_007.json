[
    {
        "source_sentence": "佛祖綱目卷第七(洪字號)",
        "from": "X85n1594_007:0573a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甲子(周桓王三年起)乙酉(莊王元年)庚子(釐王元年)乙巳(惠王元年)癸亥(周惠王十九年止)周莊王十年甲午夏五月五日。",
        "from": "X85n1594_007:0573a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒星不現。",
        "from": "X85n1594_007:0573a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "星殞如雨。",
        "from": "X85n1594_007:0573a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃西方文殊菩薩。",
        "from": "X85n1594_007:0573a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於雪山化。",
        "from": "X85n1594_007:0573a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五百仙人而歸本。",
        "from": "X85n1594_007:0573a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國光明掩於常星。",
        "from": "X85n1594_007:0573a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不現也。",
        "from": "X85n1594_007:0573a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(己丑)五祖提多迦傳法彌遮迦",
        "from": "X85n1594_007:0573a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提多迦。",
        "from": "X85n1594_007:0573a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(此云通真量)摩伽陀國人。",
        "from": "X85n1594_007:0573a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初名香眾生。",
        "from": "X85n1594_007:0573a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時父夢金日自屋而出。",
        "from": "X85n1594_007:0573a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照耀天地。",
        "from": "X85n1594_007:0573a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前有大山。",
        "from": "X85n1594_007:0573a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸寶嚴飾。",
        "from": "X85n1594_007:0573a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "山嵿泉湧。",
        "from": "X85n1594_007:0573a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滂沱四流。",
        "from": "X85n1594_007:0573a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後遇毱多尊者。",
        "from": "X85n1594_007:0573a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為解之曰。",
        "from": "X85n1594_007:0573a15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶山者吾身也。",
        "from": "X85n1594_007:0573a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "泉湧者法無盡也。",
        "from": "X85n1594_007:0573a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日從屋出者。",
        "from": "X85n1594_007:0573a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今入道之相也。",
        "from": "X85n1594_007:0573a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照耀天地者。",
        "from": "X85n1594_007:0573a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝智慧超越也。",
        "from": "X85n1594_007:0573a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因為更今名。",
        "from": "X85n1594_007:0573a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提多迦聞偈已。",
        "from": "X85n1594_007:0573a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "踊躍述偈曰。",
        "from": "X85n1594_007:0573a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "巍巍七寶山。",
        "from": "X85n1594_007:0573a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常出智慧泉。",
        "from": "X85n1594_007:0573a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "回為真法味。",
        "from": "X85n1594_007:0573a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能度諸有緣。",
        "from": "X85n1594_007:0573a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毱多亦說偈曰。",
        "from": "X85n1594_007:0573a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我法傳於汝。",
        "from": "X85n1594_007:0573a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當現大智慧。",
        "from": "X85n1594_007:0573a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金日從屋出。",
        "from": "X85n1594_007:0573a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照耀於天地。",
        "from": "X85n1594_007:0573a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提多迦聞偈。",
        "from": "X85n1594_007:0573a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設禮奉持。",
        "from": "X85n1594_007:0573a20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後至中印度。",
        "from": "X85n1594_007:0573a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼國有八千大仙。",
        "from": "X85n1594_007:0573a21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彌遮迦為首。",
        "from": "X85n1594_007:0573a21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞提多迦至。",
        "from": "X85n1594_007:0573a21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "率眾瞻禮。",
        "from": "X85n1594_007:0573a22_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂提多迦曰。",
        "from": "X85n1594_007:0573a22_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔與師同生梵天。",
        "from": "X85n1594_007:0573a22_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我遇阿私陀仙。",
        "from": "X85n1594_007:0573a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "授我仙法。",
        "from": "X85n1594_007:0573a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師逢十力弟子。",
        "from": "X85n1594_007:0573a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修習禪那。",
        "from": "X85n1594_007:0573a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自此報分殊途。",
        "from": "X85n1594_007:0573a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已經六劫。",
        "from": "X85n1594_007:0573a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提多迦曰。",
        "from": "X85n1594_007:0573a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "支離累劫。",
        "from": "X85n1594_007:0573a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誠哉不虗。",
        "from": "X85n1594_007:0573a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今可捨邪歸正。",
        "from": "X85n1594_007:0573b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以入佛乘。",
        "from": "X85n1594_007:0573b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彌遮迦曰。",
        "from": "X85n1594_007:0573b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔阿私陀仙人。",
        "from": "X85n1594_007:0573b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "授我記云。",
        "from": "X85n1594_007:0573b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝却後六劫。",
        "from": "X85n1594_007:0573b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當遇同學。",
        "from": "X85n1594_007:0573b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲無漏果。",
        "from": "X85n1594_007:0573b02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今也相遇。",
        "from": "X85n1594_007:0573b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非宿緣耶。",
        "from": "X85n1594_007:0573b03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願師慈悲。",
        "from": "X85n1594_007:0573b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令我解脫。",
        "from": "X85n1594_007:0573b03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提多迦即度出家。",
        "from": "X85n1594_007:0573b03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命諸聖授戒。",
        "from": "X85n1594_007:0573b04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其餘仙眾。",
        "from": "X85n1594_007:0573b04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始生我慢。",
        "from": "X85n1594_007:0573b04_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提多迦示大神通。",
        "from": "X85n1594_007:0573b04_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是俱發菩提心。",
        "from": "X85n1594_007:0573b05_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時出家。",
        "from": "X85n1594_007:0573b05_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提多迦乃告彌遮迦曰。",
        "from": "X85n1594_007:0573b05_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔如來以大法眼藏。",
        "from": "X85n1594_007:0573b06_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密付迦葉。",
        "from": "X85n1594_007:0573b06_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "展轉相授。",
        "from": "X85n1594_007:0573b06_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而至於我。",
        "from": "X85n1594_007:0573b06_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今付汝。",
        "from": "X85n1594_007:0573b07_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當護念之。",
        "from": "X85n1594_007:0573b07_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃說偈曰。",
        "from": "X85n1594_007:0573b07_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通達本法。",
        "from": "X85n1594_007:0573b07_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心無法無。",
        "from": "X85n1594_007:0573b07_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非法悟了。",
        "from": "X85n1594_007:0573b08_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同未悟無。",
        "from": "X85n1594_007:0573b08_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心亦無法。",
        "from": "X85n1594_007:0573b08_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說偈已。",
        "from": "X85n1594_007:0573b08_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "踴身虗空。",
        "from": "X85n1594_007:0573b08_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作十八變。",
        "from": "X85n1594_007:0573b09_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火光三昧。",
        "from": "X85n1594_007:0573b09_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自焚其軀。",
        "from": "X85n1594_007:0573b09_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彌遮迦與八千比丘。",
        "from": "X85n1594_007:0573b09_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同收舍利。",
        "from": "X85n1594_007:0573b10_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於班茶山中。",
        "from": "X85n1594_007:0573b10_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起塔供養。",
        "from": "X85n1594_007:0573b10_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當此土周莊王五年己丑歲也。",
        "from": "X85n1594_007:0573b10_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛祖綱目卷第七",
        "from": "X85n1594_007:0573b13_0",
        "context_with_next_sentence": false
    }
]